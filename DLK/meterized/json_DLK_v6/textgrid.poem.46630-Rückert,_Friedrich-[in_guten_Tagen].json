{"textgrid.poem.46630": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in guten Tagen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In guten Tagen", "tokens": ["In", "gu\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Konnt' ich den Menschen leicht entsagen;", "tokens": ["Konnt'", "ich", "den", "Men\u00b7schen", "leicht", "ent\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In b\u00f6sen Stunden", "tokens": ["In", "b\u00f6\u00b7sen", "Stun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gern h\u00e4tt' ich ihren Trost empfunden;", "tokens": ["Gern", "h\u00e4tt'", "ich", "ih\u00b7ren", "Trost", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein sie lie\u00dfen", "tokens": ["Al\u00b7lein", "sie", "lie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mich meinen Schmerz allein genie\u00dfen.", "tokens": ["Mich", "mei\u00b7nen", "Schmerz", "al\u00b7lein", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}