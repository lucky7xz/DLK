{"textgrid.poem.26664": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wes Geistes Kind im Kopf gesessen,", "tokens": ["Wes", "Geis\u00b7tes", "Kind", "im", "Kopf", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Konnt er auf jeder Nase lesen:", "tokens": ["Konnt", "er", "auf", "je\u00b7der", "Na\u00b7se", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch \u2013 da\u00df ", "tokens": ["Und", "doch", "\u2013", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADV", "$(", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Den Gott zu diesem Werk erlesen,", "tokens": ["Den", "Gott", "zu", "die\u00b7sem", "Werk", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Konnt er nicht auf der ", "tokens": ["Konnt", "er", "nicht", "auf", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "ART"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}