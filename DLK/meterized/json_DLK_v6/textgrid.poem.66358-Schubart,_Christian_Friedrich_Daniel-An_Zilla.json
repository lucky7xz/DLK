{"textgrid.poem.66358": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Zilla", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Fr\u00fchling l\u00e4chelt!", "tokens": ["Wie", "der", "Fr\u00fch\u00b7ling", "l\u00e4\u00b7chelt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie der junge West", "tokens": ["Wie", "der", "jun\u00b7ge", "West"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Den erhitzten Sch\u00e4fer f\u00e4chelt,", "tokens": ["Den", "er\u00b7hitz\u00b7ten", "Sch\u00e4\u00b7fer", "f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Busenrose seines M\u00e4dchens bl\u00e4st!", "tokens": ["In", "die", "Bu\u00b7sen\u00b7ro\u00b7se", "sei\u00b7nes", "M\u00e4d\u00b7chens", "bl\u00e4st", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Wie die Regenbogenschale", "tokens": ["Wie", "die", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7scha\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Siebenfarbig gl\u00e4nzt!", "tokens": ["Sie\u00b7ben\u00b7far\u00b7big", "gl\u00e4nzt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wie im nahen Thale", "tokens": ["Wie", "im", "na\u00b7hen", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Wie der Fr\u00fchling in dem stillern", "tokens": ["Wie", "der", "Fr\u00fch\u00b7ling", "in", "dem", "stil\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sturmbefreiten Aether schwebt!", "tokens": ["Sturm\u00b7be\u00b7frei\u00b7ten", "A\u00b7e\u00b7ther", "schwebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+----+", "measure": "unknown.measure.tri"}, "line.11": {"text": "Wie die Nachtigall mit Trillern", "tokens": ["Wie", "die", "Nach\u00b7ti\u00b7gall", "mit", "Tril\u00b7lern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wei\u00dfe Bl\u00fcthen hebt!", "tokens": ["Wei\u00b7\u00dfe", "Bl\u00fc\u00b7then", "hebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Ach, wie lieblichbl\u00fchend ist die Flur!", "tokens": ["Ach", ",", "wie", "lieb\u00b7lich\u00b7bl\u00fc\u00b7hend", "ist", "die", "Flur", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Wie elysisch die Natur!", "tokens": ["Wie", "e\u00b7ly\u00b7sisch", "die", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Doch ich f\u00fchle keinen Maien,", "tokens": ["Doch", "ich", "f\u00fch\u00b7le", "kei\u00b7nen", "Mai\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Keinen Junius.", "tokens": ["Kei\u00b7nen", "Ju\u00b7nius", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.17": {"text": "Kann den J\u00fcngling ein Olympus freuen,", "tokens": ["Kann", "den", "J\u00fcng\u00b7ling", "ein", "O\u00b7lym\u00b7pus", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Ohne deinen Ku\u00df?", "tokens": ["Oh\u00b7ne", "dei\u00b7nen", "Ku\u00df", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Drohend steh' ich hier, wie Werther,", "tokens": ["Dro\u00b7hend", "steh'", "ich", "hier", ",", "wie", "Wert\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "PWAV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Mit dem Mordgewehr,", "tokens": ["Mit", "dem", "Mord\u00b7ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "Alle Haine, Th\u00e4ler, Oerter", "tokens": ["Al\u00b7le", "Hai\u00b7ne", ",", "Th\u00e4\u00b7ler", ",", "O\u00b7er\u00b7ter"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.22": {"text": "Liegen um mich freudenleer!", "tokens": ["Lie\u00b7gen", "um", "mich", "freu\u00b7den\u00b7leer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Denn nicht ich, en andrer", "tokens": ["Denn", "nicht", "ich", ",", "en", "an\u00b7drer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "PPER", "$,", "FM", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "War's, den, ", "tokens": ["Wa\u00b7r's", ",", "den", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Donnre, Mordgewehr! \u2013 ich sinke! \u2013 Wandrer,", "tokens": ["Donn\u00b7re", ",", "Mord\u00b7ge\u00b7wehr", "!", "\u2013", "ich", "sin\u00b7ke", "!", "\u2013", "Wand\u00b7rer", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "$,", "ADV", "$.", "$(", "PPER", "VVFIN", "$.", "$(", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Liebe hat den J\u00fcngling hier entseelt.", "tokens": ["Lie\u00b7be", "hat", "den", "J\u00fcng\u00b7ling", "hier", "ent\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}