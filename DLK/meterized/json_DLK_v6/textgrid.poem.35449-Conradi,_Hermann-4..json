{"textgrid.poem.35449": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Sklavendienst der L\u00fcge", "tokens": ["Im", "Skla\u00b7ven\u00b7dienst", "der", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich den Tag verbracht ...", "tokens": ["Hab'", "ich", "den", "Tag", "ver\u00b7bracht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun hat den Gnadenschleier leis", "tokens": ["Nun", "hat", "den", "Gna\u00b7den\u00b7schlei\u00b7er", "leis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herabgesenkt die Nacht.", "tokens": ["Her\u00b7ab\u00b7ge\u00b7senkt", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schweigt vertr\u00e4umt die Runde,", "tokens": ["Es", "schweigt", "ver\u00b7tr\u00e4umt", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur raunend der Nachtwind rauscht \u2013", "tokens": ["Nur", "rau\u00b7nend", "der", "Nacht\u00b7wind", "rauscht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ich aber mit brennendem Munde", "tokens": ["Ich", "a\u00b7ber", "mit", "bren\u00b7nen\u00b7dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Habe Stunde um Stunde", "tokens": ["Ha\u00b7be", "Stun\u00b7de", "um", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Mit Geistern aus n\u00e4cht'gem Grunde", "tokens": ["Mit", "Geis\u00b7tern", "aus", "n\u00e4cht'\u00b7gem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wilde Zwiesprach getauscht!", "tokens": ["Wil\u00b7de", "Zwie\u00b7sprach", "ge\u00b7tauscht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Hei! Wie er mich umflattert,", "tokens": ["Hei", "!", "Wie", "er", "mich", "um\u00b7flat\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Geister toller Schwarm!", "tokens": ["Der", "Geis\u00b7ter", "tol\u00b7ler", "Schwarm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie er mich pre\u00dft mit trunkner Lust", "tokens": ["Wie", "er", "mich", "pre\u00dft", "mit", "trunk\u00b7ner", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinen Riesenarm!", "tokens": ["In", "sei\u00b7nen", "Rie\u00b7se\u00b7narm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Frage er auf Frage", "tokens": ["Wie", "Fra\u00b7ge", "er", "auf", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In meine Seele schreit!", "tokens": ["In", "mei\u00b7ne", "See\u00b7le", "schreit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ob ich bang verzage,", "tokens": ["Und", "ob", "ich", "bang", "ver\u00b7za\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Brust mir blutig schlage", "tokens": ["Die", "Brust", "mir", "blu\u00b7tig", "schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und bete, da\u00df es tage:", "tokens": ["Und", "be\u00b7te", ",", "da\u00df", "es", "ta\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}