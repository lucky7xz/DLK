{"textgrid.poem.63414": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nu schint de S\u00fcnn so warm un hell", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nu schint de S\u00fcnn so warm un hell", "tokens": ["Nu", "schint", "de", "S\u00fcnn", "so", "warm", "un", "hell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NE", "ADV", "ADJD", "FM", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "up Feld un Wold un Wisch;", "tokens": ["up", "Feld", "un", "Wold", "un", "Wisch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "FM", "FM", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "as S\u00fclwer blankem B\u00e4k un S\u00f6ll,", "tokens": ["as", "S\u00fcl\u00b7wer", "blan\u00b7kem", "B\u00e4k", "un", "S\u00f6ll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un allens s\u00fcht so frisch,", "tokens": ["un", "al\u00b7lens", "s\u00fcht", "so", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "so grall, so quick ut un vergn\u00f6gt,", "tokens": ["so", "grall", ",", "so", "quick", "ut", "un", "ver\u00b7gn\u00f6gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "as een, de insl\u00f6p \u00fcngeweegt", "tokens": ["as", "e\u00b7en", ",", "de", "ins\u00b7l\u00f6p", "\u00fcn\u00b7ge\u00b7weegt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "un, nu he orig slapen,", "tokens": ["un", ",", "nu", "he", "o\u00b7rig", "sla\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "ADV", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "ritt beid sin Ogen apen.", "tokens": ["ritt", "beid", "sin", "O\u00b7gen", "a\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wu quinkeleert de Lewark sch\u00f6n,", "tokens": ["Wu", "quin\u00b7ke\u00b7leert", "de", "Le\u00b7wark", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wu lustig quakt de Pogg!", "tokens": ["wu", "lus\u00b7tig", "quakt", "de", "Pogg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dor achter schint de Saat so gr\u00f6n,", "tokens": ["Dor", "ach\u00b7ter", "schint", "de", "Saat", "so", "gr\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "VVFIN", "NE", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "de Weit, de Rapp un Rogg.", "tokens": ["de", "Weit", ",", "de", "Rapp", "un", "Rogg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "FM", "FM", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dat k\u00fcmmt sick ganz gef\u00e4hrlich all,", "tokens": ["Dat", "k\u00fcmmt", "sick", "ganz", "ge\u00b7f\u00e4hr\u00b7lich", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "ADJD", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "dat helpt sick mal, dor kann sick ball,", "tokens": ["dat", "helpt", "sick", "mal", ",", "dor", "kann", "sick", "ball", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "ADV", "$,", "ADV", "VMFIN", "PRF", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "v\u00f6rut tenz bi de Eeken,", "tokens": ["v\u00f6\u00b7rut", "tenz", "bi", "de", "E\u00b7e\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "'ne Kreih all in verst\u00e4ken.", "tokens": ["'ne", "Kreih", "all", "in", "ver\u00b7st\u00e4\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "De Fleeder un de Stickelbeer,", "tokens": ["De", "Flee\u00b7der", "un", "de", "Sti\u00b7ckel\u00b7beer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wu smuck dat an sick lett!", "tokens": ["wu", "smuck", "dat", "an", "sick", "lett", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "----+-", "measure": "unknown.measure.single"}, "line.3": {"text": "De heww' all grote gr\u00f6ne Bl\u00e4r", "tokens": ["De", "hew\u00b7w'", "all", "gro\u00b7te", "gr\u00f6\u00b7ne", "Bl\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "un richtig an all sett't;", "tokens": ["un", "rich\u00b7tig", "an", "all", "sett't", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJD", "APPR", "PIAT", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "man \u0153werst \u00fcns' oll Appelbom", "tokens": ["man", "\u0153werst", "\u00fcns'", "oll", "Ap\u00b7pel\u00b7bom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADV", "ADJD", "NE"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "de reckt sick noch as half in Drom,", "tokens": ["de", "reckt", "sick", "noch", "as", "half", "in", "Drom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "as w\u00fcrr dor nicks vers\u00e4ten,", "tokens": ["as", "w\u00fcrr", "dor", "nicks", "ver\u00b7s\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "sl\u00f6p he ok noch'n b\u00e4ten.", "tokens": ["sl\u00f6p", "he", "ok", "noch'n", "b\u00e4\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nu kik eens di\u00df twee Knuppens, kik!", "tokens": ["Nu", "kik", "e\u00b7ens", "di\u00df", "twee", "Knup\u00b7pens", ",", "kik", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "ADV", "PDS", "NE", "NE", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sehn s' nich as Eier ut,", "tokens": ["Sehn", "s'", "nich", "as", "Ei\u00b7er", "ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "de hickt all heww', un wu nu glik", "tokens": ["de", "hickt", "all", "hew\u00b7w'", ",", "un", "wu", "nu", "glik"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,", "FM", "FM", "ADV", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "de K\u00fckens krupen rut?", "tokens": ["de", "K\u00fc\u00b7kens", "kru\u00b7pen", "rut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "As ob se s\u00fclsten up sick pickt", "tokens": ["As", "ob", "se", "s\u00fcls\u00b7ten", "up", "sick", "pickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "FM", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "un nu de Snabel ruter kickt?", "tokens": ["un", "nu", "de", "Sna\u00b7bel", "ru\u00b7ter", "kickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wu lang will dat noch wohr'n,", "tokens": ["Wu", "lang", "will", "dat", "noch", "wohr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VMFIN", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "is gr\u00f6n de heel oll Gor'n.", "tokens": ["is", "gr\u00f6n", "de", "heel", "oll", "Gor'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So licht ward mi dat im vergn\u00f6gt;", "tokens": ["So", "licht", "ward", "mi", "dat", "im", "ver\u00b7gn\u00f6gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NE", "ART", "APPRART", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "ick weet nich, wat ick m\u00fccht \u2013", "tokens": ["ick", "weet", "nich", ",", "wat", "ick", "m\u00fccht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "as ob dor wat in mi sick r\u00f6gt,", "tokens": ["as", "ob", "dor", "wat", "in", "mi", "sick", "r\u00f6gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "wat Fl\u00fcnken hett un fl\u00fcggt;", "tokens": ["wat", "Fl\u00fcn\u00b7ken", "hett", "un", "fl\u00fcggt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VAFIN", "FM", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "so heet ward mi dat in't Gebl\u00f6t,", "tokens": ["so", "heet", "ward", "mi", "dat", "in't", "Ge\u00b7bl\u00f6t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "FM", "FM", "FM", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "as ob ick s\u00fclm girn Knuppens sch\u00f6t,", "tokens": ["as", "ob", "ick", "s\u00fclm", "girn", "Knup\u00b7pens", "sch\u00f6t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "as wir noch half in Drom ick", "tokens": ["as", "wir", "noch", "half", "in", "Drom", "ick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "APPR", "NN", "PPER"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "un ok sonn Ort vun Bom ick.", "tokens": ["un", "ok", "sonn", "Ort", "vun", "Bom", "ick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}