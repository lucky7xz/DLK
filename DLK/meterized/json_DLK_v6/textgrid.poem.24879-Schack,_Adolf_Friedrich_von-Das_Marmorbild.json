{"textgrid.poem.24879": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Marmorbild", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn beim Fr\u00fchglanz des Hymett", "tokens": ["Wenn", "beim", "Fr\u00fch\u00b7glanz", "des", "Hy\u00b7mett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgens auf mein Ruhebett", "tokens": ["Mor\u00b7gens", "auf", "mein", "Ru\u00b7he\u00b7bett"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanft die Strahlen zittern,", "tokens": ["Sanft", "die", "Strah\u00b7len", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Immer l\u00e4chelst, teures Bild,", "tokens": ["Im\u00b7mer", "l\u00e4\u00b7chelst", ",", "teu\u00b7res", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du auf mich herab so mild", "tokens": ["Du", "auf", "mich", "her\u00b7ab", "so", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus den Epheugittern.", "tokens": ["Aus", "den", "E\u00b7pheu\u00b7git\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Deine Z\u00fcge, hold und traut,", "tokens": ["Dei\u00b7ne", "Z\u00fc\u00b7ge", ",", "hold", "und", "traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! da\u00df ich sie doch geschaut,", "tokens": ["Ach", "!", "da\u00df", "ich", "sie", "doch", "ge\u00b7schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sie lebend waren,", "tokens": ["Als", "sie", "le\u00b7bend", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In die Augen dir geblickt,", "tokens": ["In", "die", "Au\u00b7gen", "dir", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh' sie in den Schlaf genickt", "tokens": ["Eh'", "sie", "in", "den", "Schlaf", "ge\u00b7nickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von zweitausend Jahren!", "tokens": ["Von", "zweit\u00b7au\u00b7send", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dann in Delphis Waldesschlucht \u2013", "tokens": ["Dann", "in", "Del\u00b7phis", "Wal\u00b7des\u00b7schlucht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber uns die Purpurfrucht", "tokens": ["Ue\u00b7ber", "uns", "die", "Pur\u00b7pur\u00b7frucht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Granate leuchtend \u2013", "tokens": ["Der", "Gra\u00b7na\u00b7te", "leuch\u00b7tend", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "H\u00e4tten wir am Quell geruht,", "tokens": ["H\u00e4t\u00b7ten", "wir", "am", "Quell", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Apollons heil'ger Flut", "tokens": ["Mit", "A\u00b7pol\u00b7lons", "heil'\u00b7ger", "Flut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Unsre Lippen feuchtend.", "tokens": ["Uns\u00b7re", "Lip\u00b7pen", "feuch\u00b7tend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schauten von den Propyl\u00e4'n,", "tokens": ["Schau\u00b7ten", "von", "den", "Pro\u00b7py\u00b7l\u00e4'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Tempel von Athen", "tokens": ["Wie", "die", "Tem\u00b7pel", "von", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NE"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Felshinan sich bauten", "tokens": ["Fels\u00b7hi\u00b7nan", "sich", "bau\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "PRF", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und aus segelvollem Meer", "tokens": ["Und", "aus", "se\u00b7gel\u00b7vol\u00b7lem", "Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Vom Pir\u00e4us ferneher", "tokens": ["Vom", "Pi\u00b7r\u00e4us", "fer\u00b7ne\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "All die Inseln blauten.", "tokens": ["All", "die", "In\u00b7seln", "blau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Schweiften den Kephi\u00df entlang,", "tokens": ["Schweif\u00b7ten", "den", "Ke\u00b7phi\u00df", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Wo der Nachtigall Gesang", "tokens": ["Wo", "der", "Nach\u00b7ti\u00b7gall", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie im Walde stockte", "tokens": ["Nie", "im", "Wal\u00b7de", "stock\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und auf gr\u00fcnem Wiesenplan", "tokens": ["Und", "auf", "gr\u00fc\u00b7nem", "Wie\u00b7sen\u00b7plan"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fl\u00f6tenhauch der alte Pan", "tokens": ["Fl\u00f6\u00b7ten\u00b7hauch", "der", "al\u00b7te", "Pan"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Syrinx lockte.", "tokens": ["Aus", "der", "Sy\u00b7rinx", "lock\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "N\u00e4chtlich in Kolonos' Hain", "tokens": ["N\u00e4cht\u00b7lich", "in", "Ko\u00b7lo\u00b7nos'", "Hain"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Lauschten wir dem Jubelreihn,", "tokens": ["Lauschten", "wir", "dem", "Ju\u00b7bel\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie die Zimbel schallte", "tokens": ["Wie", "die", "Zim\u00b7bel", "schall\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und der Tanz von Nymph' und Faun", "tokens": ["Und", "der", "Tanz", "von", "Nym\u00b7ph'", "und", "Faun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Durch die rebenvollen Aun", "tokens": ["Durch", "die", "re\u00b7ben\u00b7vol\u00b7len", "Aun"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Labyrinthisch wallte;", "tokens": ["La\u00b7by\u00b7rin\u00b7thisch", "wall\u00b7te", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Und der Chiertraube Trank", "tokens": ["Und", "der", "Chier\u00b7trau\u00b7be", "Trank"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--++-+", "measure": "anapaest.init"}, "line.2": {"text": "Schl\u00fcrften wir im Laubgerank,", "tokens": ["Schl\u00fcrf\u00b7ten", "wir", "im", "Laub\u00b7ge\u00b7rank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberweht von Bl\u00fcten,", "tokens": ["Ue\u00b7ber\u00b7weht", "von", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "W\u00e4hrend bei der Leier Ton", "tokens": ["W\u00e4h\u00b7rend", "bei", "der", "Lei\u00b7er", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Alc\u00e4us' Skolion", "tokens": ["Und", "Al\u00b7c\u00e4us'", "Sko\u00b7li\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Unsre K\u00fcsse gl\u00fchten.", "tokens": ["Uns\u00b7re", "K\u00fcs\u00b7se", "gl\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Doch was tr\u00e4um' ich?", "tokens": ["Doch", "was", "tr\u00e4um'", "ich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ach, nur Gram", "tokens": ["Ach", ",", "nur", "Gram"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADV", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Bleibt mir, da\u00df zu sp\u00e4t ich kam", "tokens": ["Bleibt", "mir", ",", "da\u00df", "zu", "sp\u00e4t", "ich", "kam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PTKA", "ADJD", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Lebens Feste,", "tokens": ["Zu", "des", "Le\u00b7bens", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und, o Weib, verweht vom Wind", "tokens": ["Und", ",", "o", "Weib", ",", "ver\u00b7weht", "vom", "Wind"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seit zweitausend Jahren sind", "tokens": ["Seit", "zweit\u00b7au\u00b7send", "Jah\u00b7ren", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Aschenreste.", "tokens": ["Dei\u00b7ne", "A\u00b7schen\u00b7res\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}