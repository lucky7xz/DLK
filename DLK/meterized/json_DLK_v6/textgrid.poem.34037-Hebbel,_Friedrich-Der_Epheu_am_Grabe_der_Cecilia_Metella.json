{"textgrid.poem.34037": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Epheu am Grabe der Cecilia Metella", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Epheu, man hat dich verklagt, du sollst die B\u00e4ume entseelen,", "tokens": ["E\u00b7pheu", ",", "man", "hat", "dich", "ver\u00b7klagt", ",", "du", "sollst", "die", "B\u00e4u\u00b7me", "ent\u00b7see\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aber ich spreche dich los, da du die Steine belebst!", "tokens": ["A\u00b7ber", "ich", "spre\u00b7che", "dich", "los", ",", "da", "du", "die", "Stei\u00b7ne", "be\u00b7lebst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jenen Frevel erblickt' ich noch nie; dies reizende Wunder", "tokens": ["Je\u00b7nen", "Fre\u00b7vel", "er\u00b7blickt'", "ich", "noch", "nie", ";", "dies", "rei\u00b7zen\u00b7de", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "$.", "PDS", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sah ich noch heute vollbracht: gr\u00fcnt doch das traurige Grab.", "tokens": ["Sah", "ich", "noch", "heu\u00b7te", "voll\u00b7bracht", ":", "gr\u00fcnt", "doch", "das", "trau\u00b7ri\u00b7ge", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}