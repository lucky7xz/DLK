{"textgrid.poem.49047": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "13. Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur sch\u00f6nern Vorzeit flieht aus d\u00fcstrer H\u00fclle", "tokens": ["Zur", "sch\u00f6\u00b7nern", "Vor\u00b7zeit", "flieht", "aus", "d\u00fcst\u00b7rer", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Gegenwart mein sehnsuchtsvoller Blick,", "tokens": ["Der", "Ge\u00b7gen\u00b7wart", "mein", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und goldnes Licht und heitre Lebensf\u00fclle,", "tokens": ["Und", "gold\u00b7nes", "Licht", "und", "heit\u00b7re", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bringt schnell ihr Bild dem Trauernden zur\u00fcck!", "tokens": ["Bringt", "schnell", "ihr", "Bild", "dem", "Trau\u00b7ern\u00b7den", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Rosendufte schwebt Erinnrung nieder,", "tokens": ["Im", "Ro\u00b7sen\u00b7duf\u00b7te", "schwebt", "E\u00b7rinn\u00b7rung", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Immergr\u00fcn des Mitgef\u00fchls geschm\u00fcckt,", "tokens": ["Vom", "Im\u00b7mer\u00b7gr\u00fcn", "des", "Mit\u00b7ge\u00b7f\u00fchls", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mild belebt ihr Hauch die Bl\u00fcthen wieder,", "tokens": ["Und", "mild", "be\u00b7lebt", "ihr", "Hauch", "die", "Bl\u00fc\u00b7then", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Psyche der Vergangenheit entpfl\u00fcckt.", "tokens": ["Die", "Psy\u00b7che", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "ent\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die tief verh\u00fcllte Gegenwart verschwindet", "tokens": ["Die", "tief", "ver\u00b7h\u00fcll\u00b7te", "Ge\u00b7gen\u00b7wart", "ver\u00b7schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor jenem Glanz \u2013 hin zu der s\u00fc\u00dfen Flur", "tokens": ["Vor", "je\u00b7nem", "Glanz", "\u2013", "hin", "zu", "der", "s\u00fc\u00b7\u00dfen", "Flur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$(", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Vaterlandes schwebt der Blick und findet", "tokens": ["Des", "Va\u00b7ter\u00b7lan\u00b7des", "schwebt", "der", "Blick", "und", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Jugend Freuden unverwischte Spur.", "tokens": ["Der", "Ju\u00b7gend", "Freu\u00b7den", "un\u00b7ver\u00b7wischte", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Dort wandeln sie \u2013 die heiligen Gestalten", "tokens": ["Dort", "wan\u00b7deln", "sie", "\u2013", "die", "hei\u00b7li\u00b7gen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hochgef\u00fchl der Liebe mir verband,", "tokens": ["Die", "Hoch\u00b7ge\u00b7f\u00fchl", "der", "Lie\u00b7be", "mir", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die treu und sch\u00fctzend \u00fcber mir zu walten,", "tokens": ["Die", "treu", "und", "sch\u00fct\u00b7zend", "\u00fc\u00b7ber", "mir", "zu", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Tausenden des Herzens Stimme fand!", "tokens": ["Aus", "Tau\u00b7sen\u00b7den", "des", "Her\u00b7zens", "Stim\u00b7me", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ihr sch\u00f6nen Ideale jener Stunden,", "tokens": ["Ihr", "sch\u00f6\u00b7nen", "I\u00b7dea\u00b7le", "je\u00b7ner", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo mich der Freundschaft holder Kreis umwand,", "tokens": ["Wo", "mich", "der", "Freund\u00b7schaft", "hol\u00b7der", "Kreis", "um\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr bleibt dem Trauernden stets neu verbunden,", "tokens": ["Ihr", "bleibt", "dem", "Trau\u00b7ern\u00b7den", "stets", "neu", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn selbst sein Schmerz b\u00fcrgt ihm, da\u00df er euch fand!", "tokens": ["Denn", "selbst", "sein", "Schmerz", "b\u00fcrgt", "ihm", ",", "da\u00df", "er", "euch", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und in der Trennung tief verh\u00fcllten Tagen,", "tokens": ["Und", "in", "der", "Tren\u00b7nung", "tief", "ver\u00b7h\u00fcll\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus jenem sel'gen Kreise weit verbannt,", "tokens": ["Aus", "je\u00b7nem", "sel'\u00b7gen", "Krei\u00b7se", "weit", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird tr\u00f6stend mir noch die Erinnrung sagen:", "tokens": ["Wird", "tr\u00f6s\u00b7tend", "mir", "noch", "die", "E\u00b7rinn\u00b7rung", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u00bbdu hast das H\u00f6chste, G\u00f6ttlichste gekannt!\u00ab", "tokens": ["\u00bb", "du", "hast", "das", "H\u00f6chs\u00b7te", ",", "G\u00f6tt\u00b7lichs\u00b7te", "ge\u00b7kannt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "$,", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Umsonst mag dann die d\u00fcstre Lethe winken,", "tokens": ["Um\u00b7sonst", "mag", "dann", "die", "d\u00fcst\u00b7re", "Le\u00b7the", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die welken Kr\u00e4nze ihrer Fluth zu weih'n;", "tokens": ["Die", "wel\u00b7ken", "Kr\u00e4n\u00b7ze", "ih\u00b7rer", "Fluth", "zu", "weih'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Herz verschm\u00e4ht Vergessenheit zu trinken,", "tokens": ["Das", "Herz", "ver\u00b7schm\u00e4ht", "Ver\u00b7ges\u00b7sen\u00b7heit", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was durch Erinnrung lebt, bleibt ewig Mein!", "tokens": ["Was", "durch", "E\u00b7rinn\u00b7rung", "lebt", ",", "bleibt", "e\u00b7wig", "Mein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wenn jeder Lebensfreude Strahl vergl\u00fchet,", "tokens": ["Wenn", "je\u00b7der", "Le\u00b7bens\u00b7freu\u00b7de", "Strahl", "ver\u00b7gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Hoffnungsstern die Zukunft uns erhellt,", "tokens": ["Kein", "Hoff\u00b7nungs\u00b7stern", "die", "Zu\u00b7kunft", "uns", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann winkt Erinnerung, und schnell entbl\u00fchet", "tokens": ["Dann", "winkt", "E\u00b7rin\u00b7ne\u00b7rung", ",", "und", "schnell", "ent\u00b7bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihrem Schein uns eine sch\u00f6n're Welt.", "tokens": ["In", "ih\u00b7rem", "Schein", "uns", "ei\u00b7ne", "sch\u00f6n'\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "(so f\u00fchlt das Herz, wenn Licht und Sterne fliehen,", "tokens": ["(", "so", "f\u00fchlt", "das", "Herz", ",", "wenn", "Licht", "und", "Ster\u00b7ne", "flie\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dunkle Nacht auf Flur und Hainen liegt,", "tokens": ["Und", "dunk\u00b7le", "Nacht", "auf", "Flur", "und", "Hai\u00b7nen", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bey Philomelens Zaubermelodien,", "tokens": ["Bey", "Phi\u00b7lo\u00b7me\u00b7lens", "Zau\u00b7ber\u00b7me\u00b7lo\u00b7di\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Sich in Elysiums Tr\u00e4ume eingewiegt!)", "tokens": ["Sich", "in", "E\u00b7ly\u00b7si\u00b7ums", "Tr\u00e4u\u00b7me", "ein\u00b7ge\u00b7wiegt", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPR", "NE", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Und w\u00e4r' nicht ohne sie das Gl\u00fcck verloren,", "tokens": ["Und", "w\u00e4r'", "nicht", "oh\u00b7ne", "sie", "das", "Gl\u00fcck", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Jubelton, den nur Empfindung leiht,", "tokens": ["Mit", "Ju\u00b7bel\u00b7ton", ",", "den", "nur", "Emp\u00b7fin\u00b7dung", "leiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Tag zu gr\u00fc\u00dfen, der einst dich geboren,", "tokens": ["Den", "Tag", "zu", "gr\u00fc\u00b7\u00dfen", ",", "der", "einst", "dich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich heut' auf's neu' dem s\u00fc\u00dfen Daseyn weiht?", "tokens": ["Dich", "heut'", "auf's", "neu'", "dem", "s\u00fc\u00b7\u00dfen", "Da\u00b7seyn", "weiht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Sey gl\u00fccklich! rief dir einst mit heitern Herzen", "tokens": ["Sey", "gl\u00fcck\u00b7lich", "!", "rief", "dir", "einst", "mit", "hei\u00b7tern", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$.", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Knabe zu, vom Hoffnungstraum erf\u00fcllt,", "tokens": ["Der", "Kna\u00b7be", "zu", ",", "vom", "Hoff\u00b7nungs\u00b7traum", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als einst ein Maskenspiel dir unter Scherzen,", "tokens": ["Als", "einst", "ein", "Mas\u00b7ken\u00b7spiel", "dir", "un\u00b7ter", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freunde Wunsch zum Wiegenfest enth\u00fcllt.", "tokens": ["Der", "Freun\u00b7de", "Wunsch", "zum", "Wie\u00b7gen\u00b7fest", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ach! Jahre sind seit jenem Tag verschwunden,", "tokens": ["Ach", "!", "Jah\u00b7re", "sind", "seit", "je\u00b7nem", "Tag", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im raschen Flug, wie Rosen schnell vergl\u00fchn. \u2013", "tokens": ["Im", "ra\u00b7schen", "Flug", ",", "wie", "Ro\u00b7sen", "schnell", "ver\u00b7gl\u00fchn", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Kr\u00e4nze nur, die dir dein Werth gewunden,", "tokens": ["Die", "Kr\u00e4n\u00b7ze", "nur", ",", "die", "dir", "dein", "Werth", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", 