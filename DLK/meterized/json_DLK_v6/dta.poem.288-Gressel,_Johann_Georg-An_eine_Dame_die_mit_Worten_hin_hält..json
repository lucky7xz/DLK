{"dta.poem.288": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine  Dame  die mit Worten hin h\u00e4lt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wolt' ich deinen Worten glauben", "tokens": ["Wolt'", "ich", "dei\u00b7nen", "Wor\u00b7ten", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt ich nur mit leeren Schein/", "tokens": ["Spielt", "ich", "nur", "mit", "lee\u00b7ren", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mein Engel deine Liebe", "tokens": ["Denn", "mein", "En\u00b7gel", "dei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hegt nicht Flammen-reiche Triebe/", "tokens": ["Hegt", "nicht", "Flam\u00b7men\u00b7rei\u00b7che", "Trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der Liebe Nahrung seyn.", "tokens": ["Die", "der", "Lie\u00b7be", "Nah\u00b7rung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wolt ich deinen Worten glauben", "tokens": ["Wolt", "ich", "dei\u00b7nen",