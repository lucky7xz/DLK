in", "En\u00b7gel", "/", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Linder doch die Liebes-Pein.", "tokens": ["Lin\u00b7der", "doch", "die", "Lie\u00b7bes\u00b7Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gib der Augen helle Strahlen", "tokens": ["Gib", "der", "Au\u00b7gen", "hel\u00b7le", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Seel zum Anfuhrts-Licht", "tokens": ["Mei\u00b7ner", "Seel", "zum", "An\u00b7fuhrts\u00b7Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich deinen Port erblicke", "tokens": ["Da\u00df", "ich", "dei\u00b7nen", "Port", "er\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Anmuth sammt dem Gl\u00fccke", "tokens": ["Wo", "die", "An\u00b7muth", "sammt", "dem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhre Wohnung aufgericht", "tokens": ["Ih\u00b7re", "Woh\u00b7nung", "auf\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gib der Augen helle Strahlen", "tokens": ["Gib", "der", "Au\u00b7gen", "hel\u00b7le", "Strah\