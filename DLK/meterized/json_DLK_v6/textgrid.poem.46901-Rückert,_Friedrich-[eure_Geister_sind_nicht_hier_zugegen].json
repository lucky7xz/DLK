{"textgrid.poem.46901": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[eure Geister sind nicht hier zugegen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eure Geister sind nicht hier zugegen,", "tokens": ["Eu\u00b7re", "Geis\u00b7ter", "sind", "nicht", "hier", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Todten ihre Todten legen.", "tokens": ["Wo", "die", "Tod\u00b7ten", "ih\u00b7re", "Tod\u00b7ten", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch die Gegenwart auch eurer Leiber", "tokens": ["Doch", "die", "Ge\u00b7gen\u00b7wart", "auch", "eu\u00b7rer", "Lei\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fchl' ich nicht in diesen Grabgehegen.", "tokens": ["F\u00fchl'", "ich", "nicht", "in", "die\u00b7sen", "Grab\u00b7ge\u00b7he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df ihr l\u00e4get unter diesen H\u00fcgeln,", "tokens": ["Da\u00df", "ihr", "l\u00e4\u00b7get", "un\u00b7ter", "die\u00b7sen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Das zu glauben kann mich nichts bewegen.", "tokens": ["Das", "zu", "glau\u00b7ben", "kann", "mich", "nichts", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "VMFIN", "PRF", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "W\u00e4ret ihr mir leiblich nah, es m\u00fc\u00dfte", "tokens": ["W\u00e4\u00b7ret", "ihr", "mir", "leib\u00b7lich", "nah", ",", "es", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "ADJD", "$,", "PPER", "VMFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mir im Herzen ein Gef\u00fchl sich regen.", "tokens": ["Mir", "im", "Her\u00b7zen", "ein", "Ge\u00b7f\u00fchl", "sich", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "PRF", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Doch ich f\u00fchl's, mit Leib zugleich und Geiste", "tokens": ["Doch", "ich", "f\u00fchl's", ",", "mit", "Leib", "zu\u00b7gleich", "und", "Geis\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Seid ihr aufgeschwebt dem Licht entgegen.", "tokens": ["Seid", "ihr", "auf\u00b7ge\u00b7schwebt", "dem", "Licht", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}