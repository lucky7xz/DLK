{"dta.poem.23561": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Weltgericht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Weltgeschichte ist das Weltgericht,", "tokens": ["Die", "Welt\u00b7ge\u00b7schich\u00b7te", "ist", "das", "Welt\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch kein Gericht f\u00fcr jeden Magen,", "tokens": ["Doch", "kein", "Ge\u00b7richt", "f\u00fcr", "je\u00b7den", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn solche derbe Speise w\u00fcrde nicht", "tokens": ["Denn", "sol\u00b7che", "der\u00b7be", "Spei\u00b7se", "w\u00fcr\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein jeder Herr und Knecht vertragen.", "tokens": ["Ein", "je\u00b7der", "Herr", "und", "Knecht", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum hat man viele M\u00e4nner angestellt,", "tokens": ["Drum", "hat", "man", "vie\u00b7le", "M\u00e4n\u00b7ner", "an\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die m\u00fcssen's klopfen, kochen, braten,", "tokens": ["Die", "m\u00fcs\u00b7sen's", "klop\u00b7fen", ",", "ko\u00b7chen", ",", "bra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dies Gericht der ganzen Welt gef\u00e4llt,", "tokens": ["Da\u00df", "dies", "Ge\u00b7richt", "der", "gan\u00b7zen", "Welt", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zumal den hohen Potentaten.", "tokens": ["Zu\u00b7mal", "den", "ho\u00b7hen", "Po\u00b7ten\u00b7ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu haben ist es dann an jedem Ort,", "tokens": ["Zu", "ha\u00b7ben", "ist", "es", "dann", "an", "je\u00b7dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VAINF", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Geld bekommt es leicht ein Jeder;", "tokens": ["F\u00fcr", "Geld", "be\u00b7kommt", "es", "leicht", "ein", "Je\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "ART", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einer Br\u00fche giebt man's ", "tokens": ["Mit", "ei\u00b7ner", "Br\u00fc\u00b7he", "giebt", "man's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sogar auch wohl noch vom Katheder.", "tokens": ["So\u00b7gar", "auch", "wohl", "noch", "vom", "Ka\u00b7the\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es ist bereitet dann so excellent,", "tokens": ["Es", "ist", "be\u00b7rei\u00b7tet", "dann", "so", "ex\u00b7cel\u00b7lent", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df man die Finger danach lecket;", "tokens": ["Da\u00df", "man", "die", "Fin\u00b7ger", "da\u00b7nach", "le\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ges\u00e4ttigt rufen wir: potz Element!", "tokens": ["Ge\u00b7s\u00e4t\u00b7tigt", "ru\u00b7fen", "wir", ":", "potz", "E\u00b7le\u00b7ment", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$.", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie gut doch die Geschichte schmecket!", "tokens": ["Wie", "gut", "doch", "die", "Ge\u00b7schich\u00b7te", "schme\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}