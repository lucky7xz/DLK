{"textgrid.poem.62450": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! es ist ein Mann gesunken,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! es ist ein Mann gesunken,", "tokens": ["Ach", "!", "es", "ist", "ein", "Mann", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer aus der Treuen Schaar,", "tokens": ["Ei\u00b7ner", "aus", "der", "Treu\u00b7en", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den mit hellen Himmelsfunken", "tokens": ["Den", "mit", "hel\u00b7len", "Him\u00b7mels\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00fcngst entz\u00fcndet dieses Jahr.", "tokens": ["J\u00fcngst", "ent\u00b7z\u00fcn\u00b7det", "die\u00b7ses", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Held auf seinem Schilde", "tokens": ["Wie", "ein", "Held", "auf", "sei\u00b7nem", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt er hier an Leipzigs Thor", "tokens": ["Liegt", "er", "hier", "an", "Leip\u00b7zigs", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem deutschen Lustgefilde,", "tokens": ["Auf", "dem", "deut\u00b7schen", "Lust\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das zur Wahlstatt Gott erkor.", "tokens": ["Das", "zur", "Wahl\u00b7statt", "Gott", "er\u00b7kor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sollen wir so bald dich missen?", "tokens": ["Sol\u00b7len", "wir", "so", "bald", "dich", "mis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hauptmann, deine Compagnie", "tokens": ["Haupt\u00b7mann", ",", "dei\u00b7ne", "Com\u00b7pag\u00b7nie"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will von keinem Andern wissen,", "tokens": ["Will", "von", "kei\u00b7nem", "An\u00b7dern", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vergi\u00dft dich nun und nie.", "tokens": ["Und", "ver\u00b7gi\u00dft", "dich", "nun", "und", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Vaterhaus und Vatersitte", "tokens": ["Va\u00b7ter\u00b7haus", "und", "Va\u00b7ter\u00b7sit\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Freiheit war dir werth,", "tokens": ["Und", "die", "Frei\u00b7heit", "war", "dir", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also hat ein freier Britte,", "tokens": ["Al\u00b7so", "hat", "ein", "frei\u00b7er", "Brit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat dein Vater dich gelehrt.", "tokens": ["Hat", "dein", "Va\u00b7ter", "dich", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Kraft war dir gewachsen", "tokens": ["Und", "die", "Kraft", "war", "dir", "ge\u00b7wach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "In der Freiheit Morgenroth,", "tokens": ["In", "der", "Frei\u00b7heit", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem sch\u00f6nen Lande Sachsen", "tokens": ["In", "dem", "sch\u00f6\u00b7nen", "Lan\u00b7de", "Sach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lohnte dich der Freiheitstod.", "tokens": ["Lohn\u00b7te", "dich", "der", "Frei\u00b7heits\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wandeln wird die Heldenkunde", "tokens": ["Wan\u00b7deln", "wird", "die", "Hel\u00b7den\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der m\u00fctterlichen Stadt,", "tokens": ["Nach", "der", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mit Gott und Recht im Bunde", "tokens": ["Die", "mit", "Gott", "und", "Recht", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Schaar ger\u00fcstet hat.", "tokens": ["Uns\u00b7re", "Schaar", "ge\u00b7r\u00fcs\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier im deutschen Boden senken", "tokens": ["Hier", "im", "deut\u00b7schen", "Bo\u00b7den", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neben ", "tokens": ["Ne\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "M\u00f6chte Gott uns Allen schenken,", "tokens": ["M\u00f6ch\u00b7te", "Gott", "uns", "Al\u00b7len", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Todes werth zu sein.", "tokens": ["Dei\u00b7nes", "To\u00b7des", "werth", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}