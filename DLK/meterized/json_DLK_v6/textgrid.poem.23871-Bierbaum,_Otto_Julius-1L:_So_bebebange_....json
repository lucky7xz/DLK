{"textgrid.poem.23871": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: So bebebange ...", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bebebange ...", "tokens": ["So", "be\u00b7be\u00b7ban\u00b7ge", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die schwarze Nacht", "tokens": ["Die", "schwar\u00b7ze", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hat mit hohem Gew\u00f6lbe die Welt \u00fcberdacht.", "tokens": ["Hat", "mit", "ho\u00b7hem", "Ge\u00b7w\u00f6l\u00b7be", "die", "Welt", "\u00fc\u00b7berd\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Willst schlafen und tr\u00e4umen?", "tokens": ["Willst", "schla\u00b7fen", "und", "tr\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es geht nicht an.", "tokens": ["Es", "geht", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dich knebelt und knechtet ein dumpfer Bann.", "tokens": ["Dich", "kne\u00b7belt", "und", "knech\u00b7tet", "ein", "dum\u00b7pfer", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Lieg stille und lausche", "tokens": ["Lieg", "stil\u00b7le", "und", "lau\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im schweigenden Raum,", "tokens": ["Im", "schwei\u00b7gen\u00b7den", "Raum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dich umschleiert kein Schlaf, dich tr\u00f6stet kein Traum.", "tokens": ["Dich", "um\u00b7schlei\u00b7ert", "kein", "Schlaf", ",", "dich", "tr\u00f6s\u00b7tet", "kein", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Gedulde und warte:", "tokens": ["Ge\u00b7dul\u00b7de", "und", "war\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es wird schon Licht,", "tokens": ["Es", "wird", "schon", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und es hebt sich das schwere, das schwarze Gewicht.", "tokens": ["Und", "es", "hebt", "sich", "das", "schwe\u00b7re", ",", "das", "schwar\u00b7ze", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}