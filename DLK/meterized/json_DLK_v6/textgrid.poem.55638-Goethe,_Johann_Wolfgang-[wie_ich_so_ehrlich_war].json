{"textgrid.poem.55638": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[wie ich so ehrlich war]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich so ehrlich war,", "tokens": ["Wie", "ich", "so", "ehr\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hab ich gefehlt", "tokens": ["Hab", "ich", "ge\u00b7fehlt"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVPP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und habe jahrelang", "tokens": ["Und", "ha\u00b7be", "jah\u00b7re\u00b7lang"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich durchgequ\u00e4lt;", "tokens": ["Mich", "durch\u00b7ge\u00b7qu\u00e4lt", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich galt und galt auch nicht,", "tokens": ["Ich", "galt", "und", "galt", "auch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was sollt es hei\u00dfen?", "tokens": ["Was", "sollt", "es", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Nun wollt ich Schelm sein,", "tokens": ["Nun", "wollt", "ich", "Schelm", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.8": {"text": "T\u00e4t mich beflei\u00dfen;", "tokens": ["T\u00e4t", "mich", "be\u00b7flei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Das wollt mir gar nicht ein,", "tokens": ["Das", "wollt", "mir", "gar", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mu\u00dft mich zerrei\u00dfen.", "tokens": ["Mu\u00dft", "mich", "zer\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Da dacht ich: ehrlich sein", "tokens": ["Da", "dacht", "ich", ":", "ehr\u00b7lich", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ist doch das Beste,", "tokens": ["Ist", "doch", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "War es nur k\u00fcmmerlich,", "tokens": ["War", "es", "nur", "k\u00fcm\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "So steht es feste.", "tokens": ["So", "steht", "es", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}