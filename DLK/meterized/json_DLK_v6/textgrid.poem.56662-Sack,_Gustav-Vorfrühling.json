{"textgrid.poem.56662": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Vorfr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, die den schw\u00fclen Fr\u00fchlingstag", "tokens": ["Die", "Son\u00b7ne", ",", "die", "den", "schw\u00fc\u00b7len", "Fr\u00fch\u00b7lings\u00b7tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "t\u00fcckisch in ihren Wolken lag,", "tokens": ["t\u00fc\u00b7ckisch", "in", "ih\u00b7ren", "Wol\u00b7ken", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "zog blitzend ihren Degen", "tokens": ["zog", "blit\u00b7zend", "ih\u00b7ren", "De\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und stie\u00df in j\u00e4her Wut", "tokens": ["und", "stie\u00df", "in", "j\u00e4\u00b7her", "Wut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "ihn in des Himmels Purpurblut \u2013", "tokens": ["ihn", "in", "des", "Him\u00b7mels", "Pur\u00b7pur\u00b7blut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und ohne Ende str\u00f6mt der Regen!", "tokens": ["und", "oh\u00b7ne", "En\u00b7de", "str\u00f6mt", "der", "Re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der \u2013 sp\u00fclt mich fort; und meine Seele flie\u00dft", "tokens": ["Der", "\u2013", "sp\u00fclt", "mich", "fort", ";", "und", "mei\u00b7ne", "See\u00b7le", "flie\u00dft"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$(", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und flie\u00dft und schwimmt, ein tr\u00e4ges Boot,", "tokens": ["und", "flie\u00dft", "und", "schwimmt", ",", "ein", "tr\u00e4\u00b7ges", "Boot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das \u00fcberladen gleich zur Tiefe schie\u00dft,", "tokens": ["das", "\u00fc\u00b7berl\u00b7a\u00b7den", "gleich", "zur", "Tie\u00b7fe", "schie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in Nacht und Tod.", "tokens": ["in", "Nacht", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "All meine Jahre lastete ich ein", "tokens": ["All", "mei\u00b7ne", "Jah\u00b7re", "las\u00b7te\u00b7te", "ich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und alles, was ich von mir selber litt,", "tokens": ["und", "al\u00b7les", ",", "was", "ich", "von", "mir", "sel\u00b7ber", "litt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mein Hoffen, mein Verachten gab ich mit", "tokens": ["mein", "Hof\u00b7fen", ",", "mein", "Ver\u00b7ach\u00b7ten", "gab", "ich", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und meinen Hohn noch obendrein.", "tokens": ["und", "mei\u00b7nen", "Hohn", "noch", "o\u00b7ben\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sink! sink, mein Kahn!", "tokens": ["Sink", "!", "sink", ",", "mein", "Kahn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Denn Tag ist Tag und Nacht ist Nacht", "tokens": ["Denn", "Tag", "ist", "Tag", "und", "Nacht", "ist", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "NN", "KON", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und was dir Tag und Nacht gebracht \u2013:", "tokens": ["und", "was", "dir", "Tag", "und", "Nacht", "ge\u00b7bracht", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "KON", "NN", "VVPP", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sink! sink, mein Kahn!", "tokens": ["Sink", "!", "sink", ",", "mein", "Kahn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}