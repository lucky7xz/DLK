{"dta.poem.5509": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nacht-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bet', in diesem Heer der Sternen,", "tokens": ["Ich", "bet'", ",", "in", "die\u00b7sem", "Heer", "der", "Ster\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, HErr der Sternen, innig an!", "tokens": ["Dich", ",", "Herr", "der", "Ster\u00b7nen", ",", "in\u00b7nig", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil man, in Nichts so kl\u00e4rlich lernen,", "tokens": ["Weil", "man", ",", "in", "Nichts", "so", "kl\u00e4r\u00b7lich", "ler\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "APPR", "PIS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Nichts so deutlich finden kann,", "tokens": ["In", "Nichts", "so", "deut\u00b7lich", "fin\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie unbegreiflich, herrlich, m\u00e4chtig,", "tokens": ["Wie", "un\u00b7be\u00b7greif\u00b7lich", ",", "herr\u00b7lich", ",", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erhaben, Majest\u00e4tisch, pr\u00e4chtig,", "tokens": ["Er\u00b7ha\u00b7ben", ",", "Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", ",", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dein all-erschaffend ewigs Wesen.", "tokens": ["Dein", "all\u00b7er\u00b7schaf\u00b7fend", "e\u00b7wigs", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von deiner GOttheit Tieff\u2019 und H\u00f6h", "tokens": ["Von", "dei\u00b7ner", "Got\u00b7theit", "Tief\u00b7f'", "und", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Giebt uns aufs Ueberzeuglichste", "tokens": ["Giebt", "uns", "aufs", "Ue\u00b7ber\u00b7zeug\u00b7lichs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "++-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Das grosse Sternen A. B. C.", "tokens": ["Das", "gros\u00b7se", "Ster\u00b7nen", "A.", "B.", "C."], "token_info": ["word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Die Unbegreiflichkeit zu lesen.", "tokens": ["Die", "Un\u00b7be\u00b7greif\u00b7lich\u00b7keit", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}