{"dta.poem.16003": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus vier Grundstoffen ist gemischt die K\u00f6rperwelt,", "tokens": ["Aus", "vier", "Grund\u00b7stof\u00b7fen", "ist", "ge\u00b7mischt", "die", "K\u00f6r\u00b7per\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die als Grundstimmungen dein Innres auch enth\u00e4lt.", "tokens": ["Die", "als", "Grunds\u00b7tim\u00b7mun\u00b7gen", "dein", "Inn\u00b7res", "auch", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Zorn ist eine Glut, dem hei\u00dfen Feuer gleich,", "tokens": ["Der", "Zorn", "ist", "ei\u00b7ne", "Glut", ",", "dem", "hei\u00b7\u00dfen", "Feu\u00b7er", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Traurigkeit wie Flut des Wassers feucht und weich.", "tokens": ["Die", "Trau\u00b7rig\u00b7keit", "wie", "Flut", "des", "Was\u00b7sers", "feucht", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Lust ist wie die Luft, leicht, licht und wandelreich,", "tokens": ["Die", "Lust", "ist", "wie", "die", "Luft", ",", "leicht", ",", "licht", "und", "wan\u00b7del\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "ADJD", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Furcht wie Erdengruft, schwer, dumpf und todtenbleich.", "tokens": ["Die", "Furcht", "wie", "Er\u00b7den\u00b7gruft", ",", "schwer", ",", "dumpf", "und", "tod\u00b7ten\u00b7bleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00df deines Zornes Glut nie werden wilde Wuth;", "tokens": ["La\u00df", "dei\u00b7nes", "Zor\u00b7nes", "Glut", "nie", "wer\u00b7den", "wil\u00b7de", "Wuth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sei ein steter Muth im Kampf f\u00fcrs h\u00f6chste Gut.", "tokens": ["Sie", "sei", "ein", "ste\u00b7ter", "Muth", "im", "Kampf", "f\u00fcrs", "h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den Glutmuth d\u00e4mpfe dir die Traurigkeit zur Demuth;", "tokens": ["Den", "Glut\u00b7mu\u00b7th", "d\u00e4mp\u00b7fe", "dir", "die", "Trau\u00b7rig\u00b7keit", "zur", "De\u00b7muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schwimm, und verschwimm nur nicht, in Sehnsucht und in Wehmuth;", "tokens": ["Schwimm", ",", "und", "ver\u00b7schwimm", "nur", "nicht", ",", "in", "Sehn\u00b7sucht", "und", "in", "Weh\u00b7muth", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVIMP", "ADV", "PTKNEG", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Im Weh' ist eine Wonn', und in der Lust ein Leid;", "tokens": ["Im", "Weh'", "ist", "ei\u00b7ne", "Wonn'", ",", "und", "in", "der", "Lust", "ein", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die reinste Lebenslust ist Liebe ohne Neid.", "tokens": ["Die", "reins\u00b7te", "Le\u00b7bens\u00b7lust", "ist", "Lie\u00b7be", "oh\u00b7ne", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Aus Furcht kommt Neid und Geiz und aller Selbstsucht Pein;", "tokens": ["Aus", "Furcht", "kommt", "Neid", "und", "Geiz", "und", "al\u00b7ler", "Selbst\u00b7sucht", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "KON", "NN", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deinem Herzen sei nur Gottesfurcht allein.", "tokens": ["In", "dei\u00b7nem", "Her\u00b7zen", "sei", "nur", "Got\u00b7tes\u00b7furcht", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}