{"textgrid.poem.55192": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An Uranius", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbhimmel, ach!\u00ab so ruft man aus,", "tokens": ["\u00bb", "him\u00b7mel", ",", "ach", "!", "\u00ab", "so", "ruft", "man", "aus", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "ITJ", "$.", "$(", "ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn's uns schlecht geworden.", "tokens": ["Wenn's", "uns", "schlecht", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Himmel will verdienen sich", "tokens": ["Him\u00b7mel", "will", "ver\u00b7die\u00b7nen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pfaff und Ritterorden.", "tokens": ["Pfaff", "und", "Rit\u00b7ter\u00b7or\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ihren Himmel finden viel'", "tokens": ["Ih\u00b7ren", "Him\u00b7mel", "fin\u00b7den", "viel'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Weltget\u00fcmmel;", "tokens": ["In", "dem", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jugend unter Tanz und Spiel", "tokens": ["Ju\u00b7gend", "un\u00b7ter", "Tanz", "und", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meint, sie sei im Himmel.", "tokens": ["Meint", ",", "sie", "sei", "im", "Him\u00b7mel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Doch von dem Klaviere t\u00f6nt", "tokens": ["Doch", "von", "dem", "Kla\u00b7vie\u00b7re", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ganz ein andrer Himmel;", "tokens": ["Ganz", "ein", "an\u00b7drer", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Morgen gr\u00fc\u00df ich ihn,", "tokens": ["Al\u00b7le", "Mor\u00b7gen", "gr\u00fc\u00df", "ich", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nickt er mir vom Schimmel.", "tokens": ["Nickt", "er", "mir", "vom", "Schim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}