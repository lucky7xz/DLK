{"textgrid.poem.61087": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verhalle nun, Gesang der Liebesklagen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verhalle nun, Gesang der Liebesklagen,", "tokens": ["Ver\u00b7hal\u00b7le", "nun", ",", "Ge\u00b7sang", "der", "Lie\u00b7bes\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du langes, banges Echo meiner Leiden!", "tokens": ["Du", "lan\u00b7ges", ",", "ban\u00b7ges", "E\u00b7cho", "mei\u00b7ner", "Lei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Tag erscheint, die tr\u00fcbe Nacht mu\u00df scheiden,", "tokens": ["Der", "Tag", "er\u00b7scheint", ",", "die", "tr\u00fc\u00b7be", "Nacht", "mu\u00df", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Stunde der Erl\u00f6sung hat geschlagen.", "tokens": ["Die", "Stun\u00b7de", "der", "Er\u00b7l\u00f6\u00b7sung", "hat", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht l\u00e4nger sollt ihr Trauerfarben tragen,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "sollt", "ihr", "Trau\u00b7er\u00b7far\u00b7ben", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr meine Lieder! Nein, in bunte Seiden,", "tokens": ["Ihr", "mei\u00b7ne", "Lie\u00b7der", "!", "Nein", ",", "in", "bun\u00b7te", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "PTKANT", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Gold und Purpur will ich nun euch kleiden", "tokens": ["In", "Gold", "und", "Pur\u00b7pur", "will", "ich", "nun", "euch", "klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu w\u00fcrd'ger Feier diesen Jubeltagen.", "tokens": ["Zu", "w\u00fcrd'\u00b7ger", "Fei\u00b7er", "die\u00b7sen", "Ju\u00b7bel\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf denn! Im Festgewand den Tanz zu schlingen,", "tokens": ["Auf", "denn", "!", "Im", "Fest\u00b7ge\u00b7wand", "den", "Tanz", "zu", "schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kr\u00e4nzt euch mit Blumen, z\u00fcndet lust'ge Kerzen!", "tokens": ["Kr\u00e4nzt", "euch", "mit", "Blu\u00b7men", ",", "z\u00fcn\u00b7det", "lust'\u00b7ge", "Ker\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die vollsten eurer T\u00f6ne la\u00dft erklingen!", "tokens": ["Die", "volls\u00b7ten", "eu\u00b7rer", "T\u00f6\u00b7ne", "la\u00dft", "er\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun gilt es, leicht in holder Form zu scherzen;", "tokens": ["Nun", "gilt", "es", ",", "leicht", "in", "hol\u00b7der", "Form", "zu", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn Fr\u00fchling kam auf Regenbogenschwingen,", "tokens": ["Denn", "Fr\u00fch\u00b7ling", "kam", "auf", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Fr\u00fchling bl\u00fcht und leuchtet mir im Herzen.", "tokens": ["Und", "Fr\u00fch\u00b7ling", "bl\u00fcht", "und", "leuch\u00b7tet", "mir", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}