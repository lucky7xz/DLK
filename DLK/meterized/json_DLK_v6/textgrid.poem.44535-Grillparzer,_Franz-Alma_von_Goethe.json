{"textgrid.poem.44535": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Alma von Goethe", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hast du nicht gedacht, Gewaltger du,", "tokens": ["Das", "hast", "du", "nicht", "ge\u00b7dacht", ",", "Ge\u00b7walt\u00b7ger", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als du noch weiltest in der Menschheit Schlacken,", "tokens": ["Als", "du", "noch", "weil\u00b7test", "in", "der", "Menschheit", "Schla\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df einst dein Enkelkind fr\u00fchzeitge Ruh", "tokens": ["Da\u00df", "einst", "dein", "En\u00b7kel\u00b7kind", "fr\u00fch\u00b7zeit\u00b7ge", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll finden in dem \u00bbLande der Ph\u00e4aken\u00ab.", "tokens": ["Soll", "fin\u00b7den", "in", "dem", "\u00bb", "Lan\u00b7de", "der", "Ph\u00e4\u00b7a\u00b7ken", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "$(", "NN", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und da\u00df der Mann, der sch\u00fcchtern vor dir stand,", "tokens": ["Und", "da\u00df", "der", "Mann", ",", "der", "sch\u00fcch\u00b7tern", "vor", "dir", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Blick gesenkt vorm hehren Strahl des deinen,", "tokens": ["Den", "Blick", "ge\u00b7senkt", "vorm", "heh\u00b7ren", "Strahl", "des", "dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am fabelgleichen fernen Isterstrand", "tokens": ["Am", "fa\u00b7bel\u00b7glei\u00b7chen", "fer\u00b7nen", "Is\u00b7ter\u00b7strand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bei ihrem offnen Grabe werde weinen.", "tokens": ["Bei", "ih\u00b7rem", "off\u00b7nen", "Gra\u00b7be", "wer\u00b7de", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es kommt so manches anders, als man meint,", "tokens": ["Es", "kommt", "so", "man\u00b7ches", "an\u00b7ders", ",", "als", "man", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ist gekommen, warst du gleich der Weise.", "tokens": ["Und", "ist", "ge\u00b7kom\u00b7men", ",", "warst", "du", "gleich", "der", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne, wenn sie hoch im Mittag scheint,", "tokens": ["Die", "Son\u00b7ne", ",", "wenn", "sie", "hoch", "im", "Mit\u00b7tag", "scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Senkt schon zum Untergang sich m\u00e4hlich leise.", "tokens": ["Senkt", "schon", "zum", "Un\u00b7ter\u00b7gang", "sich", "m\u00e4h\u00b7lich", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PRF", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nach neuen Zonen wendet sich der Geist", "tokens": ["Nach", "neu\u00b7en", "Zo\u00b7nen", "wen\u00b7det", "sich", "der", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4\u00dft, was blank, in grauen Dunkel rosten,", "tokens": ["Und", "l\u00e4\u00dft", ",", "was", "blank", ",", "in", "grau\u00b7en", "Dun\u00b7kel", "ros\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PTKVZ", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist doch, was uns der ferne Westen hei\u00dft,", "tokens": ["Ist", "doch", ",", "was", "uns", "der", "fer\u00b7ne", "Wes\u00b7ten", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr andre V\u00f6lker auch zugleich ein Osten.", "tokens": ["F\u00fcr", "and\u00b7re", "V\u00f6l\u00b7ker", "auch", "zu\u00b7gleich", "ein", "Os\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So drang dein Wort, so kam dein Enkelkind", "tokens": ["So", "drang", "dein", "Wort", ",", "so", "kam", "dein", "En\u00b7kel\u00b7kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In unsre Morgenrot-bestrahlte Fluren;", "tokens": ["In", "uns\u00b7re", "Mor\u00b7gen\u00b7rot\u00b7be\u00b7strahl\u00b7te", "Flu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hoch schlug mein Herz, versch\u00f6nt, wie Weiber sind,", "tokens": ["Hoch", "schlug", "mein", "Herz", ",", "ver\u00b7sch\u00f6nt", ",", "wie", "Wei\u00b7ber", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PWAV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihr zu finden deiner Z\u00fcge Spuren.", "tokens": ["In", "ihr", "zu", "fin\u00b7den", "dei\u00b7ner", "Z\u00fc\u00b7ge", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und so trat ich, zu huldgen, in den Saal,", "tokens": ["Und", "so", "trat", "ich", ",", "zu", "huld\u00b7gen", ",", "in", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo schon das Teeger\u00e4t die Tische kr\u00f6nte,", "tokens": ["Wo", "schon", "das", "Tee\u00b7ge\u00b7r\u00e4t", "die", "Ti\u00b7sche", "kr\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Frau begr\u00fc\u00dfend, deines Sohnes Wahl,", "tokens": ["Die", "Frau", "be\u00b7gr\u00fc\u00b7\u00dfend", ",", "dei\u00b7nes", "Soh\u00b7nes", "Wahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die dir des Lebens Abendrot versch\u00f6nte.", "tokens": ["Die", "dir", "des", "Le\u00b7bens", "A\u00b7ben\u00b7drot", "ver\u00b7sch\u00f6n\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch war kein weiblich Wesen sonst im Kreis,", "tokens": ["Doch", "war", "kein", "weib\u00b7lich", "We\u00b7sen", "sonst", "im", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "ADJD", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur Herren, schwarz, als w\u00e4r ein Sarg zur Stelle.", "tokens": ["Nur", "Her\u00b7ren", ",", "schwarz", ",", "als", "w\u00e4r", "ein", "Sarg", "zur", "Stel\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJD", "$,", "KOKOM", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da \u00f6ffnet sich die T\u00fcr, und hell und wei\u00df", "tokens": ["Da", "\u00f6ff\u00b7net", "sich", "die", "T\u00fcr", ",", "und", "hell", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tritt kinderhaft das M\u00e4dchen auf die Schwelle.", "tokens": ["Tritt", "kin\u00b7der\u00b7haft", "das", "M\u00e4d\u00b7chen", "auf", "die", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die ich gedacht mir in der Hoheit Schein,", "tokens": ["Die", "ich", "ge\u00b7dacht", "mir", "in", "der", "Ho\u00b7heit", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von angestammter Herrlichkeit ergl\u00e4nzend,", "tokens": ["Von", "an\u00b7ge\u00b7stamm\u00b7ter", "Herr\u00b7lich\u00b7keit", "er\u00b7gl\u00e4n\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Teebrett in den H\u00e4nden, trat sie ein,", "tokens": ["Ein", "Tee\u00b7brett", "in", "den", "H\u00e4n\u00b7den", ",", "trat", "sie", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem\u00fctig Brot zum hei\u00dfen Trank kredenzend.", "tokens": ["De\u00b7m\u00fc\u00b7tig", "Brot", "zum", "hei\u00b7\u00dfen", "Trank", "kre\u00b7den\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch wars, als ob, dem Erlenk\u00f6nig gleich,", "tokens": ["Doch", "wars", ",", "als", "ob", ",", "dem", "Er\u00b7len\u00b7k\u00f6\u00b7nig", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOKOM", "KOUS", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Ahnherrn Geist ob ihrem Scheitel schwebte,", "tokens": ["Des", "Ahn\u00b7herrn", "Geist", "ob", "ih\u00b7rem", "Schei\u00b7tel", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sie, das Kind, dem Kind im Liede gleich,", "tokens": ["Und", "sie", ",", "das", "Kind", ",", "dem", "Kind", "im", "Lie\u00b7de", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vorm Anhauch einer geistgen Ladung bebte.", "tokens": ["Vorm", "An\u00b7hauch", "ei\u00b7ner", "geist\u00b7gen", "La\u00b7dung", "beb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie an dem Eichstamm, den der Blitz geneigt,", "tokens": ["Wie", "an", "dem", "Eichs\u00b7tamm", ",", "den", "der", "Blitz", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Blume hell empor die Bl\u00e4tter richtet,", "tokens": ["Die", "Blu\u00b7me", "hell", "em\u00b7por", "die", "Bl\u00e4t\u00b7ter", "rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als ob nicht dein Erzeugter sie erzeugt,", "tokens": ["Als", "ob", "nicht", "dein", "Er\u00b7zeug\u00b7ter", "sie", "er\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PTKNEG", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob ihr Ahn sie Kl\u00e4rchen-gleich gedichtet.", "tokens": ["Als", "ob", "ihr", "Ahn", "sie", "Kl\u00e4r\u00b7chen\u00b7gleich", "ge\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "PPER", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Sie f\u00fchlte wohl den Wink der fernen Hand,", "tokens": ["Sie", "f\u00fchl\u00b7te", "wohl", "den", "Wink", "der", "fer\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Sehnsucht nach dem Land der reinen Lilien,", "tokens": ["Die", "Sehn\u00b7sucht", "nach", "dem", "Land", "der", "rei\u00b7nen", "Li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ging dahin, so stamm- als wahlverwandt,", "tokens": ["Und", "ging", "da\u00b7hin", ",", "so", "stam\u00b7m", "als", "wahl\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "ADV", "TRUNC", "KOKOM", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Verwaisend und verdoppelnd die Ottilien.", "tokens": ["Ver\u00b7wai\u00b7send", "und", "ver\u00b7dop\u00b7pelnd", "die", "Ot\u00b7ti\u00b7li\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "ART", "NE", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Du aber schaust mit ernstem Blick herab,", "tokens": ["Du", "a\u00b7ber", "schaust", "mit", "erns\u00b7tem", "Blick", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo sie der Grund, Beethoven nah, verschlungen,", "tokens": ["Wo", "sie", "der", "Grund", ",", "Beet\u00b7ho\u00b7ven", "nah", ",", "ver\u00b7sc