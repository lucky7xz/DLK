{"dta.poem.879": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "DiE JUNGE MAGD", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Oft am Brunnen, wenn es d\u00e4mmert,", "tokens": ["Oft", "am", "Brun\u00b7nen", ",", "wenn", "es", "d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht man sie verzaubert stehen", "tokens": ["Sieht", "man", "sie", "ver\u00b7zau\u00b7bert", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wasser sch\u00f6pfen, wenn es d\u00e4mmert.", "tokens": ["Was\u00b7ser", "sch\u00f6p\u00b7fen", ",", "wenn", "es", "d\u00e4m\u00b7mert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eimer auf und niedergehen.", "tokens": ["Ei\u00b7mer", "auf", "und", "nie\u00b7der\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den Buchen Dohlen flattern", "tokens": ["In", "den", "Bu\u00b7chen", "Doh\u00b7len", "flat\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie gleichet einem Schatten.", "tokens": ["Und", "sie", "glei\u00b7chet", "ei\u00b7nem", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre gelben Haare flattern", "tokens": ["Ih\u00b7re", "gel\u00b7ben", "Haa\u00b7re", "flat\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Hofe schrein die Ratten.", "tokens": ["Und", "im", "Ho\u00b7fe", "schrein", "die", "Rat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und umschmeichelt von Verfalle", "tokens": ["Und", "um\u00b7schmei\u00b7chelt", "von", "Ver\u00b7fal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senkt sie die entzundenen Lider.", "tokens": ["Senkt", "sie", "die", "ent\u00b7zun\u00b7de\u00b7nen", "Li\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "D\u00fcrres Gras neigt im Verfalle", "tokens": ["D\u00fcr\u00b7res", "Gras", "neigt", "im", "Ver\u00b7fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zu ihren F\u00fc\u00dfen nieder.", "tokens": ["Sich", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stille schafft sie in der Kammer", "tokens": ["Stil\u00b7le", "schafft", "sie", "in", "der", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Hof liegt l\u00e4ngst ver\u00f6det.", "tokens": ["Und", "der", "Hof", "liegt", "l\u00e4ngst", "ver\u00b7\u00f6\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Hollunder vor der Kammer", "tokens": ["Im", "Hol\u00b7lun\u00b7der", "vor", "der", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kl\u00e4glich eine Amsel fl\u00f6tet.", "tokens": ["Kl\u00e4g\u00b7lich", "ei\u00b7ne", "Am\u00b7sel", "fl\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Silbern schaut ihr Bild im Spiegel", "tokens": ["Sil\u00b7bern", "schaut", "ihr", "Bild", "im", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fremd sie an im Zwielichtscheine", "tokens": ["Fremd", "sie", "an", "im", "Zwie\u00b7licht\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verd\u00e4mmert fahl im Spiegel", "tokens": ["Und", "ver\u00b7d\u00e4m\u00b7mert", "fahl", "im", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr graut vor seiner Reine.", "tokens": ["Und", "ihr", "graut", "vor", "sei\u00b7ner", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Traumhaft singt ein Knecht im Dunkel", "tokens": ["Traum\u00b7haft", "singt", "ein", "Knecht", "im", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie starrt von Schmerz gesch\u00fcttelt.", "tokens": ["Und", "sie", "starrt", "von", "Schmerz", "ge\u00b7sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00f6te tr\u00e4ufelt durch das Dunkel.", "tokens": ["R\u00f6\u00b7te", "tr\u00e4u\u00b7felt", "durch", "das", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00e4h am Tor der S\u00fcdwind r\u00fcttelt.", "tokens": ["J\u00e4h", "am", "Tor", "der", "S\u00fcd\u00b7wind", "r\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "N\u00e4chtens \u00fcbern kahlen Anger", "tokens": ["N\u00e4ch\u00b7tens", "\u00fc\u00b7bern", "kah\u00b7len", "An\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gaukelt sie in Fiebertr\u00e4umen.", "tokens": ["Gau\u00b7kelt", "sie", "in", "Fie\u00b7ber\u00b7tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcrrisch greint der Wind im Anger", "tokens": ["M\u00fcr\u00b7risch", "greint", "der", "Wind", "im", "An\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Mond lauscht aus den B\u00e4umen.", "tokens": ["Und", "der", "Mond", "lauscht", "aus", "den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+++-+-", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Balde rings die Sterne bleichen", "tokens": ["Bal\u00b7de", "rings", "die", "Ster\u00b7ne", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ermattet von Beschwerde", "tokens": ["Und", "er\u00b7mat\u00b7tet", "von", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4chsern ihre Wangen bleichen.", "tokens": ["W\u00e4ch\u00b7sern", "ih\u00b7re", "Wan\u00b7gen", "blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4ulnis wittert aus der Erde.", "tokens": ["F\u00e4ul\u00b7nis", "wit\u00b7tert", "aus", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Traurig rauscht das Rohr im T\u00fcmpel", "tokens": ["Trau\u00b7rig", "rauscht", "das", "Rohr", "im", "T\u00fcm\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie friert in sich gekauert.", "tokens": ["Und", "sie", "friert", "in", "sich", "ge\u00b7kau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern ein Hahn kr\u00e4ht. \u00dcbern T\u00fcmpel", "tokens": ["Fern", "ein", "Hahn", "kr\u00e4ht", ".", "\u00dc\u00b7bern", "T\u00fcm\u00b7pel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "$.", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hart und grau der Morgen schauert.", "tokens": ["Hart", "und", "grau", "der", "Mor\u00b7gen", "schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In der Schmiede dr\u00f6hnt der Hammer", "tokens": ["In", "der", "Schmie\u00b7de", "dr\u00f6hnt", "der", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie huscht am Tor vor\u00fcber.", "tokens": ["Und", "sie", "huscht", "am", "Tor", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fchrot schwingt der Knecht den Hammer", "tokens": ["Gl\u00fch\u00b7rot", "schwingt", "der", "Knecht", "den", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie schaut wie tot hin\u00fcber.", "tokens": ["Und", "sie", "schaut", "wie", "tot", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ADJD", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie im Traum trifft sie ein Lachen;", "tokens": ["Wie", "im", "Traum", "trifft", "sie", "ein", "La\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und sie taumelt in die Schmiede,", "tokens": ["Und", "sie", "tau\u00b7melt", "in", "die", "Schmie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheu geduckt vor seinem Lachen,", "tokens": ["Scheu", "ge\u00b7duckt", "vor", "sei\u00b7nem", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Hammer hart und r\u00fcde.", "tokens": ["Wie", "der", "Ham\u00b7mer", "hart", "und", "r\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Hell verspr\u00fchn im Raum die Funken", "tokens": ["Hell", "ver\u00b7spr\u00fchn", "im", "Raum", "die", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit hilfloser Geberde", "tokens": ["Und", "mit", "hilf\u00b7lo\u00b7ser", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.3": {"text": "Hascht sie nach den wilden Funken", "tokens": ["Hascht", "sie", "nach", "den", "wil\u00b7den", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie st\u00fcrzt bet\u00e4ubt zur Erde.", "tokens": ["Und", "sie", "st\u00fcrzt", "be\u00b7t\u00e4ubt", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Schm\u00e4chtig hingestreckt im Bette", "tokens": ["Schm\u00e4ch\u00b7tig", "hin\u00b7ge\u00b7streckt", "im", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wacht sie auf voll s\u00fc\u00dfem Bangen", "tokens": ["Wacht", "sie", "auf", "voll", "s\u00fc\u00b7\u00dfem", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie sieht ihr sch