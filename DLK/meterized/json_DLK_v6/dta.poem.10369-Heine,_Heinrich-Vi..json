{"dta.poem.10369": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu gabst, als ich vor'm Jahr dich wiederblickte,", "tokens": ["\u201e", "du", "gabst", ",", "als", "ich", "vor'm", "Jahr", "dich", "wie\u00b7der\u00b7blick\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir keinen Ku\u00df in jener Willkommstund'.\u201c", "tokens": ["Mir", "kei\u00b7nen", "Ku\u00df", "in", "je\u00b7ner", "Will\u00b7komm\u00b7stund'", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PIAT", "NN", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sprach ich, und der Liebsten rother Mund", "tokens": ["So", "sprach", "ich", ",", "und", "der", "Liebs\u00b7ten", "ro\u00b7ther", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den sch\u00f6nsten Ku\u00df auf meine Lippen dr\u00fcckte.", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Ku\u00df", "auf", "mei\u00b7ne", "Lip\u00b7pen", "dr\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und l\u00e4chelnd s\u00fc\u00df ein Myrthenreis sie pfl\u00fcckte", "tokens": ["Und", "l\u00e4\u00b7chelnd", "s\u00fc\u00df", "ein", "Myr\u00b7then\u00b7reis", "sie", "pfl\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vom Myrthenstrauche, der am Fenster stund:", "tokens": ["Vom", "Myr\u00b7then\u00b7strau\u00b7che", ",", "der", "am", "Fens\u00b7ter", "stund", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201enimm hin, und pflanz' dies Reis in frischen Grund,", "tokens": ["\u201e", "nimm", "hin", ",", "und", "pflanz'", "dies", "Reis", "in", "fri\u00b7schen", "Grund", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "KON", "VVFIN", "PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und stell' ein Glas darauf,\u201c sprach sie und nickte. \u2014", "tokens": ["Und", "stell'", "ein", "Glas", "da\u00b7rauf", ",", "\u201c", "sprach", "sie", "und", "nick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "$,", "$(", "VVFIN", "PPER", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schon lang ist's her. Es starb das Reis im Topf'.", "tokens": ["Schon", "lang", "ist's", "her", ".", "Es", "starb", "das", "Reis", "im", "Topf'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sie selbst hab' ich seit Jahren nicht gesehn;", "tokens": ["Sie", "selbst", "hab'", "ich", "seit", "Jah\u00b7ren", "nicht", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch brennt der Ku\u00df mir immer noch im Kopf'.", "tokens": ["Doch", "brennt", "der", "Ku\u00df", "mir", "im\u00b7mer", "noch", "im", "Kopf'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und aus der Ferne trieb's mich j\u00fcngst zum Ort,", "tokens": ["Und", "aus", "der", "Fer\u00b7ne", "trie\u00b7b's", "mich", "j\u00fcngst", "zum", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wo Liebchen wohnt. Vor'm Hause blieb ich stehn", "tokens": ["Wo", "Lieb\u00b7chen", "wohnt", ".", "Vor'm", "Hau\u00b7se", "blieb", "ich", "stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$.", "APPRART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die ganze Nacht, ging erst am Morgen fort.", "tokens": ["Die", "gan\u00b7ze", "Nacht", ",", "ging", "erst", "am", "Mor\u00b7gen", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}