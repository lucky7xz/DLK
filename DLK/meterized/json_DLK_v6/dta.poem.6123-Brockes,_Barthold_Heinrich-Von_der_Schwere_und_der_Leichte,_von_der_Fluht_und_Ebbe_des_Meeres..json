{"dta.poem.6123": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es \u00fcbersteigt die Lufft durch seinen Schwall: Allein", "tokens": ["Es", "\u00fc\u00b7bers\u00b7teigt", "die", "Lufft", "durch", "sei\u00b7nen", "Schwall", ":", "Al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn unser Blick das weite Reich der Wellen", "tokens": ["Wenn", "un\u00b7ser", "Blick", "das", "wei\u00b7te", "Reich", "der", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Betrachtet; wird es uns gar leicht begreifflich seyn:", "tokens": ["Be\u00b7trach\u00b7tet", ";", "wird", "es", "uns", "gar", "leicht", "be\u00b7greif\u00b7flich", "seyn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "VAFIN", "PPER", "PRF", "ADV", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sonst gepresste Lufft erreg\u2019 ein solches Schwellen.", "tokens": ["Die", "sonst", "ge\u00b7press\u00b7te", "Lufft", "er\u00b7reg'", "ein", "sol\u00b7ches", "Schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bemerckt man, da\u00df die Ebb\u2019 und Fluht des Mondes Lauff", "tokens": ["Be\u00b7merckt", "man", ",", "da\u00df", "die", "Ebb'", "und", "Fluht", "des", "Mon\u00b7des", "Lauff"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "KON", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Best\u00e4ndig folge; h\u00f6rt bald die Bewundrung auf.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "fol\u00b7ge", ";", "h\u00f6rt", "bald", "die", "Be\u00b7wund\u00b7rung", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man nimmt an beyden fast denselben Gaug in Acht,", "tokens": ["Man", "nimmt", "an", "bey\u00b7den", "fast", "den\u00b7sel\u00b7ben", "Gaug", "in", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "ADV", "PDAT", "NN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So uns denn muthiger zur Untersuchung macht.", "tokens": ["So", "uns", "denn", "mut\u00b7hi\u00b7ger", "zur", "Un\u00b7ter\u00b7su\u00b7chung", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}