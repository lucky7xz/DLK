{"textgrid.poem.49397": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Frau und Katze", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie spielte mit ihrem K\u00e4tzchen", "tokens": ["Sie", "spiel\u00b7te", "mit", "ih\u00b7rem", "K\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und reizend waren zu schau'n", "tokens": ["Und", "rei\u00b7zend", "wa\u00b7ren", "zu", "schau'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die weissen H\u00e4nde und T\u00e4tzchen", "tokens": ["Die", "weis\u00b7sen", "H\u00e4n\u00b7de", "und", "T\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beim T\u00e4ndeln im Abendgrau'n.", "tokens": ["Beim", "T\u00e4n\u00b7deln", "im", "A\u00b7bend\u00b7grau'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "APPRART", "NN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Versteckt hielt voll lustiger M\u00e4tzchen", "tokens": ["Ver\u00b7steckt", "hielt", "voll", "lus\u00b7ti\u00b7ger", "M\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im Handschuh, o T\u00fccke der Frau'n,", "tokens": ["Im", "Hand\u00b7schuh", ",", "o", "T\u00fc\u00b7cke", "der", "Frau'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die spitzigen N\u00e4gel mein Sch\u00e4tzchen,", "tokens": ["Die", "spit\u00b7zi\u00b7gen", "N\u00e4\u00b7gel", "mein", "Sch\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die scharf wie Messer, traun.", "tokens": ["Die", "scharf", "wie", "Mes\u00b7ser", ",", "traun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch die andere wollte gefallen", "tokens": ["Auch", "die", "an\u00b7de\u00b7re", "woll\u00b7te", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PIS", "VMFIN", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und versteckt ihre grausamen Krallen,", "tokens": ["Und", "ver\u00b7steckt", "ih\u00b7re", "grau\u00b7sa\u00b7men", "Kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Doch w\u00e4hrt ihre Sanftmut nicht lang ...", "tokens": ["Doch", "w\u00e4hrt", "ih\u00b7re", "Sanft\u00b7mut", "nicht", "lang", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und im Zimmer, in D\u00e4mm'rung versunken,", "tokens": ["Und", "im", "Zim\u00b7mer", ",", "in", "D\u00e4m\u00b7m'\u00b7rung", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo ihr silbernes Lachen erklang,", "tokens": ["Wo", "ihr", "sil\u00b7ber\u00b7nes", "La\u00b7chen", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ergl\u00e4nzten vier Phosphorfunken.", "tokens": ["Er\u00b7gl\u00e4nz\u00b7ten", "vier", "Phos\u00b7phor\u00b7fun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}