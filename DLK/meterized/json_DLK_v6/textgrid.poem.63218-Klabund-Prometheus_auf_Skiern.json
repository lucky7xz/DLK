{"textgrid.poem.63218": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Prometheus auf Skiern", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists Schnee, der rosa unter meinen Skiern bl\u00fcht?", "tokens": ["Ists", "Schnee", ",", "der", "ro\u00b7sa", "un\u00b7ter", "mei\u00b7nen", "Ski\u00b7ern", "bl\u00fcht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ists Winterluft, die heiss um meine Schl\u00e4fen zieht?", "tokens": ["Ists", "Win\u00b7ter\u00b7luft", ",", "die", "heiss", "um", "mei\u00b7ne", "Schl\u00e4\u00b7fen", "zieht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Watzmann, der sich frierend fr\u00fch in Schleiern barg,", "tokens": ["Der", "Watz\u00b7mann", ",", "der", "sich", "frie\u00b7rend", "fr\u00fch", "in", "Schlei\u00b7ern", "barg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Liegt nackt und gl\u00e4nzend da, noch unverratzt vom Telemark ...", "tokens": ["Liegt", "nackt", "und", "gl\u00e4n\u00b7zend", "da", ",", "noch", "un\u00b7ver\u00b7ratzt", "vom", "Te\u00b7le\u00b7mark", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Ich reisse Hemd und Sweater von der feuchten", "tokens": ["Ich", "reis\u00b7se", "Hemd", "und", "Swea\u00b7ter", "von", "der", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Haut und lass sie bronzen in der Sonne leuchten ...", "tokens": ["Haut", "und", "lass", "sie", "bron\u00b7zen", "in", "der", "Son\u00b7ne", "leuch\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Nun \u00fcber diesen Hang hinab ... das Tal", "tokens": ["Nun", "\u00fc\u00b7ber", "die\u00b7sen", "Hang", "hin\u00b7ab", "...", "das", "Tal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "PTKVZ", "$(", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Brandet noch grau in dumpfer Nebelqual ...", "tokens": ["Bran\u00b7det", "noch", "grau", "in", "dum\u00b7pfer", "Ne\u00b7bel\u00b7qual", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Ich sause ... trage die Sonne auf meinem R\u00fccken ... flammenbeschwingt ...", "tokens": ["Ich", "sau\u00b7se", "...", "tra\u00b7ge", "die", "Son\u00b7ne", "auf", "mei\u00b7nem", "R\u00fc\u00b7cken", "...", "flam\u00b7men\u00b7be\u00b7schwingt", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$(", "VVFIN", "$("], "meter": "-+-+--+--+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Prometheus bin ich, der das Licht in Eure Tiefen bringt ...", "tokens": ["Pro\u00b7me\u00b7theus", "bin", "ich", ",", "der", "das", "Licht", "in", "Eu\u00b7re", "Tie\u00b7fen", "bringt", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}