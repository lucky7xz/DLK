{"textgrid.poem.53775": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Pariser Vorort", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von bunten H\u00fchnern sanft umgackert,", "tokens": ["Von", "bun\u00b7ten", "H\u00fch\u00b7nern", "sanft", "um\u00b7ga\u00b7ckert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "weht still am Fenster der Kattun.", "tokens": ["weht", "still", "am", "Fens\u00b7ter", "der", "Kat\u00b7tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich hab mich so viel abgerackert.", "tokens": ["Ich", "hab", "mich", "so", "viel", "ab\u00b7ge\u00b7ra\u00b7ckert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Jetzt will ich ruhn.", "tokens": ["Jetzt", "will", "ich", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Hundeh\u00fctte bellt und winselt.", "tokens": ["Die", "Hun\u00b7de\u00b7h\u00fct\u00b7te", "bellt", "und", "win\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schildkr\u00f6ten stolpern auf dem Kies.", "tokens": ["Schild\u00b7kr\u00f6\u00b7ten", "stol\u00b7pern", "auf", "dem", "Kies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab sie schwarz-wei\u00df-rot gepinselt,", "tokens": ["Ich", "hab", "sie", "schwa\u00b7rz\u00b7wei\u00df\u00b7rot", "ge\u00b7pin\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "denn die Franzosen lieben dies.", "tokens": ["denn", "die", "Fran\u00b7zo\u00b7sen", "lie\u00b7ben", "dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Kater klimmt, der Obersteiger,", "tokens": ["Der", "Ka\u00b7ter", "klimmt", ",", "der", "O\u00b7bers\u00b7tei\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "auf das Regal zwecks \u00dcbersicht.", "tokens": ["auf", "das", "Re\u00b7gal", "zwecks", "\u00dc\u00b7bers\u00b7icht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Doch da liegt der ", "tokens": ["Doch", "da", "liegt", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "er sch\u00fcttelt sich. Das fri\u00dft er nicht.", "tokens": ["er", "sch\u00fct\u00b7telt", "sich", ".", "Das", "fri\u00dft", "er", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$.", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und hier, inmitten des Krawalles,", "tokens": ["Und", "hier", ",", "in\u00b7mit\u00b7ten", "des", "Kra\u00b7wal\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ist auch ein zahmer Rabe da.", "tokens": ["ist", "auch", "ein", "zah\u00b7mer", "Ra\u00b7be", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kann schon: \u00bbDeutschland \u00fcber alles!\u00ab,", "tokens": ["Er", "kann", "schon", ":", "\u00bb", "Deutschland", "\u00fc\u00b7ber", "al\u00b7les", "!", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$.", "$(", "NE", "APPR", "PIS", "$.", "$(", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "sitzt auf dem Mist und schreit Hurra.", "tokens": ["sitzt", "auf", "dem", "Mist", "und", "schreit", "Hur\u00b7ra", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Nun fehlt mir noch ein hehres Bildnis,", "tokens": ["Nun", "fehlt", "mir", "noch", "ein", "heh\u00b7res", "Bild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein Hakenkreuz im Kabinett:", "tokens": ["ein", "Ha\u00b7ken\u00b7kreuz", "im", "Ka\u00b7bi\u00b7nett", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dann bin ich in der welschen Wildnis", "tokens": ["dann", "bin", "ich", "in", "der", "wel\u00b7schen", "Wild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "komplett.", "tokens": ["kom\u00b7plett", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}