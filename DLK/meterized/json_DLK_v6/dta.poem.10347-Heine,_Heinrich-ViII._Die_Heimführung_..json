{"dta.poem.10347": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  Die Heimf\u00fchrung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich geh' nicht allein, mein feines Lieb,", "tokens": ["Ich", "geh'", "nicht", "al\u00b7lein", ",", "mein", "fei\u00b7nes", "Lieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du mu\u00dft mit mir wandern", "tokens": ["Du", "mu\u00dft", "mit", "mir", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nach der lieben, alten, schaurigen Klause,", "tokens": ["Nach", "der", "lie\u00b7ben", ",", "al\u00b7ten", ",", "schau\u00b7ri\u00b7gen", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In dem tr\u00fcben, kalten, traurigen Hause,", "tokens": ["In", "dem", "tr\u00fc\u00b7ben", ",", "kal\u00b7ten", ",", "trau\u00b7ri\u00b7gen", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Wo meine Mutter am Eingang kau'rt,", "tokens": ["Wo", "mei\u00b7ne", "Mut\u00b7ter", "am", "Ein\u00b7gang", "kau'rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und auf des Sohnes Heimkehr lau'rt.", "tokens": ["Und", "auf", "des", "Soh\u00b7nes", "Heim\u00b7kehr", "lau'rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201ela\u00df ab von mir, du finstrer Mann!", "tokens": ["\u201e", "la\u00df", "ab", "von", "mir", ",", "du", "finst\u00b7rer", "Mann", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer hat dich gerufen?", "tokens": ["Wer", "hat", "dich", "ge\u00b7ru\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dein Odem gl\u00fcht, deine Hand ist Eis,", "tokens": ["Dein", "O\u00b7dem", "gl\u00fcht", ",", "dei\u00b7ne", "Hand", "ist", "Eis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Auge spr\u00fcht, deine Wang' ist wei\u00df;", "tokens": ["Dein", "Au\u00b7ge", "spr\u00fcht", ",", "dei\u00b7ne", "Wang'", "ist", "wei\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich aber will mich lustig freu'n", "tokens": ["Ich", "a\u00b7ber", "will", "mich", "lus\u00b7tig", "freu'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An Rosenduft und Sonnenschein.\u201c", "tokens": ["An", "Ro\u00b7sen\u00b7duft", "und", "Son\u00b7nen\u00b7schein", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df duften die Rosen, la\u00df scheinen die Sonn',", "tokens": ["La\u00df", "duf\u00b7ten", "die", "Ro\u00b7sen", ",", "la\u00df", "schei\u00b7nen", "die", "Sonn'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$,", "VVIMP", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mein s\u00fc\u00dfes Liebchen!", "tokens": ["Mein", "s\u00fc\u00b7\u00dfes", "Lieb\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "H\u00fcll' ein dich im weiten, wei\u00dfwallenden Schleier,", "tokens": ["H\u00fcll'", "ein", "dich", "im", "wei\u00b7ten", ",", "wei\u00df\u00b7wal\u00b7len\u00b7den", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Spiel fein auf den Saiten der schallenden Leyer,", "tokens": ["Spiel", "fein", "auf", "den", "Sai\u00b7ten", "der", "schal\u00b7len\u00b7den", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und singe ein Hochzeitlied dabei;", "tokens": ["Und", "sin\u00b7ge", "ein", "Hoch\u00b7zeit\u00b7lied", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Nachtwind pfeift die Melodei.", "tokens": ["Der", "Nacht\u00b7wind", "pfeift", "die", "Me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}