{"dta.poem.11644": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An \u2014", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ei, wer h\u00e4tt' es je gemeint,", "tokens": ["Ei", ",", "wer", "h\u00e4tt'", "es", "je", "ge\u00b7meint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00e4ulein Ludovike!", "tokens": ["Fr\u00e4u\u00b7lein", "Lu\u00b7do\u00b7vi\u00b7ke", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat man denn, so lieb man scheint,", "tokens": ["Hat", "man", "denn", ",", "so", "lieb", "man", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "$,", "ADV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch geheime T\u00fccke?", "tokens": ["Auch", "ge\u00b7hei\u00b7me", "T\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "M\u00e4dchen! wer ergr\u00fcndet euch?", "tokens": ["M\u00e4d\u00b7chen", "!", "wer", "er\u00b7gr\u00fcn\u00b7det", "euch", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00e4thsel ohne Ende!", "tokens": ["R\u00e4th\u00b7sel", "oh\u00b7ne", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Arg und falsch und engelgleich,", "tokens": ["Arg", "und", "falsch", "und", "en\u00b7gel\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer das reimen k\u00f6nnte!", "tokens": ["Wer", "das", "rei\u00b7men", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "O, nicht s\u00fc\u00dfen Honig nur", "tokens": ["O", ",", "nicht", "s\u00fc\u00b7\u00dfen", "Ho\u00b7nig", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PTKNEG", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchren eure Lippen;", "tokens": ["F\u00fch\u00b7ren", "eu\u00b7re", "Lip\u00b7pen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und so seyd ihr von Natur", "tokens": ["Und", "so", "seyd", "ihr", "von", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebliche X \u2014 \u2014 \u2014.", "tokens": ["Lieb\u00b7li\u00b7che", "X", "."], "token_info": ["word", "word", "punct", "punct", "punct", "punct"], "pos": ["XY", "XY", "$(", "$(", "$(", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}