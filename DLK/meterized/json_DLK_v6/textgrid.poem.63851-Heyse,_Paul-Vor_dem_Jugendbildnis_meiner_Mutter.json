{"textgrid.poem.63851": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Vor dem Jugendbildnis meiner Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O h\u00e4tt' ich damals dich gesehn,", "tokens": ["O", "h\u00e4tt'", "ich", "da\u00b7mals", "dich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als so du in die Welt geschaut,", "tokens": ["Als", "so", "du", "in", "die", "Welt", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So morgenklar, so jugendsch\u00f6n,", "tokens": ["So", "mor\u00b7gen\u00b7klar", ",", "so", "ju\u00b7gend\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch mit dem Leben unvertraut;", "tokens": ["Noch", "mit", "dem", "Le\u00b7ben", "un\u00b7ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch unterm schwarzen Haar so licht", "tokens": ["Noch", "un\u00b7term", "schwar\u00b7zen", "Haar", "so", "licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4dchenstirne, wei\u00df und glatt,", "tokens": ["Die", "M\u00e4d\u00b7chens\u00b7tir\u00b7ne", ",", "wei\u00df", "und", "glatt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So blau das Auge, das noch nicht", "tokens": ["So", "blau", "das", "Au\u00b7ge", ",", "das", "noch", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getr\u00fcbt ein Strom von Tr\u00e4nen hat;", "tokens": ["Ge\u00b7tr\u00fcbt", "ein", "Strom", "von", "Tr\u00e4\u00b7nen", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dieses liebliche Profil,", "tokens": ["Und", "die\u00b7ses", "lieb\u00b7li\u00b7che", "Pro\u00b7fil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schalkhaft feingesch\u00fcrzte Mund,", "tokens": ["Der", "schalk\u00b7haft", "fein\u00b7ge\u00b7sch\u00fcrz\u00b7te", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bereit zu munterm Witzesspiel", "tokens": ["Be\u00b7reit", "zu", "mun\u00b7term", "Wit\u00b7zes\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sinn'gem Wort in ernster Stund'!", "tokens": ["Und", "sinn'\u00b7gem", "Wort", "in", "erns\u00b7ter", "Stund'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir ist, ich seh' dich am Klavier,", "tokens": ["Mir", "ist", ",", "ich", "seh'", "dich", "am", "Kla\u00b7vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wie du die alten Lieder singst", "tokens": ["Wie", "du", "die", "al\u00b7ten", "Lie\u00b7der", "singst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dann im wei\u00dfen Kleid, wie hier,", "tokens": ["Und", "dann", "im", "wei\u00b7\u00dfen", "Kleid", ",", "wie", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Tanz die kleinen F\u00fc\u00dfe schwingst.", "tokens": ["Im", "Tanz", "die", "klei\u00b7nen", "F\u00fc\u00b7\u00dfe", "schwingst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wieder, wie du ernst bem\u00fcht", "tokens": ["Und", "wie\u00b7der", ",", "wie", "du", "ernst", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Schreibtisch sitzest stundenlang,", "tokens": ["Am", "Schreib\u00b7tisch", "sit\u00b7zest", "stun\u00b7den\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Robert Burns ein kleines Lied", "tokens": ["Von", "Ro\u00b7bert", "Burns", "ein", "klei\u00b7nes", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdichtend, froh, wenn dir's gelang.", "tokens": ["Nach\u00b7dich\u00b7tend", ",", "froh", ",", "wenn", "dir's", "ge\u00b7lang", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und alles, was dich rings umgibt,", "tokens": ["Und", "al\u00b7les", ",", "was", "dich", "rings", "um\u00b7gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du mit Anmut angestrahlt.", "tokens": ["Hast", "du", "mit", "An\u00b7mut", "an\u00b7ge\u00b7strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df war auch in dich verliebt", "tokens": ["Ge\u00b7wi\u00df", "war", "auch", "in", "dich", "ver\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Maler, der dies Bild gemalt.", "tokens": ["Der", "Ma\u00b7ler", ",", "der", "dies", "Bild", "ge\u00b7malt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O h\u00e4tt' ich jemals dich gesehn", "tokens": ["O", "h\u00e4tt'", "ich", "je\u00b7mals", "dich", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leibhaftig so, du lieb Gesicht!", "tokens": ["Leib\u00b7haf\u00b7tig", "so", ",", "du", "lieb", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nein! w\u00e4r' mir so wohl geschehn,", "tokens": ["Doch", "nein", "!", "w\u00e4r'", "mir", "so", "wohl", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4rst du meine Mutter nicht.", "tokens": ["W\u00e4rst", "du", "mei\u00b7ne", "Mut\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun kannt' ich deine Z\u00fcge nur", "tokens": ["Nun", "kannt'", "ich", "dei\u00b7ne", "Z\u00fc\u00b7ge", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht morgenklar und jugendfroh,", "tokens": ["Nicht", "mor\u00b7gen\u00b7klar", "und", "ju\u00b7gend\u00b7froh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gefurcht von mancher Leidensspur,", "tokens": ["Ge\u00b7furcht", "von", "man\u00b7cher", "Lei\u00b7dens\u00b7spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sch\u00f6n erschienst du mir auch so.", "tokens": ["Doch", "sch\u00f6n", "er\u00b7schienst", "du", "mir", "auch", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Vor\u00fcber zog die Jugendzeit", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zog", "die", "Ju\u00b7gend\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lang eh' dein Herzensgl\u00fcck du fandst,", "tokens": ["Lang", "eh'", "dein", "Her\u00b7zens\u00b7gl\u00fcck", "du", "fandst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast, eh' mein Vater dich gefreit,", "tokens": ["Hast", ",", "eh'", "mein", "Va\u00b7ter", "dich", "ge\u00b7freit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4ngst nicht gespielt mehr und getanzt.", "tokens": ["L\u00e4ngst", "nicht", "ge\u00b7spielt", "mehr", "und", "ge\u00b7tanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "ADV", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch dir im Herzen sang und klang", "tokens": ["Doch", "dir", "im", "Her\u00b7zen", "sang", "und", "klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch stets das Lied der Jugend hell,", "tokens": ["Noch", "stets", "das", "Lied", "der", "Ju\u00b7gend", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprudelnd dir vom Munde sprang", "tokens": ["Und", "spru\u00b7delnd", "dir", "vom", "Mun\u00b7de", "sprang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Witzes und des Frohsinns Quell.", "tokens": ["Des", "Wit\u00b7zes", "und", "des", "Froh\u00b7sinns", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und z\u00e4rtlich in des Sohnes Ohr", "tokens": ["Und", "z\u00e4rt\u00b7lich", "in", "des", "Soh\u00b7nes", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast Liebesworte du gehaucht,", "tokens": ["Hast", "Lie\u00b7bes\u00b7wor\u00b7te", "du", "ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die h\u00f6r' ich wieder, wenn empor", "tokens": ["Die", "h\u00f6r'", "ich", "wie\u00b7der", ",", "wenn", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Bild vor meiner Seele taucht.", "tokens": ["Dein", "Bild", "vor", "mei\u00b7ner", "See\u00b7le", "taucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich h\u00f6re, wie du gute Nacht", "tokens": ["Ich", "h\u00f6\u00b7re", ",", "wie", "du", "gu\u00b7te", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir sagtest, wenn aus fernem Land", "tokens": ["Mir", "sag\u00b7test", ",", "wenn", "aus", "fer\u00b7nem", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heimkehrend ich so wohlgemacht", "tokens": ["Heim\u00b7keh\u00b7rend", "ich", "so", "wohl\u00b7ge\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Bett im Mutterhause fand,", "tokens": ["Mein", "Bett", "im", "Mut\u00b7ter\u00b7hau\u00b7se", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und seh' dein Auge \u00fcbergehn,", "tokens": ["Und", "seh'", "dein", "Au\u00b7ge", "\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn meine Kinder du gek\u00fc\u00dft.", "tokens": ["Wenn", "mei\u00b7ne", "Kin\u00b7der", "du", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schienst du mir so hold und sch\u00f6n,", "tokens": ["Da", "schienst", "du", "mir", "so", "hold", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie jung du nie gewesen bist.", "tokens": ["Wie", "jung", "du", "nie", "ge\u00b7we\u00b7sen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Nun ward ich selber alt wie du;", "tokens": ["Nun", "ward", "ich", "sel\u00b7ber", "alt", "wie", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur noch ein Weilchen athm' ich hier,", "tokens": ["Nur", "noch", "ein", "Weil\u00b7chen", "athm'", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bis ich geh' zur letzten Ruh',", "tokens": ["Doch", "bis", "ich", "geh'", "zur", "letz\u00b7ten", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was an mir jung bleibt, dank' ich dir.", "tokens": ["Was", "an", "mir", "jung", "bleibt", ",", "dank'", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}