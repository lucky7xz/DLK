{"textgrid.poem.57531": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber die Heide geht mein Gedenken,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Heide geht mein Gedenken,", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "geht", "mein", "Ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du kleines M\u00e4dchen,", "tokens": ["Du", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nach dir, nach dir allein;", "tokens": ["Nach", "dir", ",", "nach", "dir", "al\u00b7lein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00dcber die Heide m\u00f6chte ich wandern,", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "m\u00f6ch\u00b7te", "ich", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Du kleines M\u00e4dchen,", "tokens": ["Du", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bei dir zu sein.", "tokens": ["Bei", "dir", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u00dcber die Heide flogen die Schwalben,", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "flo\u00b7gen", "die", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du kleines M\u00e4dchen,", "tokens": ["Du", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sie gr\u00fc\u00dften mich von dir;", "tokens": ["Sie", "gr\u00fc\u00df\u00b7ten", "mich", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00dcber die Heide kr\u00e4chzten die Raben,", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "kr\u00e4chz\u00b7ten", "die", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Du kleines M\u00e4dchen,", "tokens": ["Du", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Antwort von mir.", "tokens": ["Ant\u00b7wort", "von", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "\u00dcber die Heide fallen die Flocken,", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "fal\u00b7len", "die", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du kleines M\u00e4dchen,", "tokens": ["Du", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und fu\u00dfhoch liegt der Schnee;", "tokens": ["Und", "fu\u00df\u00b7hoch", "liegt", "der", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00dcber die Heide ging einst mein Hoffen,", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "ging", "einst", "mein", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Du kleines M\u00e4dchen,", "tokens": ["Du", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ade, Ade!", "tokens": ["A\u00b7de", ",", "A\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}