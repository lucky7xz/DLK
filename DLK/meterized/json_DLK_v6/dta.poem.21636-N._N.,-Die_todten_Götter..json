{"dta.poem.21636": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Die todten G\u00f6tter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Friedhof schritt ich hin im Traume,               ", "tokens": ["Auf", "ei\u00b7nem", "Fried\u00b7hof", "schritt", "ich", "hin", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es rauschten dunkelragend die Cypressen,", "tokens": ["Es", "rauschten", "dun\u00b7kel\u00b7ra\u00b7gend", "die", "Cyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es d\u00e4mmerte vom bleichen Wolkenraume,", "tokens": ["Es", "d\u00e4m\u00b7mer\u00b7te", "vom", "blei\u00b7chen", "Wol\u00b7ken\u00b7rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gr\u00e4ber schliefen rings in Nacht vergessen.", "tokens": ["Die", "Gr\u00e4\u00b7ber", "schlie\u00b7fen", "rings", "in", "Nacht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und in die Friedhofhalle trat ich traurig,", "tokens": ["Und", "in", "die", "Fried\u00b7hof\u00b7hal\u00b7le", "trat", "ich", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo aufgebahrt die todten G\u00f6tter ruhten", "tokens": ["Wo", "auf\u00b7ge\u00b7bahrt", "die", "tod\u00b7ten", "G\u00f6t\u00b7ter", "ruh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im offnen Sarkophage. Ernst und schaurig,", "tokens": ["Im", "off\u00b7nen", "Sar\u00b7ko\u00b7pha\u00b7ge", ".", "Ernst", "und", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NE", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So schliefen sie den ew\u2019gen Tod, die Guten.", "tokens": ["So", "schlie\u00b7fen", "sie", "den", "ew'\u00b7gen", "Tod", ",", "die", "Gu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und in die Z\u00fcge staunend stand ich lange", "tokens": ["Und", "in", "die", "Z\u00fc\u00b7ge", "stau\u00b7nend", "stand", "ich", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der s\u00fc\u00dfen Aphrodite, der erbla\u00dften,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "A\u00b7phro\u00b7di\u00b7te", ",", "der", "er\u00b7bla\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die kalt und l\u00e4chelnd schlief. Bleich war die Wange", "tokens": ["Die", "kalt", "und", "l\u00e4\u00b7chelnd", "schlief", ".", "Bleich", "war", "die", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "$.", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Des todten Zeus. Und ewig sah ich rasten", "tokens": ["Des", "tod\u00b7ten", "Zeus", ".", "Und", "e\u00b7wig", "sah", "ich", "ras\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "$.", "KON", "ADJD", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im offnen Sarg die Asgardg\u00f6tter alle.", "tokens": ["Im", "off\u00b7nen", "Sarg", "die", "As\u00b7gard\u00b7g\u00f6t\u00b7ter", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es flatterten um Wodans Haupt die Raben", "tokens": ["Es", "flat\u00b7ter\u00b7ten", "um", "Wo\u00b7dans", "Haupt", "die", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nieder schwebten sie, die schn\u00f6de Kralle", "tokens": ["Und", "nie\u00b7der", "schweb\u00b7ten", "sie", ",", "die", "schn\u00f6\u00b7de", "Kral\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihres Vaters Leichnam zu begraben.", "tokens": ["In", "ih\u00b7res", "Va\u00b7ters", "Leich\u00b7nam", "zu", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und weinend sank ich hin am Sarkophage.", "tokens": ["Und", "wei\u00b7nend", "sank", "ich", "hin", "am", "Sar\u00b7ko\u00b7pha\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da t\u00f6nte Orgelklang ernst durch die Halle,", "tokens": ["Da", "t\u00f6n\u00b7te", "Or\u00b7gel\u00b7klang", "ernst", "durch", "die", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da war\u2019s, als ob die Sonne gl\u00e4nzend tage,", "tokens": ["Da", "wa\u00b7r's", ",", "als", "ob", "die", "Son\u00b7ne", "gl\u00e4n\u00b7zend", "ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "ADJD", "NN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Im reinen Licht erklang\u2019s zum Jubelschalle:", "tokens": ["Im", "rei\u00b7nen", "Licht", "er\u00b7klang's", "zum", "Ju\u00b7bel\u00b7schal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Todt sind sie all, die gro\u00dfen G\u00f6tter,", "tokens": ["Todt", "sind", "sie", "all", ",", "die", "gro\u00b7\u00dfen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIAT", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gestorben ist ihr stolzer Ruhm;", "tokens": ["Ge\u00b7stor\u00b7ben", "ist", "ihr", "stol\u00b7zer", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Zeitensturm, im Himmelswetter", "tokens": ["Im", "Zei\u00b7ten\u00b7sturm", ",", "im", "Him\u00b7mels\u00b7wet\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ver\u00f6det st\u00fcrzt ihr Heiligthum.", "tokens": ["Ver\u00b7\u00f6\u00b7det", "st\u00fcrzt", "ihr", "Hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und Seelen, die von G\u00f6ttern sangen,", "tokens": ["Und", "See\u00b7len", ",", "die", "von", "G\u00f6t\u00b7tern", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die betend sanken in den Staub,", "tokens": ["Die", "be\u00b7tend", "san\u00b7ken", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind verschollen, sind vergangen", "tokens": ["Sie", "sind", "ver\u00b7schol\u00b7len", ",", "sind", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlummern wie die Erde taub.", "tokens": ["Und", "schlum\u00b7mern", "wie", "die", "Er\u00b7de", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und aus der frischen Lebensquelle", "tokens": ["Und", "aus", "der", "fri\u00b7schen", "Le\u00b7bens\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Taucht neuer Geist verj\u00fcngt hervor,", "tokens": ["Taucht", "neu\u00b7er", "Geist", "ver\u00b7j\u00fcngt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wie die Welle dr\u00e4ngt die Welle,", "tokens": ["Und", ",", "wie", "die", "Wel\u00b7le", "dr\u00e4ngt", "die", "Wel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flieht vor dem Geist der G\u00f6tter Chor;", "tokens": ["Flieht", "vor", "dem", "Geist", "der", "G\u00f6t\u00b7ter", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es w\u00fcrgt der Tod das rauhe Streben", "tokens": ["Es", "w\u00fcrgt", "der", "Tod", "das", "rau\u00b7he", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Sense rastet nie,", "tokens": ["Und", "sei\u00b7ne", "Sen\u00b7se", "ras\u00b7tet", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und doch aus S\u00e4rgen G\u00f6tterleben", "tokens": ["Und", "doch", "aus", "S\u00e4r\u00b7gen", "G\u00f6t\u00b7ter\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weckt ewig auf die Phantasie.", "tokens": ["Weckt", "e\u00b7wig", "auf", "die", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Todt sind sie all, die gro\u00dfen G\u00f6tter,", "tokens": ["Todt", "sind", "sie", "all", ",", "die", "gro\u00b7\u00dfen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIAT", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ewig lebt ein Weltengeist,", "tokens": ["Doch", "e\u00b7wig", "lebt", "ein", "Wel\u00b7ten\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist sich ewig Selbsterretter,", "tokens": ["Er", "ist", "sich", "e\u00b7wig", "Selbs\u00b7ter\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Todesfesseln k\u00fchn zerrei\u00dft.", "tokens": ["Der", "To\u00b7des\u00b7fes\u00b7seln", "k\u00fchn", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lang noch holde Tr\u00e4ume weben,", "tokens": ["So", "lang", "noch", "hol\u00b7de", "Tr\u00e4u\u00b7me", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wann dunkler Schlaf die Welt umh\u00fcllt,", "tokens": ["Wann", "dunk\u00b7ler", "Schlaf", "die", "Welt", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So lang noch sanfte T\u00f6ne schweben", "tokens": ["So", "lang", "noch", "sanf\u00b7te", "T\u00f6\u00b7ne", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Harmonie das Ohr erf\u00fcllt;", "tokens": ["Und", "Har\u00b7mo\u00b7nie", "das", "Ohr", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So lang des Daseins bunte Schatten", "tokens": ["So", "lang", "des", "Da\u00b7seins", "bun\u00b7te", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Malers weise Hand belebt,", "tokens": ["Des", "Ma\u00b7lers", "wei\u00b7se", "Hand", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang auf bl\u00fcthenreichen Matten", "tokens": ["So", "lang", "auf", "bl\u00fc\u00b7then\u00b7rei\u00b7chen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Dichters Auge trunken schwebt", "tokens": ["Des", "Dich\u00b7ters", "Au\u00b7ge", "trun\u00b7ken", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in des Herzens dunklem Grunde", "tokens": ["Und", "in", "des", "Her\u00b7zens", "dunk\u00b7lem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gestalten seelenvoll erschaut \u2014", "tokens": ["Ge\u00b7stal\u00b7ten", "see\u00b7len\u00b7voll", "er\u00b7schaut"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist \u00fcber diesem Erdenrunde", "tokens": ["Ist", "\u00fc\u00b7ber", "die\u00b7sem", "Er\u00b7den\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Tempel ewig neu erbaut. \u2014", "tokens": ["Ein", "Tem\u00b7pel", "e\u00b7wig", "neu", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}