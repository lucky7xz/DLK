{"textgrid.poem.55662": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[ist's m\u00f6glich, da\u00df ich, Liebchen, dich kose]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's m\u00f6glich, da\u00df ich, Liebchen, dich kose,", "tokens": ["Ist's", "m\u00f6g\u00b7lich", ",", "da\u00df", "ich", ",", "Lieb\u00b7chen", ",", "dich", "ko\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "$,", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vernehme der g\u00f6ttlichen Stimme Schall!", "tokens": ["Ver\u00b7neh\u00b7me", "der", "g\u00f6tt\u00b7li\u00b7chen", "Stim\u00b7me", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Unm\u00f6glich scheint immer die Rose,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "scheint", "im\u00b7mer", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unbegreiflich die Nachtigall.", "tokens": ["Un\u00b7be\u00b7greif\u00b7lich", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}