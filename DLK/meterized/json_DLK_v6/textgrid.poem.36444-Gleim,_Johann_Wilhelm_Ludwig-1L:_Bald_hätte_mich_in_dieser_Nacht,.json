{"textgrid.poem.36444": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bald h\u00e4tte mich in dieser Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald h\u00e4tte mich in dieser Nacht,", "tokens": ["Bald", "h\u00e4t\u00b7te", "mich", "in", "die\u00b7ser", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Traum ins Todtenreich gebracht.", "tokens": ["Ein", "Traum", "ins", "Tod\u00b7ten\u00b7reich", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich deucht, ich ritt spatzieren,", "tokens": ["Mich", "deucht", ",", "ich", "ritt", "spat\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Grillen zu verlieren;", "tokens": ["Die", "Gril\u00b7len", "zu", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da traf ich, welch ein Gl\u00fckke!", "tokens": ["Da", "traf", "ich", ",", "welch", "ein", "Gl\u00fck\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein M\u00e4dchen auf der Br\u00fckke,", "tokens": ["Mein", "M\u00e4d\u00b7chen", "auf", "der", "Br\u00fck\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf die ich einst, mit Ruthen, schlug,", "tokens": ["Auf", "die", "ich", "einst", ",", "mit", "Ru\u00b7then", ",", "schlug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "$,", "APPR", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als sie mein M\u00e4dchen von mir trug.", "tokens": ["Als", "sie", "mein", "M\u00e4d\u00b7chen", "von", "mir", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Itzt wards, in ofnem Wagen,", "tokens": ["Itzt", "wards", ",", "in", "of\u00b7nem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Rappen hergetragen.", "tokens": ["Von", "Rap\u00b7pen", "her\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sahen uns, o Freude!", "tokens": ["Wir", "sa\u00b7hen", "uns", ",", "o", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich deucht, wir w\u00fcnschten beide:", "tokens": ["Mich", "deucht", ",", "wir", "w\u00fcnschten", "bei\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ach m\u00f6chte doch, uns zu erfreun,", "tokens": ["Ach", "m\u00f6ch\u00b7te", "doch", ",", "uns", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Mutter nicht im Wagen seyn!", "tokens": ["Die", "Mut\u00b7ter", "nicht", "im", "Wa\u00b7gen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Indem der Wunsch geschahe,", "tokens": ["In\u00b7dem", "der", "Wunsch", "ge\u00b7scha\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam uns ein Tolpatsch nahe,", "tokens": ["Kam", "uns", "ein", "Tol\u00b7patsch", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, ach! f\u00fcr seinen Lappen,", "tokens": ["Und", ",", "ach", "!", "f\u00fcr", "sei\u00b7nen", "Lap\u00b7pen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erschrekken sich die Rappen,", "tokens": ["Er\u00b7schrek\u00b7ken", "sich", "die", "Rap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und springen seitwerts in den Flu\u00df,", "tokens": ["Und", "sprin\u00b7gen", "seit\u00b7werts", "in", "den", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df auch der Wagen fallen mu\u00df!", "tokens": ["Da\u00df", "auch", "der", "Wa\u00b7gen", "fal\u00b7len", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da f\u00e4llt, ach Ungel\u00fckke,", "tokens": ["Da", "f\u00e4llt", ",", "ach", "Un\u00b7ge\u00b7l\u00fck\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "XY", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein M\u00e4dchen von der Br\u00fckke!", "tokens": ["Mein", "M\u00e4d\u00b7chen", "von", "der", "Br\u00fck\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Blut f\u00e4ngt an zu wallen,", "tokens": ["Mein", "Blut", "f\u00e4ngt", "an", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich denk ihm nachzufallen.", "tokens": ["Ich", "denk", "ihm", "nach\u00b7zu\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein M\u00e4dchen stirbt! ach, welche Noth!", "tokens": ["Mein", "M\u00e4d\u00b7chen", "stirbt", "!", "ach", ",", "wel\u00b7che", "Noth", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "XY", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Wasser ... Wasser sei mein Tod!", "tokens": ["Im", "Was\u00b7ser", "...", "Was\u00b7ser", "sei", "mein", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drauf soll mein Pferd sich schwingen,", "tokens": ["Drauf", "soll", "mein", "Pferd", "sich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schnell ins Wasser springen.", "tokens": ["Und", "schnell", "ins", "Was\u00b7ser", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein, es b\u00e4umt zur\u00fckke,", "tokens": ["Al\u00b7lein", ",", "es", "b\u00e4umt", "zu\u00b7r\u00fck\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und will nicht von der Br\u00fckke,", "tokens": ["Und", "will", "nicht", "von", "der", "Br\u00fck\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So traurig auch der Reuter sprach:", "tokens": ["So", "trau\u00b7rig", "auch", "der", "Reu\u00b7ter", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, springe doch dem M\u00e4dchen nach!", "tokens": ["Ach", ",", "sprin\u00b7ge", "doch", "dem", "M\u00e4d\u00b7chen", "nach", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Itzt wach ich, und es kommt gelaufen:", "tokens": ["Itzt", "wach", "ich", ",", "und", "es", "kommt", "ge\u00b7lau\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun werd ich mich wol nicht ersaufen.", "tokens": ["Nun", "werd", "ich", "mich", "wol", "nicht", "er\u00b7sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}