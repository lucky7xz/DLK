{"textgrid.poem.32835": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sehr lieb ich mein M\u00e4dchen nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sehr lieb ich mein M\u00e4dchen nicht!", "tokens": ["Wie", "sehr", "lieb", "ich", "mein", "M\u00e4d\u00b7chen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat ein allerliebst Gesicht,", "tokens": ["Sie", "hat", "ein", "al\u00b7ler\u00b7liebst", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sch\u00f6n, um es recht zu beschreiben:", "tokens": ["Zu", "sch\u00f6n", ",", "um", "es", "recht", "zu", "be\u00b7schrei\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Doch da\u00df, wenn sie mir untreu w\u00e4r,", "tokens": ["Doch", "da\u00df", ",", "wenn", "sie", "mir", "un\u00b7treu", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich dr\u00fcber den Verstand verl\u00f6hr:", "tokens": ["Ich", "dr\u00fc\u00b7ber", "den", "Ver\u00b7stand", "ver\u00b7l\u00f6hr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das la\u00df ich wohl bleiben.", "tokens": ["Das", "la\u00df", "ich", "wohl", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich trinke gern ein Gl\u00e4\u00dfgen Wein,", "tokens": ["Ich", "trin\u00b7ke", "gern", "ein", "Gl\u00e4\u00df\u00b7gen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schenkt mir Freund oder M\u00e4dchen ein,", "tokens": ["Schenkt", "mir", "Freund", "o\u00b7der", "M\u00e4d\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Darzu la\u00df ich mich niemals treiben:", "tokens": ["Dar\u00b7zu", "la\u00df", "ich", "mich", "nie\u00b7mals", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch da\u00df ich N\u00e4chte lang die\u00df trieb,", "tokens": ["Doch", "da\u00df", "ich", "N\u00e4ch\u00b7te", "lang", "die\u00df", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "ADJD", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und morgen mir die Stirne rieb:", "tokens": ["Und", "mor\u00b7gen", "mir", "die", "Stir\u00b7ne", "rieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das la\u00df ich wohl bleiben.", "tokens": ["Das", "la\u00df", "ich", "wohl", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich w\u00fcnschte mir ein eigen Haus,", "tokens": ["Ich", "w\u00fcnschte", "mir", "ein", "ei\u00b7gen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und theilte gern auch Gnaden aus", "tokens": ["Und", "theil\u00b7te", "gern", "auch", "Gna\u00b7den", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lie\u00df Hypothecken mir verschreiben;", "tokens": ["Lie\u00df", "Hy\u00b7po\u00b7thec\u00b7ken", "mir", "ver\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch da\u00df durch niedrigen Gewinn", "tokens": ["Doch", "da\u00df", "durch", "nied\u00b7ri\u00b7gen", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich reicher w\u00fcrd, als ich jetzt bin:", "tokens": ["Ich", "rei\u00b7cher", "w\u00fcrd", ",", "als", "ich", "jetzt", "bin", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das la\u00df ich wohl bleiben.", "tokens": ["Das", "la\u00df", "ich", "wohl", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit einer Frau von vielem Geld,", "tokens": ["Mit", "ei\u00b7ner", "Frau", "von", "vie\u00b7lem", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat sie noch sonst, was mir gef\u00e4llt,", "tokens": ["Hat", "sie", "noch", "sonst", ",", "was", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00fcrd ich mich allenfalls beweiben:", "tokens": ["W\u00fcrd", "ich", "mich", "al\u00b7len\u00b7falls", "be\u00b7wei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch da\u00df ich Schw\u00e4gern demuthsvoll", "tokens": ["Doch", "da\u00df", "ich", "Schw\u00e4\u00b7gern", "de\u00b7muths\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "ADJD"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Viel Reverenze machen soll,", "tokens": ["Viel", "Re\u00b7ve\u00b7ren\u00b7ze", "ma\u00b7chen", "soll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das la\u00df ich wohl bleiben.", "tokens": ["Das", "la\u00df", "ich", "wohl", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein Amt, wobey die Welt mich ehrt,", "tokens": ["Ein", "Amt", ",", "wo\u00b7bey", "die", "Welt", "mich", "ehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mich bequem und reichlich n\u00e4hrt,", "tokens": ["Das", "mich", "be\u00b7quem", "und", "reich\u00b7lich", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darwider werd ich mich nicht str\u00e4uben:", "tokens": ["Dar\u00b7wi\u00b7der", "werd", "ich", "mich", "nicht", "str\u00e4u\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch soll ichs durch Laqueyn erflehn,", "tokens": ["Doch", "soll", "ichs", "durch", "La\u00b7que\u00b7yn", "er\u00b7flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mich krank in Antichambern stehn?", "tokens": ["Mich", "krank", "in", "An\u00b7ti\u00b7ch\u00b7am\u00b7bern", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das la\u00df ich wohl bleiben.", "tokens": ["Das", "la\u00df", "ich", "wohl", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Des Lebens Gl\u00fcck ist mir ein Freund:", "tokens": ["Des", "Le\u00b7bens", "Gl\u00fcck", "ist", "mir", "ein", "Freund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch bin ich keines Menschen Feind,", "tokens": ["Auch", "bin", "ich", "kei\u00b7nes", "Men\u00b7schen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die oft durch Thorheit uns bet\u00e4uben:", "tokens": ["Die", "oft", "durch", "Thor\u00b7heit", "uns", "be\u00b7t\u00e4u\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch da\u00df ich aus der Narren Zahl", "tokens": ["Doch", "da\u00df", "ich", "aus", "der", "Nar\u00b7ren", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vertraute hole, sonder Wahl:", "tokens": ["Ver\u00b7trau\u00b7te", "ho\u00b7le", ",", "son\u00b7der", "Wahl", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das la\u00df ich wohl bleiben.", "tokens": ["Das", "la\u00df", "ich", "wohl", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Noch wallt die Freud in meiner Brust;", "tokens": ["Noch", "wallt", "die", "Freud", "in", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch hab ich viel zu leben Lust,", "tokens": ["Noch", "hab", "ich", "viel", "zu", "le\u00b7ben", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mich die Welt nicht will vertreiben:", "tokens": ["Wenn", "mich", "die", "Welt", "nicht", "will", "ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch da\u00df ich winselnd Abschied n\u00e4hm,", "tokens": ["Doch", "da\u00df", "ich", "win\u00b7selnd", "Ab\u00b7schied", "n\u00e4hm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn schon so fr\u00fch die Parce k\u00e4m:", "tokens": ["Wenn", "schon", "so", "fr\u00fch", "die", "Par\u00b7ce", "k\u00e4m", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das la\u00df ich wohl bleiben.", "tokens": ["Das", "la\u00df", "ich", "wohl", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}