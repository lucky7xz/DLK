{"textgrid.poem.45501": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Pavia", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Studenten sind armselige Gesellen,", "tokens": ["Stu\u00b7den\u00b7ten", "sind", "arm\u00b7se\u00b7li\u00b7ge", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und doch des Kranken Puls, weil sie sich nicht verstellen.", "tokens": ["Und", "doch", "des", "Kran\u00b7ken", "Puls", ",", "weil", "sie", "sich", "nicht", "ver\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}