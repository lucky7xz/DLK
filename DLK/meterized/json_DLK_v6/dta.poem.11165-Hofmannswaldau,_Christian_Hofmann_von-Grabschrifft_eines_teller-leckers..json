{"dta.poem.11165": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines teller-leckers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer lieg ich, der ich offt in sau\u00df und schmau\u00df gelegen,", "tokens": ["Hjer", "lieg", "ich", ",", "der", "ich", "offt", "in", "sau\u00df", "und", "schmau\u00df", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "NE", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein tisch war \u00fcberall, mein teller allerwegen.", "tokens": ["Mein", "tisch", "war", "\u00fc\u00b7be\u00b7rall", ",", "mein", "tel\u00b7ler", "al\u00b7ler\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo es mich nichts gekost, da gieng ich gerne hin;", "tokens": ["Wo", "es", "mich", "nichts", "ge\u00b7kost", ",", "da", "gieng", "ich", "ger\u00b7ne", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PIS", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es kostet mich auch nichts, da\u00df ich gestorben bin.", "tokens": ["Es", "kos\u00b7tet", "mich", "auch", "nichts", ",", "da\u00df", "ich", "ge\u00b7stor\u00b7ben", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}