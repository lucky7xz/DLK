{"textgrid.poem.39664": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein rechtsgelehrter Freund ist endlich doch der Stadt,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein rechtsgelehrter Freund ist endlich doch der Stadt,", "tokens": ["Mein", "rechts\u00b7ge\u00b7lehr\u00b7ter", "Freund", "ist", "end\u00b7lich", "doch", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Consultation und der Processe satt.", "tokens": ["Der", "Con\u00b7sul\u00b7ta\u00b7ti\u00b7on", "und", "der", "Pro\u00b7ces\u00b7se", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ganz l\u00e4ndlich wandert er hinaus zu seinem Garten;", "tokens": ["Ganz", "l\u00e4nd\u00b7lich", "wan\u00b7dert", "er", "hin\u00b7aus", "zu", "sei\u00b7nem", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APZR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er will des Obstes dort und der Gem\u00fcse warten.", "tokens": ["Er", "will", "des", "Obs\u00b7tes", "dort", "und", "der", "Ge\u00b7m\u00fc\u00b7se", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als er das Gut gekauft, besucht' ich ihn einmal:", "tokens": ["Als", "er", "das", "Gut", "ge\u00b7kauft", ",", "be\u00b7sucht'", "ich", "ihn", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vom Gr\u00fcnen ganz umringt fand ich den Gartensaal;", "tokens": ["Vom", "Gr\u00fc\u00b7nen", "ganz", "um\u00b7ringt", "fand", "ich", "den", "Gar\u00b7ten\u00b7saal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch war's ein Exemplar dickleibiger Pandekten,", "tokens": ["Doch", "wa\u00b7r's", "ein", "Ex\u00b7emp\u00b7lar", "dick\u00b7lei\u00b7bi\u00b7ger", "Pan\u00b7dek\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Die, an Tapeten statt, die W\u00e4nde rings bedeckten.", "tokens": ["Die", ",", "an", "Ta\u00b7pe\u00b7ten", "statt", ",", "die", "W\u00e4n\u00b7de", "rings", "be\u00b7deck\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "PTKVZ", "$,", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie sehr entz\u00fcckte mich der blumenreiche Lenz!", "tokens": ["Wie", "sehr", "ent\u00b7z\u00fcck\u00b7te", "mich", "der", "blu\u00b7men\u00b7rei\u00b7che", "Lenz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hier sah ich die Natur, da die Jurisprudenz.", "tokens": ["Hier", "sah", "ich", "die", "Na\u00b7tur", ",", "da", "die", "Ju\u00b7ris\u00b7pru\u00b7denz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Der Saal ist nun verziert und das Papier bemalet,", "tokens": ["Der", "Saal", "ist", "nun", "ver\u00b7ziert", "und", "das", "Pa\u00b7pier", "be\u00b7ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch so, da\u00df noch hindurch die alte Druckschrift strahlet.", "tokens": ["Doch", "so", ",", "da\u00df", "noch", "hin\u00b7durch", "die", "al\u00b7te", "Druck\u00b7schrift", "strah\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da geht der Gartenfreund im Gr\u00fcnen ab und auf,", "tokens": ["Da", "geht", "der", "Gar\u00b7ten\u00b7freund", "im", "Gr\u00fc\u00b7nen", "ab", "und", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und lies't Papinian und Ulpian im Lauf.", "tokens": ["Und", "lies't", "Pa\u00b7pi\u00b7ni\u00b7an", "und", "Ul\u00b7pi\u00b7an", "im", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "KON", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer ihn zu Rathe zog, sei immer au\u00dfer Sorgen;", "tokens": ["Wer", "ihn", "zu", "Ra\u00b7the", "zog", ",", "sei", "im\u00b7mer", "au\u00b7\u00dfer", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er bringt vom Land zur\u00fcck uns neuen Ausspruch morgen.", "tokens": ["Er", "bringt", "vom", "Land", "zu\u00b7r\u00fcck", "uns", "neu\u00b7en", "Aus\u00b7spruch", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}