{"textgrid.poem.42706": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob auch wohl Haschka's Mund noch manchmal mein erw\u00e4hnt?", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob auch wohl Haschka's Mund noch manchmal mein erw\u00e4hnt?", "tokens": ["Ob", "auch", "wohl", "Haschka's", "Mund", "noch", "manch\u00b7mal", "mein", "er\u00b7w\u00e4hnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NE", "NN", "ADV", "ADV", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ob sich im fernen Wien das H\u00e4uflein meiner andern", "tokens": ["Ob", "sich", "im", "fer\u00b7nen", "Wi\u00b7en", "das", "H\u00e4uf\u00b7lein", "mei\u00b7ner", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Geliebten in Apoll zuweilen nach mir sehnt?", "tokens": ["Ge\u00b7lieb\u00b7ten", "in", "A\u00b7poll", "zu\u00b7wei\u00b7len", "nach", "mir", "sehnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ob ich noch werth euch bin? Freund! diese Zweifel wandern", "tokens": ["Ob", "ich", "noch", "werth", "euch", "bin", "?", "Freund", "!", "die\u00b7se", "Zwei\u00b7fel", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VAFIN", "$.", "NN", "$.", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Oft bang mit mir umher, wenn ich mit irrem Fuss", "tokens": ["Oft", "bang", "mit", "mir", "um\u00b7her", ",", "wenn", "ich", "mit", "ir\u00b7rem", "Fuss"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch einsames Gestr\u00e4uch im Abendglanze walle:", "tokens": ["Durch", "ein\u00b7sa\u00b7mes", "Ge\u00b7str\u00e4uch", "im", "A\u00b7bend\u00b7glan\u00b7ze", "wal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Doch traulich lispelt mir der holde Genius", "tokens": ["Doch", "trau\u00b7lich", "lis\u00b7pelt", "mir", "der", "hol\u00b7de", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Der Freundschaft in das Ohr: sie lieben dich noch alle.", "tokens": ["Der", "Freund\u00b7schaft", "in", "das", "Ohr", ":", "sie", "lie\u00b7ben", "dich", "noch", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie lieben, wiederholt, des Herzens Lustgef\u00fchl", "tokens": ["Sie", "lie\u00b7ben", ",", "wie\u00b7der\u00b7holt", ",", "des", "Her\u00b7zens", "Lust\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verk\u00fcndigend, mein Mund, sie lieben dich noch immer,", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7gend", ",", "mein", "Mund", ",", "sie", "lie\u00b7ben", "dich", "noch", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Und rasch ergreif' ich dann den tr\u00e4gen Dichterkiel,", "tokens": ["Und", "rasch", "er\u00b7greif'", "ich", "dann", "den", "tr\u00e4\u00b7gen", "Dich\u00b7ter\u00b7kiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um euch auch meinerseits, dass eher Titans Schimmer,", "tokens": ["Um", "euch", "auch", "mei\u00b7ner\u00b7seits", ",", "dass", "e\u00b7her", "Ti\u00b7tans", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "ADV", "$,", "KOUS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als meiner Liebe Glut, f\u00fcr euch erl\u00f6schen soll,", "tokens": ["Als", "mei\u00b7ner", "Lie\u00b7be", "Glut", ",", "f\u00fcr", "euch", "er\u00b7l\u00f6\u00b7schen", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So gut ich's noch vermag, in Reimen zu betheuern:", "tokens": ["So", "gut", "ich's", "noch", "ver\u00b7mag", ",", "in", "Rei\u00b7men", "zu", "be\u00b7theu\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "ADV", "VVFIN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch spr\u00f6de weigert sich der Versegott Apoll,", "tokens": ["Doch", "spr\u00f6\u00b7de", "wei\u00b7gert", "sich", "der", "Ver\u00b7se\u00b7gott", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PRF", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Vom Aktenstaub verscheucht, mich Armen zu befeuern,", "tokens": ["Vom", "Ak\u00b7ten\u00b7staub", "ver\u00b7scheucht", ",", "mich", "Ar\u00b7men", "zu", "be\u00b7feu\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und f\u00fchl' ich manchmal auch den Einfluss seiner Macht,", "tokens": ["Und", "f\u00fchl'", "ich", "manch\u00b7mal", "auch", "den", "Ein\u00b7fluss", "sei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So str\u00e4ubt die Sprache sich. Statt munterer Troch\u00e4en", "tokens": ["So", "str\u00e4ubt", "die", "Spra\u00b7che", "sich", ".", "Statt", "mun\u00b7te\u00b7rer", "Troc\u00b7h\u00e4\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$.", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Naht meinem Pulte sich phlegmatisch, voll Bedacht,", "tokens": ["Naht", "mei\u00b7nem", "Pul\u00b7te", "sich", "phleg\u00b7ma\u00b7tisch", ",", "voll", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PRF", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ein gravit\u00e4tisch Paar schwerf\u00e4lliger Spond\u00e4en:", "tokens": ["Ein", "gra\u00b7vi\u00b7t\u00e4\u00b7tisch", "Paar", "schwer\u00b7f\u00e4l\u00b7li\u00b7ger", "Spon\u00b7d\u00e4\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Statt eines Anap\u00e4sts hinkt langsam, wie ein Dachs,", "tokens": ["Statt", "ei\u00b7nes", "A\u00b7na\u00b7p\u00e4sts", "hinkt", "lang\u00b7sam", ",", "wie", "ein", "Dachs", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit seinem dicken Wanst mir ein Moloss entgegen,", "tokens": ["Mit", "sei\u00b7nem", "di\u00b7cken", "Wanst", "mir", "ein", "Mo\u00b7loss", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und fl\u00fcchtig, wie ein Reh, h\u00fcpft statt des Amphybrachs", "tokens": ["Und", "fl\u00fcch\u00b7tig", ",", "wie", "ein", "Reh", ",", "h\u00fcpft", "statt", "des", "Am\u00b7phy\u00b7brachs"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein Daktylus herbey, und machet mich verlegen.", "tokens": ["Ein", "Dak\u00b7ty\u00b7lus", "her\u00b7bey", ",", "und", "ma\u00b7chet", "mich", "ver\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kurz, Klio h\u00e4lt nicht mehr mich eines Blickes werth,", "tokens": ["Kurz", ",", "Klio", "h\u00e4lt", "nicht", "mehr", "mich", "ei\u00b7nes", "Bli\u00b7ckes", "werth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NE", "VVFIN", "PTKNEG", "ADV", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Und wag' ich's, ohne sie ein Lied euch darzubringen,", "tokens": ["Und", "wag'", "ich's", ",", "oh\u00b7ne", "sie", "ein", "Lied", "euch", "dar\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUI", "PPER", "ART", "NN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So widersetzet sich das stolze Fl\u00fcgelpferd,", "tokens": ["So", "wi\u00b7der\u00b7set\u00b7zet", "sich", "das", "stol\u00b7ze", "Fl\u00fc\u00b7gel\u00b7pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sich mit der schlechten Fracht zu euch hinabzuschwingen.", "tokens": ["Sich", "mit", "der", "schlech\u00b7ten", "Fracht", "zu", "euch", "hin\u00b7ab\u00b7zu\u00b7schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Missm\u00fcthig w\u00fcnsch' ich dann das leidige Gesch\u00e4ft,", "tokens": ["Miss\u00b7m\u00fct\u00b7hig", "w\u00fcn\u00b7sch'", "ich", "dann", "das", "lei\u00b7di\u00b7ge", "Ge\u00b7sch\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vergeblich stundenlang mit Sylben mich zu balgen,", "tokens": ["Ver\u00b7geb\u00b7lich", "stun\u00b7den\u00b7lang", "mit", "Syl\u00b7ben", "mich", "zu", "bal\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Samt der erlauchten Schaar der Musen, die mich \u00e4fft,", "tokens": ["Samt", "der", "er\u00b7lauch\u00b7ten", "Schaar", "der", "Mu\u00b7sen", ",", "die", "mich", "\u00e4fft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(apoll vergebe mir die L\u00e4strung!) an den Galgen,", "tokens": ["(", "a\u00b7poll", "ver\u00b7ge\u00b7be", "mir", "die", "L\u00e4st\u00b7rung", "!", ")", "an", "den", "Gal\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Vernichte, was ich erst mit M\u00fch und Schweiss ersann,", "tokens": ["Ver\u00b7nich\u00b7te", ",", "was", "ich", "erst", "mit", "M\u00fch", "und", "Schweiss", "er\u00b7sann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und w\u00fcnsche sehnsuchtsvoll mir bald Medeens Drachen,", "tokens": ["Und", "w\u00fcn\u00b7sche", "sehn\u00b7suchts\u00b7voll", "mir", "bald", "Me\u00b7de\u00b7ens", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mit denen sie Korinths verhasster Burg entrann,", "tokens": ["Mit", "de\u00b7nen", "sie", "Ko\u00b7rinths", "ver\u00b7hass\u00b7ter", "Burg", "ent\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bald D\u00e4dals Fittige, bald Blanchards Zaubernachen,", "tokens": ["Bald", "D\u00e4\u00b7dals", "Fit\u00b7ti\u00b7ge", ",", "bald", "Blan\u00b7chards", "Zau\u00b7ber\u00b7na\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der dreist den Ocean des \u00c4thers rings durchkreutzt,", "tokens": ["Der", "dreist", "den", "O\u00b7cean", "des", "\u00c4\u00b7thers", "rings", "durch\u00b7kreutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Um pl\u00f6tzlich \u00fcber Berg' und H\u00fcgel fern von hinnen,", "tokens": ["Um", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7ber", "Ber\u00b7g'", "und", "H\u00fc\u00b7gel", "fern", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "NN", "KON", "NN", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wo keines Freundes Lied mich zu Ges\u00e4ngen reitzt,", "tokens": ["Wo", "kei\u00b7nes", "Freun\u00b7des", "Lied", "mich", "zu", "Ge\u00b7s\u00e4n\u00b7gen", "reitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu euch, ihr G\u00fcnstlinge der Dichtkunst, zu entrinnen,", "tokens": ["Zu", "euch", ",", "ihr", "G\u00fcnst\u00b7lin\u00b7ge", "der", "Dicht\u00b7kunst", ",", "zu", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Zu euch, in deren Kreis auch ich den s\u00fcssen Hang", "tokens": ["Zu", "euch", ",", "in", "de\u00b7ren", "Kreis", "auch", "ich", "den", "s\u00fcs\u00b7sen", "Hang"], "token_info": ["word", "word", "punct", "word", "wor