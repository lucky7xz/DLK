{"dta.poem.14329": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an meinen Bruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df fahren die Tr\u00e4ume!", "tokens": ["La\u00df", "fah\u00b7ren", "die", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Mond scheint durch B\u00e4ume,", "tokens": ["Der", "Mond", "scheint", "durch", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die W\u00e4lder nur rauschen,", "tokens": ["Die", "W\u00e4l\u00b7der", "nur", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Th\u00e4ler still lauschen,", "tokens": ["Die", "Th\u00e4\u00b7ler", "still", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}