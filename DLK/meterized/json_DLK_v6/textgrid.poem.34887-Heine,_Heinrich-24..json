{"textgrid.poem.34887": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk ich an Deutschland in der Nacht,", "tokens": ["Denk", "ich", "an", "Deutschland", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Dann bin ich um den Schlaf gebracht,", "tokens": ["Dann", "bin", "ich", "um", "den", "Schlaf", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kann nicht mehr die Augen schlie\u00dfen.", "tokens": ["Ich", "kann", "nicht", "mehr", "die", "Au\u00b7gen", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine hei\u00dfen Tr\u00e4nen flie\u00dfen.", "tokens": ["Und", "mei\u00b7ne", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Jahre kommen und vergehn!", "tokens": ["Die", "Jah\u00b7re", "kom\u00b7men", "und", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit ich die Mutter nicht gesehn,", "tokens": ["Seit", "ich", "die", "Mut\u00b7ter", "nicht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zw\u00f6lf Jahre sind schon hingegangen;", "tokens": ["Zw\u00f6lf", "Jah\u00b7re", "sind", "schon", "hin\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es w\u00e4chst mein Sehnen und Verlangen.", "tokens": ["Es", "w\u00e4chst", "mein", "Seh\u00b7nen", "und", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Sehnen und Verlangen w\u00e4chst.", "tokens": ["Mein", "Seh\u00b7nen", "und", "Ver\u00b7lan\u00b7gen", "w\u00e4chst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die alte Frau hat mich behext,", "tokens": ["Die", "al\u00b7te", "Frau", "hat", "mich", "be\u00b7hext", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich denke immer an die alte,", "tokens": ["Ich", "den\u00b7ke", "im\u00b7mer", "an", "die", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die alte Frau, die Gott erhalte!", "tokens": ["Die", "al\u00b7te", "Frau", ",", "die", "Gott", "er\u00b7hal\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die alte Frau hat mich so lieb,", "tokens": ["Die", "al\u00b7te", "Frau", "hat", "mich", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in den Briefen, die sie schrieb,", "tokens": ["Und", "in", "den", "Brie\u00b7fen", ",", "die", "sie", "schrieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh ich, wie ihre Hand gezittert,", "tokens": ["Seh", "ich", ",", "wie", "ih\u00b7re", "Hand", "ge\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie tief das Mutterherz ersch\u00fcttert.", "tokens": ["Wie", "tief", "das", "Mut\u00b7ter\u00b7herz", "er\u00b7sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Mutter liegt mir stets im Sinn.", "tokens": ["Die", "Mut\u00b7ter", "liegt", "mir", "stets", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zw\u00f6lf lange Jahre flossen hin,", "tokens": ["Zw\u00f6lf", "lan\u00b7ge", "Jah\u00b7re", "flos\u00b7sen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zw\u00f6lf lange Jahre sind verflossen,", "tokens": ["Zw\u00f6lf", "lan\u00b7ge", "Jah\u00b7re", "sind", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit ich sie nicht ans Herz geschlossen.", "tokens": ["Seit", "ich", "sie", "nicht", "ans", "Herz", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Deutschland hat ewigen Bestand,", "tokens": ["Deutschland", "hat", "e\u00b7wi\u00b7gen", "Be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es ist ein kerngesundes Land;", "tokens": ["Es", "ist", "ein", "kern\u00b7ge\u00b7sun\u00b7des", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seinen Eichen, seinen Linden,", "tokens": ["Mit", "sei\u00b7nen", "Ei\u00b7chen", ",", "sei\u00b7nen", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werd ich es immer wiederfinden.", "tokens": ["Werd", "ich", "es", "im\u00b7mer", "wie\u00b7der\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nach Deutschland lechzt' ich nicht so sehr,", "tokens": ["Nach", "Deutschland", "lechzt'", "ich", "nicht", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn nicht die Mutter dorten w\u00e4r;", "tokens": ["Wenn", "nicht", "die", "Mut\u00b7ter", "dor\u00b7ten", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Vaterland wird nie verderben,", "tokens": ["Das", "Va\u00b7ter\u00b7land", "wird", "nie", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedoch die alte Frau kann sterben.", "tokens": ["Je\u00b7doch", "die", "al\u00b7te", "Frau", "kann", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Seit ich das Land verlassen hab,", "tokens": ["Seit", "ich", "das", "Land", "ver\u00b7las\u00b7sen", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So viele sanken dort ins Grab,", "tokens": ["So", "vie\u00b7le", "san\u00b7ken", "dort", "ins", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ich geliebt \u2013 wenn ich sie z\u00e4hle,", "tokens": ["Die", "ich", "ge\u00b7liebt", "\u2013", "wenn", "ich", "sie", "z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$(", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So will verbluten meine Seele.", "tokens": ["So", "will", "ver\u00b7blu\u00b7ten", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und z\u00e4hlen mu\u00df ich \u2013 Mit der Zahl", "tokens": ["Und", "z\u00e4h\u00b7len", "mu\u00df", "ich", "\u2013", "Mit", "der", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwillt immer h\u00f6her meine Qual,", "tokens": ["Schwillt", "im\u00b7mer", "h\u00f6\u00b7her", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist, als w\u00e4lzten sich die Leichen", "tokens": ["Mir", "ist", ",", "als", "w\u00e4lz\u00b7ten", "sich", "die", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf meine Brust \u2013 Gottlob! sie weichen!", "tokens": ["Auf", "mei\u00b7ne", "Brust", "\u2013", "Gott\u00b7lob", "!", "sie", "wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "NN", "$.", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Gottlob! durch meine Fenster bricht", "tokens": ["Gott\u00b7lob", "!", "durch", "mei\u00b7ne", "Fens\u00b7ter", "bricht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Franz\u00f6sisch heitres Tageslicht;", "tokens": ["Fran\u00b7z\u00f6\u00b7sisch", "heit\u00b7res", "Ta\u00b7ges\u00b7licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Es kommt mein Weib, sch\u00f6n wie der Morgen,", "tokens": ["Es", "kommt", "mein", "Weib", ",", "sch\u00f6n", "wie", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4chelt fort die deutschen Sorgen.", "tokens": ["Und", "l\u00e4\u00b7chelt", "fort", "die", "deut\u00b7schen", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}