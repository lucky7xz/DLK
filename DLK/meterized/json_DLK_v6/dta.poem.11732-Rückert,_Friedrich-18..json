{"dta.poem.11732": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lf Jahre war ich alt, da hatt' ich ohne Flei\u00df", "tokens": ["Zw\u00f6lf", "Jah\u00b7re", "war", "ich", "alt", ",", "da", "hatt'", "ich", "oh\u00b7ne", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fast alles und noch mehr gelernt, als ich nun wei\u00df.", "tokens": ["Fast", "al\u00b7les", "und", "noch", "mehr", "ge\u00b7lernt", ",", "als", "ich", "nun", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KON", "ADV", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich hatte schon die Frucht, wovon den Ruhm nun haben", "tokens": ["Ich", "hat\u00b7te", "schon", "die", "Frucht", ",", "wo\u00b7von", "den", "Ruhm", "nun", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "ART", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manch andre, die zuerst ans Licht der Welt sie gaben.", "tokens": ["Manch", "and\u00b7re", ",", "die", "zu\u00b7erst", "ans", "Licht", "der", "Welt", "sie", "ga\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "$,", "PRELS", "ADV", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und r\u00fchm' ich dessen mich? Ich r\u00fchme nur die Zeit,", "tokens": ["Und", "r\u00fchm'", "ich", "des\u00b7sen", "mich", "?", "Ich", "r\u00fch\u00b7me", "nur", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "PPER", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deren neuen Trieb das Neu' allein gedeiht.", "tokens": ["Durch", "de\u00b7ren", "neu\u00b7en", "Trieb", "das", "Neu'", "al\u00b7lein", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gedanken kommen wie des Fr\u00fchlings goldner Duft,", "tokens": ["Ge\u00b7dan\u00b7ken", "kom\u00b7men", "wie", "des", "Fr\u00fch\u00b7lings", "gold\u00b7ner", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sind nicht mein noch dein, sie schwimmen in der Luft.", "tokens": ["Sie", "sind", "nicht", "mein", "noch", "dein", ",", "sie", "schwim\u00b7men", "in", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPOSAT", "ADV", "PPOSAT", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sei dankbar, da\u00df die Welt so reich dir dargeboten", "tokens": ["Sei", "dank\u00b7bar", ",", "da\u00df", "die", "Welt", "so", "reich", "dir", "dar\u00b7ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des besten Wissens Schatz von Lebenden und Todten.", "tokens": ["Des", "bes\u00b7ten", "Wis\u00b7sens", "Schatz", "von", "Le\u00b7ben\u00b7den", "und", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du hast ihn nicht gesucht, du hast ihn nur gefunden;", "tokens": ["Du", "hast", "ihn", "nicht", "ge\u00b7sucht", ",", "du", "hast", "ihn", "nur", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun spend' ihn liebend aus und sei der Welt verbunden.", "tokens": ["Nun", "spend'", "ihn", "lie\u00b7bend", "aus", "und", "sei", "der", "Welt", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}