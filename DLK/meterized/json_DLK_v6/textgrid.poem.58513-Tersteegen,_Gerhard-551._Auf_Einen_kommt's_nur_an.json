{"textgrid.poem.58513": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "551. Auf Einen kommt's nur an", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab' ich ", "tokens": ["Hab'", "ich"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "++", "measure": "spondeus"}, "line.2": {"text": "K\u00f6nnen tausend ", "tokens": ["K\u00f6n\u00b7nen", "tau\u00b7send"], "token_info": ["word", "word"], "pos": ["NN", "CARD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Alle Feinde um mich her schaden gleichfalls nicht,", "tokens": ["Al\u00b7le", "Fein\u00b7de", "um", "mich", "her", "scha\u00b7den", "gleich\u00b7falls", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PRF", "APZR", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wenn mein Freund, der drinnen nah, mich zufrieden spricht.", "tokens": ["Wenn", "mein", "Freund", ",", "der", "drin\u00b7nen", "nah", ",", "mich", "zu\u00b7frie\u00b7den", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}