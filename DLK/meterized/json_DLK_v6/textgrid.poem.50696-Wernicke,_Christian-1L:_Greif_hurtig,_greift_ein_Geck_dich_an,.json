{"textgrid.poem.50696": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Greif hurtig, greift ein Geck dich an,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Greif hurtig, greift ein Geck dich an,", "tokens": ["Greif", "hur\u00b7tig", ",", "greift", "ein", "Geck", "dich", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Feder oder zum Gewehr;", "tokens": ["Zur", "Fe\u00b7der", "o\u00b7der", "zum", "Ge\u00b7wehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wer den Schimpf verachten kann,", "tokens": ["Denn", "wer", "den", "Schimpf", "ver\u00b7ach\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mu\u00df viel besser sein, als er.", "tokens": ["Der", "mu\u00df", "viel", "bes\u00b7ser", "sein", ",", "als", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "VAINF", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}