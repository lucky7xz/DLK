{"textgrid.poem.37263": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es sprach der Fritz zu dem Papa]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sprach der Fritz zu dem Papa:", "tokens": ["Es", "sprach", "der", "Fritz", "zu", "dem", "Pa\u00b7pa", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sie nur wieder hat?", "tokens": ["Was", "sie", "nur", "wie\u00b7der", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch gestern sagte mir Mama:", "tokens": ["Noch", "ge\u00b7stern", "sag\u00b7te", "mir", "Ma\u00b7ma", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Du f\u00e4hrst mit in die Stadt.", "tokens": ["Du", "f\u00e4hrst", "mit", "in", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich hatte mich schon so gefreut", "tokens": ["Ich", "hat\u00b7te", "mich", "schon", "so", "ge\u00b7freut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und war so voll Pl\u00e4sier.", "tokens": ["Und", "war", "so", "voll", "Pl\u00e4\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun soll ich doch nicht mit, denn heut", "tokens": ["Nun", "soll", "ich", "doch", "nicht", "mit", ",", "denn", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da hei\u00dft es: Fritz bleibt hier!", "tokens": ["Da", "hei\u00dft", "es", ":", "Fritz", "bleibt", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Vater sa\u00df im Sorgensitz.", "tokens": ["Der", "Va\u00b7ter", "sa\u00df", "im", "Sor\u00b7gen\u00b7sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sagte ernst und still:", "tokens": ["Er", "sag\u00b7te", "ernst", "und", "still", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trau Langhals nicht, mein lieber Fritz,", "tokens": ["Trau", "Lang\u00b7hals", "nicht", ",", "mein", "lie\u00b7ber", "Fritz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "$,", "PPOSAT", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hustet, wann er will!", "tokens": ["Der", "hus\u00b7tet", ",", "wann", "er", "will", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}