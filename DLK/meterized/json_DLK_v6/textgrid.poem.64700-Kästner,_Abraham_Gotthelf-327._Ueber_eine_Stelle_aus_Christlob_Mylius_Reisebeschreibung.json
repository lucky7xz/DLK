{"textgrid.poem.64700": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "327. Ueber eine Stelle aus Christlob Mylius Reisebeschreibung", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus einem Paradies voll Ochsen wollt' er fliehn:", "tokens": ["Aus", "ei\u00b7nem", "Pa\u00b7ra\u00b7dies", "voll", "Och\u00b7sen", "wollt'", "er", "fliehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was aber h\u00e4tt' er angefangen,", "tokens": ["Was", "a\u00b7ber", "h\u00e4tt'", "er", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00e4m' er, durch seinen Ruhm und eifriges Bem\u00fchn,", "tokens": ["K\u00e4m'", "er", ",", "durch", "sei\u00b7nen", "Ruhm", "und", "eif\u00b7ri\u00b7ges", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In ein Sibirien voll Rinder, Schwein' und Schlangen?", "tokens": ["In", "ein", "Si\u00b7bi\u00b7ri\u00b7en", "voll", "Rin\u00b7der", ",", "Schwein'", "und", "Schlan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}