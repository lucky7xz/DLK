{"textgrid.poem.67217": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ungesehn und nun vor\u00fcber sind die Bilder", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ungesehn und nun vor\u00fcber sind die Bilder", "tokens": ["Wenn", "un\u00b7ge\u00b7sehn", "und", "nun", "vor\u00b7\u00fc\u00b7ber", "sind", "die", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Jahreszeit, so kommt des Winters Dauer,", "tokens": ["Der", "Jah\u00b7res\u00b7zeit", ",", "so", "kommt", "des", "Win\u00b7ters", "Dau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Feld ist leer, die Ansicht scheinet milder,", "tokens": ["Das", "Feld", "ist", "leer", ",", "die", "An\u00b7sicht", "schei\u00b7net", "mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und St\u00fcrme wehn umher und Regenschauer.", "tokens": ["Und", "St\u00fcr\u00b7me", "wehn", "um\u00b7her", "und", "Re\u00b7gen\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als wie ein Ruhetag, so ist des Jahres Ende,", "tokens": ["Als", "wie", "ein", "Ru\u00b7he\u00b7tag", ",", "so", "ist", "des", "Jah\u00b7res", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie einer Frage Ton, da\u00df dieser sich vollende,", "tokens": ["Wie", "ei\u00b7ner", "Fra\u00b7ge", "Ton", ",", "da\u00df", "die\u00b7ser", "sich", "voll\u00b7en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "KOUS", "PDAT", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alsdann erscheint des Fr\u00fchlings neues Werden,", "tokens": ["Als\u00b7dann", "er\u00b7scheint", "des", "Fr\u00fch\u00b7lings", "neu\u00b7es", "Wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So gl\u00e4nzet die Natur mit ihrer Pracht auf Erden.", "tokens": ["So", "gl\u00e4n\u00b7zet", "die", "Na\u00b7tur", "mit", "ih\u00b7rer", "Pracht", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}