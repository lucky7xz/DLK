{"textgrid.poem.67113": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Einladung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Morgen, Bruder, ging so sch\u00f6n hervor,", "tokens": ["Dein", "Mor\u00b7gen", ",", "Bru\u00b7der", ",", "ging", "so", "sch\u00f6n", "her\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein heitres Fr\u00fchrot gl\u00e4nzte dir entgegen,", "tokens": ["Ein", "heit\u00b7res", "Fr\u00fch\u00b7rot", "gl\u00e4nz\u00b7te", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Den wonnevollsten Lebenstag verhei\u00dfend.", "tokens": ["Den", "won\u00b7ne\u00b7volls\u00b7ten", "Le\u00b7bens\u00b7tag", "ver\u00b7hei\u00b7\u00dfend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Musen weihten dich zu ihrem Priester,", "tokens": ["Die", "Mu\u00b7sen", "weih\u00b7ten", "dich", "zu", "ih\u00b7rem", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe kr\u00e4nzte dir das Haupt mit Rosen,", "tokens": ["Die", "Lie\u00b7be", "kr\u00e4nz\u00b7te", "dir", "das", "Haupt", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und go\u00df die reinsten Freuden in dein Herz.", "tokens": ["Und", "go\u00df", "die", "reins\u00b7ten", "Freu\u00b7den", "in", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer war wie du begl\u00fcckt? Das Schicksal hat", "tokens": ["Wer", "war", "wie", "du", "be\u00b7gl\u00fcckt", "?", "Das", "Schick\u00b7sal", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "KOKOM", "PPER", "VVPP", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es anders nun gemacht; ein schwarzer Sturm", "tokens": ["Es", "an\u00b7ders", "nun", "ge\u00b7macht", ";", "ein", "schwar\u00b7zer", "Sturm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verschlang des Tages Licht; der Donner rollte", "tokens": ["Ver\u00b7schlang", "des", "Ta\u00b7ges", "Licht", ";", "der", "Don\u00b7ner", "roll\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und traf dein sichres Haupt; im Grabe liegt,", "tokens": ["Und", "traf", "dein", "sich\u00b7res", "Haupt", ";", "im", "Gra\u00b7be", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Was du geliebt; dein Eden ist vernichtet.", "tokens": ["Was", "du", "ge\u00b7liebt", ";", "dein", "E\u00b7den", "ist", "ver\u00b7nich\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Bruder, Bruder, da\u00df dein Schicksal mir", "tokens": ["O", "Bru\u00b7der", ",", "Bru\u00b7der", ",", "da\u00df", "dein", "Schick\u00b7sal", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "KOUS", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So schrecklichwahr des Lebens Wechsel deutet!", "tokens": ["So", "schreck\u00b7lich\u00b7wahr", "des", "Le\u00b7bens", "Wech\u00b7sel", "deu\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Disteln hinter Blumeng\u00e4ngen lauern,", "tokens": ["Da\u00df", "Dis\u00b7teln", "hin\u00b7ter", "Blu\u00b7men\u00b7g\u00e4n\u00b7gen", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df giftger Tod in Jugendadern schleicht,", "tokens": ["Da\u00df", "gift\u00b7ger", "Tod", "in", "Ju\u00b7gen\u00b7da\u00b7dern", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df bittre Trennung selbst den Freunden oft", "tokens": ["Da\u00df", "bitt\u00b7re", "Tren\u00b7nung", "selbst", "den", "Freun\u00b7den", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den armen Trost versagt, den Schmerz zu teilen!", "tokens": ["Den", "ar\u00b7men", "Trost", "ver\u00b7sagt", ",", "den", "Schmerz", "zu", "tei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da baun wir Plane, tr\u00e4umen so entz\u00fcckt", "tokens": ["Da", "baun", "wir", "Pla\u00b7ne", ",", "tr\u00e4u\u00b7men", "so", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vom nahen Ziel, und pl\u00f6tzlich, pl\u00f6tzlich zuckt", "tokens": ["Vom", "na\u00b7hen", "Ziel", ",", "und", "pl\u00f6tz\u00b7lich", ",", "pl\u00f6tz\u00b7lich", "zuckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "ADJD", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Blitz herab, und \u00f6ffnet uns das Grab.", "tokens": ["Ein", "Blitz", "her\u00b7ab", ",", "und", "\u00f6ff\u00b7net", "uns", "das", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich sah im Geist dein Leiden all. Da ging", "tokens": ["Ich", "sah", "im", "Geist", "dein", "Lei\u00b7den", "all", ".", "Da", "ging"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "PIAT", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich tr\u00fcben Blicks hinab am Maingestade,", "tokens": ["Ich", "tr\u00fc\u00b7ben", "Blicks", "hin\u00b7ab", "am", "Main\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sah in die Wogen, bis mir schwindelte,", "tokens": ["Sah", "in", "die", "Wo\u00b7gen", ",", "bis", "mir", "schwin\u00b7del\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und kehrte still und voll der dunkeln Zukunft,", "tokens": ["Und", "kehr\u00b7te", "still", "und", "voll", "der", "dun\u00b7keln", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und voll des Schicksals, welches unser wartet,", "tokens": ["Und", "voll", "des", "Schick\u00b7sals", ",", "wel\u00b7ches", "un\u00b7ser", "war\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Beim Untergang der Sonn in meine Klause.", "tokens": ["Beim", "Un\u00b7ter\u00b7gang", "der", "Sonn", "in", "mei\u00b7ne", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O Bruder, komm nach jahrelanger Trennung", "tokens": ["O", "Bru\u00b7der", ",", "komm", "nach", "jah\u00b7re\u00b7lan\u00b7ger", "Tren\u00b7nung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "An meine Brust! Vielleicht gelingt es uns,", "tokens": ["An", "mei\u00b7ne", "Brust", "!", "Viel\u00b7leicht", "ge\u00b7lingt", "es", "uns", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch einen jener sch\u00f6nen Abende,", "tokens": ["Noch", "ei\u00b7nen", "je\u00b7ner", "sch\u00f6\u00b7nen", "A\u00b7ben\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die wir so oft am Herzen der Natur", "tokens": ["Die", "wir", "so", "oft", "am", "Her\u00b7zen", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit reinem Sinn und mit Gesang gefeiert,", "tokens": ["Mit", "rei\u00b7nem", "Sinn", "und", "mit", "Ge\u00b7sang", "ge\u00b7fei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur\u00fcck zu zaubern, und noch einmal froh", "tokens": ["Zu\u00b7r\u00fcck", "zu", "zau\u00b7bern", ",", "und", "noch", "ein\u00b7mal", "froh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$,", "KON", "ADV", "ADV", "ADJD"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Hinein zu schauen in das Leben! Komm,", "tokens": ["Hin\u00b7ein", "zu", "schau\u00b7en", "in", "das", "Le\u00b7ben", "!", "Komm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "APPR", "ART", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es wartet dein ein eigen Deckelglas,", "tokens": ["Es", "war\u00b7tet", "dein", "ein", "ei\u00b7gen", "De\u00b7ckel\u00b7glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Stiefm\u00fctterlich soll nicht mein F\u00e4\u00dfchen flie\u00dfen.", "tokens": ["Stief\u00b7m\u00fct\u00b7ter\u00b7lich", "soll", "nicht", "mein", "F\u00e4\u00df\u00b7chen", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es wartet dein ein freundliches Gemach,", "tokens": ["Es", "war\u00b7tet", "dein", "ein", "freund\u00b7li\u00b7ches", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wo unsre Herzen liebend sich ergie\u00dfen!", "tokens": ["Wo", "uns\u00b7re", "Her\u00b7zen", "lie\u00b7bend", "sich", "er\u00b7gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Komm, eh der Herbst der G\u00e4rten Schmuck verderbt,", "tokens": ["Komm", ",", "eh", "der", "Herbst", "der", "G\u00e4r\u00b7ten", "Schmuck", "ver\u00b7derbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Bevor die sch\u00f6nen Tage von uns eilen,", "tokens": ["Be\u00b7vor", "die", "sch\u00f6\u00b7nen", "Ta\u00b7ge", "von", "uns", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und la\u00df durch Freundschaft uns des Herzens Wunden heilen.", "tokens": ["Und", "la\u00df", "durch", "Freund\u00b7schaft", "uns", "des", "Her\u00b7zens", "Wun\u00b7den", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "NN", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}