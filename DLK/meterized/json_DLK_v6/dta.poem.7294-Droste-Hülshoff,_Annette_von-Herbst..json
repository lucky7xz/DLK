{"dta.poem.7294": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich an einem sch\u00f6nen Tag", "tokens": ["Wenn", "ich", "an", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mittagsstunde habe Acht,", "tokens": ["Der", "Mit\u00b7tags\u00b7stun\u00b7de", "ha\u00b7be", "Acht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lehne unter meinem Baum", "tokens": ["Und", "leh\u00b7ne", "un\u00b7ter", "mei\u00b7nem", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So mitten in der Trauben Pracht:", "tokens": ["So", "mit\u00b7ten", "in", "der", "Trau\u00b7ben", "Pracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Zeitlose \u00fcber's Thal", "tokens": ["Wenn", "die", "Zeit\u00b7lo\u00b7se", "\u00fc\u00b7ber's", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den amethystnen Teppich webt,", "tokens": ["Den", "a\u00b7met\u00b7hyst\u00b7nen", "Tep\u00b7pich", "webt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dem der letzte Schmetterling", "tokens": ["Auf", "dem", "der", "letz\u00b7te", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schillernd wie der fr\u00fchste bebt:", "tokens": ["So", "schil\u00b7lernd", "wie", "der", "fr\u00fchs\u00b7te", "bebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann denk' ich wenig dr\u00fcber nach,", "tokens": ["Dann", "denk'", "ich", "we\u00b7nig", "dr\u00fc\u00b7ber", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie's nun verk\u00fcmmert Tag f\u00fcr Tag,", "tokens": ["Wie's", "nun", "ver\u00b7k\u00fcm\u00b7mert", "Tag", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kann mit halbverschlossnem Blick", "tokens": ["Und", "kann", "mit", "halb\u00b7ver\u00b7schloss\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Lenze tr\u00e4umen und von Gl\u00fcck.", "tokens": ["Vom", "Len\u00b7ze", "tr\u00e4u\u00b7men", "und", "von", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du mit dem frischgefall'nen Schnee,", "tokens": ["Du", "mit", "dem", "frischge\u00b7fall'\u00b7nen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Du thust mir in den Augen weh!", "tokens": ["Du", "thust", "mir", "in", "den", "Au\u00b7gen", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst uns den Winter schon bereiten:", "tokens": ["Willst", "uns", "den", "Win\u00b7ter", "schon", "be\u00b7rei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Schlucht zu Schlucht sieht man ihn gleiten,", "tokens": ["Von", "Schlucht", "zu", "Schlucht", "sieht", "man", "ihn", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und bald, bald w\u00e4lzt er sich herab", "tokens": ["Und", "bald", ",", "bald", "w\u00e4lzt", "er", "sich", "her\u00b7ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von dir, o S\u00e4ntis! \u00f6des Grab!", "tokens": ["Von", "dir", ",", "o", "S\u00e4n\u00b7tis", "!", "\u00f6\u00b7des", "Grab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "FM", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}