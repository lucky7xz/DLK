{"dta.poem.16329": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn etwas Sch\u00f6nes f\u00fcr mich selbst und f\u00fcr die Welt", "tokens": ["Wenn", "et\u00b7was", "Sch\u00f6\u00b7nes", "f\u00fcr", "mich", "selbst", "und", "f\u00fcr", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "APPR", "PPER", "ADV", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In mir geworden ist den K\u00e4mpfen zum Vergelt,", "tokens": ["In", "mir", "ge\u00b7wor\u00b7den", "ist", "den", "K\u00e4mp\u00b7fen", "zum", "Ver\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAPP", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die ich gek\u00e4mpft, so will ich gern gek\u00e4mpft sie haben,", "tokens": ["Die", "ich", "ge\u00b7k\u00e4mpft", ",", "so", "will", "ich", "gern", "ge\u00b7k\u00e4mpft", "sie", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVPP", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und m\u00f6gen sich mit mir am Sch\u00f6nen viele laben!", "tokens": ["Und", "m\u00f6\u00b7gen", "sich", "mit", "mir", "am", "Sch\u00f6\u00b7nen", "vie\u00b7le", "la\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPER", "APPRART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch manchmal denk' ich, ob nicht sei erkauft zu theuer", "tokens": ["Doch", "manch\u00b7mal", "denk'", "ich", ",", "ob", "nicht", "sei", "er\u00b7kauft", "zu", "theu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "VAFIN", "VVPP", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Bischen lautres Gold f\u00fcr soviel L\u00e4utrungsfeuer.", "tokens": ["Ein", "Bi\u00b7schen", "laut\u00b7res", "Gold", "f\u00fcr", "so\u00b7viel", "L\u00e4ut\u00b7rungs\u00b7feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}