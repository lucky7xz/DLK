{"textgrid.poem.45381": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[bescheiden will ich ihm erscheinen?]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bescheiden will ich ihm erscheinen?", "tokens": ["Be\u00b7schei\u00b7den", "will", "ich", "ihm", "er\u00b7schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kanns nicht lassen gelten.", "tokens": ["Ich", "kanns", "nicht", "las\u00b7sen", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Routin und Kunst sind zwei geschiedne Welten.", "tokens": ["Rou\u00b7tin", "und", "Kunst", "sind", "zwei", "ge\u00b7schied\u00b7ne", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "CARD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Er lese seine Dramen, dann die meinen,", "tokens": ["Er", "le\u00b7se", "sei\u00b7ne", "Dra\u00b7men", ",", "dann", "die", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erw\u00e4gen wird er dann mit weisem Sinn,", "tokens": ["Er\u00b7w\u00e4\u00b7gen", "wird", "er", "dann", "mit", "wei\u00b7sem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ob ich denn gar so sehr bescheiden bin!", "tokens": ["Ob", "ich", "denn", "gar", "so", "sehr", "be\u00b7schei\u00b7den", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}