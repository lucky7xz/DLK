{"textgrid.poem.56811": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwerer Nebel dunkle Lasten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwerer Nebel dunkle Lasten", "tokens": ["Schwe\u00b7rer", "Ne\u00b7bel", "dunk\u00b7le", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sinken von dem Schnee der K\u00e4mme", "tokens": ["sin\u00b7ken", "von", "dem", "Schnee", "der", "K\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00fcber \u00f6de Herdenrasten", "tokens": ["\u00fc\u00b7ber", "\u00f6\u00b7de", "Her\u00b7den\u00b7ras\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "in des Tannichts finstre St\u00e4mme.", "tokens": ["in", "des", "Tan\u00b7nichts", "finst\u00b7re", "St\u00e4m\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur des Baches bleiche Brandung", "tokens": ["Nur", "des", "Ba\u00b7ches", "blei\u00b7che", "Bran\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "rauscht und leuchtet noch gerettet, \u2013", "tokens": ["rauscht", "und", "leuch\u00b7tet", "noch", "ge\u00b7ret\u00b7tet", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "VVPP", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "bis die d\u00fcstre Dunstgewandung", "tokens": ["bis", "die", "d\u00fcst\u00b7re", "Dunst\u00b7ge\u00b7wan\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "endlich ihn auch \u00fcberbettet.", "tokens": ["end\u00b7lich", "ihn", "auch", "\u00fc\u00b7ber\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}