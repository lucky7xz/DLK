{"textgrid.poem.40499": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir schwebten mit vollen Segeln", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir schwebten mit vollen Segeln", "tokens": ["Wir", "schweb\u00b7ten", "mit", "vol\u00b7len", "Se\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch gr\u00fcne Meeresfluth,", "tokens": ["Durch", "gr\u00fc\u00b7ne", "Mee\u00b7res\u00b7fluth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein buntes Wanderv\u00f6lklein,", "tokens": ["Ein", "bun\u00b7tes", "Wan\u00b7der\u00b7v\u00f6l\u00b7klein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mit leichtem frohem Muth!", "tokens": ["Mit", "leich\u00b7tem", "fro\u00b7hem", "Muth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein V\u00f6lklein, wie es heute", "tokens": ["Ein", "V\u00f6l\u00b7klein", ",", "wie", "es", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wind zusammens\u00e4t,", "tokens": ["Der", "Wind", "zu\u00b7sam\u00b7men\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie er's morgen wieder", "tokens": ["Und", "wie", "er's", "mor\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Flink auseinander weht.", "tokens": ["Flink", "aus\u00b7ein\u00b7an\u00b7der", "weht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da war ein Mann aus Frankreich,", "tokens": ["Da", "war", "ein", "Mann", "aus", "Fran\u00b7kreich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Vom gr\u00fcnen Rhonestrand;", "tokens": ["Vom", "gr\u00fc\u00b7nen", "Rho\u00b7nes\u00b7trand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Goldsaaten, Rebenh\u00fcgel", "tokens": ["Gold\u00b7saa\u00b7ten", ",", "Re\u00b7ben\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nannt' er sein Vaterland.", "tokens": ["Nannt'", "er", "sein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Andrer pries als Heimat", "tokens": ["Ein", "A\u00b7ndrer", "pries", "als", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Nordens Felsenwall,", "tokens": ["Des", "Nor\u00b7dens", "Fel\u00b7sen\u00b7wall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Gletscher Skandinaviens,", "tokens": ["Die", "Glet\u00b7scher", "Skan\u00b7di\u00b7na\u00b7vi\u00b7ens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seen von Kristall.", "tokens": ["Die", "Seen", "von", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Dort wo als ew'ger Leuchtthurm", "tokens": ["Dort", "wo", "als", "ew'\u00b7ger", "Leucht\u00b7thurm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vesuv, der hohe, gl\u00fcht,", "tokens": ["Ve\u00b7suv", ",", "der", "ho\u00b7he", ",", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stand eines Dritten Wiege,", "tokens": ["Stand", "ei\u00b7nes", "Drit\u00b7ten", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Lorbern \u00fcberbl\u00fcht.", "tokens": ["Von", "Lor\u00b7bern", "\u00fc\u00b7berb\u00b7l\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In deutsche Eichenforste,", "tokens": ["In", "deut\u00b7sche", "Ei\u00b7chen\u00b7fors\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf gr\u00fcnen Alpenhang,", "tokens": ["Auf", "gr\u00fc\u00b7nen", "Al\u00b7pen\u00b7hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu frischen Au'n der Donau", "tokens": ["Zu", "fri\u00b7schen", "Au'n", "der", "Do\u00b7nau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Zog mich des Heimwehs Drang.", "tokens": ["Zog", "mich", "des", "Heim\u00b7wehs", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbla\u00dft hoch die Heimat leben!", "tokens": ["\u00bb", "la\u00dft", "hoch", "die", "Hei\u00b7mat", "le\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nehmt All' ein Glas zur Hand!", "tokens": ["Nehmt", "All'", "ein", "Glas", "zur", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht Jeder hat ein Liebchen!", "tokens": ["Nicht", "Je\u00b7der", "hat", "ein", "Lieb\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch Jeder ein Vaterland!\u00ab", "tokens": ["Doch", "Je\u00b7der", "ein", "Va\u00b7ter\u00b7land", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "ART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und Jeder trank den Becher", "tokens": ["Und", "Je\u00b7der", "trank", "den", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit flammendem Antlitz aus;", "tokens": ["Mit", "flam\u00b7men\u00b7dem", "Ant\u00b7litz", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur Einer starrte schweigend", "tokens": ["Nur", "Ei\u00b7ner", "starr\u00b7te", "schwei\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weit in die See hinaus.", "tokens": ["Weit", "in", "die", "See", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Mann war's aus Venedig,", "tokens": ["Ein", "Mann", "wa\u00b7r's", "aus", "Ve\u00b7ne\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sprach in sich hinein:", "tokens": ["Der", "sprach", "in", "sich", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Vaterland, o Heimat,", "tokens": ["\u00bb", "mein", "Va\u00b7ter\u00b7land", ",", "o", "Hei\u00b7mat", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bist nur Wasser und Stein!", "tokens": ["Du", "bist", "nur", "Was\u00b7ser", "und", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Einst glomm der Freiheit Sonne,", "tokens": ["Einst", "glomm", "der", "Frei\u00b7heit", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lebt' und sprach der Stein,", "tokens": ["Da", "lebt'", "und", "sprach", "der", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und t\u00f6nte, wie Memnon's S\u00e4ule,", "tokens": ["Und", "t\u00f6n\u00b7te", ",", "wie", "Mem\u00b7non's", "S\u00e4u\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ins Morgenroth hinein!", "tokens": ["Ins", "Mor\u00b7gen\u00b7roth", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da wogte gl\u00fchend das Wasser,", "tokens": ["Da", "wog\u00b7te", "gl\u00fc\u00b7hend", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Purpur g\u00fcrtend die Welt,", "tokens": ["Mit", "Pur\u00b7pur", "g\u00fcr\u00b7tend", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Regenbogen schleudernd", "tokens": ["Und", "Re\u00b7gen\u00b7bo\u00b7gen", "schleu\u00b7dernd"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinauf ins Himmelszelt!", "tokens": ["Hin\u00b7auf", "ins", "Him\u00b7mels\u00b7zelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Warum bist du erloschen,", "tokens": ["Wa\u00b7rum", "bist", "du", "er\u00b7lo\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du sch\u00f6ner Sonnenschein?", "tokens": ["Du", "sch\u00f6\u00b7ner", "Son\u00b7nen\u00b7schein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warum bist du, o Heimat,", "tokens": ["Wa\u00b7rum", "bist", "du", ",", "o", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt Wasser nur und Stein?\u00ab", "tokens": ["Jetzt", "Was\u00b7ser", "nur", "und", "Stein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "ADV", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er schwieg und starrte lange", "tokens": ["Er", "schwieg", "und", "starr\u00b7te", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aufs Meer hin unverwandt,", "tokens": ["Aufs", "Meer", "hin", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, unber\u00fchrt noch, gl\u00e4nzte", "tokens": ["Und", ",", "un\u00b7be\u00b7r\u00fchrt", "noch", ",", "gl\u00e4nz\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ADJD", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Glas in seiner Hand.", "tokens": ["Das", "Glas", "in", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Jetzt, wie zum Todtenopfer,", "tokens": ["Jetzt", ",", "wie", "zum", "Tod\u00b7ten\u00b7op\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Go\u00df er's hinab ins Meer!", "tokens": ["Go\u00df", "er's", "hin\u00b7ab", "ins", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie funkelnde Thr\u00e4nen stoben", "tokens": ["Wie", "fun\u00b7keln\u00b7de", "Thr\u00e4\u00b7nen", "sto\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die goldenen Tropfen umher.", "tokens": ["Die", "gol\u00b7de\u00b7nen", "Trop\u00b7fen", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}