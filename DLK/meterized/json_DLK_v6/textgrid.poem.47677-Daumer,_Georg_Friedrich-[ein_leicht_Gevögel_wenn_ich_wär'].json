{"textgrid.poem.47677": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ein leicht Gev\u00f6gel wenn ich w\u00e4r']", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein leicht Gev\u00f6gel wenn ich w\u00e4r',", "tokens": ["Ein", "leicht", "Ge\u00b7v\u00f6\u00b7gel", "wenn", "ich", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinem Munde z\u00f6g' ich,", "tokens": ["Zu", "dei\u00b7nem", "Mun\u00b7de", "z\u00f6g'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann rasch das ein' und andre Mal", "tokens": ["Dann", "rasch", "das", "ein'", "und", "and\u00b7re", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die s\u00fc\u00dfe Nippe s\u00f6g' ich,", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Nip\u00b7pe", "s\u00f6g'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann wieder rasch mit meinem Raub", "tokens": ["Dann", "wie\u00b7der", "rasch", "mit", "mei\u00b7nem", "Raub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In alle Weite fl\u00f6g' ich.", "tokens": ["In", "al\u00b7le", "Wei\u00b7te", "fl\u00f6g'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}