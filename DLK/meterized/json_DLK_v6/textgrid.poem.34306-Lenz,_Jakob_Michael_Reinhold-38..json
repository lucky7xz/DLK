{"textgrid.poem.34306": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Erschr\u00f6cken, dies Verlangen", "tokens": ["Dies", "Er\u00b7schr\u00f6\u00b7cken", ",", "dies", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "NN", "$,", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das mich als du kamst, umfangen,", "tokens": ["Das", "mich", "als", "du", "kamst", ",", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "KOUS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dies Gef\u00fchl \u2013 wer zauberts nach?", "tokens": ["Dies", "Ge\u00b7f\u00fchl", "\u2013", "wer", "zau\u00b7berts", "nach", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "PWS", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott! wie schlug das Herz so schwach \u2013", "tokens": ["Gott", "!", "wie", "schlug", "das", "Herz", "so", "schwach", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als mein Glas ihn \u00fcberraschte", "tokens": ["Als", "mein", "Glas", "ihn", "\u00fc\u00b7berr\u00b7aschte"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jenen Blick, nach dem ich haschte,", "tokens": ["Je\u00b7nen", "Blick", ",", "nach", "dem", "ich", "haschte", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jenen Blick \u2013 o Huldg\u00f6ttin!", "tokens": ["Je\u00b7nen", "Blick", "\u2013", "o", "Huld\u00b7g\u00f6t\u00b7tin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "FM", "NE", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Welch ein Himmel war darin!", "tokens": ["Welch", "ein", "Him\u00b7mel", "war", "da\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh mein Herz, das nach dir bebte,", "tokens": ["Sieh", "mein", "Herz", ",", "das", "nach", "dir", "beb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kannt' ich gleich die Ursach nicht,", "tokens": ["Kannt'", "ich", "gleich", "die", "Ur\u00b7sach", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zog, obschon ich widerstrebte,", "tokens": ["Zog", ",", "ob\u00b7schon", "ich", "wi\u00b7der\u00b7streb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets mein Aug' auf dein Gesicht,", "tokens": ["Stets", "mein", "Aug'", "auf", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis ich, ohne da\u00df ich wuste", "tokens": ["Bis", "ich", ",", "oh\u00b7ne", "da\u00df", "ich", "wus\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUI", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer du w\u00e4rest, weinen muste.", "tokens": ["Wer", "du", "w\u00e4\u00b7rest", ",", "wei\u00b7nen", "mus\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}