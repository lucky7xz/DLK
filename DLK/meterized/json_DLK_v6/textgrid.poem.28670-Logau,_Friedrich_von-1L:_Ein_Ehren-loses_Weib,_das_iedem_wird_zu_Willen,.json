{"textgrid.poem.28670": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Ehren-loses Weib, das iedem wird zu Willen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ehren-loses Weib, das iedem wird zu Willen,", "tokens": ["Ein", "Eh\u00b7ren\u00b7lo\u00b7ses", "Weib", ",", "das", "ie\u00b7dem", "wird", "zu", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gt selten eine Frucht, pflegt nur die Brunst zu stillen:", "tokens": ["Tr\u00e4gt", "sel\u00b7ten", "ei\u00b7ne", "Frucht", ",", "pflegt", "nur", "die", "Brunst", "zu", "stil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer aller Treu sich traut und Glauben tr\u00e4gt so feil,", "tokens": ["Wer", "al\u00b7ler", "Treu", "sich", "traut", "und", "Glau\u00b7ben", "tr\u00e4gt", "so", "feil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "PRF", "VVFIN", "KON", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gibt seinen Theil zwar au\u00df, nimmt aber keinen Theil.", "tokens": ["Gibt", "sei\u00b7nen", "Theil", "zwar", "au\u00df", ",", "nimmt", "a\u00b7ber", "kei\u00b7nen", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word",