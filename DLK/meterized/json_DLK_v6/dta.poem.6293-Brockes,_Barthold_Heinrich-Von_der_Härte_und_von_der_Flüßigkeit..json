{"dta.poem.6293": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lange noch das Wasser seine Theile,", "tokens": ["So", "lan\u00b7ge", "noch", "das", "Was\u00b7ser", "sei\u00b7ne", "Thei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die biegsam, glatt, unwiderspenstig seyn,", "tokens": ["Die", "bieg\u00b7sam", ",", "glatt", ",", "un\u00b7wi\u00b7der\u00b7spens\u00b7tig", "seyn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zusammen f\u00fcgt, in leichter Eile,", "tokens": ["Zu\u00b7sam\u00b7men", "f\u00fcgt", ",", "in", "leich\u00b7ter", "Ei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmt es so grossen Raum nicht ein.", "tokens": ["Nimmt", "es", "so", "gros\u00b7sen", "Raum", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies ist gantz deutlich vorzustellen,", "tokens": ["Dies", "ist", "gantz", "deut\u00b7lich", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man siehet allezeit ein frierend Wasser schwellen.", "tokens": ["Man", "sie\u00b7het", "al\u00b7le\u00b7zeit", "ein", "frie\u00b7rend", "Was\u00b7ser", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}