{"dta.poem.7613": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch den allein ich mit der Welt zusammenh\u00e4nge,", "tokens": ["Durch", "den", "al\u00b7lein", "ich", "mit", "der", "Welt", "zu\u00b7sam\u00b7men\u00b7h\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seitdem ich nebenaus mich stellte vom Gedr\u00e4nge!", "tokens": ["Seit\u00b7dem", "ich", "ne\u00b7be\u00b7naus", "mich", "stell\u00b7te", "vom", "Ge\u00b7dr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Du bringst, o Freund, die Welt mir her von Zeit zu Zeit,", "tokens": ["Du", "bringst", ",", "o", "Freund", ",", "die", "Welt", "mir", "her", "von", "Zeit", "zu", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "ART", "NN", "PPER", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich merkte sonst sie nicht in meiner Einsamkeit.", "tokens": ["Ich", "merk\u00b7te", "sonst", "sie", "nicht", "in", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bringest von der Welt die Kunden mir getreulich,", "tokens": ["Du", "brin\u00b7gest", "von", "der", "Welt", "die", "Kun\u00b7den", "mir", "ge\u00b7treu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch weniges dem Sinn, nichts dem Gem\u00fcth erfreulich.", "tokens": ["Doch", "we\u00b7ni\u00b7ges", "dem", "Sinn", ",", "nichts", "dem", "Ge\u00b7m\u00fcth", "er\u00b7freu\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "$,", "PIS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nichts h\u00f6r' ich von der Welt, was mich verlocken kann,", "tokens": ["Nichts", "h\u00f6r'", "ich", "von", "der", "Welt", ",", "was", "mich", "ver\u00b7lo\u00b7cken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Neu auf das Meer zu gehn, da ich zum Port entrann.", "tokens": ["Neu", "auf", "das", "Meer", "zu", "gehn", ",", "da", "ich", "zum", "Port", "ent\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "APPRART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich sehe tr\u00fcb', und mu\u00df mir leider es gestehn:", "tokens": ["Ich", "se\u00b7he", "tr\u00fcb'", ",", "und", "mu\u00df", "mir", "lei\u00b7der", "es", "ge\u00b7stehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$,", "KON", "VMFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Alter ist es nicht, was mich macht tr\u00fcbe sehn.", "tokens": ["Das", "Al\u00b7ter", "ist", "es", "nicht", ",", "was", "mich", "macht", "tr\u00fc\u00b7be", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein unzufriedenes Geschlecht mit Zorngeberden", "tokens": ["Ein", "un\u00b7zu\u00b7frie\u00b7de\u00b7nes", "Ge\u00b7schlecht", "mit", "Zorn\u00b7ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will \u00e4ndern seine Welt, und selbst nicht anders werden.", "tokens": ["Will", "\u00e4n\u00b7dern", "sei\u00b7ne", "Welt", ",", "und", "selbst", "nicht", "an\u00b7ders", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$,", "KON", "ADV", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wo nicht ein \u00e4u\u00dfrer tobt, ein innerlicher Kampf,", "tokens": ["Wo", "nicht", "ein", "\u00e4u\u00df\u00b7rer", "tobt", ",", "ein", "in\u00b7ner\u00b7li\u00b7cher", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "ADJA", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird selbst des Lebens Lustgeberd' ein Todeskrampf.", "tokens": ["Wird", "selbst", "des", "Le\u00b7bens", "Lust\u00b7ge\u00b7berd'", "ein", "To\u00b7des\u00b7krampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Den Wehen des Geschicks ist Fehlgeburt entrungen,", "tokens": ["Den", "We\u00b7hen", "des", "Ge\u00b7schicks", "ist", "Fehl\u00b7ge\u00b7burt", "ent\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Drang des Augenblicks Ruh und Genu\u00df verschlungen.", "tokens": ["Vom", "Drang", "des", "Au\u00b7gen\u00b7blicks", "Ruh", "und", "Ge\u00b7nu\u00df", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Ich wei\u00df nicht, wo sich wird die Wissenschaft verkriechen,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wo", "sich", "wird", "die", "Wis\u00b7sen\u00b7schaft", "ver\u00b7krie\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PRF", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Poesie doch wird unzweifelhaft versiechen.", "tokens": ["Die", "Poe\u00b7sie", "doch", "wird", "un\u00b7zwei\u00b7fel\u00b7haft", "ver\u00b7sie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Wo sich gen\u00fcberstehn Unglaub' und Ueberglauben,", "tokens": ["Wo", "sich", "ge\u00b7n\u00fc\u00b7bers\u00b7tehn", "Un\u00b7glaub'", "und", "Ue\u00b7berg\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Will dir die Seele der, und der die Sinne rauben.", "tokens": ["Will", "dir", "die", "See\u00b7le", "der", ",", "und", "der", "die", "Sin\u00b7ne", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "$,", "KON", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Die Sinne raubt er nicht, doch hat er sie verdumpft;", "tokens": ["Die", "Sin\u00b7ne", "raubt", "er", "nicht", ",", "doch", "hat", "er", "sie", "ver\u00b7dumpft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seele raubt er nicht, doch hat er sie versumpft.", "tokens": ["Die", "See\u00b7le", "raubt", "er", "nicht", ",", "doch", "hat", "er", "sie", "ver\u00b7sumpft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "In diesem S\u00fcndenpfuhl, in diesen Jammerfr\u00f6sten,", "tokens": ["In", "die\u00b7sem", "S\u00fcn\u00b7den\u00b7pfuhl", ",", "in", "die\u00b7sen", "Jam\u00b7mer\u00b7fr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann f\u00fcr die Welt mich nur ein neuer Glaube tr\u00f6sten;", "tokens": ["Kann", "f\u00fcr", "die", "Welt", "mich", "nur", "ein", "neu\u00b7er", "Glau\u00b7be", "tr\u00f6s\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Der Glaube, da\u00df der Geist, der mit der Sonne blickt,", "tokens": ["Der", "Glau\u00b7be", ",", "da\u00df", "der", "Geist", ",", "der", "mit", "der", "Son\u00b7ne", "blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Zeit zu Zeit, wo H\u00fclfe noth ist, Helfer schickt;", "tokens": ["Von", "Zeit", "zu", "Zeit", ",", "wo", "H\u00fcl\u00b7fe", "noth", "ist", ",", "Hel\u00b7fer", "schickt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "PWAV", "NN", "NN", "VAFIN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Und wenn das Unheil sich unheilbar Menschen zeigt,", "tokens": ["Und", "wenn", "das", "Un\u00b7heil", "sich", "un\u00b7heil\u00b7bar", "Men\u00b7schen", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In menschlicher Gestalt er selbst herniedersteigt.", "tokens": ["In", "menschli\u00b7cher", "Ge\u00b7stalt", "er", "selbst", "her\u00b7nie\u00b7ders\u00b7teigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "So mehr als einmal schon ist er herab gestiegen,", "tokens": ["So", "mehr", "als", "ein\u00b7mal", "schon", "ist", "er", "her\u00b7ab", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "ADV", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jetzo denkt er, wo er will geboren liegen.", "tokens": ["Und", "jet\u00b7zo", "denkt", "er", ",", "wo", "er", "will", "ge\u00b7bo\u00b7ren", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}