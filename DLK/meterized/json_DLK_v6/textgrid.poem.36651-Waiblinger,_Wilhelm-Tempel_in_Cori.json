{"textgrid.poem.36651": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Tempel in Cori", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du des Helden Tempel, der hier dem blumigen Felsen,", "tokens": ["Bist", "du", "des", "Hel\u00b7den", "Tem\u00b7pel", ",", "der", "hier", "dem", "blu\u00b7mi\u00b7gen", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Einem Elysium hier, Myrthen und Rosen entragt,", "tokens": ["Ei\u00b7nem", "E\u00b7ly\u00b7si\u00b7um", "hier", ",", "Myr\u00b7then", "und", "Ro\u00b7sen", "en\u00b7tragt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wahrlich dann bauten die Grazien dich, zum lieblichsten Denkmal,", "tokens": ["Wahr\u00b7lich", "dann", "bau\u00b7ten", "die", "Gra\u00b7zi\u00b7en", "dich", ",", "zum", "lieb\u00b7lichs\u00b7ten", "Denk\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Da\u00df dir die G\u00f6ttin den Trank ewiger Jugend gereicht.", "tokens": ["Da\u00df", "dir", "die", "G\u00f6t\u00b7tin", "den", "Trank", "e\u00b7wi\u00b7ger", "Ju\u00b7gend", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}