{"textgrid.poem.40552": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hie Welf! Hie Waiblinger! La\u00df sehn!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hie Welf! Hie Waiblinger! La\u00df sehn!", "tokens": ["Hie", "Welf", "!", "Hie", "Waib\u00b7lin\u00b7ger", "!", "La\u00df", "sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "ADV", "NN", "$.", "VVIMP", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Nur schwanke nicht hin und her!", "tokens": ["Nur", "schwan\u00b7ke", "nicht", "hin", "und", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du kannst, ein Ehrenmann, auch stehn", "tokens": ["Du", "kannst", ",", "ein", "Eh\u00b7ren\u00b7mann", ",", "auch", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "ART", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gegen\u00fcber im Feindesheer.", "tokens": ["Ge\u00b7gen\u00b7\u00fc\u00b7ber", "im", "Fein\u00b7des\u00b7heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Magst B\u00e4r im Gekl\u00fcft, magst Falk' im Licht,", "tokens": ["Magst", "B\u00e4r", "im", "Ge\u00b7kl\u00fcft", ",", "magst", "Fal\u00b7k'", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,", "VMFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur Fledermaus nicht sein;", "tokens": ["Nur", "Fle\u00b7der\u00b7maus", "nicht", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei Palme oder Eiche, nur nicht", "tokens": ["Sei", "Pal\u00b7me", "o\u00b7der", "Ei\u00b7che", ",", "nur", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "ADV", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das Schlingkraut zwischen den Zwei'n!", "tokens": ["Das", "Schling\u00b7kraut", "zwi\u00b7schen", "den", "Zwei'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ob Wahn, ob Wahrheit dein Panier!", "tokens": ["Ob", "Wahn", ",", "ob", "Wahr\u00b7heit", "dein", "Pa\u00b7nier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer l\u00f6st's, wem glaube dein Herz?", "tokens": ["Wer", "l\u00f6st's", ",", "wem", "glau\u00b7be", "dein", "Herz", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Am Feuer der Treue l\u00e4ut're dir", "tokens": ["Am", "Feu\u00b7er", "der", "Treu\u00b7e", "l\u00e4ut'\u00b7re", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu Gold unechtes Erz!", "tokens": ["Zu", "Gold", "un\u00b7ech\u00b7tes", "Erz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer trommelnd, trompetend mit uns geht,", "tokens": ["Wer", "trom\u00b7melnd", ",", "trom\u00b7pe\u00b7tend", "mit", "uns", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der bessere Held ist's nicht,", "tokens": ["Der", "bes\u00b7se\u00b7re", "Held", "ist's", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch der, so fest zur Fahne steht,", "tokens": ["Doch", "der", ",", "so", "fest", "zur", "Fah\u00b7ne", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er kein Wort auch spricht.", "tokens": ["Wenn", "er", "kein", "Wort", "auch", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch schm\u00e4ht nicht den Mann, der, dr\u00fcben itzt,", "tokens": ["Doch", "schm\u00e4ht", "nicht", "den", "Mann", ",", "der", ",", "dr\u00fc\u00b7ben", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "$,", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei unsrer Fahn' einst stund!", "tokens": ["Bei", "uns\u00b7rer", "Fahn'", "einst", "stund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Blut, schon einst f\u00fcr uns verspritzt,", "tokens": ["Sein", "Blut", ",", "schon", "einst", "f\u00fcr", "uns", "ver\u00b7spritzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Siegel ist's meinem Mund.", "tokens": ["Ein", "Sie\u00b7gel", "ist's", "mei\u00b7nem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich sah auch Locken, braun und lang,", "tokens": ["Ich", "sah", "auch", "Lo\u00b7cken", ",", "braun", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu d\u00fcnnem Schnee verwehn,", "tokens": ["Zu", "d\u00fcn\u00b7nem", "Schnee", "ver\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Manch nervigen Arm, der das Schwert einst schwang,", "tokens": ["Manch", "ner\u00b7vi\u00b7gen", "Arm", ",", "der", "das", "Schwert", "einst", "schwang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Betk\u00fcgelchen zitternd drehn.", "tokens": ["Bet\u00b7k\u00fc\u00b7gel\u00b7chen", "zit\u00b7ternd", "drehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich sah's, wie Fieber des Weisen Wort", "tokens": ["Ich", "sah's", ",", "wie", "Fie\u00b7ber", "des", "Wei\u00b7sen", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Unsinns Gr\u00e4uel zerbrach,", "tokens": ["In", "Un\u00b7sinns", "Gr\u00e4u\u00b7el", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich h\u00f6rte den Thoren im Irrsinn dort,", "tokens": ["Ich", "h\u00f6r\u00b7te", "den", "Tho\u00b7ren", "im", "Irr\u00b7sinn", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Perlen der Weisheit sprach.", "tokens": ["Der", "Per\u00b7len", "der", "Weis\u00b7heit", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich sah den Raufbold friedlich gemacht,", "tokens": ["Ich", "sah", "den", "Rauf\u00b7bold", "fried\u00b7lich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Verwittert der Jugend Roth,", "tokens": ["Ver\u00b7wit\u00b7tert", "der", "Ju\u00b7gend", "Roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den Schw\u00e4tzer zu ewigem Schweigen gebracht!", "tokens": ["Den", "Schw\u00e4t\u00b7zer", "zu", "e\u00b7wi\u00b7gem", "Schwei\u00b7gen", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wer kann f\u00fcr Krankheit und Tod?", "tokens": ["Wer", "kann", "f\u00fcr", "Krank\u00b7heit", "und", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Will's Gott, so lang ich gesund, ersp\u00e4ht", "tokens": ["Will's", "Gott", ",", "so", "lang", "ich", "ge\u00b7sund", ",", "er\u00b7sp\u00e4ht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "ADV", "ADJD", "PPER", "ADJD", "$,", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei diesen Fahnen ihr mich!", "tokens": ["Bei", "die\u00b7sen", "Fah\u00b7nen", "ihr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "PRF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wahr's Gott, wenn ihr je mich dr\u00fcben s\u00e4ht,", "tokens": ["Wahr's", "Gott", ",", "wenn", "ihr", "je", "mich", "dr\u00fc\u00b7ben", "s\u00e4ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "++--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dann krank oder todt w\u00e4r' ich.", "tokens": ["Dann", "krank", "o\u00b7der", "todt", "w\u00e4r'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Denkt mein wie eines Todten dann;", "tokens": ["Denkt", "mein", "wie", "ei\u00b7nes", "Tod\u00b7ten", "dann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es mag wohl bitter sein,", "tokens": ["Es", "mag", "wohl", "bit\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vorbeizugehn als lebend'ger Mann", "tokens": ["Vor\u00b7bei\u00b7zu\u00b7gehn", "als", "le\u00b7ben\u00b7d'\u00b7ger", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am eignen Leichenstein.", "tokens": ["Am", "eig\u00b7nen", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}