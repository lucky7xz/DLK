{"textgrid.poem.59788": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief in dich hinab, tief in dich hinein!", "tokens": ["Tief", "in", "dich", "hin\u00b7ab", ",", "tief", "in", "dich", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$,", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Bricht da dir aus der Tiefe kein Schein,", "tokens": ["Bricht", "da", "dir", "aus", "der", "Tie\u00b7fe", "kein", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der von helleren Scheinen was kann erz\u00e4hlen,", "tokens": ["Der", "von", "hel\u00b7le\u00b7ren", "Schei\u00b7nen", "was", "kann", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PWS", "VMFIN", "VVINF", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So denk' an die Sch\u00e4den der Menschenseelen \u2013", "tokens": ["So", "denk'", "an", "die", "Sch\u00e4\u00b7den", "der", "Men\u00b7schen\u00b7see\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Es mu\u00df in dir was versch\u00fcttet sein.", "tokens": ["Es", "mu\u00df", "in", "dir", "was", "ver\u00b7sch\u00fct\u00b7tet", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kannst du solchen Schutt nicht tapfer r\u00e4umen,", "tokens": ["Kannst", "du", "sol\u00b7chen", "Schutt", "nicht", "tap\u00b7fer", "r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "So bleibt's beim eitlen W\u00e4hnen und Tr\u00e4umen.", "tokens": ["So", "bleibt's", "beim", "eit\u00b7len", "W\u00e4h\u00b7nen", "und", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}