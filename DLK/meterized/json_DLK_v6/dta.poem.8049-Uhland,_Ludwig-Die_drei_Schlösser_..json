{"dta.poem.8049": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Schl\u00f6sser .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Schl\u00f6sser sind in meinem Gaue,", "tokens": ["Drei", "Schl\u00f6s\u00b7ser", "sind", "in", "mei\u00b7nem", "Gau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich mit Liebe stets beschaue;", "tokens": ["Die", "ich", "mit", "Lie\u00b7be", "stets", "be\u00b7schau\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich, der wohlbestellte S\u00e4nger,", "tokens": ["Und", "ich", ",", "der", "wohl\u00b7be\u00b7stell\u00b7te", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Feld und Wald der rasche G\u00e4nger,", "tokens": ["Durch", "Feld", "und", "Wald", "der", "ra\u00b7sche", "G\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sollt\u2019 ich schweigen von den Dreien,", "tokens": ["Wie", "sollt'", "ich", "schwei\u00b7gen", "von", "den", "Drei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sich dem Gau zum Schmucke reihen?", "tokens": ["Die", "sich", "dem", "Gau", "zum", "Schmu\u00b7cke", "rei\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "An wenig Tr\u00fcmmern zu erkennen,", "tokens": ["An", "we\u00b7nig", "Tr\u00fcm\u00b7mern", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Versunken dort am Waldeshange,", "tokens": ["Ver\u00b7sun\u00b7ken", "dort", "am", "Wal\u00b7des\u00b7han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Name selbst verschollen lange,", "tokens": ["Sein", "Na\u00b7me", "selbst", "ver\u00b7schol\u00b7len", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn seit nicht mehr die Th\u00fcrme ragen,", "tokens": ["Denn", "seit", "nicht", "mehr", "die", "Th\u00fcr\u00b7me", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verging nach ihm der Wandrer Fragen.", "tokens": ["Ver\u00b7ging", "nach", "ihm", "der", "Wand\u00b7rer", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch schreckt dich nicht durch Waldes Dichte", "tokens": ["Doch", "schreckt", "dich", "nicht", "durch", "Wal\u00b7des", "Dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Zweige Schlagen in\u2019s Gesichte:", "tokens": ["Der", "Zwei\u00b7ge", "Schla\u00b7gen", "in's", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dort, wo des Beiles Schl\u00e4ge fallen,", "tokens": ["Dort", ",", "wo", "des", "Bei\u00b7les", "Schl\u00e4\u00b7ge", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Einsame Waldhornkl\u00e4nge hallen,", "tokens": ["Ein\u00b7sa\u00b7me", "Wald\u00b7horn\u00b7kl\u00e4n\u00b7ge", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Dort kannst du Wunderm\u00e4hr\u2019 erfragen", "tokens": ["Dort", "kannst", "du", "Wun\u00b7der\u00b7m\u00e4hr'", "er\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Von Mauern, welche nicht mehr ragen.", "tokens": ["Von", "Mau\u00b7ern", ",", "wel\u00b7che", "nicht", "mehr", "ra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ja! setzest du im Mondenscheine", "tokens": ["Ja", "!", "set\u00b7zest", "du", "im", "Mon\u00b7den\u00b7schei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dich auf\u2019s verfallene Gesteine:", "tokens": ["Dich", "auf's", "ver\u00b7fal\u00b7le\u00b7ne", "Ge\u00b7stei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "So wird die Kund\u2019, auch unerbeten,", "tokens": ["So", "wird", "die", "Kund'", ",", "auch", "un\u00b7er\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Dir vor die stille Seele treten.", "tokens": ["Dir", "vor", "die", "stil\u00b7le", "See\u00b7le", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Es scheint ein Schlo\u00df, doch ist es keines.", "tokens": ["Es", "scheint", "ein", "Schlo\u00df", ",", "doch", "ist", "es", "kei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du siehst vom hohen Bergesr\u00fccken", "tokens": ["Du", "siehst", "vom", "ho\u00b7hen", "Ber\u00b7ge\u00b7sr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es stolz im Sonnenstrale blicken,", "tokens": ["Es", "stolz", "im", "Son\u00b7nen\u00b7stra\u00b7le", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Th\u00fcrmen und mit Zinnen prangen,", "tokens": ["Mit", "Th\u00fcr\u00b7men", "und", "mit", "Zin\u00b7nen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit tiefem Graben rings umfangen,", "tokens": ["Mit", "tie\u00b7fem", "Gra\u00b7ben", "rings", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Voll Heldenbilder aller Orte,", "tokens": ["Voll", "Hel\u00b7den\u00b7bil\u00b7der", "al\u00b7ler", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zween Marmorl\u00f6wen an der Pforte:", "tokens": ["Zween", "Mar\u00b7mor\u00b7l\u00f6\u00b7wen", "an", "der", "Pfor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch drinnen ist es \u00f6d\u2019 und stille,", "tokens": ["Doch", "drin\u00b7nen", "ist", "es", "\u00f6d'", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Im Hofe hohes Gras in F\u00fclle,", "tokens": ["Im", "Ho\u00b7fe", "ho\u00b7hes", "Gras", "in", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Im Graben quillt das Wasser nimmer,", "tokens": ["Im", "Gra\u00b7ben", "quillt", "das", "Was\u00b7ser", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Im Haus ist Treppe nicht, noch Zimmer,", "tokens": ["Im", "Haus", "ist", "Trep\u00b7pe", "nicht", ",", "noch", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "PTKNEG", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ringsum die Epheuranken schleichen,", "tokens": ["Ring\u00b7sum", "die", "E\u00b7pheu\u00b7ran\u00b7ken", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Zugv\u00f6gel durch die Fenster streichen.", "tokens": ["Zug\u00b7v\u00f6\u00b7gel", "durch", "die", "Fens\u00b7ter", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dort sa\u00dfen mit der goldnen Krone", "tokens": ["Dort", "sa\u00b7\u00dfen", "mit", "der", "gold\u00b7nen", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Voreinst die Herrscher auf dem Throne,", "tokens": ["Vor\u00b7e\u00b7inst", "die", "Herr\u00b7scher", "auf", "dem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Von dortaus zogen einst die Helden,", "tokens": ["Von", "dor\u00b7taus", "zo\u00b7gen", "einst", "die", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Von denen die Geschichten melden.", "tokens": ["Von", "de\u00b7nen", "die", "Ge\u00b7schich\u00b7ten", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die Herrscher ruhn in Gr\u00e4berhallen,", "tokens": ["Die", "Herr\u00b7scher", "ruhn", "in", "Gr\u00e4\u00b7ber\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die Helden sind im Kampf gefallen;", "tokens": ["Die", "Hel\u00b7den", "sind", "im", "Kampf", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Verhallet war der Burg Get\u00fcmmel,", "tokens": ["Ver\u00b7hal\u00b7let", "war", "der", "Burg", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Da fuhr ein Feuerstral vom Himmel,", "tokens": ["Da", "fuhr", "ein", "Feu\u00b7er\u00b7stral", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der reiche Schatz verging in Flammen,", "tokens": ["Der", "rei\u00b7che", "Schatz", "ver\u00b7ging", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Gemach und Treppe fiel zusammen.", "tokens": ["Ge\u00b7mach", "und", "Trep\u00b7pe", "fiel", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Inwendig ward das Schlo\u00df verheeret,", "tokens": ["In\u00b7wen\u00b7dig", "ward", "das", "Schlo\u00df", "ver\u00b7hee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Doch au\u00dfen blieb es unversehret.", "tokens": ["Doch", "au\u00b7\u00dfen", "blieb", "es", "un\u00b7ver\u00b7seh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Sobald erlosch der Edeln Orden,", "tokens": ["So\u00b7bald", "er\u00b7losch", "der", "E\u00b7deln", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Ist auch ihr Haus ver\u00f6det worden.", "tokens": ["Ist", "auch", "ihr", "Haus", "ver\u00b7\u00f6\u00b7det", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Doch wie noch die Geschichten melden", "tokens": ["Doch", "wie", "noch", "die", "Ge\u00b7schich\u00b7ten", "mel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Der Herrscher Namen und der Helden:", "tokens": ["Der", "Herr\u00b7scher", "Na\u00b7men", "und", "der", "Hel\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "So sieht man auch die Th\u00fcrm\u2019 und Mauern", "tokens": ["So", "sieht", "man", "auch", "die", "Th\u00fcrm'", "und", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mit ihren Heldenbildern dauern.", "tokens": ["Mit", "ih\u00b7ren", "Hel\u00b7den\u00b7bil\u00b7dern", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Auch wird noch ferner manch Jahrhundert", "tokens": ["Auch", "wird", "noch", "fer\u00b7ner", "manch", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN"