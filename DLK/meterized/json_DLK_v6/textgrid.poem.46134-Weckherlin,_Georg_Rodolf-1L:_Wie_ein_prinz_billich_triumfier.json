{"textgrid.poem.46134": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ein prinz billich triumfier", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein prinz billich triumfier", "tokens": ["Wie", "ein", "prinz", "bil\u00b7lich", "tri\u00b7um\u00b7fier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "un\u00fcberwindlich \u00fcberwunden,", "tokens": ["un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "sein land frei halt und wol regier", "tokens": ["sein", "land", "frei", "halt", "und", "wol", "re\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "KON", "ADV", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie frei die menschen und gebunden:", "tokens": ["wie", "frei", "die", "men\u00b7schen", "und", "ge\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sie die lieb mach allzeit frei,", "tokens": ["Wie", "sie", "die", "lieb", "mach", "all\u00b7zeit", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJD", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wie fro sie mach die forcht und treu,", "tokens": ["wie", "fro", "sie", "mach", "die", "forcht", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "solt, meine Musa, du betrachten", "tokens": ["solt", ",", "mei\u00b7ne", "Mu\u00b7sa", ",", "du", "be\u00b7trach\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und singen mit so hohem klang,", "tokens": ["und", "sin\u00b7gen", "mit", "so", "ho\u00b7hem", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "da\u00df die nachk\u00f6mling dein gesang", "tokens": ["da\u00df", "die", "nach\u00b7k\u00f6m\u00b7ling", "dein", "ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "allzeit ganz wunderreich hochachten.", "tokens": ["all\u00b7zeit", "ganz", "wun\u00b7der\u00b7reich", "hoch\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er, in sich selbs gerecht und rein,", "tokens": ["Er", ",", "in", "sich", "selbs", "ge\u00b7recht", "und", "rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PRF", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bei sich die billichkeit losieret,", "tokens": ["bei", "sich", "die", "bil\u00b7lich\u00b7keit", "lo\u00b7sie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "da\u00df seines rechts gerechter schein", "tokens": ["da\u00df", "sei\u00b7nes", "rechts", "ge\u00b7rech\u00b7ter", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ihn um und um klar also zieret:", "tokens": ["ihn", "um", "und", "um", "klar", "al\u00b7so", "zie\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "APPR", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und zumal seines rechts gewalt", "tokens": ["Und", "zu\u00b7mal", "sei\u00b7nes", "rechts", "ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "macht frei und sicher dergestalt,", "tokens": ["macht", "frei", "und", "si\u00b7cher", "der\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "da\u00df seine leut gl\u00fcckselig leben;", "tokens": ["da\u00df", "sei\u00b7ne", "leut", "gl\u00fcck\u00b7se\u00b7lig", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "so wird auch sein gesatz mit lehr", "tokens": ["so", "wird", "auch", "sein", "ge\u00b7satz", "mit", "lehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "auf seiner stirnen, nein vil mehr", "tokens": ["auf", "sei\u00b7ner", "stir\u00b7nen", ",", "nein", "vil", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "PTKANT", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "durch sein thun an das liecht gegeben.", "tokens": ["durch", "sein", "thun", "an", "das", "liecht", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVINF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sein g\u00f6tlich gutes angesicht", "tokens": ["Sein", "g\u00f6t\u00b7lich", "gu\u00b7tes", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "macht, da\u00df die sitten, so auf erden", "tokens": ["macht", ",", "da\u00df", "die", "sit\u00b7ten", ",", "so", "auf", "er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "$,", "ADV", "APPR", "NN"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.3": {"text": "sunst menschlich, mehr f\u00fcr menschlich nicht,", "tokens": ["sunst", "menschlich", ",", "mehr", "f\u00fcr", "menschlich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "APPR", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "sondern f\u00fcr g\u00f6tlich erkant werden:", "tokens": ["son\u00b7dern", "f\u00fcr", "g\u00f6t\u00b7lich", "er\u00b7kant", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "VVPP", "VAINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Des himmels beispiel kraft und zier", "tokens": ["Des", "him\u00b7mels", "bei\u00b7spiel", "kraft", "und", "zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "uns seine gotsforcht stellet f\u00fcr:", "tokens": ["uns", "sei\u00b7ne", "gots\u00b7forcht", "stel\u00b7let", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "sein leib, damit sein land belohnet", "tokens": ["sein", "leib", ",", "da\u00b7mit", "sein", "land", "be\u00b7loh\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "auf erden ist ein himmelreich,", "tokens": ["auf", "er\u00b7den", "ist", "ein", "him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "darinnen sein geist gro\u00df, stark, reich", "tokens": ["da\u00b7rin\u00b7nen", "sein", "geist", "gro\u00df", ",", "stark", ",", "reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "in herrlichkeit, gl\u00fcckselig, wohnet.", "tokens": ["in", "herr\u00b7lich\u00b7keit", ",", "gl\u00fcck\u00b7se\u00b7lig", ",", "woh\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcrsichtig \u00f6ffnet seine hand,", "tokens": ["F\u00fcr\u00b7sich\u00b7tig", "\u00f6ff\u00b7net", "sei\u00b7ne", "hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie er nu will, der weisheit tempel;", "tokens": ["wie", "er", "nu", "will", ",", "der", "weis\u00b7heit", "tem\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "$,", "ART", "NN", "NE", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "da sein allgr\u00fcndender verstand", "tokens": ["da", "sein", "all\u00b7gr\u00fcn\u00b7den\u00b7der", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "gibt andern weisen ein exempel,", "tokens": ["gibt", "an\u00b7dern", "wei\u00b7sen", "ein", "ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Mit einem w\u00fcrkenden anblick", "tokens": ["Mit", "ei\u00b7nem", "w\u00fcr\u00b7ken\u00b7den", "an\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "in einem und in anderm gl\u00fcck;", "tokens": ["in", "ei\u00b7nem", "und", "in", "an\u00b7derm", "gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "KON", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ernst, g\u00fcte, straf und gnad zu \u00fcben,", "tokens": ["ernst", ",", "g\u00fc\u00b7te", ",", "straf", "und", "gnad", "zu", "\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "VVFIN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und wie ein mensch zu aller frist,", "tokens": ["und", "wie", "ein", "mensch", "zu", "al\u00b7ler", "frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "der g\u00fctig, mild und gn\u00e4dig ist,", "tokens": ["der", "g\u00fc\u00b7tig", ",", "mild", "und", "gn\u00e4\u00b7dig", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "auf erden als ein got zu lieben.", "tokens": ["auf", "er\u00b7den", "als", "ein", "got", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er, als der in der tugend kreis", "tokens": ["Er", ",", "als", "der", "in", "der", "tu\u00b7gend", "kreis"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein andrer Herkules geboren,", "tokens": ["ein", "an\u00b7drer", "Her\u00b7ku\u00b7les", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "hat auch der tugend m\u00fch und schwei\u00df", "tokens": ["hat", "auch", "der", "tu\u00b7gend", "m\u00fch", "und", "schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "f\u00fcr seine lieb und lust erkoren:", "tokens": ["f\u00fcr", "sei\u00b7ne", "lieb", "und", "lust", "er\u00b7ko\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der tugend werk seind seine ruh,", "tokens": ["Der", "tu\u00b7gend", "werk", "seind", "sei\u00b7ne", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "er bringet die zeit m\u00fcssig zu", "tokens": ["er", "brin\u00b7get", "die", "zeit", "m\u00fcs\u00b7sig", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "mit pflanzung n\u00fctzlicher gesetzen;", "tokens": ["mit", "pflan\u00b7zung", "n\u00fctz\u00b7li\u00b7cher", "ge\u00b7set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "es ist kein ha\u00df in seiner seel", "tokens": ["es", "ist", "kein", "ha\u00df", "in", "sei\u00b7ner", "seel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "doch seine sinn, die auch ohn fehl,", "tokens": ["doch", "sei\u00b7ne", "sinn", ",", "die", "auch", "ohn", "fehl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "den lastern sich stark widersetzen.", "tokens": ["den", "las\u00b7tern", "sich", "stark", "wi\u00b7der\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es pfleget seines augs gestirn", "tokens": ["Es", "pfle\u00b7get", "sei\u00b7nes", "augs", "ge\u00b7stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sein land mit friden stets zu segnen,", "tokens": ["sein", "land", "mit", "fri\u00b7den", "stets", "zu", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und seiner weisheit reiche stirn", "tokens": ["und", "sei\u00b7ner", "weis\u00b7heit", "rei\u00b7che", "stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "macht darauf allen segen regnen.", "tokens": ["macht", "da\u00b7rauf", "al\u00b7len", "se\u00b7gen", "reg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar seines sigs und triumfs pracht", "tokens": ["Zwar", "sei\u00b7nes", "sigs", "und", "tri\u00b7umfs", "pracht"], "token_info": ["word", "word", "word", "w