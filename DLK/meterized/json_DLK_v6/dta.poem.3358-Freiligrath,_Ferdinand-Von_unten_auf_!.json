{"dta.poem.3358": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Von unten auf !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1846", "urn": "urn:nbn:de:kobv:b4-200905191464", "language": ["de:0.99"], "booktitle": "Freiligrath, Ferdinand: \u00c7a ira! Herisau, 1846."}, "poem": {"stanza.1": {"line.1": {"text": "Ein D\u00e4mpfer kam von Bieberich: \u2014 stolz war", "tokens": ["Ein", "D\u00e4mp\u00b7fer", "kam", "von", "Bie\u00b7be\u00b7rich", ":", "stolz", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$.", "$(", "ADJD", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Furche, die er zog!", "tokens": ["die", "Fur\u00b7che", ",", "die", "er", "zog", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er qualmt\u2019 und r\u00e4derte zu Thal, da\u00df rechts und", "tokens": ["Er", "qualmt'", "und", "r\u00e4\u00b7der\u00b7te", "zu", "Thal", ",", "da\u00df", "rechts", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,", "KOUS", "ADV", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "links die Brandung flog!", "tokens": ["links", "die", "Bran\u00b7dung", "flog", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Von Wimpeln und von Flaggen voll, scho\u00df er", "tokens": ["Von", "Wim\u00b7peln", "und", "von", "Flag\u00b7gen", "voll", ",", "scho\u00df", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "hinab keck und erfreut:", "tokens": ["hin\u00b7ab", "keck", "und", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den K\u00f6nig, der in Preu\u00dfen herrscht, nach seiner", "tokens": ["Den", "K\u00f6\u00b7nig", ",", "der", "in", "Preu\u00b7\u00dfen", "herrscht", ",", "nach", "sei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Rheinburg trug er heut!", "tokens": ["Rhein\u00b7burg", "trug", "er", "heut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne schien wie lauter Gold! Auftauchte", "tokens": ["Die", "Son\u00b7ne", "schien", "wie", "lau\u00b7ter", "Gold", "!", "Auf\u00b7tauch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "PIAT", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schimmernd Stadt um Stadt!", "tokens": ["schim\u00b7mernd", "Stadt", "um", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der Rhein war wie ein Spiegel schier, und das", "tokens": ["Der", "Rhein", "war", "wie", "ein", "Spie\u00b7gel", "schier", ",", "und", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VAFIN", "KOKOM", "ART", "NN", "ADJD", "$,", "KON", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verdeck war blank und glatt!", "tokens": ["Ver\u00b7deck", "war", "blank", "und", "glatt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Dielen blitzten frisch gebohnt, und auf den", "tokens": ["Die", "Die\u00b7len", "blitz\u00b7ten", "frisch", "ge\u00b7bohnt", ",", "und", "auf", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$,", "KON", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schmalen her und hin", "tokens": ["schma\u00b7len", "her", "und", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Vergn\u00fcgten Auges wandelten der K\u00f6nig und die", "tokens": ["Ver\u00b7gn\u00fcg\u00b7ten", "Au\u00b7ges", "wan\u00b7del\u00b7ten", "der", "K\u00f6\u00b7nig", "und", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "KON", "ART"], "meter": "-+-+-+---+-++", "measure": "unknown.measure.hexa"}, "line.8": {"text": "K\u00f6nigin!", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Nach allen Seiten schaut\u2019 umher und winkte das", "tokens": ["Nach", "al\u00b7len", "Sei\u00b7ten", "schaut'", "um\u00b7her", "und", "wink\u00b7te", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "erhabne Paar;", "tokens": ["er\u00b7hab\u00b7ne", "Paar", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Rheingau\u2019s Reben gr\u00fc\u00dften sie und auch dein", "tokens": ["Des", "Rhein\u00b7gau's", "Re\u00b7ben", "gr\u00fc\u00df\u00b7ten", "sie", "und", "auch", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "KON", "ADV", "PPOSAT"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nu\u00dflaub, Sankt Goar!", "tokens": ["Nu\u00df\u00b7laub", ",", "Sankt", "Go\u00b7ar", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sie sahn zu Rhein, sie sahn zu Berg: \u2014 wie", "tokens": ["Sie", "sahn", "zu", "Rhein", ",", "sie", "sahn", "zu", "Berg", ":", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "PPER", "VVFIN", "APPR", "NN", "$.", "$(", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "war das Schifflein doch so nett!", "tokens": ["war", "das", "Schif\u00b7flein", "doch", "so", "nett", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es ging sich auf den Dielen fast, als wie auf", "tokens": ["Es", "ging", "sich", "auf", "den", "Die\u00b7len", "fast", ",", "als", "wie", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$,", "KOUS", "KOKOM", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Sanssouci\u2019s Parket!", "tokens": ["Sans\u00b7sou\u00b7ci's", "Par\u00b7ket", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch unter all der Nettigkeit und unter all der", "tokens": ["Doch", "un\u00b7ter", "all", "der", "Net\u00b7tig\u00b7keit", "und", "un\u00b7ter", "all", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ART", "NN", "KON", "APPR", "PIAT", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "schwimmenden Pracht,", "tokens": ["schwim\u00b7men\u00b7den", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da fri\u00dft und flammt das Element, das sie von", "tokens": ["Da", "fri\u00dft", "und", "flammt", "das", "E\u00b7le\u00b7ment", ",", "das", "sie", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.4": {"text": "dannen schie\u00dfen macht;", "tokens": ["dan\u00b7nen", "schie\u00b7\u00dfen", "macht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Da schafft in Ru\u00df und Feuersgluth, der dieses", "tokens": ["Da", "schafft", "in", "Ru\u00df", "und", "Feu\u00b7ers\u00b7gluth", ",", "der", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "PRELS", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Glanzes Seele ist;", "tokens": ["Glan\u00b7zes", "See\u00b7le", "ist", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Da steht und sch\u00fcrt und ordnet er \u2014 der Prole-", "tokens": ["Da", "steht", "und", "sch\u00fcrt", "und", "ord\u00b7net", "er", "der", "Pro\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "$(", "ART", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "tarier-Maschinist!", "tokens": ["ta\u00b7rier\u00b7Ma\u00b7schi\u00b7nist", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Da drau\u00dfen lacht und gr\u00fcnt die Welt, da drau\u00dfen", "tokens": ["Da", "drau\u00b7\u00dfen", "lacht", "und", "gr\u00fcnt", "die", "Welt", ",", "da", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "blitzt und rauscht der Rhein \u2014", "tokens": ["blitzt", "und", "rauscht", "der", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NE", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Er stiert den lieben langen Tag in seine Flam-", "tokens": ["Er", "stiert", "den", "lie\u00b7ben", "lan\u00b7gen", "Tag", "in", "sei\u00b7ne", "Flam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "men nur hinein!", "tokens": ["men", "nur", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Im wollnen Hemde, halbernackt, vor seiner Esse", "tokens": ["Im", "woll\u00b7nen", "Hem\u00b7de", ",", "hal\u00b7bern\u00b7ackt", ",", "vor", "sei\u00b7ner", "Es\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mu\u00df er steh\u2019n,", "tokens": ["mu\u00df", "er", "steh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Derweil ein K\u00f6nig \u00fcber ihm einschl\u00fcrft der Berge", "tokens": ["Der\u00b7weil", "ein", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "ihm", "ein\u00b7schl\u00fcrft", "der", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "freies Weh\u2019n!", "tokens": ["frei\u00b7es", "Weh'n", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Jetzt ist der Ofen zugekeilt, und Alles geht und", "tokens": ["Jetzt", "ist", "der", "O\u00b7fen", "zu\u00b7ge\u00b7keilt", ",", "und", "Al\u00b7les", "geht", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "KON", "PIS", "VVFIN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alles pa\u00dft;", "tokens": ["Al\u00b7les", "pa\u00dft", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So g\u00f6nnt er auf Minuten denn sich eine kurze", "tokens": ["So", "g\u00f6nnt", "er", "auf", "Mi\u00b7nu\u00b7ten", "denn", "sich", "ei\u00b7ne", "kur\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sklavenrast.", "tokens": ["Skla\u00b7ven\u00b7rast", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Mit halbem Leibe taucht er auf aus seinem lo-", "tokens": ["Mit", "hal\u00b7bem", "Lei\u00b7be", "taucht", "er", "auf", "aus", "sei\u00b7nem", "lo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dernden Versteck;", "tokens": ["dern\u00b7den", "Ver\u00b7steck", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "In seiner Fallth\u00fcr steht er da, und \u00fcberschaut sich", "tokens": ["In", "sei\u00b7ner", "Fallt\u00b7h\u00fcr", "steht", "er", "da", ",", "und", "\u00fc\u00b7bersc\u00b7haut", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "APPR", "PRF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "das Verdeck.", "tokens": ["das", "Ver\u00b7deck", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Das gl\u00fch\u2019nde Eisen in der Hand, Antlitz und Arme", "tokens": ["Das", "gl\u00fch'n\u00b7de", "Ei\u00b7sen", "in", "der", "Hand", ",", "Ant\u00b7litz", "und", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "roth erhitzt,", "tokens": ["roth", "er\u00b7hitzt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mit der gew\u00f6lbten haar\u2019gen Brust auf das Ge-", "tokens": ["Mit", "der", "ge\u00b7w\u00f6lb\u00b7ten", "haa\u00b7r'\u00b7gen", "Brust", "auf", "das", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+---", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "l\u00e4nder breit gest\u00fctzt \u2014", "tokens": ["l\u00e4n\u00b7der", "breit", "ge\u00b7st\u00fctzt"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "So l\u00e4\u00dft er schweifen seinen Blick, so murrt er", "tokens": ["So", "l\u00e4\u00dft", "er", "schwei\u00b7fen", "sei\u00b7nen", "Blick", ",", "so", "murrt", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "leis dem F\u00fcrsten zu:", "tokens": ["leis", "dem", "F\u00fcrs\u00b7ten", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "\u201ewie mahnt dies Boot mich an den Staat! Licht", "tokens": ["\u201e", "wie", "mahnt", "dies", "Boot", "mich", "an", "den", "Staat", "!", "Licht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "PWAV", "VVFIN", "PDS", "NN", "PRF", "APPR", "ART", "NN", "$.", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "auf den H\u00f6hen wandelst ", "tokens": ["auf", "den", "H\u00f6\u00b7hen", "wan\u00b7delst"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "\u201etief unten aber, in der Nacht und in der Arbeit", "tokens": ["\u201e", "tief", "un\u00b7ten", "a\u00b7ber", ",", "in", "der", "Nacht", "und", "in", "der", "Ar\u00b7beit"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "ADV", "ADV", "$,", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dunkelm Schoos,", "tokens": ["dun\u00b7kelm", "Schoos", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Tief unten, von der Noth gespornt, da sch\u00fcr\u2019 und", "tokens": ["Tief", "un\u00b7ten", ",", "von", "der", "Noth", "ge\u00b7spornt", ",", "da", "sch\u00fcr'", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schmied\u2019 ", "tokens": ["schmied'"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Nicht meines nur, auch Deines, Herr! Wer h\u00e4lt", "tokens": ["Nicht", "mei\u00b7nes", "nur", ",", "auch", "Dei\u00b7nes", ",", "Herr", "!", "Wer", "h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "ADV", "$,", "ADV", "PPOSAT", "$,", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die R\u00e4der Dir im Takt,", "tokens": ["die", "R\u00e4\u00b7der", "Dir", "im", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn nicht mit schwielenharter Faust der Heizer", "tokens": ["Wenn", "nicht", "mit", "schwie\u00b7len\u00b7har\u00b7ter", "Faust", "der", "Hei\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "seine Eisen packt?", "tokens": ["sei\u00b7ne", "Ei\u00b7sen", "packt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "\u201edu bist viel weniger ein Zeus, als ich, o K\u00f6nig,", "tokens": ["\u201e", "du", "bist", "viel", "we\u00b7ni\u00b7ger", "ein", "Zeus", ",", "als", "ich", ",", "o", "K\u00f6\u00b7nig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ART", "NE", "$,", "KOUS", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ein Titan!", "tokens": ["ein", "Ti\u00b7tan", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Beherrsch\u2019 ich nicht, auf dem Du gehst, den all-", "tokens": ["Be\u00b7herr\u00b7sch'", "ich", "nicht", ",", "auf", "dem", "Du", "gehst", ",", "den", "all"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "ART", "TRUNC"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "zeit kochenden Vulkan?", "tokens": ["zeit", "ko\u00b7chen\u00b7den", "Vul\u00b7kan", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Es liegt an mir: \u2014 Ein Ruck von mir, Ein", "tokens": ["Es", "liegt", "an", "mir", ":", "Ein", "Ruck", "von", "mir", ",", "Ein"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "$(", "ART", "NN", "APPR", "PPER", "$,", "ART"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Schlag von mir zu dieser Frist,", "tokens": ["Schlag", "von", "mir", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und siehe, das Geb\u00e4ude st\u00fcrzt, von welchem Du", "tokens": ["Und", "sie\u00b7he", ",", "das", "Ge\u00b7b\u00e4u\u00b7de", "st\u00fcrzt", ",", "von", "wel\u00b7chem", "Du"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "VVFIN", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die Spitze bist!", "tokens": ["die", "Spit\u00b7ze", "bist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "\u201eder Boden birst, aufschl\u00e4gt die Gluth und sprengt", "tokens": ["\u201e", "der", "Bo\u00b7den", "birst", ",", "auf\u00b7schl\u00e4gt", "die", "Gluth", "und", "sprengt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ADV", "$,", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich krachend in die Luft!", "tokens": ["Dich", "kra\u00b7chend", "in", "die", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir aber steigen feuerfest aufw\u00e4rts an\u2019s Licht aus", "tokens": ["Wir", "a\u00b7ber", "stei\u00b7gen", "feu\u00b7er\u00b7fest", "auf\u00b7w\u00e4rts", "an's", "Licht", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "VVFIN", "ADV", "APPRART", "NN", "APPR"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "unsrer Gruft!", "tokens": ["uns\u00b7rer", "Gruft", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wir sind die Kraft! Wir h\u00e4mmern jung das alte", "tokens": ["Wir", "sind", "die", "Kraft", "!", "Wir", "h\u00e4m\u00b7mern", "jung", "das", "al\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "morsche Ding, den Staat,", "tokens": ["mor\u00b7sche", "Ding", ",", "den", "Staat", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die wir von Gottes Zorne sind bis jetzt das Pro-", "tokens": ["Die", "wir", "von", "Got\u00b7tes", "Zor\u00b7ne", "sind", "bis", "jetzt", "das", "Pro"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "letariat!", "tokens": ["le\u00b7ta\u00b7ri\u00b7at", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "\u201edann schreit\u2019 ich jauchzend durch die Welt! Auf mei-", "tokens": ["\u201e", "dann", "schreit'", "ich", "jauch\u00b7zend", "durch", "die", "Welt", "!", "Auf", "mei"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$.", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nen Schultern, stark und breit,", "tokens": ["nen", "Schul\u00b7tern", ",", "stark", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein neuer Sankt Christophorus, trag\u2019 ich den Christ", "tokens": ["Ein", "neu\u00b7er", "Sankt", "Chris\u00b7to\u00b7pho\u00b7rus", ",", "trag'", "ich", "den", "Christ"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "NE", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "der neuen Zeit!", "tokens": ["der", "neu\u00b7en", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich bin der Riese, der nicht wankt! Ich bin\u2019s,", "tokens"