{"dta.poem.11034": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schreiben  \n an  Mademoiselle  Weisen  \n in Merseburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es schrieb bishero noch kein einzigs mal", "tokens": ["Es", "schrieb", "bis\u00b7he\u00b7ro", "noch", "kein", "ein\u00b7zigs", "mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIAT", "PIS", "ADV"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Mensch, mit schwarzer Schrift und einem klugen Kiel,", "tokens": ["Ein", "Mensch", ",", "mit", "schwar\u00b7zer", "Schrift", "und", "ei\u00b7nem", "klu\u00b7gen", "Kiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu seinem Ruhm, so viel,", "tokens": ["Zu", "sei\u00b7nem", "Ruhm", ",", "so", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als sie, mit einem kiel vom Stal,", "tokens": ["Als", "sie", ",", "mit", "ei\u00b7nem", "kiel", "vom", "Stal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit weisser Schrift, und trocknen Z\u00fcgen schreibet,", "tokens": ["Mit", "weis\u00b7ser", "Schrift", ",", "und", "trock\u00b7nen", "Z\u00fc\u00b7gen", "schrei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ihren Ruhm dadurch bis auf die Nachwelt treibet.", "tokens": ["Und", "ih\u00b7ren", "Ruhm", "da\u00b7durch", "bis", "auf", "die", "Nach\u00b7welt", "trei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}