{"dta.poem.21729": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An die Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heilige Nacht, du k&#252;hlst mit leisem Fittig", "tokens": ["Hei\u00b7li\u00b7ge", "Nacht", ",", "du", "k", "&#252;", "hlst", "mit", "lei\u00b7sem", "Fit\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "$(", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Jede versengte Wange, trocknest jede", "tokens": ["Je\u00b7de", "ver\u00b7seng\u00b7te", "Wan\u00b7ge", ",", "trock\u00b7nest", "je\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "VVFIN", "PIAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Thr\u00e4nende Wimper, lullst in s\u00fcssen Frieden", "tokens": ["Thr\u00e4\u00b7nen\u00b7de", "Wim\u00b7per", ",", "lullst", "in", "s\u00fcs\u00b7sen", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Jeglichen Jammer.", "tokens": ["Jeg\u00b7li\u00b7chen", "Jam\u00b7mer", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "F\u00e4chl', o gew\u00fcnschte Nacht, auch Ihre Wangen,", "tokens": ["F\u00e4\u00b7chl'", ",", "o", "ge\u00b7w\u00fcnschte", "Nacht", ",", "auch", "Ih\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schleuss ihr die seidnen Wimper freundlich", "tokens": ["Schleuss", "ihr", "die", "seid\u00b7nen", "Wim\u00b7per", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "lullend;", "tokens": ["lul\u00b7lend", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Lispel' in s\u00fcssem ahnungreichem Traum ihr", "tokens": ["Lis\u00b7pel'", "in", "s\u00fcs\u00b7sem", "ah\u00b7nun\u00b7grei\u00b7chem", "Traum", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schmeichelnde Tr\u00f6stung,", "tokens": ["Schmei\u00b7cheln\u00b7de", "Tr\u00f6s\u00b7tung", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dass sie des Trostes froh im Schlafe l\u00e4chle,", "tokens": ["Dass", "sie", "des", "Tros\u00b7tes", "froh", "im", "Schla\u00b7fe", "l\u00e4ch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4chelnd die seidnen Wimper wieder \u00f6ffne,", "tokens": ["L\u00e4\u00b7chelnd", "die", "seid\u00b7nen", "Wim\u00b7per", "wie\u00b7der", "\u00f6ff\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sch\u00f6ner err\u00f6thend, als des jungen Morgens", "tokens": ["Sch\u00f6\u00b7ner", "er\u00b7r\u00f6\u00b7thend", ",", "als", "des", "jun\u00b7gen", "Mor\u00b7gens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Z\u00fcchtige Schimmer \u2014 \u2014", "tokens": ["Z\u00fcch\u00b7ti\u00b7ge", "Schim\u00b7mer"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Heilige Nacht, mit deinem Rabenfittig", "tokens": ["Hei\u00b7li\u00b7ge", "Nacht", ",", "mit", "dei\u00b7nem", "Ra\u00b7ben\u00b7fit\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "F\u00e4chel' auch mir die mattgesengte Schl\u00e4ffe,", "tokens": ["F\u00e4\u00b7chel'", "auch", "mir", "die", "matt\u00b7ge\u00b7seng\u00b7te", "Schl\u00e4f\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tauch' in des Schlummers Lethe dieses", "tokens": ["Tauch'", "in", "des", "Schlum\u00b7mers", "Le\u00b7the", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PDAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herzens", "tokens": ["Her\u00b7zens"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Lechzende Sehnsucht.", "tokens": ["Lech\u00b7zen\u00b7de", "Sehn\u00b7sucht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Oder umgaukle, Fantasus, mich tr\u00f6stend.", "tokens": ["O\u00b7der", "um\u00b7gauk\u00b7le", ",", "Fan\u00b7ta\u00b7sus", ",", "mich", "tr\u00f6s\u00b7tend", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NE", "$,", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wiege mich ein in ihre Feenarme,", "tokens": ["Wie\u00b7ge", "mich", "ein", "in", "ih\u00b7re", "Feen\u00b7ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Lass auf dem Schwanenflaum des edlen Busens", "tokens": ["Lass", "auf", "dem", "Schwa\u00b7nen\u00b7flaum", "des", "ed\u00b7len", "Bu\u00b7sens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nieder mich schlummern.", "tokens": ["Nie\u00b7der", "mich", "schlum\u00b7mern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Schlummern zu d\u00fcrfen, ach, in ihren Armen,", "tokens": ["Schlum\u00b7mern", "zu", "d\u00fcr\u00b7fen", ",", "ach", ",", "in", "ih\u00b7ren", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VMINF", "$,", "ITJ", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Opferten K\u00f6nige gern die Diademe,", "tokens": ["Op\u00b7fer\u00b7ten", "K\u00f6\u00b7ni\u00b7ge", "gern", "die", "Di\u00b7a\u00b7de\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gerne der Held sein Schwert, der Dichter gerne", "tokens": ["Ger\u00b7ne", "der", "Held", "sein", "Schwert", ",", "der", "Dich\u00b7ter", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "ADV"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Leyer und Lorbeer \u2014 \u2014", "tokens": ["Le\u00b7yer", "und", "Lor\u00b7beer"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "NN", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Heilige Nacht, du linderst jeden Kummer,", "tokens": ["Hei\u00b7li\u00b7ge", "Nacht", ",", "du", "lin\u00b7derst", "je\u00b7den", "Kum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kr\u00e4ftigest jeden Nerv, st\u00e4hlst jeden Muskel.", "tokens": ["Kr\u00e4f\u00b7ti\u00b7gest", "je\u00b7den", "Nerv", ",", "st\u00e4hlst", "je\u00b7den", "Mus\u00b7kel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sey denn, Gew\u00fcnschte, auch dem frommen", "tokens": ["Sey", "denn", ",", "Ge\u00b7w\u00fcnschte", ",", "auch", "dem", "from\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "NN", "$,", "ADV", "ART", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Fleher", "tokens": ["Fle\u00b7her"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Hold und gew\u00e4rtig!", "tokens": ["Hold", "und", "ge\u00b7w\u00e4r\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "++-+-", "measure": "iambic.di"}}}}}