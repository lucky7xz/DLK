{"dta.poem.21846": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Du Morfeus hast mich k\u00fchn gemacht/", "tokens": ["Du", "Mor\u00b7feus", "hast", "mich", "k\u00fchn", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "h\u00e4tt\u2019 ich dein Spielwerk nicht erfahren:", "tokens": ["h\u00e4tt'", "ich", "dein", "Spiel\u00b7werk", "nicht", "er\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wir w\u00e4ren noch/ als wie wir waren.", "tokens": ["wir", "w\u00e4\u00b7ren", "noch", "/", "als", "wie", "wir", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "KOUS", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4tt\u2019 ich di\u00df Ding doch l\u00e4ngst bedacht.", "tokens": ["H\u00e4tt'", "ich", "di\u00df", "Ding", "doch", "l\u00e4ngst", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}