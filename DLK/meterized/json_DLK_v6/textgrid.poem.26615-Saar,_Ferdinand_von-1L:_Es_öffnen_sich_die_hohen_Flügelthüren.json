{"textgrid.poem.26615": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es \u00f6ffnen sich die hohen Fl\u00fcgelth\u00fcren", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es \u00f6ffnen sich die hohen Fl\u00fcgelth\u00fcren", "tokens": ["Es", "\u00f6ff\u00b7nen", "sich", "die", "ho\u00b7hen", "Fl\u00fc\u00b7gelt\u00b7h\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und g\u00f6ttlich l\u00e4chelnd trittst du in den Saal;", "tokens": ["Und", "g\u00f6tt\u00b7lich", "l\u00e4\u00b7chelnd", "trittst", "du", "in", "den", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bei deinem Nah'n ist s\u00fc\u00dfer Hauch zu sp\u00fcren", "tokens": ["Bei", "dei\u00b7nem", "Nah'n", "ist", "s\u00fc\u00b7\u00dfer", "Hauch", "zu", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und funkeln sieht man deines Auges Strahl.", "tokens": ["Und", "fun\u00b7keln", "sieht", "man", "dei\u00b7nes", "Au\u00b7ges", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PIS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und Alles beugt entz\u00fcckt sich vor dir nieder \u2013", "tokens": ["Und", "Al\u00b7les", "beugt", "ent\u00b7z\u00fcckt", "sich", "vor", "dir", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja, deine Sch\u00f6nheit, sie ist wunderbar!", "tokens": ["Ja", ",", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "sie", "ist", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo fand man jemals solchen Reiz der Glieder,", "tokens": ["Wo", "fand", "man", "je\u00b7mals", "sol\u00b7chen", "Reiz", "der", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Solch lichtes Antlitz \u2013 und so dunkles Haar?", "tokens": ["Solch", "lich\u00b7tes", "Ant\u00b7litz", "\u2013", "und", "so", "dunk\u00b7les", "Haar", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$(", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und lauschend hangt man jetzt an deinem Munde \u2013", "tokens": ["Und", "lau\u00b7schend", "hangt", "man", "jetzt", "an", "dei\u00b7nem", "Mun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie inhaltsvoll erklingt ein jedes Wort!", "tokens": ["Wie", "in\u00b7halts\u00b7voll", "er\u00b7klingt", "ein", "je\u00b7des", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nichts scheint dir fremd auf diesem Erdenrunde,", "tokens": ["Nichts", "scheint", "dir", "fremd", "auf", "die\u00b7sem", "Er\u00b7den\u00b7run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erschlossen ist dir selbst der Weisheit Hort.", "tokens": ["Er\u00b7schlos\u00b7sen", "ist", "dir", "selbst", "der", "Weis\u00b7heit", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was K\u00fcnstler schufen und Po\u00ebten sangen,", "tokens": ["Was", "K\u00fcnst\u00b7ler", "schu\u00b7fen", "und", "Po\u00ebten", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat hell leuchtend sich dir eingepr\u00e4gt;", "tokens": ["Es", "hat", "hell", "leuch\u00b7tend", "sich", "dir", "ein\u00b7ge\u00b7pr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "PRF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Athmen ist ein zitterndes Verlangen", "tokens": ["Dein", "Ath\u00b7men", "ist", "ein", "zit\u00b7tern\u00b7des", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Allem, was im Geiste Wurzel schl\u00e4gt.", "tokens": ["Nach", "Al\u00b7lem", ",", "was", "im", "Geis\u00b7te", "Wur\u00b7zel", "schl\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und dennoch \u2013 rings gefeiert lauten Preises,", "tokens": ["Und", "den\u00b7noch", "\u2013", "rings", "ge\u00b7fei\u00b7ert", "lau\u00b7ten", "Prei\u00b7ses", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VVPP", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erf\u00fcllst du mich mit einem bitt'ren Schmerz;", "tokens": ["Er\u00b7f\u00fcllst", "du", "mich", "mit", "ei\u00b7nem", "bitt'\u00b7ren", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn sieh', ich ahn' es \u2013 besser noch, ich wei\u00df es:", "tokens": ["Denn", "sieh'", ",", "ich", "ahn'", "es", "\u2013", "bes\u00b7ser", "noch", ",", "ich", "wei\u00df", "es", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPER", "VVFIN", "PPER", "$(", "ADJD", "ADV", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es schl\u00e4gt in deiner zarten Brust kein Herz.", "tokens": ["Es", "schl\u00e4gt", "in", "dei\u00b7ner", "zar\u00b7ten", "Brust", "kein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Aus Hirn und Nerven blo\u00df besteht dein Wesen,", "tokens": ["Aus", "Hirn", "und", "Ner\u00b7ven", "blo\u00df", "be\u00b7steht", "dein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist dein Blut nur \u00bbein besond'rer Saft\u00ab;", "tokens": ["Es", "ist", "dein", "Blut", "nur", "\u00bb", "ein", "be\u00b7son\u00b7d'\u00b7rer", "Saft", "\u00ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$(", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Es giebt kein Buch, in dem du nicht gelesen \u2013", "tokens": ["Es", "giebt", "kein", "Buch", ",", "in", "dem", "du", "nicht", "ge\u00b7le\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch fehlt die Tiefe dir der Leidenschaft.", "tokens": ["Doch", "fehlt", "die", "Tie\u00b7fe", "dir", "der", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zwar spricht man auch von deinen hei\u00dfen Sinnen,", "tokens": ["Zwar", "spricht", "man", "auch", "von", "dei\u00b7nen", "hei\u00b7\u00dfen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von diesem und von jenem Seelenkampf \u2013", "tokens": ["Von", "die\u00b7sem", "und", "von", "je\u00b7nem", "See\u00b7len\u00b7kampf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "KON", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber kenne dieses irre Minnen,", "tokens": ["Ich", "a\u00b7ber", "ken\u00b7ne", "die\u00b7ses", "ir\u00b7re", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ekstase halb, halb ungest\u00fcmer Krampf.", "tokens": ["Eks\u00b7ta\u00b7se", "halb", ",", "halb", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Krampf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Bei jenes Meisters \u00bbTristan und Isolde\u00ab", "tokens": ["Bei", "je\u00b7nes", "Meis\u00b7ters", "\u00bb", "Tris\u00b7tan", "und", "I\u00b7sol\u00b7de", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst in geheimster Fiber du erregt,", "tokens": ["Wirst", "in", "ge\u00b7heims\u00b7ter", "Fi\u00b7ber", "du", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df dich nie mit seinem laut'ren Golde", "tokens": ["In\u00b7de\u00df", "dich", "nie", "mit", "sei\u00b7nem", "laut'\u00b7ren", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein schlicht empfund'nes Manneswort bewegt.", "tokens": ["Ein", "schlicht", "emp\u00b7fun\u00b7d'\u00b7nes", "Man\u00b7nes\u00b7wort", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Vergieb! Kein Vorwurf liegt in diesen Worten;", "tokens": ["Ver\u00b7gieb", "!", "Kein", "Vor\u00b7wurf", "liegt", "in", "die\u00b7sen", "Wor\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PIAT", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist die \u00e4chte Tochter deiner Zeit \u2013", "tokens": ["Du", "bist", "die", "\u00e4ch\u00b7te", "Toch\u00b7ter", "dei\u00b7ner", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Zeit, die eines neuen Daseins Pforten", "tokens": ["Der", "Zeit", ",", "die", "ei\u00b7nes", "neu\u00b7en", "Da\u00b7seins", "Pfor\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erschlossen hat, mit der Natur im Streit.", "tokens": ["Er\u00b7schlos\u00b7sen", "hat", ",", "mit", "der", "Na\u00b7tur", "im", "Streit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wohin sie f\u00fchrt, die ungeduldig schnelle,", "tokens": ["Wo\u00b7hin", "sie", "f\u00fchrt", ",", "die", "un\u00b7ge\u00b7dul\u00b7dig", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df es nicht \u2013 verschleiert liegt die Bahn;", "tokens": ["Ich", "wei\u00df", "es", "nicht", "\u2013", "ver\u00b7schlei\u00b7ert", "liegt", "die", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$(", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du aber stehst bereits an ihrer Schwelle", "tokens": ["Du", "a\u00b7ber", "stehst", "be\u00b7reits", "an", "ih\u00b7rer", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und leuchtest strahlend, wie ein Stern, voran!", "tokens": ["Und", "leuch\u00b7test", "strah\u00b7lend", ",", "wie", "ein", "Stern", ",", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}