{"textgrid.poem.52697": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am H\u00fcgel geht der Mondschein", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am H\u00fcgel geht der Mondschein", "tokens": ["Am", "H\u00fc\u00b7gel", "geht", "der", "Mond\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Hauch der Sehnsucht um,", "tokens": ["Wie", "Hauch", "der", "Sehn\u00b7sucht", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die frischen schallenden Wellen", "tokens": ["Die", "fri\u00b7schen", "schal\u00b7len\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Werden am Ufer stumm.", "tokens": ["Wer\u00b7den", "am", "U\u00b7fer", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort auf der steinernen Br\u00fccke", "tokens": ["Dort", "auf", "der", "stei\u00b7ner\u00b7nen", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Steht eine dunkle Gestalt;", "tokens": ["Steht", "ei\u00b7ne", "dunk\u00b7le", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die k\u00fchnen Augen blitzen,", "tokens": ["Die", "k\u00fch\u00b7nen", "Au\u00b7gen", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die goldene Locke wallt.", "tokens": ["Die", "gol\u00b7de\u00b7ne", "Lo\u00b7cke", "wallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Er l\u00e4\u00dft die Blicke schweifen", "tokens": ["Er", "l\u00e4\u00dft", "die", "Bli\u00b7cke", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im weiten Nachtgebiet,", "tokens": ["Im", "wei\u00b7ten", "Nacht\u00b7ge\u00b7biet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seiner Brust entsteiget", "tokens": ["Und", "sei\u00b7ner", "Brust", "ent\u00b7stei\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein schauerliches Lied.", "tokens": ["Ein", "schau\u00b7er\u00b7li\u00b7ches", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr stummen schwarzen Berge,", "tokens": ["Ihr", "stum\u00b7men", "schwar\u00b7zen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was starret ihr mich an?", "tokens": ["Was", "star\u00b7ret", "ihr", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr k\u00fchlen grauen Wellen,", "tokens": ["Ihr", "k\u00fch\u00b7len", "grau\u00b7en", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was hab ich euch gethan?", "tokens": ["Was", "hab", "ich", "euch", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was wollt ihr grauen Wellen", "tokens": ["Was", "wollt", "ihr", "grau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit eurem schaurigen Sang?", "tokens": ["Mit", "eu\u00b7rem", "schau\u00b7ri\u00b7gen", "Sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit eurem grausigen s\u00fc\u00dfen,", "tokens": ["Mit", "eu\u00b7rem", "grau\u00b7si\u00b7gen", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit eurem gierigen Klang?", "tokens": ["Mit", "eu\u00b7rem", "gie\u00b7ri\u00b7gen", "Klang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ich kann vor euch nicht schlafen,", "tokens": ["Ich", "kann", "vor", "euch", "nicht", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich kann vor euch nicht ruhn;", "tokens": ["Ich", "kann", "vor", "euch", "nicht", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was habt ihr mit meiner Liebe,", "tokens": ["Was", "habt", "ihr", "mit", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit meinem Leben zu thun?", "tokens": ["Mit", "mei\u00b7nem", "Le\u00b7ben", "zu", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Mein wissenschaftlicher Eifer", "tokens": ["Mein", "wis\u00b7sen\u00b7schaft\u00b7li\u00b7cher", "Ei\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War ehmals gar so gro\u00df \u2013", "tokens": ["War", "eh\u00b7mals", "gar", "so", "gro\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O weh, die hei\u00dfen Gedanken", "tokens": ["O", "weh", ",", "die", "hei\u00b7\u00dfen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Werde ich nimmer los!", "tokens": ["Wer\u00b7de", "ich", "nim\u00b7mer", "los", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es sungen meine Br\u00fcder", "tokens": ["Es", "sun\u00b7gen", "mei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort dr\u00fcben im trauten Haus,", "tokens": ["Dort", "dr\u00fc\u00b7ben", "im", "trau\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich h\u00f6re das Wasser rauschen", "tokens": ["Ich", "h\u00f6\u00b7re", "das", "Was\u00b7ser", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und st\u00fcrze stumm hinaus.", "tokens": ["Und", "st\u00fcr\u00b7ze", "stumm", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das M\u00e4dchen, das ich liebe,", "tokens": ["Das", "M\u00e4d\u00b7chen", ",", "das", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist so zum Sterben sch\u00f6n!", "tokens": ["Ist", "so", "zum", "Ster\u00b7ben", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich glaube fast, ihr Wellen,", "tokens": ["Ich", "glau\u00b7be", "fast", ",", "ihr", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr k\u00fchlen, wollt mich verstehn.", "tokens": ["Ihr", "k\u00fch\u00b7len", ",", "wollt", "mich", "ver\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}