{"textgrid.poem.67415": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Meine Blume", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei gegr\u00fc\u00dfet, kleine Blume,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "klei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blume der Vollkommenheit,", "tokens": ["Blu\u00b7me", "der", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die die Heiligen und Weisen", "tokens": ["Die", "die", "Hei\u00b7li\u00b7gen", "und", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Namlos preisen;", "tokens": ["Nam\u00b7los", "prei\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Denn des Herzens sch\u00f6nste Zier", "tokens": ["Denn", "des", "Her\u00b7zens", "sch\u00f6ns\u00b7te", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohnt in Dir.", "tokens": ["Wohnt", "in", "Dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Nicht auf H\u00f6hn, im stillen Thale", "tokens": ["Nicht", "auf", "H\u00f6hn", ",", "im", "stil\u00b7len", "Tha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchest Du, am frischen Quell,", "tokens": ["Bl\u00fc\u00b7hest", "Du", ",", "am", "fri\u00b7schen", "Quell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigst des weiten Himmels Bl\u00e4ue,", "tokens": ["Zeigst", "des", "wei\u00b7ten", "Him\u00b7mels", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reine Treue,", "tokens": ["Rei\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und in ihr der Sonne Gold,", "tokens": ["Und", "in", "ihr", "der", "Son\u00b7ne", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ART", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Mild und hold.", "tokens": ["Mild", "und", "hold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Fragst Du mich, wie hei\u00dft die Blume,", "tokens": ["Fragst", "Du", "mich", ",", "wie", "hei\u00dft", "die", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den hohen Schmuck uns zeigt:", "tokens": ["Die", "den", "ho\u00b7hen", "Schmuck", "uns", "zeigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnengluth und Himmelsbl\u00e4ue,", "tokens": ["Son\u00b7nen\u00b7gluth", "und", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb' und Treue?", "tokens": ["Lieb'", "und", "Treu\u00b7e", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nimm hier dies ", "tokens": ["Nimm", "hier", "dies"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "PDS"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Treu' und Licht.", "tokens": ["Treu'", "und", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}