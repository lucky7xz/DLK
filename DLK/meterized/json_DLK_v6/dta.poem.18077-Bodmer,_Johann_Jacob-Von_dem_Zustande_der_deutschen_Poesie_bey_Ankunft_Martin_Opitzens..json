{"dta.poem.18077": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Zustande der deutschen  \n Poesie bey Ankunft Martin Opitzens.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.85", "et:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckselig mu\u00df man preisen,", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "mu\u00df", "man", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die gleiche Lieb und Treu", "tokens": ["Die", "glei\u00b7che", "Lieb", "und", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einander thun erweisen,", "tokens": ["Ein\u00b7an\u00b7der", "thun", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stetigs und ohne Reu.", "tokens": ["Ste\u00b7tigs", "und", "oh\u00b7ne", "Reu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NE", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "In Noth und schweren Zeiten", "tokens": ["In", "Noth", "und", "schwe\u00b7ren", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tr\u00f6st eins des andern Leid,", "tokens": ["Tr\u00f6st", "eins", "des", "an\u00b7dern", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In Lieb und Fr\u00f6lichkeiten", "tokens": ["In", "Lieb", "und", "Fr\u00f6\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mehrt eins des andern Freud.", "tokens": ["Mehrt", "eins", "des", "an\u00b7dern", "Freud", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ist keinem angelegen", "tokens": ["Ist", "kei\u00b7nem", "an\u00b7ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was wohl und weh ihm thut,", "tokens": ["Was", "wohl", "und", "weh", "ihm", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des andern Gl\u00fcck dargegen", "tokens": ["Des", "an\u00b7dern", "Gl\u00fcck", "dar\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimmt und gibt ihm den Muth.", "tokens": ["Nimmt", "und", "gibt", "ihm", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Das s\u00fc\u00df ihn ist gemeine,", "tokens": ["Das", "s\u00fc\u00df", "ihn", "ist", "ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VAFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das bitter jedes wolt", "tokens": ["Das", "bit\u00b7ter", "je\u00b7des", "wolt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Haben f\u00fcr sich alleine,", "tokens": ["Ha\u00b7ben", "f\u00fcr", "sich", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wanns dr\u00fcber sterben solt.", "tokens": ["Wanns", "dr\u00fc\u00b7ber", "ster\u00b7ben", "solt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Alcestis uns kan geben", "tokens": ["Al\u00b7ces\u00b7tis", "uns", "kan", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dessen ein Richtigkeit,", "tokens": ["Des\u00b7sen", "ein", "Rich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die f\u00fcrs Admeti Leben", "tokens": ["Die", "f\u00fcrs", "Ad\u00b7me\u00b7ti", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "FM", "FM", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ward in den Tod bereit,", "tokens": ["Ward", "in", "den", "Tod", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die t\u00f6dlich Wund ihrs Hertzen", "tokens": ["Die", "t\u00f6d\u00b7lich", "Wund", "ihrs", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Arria gar nicht acht,", "tokens": ["Ar\u00b7ria", "gar", "nicht", "acht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "CARD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Des Stichs sie f\u00fchlt den Schmertzen,", "tokens": ["Des", "Stichs", "sie", "f\u00fchlt", "den", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der P\u00e4tum um hat bracht.", "tokens": ["Der", "P\u00e4\u00b7tum", "um", "hat", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie sich zusammen reimen,", "tokens": ["Wie", "sich", "zu\u00b7sam\u00b7men", "rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Neglein und Ro\u00dfmarein,", "tokens": ["Ne\u00b7glein", "und", "Ro\u00df\u00b7ma\u00b7rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weinreben zu R\u00fcstb\u00e4umen,", "tokens": ["Wein\u00b7re\u00b7ben", "zu", "R\u00fcst\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "K\u00f6stliche W\u00fcrtz in Wein,", "tokens": ["K\u00f6st\u00b7li\u00b7che", "W\u00fcrtz", "in", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "So schicken sich zusammen", "tokens": ["So", "schi\u00b7cken", "sich", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Mann und ehlich Weib,", "tokens": ["Ein", "Mann", "und", "eh\u00b7lich", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die werden in Gotts Nahmen", "tokens": ["Die", "wer\u00b7den", "in", "Gotts", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Sinn, ein Seel, ein Leib.", "tokens": ["Ein", "Sinn", ",", "ein", "Seel", ",", "ein", "Leib", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie Rosen an den Hecken,", "tokens": ["Wie", "Ro\u00b7sen", "an", "den", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frisch Weiden am Gestad,", "tokens": ["Frisch", "Wei\u00b7den", "am", "Ge\u00b7stad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Trauben an den St\u00f6cken,", "tokens": ["Wie", "Trau\u00b7ben", "an", "den", "St\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Zimmet und Muscat,", "tokens": ["Wie", "Zim\u00b7met", "und", "Mus\u00b7cat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Also thut sich vergleichen", "tokens": ["Al\u00b7so", "thut", "sich", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Der werthe Lingelsheim,", "tokens": ["Der", "wert\u00b7he", "Lin\u00b7gels\u00b7heim", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und die nit hat ihrs gleichen", "tokens": ["Und", "die", "nit", "hat", "ihrs", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PTKNEG", "VAFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Agnes die Jungfrau rein.", "tokens": ["Ag\u00b7nes", "die", "Jung\u00b7frau", "rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Agnes die sch\u00f6n und zarte", "tokens": ["Ag\u00b7nes", "die", "sch\u00f6n", "und", "zar\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJD", "KON", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Aus l\u00f6blichem Geschlecht", "tokens": ["Aus", "l\u00f6b\u00b7li\u00b7chem", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erbohren, und von Arte", "tokens": ["Er\u00b7boh\u00b7ren", ",", "und", "von", "Ar\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zun Tugenden gerecht.", "tokens": ["Zun", "Tu\u00b7gen\u00b7den", "ge\u00b7recht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fromm, z\u00fcchtig, keusch und g\u00fctig,", "tokens": ["Fromm", ",", "z\u00fcch\u00b7tig", ",", "keusch", "und", "g\u00fc\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verst\u00e4ndig, klug, bedacht,", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", ",", "klug", ",", "be\u00b7dacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Still, freundlich und anm\u00fcthig,", "tokens": ["Still", ",", "freund\u00b7lich", "und", "an\u00b7m\u00fct\u00b7hig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ohn allen Stoltz und Pracht.", "tokens": ["Ohn", "al\u00b7len", "Stoltz", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Unnoth hie viel zu loben", "tokens": ["Un\u00b7noth", "hie", "viel", "zu", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Di\u00df auserlesen Paar,", "tokens": ["Di\u00df", "au\u00b7ser\u00b7le\u00b7sen", "Paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jhr Ruhm und Prei\u00df erhoben", "tokens": ["Ihr", "Ruhm", "und", "Prei\u00df", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt ohn das immerdar.", "tokens": ["Bleibt", "ohn", "das", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hochzeiter und Vertraute", "tokens": ["Hoch\u00b7zei\u00b7ter", "und", "Ver\u00b7trau\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist keins am andern gf\u00e4hrt,", "tokens": ["Ist", "keins", "am", "an\u00b7dern", "gf\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Br\u00e4utgam ist der Braute,", "tokens": ["Der", "Br\u00e4ut\u00b7gam", "ist", "der", "Brau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Braut des Br\u00e4utgams werth.", "tokens": ["Die", "Braut", "des", "Br\u00e4ut\u00b7gams", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gott woll ihn beyden geben", "tokens": ["Gott", "woll", "ihn", "bey\u00b7den", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Fried und Einigkeit", "tokens": ["In", "Fried", "und", "Ei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Gsundheit langes Leben,", "tokens": ["Mit", "Gsund\u00b7heit", "lan\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df k\u00fcnfftig auch zur Zeit,", "tokens": ["Da\u00df", "k\u00fcnff\u00b7tig", "auch", "zur", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann sie werden veralten,", "tokens": ["Wann", "sie", "wer\u00b7den", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Jhr Lieb verj\u00fcnge doch,", "tokens": ["Ihr", "Lieb", "ver\u00b7j\u00fcn\u00b7ge", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann sie werden verkalten,", "tokens": ["Wann", "sie", "wer\u00b7den", "ver\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Jhr Lieb, als vor, brenn noch.", "tokens": ["Ihr", "Lieb", ",", "als", "vor", ",", "brenn", "noch", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PTKVZ", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch la\u00dft euch nicht gefehren,", "tokens": ["Doch", "la\u00dft",