{"textgrid.poem.29297": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unsers Lebens gantzer Wandel steht im lernen und vergessen;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsers Lebens gantzer Wandel steht im lernen und vergessen;", "tokens": ["Un\u00b7sers", "Le\u00b7bens", "gant\u00b7zer", "Wan\u00b7del", "steht", "im", "ler\u00b7nen", "und", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "APPRART", "VVINF", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Das vergessen nur und lernen wird gar selten recht gemessen.", "tokens": ["Das", "ver\u00b7ges\u00b7sen", "nur", "und", "ler\u00b7nen", "wird", "gar", "sel\u00b7ten", "recht", "ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KON", "VVINF", "VAFIN", "ADV", "ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Was vergessen solte bleiben, wollen wir am liebsten wissen;", "tokens": ["Was", "ver\u00b7ges\u00b7sen", "sol\u00b7te", "blei\u00b7ben", ",", "wol\u00b7len", "wir", "am", "liebs\u00b7ten", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "VVINF", "$,", "VMFIN", "PPER", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Was gelernet solte werden, wollen wir am liebsten missen.", "tokens": ["Was", "ge\u00b7ler\u00b7net", "sol\u00b7te", "wer\u00b7den", ",", "wol\u00b7len", "wir", "am", "liebs\u