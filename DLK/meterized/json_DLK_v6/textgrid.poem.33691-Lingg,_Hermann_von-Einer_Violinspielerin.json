{"textgrid.poem.33691": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Einer Violinspielerin", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heimlich schl\u00e4gt dein zartes Herz", "tokens": ["Heim\u00b7lich", "schl\u00e4gt", "dein", "zar\u00b7tes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Zauber deiner T\u00f6ne,", "tokens": ["Durch", "den", "Zau\u00b7ber", "dei\u00b7ner", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchend, wie es jeden Schmerz", "tokens": ["Su\u00b7chend", ",", "wie", "es", "je\u00b7den", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In des H\u00f6rers Brust vers\u00f6hne.", "tokens": ["In", "des", "H\u00f6\u00b7rers", "Brust", "ver\u00b7s\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Lust und alles Ach", "tokens": ["Al\u00b7le", "Lust", "und", "al\u00b7les", "Ach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rufst du auch in meiner wach.", "tokens": ["Rufst", "du", "auch", "in", "mei\u00b7ner", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An der Menschen Felsenbrust,", "tokens": ["An", "der", "Men\u00b7schen", "Fel\u00b7sen\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der N\u00e4chte bangen Stunden", "tokens": ["In", "der", "N\u00e4ch\u00b7te", "ban\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weinst du deiner Saiten Lust;", "tokens": ["Weinst", "du", "dei\u00b7ner", "Sai\u00b7ten", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich je als sch\u00f6n empfunden,", "tokens": ["Was", "ich", "je", "als", "sch\u00f6n", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "KOUS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich sehnlichst je begehrt,", "tokens": ["Was", "ich", "sehn\u00b7lichst", "je", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles, alles bist du wert!", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "bist", "du", "wert", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was gepriesen und besungen,", "tokens": ["Was", "ge\u00b7prie\u00b7sen", "und", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was geliebt ward und gelobt,", "tokens": ["Was", "ge\u00b7liebt", "ward", "und", "ge\u00b7lobt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wof\u00fcr je ein Lied erklungen,", "tokens": ["Wo\u00b7f\u00fcr", "je", "ein", "Lied", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wof\u00fcr je mein Blut getobt,", "tokens": ["Wo\u00b7f\u00fcr", "je", "mein", "Blut", "ge\u00b7tobt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jede Huldigung nimm hin,", "tokens": ["Je\u00b7de", "Hul\u00b7di\u00b7gung", "nimm", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVIMP", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zarte kleine Zauberin!", "tokens": ["Zar\u00b7te", "klei\u00b7ne", "Zau\u00b7be\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}