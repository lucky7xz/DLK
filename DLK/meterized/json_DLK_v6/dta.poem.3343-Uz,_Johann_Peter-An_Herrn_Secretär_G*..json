{"dta.poem.3343": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Secret\u00e4r G*.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Les\u2019 ich Amaliens Geschichte,", "tokens": ["Les'", "ich", "A\u00b7ma\u00b7li\u00b7ens", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die bey dem sch\u00f6nsten Angesichte", "tokens": ["Die", "bey", "dem", "sch\u00f6ns\u00b7ten", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das beste Herz und mehr Verstand besa\u00df,", "tokens": ["Das", "bes\u00b7te", "Herz", "und", "mehr", "Ver\u00b7stand", "be\u00b7sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Booth, ihr Taugenichts, der sie so oft betr\u00fcbte,", "tokens": ["Als", "Booth", ",", "ihr", "Tau\u00b7ge\u00b7nichts", ",", "der", "sie", "so", "oft", "be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So oft bey Metzen sie verga\u00df,", "tokens": ["So", "oft", "bey", "Met\u00b7zen", "sie", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihnen soff und fra\u00df,", "tokens": ["Mit", "ih\u00b7nen", "soff", "und", "fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da ihn Amalia stets gleich, stets z\u00e4rtlich liebte:", "tokens": ["Da", "ihn", "A\u00b7ma\u00b7lia", "stets", "gleich", ",", "stets", "z\u00e4rt\u00b7lich", "lieb\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADV", "ADV", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "So wallt mein schnell erregtes Blut;", "tokens": ["So", "wallt", "mein", "schnell", "er\u00b7reg\u00b7tes", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In einer Art von Wuth", "tokens": ["In", "ei\u00b7ner", "Art", "von", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Verge\u00df ich Hymens wahres Wehe:", "tokens": ["Ver\u00b7ge\u00df", "ich", "Hy\u00b7mens", "wah\u00b7res", "We\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da seufz ich nach der Ehe.", "tokens": ["Da", "seufz", "ich", "nach", "der", "E\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Doch \u00fcbersieht mein ernster Blick", "tokens": ["Doch", "\u00fc\u00b7ber\u00b7sieht", "mein", "erns\u00b7ter", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Ehen trauriges Geschick;", "tokens": ["Der", "E\u00b7hen", "trau\u00b7ri\u00b7ges", "Ge\u00b7schick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie Hymen, der die Kunst geerbet,", "tokens": ["Wie", "Hy\u00b7men", ",", "der", "die", "Kunst", "ge\u00b7er\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die Proteus aufgebracht,", "tokens": ["Die", "Pro\u00b7teus", "auf\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Das beste M\u00e4dchen ach! verderbet,", "tokens": ["Das", "bes\u00b7te", "M\u00e4d\u00b7chen", "ach", "!", "ver\u00b7der\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ITJ", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und oft in einer Nacht", "tokens": ["Und", "oft", "in", "ei\u00b7ner", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ein sanftes Lamm zum Tieger macht;", "tokens": ["Ein", "sanf\u00b7tes", "Lamm", "zum", "Tie\u00b7ger", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wie viel Vulcane sich bey ihrer Venus h\u00e4rmen,", "tokens": ["Wie", "viel", "Vul\u00b7ca\u00b7ne", "sich", "bey", "ih\u00b7rer", "Ve\u00b7nus", "h\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bey ihrem Feuer oft auch Sklaven sich erw\u00e4rmen,", "tokens": ["Bey", "ih\u00b7rem", "Feu\u00b7er", "oft", "auch", "Skla\u00b7ven", "sich", "er\u00b7w\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bey ihrer Schmach die Welt nur lacht:", "tokens": ["Bey", "ih\u00b7rer", "Schmach", "die", "Welt", "nur", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Inde\u00df die arme Treu, altv\u00e4terisch gekleidet,", "tokens": ["In\u00b7de\u00df", "die", "ar\u00b7me", "Treu", ",", "alt\u00b7v\u00e4\u00b7te\u00b7risch", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Stets hinder ihnen drein und stets vergeblich l\u00e4uft;", "tokens": ["Stets", "hin\u00b7der", "ih\u00b7nen", "drein", "und", "stets", "ver\u00b7geb\u00b7lich", "l\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Indem sie niemand h\u00f6rt, so sehr sie klagt und keift;", "tokens": ["In\u00b7dem", "sie", "nie\u00b7mand", "h\u00f6rt", ",", "so", "sehr", "sie", "klagt", "und", "keift", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie, wann ein seltnes Paar nicht H\u00f6llen-Qualen leidet,", "tokens": ["Wie", ",", "wann", "ein", "selt\u00b7nes", "Paar", "nicht", "H\u00f6l\u00b7len\u00b7Qua\u00b7len", "lei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "ADJA", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch Langeweil und Ueberdru\u00df", "tokens": ["Doch", "Lan\u00b7ge\u00b7weil", "und", "Ue\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Vom ehelichen Ku\u00df", "tokens": ["Vom", "e\u00b7he\u00b7li\u00b7chen", "Ku\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Ach! allzuselten scheidet:", "tokens": ["Ach", "!", "all\u00b7zu\u00b7sel\u00b7ten", "schei\u00b7det", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "So zittert mit gerechter Pein", "tokens": ["So", "zit\u00b7tert", "mit", "ge\u00b7rech\u00b7ter", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ein Schauer mir durch Mark und Bein;", "tokens": ["Ein", "Schau\u00b7er", "mir", "durch", "Mark", "und", "Bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "So denk ich nur an Hymens Wehe,", "tokens": ["So", "denk", "ich", "nur", "an", "Hy\u00b7mens", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "So graut mir vor der Ehe.", "tokens": ["So", "graut", "mir", "vor", "der", "E\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}