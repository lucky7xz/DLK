{"dta.poem.19287": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tod ist jedenfalls ein wicht'ger Augenblick;", "tokens": ["Der", "Tod", "ist", "je\u00b7den\u00b7falls", "ein", "wicht'\u00b7ger", "Au\u00b7gen\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie man stirbt, daran h\u00e4ngt etwas vom Geschick.", "tokens": ["Und", "wie", "man", "stirbt", ",", "da\u00b7ran", "h\u00e4ngt", "et\u00b7was", "vom", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "$,", "PAV", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Gelingt doch jeder Schritt, den man im Leben schreitet,", "tokens": ["Ge\u00b7lingt", "doch", "je\u00b7der", "Schritt", ",", "den", "man", "im", "Le\u00b7ben", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je minder oder mehr man ist dazu bereitet.", "tokens": ["Je", "min\u00b7der", "o\u00b7der", "mehr", "man", "ist", "da\u00b7zu", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "PIS", "VAFIN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So wird beim letzten auch es nicht gleichg\u00fcltig seyn,", "tokens": ["So", "wird", "beim", "letz\u00b7ten", "auch", "es", "nicht", "gleich\u00b7g\u00fcl\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "ADV", "PPER", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit welcher Fassung man hier austritt und dort ein.", "tokens": ["Mit", "wel\u00b7cher", "Fas\u00b7sung", "man", "hier", "aus\u00b7tritt", "und", "dort", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PIS", "ADV", "VVFIN", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Gewis ist f\u00f6rderlich und w\u00fcnschenswerth Besinnung", "tokens": ["Ge\u00b7wis", "ist", "f\u00f6r\u00b7der\u00b7lich", "und", "w\u00fcn\u00b7schens\u00b7werth", "Be\u00b7sin\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier zur Beendigung und dr\u00fcben zur Beginnung.", "tokens": ["Hier", "zur", "Be\u00b7en\u00b7di\u00b7gung", "und", "dr\u00fc\u00b7ben", "zur", "Be\u00b7gin\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}