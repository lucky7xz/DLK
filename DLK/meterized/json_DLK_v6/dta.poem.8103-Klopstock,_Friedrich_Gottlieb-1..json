{"dta.poem.8103": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da der Gottmensch: Werde Welt! rufte, da ward,", "tokens": ["Da", "der", "Gott\u00b7mensch", ":", "Wer\u00b7de", "Welt", "!", "ruf\u00b7te", ",", "da", "ward", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$.", "VAFIN", "NN", "$.", "VVFIN", "$,", "ADV", "VAFIN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wie der Thau tr\u00e4uft, zahllos ihr Heer, die er schuf,", "tokens": ["Wie", "der", "Thau", "tr\u00e4uft", ",", "zahl\u00b7los", "ihr", "Heer", ",", "die", "er", "schuf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADJD", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df ihr Heil stets sich erh\u00f6be. Allen rief", "tokens": ["Da\u00df", "ihr", "Heil", "stets", "sich", "er\u00b7h\u00f6\u00b7be", ".", "Al\u00b7len", "rief"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PRF", "VVFIN", "$.", "NE", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Er vom Kreuz h\u00f6heres Heil, ewiges herab!", "tokens": ["Er", "vom", "Kreuz", "h\u00f6\u00b7he\u00b7res", "Heil", ",", "e\u00b7wi\u00b7ges", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADJA", "NN", "$,", "ADJA", "ADV", "$."], "mete