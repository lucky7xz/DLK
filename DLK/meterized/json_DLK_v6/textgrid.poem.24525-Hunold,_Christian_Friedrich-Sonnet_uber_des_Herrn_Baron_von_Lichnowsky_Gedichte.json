{"textgrid.poem.24525": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet uber des Herrn Baron von Lichnowsky Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die edle Wissenschafst/ der F\u00fcrsten Zierd und Lust/", "tokens": ["Die", "ed\u00b7le", "Wis\u00b7sen\u00b7schafst", "/", "der", "F\u00fcrs\u00b7ten", "Zierd", "und", "Lust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hohen Geister Frucht/ die ein August ge\u00fcbet.", "tokens": ["Der", "ho\u00b7hen", "Geis\u00b7ter", "Frucht", "/", "die", "ein", "Au\u00b7gust", "ge\u00b7\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die mancher F\u00fcrst noch treibt/ und ein August noch liebet/", "tokens": ["Die", "man\u00b7cher", "F\u00fcrst", "noch", "treibt", "/", "und", "ein", "Au\u00b7gust", "noch", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$(", "KON", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Zunder/ Geist und Ruhm gelehrt und tapfrer Brust.", "tokens": ["Der", "Zun\u00b7der", "/", "Geist", "und", "Ruhm", "ge\u00b7lehrt", "und", "tapf\u00b7rer", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die hohen Sinnen nur/ gemeinen nie bewust.", "tokens": ["Die", "ho\u00b7hen", "Sin\u00b7nen", "nur", "/", "ge\u00b7mei\u00b7nen", "nie", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$(", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die wahren Tugenden so Glut als Leben giebet/", "tokens": ["Die", "wah\u00b7ren", "Tu\u00b7gen\u00b7den", "so", "Glut", "als", "Le\u00b7ben", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "NN", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gab Euch/ Ihr Schlesier/ die Ihr vortrefflich schriebet/", "tokens": ["Gab", "Euch", "/", "Ihr", "Schle\u00b7sier", "/", "die", "Ihr", "vor\u00b7treff\u00b7lich", "schrie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "PRELS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Di\u00df/ was du ", "tokens": ["Di\u00df", "/", "was", "du"], "token_info": ["word", "punct", "word", "word"], "pos": ["PDS", "$(", "PWS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Ich mein' Unsterblichkeit: denn was ihr seyd gewesen/", "tokens": ["Ich", "mein'", "U\u00b7nsterb\u00b7lich\u00b7keit", ":", "denn", "was", "ihr", "seyd", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "KON", "PWS", "PPER", "VAFIN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wird/ weil die Welt noch schreibt/ man mit Vergn\u00fcgen lesen.", "tokens": ["Wird", "/", "weil", "die", "Welt", "noch", "schreibt", "/", "man", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "le\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "ART", "NN", "ADV", "VVFIN", "$(", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Unsterblich schreibet ihr: zwar geht der sch\u00f6ne Lauf", "tokens": ["U\u00b7nsterb\u00b7lich", "schrei\u00b7bet", "ihr", ":", "zwar", "geht", "der", "sch\u00f6\u00b7ne", "Lauf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.12": {"text": "Das Leben manches schon/ der ewig lebt/ zu Grabe:", "tokens": ["Das", "Le\u00b7ben", "man\u00b7ches", "schon", "/", "der", "e\u00b7wig", "lebt", "/", "zu", "Gra\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADV", "$(", "ART", "ADJD", "VVFIN", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch da\u00df der Geist bey Euch den Sitz best\u00e4ndig habe.", "tokens": ["Doch", "da\u00df", "der", "Geist", "bey", "Euch", "den", "Sitz", "be\u00b7st\u00e4n\u00b7dig", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So weckt er wiederum ein edles Blut hier auf.", "tokens": ["So", "weckt", "er", "wie\u00b7de\u00b7rum", "ein", "ed\u00b7les", "Blut", "hier", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}