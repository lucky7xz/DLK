{"textgrid.poem.46219": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Herren Johan-Ernsten Herzog zu Sachsen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wol verdientes lob, frei lieblich f\u00fcrgebracht", "tokens": ["Ein", "wol", "ver\u00b7dien\u00b7tes", "lob", ",", "frei", "lieb\u00b7lich", "f\u00fcr\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "kan wol die tugend selbs verlieben und erlaben:", "tokens": ["kan", "wol", "die", "tu\u00b7gend", "selbs", "ver\u00b7lie\u00b7ben", "und", "er\u00b7la\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "daher solt billich ich, wan ich dein wert betracht,", "tokens": ["da\u00b7her", "solt", "bil\u00b7lich", "ich", ",", "wan", "ich", "dein", "wert", "be\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADJD", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dich, wie du mich mit lieb, mit hohem ruhm begaben.", "tokens": ["dich", ",", "wie", "du", "mich", "mit", "lieb", ",", "mit", "ho\u00b7hem", "ruhm", "be\u00b7ga\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "PRF", "APPR", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil aber l\u00e4ngst dein schwert, der teutschen freiheit pracht", "tokens": ["Weil", "a\u00b7ber", "l\u00e4ngst", "dein", "schwert", ",", "der", "teut\u00b7schen", "frei\u00b7heit", "pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "VVFIN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und aller frommen schutz, mit gl\u00e4nzenden buchstaben", "tokens": ["und", "al\u00b7ler", "from\u00b7men", "schutz", ",", "mit", "gl\u00e4n\u00b7zen\u00b7den", "buch\u00b7sta\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "die unausl\u00f6schlich ich mit aller welt eracht,", "tokens": ["die", "un\u00b7aus\u00b7l\u00f6schlich", "ich", "mit", "al\u00b7ler", "welt", "e\u00b7racht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "der ewigkeit dein lob ausf\u00fchrlich eingegraben:", "tokens": ["der", "e\u00b7wig\u00b7keit", "dein", "lob", "aus\u00b7f\u00fchr\u00b7lich", "ein\u00b7ge\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So hof und sprich ich noch: o zagendes Teutschland,", "tokens": ["So", "hof", "und", "sprich", "ich", "noch", ":", "o", "za\u00b7gen\u00b7des", "Teutschland", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "VVFIN", "PPER", "ADV", "$.", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "ach, da\u00df doch deinem schimpf der h\u00f6chst nu wolt verzeihen,", "tokens": ["ach", ",", "da\u00df", "doch", "dei\u00b7nem", "schimpf", "der", "h\u00f6chst", "nu", "wolt", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "KOUS", "ADV", "PPOSAT", "NN", "ART", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so w\u00fcrdest du bald frei von diesem schweren stand;", "tokens": ["so", "w\u00fcr\u00b7dest", "du", "bald", "frei", "von", "die\u00b7sem", "schwe\u00b7ren", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dan wan schon kein f\u00fcrst wolt dir seinen dienst verleihen,", "tokens": ["Dan", "wan", "schon", "kein", "f\u00fcrst", "wolt", "dir", "sei\u00b7nen", "dienst", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ADV", "PIAT", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "so w\u00fcrdest du doch frei von deiner banden schand,", "tokens": ["so", "w\u00fcr\u00b7dest", "du", "doch", "frei", "von", "dei\u00b7ner", "ban\u00b7den", "schand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "indem du diesen ", "tokens": ["in\u00b7dem", "du", "die\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}}}}}