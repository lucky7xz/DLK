{"textgrid.poem.49236": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe, was ich wil, die Tafel leugt mir nicht;", "tokens": ["Ich", "se\u00b7he", ",", "was", "ich", "wil", ",", "die", "Ta\u00b7fel", "leugt", "mir", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ist gar recht gemacht, mein Leben und mein Licht;", "tokens": ["Sie", "ist", "gar", "recht", "ge\u00b7macht", ",", "mein", "Le\u00b7ben", "und", "mein", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00f6mt sie, so d\u00fcnkt sie mich ihr sch\u00f6nes Bild zu sein,", "tokens": ["K\u00f6mt", "sie", ",", "so", "d\u00fcnkt", "sie", "mich", "ihr", "sch\u00f6\u00b7nes", "Bild", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schau' ich das Bild dann an, so fellt mein Lieb mir ein.", "tokens": ["Schau'", "ich", "das", "Bild", "dann", "an", ",", "so", "fellt", "mein", "Lieb", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ists Wunder, da\u00df sie mir so gro\u00dfe Brunst erregt,", "tokens": ["Ists", "Wun\u00b7der", ",", "da\u00df", "sie", "mir", "so", "gro\u00b7\u00dfe", "Brunst", "er\u00b7regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil blo\u00df ihr Bildni\u00df nur zur Liebe mich bewegt?", "tokens": ["Weil", "blo\u00df", "ihr", "Bild\u00b7ni\u00df", "nur", "zur", "Lie\u00b7be", "mich", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}