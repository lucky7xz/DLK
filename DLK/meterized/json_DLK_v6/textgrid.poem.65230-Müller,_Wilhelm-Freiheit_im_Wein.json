{"textgrid.poem.65230": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Freiheit im Wein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und w\u00fc\u00dft' ich, wo es besser w\u00e4r',", "tokens": ["Und", "w\u00fc\u00dft'", "ich", ",", "wo", "es", "bes\u00b7ser", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So z\u00f6g' ich aus der Welt.", "tokens": ["So", "z\u00f6g'", "ich", "aus", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "'s ist wahrlich keines Bleibens mehr", "tokens": ["'s", "ist", "wahr\u00b7lich", "kei\u00b7nes", "Blei\u00b7bens", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In diesem Erdenzelt!", "tokens": ["In", "die\u00b7sem", "Er\u00b7den\u00b7zelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hab' mit dem Teleskop von fern", "tokens": ["Hab'", "mit", "dem", "Te\u00b7les\u00b7kop", "von", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Des Himmels Rund besehn,", "tokens": ["Des", "Him\u00b7mels", "Rund", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob nicht in irgend einem Stern", "tokens": ["Ob", "nicht", "in", "ir\u00b7gend", "ei\u00b7nem", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weinst\u00f6cke sollten stehn.", "tokens": ["Wein\u00b7st\u00f6\u00b7cke", "soll\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch hab' ich keine noch entdeckt,", "tokens": ["Doch", "hab'", "ich", "kei\u00b7ne", "noch", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Herschel ist nun todt!", "tokens": ["Und", "Her\u00b7schel", "ist", "nun", "todt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn uns die Welt noch \u00e4rger neckt,", "tokens": ["Wenn", "uns", "die", "Welt", "noch", "\u00e4r\u00b7ger", "neckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin aus unsrer Noth?", "tokens": ["Wo\u00b7hin", "aus", "uns\u00b7rer", "Noth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Br\u00fcder, Br\u00fcder, schwebt mir ja", "tokens": ["O", "Br\u00fc\u00b7der", ",", "Br\u00fc\u00b7der", ",", "schwebt", "mir", "ja"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In's Blaue nicht hinaus!", "tokens": ["In's", "Blau\u00b7e", "nicht", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die beste Freistatt liegt so nah", "tokens": ["Die", "bes\u00b7te", "Freis\u00b7tatt", "liegt", "so", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In unsres Wirthes Haus.", "tokens": ["In", "uns\u00b7res", "Wirt\u00b7hes", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In seinen Keller fl\u00fcchten wir,", "tokens": ["In", "sei\u00b7nen", "Kel\u00b7ler", "fl\u00fcch\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der ist bombenfest.", "tokens": ["Und", "der", "ist", "bom\u00b7ben\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Potz alle Welt! wir trotzen dir,", "tokens": ["Potz", "al\u00b7le", "Welt", "!", "wir", "trot\u00b7zen", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Sturm du blasen l\u00e4\u00dft!", "tokens": ["Wenn", "Sturm", "du", "bla\u00b7sen", "l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wird auch die Freiheit vogelfrei", "tokens": ["Wird", "auch", "die", "Frei\u00b7heit", "vo\u00b7gel\u00b7frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier oben wohl genannt,", "tokens": ["Hier", "o\u00b7ben", "wohl", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da unten hat die Sultanei", "tokens": ["Da", "un\u00b7ten", "hat", "die", "Sul\u00b7ta\u00b7nei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie noch nicht weggebannt.", "tokens": ["Sie", "noch", "nicht", "weg\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Noch braust sie auf im jungen Wein,", "tokens": ["Noch", "braust", "sie", "auf", "im", "jun\u00b7gen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft die Reben bl\u00fchn:", "tokens": ["So", "oft", "die", "Re\u00b7ben", "bl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann will der Geist entfesselt sein", "tokens": ["Dann", "will", "der", "Geist", "ent\u00b7fes\u00b7selt", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in dem Becher gl\u00fchn.", "tokens": ["Und", "in", "dem", "Be\u00b7cher", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Und in dem Brausen toben sich", "tokens": ["Und", "in", "dem", "Brau\u00b7sen", "to\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wilden Hefen aus:", "tokens": ["Die", "wil\u00b7den", "He\u00b7fen", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der \u00e4chte Geist, er h\u00e4lt den Stich", "tokens": ["Der", "\u00e4ch\u00b7te", "Geist", ",", "er", "h\u00e4lt", "den", "Stich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und triumphirt im Strau\u00df.", "tokens": ["Und", "tri\u00b7um\u00b7phirt", "im", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Auf, Br\u00fcder, l\u00f6sen wir den Spund,", "tokens": ["Auf", ",", "Br\u00fc\u00b7der", ",", "l\u00f6\u00b7sen", "wir", "den", "Spund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und machen frei den Wein!", "tokens": ["Und", "ma\u00b7chen", "frei", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein freier Geist weih' unsern Mund", "tokens": ["Sein", "frei\u00b7er", "Geist", "weih'", "un\u00b7sern", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu freien Liedern ein!", "tokens": ["Zu", "frei\u00b7en", "Lie\u00b7dern", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}