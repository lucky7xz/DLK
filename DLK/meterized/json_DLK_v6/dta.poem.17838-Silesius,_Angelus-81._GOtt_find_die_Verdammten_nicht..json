{"dta.poem.17838": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "81.  GOtt find die Verdammten nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott kan schon ewiglich nicht die Verdammten finden;", "tokens": ["Gott", "kan", "schon", "e\u00b7wig\u00b7lich", "nicht", "die", "Ver\u00b7damm\u00b7ten", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie st\u00e4ts durch ihrn willn f\u00fcr ihm inn Pful ver-", "tokens": ["Weil", "sie", "st\u00e4ts", "durch", "ihrn", "willn", "f\u00fcr", "ihm", "inn", "Pful", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PPER", "APPR", "NN", "TRUNC"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "schwinden.", "tokens": ["schwin\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}