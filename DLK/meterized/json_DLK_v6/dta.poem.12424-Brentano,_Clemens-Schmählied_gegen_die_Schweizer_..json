{"dta.poem.12424": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Schm\u00e4hlied gegen die Schweizer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf ich h\u00f6r ein neu Get\u00f6n,               ", "tokens": ["Wohl\u00b7auf", "ich", "h\u00f6r", "ein", "neu", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der edlen V\u00f6gel Sang,", "tokens": ["Der", "ed\u00b7len", "V\u00f6\u00b7gel", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich trau es werde nun ganz sch\u00f6n,", "tokens": ["Ich", "trau", "es", "wer\u00b7de", "nun", "ganz", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unwetter hat so lang", "tokens": ["Un\u00b7wet\u00b7ter", "hat", "so", "lang"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geregnet auf der Heide,", "tokens": ["Ge\u00b7reg\u00b7net", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Blumen sind erfrorn,", "tokens": ["Die", "Blu\u00b7men", "sind", "er\u00b7frorn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem Adel, als zum Leide,", "tokens": ["Dem", "A\u00b7del", ",", "als", "zum", "Lei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Bauern zusammen schworen.", "tokens": ["Die", "Bau\u00b7ern", "zu\u00b7sam\u00b7men", "schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Wolken sind zum Berg gedr\u00fcckt,", "tokens": ["Die", "Wol\u00b7ken", "sind", "zum", "Berg", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schafft der Sonne Glanz,", "tokens": ["Das", "schafft", "der", "Son\u00b7ne", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Bauern wird ihr Gewalt entr\u00fcckt,", "tokens": ["Den", "Bau\u00b7ern", "wird", "ihr", "Ge\u00b7walt", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das thut der Pfauen Schwanz;", "tokens": ["Das", "thut", "der", "Pfau\u00b7en", "Schwanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun Kuh so la\u00df dein Lugen,", "tokens": ["Nun", "Kuh", "so", "la\u00df", "dein", "Lu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geh heim, hab gut Gemach,", "tokens": ["Geh", "heim", ",", "hab", "gut", "Ge\u00b7mach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Herren ekelt dein Mugen,", "tokens": ["Den", "Her\u00b7ren", "e\u00b7kelt", "dein", "Mu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Trink aus dem M\u00fchlenbach.", "tokens": ["Trink", "aus", "dem", "M\u00fch\u00b7len\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und bliebest du daheime,", "tokens": ["Und", "blie\u00b7best", "du", "da\u00b7hei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du h\u00e4ttest gute Weid,", "tokens": ["Du", "h\u00e4t\u00b7test", "gu\u00b7te", "Weid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dich betr\u00fcbte keiner,", "tokens": ["Und", "dich", "be\u00b7tr\u00fcb\u00b7te", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dir gescheh kein Leid,", "tokens": ["Und", "dir", "ge\u00b7scheh", "kein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du thatst zu weit ausbrechen,", "tokens": ["Du", "thatst", "zu", "weit", "aus\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das thut dem Adel Zorn,", "tokens": ["Das", "thut", "dem", "A\u00b7del", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das kommt von deinem Stechen,", "tokens": ["Das", "kommt", "von", "dei\u00b7nem", "Ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Man schl\u00e4gt dich auf dein Horn.", "tokens": ["Man", "schl\u00e4gt", "dich", "auf", "dein", "Horn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Bauern treiben Wunder,", "tokens": ["Die", "Bau\u00b7ern", "trei\u00b7ben", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Uebermuth ist gro\u00df,", "tokens": ["Ihr", "Ue\u00b7ber\u00b7muth", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Schwitz und Glarus besunder,", "tokens": ["In", "Schwitz", "und", "Gla\u00b7rus", "be\u00b7sun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Niemand ist ihr Geno\u00df;", "tokens": ["Nie\u00b7mand", "ist", "ihr", "Ge\u00b7no\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sie tragen jezt die Krone,", "tokens": ["Sie", "tra\u00b7gen", "jezt", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor Ritter und vor Knecht,", "tokens": ["Vor", "Rit\u00b7ter", "und", "vor", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird ihnen nun der Lohne,", "tokens": ["Wird", "ih\u00b7nen", "nun", "der", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist nicht wider Recht.", "tokens": ["Das", "ist", "nicht", "wi\u00b7der", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der uns dies Liedlein hat gemacht,", "tokens": ["Der", "uns", "dies", "Lied\u00b7lein", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist von Isenhofen,", "tokens": ["Der", "ist", "von", "I\u00b7sen\u00b7ho\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Bauern hatten sein kein Acht,", "tokens": ["Die", "Bau\u00b7ern", "hat\u00b7ten", "sein", "kein", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "PIAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als er sa\u00df hinterm Ofen,", "tokens": ["Als", "er", "sa\u00df", "hin\u00b7term", "O\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und horchet ihrem Rathe,", "tokens": ["Und", "hor\u00b7chet", "ih\u00b7rem", "Ra\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und was sie wollten treiben,", "tokens": ["Und", "was", "sie", "woll\u00b7ten", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An einem Abend spate,", "tokens": ["An", "ei\u00b7nem", "A\u00b7bend", "spa\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er will es nicht verschweigen.", "tokens": ["Er", "will", "es", "nicht", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Bauer sah im Glase", "tokens": ["Ein", "Bau\u00b7er", "sah", "im", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den hellen Farbenschein,", "tokens": ["Den", "hel\u00b7len", "Far\u00b7ben\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er warf, als ob er rase", "tokens": ["Er", "warf", ",", "als", "ob", "er", "ra\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinaus es in den Rhein:", "tokens": ["Hin\u00b7aus", "es", "in", "den", "Rhein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eo Pfauenschwanz ich sehe", "tokens": ["\u201e", "o", "Pfau\u00b7en\u00b7schwanz", "ich", "se\u00b7he"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "FM", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201edich doch an allem Ort,", "tokens": ["\u201e", "dich", "doch", "an", "al\u00b7lem", "Ort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201eso soll es dir auch gehen.\u201c", "tokens": ["\u201e", "so", "soll", "es", "dir", "auch", "ge\u00b7hen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er sprach ein grimmig Wort.", "tokens": ["Er", "sprach", "ein", "grim\u00b7mig", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie sprachen: \u201eWir sind Herren", "tokens": ["Sie", "spra\u00b7chen", ":", "\u201e", "Wir", "sind", "Her\u00b7ren"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201evon unsrem Land und Leut,", "tokens": ["\u201e", "von", "uns\u00b7rem", "Land", "und", "Leut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eder K\u00f6nig soll es nicht wehren,", "tokens": ["\u201e", "der", "K\u00f6\u00b7nig", "soll", "es", "nicht", "weh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201ewir geben um ihn nichts;", "tokens": ["\u201e", "wir", "ge\u00b7ben", "um", "ihn", "nichts", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPER", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eer wollte uns gern spalten,", "tokens": ["\u201e", "er", "woll\u00b7te", "uns", "gern", "spal\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eund das liegt an dem Tag,", "tokens": ["\u201e", "und", "das", "liegt", "an", "dem", "Tag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201edas B\u00fcndel Ruthen soll halten,", "tokens": ["\u201e", "das", "B\u00fcn\u00b7del", "Ru\u00b7then", "soll", "hal\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "\u201edoch mancher Herr noch klag.\u201c", "tokens": ["\u201e", "doch", "man\u00b7cher", "Herr", "noch", "klag", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und fr\u00fche vor dem Morgen", "tokens": ["Und", "fr\u00fc\u00b7he", "vor", "dem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hob mich von dannen bald,", "tokens": ["Ich", "hob", "mich", "von", "dan\u00b7nen", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich lief dahin mit Sorgen,", "tokens": ["Ich", "lief", "da\u00b7hin", "mit", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl oben durch den Wald,", "tokens": ["Wohl", "o\u00b7ben", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und da ich kam auf die Heide,", "tokens": ["Und", "da", "ich", "kam", "auf", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da hab ich dies gesungen,", "tokens": ["Da", "hab", "ich", "dies", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Frommen nicht zu Leide,", "tokens": ["Den", "From\u00b7men", "nicht", "zu", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df Feld und Wald erklungen.", "tokens": ["Da\u00df", "Feld", "und", "Wald", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}