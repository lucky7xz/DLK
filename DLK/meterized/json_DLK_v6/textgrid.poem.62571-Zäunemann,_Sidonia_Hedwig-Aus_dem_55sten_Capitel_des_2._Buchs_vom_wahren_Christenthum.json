{"textgrid.poem.62571": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem 55sten Capitel des 2. Buchs vom wahren Christenthum", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzage nicht mein Christ in deiner Noth und Pein,", "tokens": ["Ver\u00b7za\u00b7ge", "nicht", "mein", "Christ", "in", "dei\u00b7ner", "Noth", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprich, der Herr will nicht mein Schutz und Helfer seyn;", "tokens": ["Und", "sprich", ",", "der", "Herr", "will", "nicht", "mein", "Schutz", "und", "Hel\u00b7fer", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ART", "NN", "VMFIN", "PTKNEG", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es will mich seine Hand bey meinem Fall nicht fassen;", "tokens": ["Es", "will", "mich", "sei\u00b7ne", "Hand", "bey", "mei\u00b7nem", "Fall", "nicht", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott hat mich ganz und gar in meiner Noth verlassen,", "tokens": ["Gott", "hat", "mich", "ganz", "und", "gar", "in", "mei\u00b7ner", "Noth", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Christ besinne dich! es ist gewi\u00df nicht so!", "tokens": ["Mein", "Christ", "be\u00b7sin\u00b7ne", "dich", "!", "es", "ist", "ge\u00b7wi\u00df", "nicht", "so", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gott machet dich im Kreuz mit Trost und H\u00fclfe froh.", "tokens": ["Gott", "ma\u00b7chet", "dich", "im", "Kreuz", "mit", "Trost", "und", "H\u00fcl\u00b7fe", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er steht dir kr\u00e4ftig bey; der Herr will nicht verweilen,", "tokens": ["Er", "steht", "dir", "kr\u00e4f\u00b7tig", "bey", ";", "der", "Herr", "will", "nicht", "ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was du Verziehen nennst, das ist bey Gott ein Eilen.", "tokens": ["Was", "du", "Ver\u00b7zie\u00b7hen", "nennst", ",", "das", "ist", "bey", "Gott", "ein", "Ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,", "PDS", "VAFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn, weil der treue Gott nach seinem weisen Rath,", "tokens": ["Denn", ",", "weil", "der", "treu\u00b7e", "Gott", "nach", "sei\u00b7nem", "wei\u00b7sen", "Rath", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dir deine Noth gesetzt, und abgemessen hat;", "tokens": ["Dir", "dei\u00b7ne", "Noth", "ge\u00b7setzt", ",", "und", "ab\u00b7ge\u00b7mes\u00b7sen", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So darf kein Thr\u00e4nlein mehr aus deinen Augen rinnen,", "tokens": ["So", "darf", "kein", "Thr\u00e4n\u00b7lein", "mehr", "aus", "dei\u00b7nen", "Au\u00b7gen", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als Gott bestimmet hat. O g\u00fctiges Beginnen!", "tokens": ["Als", "Gott", "be\u00b7stim\u00b7met", "hat", ".", "O", "g\u00fc\u00b7ti\u00b7ges", "Be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum auf! verzweifle nicht! stellt sich gleich Kreuz und Pein,", "tokens": ["Drum", "auf", "!", "ver\u00b7zweif\u00b7le", "nicht", "!", "stellt", "sich", "gleich", "Kreuz", "und", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$.", "VVFIN", "PTKNEG", "$.", "VVFIN", "PRF", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Im Anfang, wie du meinst, mit vollen Haufen ein;", "tokens": ["Im", "An\u00b7fang", ",", "wie", "du", "meinst", ",", "mit", "vol\u00b7len", "Hau\u00b7fen", "ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es mu\u00df doch endlich noch das dir bestimmte Leiden,", "tokens": ["Es", "mu\u00df", "doch", "end\u00b7lich", "noch", "das", "dir", "be\u00b7stimm\u00b7te", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nachdem es ausgera\u00dft, aus deiner Wohnung scheiden.", "tokens": ["Nach\u00b7dem", "es", "aus\u00b7ger\u00b7a\u00dft", ",", "aus", "dei\u00b7ner", "Woh\u00b7nung", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum denke, lieber Mensch, bey jeder Noth und Schmach,", "tokens": ["Drum", "den\u00b7ke", ",", "lie\u00b7ber", "Mensch", ",", "bey", "je\u00b7der", "Noth", "und", "Schmach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADV", "NN", "$,", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Gott lob! des Kreuzes Zahl l\u00e4\u00dft alle Tage nach,", "tokens": ["Gott", "lob", "!", "des", "Kreu\u00b7zes", "Zahl", "l\u00e4\u00dft", "al\u00b7le", "Ta\u00b7ge", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "NN", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Und wird stets weniger. Was willst du denn verzagen?", "tokens": ["Und", "wird", "stets", "we\u00b7ni\u00b7ger", ".", "Was", "willst", "du", "denn", "ver\u00b7za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zu dem so hilft dir Gott auch selbst das Leiden tragen,", "tokens": ["Zu", "dem", "so", "hilft", "dir", "Gott", "auch", "selbst", "das", "Lei\u00b7den", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "VVFIN", "PPER", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und beut dir seine Hand bey jedem Tritt und Schritt,", "tokens": ["Und", "beut", "dir", "sei\u00b7ne", "Hand", "bey", "je\u00b7dem", "Tritt", "und", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wobey er dich zugleich mit Tr\u00f6stung \u00fcbersch\u00fctt.", "tokens": ["Wo\u00b7bey", "er", "dich", "zu\u00b7gleich", "mit", "Tr\u00f6s\u00b7tung", "\u00fc\u00b7ber\u00b7sch\u00fctt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und also hast du doch im Kreuz die Ruh der Seelen,", "tokens": ["Und", "al\u00b7so", "hast", "du", "doch", "im", "Kreuz", "die", "Ruh", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und kanst des Herren Gnad vor seinem Volk erzehlen.", "tokens": ["Und", "kanst", "des", "Her\u00b7ren", "Gnad", "vor", "sei\u00b7nem", "Volk", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer aber seine Noth, die Gott ihm aufgelegt,", "tokens": ["Wer", "a\u00b7ber", "sei\u00b7ne", "Noth", ",", "die", "Gott", "ihm", "auf\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit Zagheit, Ungedult und schn\u00f6dem Murren tr\u00e4gt:", "tokens": ["Mit", "Zag\u00b7heit", ",", "Un\u00b7ge\u00b7dult", "und", "schn\u00f6\u00b7dem", "Mur\u00b7ren", "tr\u00e4gt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dem wird sein Kreuz zur Last, die Seele wird beschweret,", "tokens": ["Dem", "wird", "sein", "Kreuz", "zur", "Last", ",", "die", "See\u00b7le", "wird", "be\u00b7schwe\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Er weis von keinem Trost, wenn ihm was wiederf\u00e4hret.", "tokens": ["Er", "weis", "von", "kei\u00b7nem", "Trost", ",", "wenn", "ihm", "was", "wie\u00b7der\u00b7f\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drum, Seele gieb dich drein, sich nur auf Jesum Christ,", "tokens": ["Drum", ",", "See\u00b7le", "gieb", "dich", "drein", ",", "sich", "nur", "auf", "Je\u00b7sum", "Christ", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "VVIMP", "PPER", "PTKVZ", "$,", "PRF", "ADV", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie er gelitten hat, und still gewesen ist.", "tokens": ["Wie", "er", "ge\u00b7lit\u00b7ten", "hat", ",", "und", "still", "ge\u00b7we\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "KON", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Demselben folge nach, so wirst du Ruhe finden.", "tokens": ["Dem\u00b7sel\u00b7ben", "fol\u00b7ge", "nach", ",", "so", "wirst", "du", "Ru\u00b7he", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Such auch des Glaubens Licht bey dir recht anzuz\u00fcnden,", "tokens": ["Such", "auch", "des", "Glau\u00b7bens", "Licht", "bey", "dir", "recht", "an\u00b7zu\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "NN", "APPR", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So wird dein Herz davon ganz stark und frey gemacht,", "tokens": ["So", "wird", "dein", "Herz", "da\u00b7von", "ganz", "stark", "und", "frey", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PAV", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und Gott ist desto mehr auf seine H\u00fclf bedacht.", "tokens": ["Und", "Gott", "ist", "des\u00b7to", "mehr", "auf", "sei\u00b7ne", "H\u00fclf", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der weise K\u00f6nig spricht aus seinem klugen Munde:", "tokens": ["Der", "wei\u00b7se", "K\u00f6\u00b7nig", "spricht", "aus", "sei\u00b7nem", "klu\u00b7gen", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein jedes Thun und Werk hat seine Zeit und Stunde.", "tokens": ["Ein", "je\u00b7des", "Thun", "und", "Werk", "hat", "sei\u00b7ne", "Zeit", "und", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Drum ist das Kreuz uns nicht von ohngefehr gesandt;", "tokens": ["Drum", "ist", "das", "Kreuz", "uns", "nicht", "von", "ohn\u00b7ge\u00b7fehr", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nein, Gottes weiser Rath, und dessen hohe Hand", "tokens": ["Nein", ",", "Got\u00b7tes", "wei\u00b7ser", "Rath", ",", "und", "des\u00b7sen", "ho\u00b7he", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "ADJA", "NN", "$,", "KON", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Hat schon vorher versehn, verordnet und gemessen,", "tokens": ["Hat", "schon", "vor\u00b7her", "ver\u00b7sehn", ",", "ver\u00b7ord\u00b7net", "und", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVINF", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie viele Thr\u00e4nen wir aus unsern Augen pressen.", "tokens": ["Wie", "vie\u00b7le", "Thr\u00e4\u00b7nen", "wir", "aus", "un\u00b7sern", "Au\u00b7gen", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Gl\u00fcck, Ungl\u00fcck, Ehr und Schmach, Geld, Armuth, Schmerz und Spott,", "tokens": ["Gl\u00fcck", ",", "Un\u00b7gl\u00fcck", ",", "Ehr", "und", "Schmach", ",", "Geld", ",", "Ar\u00b7muth", ",", "Schmerz", "und", "Spott", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das Leben und die Gruft kommt nur allein von Gott.", "tokens": ["Das", "Le\u00b7ben", "und", "die", "Gruft", "kommt", "nur", "al\u00b7lein", "von", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die\u00df ist gewi\u00df ein Wort, darauf wir auch im Grauen,", "tokens": ["Die\u00df", "ist", "ge\u00b7wi\u00df", "ein", "Wort", ",", "da\u00b7rauf", "wir", "auch", "im", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,", "PAV", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "All unsre Zuversicht und s\u00fc\u00dfe Hoffnung bauen.", "tokens": ["All", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "und", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ward Joseph nicht versucht in seiner Jugend-Zeit,", "tokens": ["Ward", "Jo\u00b7se\u00b7ph", "nicht", "ver\u00b7sucht", "in", "sei\u00b7ner", "Ju\u00b7gend\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "Mit Neid, Verfolgung Ha\u00df, Gef\u00e4ngni\u00df, Traurigkeit?", "tokens": ["Mit", "Neid", ",", "Ver\u00b7fol\u00b7gung", "Ha\u00df", ",", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", ",", "Trau\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und gleichwohl da er die\u00df gedultig ausgestanden,", "tokens": ["Und", "gleich\u00b7wohl", "da", "er", "die\u00df", "ge\u00b7dul\u00b7tig", "aus\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "PDS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "So war des H\u00f6chsten Gnad auch desto mehr vorhanden.", "tokens": ["So", "war", "des", "H\u00f6chs\u00b7ten", "Gnad", "auch", "des\u00b7to", "mehr", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Er brachte seine Zeit, die Gott gesetzt, mit Ruh,", "tokens": ["Er", "brach\u00b7te", "sei\u00b7ne", "Zeit", ",", "die", "Gott", "ge\u00b7setzt", ",", "mit", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Mit Ehr und Herrlichkeit, und vielem Seegen zu.", "tokens": ["Mit", "Ehr", "und", "Herr\u00b7lich\u00b7keit", ",", "und", "vie\u00b7lem", "See\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KON", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Drum, ob wohl mancher Christ auf eine Zeit viel leidet,", "tokens": ["Drum", ",", "ob", "wohl", "man\u00b7cher", "Christ", "auf", "ei\u00b7ne", "Zeit", "viel", "lei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Wird er gedruckt, verfolgt, verl\u00e4stert und geneidet;", "tokens": ["Wird", "er", "ge\u00b7druckt", ",", "ver\u00b7folgt", ",", "ver\u00b7l\u00e4s\u00b7tert", "und", "ge\u00b7nei\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "So w\u00e4hrt es doch nicht stets. Der Tag kommt dennoch an,", "tokens": ["So", "w\u00e4hrt", "es", "doch", "nicht", "stets", ".", "Der", "Tag", "kommt", "den\u00b7noch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$.", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Da man von Gottes G\u00fct und H\u00fclfe sagen kan.", "tokens": ["Da", "man", "von", "Got\u00b7tes", "G\u00fct", "und", "H\u00fcl\u00b7fe", "sa\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Gott, der die Tr\u00fcbsal ruft, und ihre Gr\u00e4nzen setzet,", "tokens": ["Gott", ",", "der", "die", "Tr\u00fcb\u00b7sal", "ruft", ",", "und", "ih\u00b7re", "Gr\u00e4n\u00b7zen", "set\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.56": {"text": "Der winket auch der Zeit, die unser Herz erg\u00f6tzet,", "tokens": ["Der", "win\u00b7ket", "auch", "der", "Zeit", ",", "die", "un\u00b7ser", "Herz", "er\u00b7g\u00f6t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Und zieht die Stunde vor, darinnen er uns tr\u00f6st,", "tokens": ["Und", "zieht", "die", "Stun\u00b7de", "vor", ",", "da\u00b7rin\u00b7nen", "er", "uns", "tr\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Und uns von unsrer Noth befreyet und erl\u00f6\u00dft.", "tokens": ["Und", "uns", "von", "uns\u00b7rer", "Noth", "be\u00b7fre\u00b7yet", "und", "er\u00b7l\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ein Vater \u00fcberlegt, wie viel in jungen Tagen,", "tokens": ["Ein", "Va\u00b7ter", "\u00fc\u00b7ber\u00b7legt", ",", "wie", "viel", "in", "jun\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Sein Kind verm\u00f6gend sey von Arbeit zu ertragen:", "tokens": ["Sein", "Kind", "ver\u00b7m\u00f6\u00b7gend", "sey", "von", "Ar\u00b7beit", "zu", "er\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Und hiernach theilt er auch die Arbeit kl\u00fcglich ein,", "tokens": ["Und", "hier\u00b7nach", "theilt", "er", "auch", "die", "Ar\u00b7beit", "kl\u00fcg\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Und wei\u00df, wenn es damit zu Stande k\u00f6nte seyn.", "tokens": ["Und", "wei\u00df", ",", "wenn", "es", "da\u00b7mit", "zu", "Stan\u00b7de", "k\u00f6n\u00b7te", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PAV", "PTKZU", "VVINF", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Gott handelt gleich also: Er kennet unsre Kr\u00e4fte,", "tokens": ["Gott", "han\u00b7delt", "gleich", "al\u00b7so", ":", "Er", "ken\u00b7net", "uns\u00b7re", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.64": {"text": "Wie viel die Schulter tr\u00e4gt; daher auch die Gesch\u00e4fte,", "tokens": ["Wie", "viel", "die", "Schul\u00b7ter", "tr\u00e4gt", ";", "da\u00b7her", "auch", "die", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$.", "PAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "So er uns auferlegt, in gleicher Wage stehn,", "tokens": ["So", "er", "uns", "auf\u00b7er\u00b7legt", ",", "in", "glei\u00b7cher", "Wa\u00b7ge", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VVPP", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Und niemahls \u00fcber uns und unsre St\u00e4rke gehn.", "tokens": ["Und", "nie\u00b7mahls", "\u00fc\u00b7ber", "uns", "und", "uns\u00b7re", "St\u00e4r\u0