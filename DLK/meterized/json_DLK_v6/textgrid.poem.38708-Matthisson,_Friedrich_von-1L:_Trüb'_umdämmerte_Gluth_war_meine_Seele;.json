{"textgrid.poem.38708": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00fcb' umd\u00e4mmerte Gluth war meine Seele;", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb' umd\u00e4mmerte Gluth war meine Seele;", "tokens": ["Tr\u00fcb'", "um\u00b7d\u00e4m\u00b7mer\u00b7te", "Gluth", "war", "mei\u00b7ne", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Eine Flamme vom Himmel, rein, wie Hespers", "tokens": ["Ei\u00b7ne", "Flam\u00b7me", "vom", "Him\u00b7mel", ",", "rein", ",", "wie", "Hes\u00b7pers"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ADJD", "$,", "PWAV", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aetherfeuer im Sp\u00e4troth, war die be\u00dfre", "tokens": ["A\u00b7e\u00b7ther\u00b7feu\u00b7er", "im", "Sp\u00e4t\u00b7roth", ",", "war", "die", "be\u00df\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "VAFIN", "ART", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Seele des M\u00e4dchens.", "tokens": ["See\u00b7le", "des", "M\u00e4d\u00b7chens", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch der Erde bew\u00f6lkte Gluth verkl\u00e4rte", "tokens": ["Doch", "der", "Er\u00b7de", "be\u00b7w\u00f6lk\u00b7te", "Gluth", "ver\u00b7kl\u00e4r\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sich im Feuer des Himmels. Ewig lodern", "tokens": ["Sich", "im", "Feu\u00b7er", "des", "Him\u00b7mels", ".", "E\u00b7wig", "lo\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "$.", "ADJD", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Heilige Liebe!", "tokens": ["Hei\u00b7li\u00b7ge", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}