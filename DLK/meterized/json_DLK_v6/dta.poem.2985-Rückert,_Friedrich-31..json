{"dta.poem.2985": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.57", "et:0.28", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wikramaditia, Hindustans Oberk\u00f6nig,", "tokens": ["Wik\u00b7ra\u00b7ma\u00b7di\u00b7tia", ",", "Hin\u00b7dus\u00b7tans", "O\u00b7ber\u00b7k\u00f6\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Dem sieben K\u00f6nige, die m\u00e4chtigsten sind fr\u00f6hnig,", "tokens": ["Dem", "sie\u00b7ben", "K\u00f6\u00b7ni\u00b7ge", ",", "die", "m\u00e4ch\u00b7tigs\u00b7ten", "sind", "fr\u00f6h\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Nicht darauf ist er stolz, stolz ist er darauf blo\u00df;", "tokens": ["Nicht", "da\u00b7rauf", "ist", "er", "stolz", ",", "stolz", "ist", "er", "da\u00b7rauf", "blo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "PAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sieben Dichter hat vereint sein F\u00fcrstenschlo\u00df.", "tokens": ["Da\u00df", "sie\u00b7ben", "Dich\u00b7ter", "hat", "ver\u00b7eint", "sein", "F\u00fcrs\u00b7ten\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "An seiner Krone sind sie sieben Edelsteine,", "tokens": ["An", "sei\u00b7ner", "Kro\u00b7ne", "sind", "sie", "sie\u00b7ben", "E\u00b7del\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dadurch ewig stralt mit unverwelktem Scheine.", "tokens": ["Die", "da\u00b7durch", "e\u00b7wig", "stralt", "mit", "un\u00b7ver\u00b7welk\u00b7tem", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In Tr\u00fcmmer hat die Zeit gelegt sein K\u00f6nigthum,", "tokens": ["In", "Tr\u00fcm\u00b7mer", "hat", "die", "Zeit", "ge\u00b7legt", "sein", "K\u00f6\u00b7nig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein sein Name steht mit Kalidasas Ruhm;", "tokens": ["Al\u00b7lein", "sein", "Na\u00b7me", "steht", "mit", "Ka\u00b7li\u00b7da\u00b7sas", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Des Kalidasa, der Sakuntala gedichtet,", "tokens": ["Des", "Ka\u00b7li\u00b7da\u00b7sa", ",", "der", "Sa\u00b7kun\u00b7ta\u00b7la", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von der im Abendland nun auch der Ruf berichtet;", "tokens": ["Von", "der", "im", "A\u00b7bend\u00b7land", "nun", "auch", "der", "Ruf", "be\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Im Abendlande, wo zu gleichem Preis und Lob,", "tokens": ["Im", "A\u00b7bend\u00b7lan\u00b7de", ",", "wo", "zu", "glei\u00b7chem", "Preis", "und", "Lob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Indiens gr\u00f6ster F\u00fcrst, ein kleinster sich erhob:", "tokens": ["Wie", "In\u00b7di\u00b7ens", "gr\u00f6s\u00b7ter", "F\u00fcrst", ",", "ein", "kleins\u00b7ter", "sich", "er\u00b7hob", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "$,", "ART", "ADJA", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.7": {"line.1": {"text": "Der soviel stralende Gestirn' um seinen Thron", "tokens": ["Der", "so\u00b7viel", "stra\u00b7len\u00b7de", "Ge\u00b7stirn'", "um", "sei\u00b7nen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versammelt, da\u00df auch er auf ewig stralt davon;", "tokens": ["Ver\u00b7sam\u00b7melt", ",", "da\u00df", "auch", "er", "auf", "e\u00b7wig", "stralt", "da\u00b7von", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADV", "PPER", "APPR", "ADJD", "VVFIN", "PAV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "In dessen F\u00fcrsten-Pfleg' ein F\u00fcrst der Genien", "tokens": ["In", "des\u00b7sen", "F\u00fcrs\u00b7ten\u00b7Pfleg'", "ein", "F\u00fcrst", "der", "Ge\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eleonoren schuf und Ifigenien:", "tokens": ["E\u00b7leo\u00b7no\u00b7ren", "schuf", "und", "I\u00b7fi\u00b7ge\u00b7ni\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NE", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Der F\u00fcrst verdiente, da\u00df gerechnet, gleich der Aere", "tokens": ["Der", "F\u00fcrst", "ver\u00b7dien\u00b7te", ",", "da\u00df", "ge\u00b7rech\u00b7net", ",", "gleich", "der", "A\u00b7e\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "VVPP", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wikramaditias, nach ihm auch eine w\u00e4re;", "tokens": ["Wik\u00b7ra\u00b7ma\u00b7di\u00b7ti\u00b7as", ",", "nach", "ihm", "auch", "ei\u00b7ne", "w\u00e4\u00b7re", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPER", "ADV", "ART", "VAFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.10": {"line.1": {"text": "Der seine Stimme nicht lie\u00df mit im Chor erschallen,", "tokens": ["Der", "sei\u00b7ne", "Stim\u00b7me", "nicht", "lie\u00df", "mit", "im", "Chor", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVFIN", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch still der Mittelpunkt war der Begeistrung allen;", "tokens": ["Doch", "still", "der", "Mit\u00b7tel\u00b7punkt", "war", "der", "Be\u00b7geis\u00b7trung", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VAFIN", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Ihr F\u00fcrst nicht, sondern Freund (den Ruhm soll ihm entrei\u00dfen", "tokens": ["Ihr", "F\u00fcrst", "nicht", ",", "son\u00b7dern", "Freund", "(", "den", "Ruhm", "soll", "ihm", "ent\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKNEG", "$,", "KON", "NN", "$(", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein andrer) stolz darauf, und w\u00fcrdig, es zu hei\u00dfen.", "tokens": ["Kein", "an\u00b7drer", ")", "stolz", "da\u00b7rauf", ",", "und", "w\u00fcr\u00b7dig", ",", "es", "zu", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$(", "ADJD", "PAV", "$,", "KON", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "War etwa F\u00fcrstenprunk und Eitelkeit der Hebel?", "tokens": ["War", "et\u00b7wa", "F\u00fcrs\u00b7ten\u00b7prunk", "und", "Ei\u00b7tel\u00b7keit", "der", "He\u00b7bel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dagegen zeugen laut die Briefe gnug an Knebel.", "tokens": ["Da\u00b7ge\u00b7gen", "zeu\u00b7gen", "laut", "die", "Brie\u00b7fe", "gnug", "an", "Kne\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Seit ich die las, steht hier im Heiligthum der Brust", "tokens": ["Seit", "ich", "die", "las", ",", "steht", "hier", "im", "Hei\u00b7lig\u00b7thum", "der", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "$,", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Bild der Andacht mir, von Weimar Karl August.", "tokens": ["Ein", "Bild", "der", "An\u00b7dacht", "mir", ",", "von", "Wei\u00b7mar", "Karl", "Au\u00b7gust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "$,", "APPR", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}