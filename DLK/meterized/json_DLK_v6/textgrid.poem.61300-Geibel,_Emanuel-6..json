{"textgrid.poem.61300": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe viel versucht und hab' erfahren", "tokens": ["Ich", "ha\u00b7be", "viel", "ver\u00b7sucht", "und", "hab'", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "KON", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein reich Geschick auf meinen Wanderz\u00fcgen;", "tokens": ["Ein", "reich", "Ge\u00b7schick", "auf", "mei\u00b7nen", "Wan\u00b7der\u00b7z\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich sah den Bauer seine Scholle pfl\u00fcgen,", "tokens": ["Ich", "sah", "den", "Bau\u00b7er", "sei\u00b7ne", "Schol\u00b7le", "pfl\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sah den reichen St\u00e4dter sich gebaren.", "tokens": ["Und", "sah", "den", "rei\u00b7chen", "St\u00e4d\u00b7ter", "sich", "ge\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Weisen sah ich und der K\u00fcnstler Scharen", "tokens": ["Die", "Wei\u00b7sen", "sah", "ich", "und", "der", "K\u00fcnst\u00b7ler", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich ewig m\u00fchn und doch sich nie gen\u00fcgen;", "tokens": ["Sich", "e\u00b7wig", "m\u00fchn", "und", "doch", "sich", "nie", "ge\u00b7n\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "KON", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah die H\u00f6fe sich am Prunk vergn\u00fcgen;", "tokens": ["Ich", "sah", "die", "H\u00f6\u00b7fe", "sich", "am", "Prunk", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch konnt' ich wenig Gl\u00fcckliche gewaren.", "tokens": ["Doch", "konnt'", "ich", "we\u00b7nig", "Gl\u00fcck\u00b7li\u00b7che", "ge\u00b7wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir selbst hat jene Glut die Brust beweget,", "tokens": ["Mir", "selbst", "hat", "je\u00b7ne", "Glut", "die", "Brust", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Liebe hei\u00dft, allein ich mu\u00dft' erproben,", "tokens": ["Die", "Lie\u00b7be", "hei\u00dft", ",", "al\u00b7lein", "ich", "mu\u00dft'", "er\u00b7pro\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df soviel Bittres sie wie S\u00fc\u00dfes hegt:", "tokens": ["Da\u00df", "so\u00b7viel", "Bitt\u00b7res", "sie", "wie", "S\u00fc\u00b7\u00dfes", "hegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drei Dinge nur vermog ich ganz zu loben,", "tokens": ["Drei", "Din\u00b7ge", "nur", "ver\u00b7mog", "ich", "ganz", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die stets zu echtem Heiligen Grund geleget:", "tokens": ["Die", "stets", "zu", "ech\u00b7tem", "Hei\u00b7li\u00b7gen", "Grund", "ge\u00b7le\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gesundheit, Mut und h\u00f6hern Blick nach oben.", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Mut", "und", "h\u00f6\u00b7hern", "Blick", "nach", "o\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}