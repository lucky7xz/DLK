{"dta.poem.7055": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie mu\u00df der Seelen-Lust so unausdr\u00fccklich,", "tokens": ["Wie", "mu\u00df", "der", "See\u00b7len\u00b7Lust", "so", "un\u00b7aus\u00b7dr\u00fcck\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfindlich, \u00fcberschwenglich, rein,", "tokens": ["Emp\u00b7find\u00b7lich", ",", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", ",", "rein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey jedes Vorwurfs Anblick seyn!", "tokens": ["Bey", "je\u00b7des", "Vor\u00b7wurfs", "An\u00b7blick", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den GOTT, (als welchem das, wodurch die Seele gl\u00fccklich", "tokens": ["Den", "GoTT", ",", "(", "als", "wel\u00b7chem", "das", ",", "wo\u00b7durch", "die", "See\u00b7le", "gl\u00fcck\u00b7lich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "$(", "KOUS", "PWAT", "PDS", "$,", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und seelig werden kann, nicht unbekannt,) bestimmt,", "tokens": ["Und", "see\u00b7lig", "wer\u00b7den", "kann", ",", "nicht", "un\u00b7be\u00b7kannt", ",", ")", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "VMFIN", "$,", "PTKNEG", "ADJD", "$,", "$(", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie dadurch begl\u00fccket,", "tokens": ["Da\u00df", "sie", "da\u00b7durch", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aufs innigste ger\u00fchrt, entz\u00fccket,", "tokens": ["Aufs", "in\u00b7nigs\u00b7te", "ge\u00b7r\u00fchrt", ",", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "VVPP", "$,", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und seelig werden soll. Es gl\u00e4ntzt des Monden Licht", "tokens": ["Und", "see\u00b7lig", "wer\u00b7den", "soll", ".", "Es", "gl\u00e4ntzt", "des", "Mon\u00b7den", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAINF", "VMFIN", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bey solchen Strahlen nicht.", "tokens": ["Bey", "sol\u00b7chen", "Strah\u00b7len", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der allerhellsten Sternen Schein,", "tokens": ["Der", "al\u00b7ler\u00b7hells\u00b7ten", "Ster\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sind bey dem Licht gantz dunckel und nicht rein.", "tokens": ["Sind", "bey", "dem", "Licht", "gantz", "dun\u00b7ckel", "und", "nicht", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Sonnen Glantz, der hellen C\u00f6rper Funckeln,", "tokens": ["Der", "Son\u00b7nen", "Glantz", ",", "der", "hel\u00b7len", "C\u00f6r\u00b7per", "Fun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die wir in solcher Pracht, im Dunckeln,", "tokens": ["Die", "wir", "in", "sol\u00b7cher", "Pracht", ",", "im", "Dun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Am Firmament so wundersch\u00f6n,", "tokens": ["Am", "Fir\u00b7ma\u00b7ment", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit fast entz\u00fcckten Augen sehn;", "tokens": ["Mit", "fast", "ent\u00b7z\u00fcck\u00b7ten", "Au\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sind schwache Strahlen nur, sind Schatten, Dunckelheiten,", "tokens": ["Sind", "schwa\u00b7che", "Strah\u00b7len", "nur", ",", "sind", "Schat\u00b7ten", ",", "Dun\u00b7ckel\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "$,", "VAFIN", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vergleicht man sie mit jenen Herrlichkeiten,", "tokens": ["Ver\u00b7gleicht", "man", "sie", "mit", "je\u00b7nen", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die um des Sch\u00f6pffers Sitz in reiner Klarheit schweben,", "tokens": ["Die", "um", "des", "Sch\u00f6pf\u00b7fers", "Sitz", "in", "rei\u00b7ner", "Klar\u00b7heit", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und GOTTES heil\u2019gen Thron umgeben.", "tokens": ["Und", "GoT\u00b7TES", "heil'\u00b7gen", "Thron", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}