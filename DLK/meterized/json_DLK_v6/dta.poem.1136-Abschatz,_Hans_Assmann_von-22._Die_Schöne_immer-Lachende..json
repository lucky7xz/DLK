{"dta.poem.1136": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "22. Die Sch\u00f6ne immer-Lachende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die s\u00fcsse Fr\u00f6ligkeit verzuckert ieden Bissen/ ", "tokens": ["Die", "s\u00fcs\u00b7se", "Fr\u00f6\u00b7lig\u00b7keit", "ver\u00b7zu\u00b7ckert", "ie\u00b7den", "Bis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ernste Sauersehn versaltzet Spei\u00df und Wein.", "tokens": ["Das", "erns\u00b7te", "Sau\u00b7er\u00b7sehn", "ver\u00b7salt\u00b7zet", "Spei\u00df", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Lachen scheidet uns von Thieren/ Holtz und Stein/", "tokens": ["Das", "La\u00b7chen", "schei\u00b7det", "uns", "von", "Thie\u00b7ren", "/", "Holtz", "und", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum kanstu nicht so viel auff Lachen seyn beflissen.", "tokens": ["Drum", "kans\u00b7tu", "nicht", "so", "viel", "auff", "La\u00b7chen", "seyn", "be\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PTKNEG", "ADV", "ADV", "APPR", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer wolte stets die Perl in ihre Muschel schl\u00fcssen?", "tokens": ["Wer", "wol\u00b7te", "stets", "die", "Perl", "in", "ih\u00b7re", "Mu\u00b7schel", "schl\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Elefante weist sein gl\u00e4ntzend Helffenbein/", "tokens": ["Der", "E\u00b7lef\u00b7an\u00b7te", "weist", "sein", "gl\u00e4nt\u00b7zend", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das freye Licht entdeckt des Silbers hellen Schein/", "tokens": ["Das", "frey\u00b7e", "Licht", "ent\u00b7deckt", "des", "Sil\u00b7bers", "hel\u00b7len", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vergrabne Sch\u00e4tze kan der N\u00e4chste nicht gen\u00fcssen.", "tokens": ["Ver\u00b7grab\u00b7ne", "Sch\u00e4t\u00b7ze", "kan", "der", "N\u00e4chs\u00b7te", "nicht", "ge\u00b7n\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach da\u00df ich k\u00f6nte recht den sch\u00f6nen Umkrei\u00df mahlen/", "tokens": ["Ach", "da\u00df", "ich", "k\u00f6n\u00b7te", "recht", "den", "sch\u00f6\u00b7nen", "Um\u00b7krei\u00df", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Damit der Liebreitz selbst den offnen Mund l\u00e4st prahlen!", "tokens": ["Da\u00b7mit", "der", "Lieb\u00b7reitz", "selbst", "den", "off\u00b7nen", "Mund", "l\u00e4st", "prah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer aber kan die Glutt recht lebhafft stellen vor?", "tokens": ["Wer", "a\u00b7ber", "kan", "die", "Glutt", "recht", "leb\u00b7hafft", "stel\u00b7len", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun \u00f6ffne/ wie du wilt/ der Lippen sch\u00f6nes Thor:", "tokens": ["Nun", "\u00f6ff\u00b7ne", "/", "wie", "du", "wilt", "/", "der", "Lip\u00b7pen", "sch\u00f6\u00b7nes", "Thor", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWAV", "PPER", "VMFIN", "$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein lachend Paar bespannt den Wagen der Zitheren/", "tokens": ["Ein", "la\u00b7chend", "Paar", "be\u00b7spannt", "den", "Wa\u00b7gen", "der", "Zi\u00b7the\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich werde dich allzeit/ wie solche T\u00e4ubchen/ ehren.", "tokens": ["Ich", "wer\u00b7de", "dich", "all\u00b7zeit", "/", "wie", "sol\u00b7che", "T\u00e4ub\u00b7chen", "/", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$(", "KOKOM", "PIAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}