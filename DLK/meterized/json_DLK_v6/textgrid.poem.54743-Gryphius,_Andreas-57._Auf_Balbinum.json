{"textgrid.poem.54743": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "57. Auf Balbinum", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie seltzam ists! ", "tokens": ["Wie", "selt\u00b7zam", "ists", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und sein Weib hat stets frembde M\u00e4nner lib.", "tokens": ["Und", "sein", "Weib", "hat", "stets", "fremb\u00b7de", "M\u00e4n\u00b7ner", "lib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "NN", "NE", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er nimmt von allen was er immer kan:", "tokens": ["Er", "nimmt", "von", "al\u00b7len", "was", "er", "im\u00b7mer", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "PWS", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dise beut sich allen selber an!", "tokens": ["Und", "di\u00b7se", "beut", "sich", "al\u00b7len", "sel\u00b7ber", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PRF", "PIAT", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was d\u00fcnckt euch wohl/ da\u00df hiraus sey zu schlissen?", "tokens": ["Was", "d\u00fcnckt", "euch", "wohl", "/", "da\u00df", "hi\u00b7raus", "sey", "zu", "schlis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$(", "KOUS", "ADV", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie will sein nehmen durch ihr geben b\u00fcssen.", "tokens": ["Sie", "will", "sein", "neh\u00b7men", "durch", "ihr", "ge\u00b7ben", "b\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "VVFIN", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}