{"textgrid.poem.40186": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sehnsucht hat mich fr\u00fch geweckt; \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehnsucht hat mich fr\u00fch geweckt; \u2013", "tokens": ["Sehn\u00b7sucht", "hat", "mich", "fr\u00fch", "ge\u00b7weckt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wo die alten Eichen rauschen,", "tokens": ["wo", "die", "al\u00b7ten", "Ei\u00b7chen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "hier am Waldsaum hingestreckt,", "tokens": ["hier", "am", "Wald\u00b7saum", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "will ich dich, Natur, belauschen.", "tokens": ["will", "ich", "dich", ",", "Na\u00b7tur", ",", "be\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "$,", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Baum und Strauch gar scheint erwacht;", "tokens": ["Baum", "und", "Strauch", "gar", "scheint", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "voller pulset alles Leben,", "tokens": ["vol\u00b7ler", "pul\u00b7set", "al\u00b7les", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn im k\u00fchlen Tau der Nacht", "tokens": ["wenn", "im", "k\u00fch\u00b7len", "Tau", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "warm die ersten Strahlen beben.", "tokens": ["warm", "die", "ers\u00b7ten", "Strah\u00b7len", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was denn nur ", "tokens": ["Was", "denn", "nur"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "soviel Gro\u00dfes! soviel Kleines!", "tokens": ["so\u00b7viel", "Gro\u00b7\u00dfes", "!", "so\u00b7viel", "Klei\u00b7nes", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wie es sich zusammenpa\u00dft", "tokens": ["wie", "es", "sich", "zu\u00b7sam\u00b7men\u00b7pa\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "in ein \u00fcberm\u00e4chtig Eines! \u2013", "tokens": ["in", "ein", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "Ei\u00b7nes", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJD", "PIS", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie der Wind im Hafer schwirrt,", "tokens": ["Wie", "der", "Wind", "im", "Ha\u00b7fer", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "rings im Gras die Grillen rispeln!", "tokens": ["rings", "im", "Gras", "die", "Gril\u00b7len", "ris\u00b7peln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "wie im Busch der Vogel girrt,", "tokens": ["wie", "im", "Busch", "der", "Vo\u00b7gel", "girrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "um sein Nest die Bl\u00e4tter lispeln!", "tokens": ["um", "sein", "Nest", "die", "Bl\u00e4t\u00b7ter", "lis\u00b7peln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "wie die Bienen taumelnd sammeln,", "tokens": ["wie", "die", "Bie\u00b7nen", "tau\u00b7melnd", "sam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "still durchs Moos die K\u00e4fer schl\u00fcpfen \u2013 \u2013", "tokens": ["still", "durchs", "Moos", "die", "K\u00e4\u00b7fer", "schl\u00fcp\u00b7fen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oh Natur! was soll mein Stammeln,", "tokens": ["Oh", "Na\u00b7tur", "!", "was", "soll", "mein", "Stam\u00b7meln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "NN", "$.", "PWS", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seh' ich alles Dich verkn\u00fcpfen:", "tokens": ["seh'", "ich", "al\u00b7les", "Dich", "ver\u00b7kn\u00fcp\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "wie es sich ins Herz mir gie\u00dft,", "tokens": ["wie", "es", "sich", "ins", "Herz", "mir", "gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "all das Gro\u00dfe, all das Kleine, \u2013", "tokens": ["all", "das", "Gro\u00b7\u00dfe", ",", "all", "das", "Klei\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ART", "ADJA", "$,", "PIAT", "ART", "ADJA", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wie's mit mir zusammenflie\u00dft", "tokens": ["wie's", "mit", "mir", "zu\u00b7sam\u00b7men\u00b7flie\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "in das \u00fcberm\u00e4chtig Eine!", "tokens": ["in", "das", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "Ei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}