{"dta.poem.12738": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als ihm der closter-stand J. D.  \n S. P. vorgeworffen wurde.  \n L. R. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du narr! du tadelst zwar den heilgen closter-stand,", "tokens": ["Du", "narr", "!", "du", "ta\u00b7delst", "zwar", "den", "heil\u00b7gen", "clos\u00b7ter\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es m\u00fcst ein armer m\u00fcnch so fromm und einsam leben;", "tokens": ["Es", "m\u00fcst", "ein", "ar\u00b7mer", "m\u00fcnch", "so", "fromm", "und", "ein\u00b7sam", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein, la\u00df dir so viel zu deiner nachricht geben:", "tokens": ["Al\u00b7lein", ",", "la\u00df", "dir", "so", "viel", "zu", "dei\u00b7ner", "nach\u00b7richt", "ge\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich habe manche nacht aufs uonnen-fleisch gewandt,", "tokens": ["Ich", "ha\u00b7be", "man\u00b7che", "nacht", "aufs", "u\u00b7on\u00b7nen\u00b7fleisch", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Denn was der pater thut, kan auch der m\u00fcnch versuchen;", "tokens": ["Denn", "was", "der", "pa\u00b7ter", "thut", ",", "kan", "auch", "der", "m\u00fcnch", "ver\u00b7su\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer will ein junges blut zur einsamkeit verfluchen?", "tokens": ["Wer", "will", "ein", "jun\u00b7ges", "blut", "zur", "ein\u00b7sam\u00b7keit", "ver\u00b7flu\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}