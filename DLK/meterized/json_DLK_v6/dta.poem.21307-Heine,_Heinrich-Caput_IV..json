{"dta.poem.21307": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu C\u00f6llen kam ich sp\u00e4t Abends an,", "tokens": ["Zu", "C\u00f6l\u00b7len", "kam", "ich", "sp\u00e4t", "A\u00b7bends", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da h\u00f6rte ich rauschen den Rheinflu\u00df,", "tokens": ["Da", "h\u00f6r\u00b7te", "ich", "rau\u00b7schen", "den", "Rhein\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da f\u00e4chelte mich schon deutsche Luft,", "tokens": ["Da", "f\u00e4\u00b7chel\u00b7te", "mich", "schon", "deut\u00b7sche", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da f\u00fchlt\u2019 ich ihren Einflu\u00df \u2013", "tokens": ["Da", "f\u00fchlt'", "ich", "ih\u00b7ren", "Ein\u00b7flu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}}}}}