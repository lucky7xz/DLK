{"textgrid.poem.39198": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Zeit ist tatendurstig, tatenschwanger,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit ist tatendurstig, tatenschwanger,", "tokens": ["Die", "Zeit", "ist", "ta\u00b7ten\u00b7durs\u00b7tig", ",", "ta\u00b7ten\u00b7schwan\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freiheitsm\u00fctze prahlt auf k\u00fchnen Stirnen,", "tokens": ["Die", "Frei\u00b7heits\u00b7m\u00fct\u00b7ze", "prahlt", "auf", "k\u00fch\u00b7nen", "Stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Diadem, der Purpur h\u00e4ngt am Pranger.", "tokens": ["Das", "Di\u00b7a\u00b7dem", ",", "der", "Pur\u00b7pur", "h\u00e4ngt", "am", "Pran\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durchs Reich der Dichtung geht ein tobend Z\u00fcrnen,", "tokens": ["Durchs", "Reich", "der", "Dich\u00b7tung", "geht", "ein", "to\u00b7bend", "Z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Aufruhr flutet um die h\u00f6chsten Spitzen,", "tokens": ["Der", "Auf\u00b7ruhr", "flu\u00b7tet", "um", "die", "h\u00f6chs\u00b7ten", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rotgl\u00fchend aus vulkanischen Gehirnen.", "tokens": ["Rot\u00b7gl\u00fc\u00b7hend", "aus", "vul\u00b7ka\u00b7ni\u00b7schen", "Ge\u00b7hir\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Aus tausend Federn l\u00e4\u00dft er Flammen spritzen,", "tokens": ["Aus", "tau\u00b7send", "Fe\u00b7dern", "l\u00e4\u00dft", "er", "Flam\u00b7men", "sprit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus tausend Zungen ruft er zu den Waffen,", "tokens": ["Aus", "tau\u00b7send", "Zun\u00b7gen", "ruft", "er", "zu", "den", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus tausend M\u00e4nteln l\u00e4\u00dft er Dolche blitzen.", "tokens": ["Aus", "tau\u00b7send", "M\u00e4n\u00b7teln", "l\u00e4\u00dft", "er", "Dol\u00b7che", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Dichtkunst ward zur Fechtkunst umgeschaffen,", "tokens": ["Die", "Dicht\u00b7kunst", "ward", "zur", "Fecht\u00b7kunst", "um\u00b7ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie mu\u00df dem Arme der Vernichtung dienen,", "tokens": ["Sie", "mu\u00df", "dem", "Ar\u00b7me", "der", "Ver\u00b7nich\u00b7tung", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mu\u00df Speere sch\u00fctteln oder Bogen straffen.", "tokens": ["Mu\u00df", "Spee\u00b7re", "sch\u00fct\u00b7teln", "o\u00b7der", "Bo\u00b7gen", "straf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVINF", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie hau'n mit ihr nach Thron und Hermelinen,", "tokens": ["Sie", "hau'n", "mit", "ihr", "nach", "Thron", "und", "Her\u00b7me\u00b7li\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie werfen sie als Pechkranz auf die Zinnen,", "tokens": ["Sie", "wer\u00b7fen", "sie", "als", "Pech\u00b7kranz", "auf", "die", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie dienen nicht der Kunst, die Kunst dient ", "tokens": ["Sie", "die\u00b7nen", "nicht", "der", "Kunst", ",", "die", "Kunst", "dient"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Wann wird der zornige Strom das Meer gewinnen?", "tokens": ["Wann", "wird", "der", "zor\u00b7ni\u00b7ge", "Strom", "das", "Meer", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wann l\u00f6scht die Glut, wann gr\u00fcnt es in den Talen?", "tokens": ["Wann", "l\u00f6scht", "die", "Glut", ",", "wann", "gr\u00fcnt", "es", "in", "den", "Ta\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann wird man wieder s\u00fc\u00dfe Lieder sinnen?", "tokens": ["Wann", "wird", "man", "wie\u00b7der", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der", "sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Es tr\u00e4gt die Kunst ihr eisern Los mit Qualen.", "tokens": ["Es", "tr\u00e4gt", "die", "Kunst", "ihr", "ei\u00b7sern", "Los", "mit", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df, Herr, die G\u00f6ttliche in ihrer Hoheit", "tokens": ["La\u00df", ",", "Herr", ",", "die", "G\u00f6tt\u00b7li\u00b7che", "in", "ih\u00b7rer", "Ho\u00b7heit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht untergehn, ein Opfer der Vandalen,", "tokens": ["Nicht", "un\u00b7ter\u00b7gehn", ",", "ein", "Op\u00b7fer", "der", "Van\u00b7da\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dieses Meinungsstreits ergrimmter Roheit!", "tokens": ["In", "die\u00b7ses", "Mei\u00b7nungs\u00b7streits", "er\u00b7grimm\u00b7ter", "Ro\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}