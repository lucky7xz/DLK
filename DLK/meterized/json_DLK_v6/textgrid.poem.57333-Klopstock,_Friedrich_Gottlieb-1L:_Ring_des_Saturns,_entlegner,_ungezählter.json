{"textgrid.poem.57333": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ring des Saturns, entlegner, ungez\u00e4hlter", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ring des Saturns, entlegner, ungez\u00e4hlter", "tokens": ["Ring", "des", "Sa\u00b7turns", ",", "ent\u00b7leg\u00b7ner", ",", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "$,", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Satelliten Gedr\u00e4ng, die um den grossen", "tokens": ["Sa\u00b7tel\u00b7li\u00b7ten", "Ge\u00b7dr\u00e4ng", ",", "die", "um", "den", "gros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Stern sich drehn, erleuchtet, und leuchtend, droben", "tokens": ["Stern", "sich", "drehn", ",", "er\u00b7leuch\u00b7tet", ",", "und", "leuch\u00b7tend", ",", "dro\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PRF", "VVINF", "$,", "VVPP", "$,", "KON", "VVPP", "$,", "ADV"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Wandeln im Himmel!", "tokens": ["Wan\u00b7deln", "im", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Inselchen, ihr der sch\u00f6nsten, die im weiten", "tokens": ["In\u00b7sel\u00b7chen", ",", "ihr", "der", "sch\u00f6ns\u00b7ten", ",", "die", "im", "wei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "ART", "ADJA", "$,", "PRELS", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Meere schwimmen umher der Sch\u00f6pfung Gottes,", "tokens": ["Mee\u00b7re", "schwim\u00b7men", "um\u00b7her", "der", "Sch\u00f6p\u00b7fung", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sch\u00f6ner, mehr f\u00fcr Gl\u00fcckliche, denn vor Alters", "tokens": ["Sch\u00f6\u00b7ner", ",", "mehr", "f\u00fcr", "Gl\u00fcck\u00b7li\u00b7che", ",", "denn", "vor", "Al\u00b7ters"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "$,", "KON", "APPR", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Die in der Fabel!", "tokens": ["Die", "in", "der", "Fa\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Eurer Bewohner Loos ward froh're Wonne,", "tokens": ["Eu\u00b7rer", "Be\u00b7woh\u00b7ner", "Loos", "ward", "froh'\u00b7re", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Als wir kennen: zwar rinnt in ihren Kelch auch", "tokens": ["Als", "wir", "ken\u00b7nen", ":", "zwar", "rinnt", "in", "ih\u00b7ren", "Kelch", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bittres, wie in unsern; doch leicht zerfl\u00f6ssbar", "tokens": ["Bitt\u00b7res", ",", "wie", "in", "un\u00b7sern", ";", "doch", "leicht", "zer\u00b7fl\u00f6ss\u00b7bar"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "PPOSAT", "$.", "ADV", "ADJD", "ADJD"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Rinnt's, und bey Tropfen.", "tokens": ["Rinnt's", ",", "und", "bey", "Trop\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Leiseres Ohrs, das Auge lichter, sehn sie", "tokens": ["Lei\u00b7se\u00b7res", "Ohrs", ",", "das", "Au\u00b7ge", "lich\u00b7ter", ",", "sehn", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADJD", "$,", "VVFIN", "PPER"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Strom und Hain in den nahen Sternen, h\u00f6ren", "tokens": ["Strom", "und", "Hain", "in", "den", "na\u00b7hen", "Ster\u00b7nen", ",", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Einen laut sich schwingen, die Wiederhalle", "tokens": ["Ei\u00b7nen", "laut", "sich", "schwin\u00b7gen", ",", "die", "Wie\u00b7der\u00b7hal\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "PRF", "VVINF", "$,", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "T\u00f6nen im andern.", "tokens": ["T\u00f6\u00b7nen", "im", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Lieblicher singt Saturn Gesang der Sph\u00e4ren", "tokens": ["Lieb\u00b7li\u00b7cher", "singt", "Sa\u00b7turn", "Ge\u00b7sang", "der", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NE", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit den Monden um ihn, als manche Sonne", "tokens": ["Mit", "den", "Mon\u00b7den", "um", "ihn", ",", "als", "man\u00b7che", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "$,", "KOUS", "PIAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "In den hohen Strassen des Lichts mit ihren", "tokens": ["In", "den", "ho\u00b7hen", "Stras\u00b7sen", "des", "Lichts", "mit", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Welten ihn singet,", "tokens": ["Wel\u00b7ten", "ihn", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "S\u00e4umend, und s\u00e4umend schwebt, auf Hinimelreisen", "tokens": ["S\u00e4u\u00b7mend", ",", "und", "s\u00e4u\u00b7mend", "schwebt", ",", "auf", "Hi\u00b7ni\u00b7mel\u00b7rei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KON", "ADJD", "VVFIN", "$,", "APPR", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Um den goldenen Ring der Engel Gottes:", "tokens": ["Um", "den", "gol\u00b7de\u00b7nen", "Ring", "der", "En\u00b7gel", "Got\u00b7tes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Selbst die kentnissdurstende Seele z\u00f6gert", "tokens": ["Selbst", "die", "kent\u00b7niss\u00b7durs\u00b7ten\u00b7de", "See\u00b7le", "z\u00f6\u00b7gert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Dort in den Lauben.", "tokens": ["Dort", "in", "den", "Lau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Wartest du, Meta, dort auf mich? dort wart' ich", "tokens": ["War\u00b7test", "du", ",", "Me\u00b7ta", ",", "dort", "auf", "mich", "?", "dort", "wart'", "ich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "NE", "$,", "ADV", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Unsres Lieblings mit dir. Doch ach der Scheidung", "tokens": ["Uns\u00b7res", "Lieb\u00b7lings", "mit", "dir", ".", "Doch", "ach", "der", "Schei\u00b7dung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$.", "KON", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Herber Kelch! Einst rann's nicht bey Tropfen! wird bey", "tokens": ["Her\u00b7ber", "Kelch", "!", "Einst", "rann's", "nicht", "bey", "Trop\u00b7fen", "!", "wird", "bey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$.", "ADV", "VVFIN", "PTKNEG", "APPR", "NN", "$.", "VAFIN", "APPR"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Tropfen nicht rinnen!", "tokens": ["Trop\u00b7fen", "nicht", "rin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Wenn ein Bewohner dort vom Nachbarsterne", "tokens": ["Wenn", "ein", "Be\u00b7woh\u00b7ner", "dort", "vom", "Nach\u00b7bars\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lang die Fr\u00fchlinge sah her\u00fcberschimmern;", "tokens": ["Lang", "die", "Fr\u00fch\u00b7lin\u00b7ge", "sah", "her\u00b7\u00fc\u00b7bers\u00b7chim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fliesst den Freunden erst, nach den frohen Z\u00e4hren,", "tokens": ["Fliesst", "den", "Freun\u00b7den", "erst", ",", "nach", "den", "fro\u00b7hen", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Eine der Wehmuth.", "tokens": ["Ei\u00b7ne", "der", "Weh\u00b7muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Jener, der unverbl\u00fcht vielleicht dem hellsten", "tokens": ["Je\u00b7ner", ",", "der", "un\u00b7ver\u00b7bl\u00fcht", "viel\u00b7leicht", "dem", "hells\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "$,", "PRELS", "ADJD", "ADV", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mond' itzt weilte, vielleicht zum Liede tanzte,", "tokens": ["Mond'", "itzt", "weil\u00b7te", ",", "viel\u00b7leicht", "zum", "Lie\u00b7de", "tanz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wird dann schnell verwandelt, betritt in Sonnen", "tokens": ["Wird", "dann", "schnell", "ver\u00b7wan\u00b7delt", ",", "be\u00b7tritt", "in", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$,", "VVFIN", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00f6lbende Tempel.", "tokens": ["W\u00f6l\u00b7ben\u00b7de", "Tem\u00b7pel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}