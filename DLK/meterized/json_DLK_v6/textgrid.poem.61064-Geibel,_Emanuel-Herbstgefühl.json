{"textgrid.poem.61064": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O w\u00e4r' es blo\u00df der Wange Pracht,", "tokens": ["O", "w\u00e4r'", "es", "blo\u00df", "der", "Wan\u00b7ge", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit den Jahren flieht!", "tokens": ["Die", "mit", "den", "Jah\u00b7ren", "flieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch das ist's, was mich traurig macht,", "tokens": ["Doch", "das", "ist's", ",", "was", "mich", "trau\u00b7rig", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df auch das Herz verbl\u00fcht;", "tokens": ["Da\u00df", "auch", "das", "Herz", "ver\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df, wie der Jugend Ruf verhallt,", "tokens": ["Da\u00df", ",", "wie", "der", "Ju\u00b7gend", "Ruf", "ver\u00b7hallt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie der Blick sich tr\u00fcbt,", "tokens": ["Und", "wie", "der", "Blick", "sich", "tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Brust, die einst so hei\u00df gewallt,", "tokens": ["Die", "Brust", ",", "die", "einst", "so", "hei\u00df", "ge\u00b7wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00dft, wie sie geliebt.", "tokens": ["Ver\u00b7gi\u00dft", ",", "wie", "sie", "ge\u00b7liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ob von der Lippe dann auch k\u00fchn", "tokens": ["Ob", "von", "der", "Lip\u00b7pe", "dann", "auch", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Witz und Scherz ergie\u00dft,", "tokens": ["Sich", "Witz", "und", "Scherz", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "'s ist nur ein heuchlerisches Gr\u00fcn,", "tokens": ["'s", "ist", "nur", "ein", "heuch\u00b7le\u00b7ri\u00b7sches", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das \u00fcber Gr\u00e4bern sprie\u00dft.", "tokens": ["Das", "\u00fc\u00b7ber", "Gr\u00e4\u00b7bern", "sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Nacht kommt, mit der Nacht der Schmerz", "tokens": ["Die", "Nacht", "kommt", ",", "mit", "der", "Nacht", "der", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eitle Flimmer bricht;", "tokens": ["Der", "eit\u00b7le", "Flim\u00b7mer", "bricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach Tr\u00e4nen sehnt sich unser Herz", "tokens": ["Nach", "Tr\u00e4\u00b7nen", "sehnt", "sich", "un\u00b7ser", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und findet Tr\u00e4nen nicht.", "tokens": ["Und", "fin\u00b7det", "Tr\u00e4\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir sind so arm, wir sind so m\u00fcd,", "tokens": ["Wir", "sind", "so", "arm", ",", "wir", "sind", "so", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum, wir wissen's kaum;", "tokens": ["Wa\u00b7rum", ",", "wir", "wis\u00b7sen's", "kaum", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir f\u00fchlen nur, das Herz verbl\u00fcht,", "tokens": ["Wir", "f\u00fch\u00b7len", "nur", ",", "das", "Herz", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles Gl\u00fcck ist Traum.", "tokens": ["Und", "al\u00b7les", "Gl\u00fcck", "ist", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}