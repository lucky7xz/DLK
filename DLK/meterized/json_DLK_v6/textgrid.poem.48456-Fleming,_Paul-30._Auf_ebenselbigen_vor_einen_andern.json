{"textgrid.poem.48456": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "30. Auf ebenselbigen vor einen andern", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, ich soll billig euch vor Andern heute binden", "tokens": ["Herr", ",", "ich", "soll", "bil\u00b7lig", "euch", "vor", "An\u00b7dern", "heu\u00b7te", "bin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADJD", "PPER", "APPR", "ADJA", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und einen Maienkranz in euer' Haare winden,", "tokens": ["und", "ei\u00b7nen", "Mai\u00b7en\u00b7kranz", "in", "eu\u00b7er'", "Haa\u00b7re", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der ihr vor langer Zeit durch eurer G\u00fcnste Band", "tokens": ["der", "ihr", "vor", "lan\u00b7ger", "Zeit", "durch", "eu\u00b7rer", "G\u00fcns\u00b7te", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "euch mich so hart verkn\u00fcpft, da\u00df diese schwache Hand", "tokens": ["euch", "mich", "so", "hart", "ver\u00b7kn\u00fcpft", ",", "da\u00df", "die\u00b7se", "schwa\u00b7che", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "ADV", "ADJD", "VVPP", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "sich niemals l\u00f6sen wird. Was aber werd' ich finden,", "tokens": ["sich", "nie\u00b7mals", "l\u00f6\u00b7sen", "wird", ".", "Was", "a\u00b7ber", "werd'", "ich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VAFIN", "$.", "PWS", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "darmit ich, Edler, euch wol billig solte binden?", "tokens": ["dar\u00b7mit", "ich", ",", "Ed\u00b7ler", ",", "euch", "wol", "bil\u00b7lig", "sol\u00b7te", "bin\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein Mittel ist nicht hier, als nur ein treuer Sinn,", "tokens": ["Kein", "Mit\u00b7tel", "ist", "nicht", "hier", ",", "als", "nur", "ein", "treu\u00b7er", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "mit dem ich l\u00e4ngst vorhin ganz euer eigen bin.", "tokens": ["mit", "dem", "ich", "l\u00e4ngst", "vor\u00b7hin", "ganz", "eu\u00b7er", "ei\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch werdet ihr f\u00fcr Tat den guten Willen nehmen?", "tokens": ["Doch", "wer\u00b7det", "ihr", "f\u00fcr", "Tat", "den", "gu\u00b7ten", "Wil\u00b7len", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Pflegt doch der Jupiter den Man nicht zu besch\u00e4men,", "tokens": ["Pflegt", "doch", "der", "Ju\u00b7pi\u00b7ter", "den", "Man", "nicht", "zu", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ART", "PIS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "++-++--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "der Milch f\u00fcr Weirauch gibt: so wei\u00df ich auch gewi\u00df,", "tokens": ["der", "Milch", "f\u00fcr", "Wei\u00b7rauch", "gibt", ":", "so", "wei\u00df", "ich", "auch", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "da\u00df ihr von mir mehr nicht erheischt als eben di\u00df.", "tokens": ["da\u00df", "ihr", "von", "mir", "mehr", "nicht", "er\u00b7heischt", "als", "e\u00b7ben", "di\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "PTKNEG", "VVFIN", "KOKOM", "ADV", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So lang' ich werde sein in eurer Diener Reihe,", "tokens": ["So", "lang'", "ich", "wer\u00b7de", "sein", "in", "eu\u00b7rer", "Die\u00b7ner", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VAFIN", "PPOSAT", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "so lange will ich sein bedacht auf neue Treue,", "tokens": ["so", "lan\u00b7ge", "will", "ich", "sein", "be\u00b7dacht", "auf", "neu\u00b7e", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPOSAT", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "und nicht bedacht nur sein, auch weisen in der Tat,", "tokens": ["und", "nicht", "be\u00b7dacht", "nur", "sein", ",", "auch", "wei\u00b7sen", "in", "der", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "ADV", "VAINF", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "da\u00df di\u00df mein Herze sei, wie sichs erkl\u00e4ret hat.", "tokens": ["da\u00df", "di\u00df", "mein", "Her\u00b7ze", "sei", ",", "wie", "sichs", "er\u00b7kl\u00e4\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "VVFIN", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}