{"dta.poem.39": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine verstorbne fromme  \n Jungfer.  \n \u00fcber  \n Offenbahr. Joh.  XIX.  7. 8. 9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So freu dich Himmels-Braut, in\ndeiner weissen Seide,", "tokens": ["So", "freu", "dich", "Him\u00b7mels\u00b7Braut", ",", "in", "dei\u00b7ner", "weis\u00b7sen", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit dein JEsus dich der", "tokens": ["Wo\u00b7mit", "dein", "Je\u00b7sus", "dich", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NE", "PRF", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geneus nach deiner Last, die", "tokens": ["Ge\u00b7neus", "nach", "dei\u00b7ner", "Last", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da dich dein Br\u00e4utigam durch", "tokens": ["Da", "dich", "dein", "Br\u00e4u\u00b7ti\u00b7gam", "durch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Du hast hier auf der Welt im Trauerkleid geweinet,", "tokens": ["Du", "hast", "hier", "auf", "der", "Welt", "im", "Trau\u00b7er\u00b7kleid", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und eine lange Zeit in K\u00fcmmernis gequ\u00e4lt:", "tokens": ["Und", "ei\u00b7ne", "lan\u00b7ge", "Zeit", "in", "K\u00fcm\u00b7mer\u00b7nis", "ge\u00b7qu\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Regen ist vorbei, da deine Sonne scheinet", "tokens": ["Der", "Re\u00b7gen", "ist", "vor\u00b7bei", ",", "da", "dei\u00b7ne", "Son\u00b7ne", "schei\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da dich der Heiland hat zum Eigenthum erw\u00e4hlt.", "tokens": ["Da", "dich", "der", "Hei\u00b7land", "hat", "zum", "Ei\u00b7gen\u00b7thum", "er\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Keuschheit Ehrenkranz, der Tugend reine Mir-", "tokens": ["Der", "Keuschheit", "Eh\u00b7ren\u00b7kranz", ",", "der", "Tu\u00b7gend", "rei\u00b7ne", "Mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "War deine sch\u00f6nste Tracht, worin du hier gegl\u00e4nzt:", "tokens": ["War", "dei\u00b7ne", "sch\u00f6ns\u00b7te", "Tracht", ",", "wo\u00b7rin", "du", "hier", "ge\u00b7gl\u00e4nzt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun wird dein Br\u00e4utigam, darum die Palmen", "tokens": ["Nun", "wird", "dein", "Br\u00e4u\u00b7ti\u00b7gam", ",", "da\u00b7rum", "die", "Pal\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NE", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Womit er die durch ihm gesieget, dort bekr\u00e4nzt.", "tokens": ["Wo\u00b7mit", "er", "die", "durch", "ihm", "ge\u00b7sie\u00b7get", ",", "dort", "be\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "APPR", "PPER", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du wohnest in der Schaar die jenem Lamme singen,", "tokens": ["Du", "woh\u00b7nest", "in", "der", "Schaar", "die", "je\u00b7nem", "Lam\u00b7me", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Womit du dich allhier in ewger Treu verm\u00e4hlt:", "tokens": ["Wo\u00b7mit", "du", "dich", "all\u00b7hier", "in", "ew\u00b7ger", "Treu", "ver\u00b7m\u00e4hlt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wol dem der Fleisch und Blut, wie du, wird recht", "tokens": ["Wol", "dem", "der", "Fleisch", "und", "Blut", ",", "wie", "du", ",", "wird", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ART", "NN", "KON", "NN", "$,", "PWAV", "PPER", "$,", "VAFIN", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Der wird von Christo auch zur Himmels-Braut er-", "tokens": ["Der", "wird", "von", "Chris\u00b7to", "auch", "zur", "Him\u00b7mels\u00b7Braut", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NE", "ADV", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}