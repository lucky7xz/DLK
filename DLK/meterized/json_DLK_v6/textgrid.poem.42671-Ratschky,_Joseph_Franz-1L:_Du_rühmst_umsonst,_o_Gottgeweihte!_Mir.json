{"textgrid.poem.42671": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du r\u00fchmst umsonst, o Gottgeweihte! Mir", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du r\u00fchmst umsonst, o Gottgeweihte! Mir", "tokens": ["Du", "r\u00fchmst", "um\u00b7sonst", ",", "o", "Gott\u00b7ge\u00b7weih\u00b7te", "!", "Mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "FM", "NN", "$.", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Unschuld Reitz, und tadelst meine Wege.", "tokens": ["Der", "Un\u00b7schuld", "Reitz", ",", "und", "ta\u00b7delst", "mei\u00b7ne", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Mund verdammt die leiseste Begier,", "tokens": ["Dein", "Mund", "ver\u00b7dammt", "die", "lei\u00b7ses\u00b7te", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ach! dein Blick macht ihrer tausend rege.", "tokens": ["Und", "ach", "!", "dein", "Blick", "macht", "ih\u00b7rer", "tau\u00b7send", "re\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "NN", "VVFIN", "PPOSAT", "CARD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du heissest mich den Keim der Sinnlichkeit", "tokens": ["Du", "heis\u00b7sest", "mich", "den", "Keim", "der", "Sinn\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Reu' und Leid aus meinem Herzen reuten:", "tokens": ["Durch", "Reu'", "und", "Leid", "aus", "mei\u00b7nem", "Her\u00b7zen", "reu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir aber thut im Grund der Seele leid,", "tokens": ["Mir", "a\u00b7ber", "thut", "im", "Grund", "der", "See\u00b7le", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dass nichts vermag, zur S\u00fcnde dich zu leiten.", "tokens": ["Dass", "nichts", "ver\u00b7mag", ",", "zur", "S\u00fcn\u00b7de", "dich", "zu", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du sprichst, es sey des Erdepilgers Pflicht,", "tokens": ["Du", "sprichst", ",", "es", "sey", "des", "Er\u00b7de\u00b7pil\u00b7gers", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dass er dem Drang der Sinne widerstehe.", "tokens": ["Dass", "er", "dem", "Drang", "der", "Sin\u00b7ne", "wi\u00b7der\u00b7ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das weiss ich wohl: allein ich glaub' es nicht,", "tokens": ["Das", "weiss", "ich", "wohl", ":", "al\u00b7lein", "ich", "glaub'", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So lang' ich dich mir gegen\u00fcber sehe.", "tokens": ["So", "lang'", "ich", "dich", "mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn mich dein Mund der Tugend Pflichten lehrt,", "tokens": ["Wenn", "mich", "dein", "Mund", "der", "Tu\u00b7gend", "Pflich\u00b7ten", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So w\u00fcnscht mein Herz, du glaubtest meinen Lehren.", "tokens": ["So", "w\u00fcnscht", "mein", "Herz", ",", "du", "glaub\u00b7test", "mei\u00b7nen", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4ngst h\u00e4tte mich dein Eifer schon bekehrt,", "tokens": ["L\u00e4ngst", "h\u00e4t\u00b7te", "mich", "dein", "Ei\u00b7fer", "schon", "be\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Glaubt' ich nicht st\u00e4ts, du w\u00fcrdest dich bekehren.", "tokens": ["Glaubt'", "ich", "nicht", "st\u00e4ts", ",", "du", "w\u00fcr\u00b7dest", "dich", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Lobpreisest du den Sch\u00f6pfer der Natur,", "tokens": ["Lob\u00b7prei\u00b7sest", "du", "den", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O so vergess' ich seiner Macht und St\u00e4rke", "tokens": ["O", "so", "ver\u00b7ge\u00b7ss'", "ich", "sei\u00b7ner", "Macht", "und", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Bey deinem Lob, und denke staunend nur", "tokens": ["Bey", "dei\u00b7nem", "Lob", ",", "und", "den\u00b7ke", "stau\u00b7nend", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An dich allein, du sch\u00f6nstes seiner Werke!", "tokens": ["An", "dich", "al\u00b7lein", ",", "du", "sch\u00f6ns\u00b7tes", "sei\u00b7ner", "Wer\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "PPER", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich w\u00fcnsche nie, so r\u00fchmlich auch die Bahn", "tokens": ["Ich", "w\u00fcn\u00b7sche", "nie", ",", "so", "r\u00fchm\u00b7lich", "auch", "die", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmel ist, als Heiliger zu schimmern;", "tokens": ["Zum", "Him\u00b7mel", "ist", ",", "als", "Hei\u00b7li\u00b7ger", "zu", "schim\u00b7mern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der, den du liebst, ist hier zu wohl daran,", "tokens": ["Der", ",", "den", "du", "liebst", ",", "ist", "hier", "zu", "wohl", "da\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um sich noch viel um jene Welt zu k\u00fcmmern.", "tokens": ["Um", "sich", "noch", "viel", "um", "je\u00b7ne", "Welt", "zu", "k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ADV", "ADV", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}