{"textgrid.poem.57445": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kilian! erw\u00fcnschtes Wort!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kilian! erw\u00fcnschtes Wort!", "tokens": ["Ki\u00b7li\u00b7an", "!", "er\u00b7w\u00fcnschtes", "Wort", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Heute bringst Du viel Vergn\u00fcgen,", "tokens": ["Heu\u00b7te", "bringst", "Du", "viel", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, wie sch\u00f6n sich Stell und Ort", "tokens": ["Seht", ",", "wie", "sch\u00f6n", "sich", "Stell", "und", "Ort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "PRF", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller deiner Sylben f\u00fcgen,", "tokens": ["Al\u00b7ler", "dei\u00b7ner", "Syl\u00b7ben", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Warlich, ", "tokens": ["War\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Klingt, da\u00df niemand sagen kan", "tokens": ["Klingt", ",", "da\u00df", "nie\u00b7mand", "sa\u00b7gen", "kan"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welcher Nahme sch\u00f6ner klinget,", "tokens": ["Wel\u00b7cher", "Nah\u00b7me", "sch\u00f6\u00b7ner", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So da\u00df auch der Wiederhall", "tokens": ["So", "da\u00df", "auch", "der", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dich bey Sch = = \u00fcberall", "tokens": ["Dich", "bey", "Sch", "=", "=", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "APPR", "XY", "XY", "XY", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Gleichsam um die Wette singet.", "tokens": ["Gleich\u00b7sam", "um", "die", "Wet\u00b7te", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lacht, ihr Sch\u00e4fer! lacht nur nicht,", "tokens": ["Lacht", ",", "ihr", "Sch\u00e4\u00b7fer", "!", "lacht", "nur", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Schertz ist nicht vergebens!", "tokens": ["Die\u00b7ser", "Schertz", "ist", "nicht", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt! ", "tokens": ["H\u00f6rt", "!"], "token_info": ["word", "punct"], "pos": ["VVIMP", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Komm du Anfang meines Lebens,", "tokens": ["Komm", "du", "An\u00b7fang", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Edle Stunde! zeige dich", "tokens": ["Ed\u00b7le", "Stun\u00b7de", "!", "zei\u00b7ge", "dich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch so sch\u00f6n, als da du mich", "tokens": ["Noch", "so", "sch\u00f6n", ",", "als", "da", "du", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An das Licht der Welt gebohren.", "tokens": ["An", "das", "Licht", "der", "Welt", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tag, dein Anbruch bringt mir Lust;", "tokens": ["Tag", ",", "dein", "An\u00b7bruch", "bringt", "mir", "Lust", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Darum hat dir meine Brust", "tokens": ["Da\u00b7rum", "hat", "dir", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Lauter Freude zugeschworen,", "tokens": ["Lau\u00b7ter", "Freu\u00b7de", "zu\u00b7ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seht! so spricht der holde Mund", "tokens": ["Seht", "!", "so", "spricht", "der", "hol\u00b7de", "Mund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer theuresten ", "tokens": ["Uns\u00b7rer", "theu\u00b7res\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Euch, ihr Sch\u00e4fer, sey es kund,", "tokens": ["Euch", ",", "ihr", "Sch\u00e4\u00b7fer", ",", "sey", "es", "kund", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt, la\u00dft eure Lieder h\u00f6ren.", "tokens": ["Kommt", ",", "la\u00dft", "eu\u00b7re", "Lie\u00b7der", "h\u00f6\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "R\u00fchmt der Sch\u00e4ferinnen Preis,", "tokens": ["R\u00fchmt", "der", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "Preis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche, wie ein jeder wei\u00df,", "tokens": ["Wel\u00b7che", ",", "wie", "ein", "je\u00b7der", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "PWAV", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tausend Tugenden bekr\u00e4ntzen:", "tokens": ["Tau\u00b7send", "Tu\u00b7gen\u00b7den", "be\u00b7kr\u00b7\u00e4nt\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "La\u00dft das angenehme Fest", "tokens": ["La\u00dft", "das", "an\u00b7ge\u00b7neh\u00b7me", "Fest"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "So man uns begehen l\u00e4\u00dft", "tokens": ["So", "man", "uns", "be\u00b7ge\u00b7hen", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "PPER", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Herrlicher als jemahls gl\u00e4ntzen.", "tokens": ["Herr\u00b7li\u00b7cher", "als", "je\u00b7mahls", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schaut auf ihren Schatz zur\u00fcck,", "tokens": ["Schaut", "auf", "ih\u00b7ren", "Schatz", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnscht dem ", "tokens": ["W\u00fcnscht", "dem"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Unserm ", "tokens": ["Un\u00b7serm"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Euch und mich pflegt zu erfreuen,", "tokens": ["Euch", "und", "mich", "pflegt", "zu", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Sieht sein liebstes Eigenthum,", "tokens": ["Sieht", "sein", "liebs\u00b7tes", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht ", "tokens": ["Sieht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Er ist froh, und treibt uns an,", "tokens": ["Er", "ist", "froh", ",", "und", "treibt", "uns", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja sucht selbst, so viel er kan,", "tokens": ["Ja", "sucht", "selbst", ",", "so", "viel", "er", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADV", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie und uns vergn\u00fcgt zu machen.", "tokens": ["Sie", "und", "uns", "ver\u00b7gn\u00fcgt", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kommt ihr Nymphen dieser Flur,", "tokens": ["Kommt", "ihr", "Nym\u00b7phen", "die\u00b7ser", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt und helft uns Kr\u00e4ntze winden,", "tokens": ["Kommt", "und", "helft", "uns", "Kr\u00e4nt\u00b7ze", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helft uns Sch\u00e4fern auf die Spur,", "tokens": ["Helft", "uns", "Sch\u00e4\u00b7fern", "auf", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie man soll ", "tokens": ["Wie", "man", "soll"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIS", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Anmuth, Artigkeit, Verstand", "tokens": ["An\u00b7muth", ",", "Ar\u00b7tig\u00b7keit", ",", "Ver\u00b7stand"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schm\u00fccken mit vereinter Hand", "tokens": ["Schm\u00fc\u00b7cken", "mit", "ver\u00b7ein\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre Blumen-gleiche Jugend:", "tokens": ["Ih\u00b7re", "Blu\u00b7men\u00b7glei\u00b7che", "Ju\u00b7gend", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihrer Sitten Munterkeit", "tokens": ["Ih\u00b7rer", "Sit\u00b7ten", "Mun\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ziert das reine Lilien-Kleid", "tokens": ["Ziert", "das", "rei\u00b7ne", "Li\u00b7li\u00b7en\u00b7Kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Ihrer unbefleckten Tugend.", "tokens": ["Ih\u00b7rer", "un\u00b7be\u00b7fleck\u00b7ten", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch = = rufe doch das Chor", "tokens": ["Sch", "=", "=", "ru\u00b7fe", "doch", "das", "Chor"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["XY", "XY", "XY", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ungezehlter Nachtigallen", "tokens": ["Un\u00b7ge\u00b7zehl\u00b7ter", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch zu guter letzt hervor,", "tokens": ["Noch", "zu", "gu\u00b7ter", "letzt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh die holden Stimmen fallen.", "tokens": ["Eh", "die", "hol\u00b7den", "Stim\u00b7men", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch was hilfts? So sch\u00f6n es klingt,", "tokens": ["Doch", "was", "hilfts", "?", "So", "sch\u00f6n", "es", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$.", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df doch, wenn ", "tokens": ["Mu\u00df", "doch", ",", "wenn"], "token_info": ["word", "word", "punct", "word"], "pos": ["VMFIN", "ADV", "$,", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Philomele selber schweigen,", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", "sel\u00b7ber", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Stimmt sie an: ", "tokens": ["Stimmt", "sie", "an", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Scheint sich warlich, vor Begier,", "tokens": ["Scheint", "sich", "war\u00b7lich", ",", "vor", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Blatt und Zweig nach ihr zu neigen.", "tokens": ["Blatt", "und", "Zweig", "nach", "ihr", "zu", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Alles war kaum halb vergn\u00fcgt,", "tokens": ["Al\u00b7les", "war", "kaum", "halb", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da wir j\u00fcngst in N = = waren:", "tokens": ["Da", "wir", "j\u00fcngst", "in", "N", "=", "=", "wa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "XY", "XY", "XY", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil das Gl\u00fcck es nicht gef\u00fcgt,", "tokens": ["Weil", "das", "Gl\u00fcck", "es", "nicht", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "H\u00e4tte sie sich eingestellt;", "tokens": ["H\u00e4t\u00b7te", "sie", "sich", "ein\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrde keine Lust der Welt", "tokens": ["W\u00fcr\u00b7de", "kei\u00b7ne", "Lust", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre Lust besieget haben:", "tokens": ["Uns\u00b7re", "Lust", "be\u00b7sie\u00b7get", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Wei\u00df mit gantz besondrer Art", "tokens": ["Wei\u00df", "mit", "gantz", "be\u00b7sond\u00b7rer", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Alles was sie sieht zu laben.", "tokens": ["Al\u00b7les", "was", "sie", "sieht", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Thirsis will nicht redlich seyn;", "tokens": ["Thir\u00b7sis", "will", "nicht", "red\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo er diesen Tag nicht ehret.", "tokens": ["Wo", "er", "die\u00b7sen", "Tag", "nicht", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4llt er stets begl\u00fcckter ein,", "tokens": ["F\u00e4llt", "er", "stets", "be\u00b7gl\u00fcck\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So wird auch sein Wunsch erh\u00f6ret.", "tokens": ["So", "wird", "auch", "sein", "Wunsch", "er\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "M\u00fcsse bey vergn\u00fcgtem Sinn", "tokens": ["M\u00fcs\u00b7se", "bey", "ver\u00b7gn\u00fcg\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Silber auf die Scheitel kriegen:", "tokens": ["Sil\u00b7ber", "auf", "die", "Schei\u00b7tel", "krie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "O! so wird sich jedermann,", "tokens": ["O", "!", "so", "wird", "sich", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "PRF", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Den dein Gl\u00fcck erfreuen kan,", "tokens": ["Den", "dein", "Gl\u00fcck", "er\u00b7freu\u00b7en", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auch an ihrer Lust vergn\u00fcgen.", "tokens": ["Auch", "an", "ih\u00b7rer", "Lust", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}