{"textgrid.poem.62957": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Fleischer ri\u00df ein Lamm im Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fleischer ri\u00df ein Lamm im Schlaf", "tokens": ["Ein", "Flei\u00b7scher", "ri\u00df", "ein", "Lamm", "im", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Euter seiner frommen Amme:", "tokens": ["Vom", "Eu\u00b7ter", "sei\u00b7ner", "from\u00b7men", "Am\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbgrausamer, \u00e4chzt das bange Schaf,", "tokens": ["\u00bb", "grau\u00b7sa\u00b7mer", ",", "\u00e4chzt", "das", "ban\u00b7ge", "Schaf", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sto\u00df, ungetrennt von meinem Lamme", "tokens": ["Sto\u00df", ",", "un\u00b7ge\u00b7trennt", "von", "mei\u00b7nem", "Lam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch mir dein Me\u00dfer in das Herz!\u00ab", "tokens": ["Auch", "mir", "dein", "Me\u00b7\u00dfer", "in", "das", "Herz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nein, rief der Mann mit bitterm Scherz,", "tokens": ["Nein", ",", "rief", "der", "Mann", "mit", "bit\u00b7term", "Scherz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich mu\u00df dich erst noch fetter machen.", "tokens": ["Ich", "mu\u00df", "dich", "erst", "noch", "fet\u00b7ter", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbdu mich?\u00ab erwiedert, mit dem Schmerz", "tokens": ["\u00bb", "du", "mich", "?", "\u00ab", "er\u00b7wie\u00b7dert", ",", "mit", "dem", "Schmerz"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "PRF", "$.", "$(", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Niobe, die arme Mutter:", "tokens": ["Der", "Ni\u00b7o\u00b7be", ",", "die", "ar\u00b7me", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbdas wirst du nicht.\u00ab Von nun an a\u00df", "tokens": ["\u00bb", "das", "wirst", "du", "nicht", ".", "\u00ab", "Von", "nun", "an", "a\u00df"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VAFIN", "PPER", "PTKNEG", "$.", "$(", "APPR", "ADV", "APZR", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie keinen Halm von ihrem Futter", "tokens": ["Sie", "kei\u00b7nen", "Halm", "von", "ih\u00b7rem", "Fut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und trank nicht mehr. Der Fleischer sahs", "tokens": ["Und", "trank", "nicht", "mehr", ".", "Der", "Flei\u00b7scher", "sahs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und trieb sie schon am vierten Tage", "tokens": ["Und", "trieb", "sie", "schon", "am", "vier\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zur W\u00fcrgbank: lieber schlacht ich dich,", "tokens": ["Zur", "W\u00fcrg\u00b7bank", ":", "lie\u00b7ber", "schlacht", "ich", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als da\u00df ich dich zum Schinder trage,", "tokens": ["Als", "da\u00df", "ich", "dich", "zum", "Schin\u00b7der", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sprach er. \u00bbDa siehst du's, W\u00fcterich;", "tokens": ["Sprach", "er", ".", "\u00bb", "Da", "siehst", "du's", ",", "W\u00fc\u00b7te\u00b7rich", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "$(", "ADV", "VVFIN", "PIS", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Versetzt das Schaf mit heitrer Seele,", "tokens": ["Ver\u00b7setzt", "das", "Schaf", "mit", "hei\u00b7trer", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Es ist auf Erden kein Tyrann", "tokens": ["Es", "ist", "auf", "Er\u00b7den", "kein", "Ty\u00b7rann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "So m\u00e4chtig, da\u00df er dem befehle,", "tokens": ["So", "m\u00e4ch\u00b7tig", ",", "da\u00df", "er", "dem", "be\u00b7feh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der sterben will und sterben kann.\u00ab", "tokens": ["Der", "ster\u00b7ben", "will", "und", "ster\u00b7ben", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "VVINF", "VMFIN", "KON", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}