{"textgrid.poem.41602": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n bin ich, Sterbliche, gleich einem Traum von Steine,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n bin ich, Sterbliche, gleich einem Traum von Steine,", "tokens": ["Sch\u00f6n", "bin", "ich", ",", "Sterb\u00b7li\u00b7che", ",", "gleich", "ei\u00b7nem", "Traum", "von", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und meine Brust, die nichts als Wunden euch gebracht,", "tokens": ["Und", "mei\u00b7ne", "Brust", ",", "die", "nichts", "als", "Wun\u00b7den", "euch", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PIS", "KOKOM", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erf\u00fcllt des Dichters Sinn mit einer Liebe Macht,", "tokens": ["Er\u00b7f\u00fcllt", "des", "Dich\u00b7ters", "Sinn", "mit", "ei\u00b7ner", "Lie\u00b7be", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die stumm ist wie der Stoff und strahlt in starrer Reine.", "tokens": ["Die", "stumm", "ist", "wie", "der", "Stoff", "und", "strahlt", "in", "star\u00b7rer", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "KOKOM", "ART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gleich einer Sphinx thron ich in blauer L\u00fcfte Wehn,", "tokens": ["Gleich", "ei\u00b7ner", "Sphinx", "thron", "ich", "in", "blau\u00b7er", "L\u00fcf\u00b7te", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Schnee ist mein Herz, mein Leib wei\u00df wie des Schwans Gefieder,", "tokens": ["Schnee", "ist", "mein", "Herz", ",", "mein", "Leib", "wei\u00df", "wie", "des", "Schwans", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Bewegung bleibe fern dem stillen Ruhn der Glieder:", "tokens": ["Be\u00b7we\u00b7gung", "blei\u00b7be", "fern", "dem", "stil\u00b7len", "Ruhn", "der", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nie wirst du weinen mich und niemals lachen sehn.", "tokens": ["Nie", "wirst", "du", "wei\u00b7nen", "mich", "und", "nie\u00b7mals", "la\u00b7chen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "PPER", "KON", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wi\u00dft, da\u00df die Dichter vor den m\u00e4chtigen Geb\u00e4rden,", "tokens": ["Wi\u00dft", ",", "da\u00df", "die", "Dich\u00b7ter", "vor", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Die ich den Statuen leihe, stolz und schicksalsschwer,", "tokens": ["Die", "ich", "den", "Sta\u00b7tu\u00b7en", "lei\u00b7he", ",", "stolz", "und", "schick\u00b7sals\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Mich zu betrachten Herz und Sinn verzehren werden;", "tokens": ["Mich", "zu", "be\u00b7trach\u00b7ten", "Herz", "und", "Sinn", "ver\u00b7zeh\u00b7ren", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mein sind, stets zu erh\u00f6hn der Liebenden Begehr,", "tokens": ["Mein", "sind", ",", "stets", "zu", "er\u00b7h\u00f6hn", "der", "Lie\u00b7ben\u00b7den", "Be\u00b7gehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "$,", "ADV", "PTKZU", "VVINF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwei Spiegel, drin versch\u00f6nt sich alle Dinge malen:", "tokens": ["Zwei", "Spie\u00b7gel", ",", "drin", "ver\u00b7sch\u00f6nt", "sich", "al\u00b7le", "Din\u00b7ge", "ma\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADV", "VVFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Augen, gro\u00df und weit, die ewge Klarheit strahlen.", "tokens": ["Die", "Au\u00b7gen", ",", "gro\u00df", "und", "weit", ",", "die", "ew\u00b7ge", "Klar\u00b7heit", "strah\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}