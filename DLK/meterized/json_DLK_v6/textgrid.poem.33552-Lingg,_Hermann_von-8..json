{"textgrid.poem.33552": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, einmal nimmt der Mensch von seinen Tagen", "tokens": ["Ja", ",", "ein\u00b7mal", "nimmt", "der", "Mensch", "von", "sei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im voraus schon des Gl\u00fcckes Zinsen ein,", "tokens": ["Im", "vo\u00b7raus", "schon", "des", "Gl\u00fc\u00b7ckes", "Zin\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und spricht: ich will den Kranz der Freude tragen,", "tokens": ["Und", "spricht", ":", "ich", "will", "den", "Kranz", "der", "Freu\u00b7de", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mag, was darauf folgt, nur noch Asche sein.", "tokens": ["Mag", ",", "was", "da\u00b7rauf", "folgt", ",", "nur", "noch", "A\u00b7sche", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PRELS", "PAV", "VVFIN", "$,", "ADV", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die vollen Becher! La\u00df uns alles wagen!", "tokens": ["Die", "vol\u00b7len", "Be\u00b7cher", "!", "La\u00df", "uns", "al\u00b7les", "wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVIMP", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ja einmal will ich auf den Mittagsh\u00f6h'n", "tokens": ["Ja", "ein\u00b7mal", "will", "ich", "auf", "den", "Mit\u00b7tags\u00b7h\u00f6h'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Lebens stehn und dann am Ende sagen:", "tokens": ["Des", "Le\u00b7bens", "stehn", "und", "dann", "am", "En\u00b7de", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie war es doch so sch\u00f6n!", "tokens": ["Wie", "war", "es", "doch", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie war der Traum so sch\u00f6n! Da wir uns liebten,", "tokens": ["Wie", "war", "der", "Traum", "so", "sch\u00f6n", "!", "Da", "wir", "uns", "lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$.", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da bl\u00fchten Rosen um den Trauerzug;", "tokens": ["Da", "bl\u00fch\u00b7ten", "Ro\u00b7sen", "um", "den", "Trau\u00b7er\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Schaum der Tage, die sonst leer zerstiebten,", "tokens": ["Im", "Schaum", "der", "Ta\u00b7ge", ",", "die", "sonst", "leer", "zer\u00b7stieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War eine Perle, reich und stolz genug.", "tokens": ["War", "ei\u00b7ne", "Per\u00b7le", ",", "reich", "und", "stolz", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich will den Arm um deinen Nacken schlingen,", "tokens": ["Ich", "will", "den", "Arm", "um", "dei\u00b7nen", "Na\u00b7cken", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und durch die Ferne der Erinnrung t\u00f6n':", "tokens": ["Und", "durch", "die", "Fer\u00b7ne", "der", "E\u00b7rinn\u00b7rung", "t\u00f6n'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kann keine Zeit das Gl\u00fcck uns wiederbringen \u2013", "tokens": ["Kann", "kei\u00b7ne", "Zeit", "das", "Gl\u00fcck", "uns", "wie\u00b7der\u00b7brin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie war es doch so sch\u00f6n!", "tokens": ["Wie", "war", "es", "doch", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}