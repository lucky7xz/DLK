{"textgrid.poem.50968": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Unruhe der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun bin ich untreu worden", "tokens": ["Nun", "bin", "ich", "un\u00b7treu", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sonn und ihrem Schein;", "tokens": ["Der", "Sonn", "und", "ih\u00b7rem", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Nacht, die Nacht soll Dame", "tokens": ["Die", "Nacht", ",", "die", "Nacht", "soll", "Da\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VMFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun meines Herzens sein!", "tokens": ["Nun", "mei\u00b7nes", "Her\u00b7zens", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie ist von d\u00fcstrer Sch\u00f6nheit,", "tokens": ["Sie", "ist", "von", "d\u00fcst\u00b7rer", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat ein bleiches Nornengesicht,", "tokens": ["Hat", "ein", "blei\u00b7ches", "Nor\u00b7nen\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und eine Sternenkrone", "tokens": ["Und", "ei\u00b7ne", "Ster\u00b7nen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr dunkles Haupt umflicht.", "tokens": ["Ihr", "dunk\u00b7les", "Haupt", "um\u00b7flicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Heut ist sie so beklommen,", "tokens": ["Heut", "ist", "sie", "so", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unruhig und voller Pein;", "tokens": ["Un\u00b7ru\u00b7hig", "und", "vol\u00b7ler", "Pein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie denkt wohl an ihre Jugend \u2013", "tokens": ["Sie", "denkt", "wohl", "an", "ih\u00b7re", "Ju\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das mu\u00df ein Ged\u00e4chtnis sein!", "tokens": ["Das", "mu\u00df", "ein", "Ge\u00b7d\u00e4cht\u00b7nis", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es weht durch alle T\u00e4ler", "tokens": ["Es", "weht", "durch", "al\u00b7le", "T\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein St\u00f6hnen, so klagend und bang;", "tokens": ["Ein", "St\u00f6h\u00b7nen", ",", "so", "kla\u00b7gend", "und", "bang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie Tr\u00e4nenb\u00e4che flie\u00dfen", "tokens": ["Wie", "Tr\u00e4\u00b7nen\u00b7b\u00e4\u00b7che", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Quellen vom Bergeshang.", "tokens": ["Die", "Quel\u00b7len", "vom", "Ber\u00b7ges\u00b7hang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die schwarzen Fichten sausen", "tokens": ["Die", "schwar\u00b7zen", "Fich\u00b7ten", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wiegen sich her und hin,", "tokens": ["Und", "wie\u00b7gen", "sich", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und \u00fcber die wilde Heide", "tokens": ["Und", "\u00fc\u00b7ber", "die", "wil\u00b7de", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verlorene Lichter fliehn.", "tokens": ["Ver\u00b7lo\u00b7re\u00b7ne", "Lich\u00b7ter", "fliehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Dem Himmel bringt ein St\u00e4ndchen", "tokens": ["Dem", "Him\u00b7mel", "bringt", "ein", "St\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das dumpf aufrauschende Meer,", "tokens": ["Das", "dumpf", "auf\u00b7rau\u00b7schen\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und \u00fcber mir zieht ein Gewitter", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "zieht", "ein", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit klingendem Spiele daher.", "tokens": ["Mit", "klin\u00b7gen\u00b7dem", "Spie\u00b7le", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Es will vielleicht bet\u00e4uben", "tokens": ["Es", "will", "viel\u00b7leicht", "be\u00b7t\u00e4u\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Nacht den uralten Schmerz?", "tokens": ["Die", "Nacht", "den", "ur\u00b7al\u00b7ten", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und an noch \u00e4ltere S\u00fcnden", "tokens": ["Und", "an", "noch", "\u00e4l\u00b7te\u00b7re", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Denkt wohl ihr reuiges Herz?", "tokens": ["Denkt", "wohl", "ihr", "reu\u00b7i\u00b7ges", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Ich m\u00f6chte mit ihr plaudern,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "mit", "ihr", "plau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie man mit dem Liebchen spricht \u2013", "tokens": ["Wie", "man", "mit", "dem", "Lieb\u00b7chen", "spricht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Umsonst, in ihrem Grame", "tokens": ["Um\u00b7sonst", ",", "in", "ih\u00b7rem", "Gra\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sieht und h\u00f6rt mich nicht!", "tokens": ["Sie", "sieht", "und", "h\u00f6rt", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich m\u00f6chte sie gern befragen", "tokens": ["Ich", "m\u00f6ch\u00b7te", "sie", "gern", "be\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und werde doch immer gest\u00f6rt,", "tokens": ["Und", "wer\u00b7de", "doch", "im\u00b7mer", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ob sie vor meiner Geburt schon", "tokens": ["Ob", "sie", "vor", "mei\u00b7ner", "Ge\u00b7burt", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo meinen Namen geh\u00f6rt?", "tokens": ["Wo", "mei\u00b7nen", "Na\u00b7men", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Sie ist eine alte Sibylle", "tokens": ["Sie", "ist", "ei\u00b7ne", "al\u00b7te", "Si\u00b7byl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und kennt sich selber kaum;", "tokens": ["Und", "kennt", "sich", "sel\u00b7ber", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie und der Tod und wir alle", "tokens": ["Sie", "und", "der", "Tod", "und", "wir", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "ART", "NN", "KON", "PPER", "PIAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sind Tr\u00e4ume von einem Traum.", "tokens": ["Sind", "Tr\u00e4u\u00b7me", "von", "ei\u00b7nem", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Ich will mich schlafen legen,", "tokens": ["Ich", "will", "mich", "schla\u00b7fen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Morgenwind schon zieht \u2013", "tokens": ["Der", "Mor\u00b7gen\u00b7wind", "schon", "zieht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Trauerweiden am Kirchhof,", "tokens": ["Ihr", "Trau\u00b7er\u00b7wei\u00b7den", "am", "Kirch\u00b7hof", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Summt mir das Schlummerlied!", "tokens": ["Summt", "mir", "das", "Schlum\u00b7mer\u00b7lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}