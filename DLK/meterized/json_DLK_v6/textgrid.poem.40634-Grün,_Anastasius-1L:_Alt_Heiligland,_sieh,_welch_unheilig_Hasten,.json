{"textgrid.poem.40634": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alt Heiligland, sieh, welch unheilig Hasten,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alt Heiligland, sieh, welch unheilig Hasten,", "tokens": ["Alt", "Hei\u00b7lig\u00b7land", ",", "sieh", ",", "welch", "un\u00b7hei\u00b7lig", "Has\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "$,", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gro\u00dfe Meeresstra\u00df' entlang welch Jagen!", "tokens": ["Die", "gro\u00b7\u00dfe", "Mee\u00b7res\u00b7stra\u00df'", "ent\u00b7lang", "welch", "Ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPO", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An dir vorbei in hohen Wogen schlagen", "tokens": ["An", "dir", "vor\u00b7bei", "in", "ho\u00b7hen", "Wo\u00b7gen", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Lebens tolle Wirbel, die nicht rasten.", "tokens": ["Des", "Le\u00b7bens", "tol\u00b7le", "Wir\u00b7bel", ",", "die", "nicht", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Da steuern hin, die liebten und die ha\u00dften,", "tokens": ["Da", "steu\u00b7ern", "hin", ",", "die", "lieb\u00b7ten", "und", "die", "ha\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PRELS", "VVFIN", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da segeln die gewonnen, die noch wagen,", "tokens": ["Da", "se\u00b7geln", "die", "ge\u00b7won\u00b7nen", ",", "die", "noch", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Thor, der Weise, Hoffnung und Entsagen,", "tokens": ["Der", "Thor", ",", "der", "Wei\u00b7se", ",", "Hoff\u00b7nung", "und", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und des Verbrechens Last mit andern Lasten.", "tokens": ["Und", "des", "Ver\u00b7bre\u00b7chens", "Last", "mit", "an\u00b7dern", "Las\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch du blickst ernst und streng ins Weltgetriebe", "tokens": ["Doch", "du", "blickst", "ernst", "und", "streng", "ins", "Welt\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Ruh, fast priesterhaft, und warst beflissen", "tokens": ["Voll", "Ruh", ",", "fast", "pries\u00b7ter\u00b7haft", ",", "und", "warst", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "ADV", "ADJD", "$,", "KON", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Rettungsboot und deines Leuchtthurms Flamme;", "tokens": ["Dein", "Ret\u00b7tungs\u00b7boot", "und", "dei\u00b7nes", "Leucht\u00b7thurms", "Flam\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So \u00fcbst du still ein Priesteramt der Liebe,", "tokens": ["So", "\u00fcbst", "du", "still", "ein", "Pries\u00b7ter\u00b7amt", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bringst H\u00fclf' in N\u00f6then, Licht in Finsternissen,", "tokens": ["Bringst", "H\u00fcl\u00b7f'", "in", "N\u00f6\u00b7then", ",", "Licht", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein heilig Land nicht blos dem Friesenstamme.", "tokens": ["Ein", "hei\u00b7lig", "Land", "nicht", "blos", "dem", "Fries\u00b7en\u00b7stam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}