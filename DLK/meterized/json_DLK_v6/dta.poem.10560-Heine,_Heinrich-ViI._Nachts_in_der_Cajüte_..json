{"dta.poem.10560": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Nachts in der Caj\u00fcte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Es tr\u00e4umte mir von einer weiten Haide,", "tokens": ["Es", "tr\u00e4um\u00b7te", "mir", "von", "ei\u00b7ner", "wei\u00b7ten", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weit \u00fcberdeckt von wei\u00dfem, wei\u00dfem Schnee,", "tokens": ["Weit", "\u00fc\u00b7berd\u00b7eckt", "von", "wei\u00b7\u00dfem", ",", "wei\u00b7\u00dfem", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unter'm wei\u00dfen Schnee lag ich begraben", "tokens": ["Und", "un\u00b7ter'm", "wei\u00b7\u00dfen", "Schnee", "lag", "ich", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schlief den einsam kalten Todesschlaf.", "tokens": ["Und", "schlief", "den", "ein\u00b7sam", "kal\u00b7ten", "To\u00b7des\u00b7schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch droben aus dem dunkeln Himmel schauten", "tokens": ["Doch", "dro\u00b7ben", "aus", "dem", "dun\u00b7keln", "Him\u00b7mel", "schau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herunter auf mein Grab die Sternenaugen,", "tokens": ["Her\u00b7un\u00b7ter", "auf", "mein", "Grab", "die", "Ster\u00b7nen\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die s\u00fc\u00dfen Augen! und sie gl\u00e4nzten sieghaft", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen", "!", "und", "sie", "gl\u00e4nz\u00b7ten", "sieg\u00b7haft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ruhig heiter, aber voller Liebe.", "tokens": ["Und", "ru\u00b7hig", "hei\u00b7ter", ",", "a\u00b7ber", "vol\u00b7ler", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}