{"textgrid.poem.25148": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[wie lieb ich das: wenn still aus goldnen Kr\u00fcgen]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb ich das: wenn still aus goldnen Kr\u00fcgen", "tokens": ["Wie", "lieb", "ich", "das", ":", "wenn", "still", "aus", "gold\u00b7nen", "Kr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "PDS", "$.", "KOUS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sommerabend flie\u00dft und die gelinden", "tokens": ["Der", "Som\u00b7mer\u00b7a\u00b7bend", "flie\u00dft", "und", "die", "ge\u00b7lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00dfw\u00f6lkchen ruhn auf duftgeschwellten Winden,", "tokens": ["Wei\u00df\u00b7w\u00f6lk\u00b7chen", "ruhn", "auf", "duft\u00b7ge\u00b7schwell\u00b7ten", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von tr\u00fcbem Denken mich hinwegzul\u00fcgen;", "tokens": ["Von", "tr\u00fc\u00b7bem", "Den\u00b7ken", "mich", "hin\u00b7weg\u00b7zu\u00b7l\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Befreit vom Kleinlichen, in vollen Z\u00fcgen", "tokens": ["Be\u00b7freit", "vom", "Klein\u00b7li\u00b7chen", ",", "in", "vol\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Glanz zu trinken, ein Versteck zu finden,", "tokens": ["Den", "Glanz", "zu", "trin\u00b7ken", ",", "ein", "Ver\u00b7steck", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Sch\u00f6nheit und Natur sich Kr\u00e4nze winden,", "tokens": ["Wo", "Sch\u00f6n\u00b7heit", "und", "Na\u00b7tur", "sich", "Kr\u00e4n\u00b7ze", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dort mein Herz zur Freude zu betr\u00fcgen;", "tokens": ["Und", "dort", "mein", "Herz", "zur", "Freu\u00b7de", "zu", "be\u00b7tr\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ans heimatlich Erhabne mich zu dr\u00e4ngen,", "tokens": ["Ans", "hei\u00b7mat\u00b7lich", "Er\u00b7hab\u00b7ne", "mich", "zu", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Schicksal Milton's, Sidney's nachzuh\u00e4ngen,", "tokens": ["Dem", "Schick\u00b7sal", "Mil\u00b7ton's", ",", "Sid\u00b7ney's", "nach\u00b7zu\u00b7h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "NE", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis beide ernst vor meiner Seele leuchten \u2013", "tokens": ["Bis", "bei\u00b7de", "ernst", "vor", "mei\u00b7ner", "See\u00b7le", "leuch\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vielleicht im Liede mich hinaufzuschwingen,", "tokens": ["Viel\u00b7leicht", "im", "Lie\u00b7de", "mich", "hin\u00b7auf\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis Melodieen mir die Augen feuchten", "tokens": ["Bis", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "mir", "die", "Au\u00b7gen", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Lust und Leid in Tr\u00e4nen sanft verklingen.", "tokens": ["Und", "Lust", "und", "Leid", "in", "Tr\u00e4\u00b7nen", "sanft", "ver\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}