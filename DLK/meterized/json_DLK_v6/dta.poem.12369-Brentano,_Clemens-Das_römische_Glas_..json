{"dta.poem.12369": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das r\u00f6mische Glas .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stand ich auf einem hohen Berg,               ", "tokens": ["Stand", "ich", "auf", "ei\u00b7nem", "ho\u00b7hen", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah wohl den tiefen, tiefen Rhein,", "tokens": ["Sah", "wohl", "den", "tie\u00b7fen", ",", "tie\u00b7fen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah ich ein Schifflein schweben,", "tokens": ["Sah", "ich", "ein", "Schif\u00b7flein", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel Ritter tranken drein.", "tokens": ["Viel", "Rit\u00b7ter", "tran\u00b7ken", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der j\u00fcngste, der darunter war,", "tokens": ["Der", "j\u00fcngs\u00b7te", ",", "der", "da\u00b7run\u00b7ter", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PAV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hob auf sein r\u00f6misches Glas,", "tokens": ["Hob", "auf", "sein", "r\u00f6\u00b7mi\u00b7sches", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Th\u00e4t mir damit zuwinken:", "tokens": ["Th\u00e4t", "mir", "da\u00b7mit", "zu\u00b7win\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201efeins Lieb, ich bring dir das!\u201c", "tokens": ["\u201e", "feins", "Lieb", ",", "ich", "bring", "dir", "das", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PDS", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201ewas thust du mir zutrinken,", "tokens": ["\u201e", "was", "thust", "du", "mir", "zu\u00b7trin\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ewas bietst du mir den Wein,", "tokens": ["\u201e", "was", "bietst", "du", "mir", "den", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201emein Vater will mich ins Kloster thun,", "tokens": ["\u201e", "mein", "Va\u00b7ter", "will", "mich", "ins", "Klos\u00b7ter", "thun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201esoll Gottes Dienerin seyn.\u201c", "tokens": ["\u201e", "soll", "Got\u00b7tes", "Die\u00b7ne\u00b7rin", "seyn", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "NN", "NN", "VAINF", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Des Nachts wohl um die halbe Nacht,", "tokens": ["Des", "Nachts", "wohl", "um", "die", "hal\u00b7be", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4umt es dem Ritter so schwer,", "tokens": ["Tr\u00e4umt", "es", "dem", "Rit\u00b7ter", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Als ob sein herzallerliebster Schatz", "tokens": ["Als", "ob", "sein", "her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "Schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ins Kloster gangen w\u00e4r.", "tokens": ["Ins", "Klos\u00b7ter", "gan\u00b7gen", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201eknecht, sattle mir und dir zwei Ro\u00df,", "tokens": ["\u201e", "knecht", ",", "satt\u00b7le", "mir", "und", "dir", "zwei", "Ro\u00df", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "KON", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emein Haupt ist mir so schwer,", "tokens": ["\u201e", "mein", "Haupt", "ist", "mir", "so", "schwer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich leerte gar viel mein r\u00f6misch Glas,", "tokens": ["\u201e", "ich", "leer\u00b7te", "gar", "viel", "mein", "r\u00f6\u00b7misch", "Glas", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201edas Schiff gieng hin und her:", "tokens": ["\u201e", "das", "Schiff", "gieng", "hin", "und", "her", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201emir tr\u00e4umt', ich h\u00e4tt' eine Nonn gesehn,", "tokens": ["\u201e", "mir", "tr\u00e4umt'", ",", "ich", "h\u00e4tt'", "ei\u00b7ne", "Nonn", "ge\u00b7sehn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u201eich trank ihr zu mein Glas,", "tokens": ["\u201e", "ich", "trank", "ihr", "zu", "mein", "Glas", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201esie wollt nicht gern ins Kloster gehn,", "tokens": ["\u201e", "sie", "wollt", "nicht", "gern", "ins", "Klos\u00b7ter", "gehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eihr Aeuglein waren na\u00df.", "tokens": ["\u201e", "ihr", "A\u00b7e\u00b7u\u00b7glein", "wa\u00b7ren", "na\u00df", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201ehalt an! Halt an am Klosterthor!", "tokens": ["\u201e", "halt", "an", "!", "Halt", "an", "am", "Klos\u00b7ter\u00b7thor", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "\u201eruf mir mein Lieb heraus!\u201c", "tokens": ["\u201e", "ruf", "mir", "mein", "Lieb", "he\u00b7raus", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da kam die \u00e4ltste Nonn hervor,", "tokens": ["Da", "kam", "die", "\u00e4lts\u00b7te", "Nonn", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201emein Lieb soll kommen heraus.", "tokens": ["\u201e", "mein", "Lieb", "soll", "kom\u00b7men", "he\u00b7raus", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "\u201ekein Feinslieb ist hier innen,", "tokens": ["\u201e", "kein", "Feins\u00b7lieb", "ist", "hier", "in\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ekein Feinslieb kann heraus.\u201c", "tokens": ["\u201e", "kein", "Feins\u00b7lieb", "kann", "he\u00b7raus", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "VMFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eund wenn kein Feinslieb drinnen ist,", "tokens": ["\u201e", "und", "wenn", "kein", "Feins\u00b7lieb", "drin\u00b7nen", "ist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PIAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eso steck ich an das Haus.\u201c", "tokens": ["\u201e", "so", "steck", "ich", "an", "das", "Haus", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da kam Feinslieb gegangen,", "tokens": ["Da", "kam", "Feins\u00b7lieb", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schneeweis war sie gekleidt:", "tokens": ["Schnee\u00b7weis", "war", "sie", "ge\u00b7kleidt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u201emein Haar ist abgeschnitten,", "tokens": ["\u201e", "mein", "Haar", "ist", "ab\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eleb wohl in Ewigkeit!\u201c", "tokens": ["\u201e", "leb", "wohl", "in", "E\u00b7wig\u00b7keit", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er vor dem Kloster niedersa\u00df,", "tokens": ["Er", "vor", "dem", "Klos\u00b7ter", "nie\u00b7der\u00b7sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah ins tiefe, tiefe Thal,", "tokens": ["Und", "sah", "ins", "tie\u00b7fe", ",", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versprang ihm wohl sein r\u00f6misch Glas,", "tokens": ["Ver\u00b7sprang", "ihm", "wohl", "sein", "r\u00f6\u00b7misch", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versprang ihm wohl sein Herz.", "tokens": ["Ver\u00b7sprang", "ihm", "wohl", "sein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}