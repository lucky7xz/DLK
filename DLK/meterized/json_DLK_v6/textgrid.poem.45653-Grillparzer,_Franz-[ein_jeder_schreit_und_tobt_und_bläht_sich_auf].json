{"textgrid.poem.45653": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ein jeder schreit und tobt und bl\u00e4ht sich auf]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder schreit und tobt und bl\u00e4ht sich auf,", "tokens": ["Ein", "je\u00b7der", "schreit", "und", "tobt", "und", "bl\u00e4ht", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "VVFIN", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soll alles ihm und seinem D\u00fcnkel dienen:", "tokens": ["Soll", "al\u00b7les", "ihm", "und", "sei\u00b7nem", "D\u00fcn\u00b7kel", "die\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist nun schon der Mode Lauf,", "tokens": ["Das", "ist", "nun", "schon", "der", "Mo\u00b7de", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es sind die m\u00e4nnlichen Krinolinen.", "tokens": ["Es", "sind", "die", "m\u00e4nn\u00b7li\u00b7chen", "Kri\u00b7no\u00b7li\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}