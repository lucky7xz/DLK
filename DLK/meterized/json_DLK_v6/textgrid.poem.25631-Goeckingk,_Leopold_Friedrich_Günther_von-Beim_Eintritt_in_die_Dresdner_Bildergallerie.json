{"textgrid.poem.25631": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Beim Eintritt in die Dresdner Bildergallerie", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich liebt ein M\u00e4dchen, reitzend wie Cythere,", "tokens": ["Mich", "liebt", "ein", "M\u00e4d\u00b7chen", ",", "reit\u00b7zend", "wie", "Cy\u00b7the\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KOKOM", "NE", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch niemals fiel der Wunsch mir ein:", "tokens": ["Doch", "nie\u00b7mals", "fiel", "der", "Wunsch", "mir", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich ein Argus m\u00f6chte seyn,", "tokens": ["Da\u00df", "ich", "ein", "Ar\u00b7gus", "m\u00f6ch\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Itzt aber w\u00fcnscht' ich, da\u00df ich's w\u00e4re.", "tokens": ["Itzt", "a\u00b7ber", "w\u00fcnscht'", "ich", ",", "da\u00df", "ich's", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}