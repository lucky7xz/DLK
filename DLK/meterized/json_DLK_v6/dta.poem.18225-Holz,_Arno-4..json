{"dta.poem.18225": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Nur der Nachtwind spielt in den B\u00e4umen,", "tokens": ["Nur", "der", "Nacht\u00b7wind", "spielt", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und heimlich vermahnt mich der Mondenschein:", "tokens": ["Und", "heim\u00b7lich", "ver\u00b7mahnt", "mich", "der", "Mon\u00b7den\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Nun ist es Zeit zum Tr\u00e4umen.", "tokens": ["Nun", "ist", "es", "Zeit", "zum", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja, tr\u00e4umen will ich, das Haupt in der Hand,", "tokens": ["Ja", ",", "tr\u00e4u\u00b7men", "will", "ich", ",", "das", "Haupt", "in", "der", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVINF", "VMFIN", "PPER", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von dir, die den Frieden mir brachte;", "tokens": ["Von", "dir", ",", "die", "den", "Frie\u00b7den", "mir", "brach\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Es ging ja noch nimmer ein St\u00fcndlein ins Land,", "tokens": ["Es", "ging", "ja", "noch", "nim\u00b7mer", "ein", "St\u00fcnd\u00b7lein", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Darin ich nicht deiner gedachte.", "tokens": ["Da\u00b7rin", "ich", "nicht", "dei\u00b7ner", "ge\u00b7dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PTKNEG", "PPOSAT", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Du Leid meines Leides, du Lust meiner Lust,", "tokens": ["Du", "Leid", "mei\u00b7nes", "Lei\u00b7des", ",", "du", "Lust", "mei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Schl\u00e4gst du doch als Herz mir hier tief in der Brust!", "tokens": ["Schl\u00e4gst", "du", "doch", "als", "Herz", "mir", "hier", "tief", "in", "der", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Augenspiel gr\u00fc\u00dft mich im Funkeln des Thaus,", "tokens": ["Dein", "Au\u00b7gen\u00b7spiel", "gr\u00fc\u00dft", "mich", "im", "Fun\u00b7keln", "des", "Thaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der rings auf die Gr\u00e4ser gefallen,", "tokens": ["Der", "rings", "auf", "die", "Gr\u00e4\u00b7ser", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und dein Athem weht dr\u00fcben ums Gartenhaus,", "tokens": ["Und", "dein", "A\u00b7them", "weht", "dr\u00fc\u00b7ben", "ums", "Gar\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "--+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das die D\u00fcfte der Maien umwallen.", "tokens": ["Das", "die", "D\u00fcf\u00b7te", "der", "Mai\u00b7en", "um\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und was nun im Flieder die Nachtigall singt,", "tokens": ["Und", "was", "nun", "im", "Flie\u00b7der", "die", "Nach\u00b7ti\u00b7gall", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sind ach, meine eignen Gedanken,", "tokens": ["Sind", "ach", ",", "mei\u00b7ne", "eig\u00b7nen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Die bl\u00fcthenumfl\u00fcstert und silberumblinkt", "tokens": ["Die", "bl\u00fc\u00b7the\u00b7num\u00b7fl\u00fcs\u00b7tert", "und", "sil\u00b7be\u00b7rum\u00b7blinkt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Um meine Liebe sich ranken.", "tokens": ["Um", "mei\u00b7ne", "Lie\u00b7be", "sich", "ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ach, was ich nur jemals gef\u00fchlt und gedacht,", "tokens": ["Ach", ",", "was", "ich", "nur", "je\u00b7mals", "ge\u00b7f\u00fchlt", "und", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "ADV", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Nun klingt es hinaus in das Schweigen der Nacht!", "tokens": ["Nun", "klingt", "es", "hin\u00b7aus", "in", "das", "Schwei\u00b7gen", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Nur Eins hat sich nie und nimmer gewu\u00dft", "tokens": ["Nur", "Eins", "hat", "sich", "nie", "und", "nim\u00b7mer", "ge\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "PRF", "ADV", "KON", "ADV", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In schmelzende T\u00f6ne zu kleiden,", "tokens": ["In", "schmel\u00b7zen\u00b7de", "T\u00f6\u00b7ne", "zu", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und das ist die aller", "tokens": ["Und", "das", "ist", "die", "al\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ART", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und das aller", "tokens": ["Und", "das", "al\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Doch wo ein Herz ein Herz versteht,", "tokens": ["Doch", "wo", "ein", "Herz", "ein", "Herz", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da \u00f6ffnen sich golden die Pforten", "tokens": ["Da", "\u00f6ff\u00b7nen", "sich", "gol\u00b7den", "die", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und fl\u00fcsternd vor Andacht, wie ein Gebet,", "tokens": ["Und", "fl\u00fcs\u00b7ternd", "vor", "An\u00b7dacht", ",", "wie", "ein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Erklingt's in den heiligen Worten:", "tokens": ["Er\u00b7klingt's", "in", "den", "hei\u00b7li\u00b7gen", "Wor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Dich liebt ich immer, dich lieb ich noch heut", "tokens": ["Dich", "liebt", "ich", "im\u00b7mer", ",", "dich", "lieb", "ich", "noch", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Und werde dich lieben in Ewigkeit!", "tokens": ["Und", "wer\u00b7de", "dich", "lie\u00b7ben", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}