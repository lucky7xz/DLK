{"textgrid.poem.34235": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ich und der Blinde", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einem Blinden wollt' ich die Gabe reichen, doch lie\u00df ich's,", "tokens": ["Ei\u00b7nem", "Blin\u00b7den", "wollt'", "ich", "die", "Ga\u00b7be", "rei\u00b7chen", ",", "doch", "lie\u00df", "ich's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "ADV", "VVFIN", "PIS", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Denn es braus'te der Sturm gar zu gewaltig daher;", "tokens": ["Denn", "es", "braus'\u00b7te", "der", "Sturm", "gar", "zu", "ge\u00b7wal\u00b7tig", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "PTKA", "ADJD", "PAV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vorw\u00e4rts eilt' ich, da jagte mir dieser ein St\u00e4ubchen in's Auge,", "tokens": ["Vor\u00b7w\u00e4rts", "eilt'", "ich", ",", "da", "jag\u00b7te", "mir", "die\u00b7ser", "ein", "St\u00e4ub\u00b7chen", "in's", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PDAT", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "So an die Blindheit gemahnt, kehrt' ich zur\u00fcck nun und gab.", "tokens": ["So", "an", "die", "Blind\u00b7heit", "ge\u00b7mahnt", ",", "kehrt'", "ich", "zu\u00b7r\u00fcck", "nun", "und", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}