{"textgrid.poem.31407": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist die Heilig Schrifft ein Himmel Thau der Seelen;", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist die Heilig Schrifft ein Himmel Thau der Seelen;", "tokens": ["Es", "ist", "die", "Hei\u00b7lig", "Schrifft", "ein", "Him\u00b7mel", "Thau", "der", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie k\u00fchlt / sie labt / sie frischt / die Geistverschmachtet Erd;", "tokens": ["Sie", "k\u00fchlt", "/", "sie", "labt", "/", "sie", "frischt", "/", "die", "Geist\u00b7ver\u00b7schmach\u00b7tet", "Erd", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "ADJD", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ietz gr\u00fcnt sie / bl\u00fcht und lacht / ohn' einige Beschwerd /", "tokens": ["ietz", "gr\u00fcnt", "sie", "/", "bl\u00fcht", "und", "lacht", "/", "ohn'", "ei\u00b7ni\u00b7ge", "Be\u00b7schwerd", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VVFIN", "KON", "VVFIN", "$(", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als dieser Perlen Safft ihr lieblich k\u00fchlt die K\u00e4hlen.", "tokens": ["als", "die\u00b7ser", "Per\u00b7len", "Safft", "ihr", "lieb\u00b7lich", "k\u00fchlt", "die", "K\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "PPOSAT", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O Herr / wer wolte nicht di\u00df edle Labsal wehlen /", "tokens": ["O", "Herr", "/", "wer", "wol\u00b7te", "nicht", "di\u00df", "ed\u00b7le", "Lab\u00b7sal", "weh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PWS", "VMFIN", "PTKNEG", "PDS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und wollen / da\u00df er st\u00e4ts damit getr\u00e4nket werd?", "tokens": ["und", "wol\u00b7len", "/", "da\u00df", "er", "st\u00e4ts", "da\u00b7mit", "ge\u00b7tr\u00e4n\u00b7ket", "werd", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "KOUS", "PPER", "VVFIN", "PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach mit was Freudigkeit besieg' ich die Gef\u00e4rd'!", "tokens": ["Ach", "mit", "was", "Freu\u00b7dig\u00b7keit", "be\u00b7sieg'", "ich", "die", "Ge\u00b7f\u00e4rd'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRELS", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "auch alles Leid und Streit / kan ich zu frieden zehlen.", "tokens": ["auch", "al\u00b7les", "Leid", "und", "Streit", "/", "kan", "ich", "zu", "frie\u00b7den", "zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$(", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O s\u00fcsses Nectar Trank! la\u00df mich dein brunn-Chor seyn /", "tokens": ["O", "s\u00fcs\u00b7ses", "Nec\u00b7tar", "Trank", "!", "la\u00df", "mich", "dein", "brunn\u00b7Chor", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.10": {"text": "und fl\u00fcsse durch die R\u00f6hr des Geistes in mich ein:", "tokens": ["und", "fl\u00fcs\u00b7se", "durch", "die", "R\u00f6hr", "des", "Geis\u00b7tes", "in", "mich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "La\u00df' ein sch\u00f6n Wasserwerk aus diesem Brunnen springen /", "tokens": ["La\u00df'", "ein", "sch\u00f6n", "Was\u00b7ser\u00b7werk", "aus", "die\u00b7sem", "Brun\u00b7nen", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJD", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "den Garten dieser Welt zu w\u00e4ssern / da\u00df er gr\u00fcnt /", "tokens": ["den", "Gar\u00b7ten", "die\u00b7ser", "Welt", "zu", "w\u00e4s\u00b7sern", "/", "da\u00df", "er", "gr\u00fcnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PTKZU", "VVINF", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und Tugend-Blumen zeugt' / auch Himmelsfr\u00fccht m\u00f6g bringen;", "tokens": ["und", "Tu\u00b7gen\u00b7dBlu\u00b7men", "zeugt'", "/", "auch", "Him\u00b7mels\u00b7fr\u00fccht", "m\u00f6g", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$(", "ADV", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "dadurch Gott werd geehrt / dem N\u00e4chsten auch gedient.", "tokens": ["da\u00b7durch", "Gott", "werd", "ge\u00b7ehrt", "/", "dem", "N\u00e4chs\u00b7ten", "auch", "ge\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}