{"textgrid.poem.54493": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht irgend ein Prophet/ nicht der au\u00df Jordans Strande", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht irgend ein Prophet/ nicht der au\u00df Jordans Strande", "tokens": ["Nicht", "ir\u00b7gend", "ein", "Pro\u00b7phet", "/", "nicht", "der", "au\u00df", "Jor\u00b7dans", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$(", "PTKNEG", "ART", "APPR", "NE", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Wasser hat getaufft/ nicht der den Himmel schlo\u00df/", "tokens": ["Mit", "Was\u00b7ser", "hat", "ge\u00b7taufft", "/", "nicht", "der", "den", "Him\u00b7mel", "schlo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$(", "PTKNEG", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd Lichte Feuer Str\u00f6m auff seine Feinde gro\u00df", "tokens": ["Vnd", "Lich\u00b7te", "Feu\u00b7er", "Str\u00f6m", "auff", "sei\u00b7ne", "Fein\u00b7de", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "NN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Gott in Flammen hol't/ bricht deine Schwefel Bande.", "tokens": ["Den", "Gott", "in", "Flam\u00b7men", "hol't", "/", "bricht", "dei\u00b7ne", "Schwe\u00b7fel", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "De\u00df H\u00f6chsten grosser Sohn gibt selbst sich dir zu pfande/", "tokens": ["De\u00df", "H\u00f6chs\u00b7ten", "gros\u00b7ser", "Sohn", "gibt", "selbst", "sich", "dir", "zu", "pfan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADV", "PRF", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd b\u00fcrgt f\u00fcr alle Schuld. Er l\u00e4st de\u00df Vatern Scho\u00df/", "tokens": ["Vnd", "b\u00fcrgt", "f\u00fcr", "al\u00b7le", "Schuld", ".", "Er", "l\u00e4st", "de\u00df", "Va\u00b7tern", "Scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er macht durch seine Pein dich deiner Straffen lo\u00df/", "tokens": ["Er", "macht", "durch", "sei\u00b7ne", "Pein", "dich", "dei\u00b7ner", "Straf\u00b7fen", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sein Spott tilgt deine Schmach/ sein Mangel; deine Schande.", "tokens": ["Sein", "Spott", "tilgt", "dei\u00b7ne", "Schmach", "/", "sein", "Man\u00b7gel", ";", "dei\u00b7ne", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auff diesem Felsen steht/ die Kirche fest gegr\u00fcndet.", "tokens": ["Auff", "die\u00b7sem", "Fel\u00b7sen", "steht", "/", "die", "Kir\u00b7che", "fest", "ge\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$(", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die nicht der Hellen Reich/ die nicht der Teufel Macht", "tokens": ["Die", "nicht", "der", "Hel\u00b7len", "Reich", "/", "die", "nicht", "der", "Teu\u00b7fel", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ART", "NN", "NN", "$(", "PRELS", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Je \u00fcberw\u00fcnden kan/ vor der der Feinde Pracht/", "tokens": ["Je", "\u00fc\u00b7ber\u00b7w\u00fcn\u00b7den", "kan", "/", "vor", "der", "der", "Fein\u00b7de", "Pracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$(", "APPR", "ART", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "List/ Toben/ Neid vnd Sturm/ vnd Ha\u00df vnd Trotz verschwindet.", "tokens": ["List", "/", "To\u00b7ben", "/", "Neid", "vnd", "Sturm", "/", "vnd", "Ha\u00df", "vnd", "Trotz", "ver\u00b7schwin\u00b7det", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da ist der Himmel selbst. Wehn Jesus hier lo\u00df spricht/", "tokens": ["Da", "ist", "der", "Him\u00b7mel", "selbst", ".", "Wehn", "Je\u00b7sus", "hier", "lo\u00df", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$.", "NN", "NE", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wird lo\u00df/ vnd wehn er bind/ erlangt die Freyheit nicht.", "tokens": ["Wird", "lo\u00df", "/", "vnd", "wehn", "er", "bind", "/", "er\u00b7langt", "die", "Frey\u00b7heit", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKVZ", "$(", "KON", "VVFIN", "PPER", "VAFIN", "$(", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}