{"textgrid.poem.25047": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Zauber ist dein: in den Wasserfall,", "tokens": ["Ein", "Zau\u00b7ber", "ist", "dein", ":", "in", "den", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Adele, ihn hast du gelegt,", "tokens": ["A\u00b7de\u00b7le", ",", "ihn", "hast", "du", "ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df aus der Wogen st\u00fcrzendem Schwall", "tokens": ["Da\u00df", "aus", "der", "Wo\u00b7gen", "st\u00fcr\u00b7zen\u00b7dem", "Schwall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Von deiner Stimme den Wiederhall", "tokens": ["Von", "dei\u00b7ner", "Stim\u00b7me", "den", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Wind entgegen mir tr\u00e4gt.", "tokens": ["Der", "Wind", "ent\u00b7ge\u00b7gen", "mir", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Rings ahn' ich dich, in der Felsenkluft,", "tokens": ["Rings", "ahn'", "ich", "dich", ",", "in", "der", "Fel\u00b7sen\u00b7kluft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "APPR", "ART", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Auf den sonnigen Halden am Meer;", "tokens": ["Auf", "den", "son\u00b7ni\u00b7gen", "Hal\u00b7den", "am", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Dein Odem, vermengt mit der Myrten Duft,", "tokens": ["Dein", "O\u00b7dem", ",", "ver\u00b7mengt", "mit", "der", "Myr\u00b7ten", "Duft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Umweht im Hauche der Sommerluft", "tokens": ["Um\u00b7weht", "im", "Hau\u00b7che", "der", "Som\u00b7mer\u00b7luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Stirne mir wonneschwer.", "tokens": ["Die", "Stir\u00b7ne", "mir", "won\u00b7ne\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die pl\u00e4tschernden Wellen am Ufersaum", "tokens": ["Die", "pl\u00e4t\u00b7schern\u00b7den", "Wel\u00b7len", "am", "U\u00b7fer\u00b7saum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Im d\u00e4mmernden Mondenschein,", "tokens": ["Im", "d\u00e4m\u00b7mern\u00b7den", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Bl\u00e4tter des Waldes, die h\u00f6rbar kaum", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "des", "Wal\u00b7des", ",", "die", "h\u00f6r\u00b7bar", "kaum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sich regen im mittern\u00e4chtlichen Traum,", "tokens": ["Sich", "re\u00b7gen", "im", "mit\u00b7ter\u00b7n\u00e4cht\u00b7li\u00b7chen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie sprechen von dir allein.", "tokens": ["Sie", "spre\u00b7chen", "von", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}