{"textgrid.poem.67013": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie hofften schon, deine Glut sei verdampft,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hofften schon, deine Glut sei verdampft,", "tokens": ["Sie", "hoff\u00b7ten", "schon", ",", "dei\u00b7ne", "Glut", "sei", "ver\u00b7dampft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und h\u00e4tten am liebsten dich eingestampft.", "tokens": ["Und", "h\u00e4t\u00b7ten", "am", "liebs\u00b7ten", "dich", "ein\u00b7ge\u00b7stampft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKA", "ADJD", "PPER", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich tat ihnen leider nicht den Gefallen", "tokens": ["Ich", "tat", "ih\u00b7nen", "lei\u00b7der", "nicht", "den", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und habe gottlob noch Klauen und Krallen.", "tokens": ["Und", "ha\u00b7be", "gott\u00b7lob", "noch", "Klau\u00b7en", "und", "Kral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}