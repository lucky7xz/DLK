{"dta.poem.2047": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Fuchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch der Fuchs dient uns zum Nutzen, ob man gleich,", "tokens": ["Auch", "der", "Fuchs", "dient", "uns", "zum", "Nut\u00b7zen", ",", "ob", "man", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PIS", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "so lang' er lebet,", "tokens": ["so", "lang'", "er", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn er G\u00e4ns\u2019 und H\u00fcner raubet, nicht viel Guts von", "tokens": ["Wenn", "er", "G\u00e4ns'", "und", "H\u00fc\u00b7ner", "rau\u00b7bet", ",", "nicht", "viel", "Guts", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,", "PTKNEG", "PIAT", "NN", "APPR"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ihm erhebet.", "tokens": ["ihm", "er\u00b7he\u00b7bet", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Au\u00dfer, da\u00df er Feldm\u00e4us\u2019, Heimen, Maulw\u00fcrf\u2019, Fr\u00f6sch\u2019", "tokens": ["Au\u00b7\u00dfer", ",", "da\u00df", "er", "Feld\u00b7m\u00e4us'", ",", "Hei\u00b7men", ",", "Maul\u00b7w\u00fcr\u00b7f'", ",", "Fr\u00f6sch'"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "und Schnecken fri\u00dft,", "tokens": ["und", "Schne\u00b7cken", "fri\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und dadurch doch auch dem Menschen noch in etwas n\u00fctz-", "tokens": ["Und", "da\u00b7durch", "doch", "auch", "dem", "Men\u00b7schen", "noch", "in", "et\u00b7was", "n\u00fctz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ADV", "ADV", "ART", "NN", "ADV", "APPR", "PIAT", "TRUNC"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "lich ist,", "tokens": ["lich", "ist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "M\u00fcssen seine B\u00e4lg hingegen f\u00fcr die K\u00e4lt\u2019 uns zu be-", "tokens": ["M\u00fcs\u00b7sen", "sei\u00b7ne", "B\u00e4lg", "hin\u00b7ge\u00b7gen", "f\u00fcr", "die", "K\u00e4lt'", "uns", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PPER", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "sch\u00fctzen,", "tokens": ["sch\u00fct\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Wozu sie besonders dienen, uns im Frost besonders", "tokens": ["Wo\u00b7zu", "sie", "be\u00b7son\u00b7ders", "die\u00b7nen", ",", "uns", "im", "Frost", "be\u00b7son\u00b7ders"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "$,", "PPER", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "n\u00fctzen.", "tokens": ["n\u00fct\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Man bewundert, und mit Recht, dieses Thieres schlaue", "tokens": ["Man", "be\u00b7wun\u00b7dert", ",", "und", "mit", "Recht", ",", "die\u00b7ses", "Thie\u00b7res", "schlau\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVPP", "$,", "KON", "APPR", "NN", "$,", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "List,", "tokens": ["List", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Womit, auf besondre Weise, die Natur es ausge-", "tokens": ["Wo\u00b7mit", ",", "auf", "be\u00b7sond\u00b7re", "Wei\u00b7se", ",", "die", "Na\u00b7tur", "es", "aus\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "r\u00fcst't,", "tokens": ["r\u00fcst't", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Wie den J\u00e4gern wohl bekannt. Seine H\u00f6len, Bau", "tokens": ["Wie", "den", "J\u00e4\u00b7gern", "wohl", "be\u00b7kannt", ".", "Sei\u00b7ne", "H\u00f6\u00b7len", ",", "Bau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$.", "PPOSAT", "NN", "$,", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "und R\u00f6hren", "tokens": ["und", "R\u00f6h\u00b7ren"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Dringt er meist den D\u00e4chsen ab, bald mit List, bald", "tokens": ["Dringt", "er", "meist", "den", "D\u00e4ch\u00b7sen", "ab", ",", "bald", "mit", "List", ",", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "APPR", "NN", "$,", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "mit Gewalt.", "tokens": ["mit", "Ge\u00b7walt", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Wenn der Dachs ihm ja zu stark, soll er seinen Aufent-", "tokens": ["Wenn", "der", "Dachs", "ihm", "ja", "zu", "stark", ",", "soll", "er", "sei\u00b7nen", "Auf\u00b7ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "PTKA", "ADJD", "$,", "VMFIN", "PPER", "PPOSAT", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "halt", "tokens": ["halt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Mit der Losung ganz verderben, durch Gestank ihn so", "tokens": ["Mit", "der", "Lo\u00b7sung", "ganz", "ver\u00b7der\u00b7ben", ",", "durch", "Ge\u00b7stank", "ihn", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,", "APPR", "NN", "PPER", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.24": {"text": "versehren,", "tokens": ["ver\u00b7seh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Da\u00df er ihm die Wohnung l\u00e4\u00dft. Schlau weis sich der", "tokens": ["Da\u00df", "er", "ihm", "die", "Woh\u00b7nung", "l\u00e4\u00dft", ".", "Schlau", "weis", "sich", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$.", "NN", "PTKVZ", "PRF", "ART"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Fuchs zu n\u00e4hren,", "tokens": ["Fuchs", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Und, wenn er gejaget wird, schlau zu fliehn, auch sich zu", "tokens": ["Und", ",", "wenn", "er", "ge\u00b7ja\u00b7get", "wird", ",", "schlau", "zu", "fliehn", ",", "auch", "sich", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,", "ADJD", "PTKZU", "VVINF", "$,", "ADV", "PRF", "APPR"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "wehren.", "tokens": ["weh\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}