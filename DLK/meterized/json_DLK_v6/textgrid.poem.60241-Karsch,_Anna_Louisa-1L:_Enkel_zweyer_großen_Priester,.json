{"textgrid.poem.60241": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Enkel zweyer gro\u00dfen Priester,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Enkel zweyer gro\u00dfen Priester,", "tokens": ["En\u00b7kel", "zwey\u00b7er", "gro\u00b7\u00dfen", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so fromm als heiter sind,", "tokens": ["Die", "so", "fromm", "als", "hei\u00b7ter", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KOKOM", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie sey deine Stirne d\u00fcster,", "tokens": ["Nie", "sey", "dei\u00b7ne", "Stir\u00b7ne", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weine nicht, du s\u00fc\u00dfes Kind.", "tokens": ["Wei\u00b7ne", "nicht", ",", "du", "s\u00fc\u00b7\u00dfes", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4chle deiner Mutter Busen", "tokens": ["L\u00e4ch\u00b7le", "dei\u00b7ner", "Mut\u00b7ter", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dankbar und zufrieden an;", "tokens": ["Dank\u00b7bar", "und", "zu\u00b7frie\u00b7den", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcnftig tr\u00e4nken dich die Musen,", "tokens": ["K\u00fcnf\u00b7tig", "tr\u00e4n\u00b7ken", "dich", "die", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn dein Geist erst saugen kann.", "tokens": ["Wenn", "dein", "Geist", "erst", "sau\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00fcnftig wirst du, holder Knabe,", "tokens": ["K\u00fcnf\u00b7tig", "wirst", "du", ",", "hol\u00b7der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durstig nach der Weisheit seyn;", "tokens": ["Durs\u00b7tig", "nach", "der", "Weis\u00b7heit", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein Vater hat die Gabe,", "tokens": ["Und", "dein", "Va\u00b7ter", "hat", "die", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundlich fl\u00f6\u00dft er sie dir ein.", "tokens": ["Freund\u00b7lich", "fl\u00f6\u00dft", "er", "sie", "dir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn du deinen Namen h\u00f6ren", "tokens": ["Wenn", "du", "dei\u00b7nen", "Na\u00b7men", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den seinen lallen kannst,", "tokens": ["Und", "den", "sei\u00b7nen", "lal\u00b7len", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird er dich schon sanft belehren,", "tokens": ["Wird", "er", "dich", "schon", "sanft", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du nicht umsonst begannst.", "tokens": ["Da\u00df", "du", "nicht", "um\u00b7sonst", "be\u00b7gannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wirst du gr\u00f6\u00dfer, kleines Wesen,", "tokens": ["Wirst", "du", "gr\u00f6\u00b7\u00dfer", ",", "klei\u00b7nes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann enth\u00fcllt sich dein Verstand;", "tokens": ["Dann", "ent\u00b7h\u00fcllt", "sich", "dein", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann wirst du ein B\u00fcchlein lesen,", "tokens": ["Dann", "wirst", "du", "ein", "B\u00fcch\u00b7lein", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Bestimmung wird genannt.", "tokens": ["Das", "Be\u00b7stim\u00b7mung", "wird", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O! da lernst du, nimmer m\u00fcde,", "tokens": ["O", "!", "da", "lernst", "du", ",", "nim\u00b7mer", "m\u00fc\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tugendhaft und gl\u00fccklich seyn;", "tokens": ["Tu\u00b7gend\u00b7haft", "und", "gl\u00fcck\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber jetzt schlaf bey dem Liede", "tokens": ["A\u00b7ber", "jetzt", "schlaf", "bey", "dem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Deiner guten Freundinn ein.", "tokens": ["Dei\u00b7ner", "gu\u00b7ten", "Freun\u00b7dinn", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}