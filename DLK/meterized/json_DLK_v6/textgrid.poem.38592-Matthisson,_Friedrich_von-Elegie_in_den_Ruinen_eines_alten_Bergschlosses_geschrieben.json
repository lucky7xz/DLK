{"textgrid.poem.38592": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Elegie in den Ruinen eines alten Bergschlosses geschrieben", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigend in der Abendd\u00e4mmrung Schleier,", "tokens": ["Schwei\u00b7gend", "in", "der", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ruht die Flur, das Lied der Haine stirbt,", "tokens": ["Ruht", "die", "Flur", ",", "das", "Lied", "der", "Hai\u00b7ne", "stirbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur da\u00df hier, im alternden Gem\u00e4uer,", "tokens": ["Nur", "da\u00df", "hier", ",", "im", "al\u00b7tern\u00b7den", "Ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Melancholisch noch ein Heimchen zirpt.", "tokens": ["Me\u00b7lan\u00b7cho\u00b7lisch", "noch", "ein", "Heim\u00b7chen", "zirpt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Stille sinkt aus unbew\u00f6lkten L\u00fcften,", "tokens": ["Stil\u00b7le", "sinkt", "aus", "un\u00b7be\u00b7w\u00f6lk\u00b7ten", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Langsam ziehn die Heerden von den Triften,", "tokens": ["Lang\u00b7sam", "ziehn", "die", "Heer\u00b7den", "von", "den", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und der m\u00fcde Landmann eilt der Ruh", "tokens": ["Und", "der", "m\u00fc\u00b7de", "Land\u00b7mann", "eilt", "der", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Seiner v\u00e4terlichen H\u00fctte zu.", "tokens": ["Sei\u00b7ner", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "H\u00fct\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Hier, auf diesen waldumkr\u00e4nzten H\u00f6hen,", "tokens": ["Hier", ",", "auf", "die\u00b7sen", "wald\u00b7um\u00b7kr\u00e4nz\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unter Tr\u00fcmmern der Vergangenheit,", "tokens": ["Un\u00b7ter", "Tr\u00fcm\u00b7mern", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo der Vorwelt Schauer mich umwehen,", "tokens": ["Wo", "der", "Vor\u00b7welt", "Schau\u00b7er", "mich", "um\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sei dies Lied, o Wehmuth, dir geweiht!", "tokens": ["Sei", "dies", "Lied", ",", "o", "Weh\u00b7muth", ",", "dir", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "$,", "FM", "NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Traurend denk' ich, was vor grauen Jahren", "tokens": ["Trau\u00b7rend", "denk'", "ich", ",", "was", "vor", "grau\u00b7en", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Diese morschen Ueberreste waren;", "tokens": ["Die\u00b7se", "mor\u00b7schen", "Ue\u00b7ber\u00b7res\u00b7te", "wa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ein beth\u00fcrmtes Schlo\u00df, voll Majest\u00e4t", "tokens": ["Ein", "bet\u00b7h\u00fcrm\u00b7tes", "Schlo\u00df", ",", "voll", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Auf des Berges Felsenstirn erh\u00f6ht!", "tokens": ["Auf", "des", "Ber\u00b7ges", "Fel\u00b7sen\u00b7stirn", "er\u00b7h\u00f6ht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dort, wo um des Pfeilers dunkle Tr\u00fcmmer", "tokens": ["Dort", ",", "wo", "um", "des", "Pfei\u00b7lers", "dunk\u00b7le", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Traurigfl\u00fcsternd sich der Epheu schlingt,", "tokens": ["Trau\u00b7rig\u00b7fl\u00fcs\u00b7ternd", "sich", "der", "E\u00b7pheu", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Abendr\u00f6the tr\u00fcber Schimmer", "tokens": ["Und", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", "tr\u00fc\u00b7ber", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch den \u00f6den Raum der Fenster blinkt,", "tokens": ["Durch", "den", "\u00f6\u00b7den", "Raum", "der", "Fens\u00b7ter", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Segneten vielleicht des Vaters Thr\u00e4nen", "tokens": ["Seg\u00b7ne\u00b7ten", "viel\u00b7leicht", "des", "Va\u00b7ters", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Einst den Edelsten von Deutschlands S\u00f6hnen,", "tokens": ["Einst", "den", "E\u00b7dels\u00b7ten", "von", "Deutschlands", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Dessen Herz der Ehrbegierde voll,", "tokens": ["Des\u00b7sen", "Herz", "der", "Ehr\u00b7be\u00b7gier\u00b7de", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Hei\u00df dem nahen Kampf entgegen schwoll.", "tokens": ["Hei\u00df", "dem", "na\u00b7hen", "Kampf", "ent\u00b7ge\u00b7gen", "schwoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPO", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Zeuch in Frieden, sprach der greise Krieger,", "tokens": ["Zeuch", "in", "Frie\u00b7den", ",", "sprach", "der", "grei\u00b7se", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihn umg\u00fcrtend mit dem Heldenschwert,", "tokens": ["Ihn", "um\u00b7g\u00fcr\u00b7tend", "mit", "dem", "Hel\u00b7den\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kehre nimmer, oder kehr' als Sieger,", "tokens": ["Keh\u00b7re", "nim\u00b7mer", ",", "o\u00b7der", "kehr'", "als", "Sie\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "VVFIN", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sei des Namens deiner V\u00e4ter werth!", "tokens": ["Sei", "des", "Na\u00b7mens", "dei\u00b7ner", "V\u00e4\u00b7ter", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und des edlen J\u00fcnglings Auge spr\u00fchte", "tokens": ["Und", "des", "ed\u00b7len", "J\u00fcng\u00b7lings", "Au\u00b7ge", "spr\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Todesflammen, seine Wange gl\u00fchte,", "tokens": ["To\u00b7des\u00b7flam\u00b7men", ",", "sei\u00b7ne", "Wan\u00b7ge", "gl\u00fch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Gleich dem aufgebl\u00fchten Rosenhain", "tokens": ["Gleich", "dem", "auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Ro\u00b7sen\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "In der Morgenr\u00f6the Purpurschein.", "tokens": ["In", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Pur\u00b7pur\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wild, wie Meere toben, flog der Ritter", "tokens": ["Wild", ",", "wie", "Mee\u00b7re", "to\u00b7ben", ",", "flog", "der", "Rit\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dann mit frohem Ungest\u00fcm zur Schlacht,", "tokens": ["Dann", "mit", "fro\u00b7hem", "Un\u00b7ge\u00b7st\u00fcm", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie der Tannenwald im Sturmgewitter,", "tokens": ["Wie", "der", "Tan\u00b7nen\u00b7wald", "im", "Sturm\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Beugte sich vor ihm des Feindes Macht!", "tokens": ["Beug\u00b7te", "sich", "vor", "ihm", "des", "Fein\u00b7des", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Mild, wie B\u00e4che, die durch Blumen wallen,", "tokens": ["Mild", ",", "wie", "B\u00e4\u00b7che", ",", "die", "durch", "Blu\u00b7men", "wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kehrt er zu des Felsenschlosses Hallen,", "tokens": ["Kehrt", "er", "zu", "des", "Fel\u00b7sen\u00b7schlos\u00b7ses", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Zu des Vaters Freudenthr\u00e4nenblick,", "tokens": ["Zu", "des", "Va\u00b7ters", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "In des keuschen M\u00e4dchens Arm zur\u00fcck.", "tokens": ["In", "des", "keu\u00b7schen", "M\u00e4d\u00b7chens", "Arm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ach! mit banger Sehnsucht blickt die Holde", "tokens": ["Ach", "!", "mit", "ban\u00b7ger", "Sehn\u00b7sucht", "blickt", "die", "Hol\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oft vom S\u00f6ller nach des Thales Pfad;", "tokens": ["Oft", "vom", "S\u00f6l\u00b7ler", "nach", "des", "Tha\u00b7les", "Pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schild' und Panzer gl\u00fchn im Abendgolde,", "tokens": ["Schild'", "und", "Pan\u00b7zer", "gl\u00fchn", "im", "A\u00b7bend\u00b7gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rosse fliegen! der Geliebte naht!", "tokens": ["Ros\u00b7se", "flie\u00b7gen", "!", "der", "Ge\u00b7lieb\u00b7te", "naht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sprachlos nun die treue Hand ihm reichend,", "tokens": ["Sprach\u00b7los", "nun", "die", "treu\u00b7e", "Hand", "ihm", "rei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Steht sie da, err\u00f6thend und erbleichend,", "tokens": ["Steht", "sie", "da", ",", "er\u00b7r\u00f6\u00b7thend", "und", "er\u00b7blei\u00b7chend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Aber was ihr sanftes Auge spricht,", "tokens": ["A\u00b7ber", "was", "ihr", "sanf\u00b7tes", "Au\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "S\u00e4nge selbst dein Mund, o Liebe, nicht!", "tokens": ["S\u00e4n\u00b7ge", "selbst", "dein", "Mund", ",", "o", "Lie\u00b7be", ",", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Laut erscholl im hochgew\u00f6lbten Saale,", "tokens": ["Laut", "er\u00b7scholl", "im", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dort wo aus dem Schutt die S\u00e4ule ragt,", "tokens": ["Dort", "wo", "aus", "dem", "Schutt", "die", "S\u00e4u\u00b7le", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann der Klang der m\u00e4chtigen Pokale,", "tokens": ["Dann", "der", "Klang", "der", "m\u00e4ch\u00b7ti\u00b7gen", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", 