{"dta.poem.18263": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Suum cuique!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Doch die Dichteritis ist heut epidemisch.", "tokens": ["Doch", "die", "Dich\u00b7te\u00b7ri\u00b7tis", "ist", "heut", "e\u00b7pi\u00b7de\u00b7misch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und kann ich ihr nicht das Maul verriegeln,", "tokens": ["Und", "kann", "ich", "ihr", "nicht", "das", "Maul", "ver\u00b7rie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "So will ich ihr doch den Hintern striegeln!", "tokens": ["So", "will", "ich", "ihr", "doch", "den", "Hin\u00b7tern", "strie\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}