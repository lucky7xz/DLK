{"textgrid.poem.62826": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Schaam, der Tugend Jungemagd,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schaam, der Tugend Jungemagd,", "tokens": ["Die", "Schaam", ",", "der", "Tu\u00b7gend", "Jun\u00b7ge\u00b7magd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der beredten Rosenstirne", "tokens": ["Mit", "der", "be\u00b7red\u00b7ten", "Ro\u00b7sens\u00b7tir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ward j\u00fcngst von einer eiteln Dirne", "tokens": ["Ward", "j\u00fcngst", "von", "ei\u00b7ner", "ei\u00b7teln", "Dir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihrem Putzgemach gejagt.", "tokens": ["Aus", "ih\u00b7rem", "Putz\u00b7ge\u00b7mach", "ge\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie floh mit ungewissem Schritte", "tokens": ["Sie", "floh", "mit", "un\u00b7ge\u00b7wis\u00b7sem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach irgend einer Sch\u00e4ferh\u00fctte:", "tokens": ["Nach", "ir\u00b7gend", "ei\u00b7ner", "Sch\u00e4\u00b7fer\u00b7h\u00fct\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es war ein hei\u00dfer Sommertag.", "tokens": ["Es", "war", "ein", "hei\u00b7\u00dfer", "Som\u00b7mer\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nachdem sie ba\u00df geschwitzet hatte,", "tokens": ["Nach\u00b7dem", "sie", "ba\u00df", "ge\u00b7schwit\u00b7zet", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Fand sie auf einer bunten Matte", "tokens": ["Fand", "sie", "auf", "ei\u00b7ner", "bun\u00b7ten", "Mat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Amor, der im Schatten lag", "tokens": ["Den", "A\u00b7mor", ",", "der", "im", "Schat\u00b7ten", "lag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und froh mit einem F\u00e4cher spielte.", "tokens": ["Und", "froh", "mit", "ei\u00b7nem", "F\u00e4\u00b7cher", "spiel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie war ersch\u00f6pft. Der lose Fant,", "tokens": ["Sie", "war", "er\u00b7sch\u00f6pft", ".", "Der", "lo\u00b7se", "Fant", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht stets ihr Freund, doch stets galant,", "tokens": ["Nicht", "stets", "ihr", "Freund", ",", "doch", "stets", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Trat l\u00e4chelnd zu ihr hin und k\u00fchlte", "tokens": ["Trat", "l\u00e4\u00b7chelnd", "zu", "ihr", "hin", "und", "k\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit seinem Schirm ihr Angesicht.", "tokens": ["Mit", "sei\u00b7nem", "Schirm", "ihr", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sonst floh sie ihn, jetzt floh sie nicht", "tokens": ["Sonst", "floh", "sie", "ihn", ",", "jetzt", "floh", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und duldete mit holden Mienen", "tokens": ["Und", "dul\u00b7de\u00b7te", "mit", "hol\u00b7den", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Den Liebesdienst: O! schenke mir", "tokens": ["Den", "Lie\u00b7bes\u00b7dienst", ":", "O", "!", "schen\u00b7ke", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NE", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den Schirm, sprach sie, was n\u00fczt er dir?", "tokens": ["Den", "Schirm", ",", "sprach", "sie", ",", "was", "n\u00fczt", "er", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Uns M\u00e4dchen kann er besser dienen,", "tokens": ["Uns", "M\u00e4d\u00b7chen", "kann", "er", "bes\u00b7ser", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wenn uns der J\u00fcngling Worte sagt", "tokens": ["Wenn", "uns", "der", "J\u00fcng\u00b7ling", "Wor\u00b7te", "sagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und Blicke giebt und Bitten wagt,", "tokens": ["Und", "Bli\u00b7cke", "giebt", "und", "Bit\u00b7ten", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Vor denen wir err\u00f6then m\u00fcssen.", "tokens": ["Vor", "de\u00b7nen", "wir", "er\u00b7r\u00f6\u00b7then", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da nimm ihn hin, mein sch\u00f6nes Kind,", "tokens": ["Da", "nimm", "ihn", "hin", ",", "mein", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Versetzt der Schalk, wir beyde wissen", "tokens": ["Ver\u00b7setzt", "der", "Schalk", ",", "wir", "bey\u00b7de", "wis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wie gro\u00df des F\u00e4chers Dienste sind.", "tokens": ["Wie", "gro\u00df", "des", "F\u00e4\u00b7chers", "Diens\u00b7te", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Doch etwas mu\u00df ich dir entdecken,", "tokens": ["Doch", "et\u00b7was", "mu\u00df", "ich", "dir", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Das du nicht ahnest: dann und wann", "tokens": ["Das", "du", "nicht", "ah\u00b7nest", ":", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$.", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Hilft er auch ein Gesicht verstecken,", "tokens": ["Hilft", "er", "auch", "ein", "Ge\u00b7sicht", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.30": {"text": "Das gar nicht mehr err\u00f6then kann.", "tokens": ["Das", "gar", "nicht", "mehr", "er\u00b7r\u00f6\u00b7then", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}