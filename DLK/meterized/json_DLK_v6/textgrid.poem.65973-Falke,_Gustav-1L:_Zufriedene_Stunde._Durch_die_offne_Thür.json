{"textgrid.poem.65973": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zufriedene Stunde. Durch die offne Th\u00fcr", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zufriedene Stunde. Durch die offne Th\u00fcr", "tokens": ["Zu\u00b7frie\u00b7de\u00b7ne", "Stun\u00b7de", ".", "Durch", "die", "off\u00b7ne", "Th\u00fcr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kommt vom Balkon die milde weiche Luft", "tokens": ["Kommt", "vom", "Bal\u00b7kon", "die", "mil\u00b7de", "wei\u00b7che", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Des niedergehenden Septembertages", "tokens": ["Des", "nie\u00b7der\u00b7ge\u00b7hen\u00b7den", "Sep\u00b7tem\u00b7ber\u00b7ta\u00b7ges"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und, minder mild, der L\u00e4rm der Stra\u00dfe: Kreischen", "tokens": ["Und", ",", "min\u00b7der", "mild", ",", "der", "L\u00e4rm", "der", "Stra\u00b7\u00dfe", ":", "Krei\u00b7schen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ADV", "ADJD", "$,", "ART", "NN", "ART", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Knaben, die sich balgen; helle Stimmen", "tokens": ["Von", "Kna\u00b7ben", ",", "die", "sich", "bal\u00b7gen", ";", "hel\u00b7le", "Stim\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der kleinen M\u00e4dchen, Ringelreihe tanzend;", "tokens": ["Der", "klei\u00b7nen", "M\u00e4d\u00b7chen", ",", "Rin\u00b7gel\u00b7rei\u00b7he", "tan\u00b7zend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das scharfe Kleffen meines Nachbarh\u00fcndchens", "tokens": ["Das", "schar\u00b7fe", "Klef\u00b7fen", "mei\u00b7nes", "Nach\u00b7bar\u00b7h\u00fcnd\u00b7chens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und dann und wann der tiefe Polterbass", "tokens": ["Und", "dann", "und", "wann", "der", "tie\u00b7fe", "Pol\u00b7ter\u00b7bass"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Des Milchmannshundes. Auch das L\u00e4uten tr\u00e4gt", "tokens": ["Des", "Milch\u00b7manns\u00b7hun\u00b7des", ".", "Auch", "das", "L\u00e4u\u00b7ten", "tr\u00e4gt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Pferdebahn zu mir der schnelle Schall,", "tokens": ["Der", "Pfer\u00b7de\u00b7bahn", "zu", "mir", "der", "schnel\u00b7le", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und, dumpfer, von der nahen Alster her", "tokens": ["Und", ",", "dum\u00b7pfer", ",", "von", "der", "na\u00b7hen", "Als\u00b7ter", "her"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den kl\u00e4glich heisern Ton der kleinen Dampfer.", "tokens": ["Den", "kl\u00e4g\u00b7lich", "hei\u00b7sern", "Ton", "der", "klei\u00b7nen", "Damp\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zufriedene Stunde. Auf den Knieen das Buch,", "tokens": ["Zu\u00b7frie\u00b7de\u00b7ne", "Stun\u00b7de", ".", "Auf", "den", "Kni\u00b7e\u00b7en", "das", "Buch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbjenseits von Gut und B\u00f6se\u00ab nennt der Vater", "tokens": ["\u00bb", "jen\u00b7seits", "von", "Gut", "und", "B\u00f6\u00b7se", "\u00ab", "nennt", "der", "Va\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "APPR", "NN", "KON", "NN", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein wundersames Kind der Einsamkeit,", "tokens": ["Sein", "wun\u00b7der\u00b7sa\u00b7mes", "Kind", "der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So auf den Knien das aufgeschlagene Buch,", "tokens": ["So", "auf", "den", "Kni\u00b7en", "das", "auf\u00b7ge\u00b7schla\u00b7ge\u00b7ne", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Lass' ich den wirren Klang des Lebens l\u00e4chelnd", "tokens": ["Lass'", "ich", "den", "wir\u00b7ren", "Klang", "des", "Le\u00b7bens", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die zarten sch\u00fcchternen Gedanken mir", "tokens": ["Die", "zar\u00b7ten", "sch\u00fcch\u00b7ter\u00b7nen", "Ge\u00b7dan\u00b7ken", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zur\u00fcck ins dunkle Nest der Seele scheuchen.", "tokens": ["Zu\u00b7r\u00fcck", "ins", "dunk\u00b7le", "Nest", "der", "See\u00b7le", "scheu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zufriedene Stunde. War ich je so fr\u00f6hlich,", "tokens": ["Zu\u00b7frie\u00b7de\u00b7ne", "Stun\u00b7de", ".", "War", "ich", "je", "so", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So herzensstill, so g\u00fctig? Oftmals schon", "tokens": ["So", "her\u00b7zens\u00b7still", ",", "so", "g\u00fc\u00b7tig", "?", "Oft\u00b7mals", "schon"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADJD", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schlug ich die Th\u00fcr mit leisem Fluche zu,", "tokens": ["Schlug", "ich", "die", "Th\u00fcr", "mit", "lei\u00b7sem", "Flu\u00b7che", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn so von drau\u00dfen mit der plumpen Faust", "tokens": ["Wenn", "so", "von", "drau\u00b7\u00dfen", "mit", "der", "plum\u00b7pen", "Faust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der w\u00fcste, rohe L\u00e4rm des Tages griff", "tokens": ["Der", "w\u00fcs\u00b7te", ",", "ro\u00b7he", "L\u00e4rm", "des", "Ta\u00b7ges", "griff"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In meine zarten feinen Seelenf\u00e4den,", "tokens": ["In", "mei\u00b7ne", "zar\u00b7ten", "fei\u00b7nen", "See\u00b7len\u00b7f\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das kaum begonnene Gespinst zerst\u00f6rend.", "tokens": ["Das", "kaum", "be\u00b7gon\u00b7ne\u00b7ne", "Ge\u00b7spinst", "zer\u00b7st\u00f6\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch heute kann ich's l\u00e4chelnd dulden. Seltsam.", "tokens": ["Doch", "heu\u00b7te", "kann", "ich's", "l\u00e4\u00b7chelnd", "dul\u00b7den", ".", "Selt\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "ADJD", "VVINF", "$.", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zufriedene Stunde. Ohn' warum, wozu.", "tokens": ["Zu\u00b7frie\u00b7de\u00b7ne", "Stun\u00b7de", ".", "Ohn'", "wa\u00b7rum", ",", "wo\u00b7zu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "FM.la", "FM.la", "$,", "PWAV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du dreimal Gl\u00fccklicher, dem jeder Tag", "tokens": ["Du", "drei\u00b7mal", "Gl\u00fcck\u00b7li\u00b7cher", ",", "dem", "je\u00b7der", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bringt solche Stunde, solche Stunden wohl.", "tokens": ["Bringt", "sol\u00b7che", "Stun\u00b7de", ",", "sol\u00b7che", "Stun\u00b7den", "wohl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und giebt's nicht Gl\u00fcckliche, die immer so,", "tokens": ["Und", "giebt's", "nicht", "Gl\u00fcck\u00b7li\u00b7che", ",", "die", "im\u00b7mer", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So fraglos, leben hin ihr ganzes Leben?", "tokens": ["So", "frag\u00b7los", ",", "le\u00b7ben", "hin", "ihr", "gan\u00b7zes", "Le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein wirrer Ton, ein unbestimmter Klang", "tokens": ["Ein", "wir\u00b7rer", "Ton", ",", "ein", "un\u00b7be\u00b7stimm\u00b7ter", "Klang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In all den wirren, unbestimmten Kl\u00e4ngen", "tokens": ["In", "all", "den", "wir\u00b7ren", ",", "un\u00b7be\u00b7stimm\u00b7ten", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der wundersamen Lebenssymphonie,", "tokens": ["Der", "wun\u00b7der\u00b7sa\u00b7men", "Le\u00b7bens\u00b7symp\u00b7ho\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "F\u00fcllstimmen nur im wuchtig lauten Tutti.", "tokens": ["F\u00fcll\u00b7stim\u00b7men", "nur", "im", "wuch\u00b7tig", "lau\u00b7ten", "Tut\u00b7ti", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zufriedene Stunde. Oder nicht? Ist Schlaf", "tokens": ["Zu\u00b7frie\u00b7de\u00b7ne", "Stun\u00b7de", ".", "O\u00b7der", "nicht", "?", "Ist", "Schlaf"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "NE", "PTKNEG", "$.", "VAFIN", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur diese Stille, diese satte Stimmung,", "tokens": ["Nur", "die\u00b7se", "Stil\u00b7le", ",", "die\u00b7se", "sat\u00b7te", "Stim\u00b7mung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wunsch- und fragelose? Wie? Nicht Gl\u00fcck?", "tokens": ["Die", "wunsch", "und", "fra\u00b7ge\u00b7lo\u00b7se", "?", "Wie", "?", "Nicht", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "VVFIN", "$.", "PWAV", "$.", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht Gl\u00fcck f\u00fcr mich? Wenn sich dem wirren L\u00e4rm", "tokens": ["Nicht", "Gl\u00fcck", "f\u00fcr", "mich", "?", "Wenn", "sich", "dem", "wir\u00b7ren", "L\u00e4rm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "APPR", "PPER", "$.", "KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun hell und klar, wie rieselnd Gold, entringen", "tokens": ["Nun", "hell", "und", "klar", ",", "wie", "rie\u00b7selnd", "Gold", ",", "ent\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "ADJD", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die zauberhaften Solostimmen wieder,", "tokens": ["Die", "zau\u00b7ber\u00b7haf\u00b7ten", "So\u00b7lost\u00b7im\u00b7men", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die feinen kirrenden Zauberfl\u00f6tent\u00f6ne?", "tokens": ["Die", "fei\u00b7nen", "kir\u00b7ren\u00b7den", "Zau\u00b7ber\u00b7fl\u00f6\u00b7ten\u00b7t\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und in dem stillen dunklen Rattennest,", "tokens": ["Und", "in", "dem", "stil\u00b7len", "dunk\u00b7len", "Rat\u00b7ten\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das meine Seele nenn' ich, wird's lebendig", "tokens": ["Das", "mei\u00b7ne", "See\u00b7le", "nenn'", "ich", ",", "wird's", "le\u00b7ben\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und l\u00e4uft und springt und dr\u00e4ngt und pfaucht und pfeift?", "tokens": ["Und", "l\u00e4uft", "und", "springt", "und", "dr\u00e4ngt", "und", "pfaucht", "und", "pfeift", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nein! tutti tutti! forte! con fuoco!", "tokens": ["Nein", "!", "tut\u00b7ti", "tut\u00b7ti", "!", "for\u00b7te", "!", "con", "fuo\u00b7co", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "FM", "FM", "$.", "VVFIN", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Recht brausend, l\u00e4rmend, alles \u00fcbert\u00e4ubend!", "tokens": ["Recht", "brau\u00b7send", ",", "l\u00e4r\u00b7mend", ",", "