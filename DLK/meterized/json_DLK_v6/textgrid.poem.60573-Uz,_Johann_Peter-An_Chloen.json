{"textgrid.poem.60573": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Chloen", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dfst, wie lange schon", "tokens": ["Du", "wei\u00dfst", ",", "wie", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Cytherens holder Sohn,", "tokens": ["Cy\u00b7the\u00b7rens", "hol\u00b7der", "Sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "O Chloe! bey mir ist", "tokens": ["O", "Chloe", "!", "bey", "mir", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$.", "APPR", "PPER", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und mir mein Leid vers\u00fc\u00dft.", "tokens": ["Und", "mir", "mein", "Leid", "ver\u00b7s\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sang meine Muse doch", "tokens": ["Sang", "mei\u00b7ne", "Mu\u00b7se", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So ziemlich artig noch.", "tokens": ["So", "ziem\u00b7lich", "ar\u00b7tig", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Oft hielt ihn schon im Lauf", "tokens": ["Oft", "hielt", "ihn", "schon", "im", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ihr schmeichlend Liedgen auf.", "tokens": ["Ihr", "schmeich\u00b7lend", "Lied\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oft lockte wohl dein Blick", "tokens": ["Oft", "lock\u00b7te", "wohl", "dein", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Liebkosend ihn zur\u00fcck.", "tokens": ["Lieb\u00b7ko\u00b7send", "ihn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun lockest du nicht mehr", "tokens": ["Nun", "lo\u00b7ckest", "du", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und z\u00fcrnst, wer wei\u00df wie sehr.", "tokens": ["Und", "z\u00fcrnst", ",", "wer", "wei\u00df", "wie", "sehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "VVFIN", "KOKOM", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum schweigt mein Saitenspiel,", "tokens": ["Drum", "schweigt", "mein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das Amorn sonst gefiel;", "tokens": ["Das", "A\u00b7morn", "sonst", "ge\u00b7fiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Amor will entfliehn,", "tokens": ["Und", "A\u00b7mor", "will", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und nichts bes\u00e4nftigt ihn.", "tokens": ["Und", "nichts", "be\u00b7s\u00e4nf\u00b7tigt", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Halt, wenn er mich verl\u00e4\u00dft,", "tokens": ["Halt", ",", "wenn", "er", "mich", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du deinen Sclaven fest:", "tokens": ["Du", "dei\u00b7nen", "Scla\u00b7ven", "fest", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil dirs ein leichtes ist,", "tokens": ["Weil", "dirs", "ein", "leich\u00b7tes", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und du ihm Venus bist.", "tokens": ["Und", "du", "ihm", "Ve\u00b7nus", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bew\u00f6lke nur, mein Licht!", "tokens": ["Be\u00b7w\u00f6l\u00b7ke", "nur", ",", "mein", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die blauen Augen nicht.", "tokens": ["Die", "blau\u00b7en", "Au\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein holder Blick von dir", "tokens": ["Ein", "hol\u00b7der", "Blick", "von", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Vers\u00f6hnet ihn mit mir.", "tokens": ["Ver\u00b7s\u00f6h\u00b7net", "ihn", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}