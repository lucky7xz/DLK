{"dta.poem.6031": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So dr\u00fcckt ihr Trieb sie stets in schneller Fahrt,", "tokens": ["So", "dr\u00fcckt", "ihr", "Trieb", "sie", "stets", "in", "schnel\u00b7ler", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ist ihr Lauff so dann von unterschiedner Art:", "tokens": ["Und", "ist", "ihr", "Lauff", "so", "dann", "von", "un\u00b7ter\u00b7schied\u00b7ner", "Art", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jm grossen Circkel-Creys sieht man sie fortgerissen,", "tokens": ["Jm", "gros\u00b7sen", "Cir\u00b7ckel\u00b7Creys", "sieht", "man", "sie", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da sie um ihre Ax zugleich sich drehen m\u00fcssen.", "tokens": ["Da", "sie", "um", "ih\u00b7re", "Ax", "zu\u00b7gleich", "sich", "dre\u00b7hen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}