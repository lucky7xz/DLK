{"textgrid.poem.56818": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mu\u00dft denn um mein ewig Leben", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00dft denn um mein ewig Leben", "tokens": ["Mu\u00dft", "denn", "um", "mein", "e\u00b7wig", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "immer arme Verse spinnen?", "tokens": ["im\u00b7mer", "ar\u00b7me", "Ver\u00b7se", "spin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubst du Gr\u00f6\u00dferes zu geben,", "tokens": ["Glaubst", "du", "Gr\u00f6\u00b7\u00dfe\u00b7res", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wo so Gro\u00dfes zu gewinnen?", "tokens": ["wo", "so", "Gro\u00b7\u00dfes", "zu", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df die undankbaren Musen,", "tokens": ["La\u00df", "die", "un\u00b7dank\u00b7ba\u00b7ren", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "bin ich Mutter nicht von allen?", "tokens": ["bin", "ich", "Mut\u00b7ter", "nicht", "von", "al\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKNEG", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Besser als an ihrem Busen", "tokens": ["Bes\u00b7ser", "als", "an", "ih\u00b7rem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "wirst du dir bei mir gefallen!", "tokens": ["wirst", "du", "dir", "bei", "mir", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}