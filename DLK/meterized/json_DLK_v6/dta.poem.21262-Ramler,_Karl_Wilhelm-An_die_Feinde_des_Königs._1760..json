{"dta.poem.21262": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An die Feinde des K\u00f6nigs.  \n 1760.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.42", "nl:0.28", "af:0.28"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lange schwingt die rasende\nMeg&#228;re", "tokens": ["Wie", "lan\u00b7ge", "schwingt", "die", "ra\u00b7sen\u00b7de", "Meg", "&#228;", "re"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Fackel? G\u00f6tter dieser Welt,", "tokens": ["Die", "Fa\u00b7ckel", "?", "G\u00f6t\u00b7ter", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum verfolgt ihr ihn, zu seiner eignen", "tokens": ["Wa\u00b7rum", "ver\u00b7folgt", "ihr", "ihn", ",", "zu", "sei\u00b7ner", "eig\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den unbezwungnen Held?", "tokens": ["Den", "un\u00b7be\u00b7zwung\u00b7nen", "Held", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ists m\u00f6glich? machen euch so viel", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "ma\u00b7chen", "euch", "so", "viel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit welchen ihr ihn ringen saht,", "tokens": ["Mit", "wel\u00b7chen", "ihr", "ihn", "rin\u00b7gen", "saht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So viele Kronen, die mit Blut zu kaufen", "tokens": ["So", "vie\u00b7le", "Kro\u00b7nen", ",", "die", "mit", "Blut", "zu", "kau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So manche G\u00f6tterthat,", "tokens": ["So", "man\u00b7che", "G\u00f6t\u00b7ter\u00b7that", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So manch von ihm zertretnes Un-", "tokens": ["So", "manch", "von", "ihm", "zer\u00b7tret\u00b7nes", "Un"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "APPR", "PPER", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wieder zur Vers\u00f6hnung Lust?", "tokens": ["Nicht", "wie\u00b7der", "zur", "Ver\u00b7s\u00f6h\u00b7nung", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lange loderte der Rache schwarzes", "tokens": ["So", "lan\u00b7ge", "lo\u00b7der\u00b7te", "der", "Ra\u00b7che", "schwar\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In keines Gottes Brust.", "tokens": ["In", "kei\u00b7nes", "Got\u00b7tes", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als Herkuls Arm den L\u00f6wen erst", "tokens": ["Als", "Her\u00b7kuls", "Arm", "den", "L\u00f6\u00b7wen", "erst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in Nem\u00e4ens Felsen lag,", "tokens": ["Der", "in", "Ne\u00b7m\u00e4ens", "Fel\u00b7sen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, mit der Panzerhaut bedeckt, sein", "tokens": ["Und", ",", "mit", "der", "Pan\u00b7zer\u00b7haut", "be\u00b7deckt", ",", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "VVPP", "$,", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schnell, und Schlag auf Schlag,", "tokens": ["Und", "schnell", ",", "und", "Schlag", "auf", "Schlag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Hydra, die ihn zu erm\u00fcden", "tokens": ["Der", "Hyd\u00b7ra", ",", "die", "ihn", "zu", "er\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr immer wachsend Leben nahm,", "tokens": ["Ihr", "im\u00b7mer", "wach\u00b7send", "Le\u00b7ben", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Obgleich die Fersen ihm ein kriechend", "tokens": ["Ob\u00b7gleich", "die", "Fer\u00b7sen", "ihm", "ein", "krie\u00b7chend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das gieng und wiederkam;", "tokens": ["Das", "gieng", "und", "wie\u00b7der\u00b7kam", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und dann die falsche Brut der Stym-", "tokens": ["Und", "dann", "die", "fal\u00b7sche", "Brut", "der", "Sty\u00b7m"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wild aus ehrnen Schn\u00e4beln schrien,", "tokens": ["Die", "wild", "aus", "ehr\u00b7nen", "Schn\u00e4\u00b7beln", "schri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ehrnen Klauen raubten, und den Kampf", "tokens": ["Mit", "ehr\u00b7nen", "Klau\u00b7en", "raub\u00b7ten", ",", "und", "den", "Kampf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus Sumpf und Busch zu ziehn,", "tokens": ["Aus", "Sumpf", "und", "Busch", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein Mittel traf; (denn diese zu", "tokens": ["Ein", "Mit\u00b7tel", "traf", ";(", "denn", "die\u00b7se", "zu"], "token_info": ["word", "word", "word", "emoticon", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "PDS", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War nur ein Spiel f\u00fcr Herkuls Hand;)", "tokens": ["War", "nur", "ein", "Spiel", "f\u00fcr", "Her\u00b7kuls", "Hand", ";)"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und drauf aus Thrazien die Rosse, die", "tokens": ["Und", "drauf", "aus", "Thra\u00b7zi\u00b7en", "die", "Ros\u00b7se", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PAV", "APPR", "NN", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Felder weggebrannt,", "tokens": ["Der", "Fel\u00b7der", "weg\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und flammenathmend in die H\u00fctten", "tokens": ["Und", "flam\u00b7me\u00b7nath\u00b7mend", "in", "die", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihren Schlund, das offne Grab,", "tokens": ["Und", "ih\u00b7ren", "Schlund", ",", "das", "off\u00b7ne", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Menschen f\u00fclleten, lebendig aufge-", "tokens": ["Mit", "Men\u00b7schen", "f\u00fcl\u00b7le\u00b7ten", ",", "le\u00b7ben\u00b7dig", "auf\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADJD", "TRUNC"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Dem wilde Viehe gab:", "tokens": ["Dem", "wil\u00b7de", "Vie\u00b7he", "gab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da sank der Zorn der reuerf\u00fcllten", "tokens": ["Da", "sank", "der", "Zorn", "der", "reu\u00b7er\u00b7f\u00fcll\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Juno, frey von Rachbegier,", "tokens": ["Und", "Ju\u00b7no", ",", "frey", "von", "Rach\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Brach aus: Sohn Jupiters, der Sterblichen", "tokens": ["Brach", "aus", ":", "Sohn", "Ju\u00b7pi\u00b7ters", ",", "der", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "NN", "NE", "$,", "ART", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "O! mehr ein Gott, als wir!", "tokens": ["O", "!", "mehr", "ein", "Gott", ",", "als", "wir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Geneuss, geneuss der Ruh, die dir", "tokens": ["Ge\u00b7neuss", ",", "ge\u00b7neuss", "der", "Ruh", ",", "die", "dir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit ich diess Feuer angefacht,", "tokens": ["Seit", "ich", "diess", "Feu\u00b7er", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Himmlischen, durch meine Wut", "tokens": ["Und", "al\u00b7le", "Himm\u00b7li\u00b7schen", ",", "durch", "mei\u00b7ne", "Wut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf dich entbrannt gemacht!", "tokens": ["Auf", "dich", "ent\u00b7brannt", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Geneuss der Opfer, die von beiden", "tokens": ["Ge\u00b7neuss", "der", "Op\u00b7fer", ",", "die", "von", "bei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erde, k\u00fcnftig jedermann", "tokens": ["Der", "Er\u00b7de", ",", "k\u00fcnf\u00b7tig", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir bringen wird, nicht uns! und nimm", "tokens": ["Dir", "brin\u00b7gen", "wird", ",", "nicht", "uns", "!", "und", "nimm"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "VAFIN", "$,", "PTKNEG", "PPER", "$.", "KON", "VVIMP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ersten Nektar an.", "tokens": ["Den", "ers\u00b7ten", "Nek\u00b7tar", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}