{"textgrid.poem.34343": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach eh ich dich, mein h\u00f6chstes Ziel,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach eh ich dich, mein h\u00f6chstes Ziel,", "tokens": ["Ach", "eh", "ich", "dich", ",", "mein", "h\u00f6chs\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PRF", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh ich dich fand, welch muthlo\u00df Streben,", "tokens": ["Eh", "ich", "dich", "fand", ",", "welch", "muth\u00b7lo\u00df", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "PWAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Welch regelloses Fiebernspiel,", "tokens": ["Welch", "re\u00b7gel\u00b7lo\u00b7ses", "Fie\u00b7bern\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bald der, bald der mein junges Leben", "tokens": ["Bald", "der", ",", "bald", "der", "mein", "jun\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "ADV", "ART", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit allen Freuden Preis zu geben,", "tokens": ["Mit", "al\u00b7len", "Freu\u00b7den", "Preis", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nachdem es ihrem Stolz gefiel.", "tokens": ["Nach\u00b7dem", "es", "ih\u00b7rem", "Stolz", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und keine sah es was ich litte,", "tokens": ["Und", "kei\u00b7ne", "sah", "es", "was", "ich", "lit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPER", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und keine h\u00f6rte meine Bitte,", "tokens": ["Und", "kei\u00b7ne", "h\u00f6r\u00b7te", "mei\u00b7ne", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Verstand mein Sehnen, meine Pein,", "tokens": ["Ver\u00b7stand", "mein", "Seh\u00b7nen", ",", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mir liebenswerth, mir was du bist zu sein.", "tokens": ["Mir", "lie\u00b7bens\u00b7werth", ",", "mir", "was", "du", "bist", "zu", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PPER", "PWS", "PPER", "VAFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Jetzt hab' ich dich \u2013 und soll dich lassen \u2013", "tokens": ["Jetzt", "hab'", "ich", "dich", "\u2013", "und", "soll", "dich", "las\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "$(", "KON", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Eh m\u00f6ge mich die H\u00f6lle fassen.", "tokens": ["Eh", "m\u00f6\u00b7ge", "mich", "die", "H\u00f6l\u00b7le", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}