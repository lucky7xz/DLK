{"dta.poem.9954": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieser giebet mir den tod/ der mir vor das licht gegeben/", "tokens": ["Die\u00b7ser", "gie\u00b7bet", "mir", "den", "tod", "/", "der", "mir", "vor", "das", "licht", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ohn ihn kan ich gar nicht seyn/ er kan ohne mich nicht leben/", "tokens": ["Ohn", "ihn", "kan", "ich", "gar", "nicht", "seyn", "/", "er", "kan", "oh\u00b7ne", "mich", "nicht", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VAINF", "$(", "PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Durch ihn werd ich nur erhalte\u0303/ durch mich er sich auch erh\u00e4lt/", "tokens": ["Durch", "ihn", "werd", "ich", "nur", "er\u00b7hal\u00b7t\u1ebd", "/", "durch", "mich", "er", "sich", "auch", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$(", "APPR", "PPER", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-+---+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Feuer bringt mich auf die erde\u0303/ feuer bringt mich aus der welt.", "tokens": ["Feu\u00b7er", "bringt", "mich", "auf", "die", "erd\u1ebd", "/", "feu\u00b7er", "bringt", "mich", "aus", "der", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}