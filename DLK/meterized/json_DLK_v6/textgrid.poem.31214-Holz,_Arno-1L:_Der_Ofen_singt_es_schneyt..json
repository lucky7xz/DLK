{"textgrid.poem.31214": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Ofen singt/ es schneyt.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ofen singt/ es schneyt.", "tokens": ["Der", "O\u00b7fen", "singt", "/", "es", "schneyt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du lihbe Weihnachts-Zeit!", "tokens": ["Du", "lih\u00b7be", "Weih\u00b7nachts\u00b7Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "R\u00fckk her/ du Traute;", "tokens": ["R\u00fckk", "her", "/", "du", "Trau\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$(", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "bey Frost und Feuer-Schein/", "tokens": ["bey", "Frost", "und", "Feu\u00b7er\u00b7Schein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "zu Mo\u00dfkateller Wein/", "tokens": ["zu", "Mo\u00df\u00b7ka\u00b7tel\u00b7ler", "Wein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "klingt s\u00fc\u00df die Laute.", "tokens": ["klingt", "s\u00fc\u00df", "die", "Lau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Er trukkt sein Himmel-Bett", "tokens": ["Er", "trukkt", "sein", "Him\u00b7mel\u00b7Bett"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "itzt vihl zu lange.", "tokens": ["itzt", "vihl", "zu", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "macht ihm itzt bange!", "tokens": ["macht", "ihm", "itzt", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Rund \u00fcmb den Disch herum", "tokens": ["Rund", "\u00fcmb", "den", "Disch", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APZR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "ein ", "tokens": ["ein"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "siht man ihn halten.", "tokens": ["siht", "man", "ihn", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wer sich ihm nicht gleich f\u00fcgt/", "tokens": ["Wer", "sich", "ihm", "nicht", "gleich", "f\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PPER", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "dem w\u00fcrd er still-vergn\u00fcgt", "tokens": ["dem", "w\u00fcrd", "er", "still\u00b7ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "den Scheddel spalten.", "tokens": ["den", "Sched\u00b7del", "spal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Die lihbe lange Nacht/", "tokens": ["Die", "lih\u00b7be", "lan\u00b7ge", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "da\u00df fast die Schwarte kracht/", "tokens": ["da\u00df", "fast", "die", "Schwar\u00b7te", "kracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "h\u00f6rt man ihn brahlen.", "tokens": ["h\u00f6rt", "man", "ihn", "brah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "jedem das Pfeiffgen an/", "tokens": ["je\u00b7dem", "das", "Pfeiff\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PTKVZ", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "horcht/ wie sie krahlen:", "tokens": ["horcht", "/", "wie", "sie", "krah\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAV", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Hann\u00df Thumm regirt die Welt/", "tokens": ["Hann\u00df", "Thumm", "re\u00b7girt", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "for Ulmer Silber-Geldt", "tokens": ["for", "Ul\u00b7mer", "Sil\u00b7ber\u00b7Geldt"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "kan man sie kauffen.", "tokens": ["kan", "man", "sie", "kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da\u00df bleibt ihr b\u00e4ster ", "tokens": ["Da\u00df", "bleibt", "ihr", "b\u00e4s\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "ein rundes Kind im Arm", "tokens": ["ein", "run\u00b7des", "Kind", "im", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und Broihan sauffen! \u2013", "tokens": ["und", "Broi\u00b7han", "sauf\u00b7fen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Itzt will ich fr\u00f6lig seyn/", "tokens": ["Itzt", "will", "ich", "fr\u00f6\u00b7lig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "bakkt mir ein Ringel-Schwein/", "tokens": ["ba\u00b7kkt", "mir", "ein", "Rin\u00b7gel\u00b7Schwein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "darzu Saulaten.", "tokens": ["dar\u00b7zu", "Sau\u00b7la\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Stopfft es gantz voll Conf\u00e4kkt/", "tokens": ["Stopfft", "es", "gantz", "voll", "Con\u00b7f\u00e4kkt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "da\u00df es noch b\u00e4sser schm\u00e4kkt/", "tokens": ["da\u00df", "es", "noch", "b\u00e4s\u00b7ser", "schm\u00e4kkt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "hihr drey Dukahten!", "tokens": ["hihr", "drey", "Du\u00b7kah\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Sind denn nicht M\u00e4gdgens da?", "tokens": ["Sind", "denn", "nicht", "M\u00e4gd\u00b7gens", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich b\u00fcn der Padischa.", "tokens": ["Ich", "b\u00fcn", "der", "Pa\u00b7di\u00b7scha", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sich dein Hertz erbarm/", "tokens": ["Da\u00df", "sich", "dein", "Hertz", "er\u00b7barm", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "gleich dr\u00fcmb so lihgt mein Arm", "tokens": ["gleich", "dr\u00fcmb", "so", "lihgt", "mein", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "qwer \u00fcmb dein Mittgen.", "tokens": ["qwer", "\u00fcmb", "dein", "Mitt\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Du nakkte ", "tokens": ["Du", "nakk\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "dein runder Feuer-Schnee", "tokens": ["dein", "run\u00b7der", "Feu\u00b7er\u00b7Schnee"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "hat mich entz\u00fcndet;", "tokens": ["hat", "mich", "ent\u00b7z\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "du Milch-gemischtes Bluht/", "tokens": ["du", "Milch\u00b7ge\u00b7mischtes", "Bluht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "du g\u00f6ldne Rohsen-Gluht/", "tokens": ["du", "g\u00f6ld\u00b7ne", "Roh\u00b7sen\u00b7Gluht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "dreyn Alles m\u00fcndet!", "tokens": ["dreyn", "Al\u00b7les", "m\u00fcn\u00b7det", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Sp\u00fczz deinen Purpur-Mund/", "tokens": ["Sp\u00fczz", "dei\u00b7nen", "Pur\u00b7pur\u00b7Mund", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "dein Duppel Kugel-Rund", "tokens": ["dein", "Dup\u00b7pel", "Ku\u00b7gel\u00b7Rund"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "f\u00fcll mir die Finger!", "tokens": ["f\u00fcll", "mir", "die", "Fin\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Verstrikk/ verf\u00e4ssle mich/", "tokens": ["Ver\u00b7strikk", "/", "ver\u00b7f\u00e4ss\u00b7le", "mich", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "du kleiner W\u00fctherich/", "tokens": ["du", "klei\u00b7ner", "W\u00fct\u00b7he\u00b7rich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "du Hertz-Betzwinger!", "tokens": ["du", "Hertz\u00b7Betzwin\u00b7ger", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "F\u00fcr einer Marmol-Haut", "tokens": ["F\u00fcr", "ei\u00b7ner", "Mar\u00b7mol\u00b7Haut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "hat mir noch nie gegraut/", "tokens": ["hat", "mir", "noch", "nie", "ge\u00b7graut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "ich kans nicht lassen/", "tokens": ["ich", "kans", "nicht", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "f\u00fchl ich wo rund ein Knie/", "tokens": ["f\u00fchl", "ich", "wo", "rund", "ein", "Knie", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "ADJD", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "dihses voll ", "tokens": ["dih\u00b7ses", "voll"], "token_info": ["word", "word"], "pos": ["PDS", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "f\u00e4st zu \u00fcmbfassen!", "tokens": ["f\u00e4st", "zu", "\u00fcmb\u00b7fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Flinck/ l\u00f6sch die Lichter au\u00df/", "tokens": ["Flinck", "/", "l\u00f6sch", "die", "Lich\u00b7ter", "au\u00df", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "weil sich lengst bundt \u00fcmbs Hau\u00df", "tokens": ["weil", "sich", "lengst", "bundt", "\u00fcmbs", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die Sterne drehen!", "tokens": ["die", "Ster\u00b7ne", "dre\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Noch wenn der Morgen scheint/", "tokens": ["Noch", "wenn", "der", "Mor\u00b7gen", "scheint", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "soll er uns froh-vereint", "tokens": ["soll", "er", "uns", "froh\u00b7ver\u00b7eint"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "NE"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "bey sammen sehen!", "tokens": ["bey", "sam\u00b7men", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}