{"textgrid.poem.40639": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun auf dem Meer die Regenschauer lasten,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun auf dem Meer die Regenschauer lasten,", "tokens": ["Nun", "auf", "dem", "Meer", "die", "Re\u00b7gen\u00b7schau\u00b7er", "las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sucht dein Lootsenaug' im Dunstgebraue?", "tokens": ["Was", "sucht", "dein", "Loot\u00b7se\u00b7naug'", "im", "Dunst\u00b7ge\u00b7brau\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbnothflaggen, die mich rufen, morsche Taue,", "tokens": ["\u00bb", "noth\u00b7flag\u00b7gen", ",", "die", "mich", "ru\u00b7fen", ",", "mor\u00b7sche", "Tau\u00b7e", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verlorne Anker und bedrohte Masten!\u00ab", "tokens": ["Ver\u00b7lor\u00b7ne", "An\u00b7ker", "und", "be\u00b7droh\u00b7te", "Mas\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie kann dein altes Aug durch Nebel tasten,", "tokens": ["Wie", "kann", "dein", "al\u00b7tes", "Aug", "durch", "Ne\u00b7bel", "tas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich mein j\u00fcngres senkt am wirren Graue?", "tokens": ["Wo", "sich", "mein", "j\u00fcng\u00b7res", "senkt", "am", "wir\u00b7ren", "Grau\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJA", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbdas kommt, weil ich in See mein Lebtag schaue", "tokens": ["\u00bb", "das", "kommt", ",", "weil", "ich", "in", "See", "mein", "Leb\u00b7tag", "schau\u00b7e"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Eures auf Papier nur pflegt zu rasten.\u00ab", "tokens": ["Und", "Eu\u00b7res", "auf", "Pa\u00b7pier", "nur", "pflegt", "zu", "ras\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "APPR", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Meer ist auch das wei\u00dfe Blatt nicht minder,", "tokens": ["Ein", "Meer", "ist", "auch", "das", "wei\u00b7\u00dfe", "Blatt", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat reiche Frachter, k\u00fchne Weltenfinder,", "tokens": ["Hat", "rei\u00b7che", "Frach\u00b7ter", ",", "k\u00fch\u00b7ne", "Wel\u00b7ten\u00b7fin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Manch treuen Lootsen, der zur Ferne schaue,", "tokens": ["Manch", "treu\u00b7en", "Loot\u00b7sen", ",", "der", "zur", "Fer\u00b7ne", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hat Wolken auch, die um die Sterne lasten;", "tokens": ["Hat", "Wol\u00b7ken", "auch", ",", "die", "um", "die", "Ster\u00b7ne", "las\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Auge sieht, wie deins, gef\u00e4llte Masten,", "tokens": ["Mein", "Au\u00b7ge", "sieht", ",", "wie", "deins", ",", "ge\u00b7f\u00e4ll\u00b7te", "Mas\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerbrochne Anker und zerri\u00df'ne Taue.", "tokens": ["Zer\u00b7broch\u00b7ne", "An\u00b7ker", "und", "zer\u00b7ri\u00df'\u00b7ne", "Tau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}