{"dta.poem.7072": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch unaussprechlich reine Lust,", "tokens": ["Welch", "un\u00b7aus\u00b7sprech\u00b7lich", "rei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch eine Wonne wird sie r\u00fchren!", "tokens": ["Welch", "ei\u00b7ne", "Won\u00b7ne", "wird", "sie", "r\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Welch eine Seelen-Ruh wird die verkl\u00e4rte Brust,", "tokens": ["Welch", "ei\u00b7ne", "See\u00b7len\u00b7Ruh", "wird", "die", "ver\u00b7kl\u00e4r\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihr verhimmelt Wesen sp\u00fcren!", "tokens": ["Und", "ihr", "ver\u00b7him\u00b7melt", "We\u00b7sen", "sp\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sich vielleicht die Schaar der ", "tokens": ["Wenn", "sich", "viel\u00b7leicht", "die", "Schaar", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Zu ihnen wird zugleich verf\u00fcgen,", "tokens": ["Zu", "ih\u00b7nen", "wird", "zu\u00b7gleich", "ver\u00b7f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ihnen die Beschaffenheiten", "tokens": ["Und", "ih\u00b7nen", "die", "Be\u00b7schaf\u00b7fen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der G\u00f6ttlich-grossen Herrlichkeiten,", "tokens": ["Der", "G\u00f6tt\u00b7lich\u00b7gros\u00b7sen", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch die Geheimnisse von der ", "tokens": ["Auch", "die", "Ge\u00b7heim\u00b7nis\u00b7se", "von", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die dem Verstande nicht, dem Glauben nur allein", "tokens": ["Die", "dem", "Ver\u00b7stan\u00b7de", "nicht", ",", "dem", "Glau\u00b7ben", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PTKNEG", "$,", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Begreifflich seyn,", "tokens": ["Be\u00b7greif\u00b7flich", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "In unaussprechlichen, uns unbekannten Lehren,", "tokens": ["In", "un\u00b7aus\u00b7sprech\u00b7li\u00b7chen", ",", "uns", "un\u00b7be\u00b7kann\u00b7ten", "Leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auf unbegreiffliche erhabne Weis\u2019 erkl\u00e4ren.", "tokens": ["Auf", "un\u00b7be\u00b7greif\u00b7fli\u00b7che", "er\u00b7hab\u00b7ne", "Weis'", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}