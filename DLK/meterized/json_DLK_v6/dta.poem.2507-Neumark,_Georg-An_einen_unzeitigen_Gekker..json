{"dta.poem.2507": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An einen unzeitigen Gekker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Neulich an dem Pregelstrohm' irrt' ich mit betr\u00fcbtem", "tokens": ["Neu\u00b7lich", "an", "dem", "Pre\u00b7gel\u00b7strohm'", "irrt'", "ich", "mit", "be\u00b7tr\u00fcb\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sinn/", "tokens": ["Sinn", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dachte gleich an Karitin\u2019 an mein \u00e4dle Sch\u00e4fferinn;", "tokens": ["Dach\u00b7te", "gleich", "an", "Ka\u00b7ri\u00b7tin'", "an", "mein", "\u00e4d\u00b7le", "Sch\u00e4f\u00b7fe\u00b7rinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Weine du mit vollen G\u00fcssen/ sagt\u2019 ich/ du du \u00e4dler", "tokens": ["Wei\u00b7ne", "du", "mit", "vol\u00b7len", "G\u00fcs\u00b7sen", "/", "sagt'", "ich", "/", "du", "du", "\u00e4d\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$(", "VVFIN", "PPER", "$(", "PPER", "PPER", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Flu\u00df/", "tokens": ["Flu\u00df", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und beklage meine Liebe/ die ich itzo meiden mu\u00df/", "tokens": ["Und", "be\u00b7kla\u00b7ge", "mei\u00b7ne", "Lie\u00b7be", "/", "die", "ich", "it\u00b7zo", "mei\u00b7den", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Weil ich mich gantz abgeweint. Ach m\u00f6cht\u2019 ich zum Was-", "tokens": ["Weil", "ich", "mich", "gantz", "ab\u00b7ge\u00b7weint", ".", "Ach", "m\u00f6cht'", "ich", "zum", "Was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$.", "NN", "VMFIN", "PPER", "APPRART", "TRUNC"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.8": {"text": "ser werden/", "tokens": ["ser", "wer\u00b7den", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Als wie du O Pregel bist! dann wolt\u2019 ich auff dieser Erden", "tokens": ["Als", "wie", "du", "O", "Pre\u00b7gel", "bist", "!", "dann", "wolt'", "ich", "auff", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "NE", "NE", "NN", "VAFIN", "$.", "ADV", "VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Stets mit bittren Thr\u00e4nen fliessen. Hierauff kam mir", "tokens": ["Stets", "mit", "bit\u00b7tren", "Thr\u00e4\u00b7nen", "flies\u00b7sen", ".", "Hier\u00b7auff", "kam", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$.", "PAV", "VVFIN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "wieder zu/", "tokens": ["wie\u00b7der", "zu", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKZU", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Aus dem Strohm\u2019 ein sachtes Seu\u00dflen: ", "tokens": ["Aus", "dem", "Strohm'", "ein", "sach\u00b7tes", "Seu\u00df\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "w\u00fcnscht vielleicht wie du.", "tokens": ["w\u00fcnscht", "viel\u00b7leicht", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}