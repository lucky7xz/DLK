{"dta.poem.753": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX.  An  H. Christophorum Loth.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Nembt wieder wehrter Freundt/ dem ich mein Hertz gelih&#7869; ", "tokens": ["Nembt", "wie\u00b7der", "wehr\u00b7ter", "Freundt", "/", "dem", "ich", "mein", "Hertz", "ge\u00b7lih", "&#7869;"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$(", "PRELS", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was jhr mir newlich liht/ das jahrbuch vnser zeit/", "tokens": ["Was", "jhr", "mir", "new\u00b7lich", "liht", "/", "das", "jahr\u00b7buch", "vn\u00b7ser", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADJD", "VVFIN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die anzahl scheller tag\u2019 in deu vns ach vnd leidt", "tokens": ["Die", "an\u00b7zahl", "schel\u00b7ler", "tag'", "in", "deu", "vns", "ach", "vnd", "leidt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PRELS", "PPER", "XY", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lange kr\u00e4nckt bi\u00df sie/ vnd wir mit jhn entfliehen.", "tokens": ["So", "lan\u00b7ge", "kr\u00e4nckt", "bi\u00df", "sie", "/", "vnd", "wir", "mit", "jhn", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPER", "$(", "KON", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir armen schawn die Sonn\u2019 jtzt von/ jtzt zu vns zihen:", "tokens": ["Wir", "ar\u00b7men", "schawn", "die", "Sonn'", "jtzt", "von", "/", "jtzt", "zu", "vns", "zi\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VVFIN", "ART", "NN", "ADV", "APPR", "$(", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vnd gehn den Sternen nach/ wir lernen welche seit", "tokens": ["Vnd", "gehn", "den", "Ster\u00b7nen", "nach", "/", "wir", "ler\u00b7nen", "wel\u00b7che", "seit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "$(", "PPER", "VVINF", "PWAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Himmel ", "tokens": ["Der", "Him\u00b7mel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Vnd sehn nicht da\u00df wir hier den ", "tokens": ["Vnd", "sehn", "nicht", "da\u00df", "wir", "hier", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "KOUS", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir rechnen Monat aus/ vnd spilen mit dem Jahr", "tokens": ["Wir", "rech\u00b7nen", "Mo\u00b7nat", "aus", "/", "vnd", "spi\u00b7len", "mit", "dem", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$(", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd ordnen tag auff tag/ vnd wenn die r\u00e4itung klar:", "tokens": ["Vnd", "ord\u00b7nen", "tag", "auff", "tag", "/", "vnd", "wenn", "die", "r\u00e4i\u00b7tung", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$(", "KON", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bleibt vns doch vnser tag vnd vntergang verholen?", "tokens": ["Bleibt", "vns", "doch", "vn\u00b7ser", "tag", "vnd", "vn\u00b7ter\u00b7gang", "ver\u00b7ho\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nichts ist das zeit vnd todt nicht an das joch gebracht", "tokens": ["Nichts", "ist", "das", "zeit", "vnd", "todt", "nicht", "an", "das", "joch", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ART", "NN", "KON", "ADJD", "PTKNEG", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn freundtschafft/ welche zeit vnd sterben keck verlacht", "tokens": ["Ohn", "freundt\u00b7schafft", "/", "wel\u00b7che", "zeit", "vnd", "ster\u00b7ben", "keck", "ver\u00b7lacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "PWAT", "NN", "KON", "VVFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herr Loth/ der haben wir zwey einen Geist befohlen. ", "tokens": ["Herr", "Loth", "/", "der", "ha\u00b7ben", "wir", "zwey", "ei\u00b7nen", "Geist", "be\u00b7foh\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ART", "VAFIN", "PPER", "CARD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}