{"textgrid.poem.59136": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "355. Besteht in der Freiheit!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich vom Werk als Sklav' nicht treiben,", "tokens": ["La\u00df", "dich", "vom", "Werk", "als", "Skla\u00b7v'", "nicht", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KOUS", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Geist ein Herr des Werks mu\u00df bleiben;", "tokens": ["Der", "Geist", "ein", "Herr", "des", "Werks", "mu\u00df", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schau nur auf Gott, dem diene treu,", "tokens": ["Schau", "nur", "auf", "Gott", ",", "dem", "die\u00b7ne", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,", "PRELS", "PDS", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sanft, willenlos und innig frei!", "tokens": ["Sanft", ",", "wil\u00b7len\u00b7los", "und", "in\u00b7nig", "frei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}