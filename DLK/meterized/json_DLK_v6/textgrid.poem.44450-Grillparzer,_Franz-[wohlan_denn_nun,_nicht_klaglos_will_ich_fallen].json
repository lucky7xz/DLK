{"textgrid.poem.44450": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wohlan denn nun, nicht klaglos will ich fallen]", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlan denn nun, nicht klaglos will ich fallen,", "tokens": ["Wo\u00b7hlan", "denn", "nun", ",", "nicht", "klag\u00b7los", "will", "ich", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "PTKNEG", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Opfertiere gleich, das st\u00f6hnt und stirbt;", "tokens": ["Dem", "Op\u00b7fer\u00b7tie\u00b7re", "gleich", ",", "das", "st\u00f6hnt", "und", "stirbt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf da\u00df Gemeinheit zu den Siegen allen,", "tokens": ["Auf", "da\u00df", "Ge\u00b7mein\u00b7heit", "zu", "den", "Sie\u00b7gen", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "NN", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die schon sie feiert, nicht noch den erwirbt,", "tokens": ["Die", "schon", "sie", "fei\u00b7ert", ",", "nicht", "noch", "den", "er\u00b7wirbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df kundlos ihre Tat, da\u00df, die sie schlachtet \u2013", "tokens": ["Da\u00df", "kund\u00b7los", "ih\u00b7re", "Tat", ",", "da\u00df", ",", "die", "sie", "schlach\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "$,", "KOUS", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn nun die Welt fr\u00fch, eh der Morgen gl\u00fcht,", "tokens": ["Wenn", "nun", "die", "Welt", "fr\u00fch", ",", "eh", "der", "Mor\u00b7gen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Leiche schaut und keinen T\u00e4ter sieht \u2013", "tokens": ["Die", "Lei\u00b7che", "schaut", "und", "kei\u00b7nen", "T\u00e4\u00b7ter", "sieht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als Frevler an sich selber sei'n geachtet.", "tokens": ["Als", "Frev\u00b7ler", "an", "sich", "sel\u00b7ber", "sei'n", "ge\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PRF", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}