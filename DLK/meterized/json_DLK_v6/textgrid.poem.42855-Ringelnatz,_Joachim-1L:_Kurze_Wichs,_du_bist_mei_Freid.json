{"textgrid.poem.42855": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kurze Wichs, du bist mei Freid", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kurze Wichs, du bist mei Freid", "tokens": ["Kur\u00b7ze", "Wichs", ",", "du", "bist", "mei", "Freid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wegen der Hygiene,", "tokens": ["We\u00b7gen", "der", "Hy\u00b7gi\u00b7e\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "L\u00e4\u00dft den Maderln zur Augenweid", "tokens": ["L\u00e4\u00dft", "den", "Ma\u00b7derln", "zur", "Au\u00b7gen\u00b7weid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trutzbehaarte, nackte Beene.", "tokens": ["Trutz\u00b7be\u00b7haar\u00b7te", ",", "nack\u00b7te", "Bee\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur ein Mann von Schrot und Korn", "tokens": ["Nur", "ein", "Mann", "von", "Schrot", "und", "Korn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Konnte dich erfinden.", "tokens": ["Konn\u00b7te", "dich", "er\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kurze Wichs, du bist von vorn", "tokens": ["Kur\u00b7ze", "Wichs", ",", "du", "bist", "von", "vorn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "APPR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Fraun von hinten.", "tokens": ["Wie", "die", "Fraun", "von", "hin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Kurze Wichs, du firmst den Bua,", "tokens": ["Kur\u00b7ze", "Wichs", ",", "du", "firmst", "den", "Bua", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dich liebt ein jeder,", "tokens": ["Und", "dich", "liebt", "ein", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Diar rh\u00f6 holi da jua", "tokens": ["Di\u00b7ar", "rh\u00f6", "ho\u00b7li", "da", "jua"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Jodelt's dir vom Leder.", "tokens": ["Jo\u00b7delt's", "dir", "vom", "Le\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Kurze Wichs! \u2013 Hei, wie das knallt,", "tokens": ["Kur\u00b7ze", "Wichs", "!", "\u2013", "Hei", ",", "wie", "das", "knallt", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "NE", "$,", "PWAV", "PDS", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Wenn ich auf dich schlage!", "tokens": ["Wenn", "ich", "auf", "dich", "schla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Alles, alles, alles prallt", "tokens": ["Al\u00b7les", ",", "al\u00b7les", ",", "al\u00b7les", "prallt"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PIS", "$,", "PIS", "$,", "PIS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ab, wenn ich dich trage.", "tokens": ["Ab", ",", "wenn", "ich", "dich", "tra\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}