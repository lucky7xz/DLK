{"textgrid.poem.47837": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitunter weicht von meiner Brust,", "tokens": ["Mi\u00b7tun\u00b7ter", "weicht", "von", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sie bedr\u00fcckt seit deinem Sterben;", "tokens": ["Was", "sie", "be\u00b7dr\u00fcckt", "seit", "dei\u00b7nem", "Ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es dr\u00e4ngt mich, wie in Jugendlust,", "tokens": ["Es", "dr\u00e4ngt", "mich", ",", "wie", "in", "Ju\u00b7gend\u00b7lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal um das Gl\u00fcck zu werben.", "tokens": ["Noch", "ein\u00b7mal", "um", "das", "Gl\u00fcck", "zu", "wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch frag ich dann: Was ist das Gl\u00fcck?", "tokens": ["Doch", "frag", "ich", "dann", ":", "Was", "ist", "das", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "$.", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kann ich keine Antwort geben", "tokens": ["So", "kann", "ich", "kei\u00b7ne", "Ant\u00b7wort", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als die, da\u00df du mir k\u00e4mst zur\u00fcck,", "tokens": ["Als", "die", ",", "da\u00df", "du", "mir", "k\u00e4mst", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "KOUS", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um so wie einst mit mir zu leben.", "tokens": ["Um", "so", "wie", "einst", "mit", "mir", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "KOKOM", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann seh ich jenen Morgenschein,", "tokens": ["Dann", "seh", "ich", "je\u00b7nen", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wir dich hin zur Gruft getragen;", "tokens": ["Da", "wir", "dich", "hin", "zur", "Gruft", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lautlos schlafen die W\u00fcnsche ein,", "tokens": ["Und", "laut\u00b7los", "schla\u00b7fen", "die", "W\u00fcn\u00b7sche", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nicht mehr will ich das Gl\u00fcck erjagen.", "tokens": ["Und", "nicht", "mehr", "will", "ich", "das", "Gl\u00fcck", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}