{"dta.poem.19236": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig L\u00f6we h\u00e4lt im Walde Mittagsruh,", "tokens": ["Der", "K\u00f6\u00b7nig", "L\u00f6\u00b7we", "h\u00e4lt", "im", "Wal\u00b7de", "Mit\u00b7tags\u00b7ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verdrie\u00dflich gehen ihm die Augen auf und zu.", "tokens": ["Ver\u00b7drie\u00df\u00b7lich", "ge\u00b7hen", "ihm", "die", "Au\u00b7gen", "auf", "und", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sorge kann er sich nicht aus dem Sinne schlagen;", "tokens": ["Die", "Sor\u00b7ge", "kann", "er", "sich", "nicht", "aus", "dem", "Sin\u00b7ne", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Unmuth minder noch vertr\u00e4umen als verjagen.", "tokens": ["Den", "Un\u00b7muth", "min\u00b7der", "noch", "ver\u00b7tr\u00e4u\u00b7men", "als", "ver\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da sieht er \u00fcber sich im Baum ein Eichhorn h\u00fcpfen,", "tokens": ["Da", "sieht", "er", "\u00fc\u00b7ber", "sich", "im", "Baum", "ein", "Eich\u00b7horn", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Behaglich durchs Gezweig und unerm\u00fcdlich schl\u00fcpfen.", "tokens": ["Be\u00b7hag\u00b7lich", "durchs", "Ge\u00b7zweig", "und", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "schl\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er ruft hinauf: Warum trag' ich des Thierreichs Krone", "tokens": ["Er", "ruft", "hin\u00b7auf", ":", "Wa\u00b7rum", "trag'", "ich", "des", "Thier\u00b7reichs", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du sitzest, kleines Thier, dort auf der Freiheit Throne.", "tokens": ["Du", "sit\u00b7zest", ",", "klei\u00b7nes", "Thier", ",", "dort", "auf", "der", "Frei\u00b7heit", "Thro\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie kommt es, da\u00df du hast ein Gl\u00fcck, das mir nicht ward?", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "du", "hast", "ein", "Gl\u00fcck", ",", "das", "mir", "nicht", "ward", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es rief herab: Das kommt von unsrer Lebensart.", "tokens": ["Es", "rief", "her\u00b7ab", ":", "Das", "kommt", "von", "uns\u00b7rer", "Le\u00b7ben\u00b7sart", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihr esset Fleisch und Blut, und habet schweren Muth;", "tokens": ["Ihr", "es\u00b7set", "Fleisch", "und", "Blut", ",", "und", "ha\u00b7bet", "schwe\u00b7ren", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KON", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich esse Knosp' und Frucht, und habe leichtes Blut.", "tokens": ["Ich", "es\u00b7se", "Knosp'", "und", "Frucht", ",", "und", "ha\u00b7be", "leich\u00b7tes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$,", "KON", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Entbehrung ist Genu\u00df, Genu\u00df ist eine B\u00fcrde;", "tokens": ["Ent\u00b7beh\u00b7rung", "ist", "Ge\u00b7nu\u00df", ",", "Ge\u00b7nu\u00df", "ist", "ei\u00b7ne", "B\u00fcr\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr K\u00f6nig, unvereint ist leichter Sinn und W\u00fcrde.", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "un\u00b7ver\u00b7eint", "ist", "leich\u00b7ter", "Sinn", "und", "W\u00fcr\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "VAFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}