{"textgrid.poem.63037": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Affe kam ins Reich der Thiere", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Affe kam ins Reich der Thiere", "tokens": ["Ein", "Af\u00b7fe", "kam", "ins", "Reich", "der", "Thie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Josephs Reich zur\u00fcck. Was neues, Freund, aus Wien?", "tokens": ["Aus", "Jo\u00b7se\u00b7phs", "Reich", "zu\u00b7r\u00fcck", ".", "Was", "neu\u00b7es", ",", "Freund", ",", "aus", "Wi\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$.", "PWS", "ADJA", "$,", "NN", "$,", "APPR", "NE", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So frug ein Klub der Esel und der Stiere", "tokens": ["So", "frug", "ein", "Klub", "der", "E\u00b7sel", "und", "der", "Stie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein feistes Schwein den Paladin.", "tokens": ["Ein", "feis\u00b7tes", "Schwein", "den", "Pa\u00b7la\u00b7din", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Tagebuch, sprach er, liegt fertig f\u00fcr die Presse;", "tokens": ["Mein", "Ta\u00b7ge\u00b7buch", ",", "sprach", "er", ",", "liegt", "fer\u00b7tig", "f\u00fcr", "die", "Pres\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Indessen h\u00f6rt was ich gesehn:", "tokens": ["In\u00b7des\u00b7sen", "h\u00f6rt", "was", "ich", "ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sah, wie Hand in Hand die Welschen in die Messe,", "tokens": ["Ich", "sah", ",", "wie", "Hand", "in", "Hand", "die", "Wel\u00b7schen", "in", "die", "Mes\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Sachsen in die Predigt gehn,", "tokens": ["Die", "Sach\u00b7sen", "in", "die", "Pre\u00b7digt", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wie bey einem Glas mit Ofner Weine", "tokens": ["Und", "wie", "bey", "ei\u00b7nem", "Glas", "mit", "Of\u00b7ner", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein Jud, in froher Harmonie,", "tokens": ["Ein", "Jud", ",", "in", "fro\u00b7her", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit Christen Schinken a\u00df. Ha, riefen Gro\u00df und Kleine,", "tokens": ["Mit", "Chris\u00b7ten", "Schin\u00b7ken", "a\u00df", ".", "Ha", ",", "rie\u00b7fen", "Gro\u00df", "und", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "ITJ", "$,", "VVFIN", "NE", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es ist ein herrlich Ding um die Philosophie!", "tokens": ["Es", "ist", "ein", "herr\u00b7lich", "Ding", "um", "die", "Phi\u00b7lo\u00b7so\u00b7phie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mag seyn, versetzt die Sau, der Herz und Knie", "tokens": ["Mag", "seyn", ",", "ver\u00b7setzt", "die", "Sau", ",", "der", "Herz", "und", "Knie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VAINF", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Beym Worte Schinken sank, nur nicht f\u00fcr fette Schweine.", "tokens": ["Beym", "Wor\u00b7te", "Schin\u00b7ken", "sank", ",", "nur", "nicht", "f\u00fcr", "fet\u00b7te", "Schwei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$,", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}