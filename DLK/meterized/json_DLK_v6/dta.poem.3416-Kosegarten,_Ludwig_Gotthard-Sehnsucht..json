{"dta.poem.3416": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.71", "et:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ida, Ida, meine Liebe,", "tokens": ["I\u00b7da", ",", "I\u00b7da", ",", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Grames Tr\u00f6sterin,", "tokens": ["Mei\u00b7nes", "Gra\u00b7mes", "Tr\u00f6s\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00fcbe sieht der Himmel, tr\u00fcbe,", "tokens": ["Tr\u00fc\u00b7be", "sieht", "der", "Him\u00b7mel", ",", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcber mein umflorter Sinn.", "tokens": ["Tr\u00fc\u00b7ber", "mein", "um\u00b7flor\u00b7ter", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mancher \u00f6de Tag ist schwunden,", "tokens": ["Man\u00b7cher", "\u00f6\u00b7de", "Tag", "ist", "schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche d\u00fcstre Nacht verfloss,", "tokens": ["Man\u00b7che", "d\u00fcst\u00b7re", "Nacht", "ver\u00b7floss", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seit ich, deinem Arm entwunden,", "tokens": ["Seit", "ich", ",", "dei\u00b7nem", "Arm", "ent\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach dir lechze, letzelos.", "tokens": ["Nach", "dir", "lech\u00b7ze", ",", "let\u00b7ze\u00b7los", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonst erquickten deine Blicke", "tokens": ["Sonst", "er\u00b7quick\u00b7ten", "dei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich, wie Mayen-Sonnenschein;", "tokens": ["Mich", ",", "wie", "Mayen\u00b7Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Deine Winke, deine Nicke", "tokens": ["Dei\u00b7ne", "Win\u00b7ke", ",", "dei\u00b7ne", "Ni\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bebten mir durch Mark und Bein;", "tokens": ["Beb\u00b7ten", "mir", "durch", "Mark", "und", "Bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines Athems duftend Wehen", "tokens": ["Dei\u00b7nes", "A\u00b7thems", "duf\u00b7tend", "We\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fchlten meiner Inbrunst Gluth;", "tokens": ["K\u00fchl\u00b7ten", "mei\u00b7ner", "In\u00b7brunst", "Gluth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner Stimme r\u00fchrend Flehen", "tokens": ["Dei\u00b7ner", "Stim\u00b7me", "r\u00fch\u00b7rend", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stillte meine wildste Wuth.", "tokens": ["Still\u00b7te", "mei\u00b7ne", "wilds\u00b7te", "Wuth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deiner Hand beredtes Dr\u00fccken,", "tokens": ["Dei\u00b7ner", "Hand", "be\u00b7red\u00b7tes", "Dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Lippen Honigkuss,", "tokens": ["Dei\u00b7ner", "Lip\u00b7pen", "Ho\u00b7nig\u00b7kuss", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein Umfangen, dein Umstricken,", "tokens": ["Dein", "Um\u00b7fan\u00b7gen", ",", "dein", "Um\u00b7stri\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Sch\u00f6ne Vollgenuss,", "tokens": ["Dei\u00b7ner", "Sch\u00f6\u00b7ne", "Voll\u00b7ge\u00b7nuss", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Umflechten, dein Umranken,", "tokens": ["Dein", "Um\u00b7flech\u00b7ten", ",", "dein", "Um\u00b7ran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Arm in Arm, und Brust an Brust,", "tokens": ["Arm", "in", "Arm", ",", "und", "Brust", "an", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Heilte ganz den Sehnsuchtkranken,", "tokens": ["Heil\u00b7te", "ganz", "den", "Sehn\u00b7sucht\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tr\u00e4nkte mich mit Edenslust.", "tokens": ["Tr\u00e4nk\u00b7te", "mich", "mit", "E\u00b7dens\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber itzt erwacht der Morgen;", "tokens": ["A\u00b7ber", "itzt", "er\u00b7wacht", "der", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Hoffnung, dich zu sehn,", "tokens": ["Und", "die", "Hoff\u00b7nung", ",", "dich", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lindert nicht mein z\u00e4rtlich Sorgen,", "tokens": ["Lin\u00b7dert", "nicht", "mein", "z\u00e4rt\u00b7lich", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht der Trennung heisse Weh'n.", "tokens": ["Nicht", "der", "Tren\u00b7nung", "heis\u00b7se", "Weh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich durchschw\u00e4rme tr\u00fcb' und traurig", "tokens": ["Ich", "durch\u00b7schw\u00e4r\u00b7me", "tr\u00fcb'", "und", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Brach und Thal, und Wald und Flur,", "tokens": ["Brach", "und", "Thal", ",", "und", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Welt d\u00fcnkt mir so schaurig,", "tokens": ["Und", "die", "Welt", "d\u00fcnkt", "mir", "so", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ein Grabmahl die Natur.", "tokens": ["Und", "ein", "Grab\u00b7mahl", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gottes Sonne steigt und sinket,", "tokens": ["Got\u00b7tes", "Son\u00b7ne", "steigt", "und", "sin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes Sp\u00e4troth bl\u00fcht und blasst,", "tokens": ["Got\u00b7tes", "Sp\u00e4t\u00b7roth", "bl\u00fcht", "und", "blasst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus milchweissen Wolken winket", "tokens": ["Aus", "milch\u00b7weis\u00b7sen", "Wol\u00b7ken", "win\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Luna mir zu s\u00fcsser Rast.", "tokens": ["Lu\u00b7na", "mir", "zu", "s\u00fcs\u00b7ser", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ach, der Ruhe F\u00e4cheln", "tokens": ["A\u00b7ber", "ach", ",", "der", "Ru\u00b7he", "F\u00e4\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fchlt nicht meines Busens Gluth;", "tokens": ["K\u00fchlt", "nicht", "mei\u00b7nes", "Bu\u00b7sens", "Gluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00fchlen mag sie nur dein L\u00e4cheln,", "tokens": ["K\u00fch\u00b7len", "mag", "sie", "nur", "dein", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur dein Kosen, hold und gut. \u2014", "tokens": ["Nur", "dein", "Ko\u00b7sen", ",", "hold", "und", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ida, Ida, meine Liebe,", "tokens": ["I\u00b7da", ",", "I\u00b7da", ",", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Grames Tr\u00f6sterin,", "tokens": ["Mei\u00b7nes", "Gra\u00b7mes", "Tr\u00f6s\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist dir auch so nebeltr\u00fcbe,", "tokens": ["Ist", "dir", "auch", "so", "ne\u00b7belt\u00b7r\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch so dumpf, und schwer zu Sinn?", "tokens": ["Auch", "so", "dumpf", ",", "und", "schwer", "zu", "Sinn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denkst du auch in deiner Ferne", "tokens": ["Denkst", "du", "auch", "in", "dei\u00b7ner", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deines Lieblings? Denkest mein,", "tokens": ["Dei\u00b7nes", "Lieb\u00b7lings", "?", "Den\u00b7kest", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebliche, beym Glanz der Sterne,", "tokens": ["Lieb\u00b7li\u00b7che", ",", "beym", "Glanz", "der", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bey Aurorens rothem Schein?", "tokens": ["Bey", "Au\u00b7ro\u00b7rens", "ro\u00b7them", "Schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "M\u00f6chtest du wohl zu mir fliegen?", "tokens": ["M\u00f6ch\u00b7test", "du", "wohl", "zu", "mir", "flie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "M\u00f6chtest, Theure, matt und krank,", "tokens": ["M\u00f6ch\u00b7test", ",", "Theu\u00b7re", ",", "matt", "und", "krank", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ADJA", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir am treuen Busen liegen,", "tokens": ["Mir", "am", "treu\u00b7en", "Bu\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur Ein selig St\u00fcndlein lang?", "tokens": ["Nur", "Ein", "se\u00b7lig", "St\u00fcnd\u00b7lein", "lang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "M\u00f6chtest an mein Herz dich dr\u00fccken,", "tokens": ["M\u00f6ch\u00b7test", "an", "mein", "Herz", "dich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit Freuden s\u00fcss, so s\u00fcss,", "tokens": ["Und", "mit", "Freu\u00b7den", "s\u00fcss", ",", "so", "s\u00fcss", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mich entz\u00fccken, mich entr\u00fccken", "tokens": ["Mich", "ent\u00b7z\u00fc\u00b7cken", ",", "mich", "ent\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Liebe Paradies? \u2014 \u2014", "tokens": ["In", "der", "Lie\u00b7be", "Pa\u00b7ra\u00b7dies", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schwindet, schwindet, \u00f6de Tage,", "tokens": ["Schwin\u00b7det", ",", "schwin\u00b7det", ",", "\u00f6\u00b7de", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr mich von Iden trennt!", "tokens": ["Die", "ihr", "mich", "von", "I\u00b7den", "trennt", "!"], "token_info": ["word", "word", "w