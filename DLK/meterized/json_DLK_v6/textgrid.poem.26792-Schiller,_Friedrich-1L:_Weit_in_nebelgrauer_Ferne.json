{"textgrid.poem.26792": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weit in nebelgrauer Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit in nebelgrauer Ferne", "tokens": ["Weit", "in", "ne\u00b7bel\u00b7grau\u00b7er", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Liegt mir das vergangne Gl\u00fcck,", "tokens": ["Liegt", "mir", "das", "ver\u00b7gang\u00b7ne", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur an ", "tokens": ["Nur", "an"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Weilt mit Liebe noch der Blick.", "tokens": ["Weilt", "mit", "Lie\u00b7be", "noch", "der", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber wie des Sternes Pracht", "tokens": ["A\u00b7ber", "wie", "des", "Ster\u00b7nes", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist es nur ein Schein der Nacht.", "tokens": ["Ist", "es", "nur", "ein", "Schein", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deckte dir der lange Schlummer,", "tokens": ["Deck\u00b7te", "dir", "der", "lan\u00b7ge", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir der Tod die Augen zu,", "tokens": ["Dir", "der", "Tod", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich bes\u00e4\u00dfe doch mein Kummer,", "tokens": ["Dich", "be\u00b7s\u00e4\u00b7\u00dfe", "doch", "mein", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinem Herzen lebtest du.", "tokens": ["Mei\u00b7nem", "Her\u00b7zen", "leb\u00b7test", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ach! du lebst im Licht,", "tokens": ["A\u00b7ber", "ach", "!", "du", "lebst", "im", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Liebe lebst du nicht.", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "lebst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kann der Liebe s\u00fc\u00df Verlangen,", "tokens": ["Kann", "der", "Lie\u00b7be", "s\u00fc\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Emma, kanns verg\u00e4nglich sein?", "tokens": ["Em\u00b7ma", ",", "kanns", "ver\u00b7g\u00e4ng\u00b7lich", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was dahin ist und vergangen,", "tokens": ["Was", "da\u00b7hin", "ist", "und", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VAFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Emma, kanns die Liebe sein?", "tokens": ["Em\u00b7ma", ",", "kanns", "die", "Lie\u00b7be", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihrer Flamme Himmelsglut,", "tokens": ["Ih\u00b7rer", "Flam\u00b7me", "Him\u00b7mels\u00b7glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Stirbt sie wie ein irdisch Gut?", "tokens": ["Stirbt", "sie", "wie", "ein", "ir\u00b7disch", "Gut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}