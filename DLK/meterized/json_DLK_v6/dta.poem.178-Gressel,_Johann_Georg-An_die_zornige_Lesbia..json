{"dta.poem.178": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die zornige  Lesbia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich mich Lesbia mit Sorgen immer qu\u00e4hlen?", "tokens": ["Soll", "ich", "mich", "Les\u00b7bia", "mit", "Sor\u00b7gen", "im\u00b7mer", "qu\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "NE", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Soll denn dein ", "tokens": ["Soll", "denn", "dein"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Erbarm dich meiner Noht/ erquicke meinen Geist/", "tokens": ["Er\u00b7barm", "dich", "mei\u00b7ner", "Noht", "/", "er\u00b7qui\u00b7cke", "mei\u00b7nen", "Geist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wehr/ da\u00df nicht ", "tokens": ["Wehr", "/", "da\u00df", "nicht"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "KOUS", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Mu\u00df denn dein Zorn ", "tokens": ["Mu\u00df", "denn", "dein", "Zorn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und mit vergrelten Blitz auf meine Scheitel zielen?", "tokens": ["Und", "mit", "ver\u00b7grel\u00b7ten", "Blitz", "auf", "mei\u00b7ne", "Schei\u00b7tel", "zie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Soll ich an statt des Lichts nur schwartzen Schatten sehn/", "tokens": ["Soll", "ich", "an", "statt", "des", "Lichts", "nur", "schwart\u00b7zen", "Schat\u00b7ten", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und statt der Rosen-Bahn auf scharffen Dornen gehn?", "tokens": ["Und", "statt", "der", "Ro\u00b7sen\u00b7Bahn", "auf", "scharf\u00b7fen", "Dor\u00b7nen", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df doch ", "tokens": ["La\u00df", "doch"], "token_info": ["word", "word"], "pos": ["VVIMP", "ADV"], "meter": "++", "measure": "spondeus"}, "line.10": {"text": "Verstelle nicht in Grimm die freundlichen Gebehrden/", "tokens": ["Ver\u00b7stel\u00b7le", "nicht", "in", "Grimm", "die", "freund\u00b7li\u00b7chen", "Ge\u00b7behr\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NE", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gib deinem Zorn verlaub/ verkl\u00e4hrte dein Gesicht", "tokens": ["Gib", "dei\u00b7nem", "Zorn", "ver\u00b7laub", "/", "ver\u00b7kl\u00e4hr\u00b7te", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$(", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es sey dein Gnaden-Strahl auf meinen Geist gericht.", "tokens": ["Es", "sey", "dein", "Gna\u00b7den\u00b7Strahl", "auf", "mei\u00b7nen", "Geist", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn du nun ", "tokens": ["Wenn", "du", "nun"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Wenn holde Freundlichkeit dich Sch\u00f6nste soll bekr\u00e4ntzen?", "tokens": ["Wenn", "hol\u00b7de", "Freund\u00b7lich\u00b7keit", "dich", "Sch\u00f6ns\u00b7te", "soll", "be\u00b7kr\u00b7\u00e4nt\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "So k\u00fcsse mich dein Mund/ so sprich mich wieder lo\u00df/", "tokens": ["So", "k\u00fcs\u00b7se", "mich", "dein", "Mund", "/", "so", "sprich", "mich", "wie\u00b7der", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "ADV", "ADJD", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und nimm den ", "tokens": ["Und", "nimm", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVIMP", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}