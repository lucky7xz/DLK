{"textgrid.poem.35031": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Gottesgedanke", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bleib dir treu. Du wardst mit mir geboren", "tokens": ["Ich", "bleib", "dir", "treu", ".", "Du", "wardst", "mit", "mir", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als mein Begleiter f\u00fcr das Erdenthal.", "tokens": ["Als", "mein", "Be\u00b7glei\u00b7ter", "f\u00fcr", "das", "Er\u00b7dent\u00b7hal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir gingen uns niemals, niemals verloren;", "tokens": ["Wir", "gin\u00b7gen", "uns", "nie\u00b7mals", ",", "nie\u00b7mals", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich war die Welt; du warst mein Sonnenstrahl.", "tokens": ["Ich", "war", "die", "Welt", ";", "du", "warst", "mein", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja, ich die Welt! Es ist der Sch\u00f6pfung Ganzes", "tokens": ["Ja", ",", "ich", "die", "Welt", "!", "Es", "ist", "der", "Sch\u00f6p\u00b7fung", "Gan\u00b7zes"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Menschen klein, doch v\u00f6llig dargestellt,", "tokens": ["Im", "Men\u00b7schen", "klein", ",", "doch", "v\u00f6l\u00b7lig", "dar\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und athmet es im Lichte deines Glanzes,", "tokens": ["Und", "ath\u00b7met", "es", "im", "Lich\u00b7te", "dei\u00b7nes", "Glan\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So ist es eine gro\u00dfe, sch\u00f6ne Welt.", "tokens": ["So", "ist", "es", "ei\u00b7ne", "gro\u00b7\u00dfe", ",", "sch\u00f6\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich bleib dir treu. Es wechselten die Zeiten", "tokens": ["Ich", "bleib", "dir", "treu", ".", "Es", "wech\u00b7sel\u00b7ten", "die", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Es kamen Jahre, Monde, Tag und Nacht.", "tokens": ["Es", "ka\u00b7men", "Jah\u00b7re", ",", "Mon\u00b7de", ",", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie waren Bilder einstger Ewigkeiten,", "tokens": ["Sie", "wa\u00b7ren", "Bil\u00b7der", "einst\u00b7ger", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und du hast sie verst\u00e4ndlich mir gemacht.", "tokens": ["Und", "du", "hast", "sie", "ver\u00b7st\u00e4nd\u00b7lich", "mir", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich leb ein \u00e4u\u00dfres und ein innres Leben,", "tokens": ["Ich", "leb", "ein", "\u00e4u\u00df\u00b7res", "und", "ein", "inn\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eins mehr f\u00fcr hier, das andre mehr f\u00fcr dort,", "tokens": ["Eins", "mehr", "f\u00fcr", "hier", ",", "das", "and\u00b7re", "mehr", "f\u00fcr", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADV", "$,", "PRELS", "PIS", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und soll ich beiden Ziel und Richtung geben,", "tokens": ["Und", "soll", "ich", "bei\u00b7den", "Ziel", "und", "Rich\u00b7tung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So find ich nur durch dich das rechte Wort.", "tokens": ["So", "find", "ich", "nur", "durch", "dich", "das", "rech\u00b7te", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bleib dir treu, du herrlicher Gedanke,", "tokens": ["Ich", "bleib", "dir", "treu", ",", "du", "herr\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Gott auch meine kleine Welt regiert.", "tokens": ["Da\u00df", "Gott", "auch", "mei\u00b7ne", "klei\u00b7ne", "Welt", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor dir f\u00e4llt jede, auch die letzte Schranke,", "tokens": ["Vor", "dir", "f\u00e4llt", "je\u00b7de", ",", "auch", "die", "letz\u00b7te", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An welcher selbst der Muth den Muth verliert.", "tokens": ["An", "wel\u00b7cher", "selbst", "der", "Muth", "den", "Muth", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du warst die einzge Leuchte mir auf Erden", "tokens": ["Du", "warst", "die", "einz\u00b7ge", "Leuch\u00b7te", "mir", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wirst sie mir f\u00fcr ewig, ewig sein.", "tokens": ["Und", "wirst", "sie", "mir", "f\u00fcr", "e\u00b7wig", ",", "e\u00b7wig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPR", "ADJD", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer darnach trachtet, selig einst zu werden,", "tokens": ["Wer", "dar\u00b7nach", "trach\u00b7tet", ",", "se\u00b7lig", "einst", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVFIN", "$,", "ADJD", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der wird es nur durch dich, durch dich allein.", "tokens": ["Der", "wird", "es", "nur", "durch", "dich", ",", "durch", "dich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$,", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}