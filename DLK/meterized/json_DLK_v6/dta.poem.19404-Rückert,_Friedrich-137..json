{"dta.poem.19404": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "137.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst, es wankt dein Kind, und, statt ihm beizuspringen,", "tokens": ["Du", "siehst", ",", "es", "wankt", "dein", "Kind", ",", "und", ",", "statt", "ihm", "bei\u00b7zu\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "$,", "KOUI", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siehst du mit Angst ihm zu, wie es ihm wird gelingen.", "tokens": ["Siehst", "du", "mit", "Angst", "ihm", "zu", ",", "wie", "es", "ihm", "wird", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wird es im Gleichgewicht sich halten, wird es fallen?", "tokens": ["Wird", "es", "im", "Gleich\u00b7ge\u00b7wicht", "sich", "hal\u00b7ten", ",", "wird", "es", "fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PRF", "VVINF", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dar\u00fcber l\u00e4ssest du die Zeit der H\u00fclf' entwallen.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "l\u00e4s\u00b7sest", "du", "die", "Zeit", "der", "H\u00fcl\u00b7f'", "ent\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Roll' ist ungeschickt, die du dabei gespielt;", "tokens": ["Die", "Roll'", "ist", "un\u00b7ge\u00b7schickt", ",", "die", "du", "da\u00b7bei", "ge\u00b7spielt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gefallen w\u00e4r' es, wenn es nicht sein Engel hielt.", "tokens": ["Ge\u00b7fal\u00b7len", "w\u00e4r'", "es", ",", "wenn", "es", "nicht", "sein", "En\u00b7gel", "hielt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch tr\u00f6ste dich, wer wei\u00df du h\u00e4ttest, wohlbeflissen", "tokens": ["Doch", "tr\u00f6s\u00b7te", "dich", ",", "wer", "wei\u00df", "du", "h\u00e4t\u00b7test", ",", "wohl\u00b7be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "VAFIN", "$,", "PWAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eingreifend, es villeicht erst hin zum Sturz gerissen.", "tokens": ["Ein\u00b7grei\u00b7fend", ",", "es", "vil\u00b7leicht", "erst", "hin", "zum", "Sturz", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "ADV", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es fiel nicht, danke Gott. Fiel es, so machtest du", "tokens": ["Es", "fiel", "nicht", ",", "dan\u00b7ke", "Gott", ".", "Fiel", "es", ",", "so", "mach\u00b7test", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "NN", "$.", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vorw\u00fcrfe dir mit Recht; nun ist kein Grund dazu.", "tokens": ["Vor\u00b7w\u00fcr\u00b7fe", "dir", "mit", "Recht", ";", "nun", "ist", "kein", "Grund", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$.", "ADV", "VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}