{"textgrid.poem.26415": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Venusine wird Frau eines Sergeanten, wobei sie", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venus wandelt n\u00e4chtlich", "tokens": ["Ve\u00b7nus", "wan\u00b7delt", "n\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Drau\u00dfen bei Kasernen,", "tokens": ["Drau\u00b7\u00dfen", "bei", "Ka\u00b7ser\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dirnenhaft gekleidet,", "tokens": ["Dir\u00b7nen\u00b7haft", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Unter den Laternen.", "tokens": ["Un\u00b7ter", "den", "La\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "F\u00e4hnrich und Sergeanten", "tokens": ["F\u00e4hn\u00b7rich", "und", "Ser\u00b7ge\u00b7an\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich mit S\u00e4belrasseln", "tokens": ["Sich", "mit", "S\u00e4\u00b7bel\u00b7ras\u00b7seln"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Flei\u00dfig nach ihr wandten.", "tokens": ["Flei\u00b7\u00dfig", "nach", "ihr", "wand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und bald folgt ihr Einer", "tokens": ["Und", "bald", "folgt", "ihr", "Ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In die Seitengassen,", "tokens": ["In", "die", "Sei\u00b7ten\u00b7gas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dr\u00fcckt ihr fest die H\u00fcften,", "tokens": ["Dr\u00fcckt", "ihr", "fest", "die", "H\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kann sie nicht mehr lassen.", "tokens": ["Kann", "sie", "nicht", "mehr", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Venus war nicht b\u00f6se,", "tokens": ["Ve\u00b7nus", "war", "nicht", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Freut sich seiner H\u00e4nde", "tokens": ["Freut", "sich", "sei\u00b7ner", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und des Schnurrbarts Gr\u00f6\u00dfe.", "tokens": ["Und", "des", "Schnurr\u00b7barts", "Gr\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ehrlich sind Soldaten,", "tokens": ["Ehr\u00b7lich", "sind", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Weil sie gradaus lieben,", "tokens": ["Weil", "sie", "gra\u00b7daus", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deshalb ist die Venus", "tokens": ["Des\u00b7halb", "ist", "die", "Ve\u00b7nus"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auch bei ihm geblieben,", "tokens": ["Auch", "bei", "ihm", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Tat mit Lust sich schenken", "tokens": ["Tat", "mit", "Lust", "sich", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PRF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jede Nacht von Neuem,", "tokens": ["Je\u00b7de", "Nacht", "von", "Neu\u00b7em", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ohne jed' Bedenken.", "tokens": ["Oh\u00b7ne", "jed'", "Be\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Der Sergeant bald sagte:", "tokens": ["Der", "Ser\u00b7ge\u00b7ant", "bald", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nie mehr wollt er scheiden.", "tokens": ["Nie", "mehr", "wollt", "er", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heirat w\u00e4r das Beste", "tokens": ["Hei\u00b7rat", "w\u00e4r", "das", "Bes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zwischen ihnen beiden.", "tokens": ["Zwi\u00b7schen", "ih\u00b7nen", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Venus, unter Lachen,", "tokens": ["Ve\u00b7nus", ",", "un\u00b7ter", "La\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Freut sich seiner Treue", "tokens": ["Freut", "sich", "sei\u00b7ner", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und tat Hochzeit machen.", "tokens": ["Und", "tat", "Hoch\u00b7zeit", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Beim Kasernenhofe", "tokens": ["Beim", "Ka\u00b7ser\u00b7nen\u00b7ho\u00b7fe"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "\u00d6ffnet Venusine,", "tokens": ["\u00d6ff\u00b7net", "Ve\u00b7nu\u00b7si\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Als die Frau Sergeantin,", "tokens": ["Als", "die", "Frau", "Ser\u00b7ge\u00b7an\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Eine Schnapskantine", "tokens": ["Ei\u00b7ne", "Schnaps\u00b7kan\u00b7ti\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Und lebt ohne Wolke", "tokens": ["Und", "lebt", "oh\u00b7ne", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lustig so drei Tage", "tokens": ["Lus\u00b7tig", "so", "drei", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "CARD", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Beim Soldatenvolke.", "tokens": ["Beim", "Sol\u00b7da\u00b7ten\u00b7vol\u00b7ke", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "An dem dritten Abend", "tokens": ["An", "dem", "drit\u00b7ten", "A\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Macht, zur Mittnachtstunde", "tokens": ["Macht", ",", "zur", "Mitt\u00b7nachts\u00b7tun\u00b7de"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der Sergeant im Hause", "tokens": ["Der", "Ser\u00b7ge\u00b7ant", "im", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch einmal die Runde,", "tokens": ["Noch", "ein\u00b7mal", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Als er in den Kellern", "tokens": ["Als", "er", "in", "den", "Kel\u00b7lern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gl\u00e4serklingen h\u00f6rte", "tokens": ["Gl\u00e4\u00b7ser\u00b7klin\u00b7gen", "h\u00f6r\u00b7te"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und Ger\u00e4usch von Tellern.", "tokens": ["Und", "Ge\u00b7r\u00e4usch", "von", "Tel\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbvenusine!\u00ab rief er;", "tokens": ["\u00bb", "ve\u00b7nu\u00b7si\u00b7ne", "!", "\u00ab", "rief", "er", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "FM.la", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "----+-", "measure": "unknown.measure.single"}, "line.2": {"text": "Ist ans Bett geschlichen.", "tokens": ["Ist", "ans", "Bett", "ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch das Bett stand einsam \u2013", "tokens": ["Doch", "das", "Bett", "stand", "ein\u00b7sam", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Venus war entwichen.", "tokens": ["Ve\u00b7nus", "war", "ent\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Der Sergeant, der blasse,", "tokens": ["Der", "Ser\u00b7ge\u00b7ant", ",", "der", "blas\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eilt und sieht im Keller", "tokens": ["Eilt", "und", "sieht", "im", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zwei bei einem Fasse.", "tokens": ["Zwei", "bei", "ei\u00b7nem", "Fas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Eine Kerze brannte.", "tokens": ["Ei\u00b7ne", "Ker\u00b7ze", "brann\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Venus sa\u00df im Schoo\u00dfe", "tokens": ["Ve\u00b7nus", "sa\u00df", "im", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.3": {"text": "Einem Mann. Sie tranken.", "tokens": ["Ei\u00b7nem", "Mann", ".", "Sie", "tran\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Er war ohne Hose,", "tokens": ["Er", "war", "oh\u00b7ne", "Ho\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.16": {"line.1": {"text": "Ohne West' und Kleider.", "tokens": ["Oh\u00b7ne", "West'", "und", "Klei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der Sergeant, er stolpert", "tokens": ["Der", "Ser\u00b7ge\u00b7ant", ",", "er", "stol\u00b7pert"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und verr\u00e4t sich leider.", "tokens": ["Und", "ver\u00b7r\u00e4t", "sich", "lei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Fluchend richtet er sich", "tokens": ["Flu\u00b7chend", "rich\u00b7tet", "er", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wieder auf die Beine.", "tokens": ["Wie\u00b7der", "auf", "die", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da stand Venus vor ihm", "tokens": ["Da", "stand", "Ve\u00b7nus", "vor", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "L\u00e4chelnd und alleine,", "tokens": ["L\u00e4\u00b7chelnd", "und", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Ihr Besuch verschwunden. \u2013", "tokens": ["Ihr", "Be\u00b7such", "ver\u00b7schwun\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nur der Teufel hatte", "tokens": ["Nur", "der", "Teu\u00b7fel", "hat\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "So schnell fortgefunden.", "tokens": ["So", "schnell", "fort\u00b7ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-++-+-", "measure": "unknown.measure.tri"}}, "stanza.19": {"line.1": {"text": "\u00bbstirb!\u00ab schrie ohn' Besinnen", "tokens": ["\u00bb", "stirb", "!", "\u00ab", "schrie", "ohn'", "Be\u00b7sin\u00b7nen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "$.", "$(", "VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der Sergeant betrogen.", "tokens": ["Der", "Ser\u00b7ge\u00b7ant", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und er hat den S\u00e4bel", "tokens": ["Und", "er", "hat", "den", "S\u00e4\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "W\u00fctend blank gezogen.", "tokens": ["W\u00fc\u00b7tend", "blank", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Venus, immer l\u00e4chelnd,", "tokens": ["Ve\u00b7nus", ",", "im\u00b7mer", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text