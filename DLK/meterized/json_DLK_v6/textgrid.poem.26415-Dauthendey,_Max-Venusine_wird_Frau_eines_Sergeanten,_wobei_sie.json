": "trochaic.tri"}}, "stanza.29": {"line.1": {"text": "Tiefe Trauer zeigt er,", "tokens": ["Tie\u00b7fe", "Trau\u00b7er", "zeigt", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Doch zeigt keine Reue,", "tokens": ["Doch", "zeigt", "kei\u00b7ne", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Legt sich auf sein Lager", "tokens": ["Legt", "sich", "auf", "sein", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und schl\u00e4ft ein aufs Neue.", "tokens": ["Und", "schl\u00e4ft", "ein", "aufs", "Neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPRART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.30": {"line.1": {"text": "Venus aber, l\u00e4chelnd,", "tokens": ["Ve\u00b7nus", "a\u00b7ber", ",", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist zur\u00fcckgekommen,", "tokens": ["Ist", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wieder hemdenf\u00e4chelnd;", "tokens": ["Wie\u00b7der", "hem\u00b7den\u00b7f\u00e4\u00b7chelnd", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.31": {"line.1": {"text": "Legt sich ihm zur Seite,", "tokens": ["Legt", "sich", "ihm", "zur", "Sei\u00b7te", 