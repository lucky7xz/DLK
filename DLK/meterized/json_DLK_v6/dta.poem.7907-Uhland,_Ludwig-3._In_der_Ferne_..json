{"dta.poem.7907": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.  In der Ferne .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Will ruhen unter den B\u00e4umen hier,", "tokens": ["Will", "ru\u00b7hen", "un\u00b7ter", "den", "B\u00e4u\u00b7men", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die V\u00f6glein h\u00f6r\u2019 ich so gerne.", "tokens": ["Die", "V\u00f6\u00b7glein", "h\u00f6r'", "ich", "so", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie singet ihr so zum Herzen mir!", "tokens": ["Wie", "sin\u00b7get", "ihr", "so", "zum", "Her\u00b7zen", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von unsrer Liebe was wisset ihr", "tokens": ["Von", "uns\u00b7rer", "Lie\u00b7be", "was", "wis\u00b7set", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PWS", "VVFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In dieser weiten Ferne?", "tokens": ["In", "die\u00b7ser", "wei\u00b7ten", "Fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Will ruhen hier an des Baches Rand,", "tokens": ["Will", "ru\u00b7hen", "hier", "an", "des", "Ba\u00b7ches", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo duftige Bl\u00fcmlein sprie\u00dfen.", "tokens": ["Wo", "duf\u00b7ti\u00b7ge", "Bl\u00fcm\u00b7lein", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer hat euch, Bl\u00fcmlein, hieher gesandt?", "tokens": ["Wer", "hat", "euch", ",", "Bl\u00fcm\u00b7lein", ",", "hie\u00b7her", "ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "PAV", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seyd ihr ein herzliches Liebespfand", "tokens": ["Seyd", "ihr", "ein", "herz\u00b7li\u00b7ches", "Lie\u00b7be\u00b7spfand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Aus der Ferne von meiner S\u00fc\u00dfen?", "tokens": ["Aus", "der", "Fer\u00b7ne", "von", "mei\u00b7ner", "S\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}