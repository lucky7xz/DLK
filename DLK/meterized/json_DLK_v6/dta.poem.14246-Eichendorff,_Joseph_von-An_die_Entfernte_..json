{"dta.poem.14246": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch kein Licht noch sah ich grauen,", "tokens": ["Doch", "kein", "Licht", "noch", "sah", "ich", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen durch die n\u00e4chtlich lauen", "tokens": ["Drau\u00b7\u00dfen", "durch", "die", "n\u00e4cht\u00b7lich", "lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00e4ume und die Wolken flogen,", "tokens": ["R\u00e4u\u00b7me", "und", "die", "Wol\u00b7ken", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Seele, mitgezogen,", "tokens": ["Da\u00df", "die", "See\u00b7le", ",", "mit\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gern versank im tiefen Schauen \u2014", "tokens": ["Gern", "ver\u00b7sank", "im", "tie\u00b7fen", "Schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unten dann die weite Runde,", "tokens": ["Un\u00b7ten", "dann", "die", "wei\u00b7te", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schl\u00f6sser gl\u00e4nzend fern erhoben,", "tokens": ["Schl\u00f6s\u00b7ser", "gl\u00e4n\u00b7zend", "fern", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nachtigallen aus dem Grunde,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "aus", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Alles wie im Traum verwoben,", "tokens": ["Al\u00b7les", "wie", "im", "Traum", "ver\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mit einander still im Bunde.", "tokens": ["Mit", "ein\u00b7an\u00b7der", "still", "im", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}