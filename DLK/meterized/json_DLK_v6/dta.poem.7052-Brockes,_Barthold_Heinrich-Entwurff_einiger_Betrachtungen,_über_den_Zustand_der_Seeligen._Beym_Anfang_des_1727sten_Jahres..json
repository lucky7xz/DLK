{"dta.poem.7052": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer aber ist nun, der begreiffen mag", "tokens": ["Wer", "a\u00b7ber", "ist", "nun", ",", "der", "be\u00b7greif\u00b7fen", "mag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "ADV", "$,", "PRELS", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Welch\u2019 eine Freude, welche Wonne", "tokens": ["Welch'", "ei\u00b7ne", "Freu\u00b7de", ",", "wel\u00b7che", "Won\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seelen dann entz\u00fccken m\u00fcssen,", "tokens": ["Die", "See\u00b7len", "dann", "ent\u00b7z\u00fc\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn aller Lichter Quell, wenn aller Sonnen Sonne", "tokens": ["Wenn", "al\u00b7ler", "Lich\u00b7ter", "Quell", ",", "wenn", "al\u00b7ler", "Son\u00b7nen", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "NN", "$,", "KOUS", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An etwas, welches mehr, als Demant und Rubinen,", "tokens": ["An", "et\u00b7was", ",", "wel\u00b7ches", "mehr", ",", "als", "De\u00b7mant", "und", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Geschickt, so reinen Glantz deu Blicken", "tokens": ["Ge\u00b7schickt", ",", "so", "rei\u00b7nen", "Glantz", "deu", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gemildert, wieder zuzuschicken,", "tokens": ["Ge\u00b7mil\u00b7dert", ",", "wie\u00b7der", "zu\u00b7zu\u00b7schi\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In seel\u2019gem Schimmer strahlt!", "tokens": ["In", "seel'\u00b7gem", "Schim\u00b7mer", "strahlt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}