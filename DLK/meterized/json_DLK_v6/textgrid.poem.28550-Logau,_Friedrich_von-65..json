{"textgrid.poem.28550": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Trogloditen See wird dreymal s\u00fcsse t\u00e4glich", "tokens": ["Der", "Trog\u00b7lo\u00b7di\u00b7ten", "See", "wird", "drey\u00b7mal", "s\u00fcs\u00b7se", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und herbe dreymal auch. Was in der Welt ist kl\u00e4glich,", "tokens": ["Und", "her\u00b7be", "drey\u00b7mal", "auch", ".", "Was", "in", "der", "Welt", "ist", "kl\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$.", "PWS", "APPR", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00fcmmt immer eh und mehr, als das, was lieblich heist,", "tokens": ["K\u00fcmmt", "im\u00b7mer", "eh", "und", "mehr", ",", "als", "das", ",", "was", "lieb\u00b7lich", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "KON", "ADV", "$,", "KOUS", "PDS", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df bittres dreymal sich, eh einmal s\u00fcsses, weist.", "tokens": ["Da\u00df", "bitt\u00b7res", "drey\u00b7mal", "sich", ",", "eh", "ein\u00b7mal", "s\u00fcs\u00b7ses", ",", "weist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "PRF", "$,", "KOUS", "ADV", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}