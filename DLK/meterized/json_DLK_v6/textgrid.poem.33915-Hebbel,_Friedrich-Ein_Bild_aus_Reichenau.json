{"textgrid.poem.33915": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Bild aus Reichenau", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einer Blume, roth und brennend, sa\u00df", "tokens": ["Auf", "ei\u00b7ner", "Blu\u00b7me", ",", "roth", "und", "bren\u00b7nend", ",", "sa\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Schmetterling, der ihren Honig sog,", "tokens": ["Ein", "Schmet\u00b7ter\u00b7ling", ",", "der", "ih\u00b7ren", "Ho\u00b7nig", "sog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sich in seiner Wollust so verga\u00df,", "tokens": ["Und", "sich", "in", "sei\u00b7ner", "Wol\u00b7lust", "so", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er vor mir nicht einmal weiter flog.", "tokens": ["Da\u00df", "er", "vor", "mir", "nicht", "ein\u00b7mal", "wei\u00b7ter", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Ich wollte seh'n, wie s\u00fc\u00df die Blume war,", "tokens": ["Ich", "woll\u00b7te", "seh'n", ",", "wie", "s\u00fc\u00df", "die", "Blu\u00b7me", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und brach sie ab: er blieb an seinem Ort;", "tokens": ["Und", "brach", "sie", "ab", ":", "er", "blieb", "an", "sei\u00b7nem", "Ort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich flocht sie der Geliebten in das Haar:", "tokens": ["Ich", "flocht", "sie", "der", "Ge\u00b7lieb\u00b7ten", "in", "das", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er sog, wie aufgel\u00f6s't in Wonne, fort!", "tokens": ["Er", "sog", ",", "wie", "auf\u00b7ge\u00b7l\u00f6s't", "in", "Won\u00b7ne", ",", "fort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "VVPP", "APPR", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}