{"textgrid.poem.32595": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "10. Hochzeit-Fragen", "genre": "verse", "period": "N.A.", "pub_year": 1675, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Jungferschafft? Ein Quintgen Hudeley,", "tokens": ["Was", "ist", "die", "Jung\u00b7fer\u00b7schafft", "?", "Ein", "Quint\u00b7gen", "Hu\u00b7de\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zehnmahl schwerer ist, als sonst ein Centner Bley.", "tokens": ["Das", "zehn\u00b7mahl", "schwe\u00b7rer", "ist", ",", "als", "sonst", "ein", "Cent\u00b7ner", "Bley", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch was ist eine Braut? Ein Ding, das gerne k\u00fcst,", "tokens": ["Doch", "was", "ist", "ei\u00b7ne", "Braut", "?", "Ein", "Ding", ",", "das", "ger\u00b7ne", "k\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und weder eine Frau noch eine Jungfer ist.", "tokens": ["Und", "we\u00b7der", "ei\u00b7ne", "Frau", "noch", "ei\u00b7ne", "Jung\u00b7fer", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was ist ein Br\u00e4utigam? Ein Mann und nicht ein Mann,", "tokens": ["Was", "ist", "ein", "Br\u00e4u\u00b7ti\u00b7gam", "?", "Ein", "Mann", "und", "nicht", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NE", "$.", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieweil er sich noch nicht der Mannheit r\u00fchmen kan.", "tokens": ["Die\u00b7weil", "er", "sich", "noch", "nicht", "der", "Mann\u00b7heit", "r\u00fch\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was mag das Jawort seyn? Es ist das erste Spiel,", "tokens": ["Was", "mag", "das", "Ja\u00b7wort", "seyn", "?", "Es", "ist", "das", "ers\u00b7te", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VAINF", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann man das Leder nun mit Ernst verkauffen will.", "tokens": ["Wann", "man", "das", "Le\u00b7der", "nun", "mit", "Ernst", "ver\u00b7kauf\u00b7fen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADV", "APPR", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sagt, was Verl\u00f6bni\u00df ist? Ein angestelltes Fest,", "tokens": ["Sagt", ",", "was", "Ver\u00b7l\u00f6b\u00b7ni\u00df", "ist", "?", "Ein", "an\u00b7ge\u00b7stell\u00b7tes", "Fest", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "NN", "VAFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Davor man in der Kirch am letzten bitten l\u00e4st.", "tokens": ["Da\u00b7vor", "man", "in", "der", "Kirch", "am", "letz\u00b7ten", "bit\u00b7ten", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "APPR", "ART", "NN", "APPRART", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was ist das Auffgebot? Es ist ein sp\u00e4ter Flei\u00df,", "tokens": ["Was", "ist", "das", "Auff\u00b7ge\u00b7bot", "?", "Es", "ist", "ein", "sp\u00e4\u00b7ter", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Darinn erzehlet wird, was sonst ein jeder wei\u00df.", "tokens": ["Da\u00b7rinn", "er\u00b7zeh\u00b7let", "wird", ",", "was", "sonst", "ein", "je\u00b7der", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "$,", "PRELS", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was ist das Hochzeit-Fest? Es ist ein warmes Bad,", "tokens": ["Was", "ist", "das", "Hoch\u00b7zeit\u00b7Fest", "?", "Es", "ist", "ein", "war\u00b7mes", "Bad", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Darinnen Wirth und Gast was auszuschwietzen hat.", "tokens": ["Da\u00b7rin\u00b7nen", "Wirth", "und", "Gast", "was", "aus\u00b7zu\u00b7schwiet\u00b7zen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PIS", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was mag die Trauung seyn? Die Zeit, da man verehrt,", "tokens": ["Was", "mag", "die", "Trau\u00b7ung", "seyn", "?", "Die", "Zeit", ",", "da", "man", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VAINF", "$.", "ART", "NN", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was einem sonst mit Recht und Ehren zugeh\u00f6rt.", "tokens": ["Was", "ei\u00b7nem", "sonst", "mit", "Recht", "und", "Eh\u00b7ren", "zu\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was ist ein Junggesell? Ein Affe, der das Spiel,", "tokens": ["Was", "ist", "ein", "Jung\u00b7ge\u00b7sell", "?", "Ein", "Af\u00b7fe", ",", "der", "das", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dem Herren Br\u00e4utigam flugs abstudiren will.", "tokens": ["Dem", "Her\u00b7ren", "Br\u00e4u\u00b7ti\u00b7gam", "flugs", "ab\u00b7stu\u00b7di\u00b7ren", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was ist ein Jungfergen? Es ist ein Gl\u00e4\u00dfgen Wein,", "tokens": ["Was", "ist", "ein", "Jung\u00b7fer\u00b7gen", "?", "Es", "ist", "ein", "Gl\u00e4\u00df\u00b7gen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das niemand trincken darff, wann alle durstig seyn.", "tokens": ["Das", "nie\u00b7mand", "trin\u00b7cken", "darff", ",", "wann", "al\u00b7le", "durs\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVINF", "VMFIN", "$,", "PWAV", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was ist die erste Nacht? Die Hochzeit in der That,", "tokens": ["Was", "ist", "die", "ers\u00b7te", "Nacht", "?", "Die", "Hoch\u00b7zeit", "in", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da manche mehr gehofft, als sie zu kosten hat.", "tokens": ["Da", "man\u00b7che", "mehr", "ge\u00b7hofft", ",", "als", "sie", "zu", "kos\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was ist die ander Nacht? Ein s\u00fcsser Ubertru\u00df,", "tokens": ["Was", "ist", "die", "an\u00b7der", "Nacht", "?", "Ein", "s\u00fcs\u00b7ser", "U\u00b7bert\u00b7ru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da man die alte Schuld von gestern zahlen mu\u00df.", "tokens": ["Da", "man", "die", "al\u00b7te", "Schuld", "von", "ge\u00b7stern", "zah\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPR", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was ist die dritte Nacht? Es ist die Rennebahn,", "tokens": ["Was", "ist", "die", "drit\u00b7te", "Nacht", "?", "Es", "ist", "die", "Ren\u00b7ne\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da man auffh\u00f6ren mu\u00df, wann mans am besten kan.", "tokens": ["Da", "man", "auff\u00b7h\u00f6\u00b7ren", "mu\u00df", ",", "wann", "mans", "am", "bes\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,", "PWAV", "PIS", "PTKA", "ADJD", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was ist die Jungefrau? Es ist ein loser Sack,", "tokens": ["Was", "ist", "die", "Jun\u00b7ge\u00b7frau", "?", "Es", "ist", "ein", "lo\u00b7ser", "Sack", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der in der Compagnie auch garstig reden mag.", "tokens": ["Der", "in", "der", "Com\u00b7pag\u00b7nie", "auch", "gars\u00b7tig", "re\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Was ist der Ehstand selbst? Es ist ein Vogel-Hau\u00df,", "tokens": ["Was", "ist", "der", "Eh\u00b7stand", "selbst", "?", "Es", "ist", "ein", "Vo\u00b7gel\u00b7Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die draussen wollen nein, die drinne wollen rau\u00df.", "tokens": ["Die", "draus\u00b7sen", "wol\u00b7len", "nein", ",", "die", "drin\u00b7ne", "wol\u00b7len", "rau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "PTKANT", "$,", "PRELS", "VVFIN", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was ist das erste Kind? ein Schmertz, wenns bald bekleibt.", "tokens": ["Was", "ist", "das", "ers\u00b7te", "Kind", "?", "ein", "Schmertz", ",", "wenns", "bald", "be\u00b7kleibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "$,", "KOUS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein Schimpf, wenns zeitlich kommt, ein Hohn, wenns aussen bleibt.", "tokens": ["Ein", "Schimpf", ",", "wenns", "zeit\u00b7lich", "kommt", ",", "ein", "Hohn", ",", "wenns", "aus\u00b7sen", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADJD", "VVFIN", "$,", "ART", "NN", "$,", "KOUS", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was ist das andre Kind? es ist ein guter Rath", "tokens": ["Was", "ist", "das", "and\u00b7re", "Kind", "?", "es", "ist", "ein", "gu\u00b7ter", "Rath"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vor Leute, welche man gern zu Gevattern hat.", "tokens": ["Vor", "Leu\u00b7te", ",", "wel\u00b7che", "man", "gern", "zu", "Ge\u00b7vat\u00b7tern", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was ist das dritte Kind? ein ungebetner Gast,", "tokens": ["Was", "ist", "das", "drit\u00b7te", "Kind", "?", "ein", "un\u00b7ge\u00b7bet\u00b7ner", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Des Vatters Geld-verderb, der Mutter Uberlast.", "tokens": ["Des", "Vat\u00b7ters", "Geld\u00b7ver\u00b7derb", ",", "der", "Mut\u00b7ter", "U\u00b7ber\u00b7last", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was ist das vierdte Kind? es ist ein gutes Ziel,", "tokens": ["Was", "ist", "das", "vierd\u00b7te", "Kind", "?", "es", "ist", "ein", "gu\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nach diesen sage man, zuviel, zuviel, zuviel.", "tokens": ["Nach", "die\u00b7sen", "sa\u00b7ge", "man", ",", "zu\u00b7viel", ",", "zu\u00b7viel", ",", "zu\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "$,", "PIS", "$,", "PIS", "$,", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was ist das f\u00fcnffte Kind? mit diesen heist es wol,", "tokens": ["Was", "ist", "das", "f\u00fcnff\u00b7te", "Kind", "?", "mit", "die\u00b7sen", "heist", "es", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "APPR", "PDAT", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich esse was mir schmeckt, und leide was ich sol.", "tokens": ["Ich", "es\u00b7se", "was", "mir", "schmeckt", ",", "und", "lei\u00b7de", "was", "ich", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was sind die S\u00f6hnigen? Ein Volck das nichts erwirbt,", "tokens": ["Was", "sind", "die", "S\u00f6h\u00b7ni\u00b7gen", "?", "Ein", "Volck", "das", "nichts", "er\u00b7wirbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und da des Beutels Krafft, als an der Schwindsucht, stirbt.", "tokens": ["Und", "da", "des", "Beu\u00b7tels", "Krafft", ",", "als", "an", "der", "Schwind\u00b7sucht", ",", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Was sind die T\u00f6chtergen? die kosten wenig Geld,", "tokens": ["Was", "sind", "die", "T\u00f6ch\u00b7ter\u00b7gen", "?", "die", "kos\u00b7ten", "we\u00b7nig", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Bi\u00df alle Pestilentz auff ihre Hochzeit f\u00e4llt.", "tokens": ["Bi\u00df", "al\u00b7le", "Pes\u00b7ti\u00b7lentz", "auff", "ih\u00b7re", "Hoch\u00b7zeit", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Was ist die beste Lust? Wann man nicht viel begehrt,", "tokens": ["Was", "ist", "die", "bes\u00b7te", "Lust", "?", "Wann", "man", "nicht", "viel", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "PWAV", "PIS", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und wenn das Wenige fein gut und lange w\u00e4hrt.", "tokens": ["Und", "wenn", "das", "We\u00b7ni\u00b7ge", "fein", "gut", "und", "lan\u00b7ge", "w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIAT", "ADJD", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}