{"textgrid.poem.44431": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Alter ist f\u00fcrwahr beklagenswert \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Alter ist f\u00fcrwahr beklagenswert \u2013", "tokens": ["Das", "Al\u00b7ter", "ist", "f\u00fcr\u00b7wahr", "be\u00b7kla\u00b7gens\u00b7wert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das wu\u00dft ich lang, doch heute f\u00fchl ichs erst.", "tokens": ["Das", "wu\u00dft", "ich", "lang", ",", "doch", "heu\u00b7te", "f\u00fchl", "ichs", "erst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indes die andern j\u00fcngeren Genossen", "tokens": ["In\u00b7des", "die", "an\u00b7dern", "j\u00fcn\u00b7ge\u00b7ren", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Herzgef\u00fchl in frischem Wirken zeigten,", "tokens": ["Ihr", "Herz\u00b7ge\u00b7f\u00fchl", "in", "fri\u00b7schem", "Wir\u00b7ken", "zeig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bleibt mir ein W\u00fcnschen blo\u00df, nur d\u00fcrftig Wollen.", "tokens": ["Bleibt", "mir", "ein", "W\u00fcn\u00b7schen", "blo\u00df", ",", "nur", "d\u00fcrf\u00b7tig", "Wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der alte Kopf beh\u00e4lt nun schwer das Wort,", "tokens": ["Der", "al\u00b7te", "Kopf", "be\u00b7h\u00e4lt", "nun", "schwer", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das andere ihm legten in den Mund;", "tokens": ["Das", "an\u00b7de\u00b7re", "ihm", "leg\u00b7ten", "in", "den", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Allein das Wort, das aus des Herzens Grund", "tokens": ["Al\u00b7lein", "das", "Wort", ",", "das", "aus", "des", "Her\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sich auf die Lippen dr\u00e4ngt, das spricht sich leicht,", "tokens": ["Sich", "auf", "die", "Lip\u00b7pen", "dr\u00e4ngt", ",", "das", "spricht", "sich", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und m\u00f6g es euch gen\u00fcgen und gefallen:", "tokens": ["Und", "m\u00f6g", "es", "euch", "ge\u00b7n\u00fc\u00b7gen", "und", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}}}}