{"dta.poem.23308": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zu fernerem Bedenken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ezu fernerem Bedenken!\u201c", "tokens": ["\u201e", "zu", "fer\u00b7ne\u00b7rem", "Be\u00b7den\u00b7ken", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du altes Reichstagswort!", "tokens": ["Du", "al\u00b7tes", "Reichs\u00b7tags\u00b7wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Reichstag ist vergangen,", "tokens": ["Der", "Reichs\u00b7tag", "ist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Bund hat angefangen,", "tokens": ["Der", "Bund", "hat", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du aber lebst noch fort.", "tokens": ["Du", "a\u00b7ber", "lebst", "noch", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im ferneren Bedenken", "tokens": ["Im", "fer\u00b7ne\u00b7ren", "Be\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlief ein das deutsche Reich:", "tokens": ["Schlief", "ein", "das", "deut\u00b7sche", "Reich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weil so s\u00fc\u00df sein Schlummer,", "tokens": ["Und", "weil", "so", "s\u00fc\u00df", "sein", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ganz ohne Sorg' und Kummer,", "tokens": ["Ganz", "oh\u00b7ne", "Sor\u00b7g'", "und", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So thut's der Bund ihm gleich.", "tokens": ["So", "thut's", "der", "Bund", "ihm", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von fernerem Bedenken", "tokens": ["Von", "fer\u00b7ne\u00b7rem", "Be\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erwach', o deutscher Bund!", "tokens": ["Er\u00b7wach'", ",", "o", "deut\u00b7scher", "Bund", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gieb etwas von Erhebni\u00df,", "tokens": ["Gieb", "et\u00b7was", "von", "Er\u00b7heb\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein freudiges Ergebni\u00df", "tokens": ["Ein", "freu\u00b7di\u00b7ges", "Er\u00b7geb\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den armen Deutschen kund!", "tokens": ["Den", "ar\u00b7men", "Deut\u00b7schen", "kund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}