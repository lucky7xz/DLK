{"dta.poem.23804": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Der 139. Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "HeRR du erforschest mich. Mein Ruhen und Be-\nwegen/", "tokens": ["HeRR", "du", "er\u00b7for\u00b7schest", "mich", ".", "Mein", "Ru\u00b7hen", "und", "Be", "we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "KON", "TRUNC", "APPR", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Ist besser dir als mir bewust.", "tokens": ["Ist", "bes\u00b7ser", "dir", "als", "mir", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du siehst es/ wenn in meiner Brust/", "tokens": ["Du", "siehst", "es", "/", "wenn", "in", "mei\u00b7ner", "Brust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wie der Wellen Sturm/ sich meine L\u00fcste regen.", "tokens": ["So", "wie", "der", "Wel\u00b7len", "Sturm", "/", "sich", "mei\u00b7ne", "L\u00fcs\u00b7te", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "$(", "PRF", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eh\u2019 mir ein Wort entf\u00e4hrt/ ist dir es schon ", "tokens": ["Eh'", "mir", "ein", "Wort", "ent\u00b7f\u00e4hrt", "/", "ist", "dir", "es", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "VAFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und was ich denck\u2019 und thu\u2019/ das steht in deiner Hand.", "tokens": ["Und", "was", "ich", "denck", "und", "thu'", "/", "das", "steht", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$(", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Allmacht! die kein Mensch auf Erden kan verstehen/", "tokens": ["O", "All\u00b7macht", "!", "die", "kein", "Mensch", "auf", "Er\u00b7den", "kan", "ver\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "PIAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ist der Ort der mich versteckt/", "tokens": ["Wo", "ist", "der", "Ort", "der", "mich", "ver\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den nicht sofort dein Geist entdenckt?", "tokens": ["Den", "nicht", "so\u00b7fort", "dein", "Geist", "ent\u00b7denckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nt\u2019 ich mich schwingen auf zu den gestirnten H\u00f6hen/", "tokens": ["K\u00f6nt'", "ich", "mich", "schwin\u00b7gen", "auf", "zu", "den", "ge\u00b7stirn\u00b7ten", "H\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVFIN", "APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein GOtt so bist du da. F\u00fchr ich zur H\u00f6llen Grund/", "tokens": ["Mein", "Gott", "so", "bist", "du", "da", ".", "F\u00fchr", "ich", "zur", "H\u00f6l\u00b7len", "Grund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PPER", "ADV", "$.", "NN", "PPER", "APPRART", "NN", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da machest du dich auch mit Schreck und Rache kund.", "tokens": ["Da", "ma\u00b7chest", "du", "dich", "auch", "mit", "Schreck", "und", "Ra\u00b7che", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "K\u00f6nt\u2019 ich der Sonne gleich/ den Himmels-Crei\u00df durch-", "tokens": ["K\u00f6nt'", "ich", "der", "Son\u00b7ne", "gleich", "/", "den", "Him\u00b7mels\u00b7Crei\u00df", "durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "$(", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und folgen/ bi\u00df sie ihre Gluth", "tokens": ["Und", "fol\u00b7gen", "/", "bi\u00df", "sie", "ih\u00b7re", "Gluth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$(", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00f6scht in des letzten Meeres-Fluth;", "tokens": ["L\u00f6scht", "in", "des", "letz\u00b7ten", "Mee\u00b7res\u00b7Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00fcrde mich auch dort dein starcker Arm erreichen.", "tokens": ["So", "w\u00fcr\u00b7de", "mich", "auch", "dort", "dein", "star\u00b7cker", "Arm", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Schatten finstrer Nacht deckt meine S\u00fcnde nicht/", "tokens": ["Der", "Schat\u00b7ten", "finst\u00b7rer", "Nacht", "deckt", "mei\u00b7ne", "S\u00fcn\u00b7de", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil deiner Augen Blitz durch alle Winckel bricht.", "tokens": ["Weil", "dei\u00b7ner", "Au\u00b7gen", "Blitz", "durch", "al\u00b7le", "Win\u00b7ckel", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und HErr wie solte dir mein Wandel seyn verborgen?", "tokens": ["Und", "Herr", "wie", "sol\u00b7te", "dir", "mein", "Wan\u00b7del", "seyn", "ver\u00b7bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "VMFIN", "PPER", "PPOSAT", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der du/ eh\u2019 ich das Licht geschaut/", "tokens": ["Der", "du", "/", "eh'", "ich", "das", "Licht", "ge\u00b7schaut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den C\u00f6rper den du mir gebaut/", "tokens": ["Den", "C\u00f6r\u00b7per", "den", "du", "mir", "ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit lebendigem Geist hast wollen selbst versorgen;", "tokens": ["Mit", "le\u00b7ben\u00b7di\u00b7gem", "Geist", "hast", "wol\u00b7len", "selbst", "ver\u00b7sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VMFIN", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der du von Ewigkeit schon hast gezeichnet auff/", "tokens": ["Der", "du", "von", "E\u00b7wig\u00b7keit", "schon", "hast", "ge\u00b7zeich\u00b7net", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VAFIN", "VVPP", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was mir begegnen soll/ und meiner Jahre Lauff.", "tokens": ["Was", "mir", "be\u00b7geg\u00b7nen", "soll", "/", "und", "mei\u00b7ner", "Jah\u00b7re", "Lauff", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$(", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es kan dis Wunderwerck allein mich \u00fcberzeugen/", "tokens": ["Es", "kan", "dis", "Wun\u00b7der\u00b7werck", "al\u00b7lein", "mich", "\u00fc\u00b7berz\u00b7eu\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "NN", "ADV", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich in unverf\u00e4lschtem Sinn", "tokens": ["Da\u00df", "ich", "in", "un\u00b7ver\u00b7f\u00e4lschtem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dir Danck und Opffer schuldig bin;", "tokens": ["Dir", "Danck", "und", "Opf\u00b7fer", "schul\u00b7dig", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da Erd\u2019 und Him\u0303el nicht von deinen Kr\u00e4fften schweigen.", "tokens": ["Da", "Erd'", "und", "Him\u0303el", "nicht", "von", "dei\u00b7nen", "Kr\u00e4ff\u00b7ten", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So da\u00df man eh den Sand der W\u00fcsten zehlen kan/", "tokens": ["So", "da\u00df", "man", "eh", "den", "Sand", "der", "W\u00fcs\u00b7ten", "zeh\u00b7len", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "KOUS", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als was du grosser GOtt f\u00fcr Wunder hast gethan.", "tokens": ["Als", "was", "du", "gros\u00b7ser", "Gott", "f\u00fcr", "Wun\u00b7der", "hast", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mein Hertz ist dessen voll. Ich finde mein Vergn\u00fcgen", "tokens": ["Mein", "Hertz", "ist", "des\u00b7sen", "voll", ".", "Ich", "fin\u00b7de", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PDS", "ADJD", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin/ da\u00df ich den gantzen Tag", "tokens": ["Da\u00b7rin", "/", "da\u00df", "ich", "den", "gant\u00b7zen", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$(", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der L\u00e4nge nach betrachten mag/", "tokens": ["Der", "L\u00e4n\u00b7ge", "nach", "be\u00b7trach\u00b7ten", "mag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sich doch alles mu\u00df nach deiner Ordnung f\u00fcgen.", "tokens": ["Wie", "sich", "doch", "al\u00b7les", "mu\u00df", "nach", "dei\u00b7ner", "Ord\u00b7nung", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "PIS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja wenn die Sinne sich vom Schlaafe lo\u00df gemacht/", "tokens": ["Ja", "wenn", "die", "Sin\u00b7ne", "sich", "vom", "Schlaa\u00b7fe", "lo\u00df", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "ART", "NN", "PRF", "APPRART", "NN", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sp\u00fcr\u2019 ich da\u00df ich auch im Traum daran gedacht.", "tokens": ["So", "sp\u00fcr'", "ich", "da\u00df", "ich", "auch", "im", "Traum", "da\u00b7ran", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "APPRART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie aber? fehlt es dir itzund an Donnerschl\u00e4gen/", "tokens": ["Wie", "a\u00b7ber", "?", "fehlt", "es", "dir", "it\u00b7zund", "an", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Hauffen/ der dir sp\u00f6ttlich flucht/", "tokens": ["Dem", "Hauf\u00b7fen", "/", "der", "dir", "sp\u00f6tt\u00b7lich", "flucht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur das Blut der Frommen sucht/", "tokens": ["Und", "nur", "das", "Blut", "der", "From\u00b7men", "sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu zeigen/ da\u00df du ihn bald in den Staub kanst legen?", "tokens": ["Zu", "zei\u00b7gen", "/", "da\u00df", "du", "ihn", "bald", "in", "den", "Staub", "kanst", "le\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "KOUS", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Stoltz und L\u00e4stern wird noch immerhin gemehrt/", "tokens": ["Sein", "Stoltz", "und", "L\u00e4s\u00b7tern", "wird", "noch", "im\u00b7mer\u00b7hin", "ge\u00b7mehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil dein gerechter Grimm/ nicht dieses Wesen st\u00f6hrt.", "tokens": ["Weil", "dein", "ge\u00b7rech\u00b7ter", "Grimm", "/", "nicht", "die\u00b7ses", "We\u00b7sen", "st\u00f6hrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "NE", "$(", "PTKNEG", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Gewi\u00df ich hasse sehr/ die dich den H\u00f6chsten hassen:", "tokens": ["Ge\u00b7wi\u00df", "ich", "has\u00b7se", "sehr", "/", "die", "dich", "den", "H\u00f6chs\u00b7ten", "has\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ADV", "$