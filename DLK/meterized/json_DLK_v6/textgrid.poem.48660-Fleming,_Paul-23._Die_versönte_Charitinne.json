{"textgrid.poem.48660": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "23. Die vers\u00f6nte Charitinne", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herze von Demant", "tokens": ["Das", "Her\u00b7ze", "von", "De\u00b7mant"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "hat sich in Fleisch gewant.", "tokens": ["hat", "sich", "in", "Fleisch", "ge\u00b7want", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die unverwanten Sinnen", "tokens": ["Die", "un\u00b7ver\u00b7wan\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der harten Charitinnen", "tokens": ["der", "har\u00b7ten", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "hab' ich in meiner Hand.", "tokens": ["hab'", "ich", "in", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bis hieher bin ich tot,", "tokens": ["Bis", "hie\u00b7her", "bin", "ich", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "nun hat es keine Not.", "tokens": ["nun", "hat", "es", "kei\u00b7ne", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie, meines Lebens Leben,", "tokens": ["Sie", ",", "mei\u00b7nes", "Le\u00b7bens", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "hat mir di\u00df wiedergeben,", "tokens": ["hat", "mir", "di\u00df", "wie\u00b7der\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "was uns den Mund macht rot.", "tokens": ["was", "uns", "den", "Mund", "macht", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du sch\u00f6nes Zimmer du,", "tokens": ["Du", "sch\u00f6\u00b7nes", "Zim\u00b7mer", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "da\u00df du mich bringst in Ruh!", "tokens": ["da\u00df", "du", "mich", "bringst", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Violen und Narzissen,", "tokens": ["Vi\u00b7o\u00b7len", "und", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die m\u00fcssen aus dir sprie\u00dfen", "tokens": ["die", "m\u00fcs\u00b7sen", "aus", "dir", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und dich ganz decken zu.", "tokens": ["und", "dich", "ganz", "de\u00b7cken", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Der \u00c4uglein milder Plitz,", "tokens": ["Der", "\u00c4ug\u00b7lein", "mil\u00b7der", "Plitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gott Amors sein Gesch\u00fctz',", "tokens": ["Gott", "A\u00b7mors", "sein", "Ge\u00b7sch\u00fctz'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und die Korallen Lippen", "tokens": ["und", "die", "Ko\u00b7ral\u00b7len", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "sind meine feste Klippen", "tokens": ["sind", "mei\u00b7ne", "fes\u00b7te", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und starker Rittersitz.", "tokens": ["und", "star\u00b7ker", "Rit\u00b7ter\u00b7sitz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}