{"dta.poem.21577": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "An das 20. Jahrhundert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wirf die Thore auf, Jahrhundert,", "tokens": ["Wirf", "die", "Tho\u00b7re", "auf", ",", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm herab begr\u00fc\u00dft, bewundert,", "tokens": ["Komm", "her\u00b7ab", "be\u00b7gr\u00fc\u00dft", ",", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnenleuchtend, Morgenklar.", "tokens": ["Son\u00b7nen\u00b7leuch\u00b7tend", ",", "Mor\u00b7gen\u00b7klar", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Krone tr\u00e4gst du golden,", "tokens": ["Kei\u00b7ne", "Kro\u00b7ne", "tr\u00e4gst", "du", "gol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ein Kranz von duftigholden", "tokens": ["Doch", "ein", "Kranz", "von", "duf\u00b7tig\u00b7hol\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fchlingsrosen schm\u00fcckt dein Haar.", "tokens": ["Fr\u00fch\u00b7lings\u00b7ro\u00b7sen", "schm\u00fcckt", "dein", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ganz verwundet, ganz zerschlagen,", "tokens": ["Ganz", "ver\u00b7wun\u00b7det", ",", "ganz", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz und Mund verdorrt von Klagen,", "tokens": ["Herz", "und", "Mund", "ver\u00b7dorrt", "von", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziehn wir m\u00fcd im Staub einher.", "tokens": ["Ziehn", "wir", "m\u00fcd", "im", "Staub", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Aug\u2019 erlischt in Thr\u00e4nen,", "tokens": ["Un\u00b7ser", "Aug'", "er\u00b7lischt", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsre Seele siecht vor Sehnen,", "tokens": ["Uns\u00b7re", "See\u00b7le", "siecht", "vor", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser Haupt gl\u00fcht fieberschwer.", "tokens": ["Un\u00b7ser", "Haupt", "gl\u00fcht", "fie\u00b7ber\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach welch Hoffen, ach welch Sinnen,", "tokens": ["Ach", "welch", "Hof\u00b7fen", ",", "ach", "welch", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAT", "NN", "$,", "XY", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch ein Jubel, welch ein Minnen", "tokens": ["Welch", "ein", "Ju\u00b7bel", ",", "welch", "ein", "Min\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ri\u00df uns flammend einst empor.", "tokens": ["Ri\u00df", "uns", "flam\u00b7mend", "einst", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Natur zu unsern F\u00fc\u00dfen \u2014", "tokens": ["Die", "Na\u00b7tur", "zu", "un\u00b7sern", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollten wir das Licht begr\u00fc\u00dfen,", "tokens": ["Woll\u00b7ten", "wir", "das", "Licht", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo es strahlend quillt hervor.", "tokens": ["Wo", "es", "strah\u00b7lend", "quillt", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf des Dampfes Sturmesfl\u00fcgeln", "tokens": ["Auf", "des", "Damp\u00b7fes", "Stur\u00b7mes\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4umten wir die Welt zu z\u00fcgeln,", "tokens": ["Tr\u00e4um\u00b7ten", "wir", "die", "Welt", "zu", "z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allem Erdenstaub entr\u00fcckt.", "tokens": ["Al\u00b7lem", "Er\u00b7dens\u00b7taub", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Sorge sollte schwinden,", "tokens": ["Al\u00b7le", "Sor\u00b7ge", "soll\u00b7te", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe sich zu Liebe finden,", "tokens": ["Lie\u00b7be", "sich", "zu", "Lie\u00b7be", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Kluft war \u00fcberbr\u00fcckt.", "tokens": ["Al\u00b7le", "Kluft", "war", "\u00fc\u00b7berb\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Traum, wie bald bist du vergangen,", "tokens": ["Traum", ",", "wie", "bald", "bist", "du", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauter Schreckni\u00df, lauter Bangen", "tokens": ["Lau\u00b7ter", "Schrec\u00b7kni\u00df", ",", "lau\u00b7ter", "Ban\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat in Nebel uns geh\u00fcllt.", "tokens": ["Hat", "in", "Ne\u00b7bel", "uns", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Blut tropft aus den Poren,", "tokens": ["Un\u00b7ser", "Blut", "tropft", "aus", "den", "Po\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Mark ist eiserfroren,", "tokens": ["Un\u00b7ser", "Mark", "ist", "ei\u00b7ser\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie vom Tod sind wir erf\u00fcllt.", "tokens": ["Wie", "vom", "Tod", "sind", "wir", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ob wir an des Nordmeer\u2019s Strande", "tokens": ["Ob", "wir", "an", "des", "Nord\u00b7meer's", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn, ob tief im W\u00fcstensande, \u2014", "tokens": ["Ziehn", ",", "ob", "tief", "im", "W\u00fcs\u00b7ten\u00b7san\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ADJD", "APPRART", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsren Weg umheult der Streit.", "tokens": ["Un\u00b7sren", "Weg", "um\u00b7heult", "der", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fried\u2019 und Freude schleicht verlassen,", "tokens": ["Fried'", "und", "Freu\u00b7de", "schleicht", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Noth st\u00fcrmt durch die Gassen,", "tokens": ["Und", "die", "Noth", "st\u00fcrmt", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wild umschw\u00e4rmt von Ha\u00df und Neid.", "tokens": ["Wild", "um\u00b7schw\u00e4rmt", "von", "Ha\u00df", "und", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie zwei Bettler, frech verh\u00f6hnet, \u2014", "tokens": ["Wie", "zwei", "Bett\u00b7ler", ",", "frech", "ver\u00b7h\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "CARD", "NN", "$,", "ADJD", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die wir einst so stolz gekr\u00f6net \u2014", "tokens": ["Die", "wir", "einst", "so", "stolz", "ge\u00b7kr\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Irren Freiheit hin und Recht.", "tokens": ["Ir\u00b7ren", "Frei\u00b7heit", "hin", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eheil den Ketten, die uns binden,", "tokens": ["\u201e", "heil", "den", "Ket\u00b7ten", ",", "die", "uns", "bin\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die uns ziehn und niederwinden,", "tokens": ["Die", "uns", "ziehn", "und", "nie\u00b7der\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Goldne Ketten!\u201c jauchzt der Knecht.", "tokens": ["Gold\u00b7ne", "Ket\u00b7ten", "!", "\u201c", "jauchzt", "der", "Knecht", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch dem Aar gleich, der geblendet", "tokens": ["Doch", "dem", "Aar", "gleich", ",", "der", "ge\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "$,", "PRELS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterbend sich zur Sonne wendet,", "tokens": ["Ster\u00b7bend", "sich", "zur", "Son\u00b7ne", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Harren wir in Br\u00fcnsten dein.", "tokens": ["Har\u00b7ren", "wir", "in", "Br\u00fcns\u00b7ten", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirf die Thore auf, Jahrhundert,", "tokens": ["Wirf", "die", "Tho\u00b7re", "auf", ",", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm herab, begr\u00fc\u00dft, bewundert,", "tokens": ["Komm", "her\u00b7ab", ",", "be\u00b7gr\u00fc\u00dft", ",", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeuch\u2019 mit Morgensturmwind ein.", "tokens": ["Zeuch'", "mit", "Mor\u00b7gen\u00b7sturm\u00b7wind", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wo du gehst, da bricht in Flammen", "tokens": ["Wo", "du", "gehst", ",", "da", "bricht", "in", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausendj\u00e4hriger Grund zusammen,", "tokens": ["Tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7ger", "Grund", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Drauf die Knechtschaft wuchernd stand.", "tokens": ["Drauf", "die", "Knecht\u00b7schaft", "wu\u00b7chernd", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Hoffahrt morsche G\u00f6tter", "tokens": ["Und", "der", "Hof\u00b7fahrt", "mor\u00b7sche", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Treiben hin wie Spreu im Wetter,", "tokens": ["Trei\u00b7ben", "hin", "wie", "Spreu", "im", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf vom Schlafe f\u00e4hrt das Land.", "tokens": ["Auf", "vom", "Schla\u00b7fe", "f\u00e4hrt", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wo du gehst, da \u00f6ffnen alle", "tokens": ["Wo", "du", "gehst", ",", "da", "\u00f6ff\u00b7nen", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefen sich mit hei\u00dfem Schwalle", "tokens": ["Tie\u00b7fen", "sich", "mit", "hei\u00b7\u00dfem", "Schwal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "AP