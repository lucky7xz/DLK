{"textgrid.poem.25013": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lang verschollne Wonnen kehren,", "tokens": ["Lang", "ver\u00b7scholl\u00b7ne", "Won\u00b7nen", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oedes Herz, in dich zur\u00fcck;", "tokens": ["O\u00b7e\u00b7des", "Herz", ",", "in", "dich", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aber wird's dich nicht verzehren,", "tokens": ["A\u00b7ber", "wird's", "dich", "nicht", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses neue Liebesgl\u00fcck?", "tokens": ["Die\u00b7ses", "neu\u00b7e", "Lie\u00b7bes\u00b7gl\u00fcck", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Selig lodernd, wie getroffen", "tokens": ["Se\u00b7lig", "lo\u00b7dernd", ",", "wie", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Himmels Wetterstrahl,", "tokens": ["Von", "des", "Him\u00b7mels", "Wet\u00b7ter\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In Verzagen und in Hoffen", "tokens": ["In", "Ver\u00b7za\u00b7gen", "und", "in", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brennst du, und in s\u00fc\u00dfer Qual.", "tokens": ["Brennst", "du", ",", "und", "in", "s\u00fc\u00b7\u00dfer", "Qual", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieses jubelnde Vergehen,", "tokens": ["Die\u00b7ses", "ju\u00b7beln\u00b7de", "Ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wenn das Ich ins Du versinkt", "tokens": ["Wenn", "das", "Ich", "ins", "Du", "ver\u00b7sinkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "PPER", "APPRART", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in hei\u00dfem Atemwehen", "tokens": ["Und", "in", "hei\u00b7\u00dfem", "A\u00b7tem\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6dliches Entz\u00fccken trinkt,", "tokens": ["T\u00f6d\u00b7li\u00b7ches", "Ent\u00b7z\u00fc\u00b7cken", "trinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bangen Zweifels mu\u00df ich fragen,", "tokens": ["Ban\u00b7gen", "Zwei\u00b7fels", "mu\u00df", "ich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob es Segen oder Fluch;", "tokens": ["Ob", "es", "Se\u00b7gen", "o\u00b7der", "Fluch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, um alles das zu tragen,", "tokens": ["O", ",", "um", "al\u00b7les", "das", "zu", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "PIAT", "PDS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du, Herz, auch stark genug?", "tokens": ["Bist", "du", ",", "Herz", ",", "auch", "stark", "ge\u00b7nug", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}