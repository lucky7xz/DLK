{"textgrid.poem.61576": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber den stillen Seen", "tokens": ["\u00dc\u00b7ber", "den", "stil\u00b7len", "Seen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ergl\u00e4nzt des Vollmonds Schein;", "tokens": ["Er\u00b7gl\u00e4nzt", "des", "Voll\u00b7monds", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein tr\u00e4umerisches Wehen", "tokens": ["Ein", "tr\u00e4u\u00b7me\u00b7ri\u00b7sches", "We\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchl\u00e4uft den Buchenhain.", "tokens": ["Durch\u00b7l\u00e4uft", "den", "Bu\u00b7chen\u00b7hain", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am tau'gen H\u00fcgelpfade", "tokens": ["Am", "tau'\u00b7gen", "H\u00fc\u00b7gel\u00b7pfa\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In D\u00fcften wallt das Korn,", "tokens": ["In", "D\u00fcf\u00b7ten", "wallt", "das", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fern vom Waldgestade", "tokens": ["Und", "fern", "vom", "Wald\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Her\u00fcber gr\u00fc\u00dft ein Horn.", "tokens": ["Her\u00b7\u00fc\u00b7ber", "gr\u00fc\u00dft", "ein", "Horn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie schwebt zu dieser Stunde", "tokens": ["Wie", "schwebt", "zu", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Geist in leichtem Flug!", "tokens": ["Mein", "Geist", "in", "leich\u00b7tem", "Flug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geheilt ist jede Wunde,", "tokens": ["Ge\u00b7heilt", "ist", "je\u00b7de", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mir die Fremde schlug.", "tokens": ["Die", "mir", "die", "Frem\u00b7de", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kaum zeugt von Kampf und Plage", "tokens": ["Kaum", "zeugt", "von", "Kampf", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwachsner Narben Spur,", "tokens": ["Ver\u00b7wachs\u00b7ner", "Nar\u00b7ben", "Spur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und an die goldnen Tage", "tokens": ["Und", "an", "die", "gold\u00b7nen", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Jugend denk' ich nur.", "tokens": ["Der", "Ju\u00b7gend", "denk'", "ich", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie damals f\u00fcllt mich innig", "tokens": ["Wie", "da\u00b7mals", "f\u00fcllt", "mich", "in\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein holdes Gl\u00fccksvertraun;", "tokens": ["Ein", "hol\u00b7des", "Gl\u00fccks\u00b7ver\u00b7traun", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fchl's, zu Hause bin ich,", "tokens": ["Ich", "f\u00fchl's", ",", "zu", "Hau\u00b7se", "bin", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O la\u00dft mich H\u00fctten baun!", "tokens": ["O", "la\u00dft", "mich", "H\u00fct\u00b7ten", "baun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}