{"textgrid.poem.39785": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Lied in der Mark", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schad' ist's um den blauen Himmel,", "tokens": ["Schad'", "ist's", "um", "den", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schad' ist's um den Sonnenschein,", "tokens": ["Schad'", "ist's", "um", "den", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schad' ums lustige Gewimmel", "tokens": ["Schad'", "ums", "lus\u00b7ti\u00b7ge", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohnungsloser V\u00f6gelein!", "tokens": ["Woh\u00b7nungs\u00b7lo\u00b7ser", "V\u00f6\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonn' und Luft hat kein Erg\u00f6tzen,", "tokens": ["Sonn'", "und", "Luft", "hat", "kein", "Er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Scheint und weht auf \u00f6den Sand,", "tokens": ["Scheint", "und", "weht", "auf", "\u00f6\u00b7den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vogel kann sich nirgends setzen,", "tokens": ["Vo\u00b7gel", "kann", "sich", "nir\u00b7gends", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil kein Schattenbaum im Land!", "tokens": ["Weil", "kein", "Schat\u00b7ten\u00b7baum", "im", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur um Eines ist's nicht Schade,", "tokens": ["Nur", "um", "Ei\u00b7nes", "ist's", "nicht", "Scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines mag ich gerne sehn:", "tokens": ["Ei\u00b7nes", "mag", "ich", "ger\u00b7ne", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn behend auf Sandespfade", "tokens": ["Wenn", "be\u00b7hend", "auf", "San\u00b7des\u00b7pfa\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Zwei der z\u00e4rtsten F\u00fc\u00dfchen gehn;", "tokens": ["Zwei", "der", "z\u00e4rts\u00b7ten", "F\u00fc\u00df\u00b7chen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn begr\u00fc\u00dfen mich zwei Augen,", "tokens": ["Wenn", "be\u00b7gr\u00fc\u00b7\u00dfen", "mich", "zwei", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PPER", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rein und hell, nordhimmelblau,", "tokens": ["Rein", "und", "hell", ",", "nord\u00b7him\u00b7mel\u00b7blau", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Draus sich l\u00e4\u00dft mehr Wonne saugen,", "tokens": ["Draus", "sich", "l\u00e4\u00dft", "mehr", "Won\u00b7ne", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als aus Wald und Blumenau!", "tokens": ["Als", "aus", "Wald", "und", "Blu\u00b7me\u00b7nau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wundersch\u00f6ne, nord'sche Blume,", "tokens": ["Wun\u00b7der\u00b7sch\u00f6\u00b7ne", ",", "nord'\u00b7sche", "Blu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der kalten Mark erbl\u00fcht,", "tokens": ["Aus", "der", "kal\u00b7ten", "Mark", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir allein zu Trost und Ruhme", "tokens": ["Dir", "al\u00b7lein", "zu", "Trost", "und", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonne noch und Himmel gl\u00fcht;", "tokens": ["Son\u00b7ne", "noch", "und", "Him\u00b7mel", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf das \u00f6de Land zu blicken,", "tokens": ["Auf", "das", "\u00f6\u00b7de", "Land", "zu", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann sie nimmermehr gereun:", "tokens": ["Kann", "sie", "nim\u00b7mer\u00b7mehr", "ge\u00b7reun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darf nur sie dir Lichter schicken,", "tokens": ["Darf", "nur", "sie", "dir", "Lich\u00b7ter", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Darf nur er dir L\u00fcfte weihn.", "tokens": ["Darf", "nur", "er", "dir", "L\u00fcf\u00b7te", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "M\u00f6ge dir der Herr beh\u00fcten,", "tokens": ["M\u00f6\u00b7ge", "dir", "der", "Herr", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Hauptes Lockenlaub,", "tokens": ["Dei\u00b7nes", "Haup\u00b7tes", "Lo\u00b7cken\u00b7laub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Wang' und Augen Bl\u00fcten,", "tokens": ["Dei\u00b7ner", "Wang'", "und", "Au\u00b7gen", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Wimpern Blumenstaub,", "tokens": ["Dei\u00b7ner", "Wim\u00b7pern", "Blu\u00b7men\u00b7staub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Lippen s\u00fc\u00dfe Fr\u00fcchte,", "tokens": ["Dei\u00b7ner", "Lip\u00b7pen", "s\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinen Wuchs, du schlankes Reis!", "tokens": ["Dei\u00b7nen", "Wuchs", ",", "du", "schlan\u00b7kes", "Reis", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich von dir sing' und dichte,", "tokens": ["Wenn", "ich", "von", "dir", "sing'", "und", "dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gr\u00fcnt der Sand und bl\u00fcht das Eis!", "tokens": ["Gr\u00fcnt", "der", "Sand", "und", "bl\u00fcht", "das", "Eis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}