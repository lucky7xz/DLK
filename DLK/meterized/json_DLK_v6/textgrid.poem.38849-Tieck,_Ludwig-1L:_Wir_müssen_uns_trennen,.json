{"textgrid.poem.38849": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir m\u00fcssen uns trennen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen uns trennen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geliebtes Saitenspiel,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zeit ist es, zu rennen", "tokens": ["Zeit", "ist", "es", ",", "zu", "ren\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Nach dem fernen erw\u00fcnschten Ziel.", "tokens": ["Nach", "dem", "fer\u00b7nen", "er\u00b7w\u00fcnschten", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich ziehe zum Streite", "tokens": ["Ich", "zie\u00b7he", "zum", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum Raube hinaus,", "tokens": ["Zum", "Rau\u00b7be", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und hab' ich die Beute", "tokens": ["Und", "hab'", "ich", "die", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dann flieg ich nach Haus.", "tokens": ["Dann", "flieg", "ich", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Im r\u00f6thlichen Glanze", "tokens": ["Im", "r\u00f6th\u00b7li\u00b7chen", "Glan\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Entflieh ich mit ihr,", "tokens": ["Ent\u00b7flieh", "ich", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es sch\u00fctzt uns die Lanze,", "tokens": ["Es", "sch\u00fctzt", "uns", "die", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Stahlharnisch hier.", "tokens": ["Der", "Stahl\u00b7har\u00b7nisch", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Kommt, liebe Waffenst\u00fccke,", "tokens": ["Kommt", ",", "lie\u00b7be", "Waf\u00b7fen\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Scherz oft angethan,", "tokens": ["Zum", "Scherz", "oft", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beschirmet jetzt mein Gl\u00fccke,", "tokens": ["Be\u00b7schir\u00b7met", "jetzt", "mein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf dieser neuen Bahn.", "tokens": ["Auf", "die\u00b7ser", "neu\u00b7en", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich werfe mich rasch in die Wogen,", "tokens": ["Ich", "wer\u00b7fe", "mich", "rasch", "in", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich gr\u00fc\u00dfe den herrlichen Lauf,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "den", "herr\u00b7li\u00b7chen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Schon mancher ward nieder gezogen,", "tokens": ["Schon", "man\u00b7cher", "ward", "nie\u00b7der", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der tapfere Schwimmer bleibt oben auf.", "tokens": ["Der", "tap\u00b7fe\u00b7re", "Schwim\u00b7mer", "bleibt", "o\u00b7ben", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ha! Lust zu vergeuden", "tokens": ["Ha", "!", "Lust", "zu", "ver\u00b7geu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das edele Blut!", "tokens": ["Das", "e\u00b7de\u00b7le", "Blut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zu sch\u00fctzen die Freuden,", "tokens": ["Zu", "sch\u00fct\u00b7zen", "die", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein k\u00f6stlichstes Gut!", "tokens": ["Mein", "k\u00f6st\u00b7lichs\u00b7tes", "Gut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nicht Hohn zu erleiden,", "tokens": ["Nicht", "Hohn", "zu", "er\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wem fehlt es an Muth?", "tokens": ["Wem", "fehlt", "es", "an", "Muth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Senke die Z\u00fcgel,", "tokens": ["Sen\u00b7ke", "die", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gl\u00fcckliche Nacht!", "tokens": ["Gl\u00fcck\u00b7li\u00b7che", "Nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Spanne die Fl\u00fcgel,", "tokens": ["Span\u00b7ne", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da\u00df uber ferne H\u00fcgel", "tokens": ["Da\u00df", "u\u00b7ber", "fer\u00b7ne", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uns schon der Morgen lacht!", "tokens": ["Uns", "schon", "der", "Mor\u00b7gen", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}