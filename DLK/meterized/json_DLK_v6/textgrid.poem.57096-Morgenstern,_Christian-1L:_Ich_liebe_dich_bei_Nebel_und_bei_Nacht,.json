{"textgrid.poem.57096": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich liebe dich bei Nebel und bei Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe dich bei Nebel und bei Nacht,", "tokens": ["Ich", "lie\u00b7be", "dich", "bei", "Ne\u00b7bel", "und", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wenn deine Linien ineinander schwimmen, \u2013", "tokens": ["wenn", "dei\u00b7ne", "Li\u00b7ni\u00b7en", "in\u00b7ein\u00b7an\u00b7der", "schwim\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVINF", "$,", "$("], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "zumal bei Nacht, wenn deine Fenster glimmen", "tokens": ["zu\u00b7mal", "bei", "Nacht", ",", "wenn", "dei\u00b7ne", "Fens\u00b7ter", "glim\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und Menschheit dein Gestein lebendig macht.", "tokens": ["und", "Menschheit", "dein", "Ge\u00b7stein", "le\u00b7ben\u00b7dig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was w\u00fcst am Tag, wird r\u00e4tselvoll im Dunkel;", "tokens": ["Was", "w\u00fcst", "am", "Tag", ",", "wird", "r\u00e4t\u00b7sel\u00b7voll", "im", "Dun\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$,", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie Seelenburgen stehn sie mystisch da,", "tokens": ["wie", "See\u00b7len\u00b7bur\u00b7gen", "stehn", "sie", "mys\u00b7tisch", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die H\u00e4userreihn, mit ihrem Lichtgefunkel;", "tokens": ["die", "H\u00e4u\u00b7ser\u00b7reihn", ",", "mit", "ih\u00b7rem", "Licht\u00b7ge\u00b7fun\u00b7kel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und Einheit ahnt, wer sonst nur Vielheit sah.", "tokens": ["und", "Ein\u00b7heit", "ahnt", ",", "wer", "sonst", "nur", "Viel\u00b7heit", "sah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PWS", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der letzte Glanz erlischt in blinden Scheiben;", "tokens": ["Der", "letz\u00b7te", "Glanz", "er\u00b7lischt", "in", "blin\u00b7den", "Schei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in seine Schachteln liegt ein Spiel ger\u00e4umt;", "tokens": ["in", "sei\u00b7ne", "Schach\u00b7teln", "liegt", "ein", "Spiel", "ge\u00b7r\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "geb\u00e4ndigt ruht ein ungest\u00fcmes Treiben,", "tokens": ["ge\u00b7b\u00e4n\u00b7digt", "ruht", "ein", "un\u00b7ge\u00b7st\u00fc\u00b7mes", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und heilig wird, was so voll Schicksal tr\u00e4umt.", "tokens": ["und", "hei\u00b7lig", "wird", ",", "was", "so", "voll", "Schick\u00b7sal", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}