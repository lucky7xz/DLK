{"textgrid.poem.58631": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich selbst bin das verruchte Weib, das ihren Mann verlassen,", "tokens": ["Ich", "selbst", "bin", "das", "ver\u00b7ruch\u00b7te", "Weib", ",", "das", "ih\u00b7ren", "Mann", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ich lief von Gott geschieden hin, verirrt auf fremder Stra\u00dfen,", "tokens": ["Ich", "lief", "von", "Gott", "ge\u00b7schie\u00b7den", "hin", ",", "ver\u00b7irrt", "auf", "frem\u00b7der", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "PTKVZ", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Ich lief viel schn\u00f6den Buhlern nach, ich machte mich gemein,", "tokens": ["Ich", "lief", "viel", "schn\u00f6\u00b7den", "Buh\u00b7lern", "nach", ",", "ich", "mach\u00b7te", "mich", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Mein Herz ging in die Kreatur, und sie ins Herze ein.", "tokens": ["Mein", "Herz", "ging", "in", "die", "Kre\u00b7a\u00b7tur", ",", "und", "sie", "ins", "Her\u00b7ze", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "PPER", "APPRART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Du Abgrund aller Lieb' und Treu, du rufst mich dennoch wieder,", "tokens": ["Du", "Ab\u00b7grund", "al\u00b7ler", "Lieb'", "und", "Treu", ",", "du", "rufst", "mich", "den\u00b7noch", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Ich sink' vor Scham und Dankbarkeit zu deinen F\u00fc\u00dfen nieder,", "tokens": ["Ich", "sink'", "vor", "Scham", "und", "Dank\u00b7bar\u00b7keit", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Nichts scheidet mich hinfort von dir, der Bund soll ewig sein,", "tokens": ["Nichts", "schei\u00b7det", "mich", "hin\u00b7fort", "von", "dir", ",", "der", "Bund", "soll", "e\u00b7wig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "ART", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Du bist mein recht' und echter Mann, mein Herr und Gott allein.", "tokens": ["Du", "bist", "mein", "recht'", "und", "ech\u00b7ter", "Mann", ",", "mein", "Herr", "und", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "KON", "ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}