{"dta.poem.16687": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "46.  Wer GOtt ist/ siehet GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich das wahre Licht/ so wie es ist/ sol sehn;", "tokens": ["Weil", "ich", "das", "wah\u00b7re", "Licht", "/", "so", "wie", "es", "ist", "/", "sol", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$(", "ADV", "KOKOM", "PPER", "VAFIN", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df ichs selber seyn: sonst kan es nicht geschehn.", "tokens": ["So", "mu\u00df", "ichs", "sel\u00b7ber", "seyn", ":", "sonst", "kan", "es", "nicht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VAINF", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}