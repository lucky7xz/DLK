{"dta.poem.2813": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sohn, erwarte nicht, da\u00df dich die Leute warnen", "tokens": ["Mein", "Sohn", ",", "er\u00b7war\u00b7te", "nicht", ",", "da\u00df", "dich", "die", "Leu\u00b7te", "war\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor B\u00f6sem, eh davon du l\u00e4ssest dich umgarnen.", "tokens": ["Vor", "B\u00f6\u00b7sem", ",", "eh", "da\u00b7von", "du", "l\u00e4s\u00b7sest", "dich", "um\u00b7gar\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KOUS", "PAV", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie werden zusehn bis um dich es schlug zusammen,", "tokens": ["Sie", "wer\u00b7den", "zu\u00b7sehn", "bis", "um", "dich", "es", "schlug", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "KON", "APPR", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um zu beklagen dann dich oder zu verdammen,", "tokens": ["Um", "zu", "be\u00b7kla\u00b7gen", "dann", "dich", "o\u00b7der", "zu", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVFIN", "ADV", "PPER", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und sich zu freuen, da\u00df sie besser sind als du,", "tokens": ["Und", "sich", "zu", "freu\u00b7en", ",", "da\u00df", "sie", "bes\u00b7ser", "sind", "als", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht, doch gl\u00fccklicher; drum sieh beizeiten zu,", "tokens": ["Wo", "nicht", ",", "doch", "gl\u00fcck\u00b7li\u00b7cher", ";", "drum", "sieh", "bei\u00b7zei\u00b7ten", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "ADJD", "$.", "PAV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mein Sohn, die Welt kann dich nur f\u00fchren in Gefahren;", "tokens": ["Mein", "Sohn", ",", "die", "Welt", "kann", "dich", "nur", "f\u00fch\u00b7ren", "in", "Ge\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich h\u00fcten mu\u00dft du selbst, und Gott mu\u00df dich bewahren.", "tokens": ["Dich", "h\u00fc\u00b7ten", "mu\u00dft", "du", "selbst", ",", "und", "Gott", "mu\u00df", "dich", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "ADV", "$,", "KON", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mein Sohn, ich lehre dich, was ich an mir erfuhr:", "tokens": ["Mein", "Sohn", ",", "ich", "leh\u00b7re", "dich", ",", "was", "ich", "an", "mir", "er\u00b7fuhr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt nimmt Theil mit Lust an unserm Schaden nur.", "tokens": ["Die", "Welt", "nimmt", "Theil", "mit", "Lust", "an", "un\u00b7serm", "Scha\u00b7den", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}