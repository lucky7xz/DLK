{"textgrid.poem.55764": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich seh' Euch mit den Fingern nach mir zeigen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' Euch mit den Fingern nach mir zeigen,", "tokens": ["Ich", "seh'", "Euch", "mit", "den", "Fin\u00b7gern", "nach", "mir", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Achseln zucken, l\u00e4cheln dann und schweigen.", "tokens": ["Die", "Ach\u00b7seln", "zu\u00b7cken", ",", "l\u00e4\u00b7cheln", "dann", "und", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr rechnet mich seit jeher zu den Tollen,", "tokens": ["Ihr", "rech\u00b7net", "mich", "seit", "je\u00b7her", "zu", "den", "Tol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Mitleid nur verm\u00f6gt Ihr mir zu zollen;", "tokens": ["Und", "Mit\u00b7leid", "nur", "ver\u00b7m\u00f6gt", "Ihr", "mir", "zu", "zol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch w\u00fcrdet Ihr in meine Seele schauen,", "tokens": ["Doch", "w\u00fcr\u00b7det", "Ihr", "in", "mei\u00b7ne", "See\u00b7le", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Euch \u00fcberfiele namenloses Grauen,", "tokens": ["Euch", "\u00fc\u00b7berf\u00b7ie\u00b7le", "na\u00b7men\u00b7lo\u00b7ses", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihr w\u00fcrdet mich der schwersten S\u00fcnden zeihen:", "tokens": ["Ihr", "w\u00fcr\u00b7det", "mich", "der", "schwers\u00b7ten", "S\u00fcn\u00b7den", "zei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbauf! Kreuzigt ihn!\u00ab so kl\u00e4nge Euer Schreien, \u2013", "tokens": ["\u00bb", "auf", "!", "Kreu\u00b7zigt", "ihn", "!", "\u00ab", "so", "kl\u00e4n\u00b7ge", "Eu\u00b7er", "Schrei\u00b7en", ",", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "VVFIN", "PPER", "$.", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und einen Vorwurf nur f\u00fcr mich es gibt:", "tokens": ["Und", "ei\u00b7nen", "Vor\u00b7wurf", "nur", "f\u00fcr", "mich", "es", "gibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich hab' im Leben viel zu viel geliebt.", "tokens": ["Ich", "hab'", "im", "Le\u00b7ben", "viel", "zu", "viel", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}