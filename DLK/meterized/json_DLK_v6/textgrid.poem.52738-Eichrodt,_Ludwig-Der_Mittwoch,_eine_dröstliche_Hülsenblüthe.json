{"textgrid.poem.52738": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Mittwoch, eine dr\u00f6stliche H\u00fclsenbl\u00fcthe", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Posaunen hauchen wilden Geisterlaut,", "tokens": ["Po\u00b7sau\u00b7nen", "hau\u00b7chen", "wil\u00b7den", "Geis\u00b7ter\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dazwischen h\u00fcrchelt banges Orgelst\u00f6hnen,", "tokens": ["Da\u00b7zwi\u00b7schen", "h\u00fcr\u00b7chelt", "ban\u00b7ges", "Or\u00b7gel\u00b7st\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leis wuchert bst! der Andacht Ginsterkraut \u2013", "tokens": ["Leis", "wu\u00b7chert", "bst", "!", "der", "An\u00b7dacht", "Gins\u00b7ter\u00b7kraut", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$.", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da steh ich in dem Dom \u2013 umringt von Sch\u00f6nen.", "tokens": ["Da", "steh", "ich", "in", "dem", "Dom", "\u2013", "um\u00b7ringt", "von", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie dort, die Blonde unter Kerzgeflimmer!", "tokens": ["Sie", "dort", ",", "die", "Blon\u00b7de", "un\u00b7ter", "Kerz\u00b7ge\u00b7flim\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein einsam gramgeboren Frauenzimmer,", "tokens": ["Ein", "ein\u00b7sam", "gram\u00b7ge\u00b7bo\u00b7ren", "Frau\u00b7en\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie zupft, ha! mit dem Finger, prickt und pocht", "tokens": ["Sie", "zupft", ",", "ha", "!", "mit", "dem", "Fin\u00b7ger", ",", "prickt", "und", "pocht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$.", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An der Laterne quirlendem Gedocht.", "tokens": ["An", "der", "La\u00b7ter\u00b7ne", "quir\u00b7len\u00b7dem", "Ge\u00b7docht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und dr\u00fcben an der schmerlegrausen S\u00e4ul',", "tokens": ["Und", "dr\u00fc\u00b7ben", "an", "der", "schmer\u00b7le\u00b7grau\u00b7sen", "S\u00e4ul'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hohlh\u00fcstelnd ragt die schwimmelnde Matrone,", "tokens": ["Hohl\u00b7h\u00fcs\u00b7telnd", "ragt", "die", "schwim\u00b7meln\u00b7de", "Mat\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der Blick des Auges schw\u00e4rmt dahin, ein Pfeil,", "tokens": ["Der", "Blick", "des", "Au\u00b7ges", "schw\u00e4rmt", "da\u00b7hin", ",", "ein", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PAV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ausschwirrend durch des Lebens Nachtschablone.", "tokens": ["Aus\u00b7schwir\u00b7rend", "durch", "des", "Le\u00b7bens", "Nacht\u00b7scha\u00b7blo\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hi! Still! na! o, ich bin verloren,", "tokens": ["Hi", "!", "Still", "!", "na", "!", "o", ",", "ich", "bin", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "$.", "ITJ", "$.", "FM", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zerr\u00fcckt, zerknistert, gluthig angeschmoren,", "tokens": ["Zer\u00b7r\u00fcckt", ",", "zer\u00b7knis\u00b7tert", ",", "glut\u00b7hig", "an\u00b7ge\u00b7schmo\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was ist das, die Laterne spritzt und kocht \u2013", "tokens": ["Was", "ist", "das", ",", "die", "La\u00b7ter\u00b7ne", "spritzt", "und", "kocht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Auch sie zupft, zirpst am eignen Lebensdocht.", "tokens": ["Auch", "sie", "zupft", ",", "zirpst", "am", "eig\u00b7nen", "Le\u00b7bens\u00b7docht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--++-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Und K\u00e4uzchenschrei ruschelt mit Geierpfiff", "tokens": ["Und", "K\u00e4uz\u00b7chen\u00b7schrei", "ru\u00b7schelt", "mit", "Gei\u00b7er\u00b7pfiff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wirr durch der Halle wimmerfahlen Brodem \u2013", "tokens": ["Wirr", "durch", "der", "Hal\u00b7le", "wim\u00b7mer\u00b7fah\u00b7len", "Bro\u00b7dem", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Verfallenes Gem\u00e4uer, eingest\u00fcrztes Schiff,", "tokens": ["Ver\u00b7fal\u00b7le\u00b7nes", "Ge\u00b7m\u00e4u\u00b7er", ",", "ein\u00b7ge\u00b7st\u00fcrz\u00b7tes", "Schiff", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Glockengebraus, Gespensterschwadenodem ...", "tokens": ["Glo\u00b7cken\u00b7ge\u00b7braus", ",", "Ge\u00b7spens\u00b7ter\u00b7schwa\u00b7de\u00b7no\u00b7dem", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und Mitternacht! Dann pl\u00f6tzlich Lichtgefunkel", "tokens": ["Und", "Mit\u00b7ter\u00b7nacht", "!", "Dann", "pl\u00f6tz\u00b7lich", "Licht\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$.", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da w\u00e4lzet aus des Dienstags Tr\u00fcmmerdunkel", "tokens": ["Da", "w\u00e4l\u00b7zet", "aus", "des", "Diens\u00b7tags", "Tr\u00fcm\u00b7mer\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Und s\u00e4uselt Schauerluft in's linke Ohr.", "tokens": ["Und", "s\u00e4u\u00b7selt", "Schau\u00b7er\u00b7luft", "in's", "lin\u00b7ke", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hahu! regnet es nicht? Saugt nicht den Duft", "tokens": ["Ha\u00b7hu", "!", "reg\u00b7net", "es", "nicht", "?", "Saugt", "nicht", "den", "Duft"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Des molchges\u00e4ugten giftiggelben Schwadens", "tokens": ["Des", "molch\u00b7ge\u00b7s\u00e4ug\u00b7ten", "gif\u00b7tig\u00b7gel\u00b7ben", "Schwa\u00b7dens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Natur? z\u00fcckt greiser Blitz in wunde Brust,", "tokens": ["Na\u00b7tur", "?", "z\u00fcckt", "grei\u00b7ser", "Blitz", "in", "wun\u00b7de", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verzweifelnd an dem Gl\u00fccke des Entladens?", "tokens": ["Ver\u00b7zwei\u00b7felnd", "an", "dem", "Gl\u00fc\u00b7cke", "des", "Ent\u00b7la\u00b7dens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Scheu flattert auf der Tro\u00df der Quazaquelen,", "tokens": ["Scheu", "flat\u00b7tert", "auf", "der", "Tro\u00df", "der", "Qua\u00b7za\u00b7que\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Lampe sch\u00fcttert l\u00e4chelnd im Verschwehlen,", "tokens": ["Die", "Lam\u00b7pe", "sch\u00fct\u00b7tert", "l\u00e4\u00b7chelnd", "im", "Ver\u00b7schweh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Damen knicken um, der Dolde gleich,", "tokens": ["Die", "Da\u00b7men", "kni\u00b7cken", "um", ",", "der", "Dol\u00b7de", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Br\u00e4utigam entrispelt dem Bereich.", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "ent\u00b7ris\u00b7pelt", "dem", "Be\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hui! ich erwach'! Des Traumes Vampyr flieht,", "tokens": ["Hui", "!", "ich", "er\u00b7wach'", "!", "Des", "Trau\u00b7mes", "Vam\u00b7pyr", "flieht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "$.", "PPER", "VVFIN", "$.", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schon an der Gardine zupfen Strahlen", "tokens": ["Und", "schon", "an", "der", "Gar\u00b7di\u00b7ne", "zup\u00b7fen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "NN"], "meter": "--+----+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Der Morgensonne bst! Frohlock', Gem\u00fcth!", "tokens": ["Der", "Mor\u00b7gen\u00b7son\u00b7ne", "bst", "!", "Frohlock'", ",", "Ge\u00b7m\u00fcth", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "$,", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Lerchen zitschern weg die lecken Qualen.", "tokens": ["Die", "Ler\u00b7chen", "zit\u00b7schern", "weg", "die", "le\u00b7cken", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Kummers feuchte Moderkatakombe", "tokens": ["Des", "Kum\u00b7mers", "feuch\u00b7te", "Mo\u00b7der\u00b7ka\u00b7ta\u00b7kom\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "St\u00fcrzt donnernd ein und nur die m\u00fc\u00df'ge Rombe", "tokens": ["St\u00fcrzt", "don\u00b7nernd", "ein", "und", "nur", "die", "m\u00fc\u00df'\u00b7ge", "Rom\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "PTKVZ", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der glitzerhaften Qualerinnerung", "tokens": ["Der", "glit\u00b7zer\u00b7haf\u00b7ten", "Qua\u00b7le\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schlirrt noch einmal \u2013 und du bliebst jung?", "tokens": ["Schlirrt", "noch", "ein\u00b7mal", "\u2013", "und", "du", "bliebst", "jung", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$(", "KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Kling klar! Johann! Die schwarzen Schimmel vor!", "tokens": ["Kling", "klar", "!", "Jo\u00b7hann", "!", "Die", "schwar\u00b7zen", "Schim\u00b7mel", "vor", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "NE", "$.", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Spann an! wir fahren heut' noch durch die Haide,", "tokens": ["Spann", "an", "!", "wir", "fah\u00b7ren", "heut'", "noch", "durch", "die", "Hai\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort, wo die Kr\u00e4he duckt, die Kr\u00f6t' im Moor,", "tokens": ["Dort", ",", "wo", "die", "Kr\u00e4\u00b7he", "duckt", ",", "die", "Kr\u00f6t'", "im", "Moor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dort soll versausen meiner Seele Freude.", "tokens": ["Dort", "soll", "ver\u00b7sau\u00b7sen", "mei\u00b7ner", "See\u00b7le", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Lang ist der Tag \u2013 d'rum gleich der Wetterwolke", "tokens": ["Lang", "ist", "der", "Tag", "\u2013", "d'\u00b7rum", "gleich", "der", "Wet\u00b7ter\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$(", "PAV", "ADV", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ausbrechen la\u00df' die Bracken bis zum Kolke!", "tokens": ["Aus\u00b7bre\u00b7chen", "la\u00df'", "die", "Bra\u00b7cken", "bis", "zum", "Kol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie geigt die Grill' wie brodelt's im Gestumpf!", "tokens": ["Wie", "geigt", "die", "Grill'", "wie", "bro\u00b7delt's", "im", "Ge\u00b7stumpf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ART", "NN", "KOKOM", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sehr mangelt mir der Sitz ... hinaus zum Sumpf!", "tokens": ["Sehr", "man\u00b7gelt", "mir", "der", "Sitz", "...", "hin\u00b7aus", "zum", "Sumpf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "APZR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}