{"textgrid.poem.38226": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die gute Sieben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein junger Knab,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "jun\u00b7ger", "Knab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der liebt sein Sch\u00e4tzlein sieben Jahr,", "tokens": ["Der", "liebt", "sein", "Sch\u00e4tz\u00b7lein", "sie\u00b7ben", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl sieben Jahr und noch vielmehr,", "tokens": ["Wohl", "sie\u00b7ben", "Jahr", "und", "noch", "viel\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lieb, die nahm kein Ende mehr.", "tokens": ["Die", "Lieb", ",", "die", "nahm", "kein", "En\u00b7de", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er liebte des Bauers T\u00f6chterlein,", "tokens": ["Er", "lieb\u00b7te", "des", "Bau\u00b7ers", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Erden konnte nichts Sch\u00f6nres seyn;", "tokens": ["Auf", "Er\u00b7den", "konn\u00b7te", "nichts", "Sch\u00f6n\u00b7res", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "ADJA", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Knaben gingen ihm um sein Haus:", "tokens": ["Die", "Kna\u00b7ben", "gin\u00b7gen", "ihm", "um", "sein", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbach Bauer geb uns dein Tochter heraus.\u00ab", "tokens": ["\u00bb", "ach", "Bau\u00b7er", "geb", "uns", "dein", "Toch\u00b7ter", "he\u00b7raus", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbich geb die Tochter nicht heraus,", "tokens": ["\u00bb", "ich", "geb", "die", "Toch\u00b7ter", "nicht", "he\u00b7raus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich geb ihr kein Geld, ich geb ihr kein Haus;", "tokens": ["Ich", "geb", "ihr", "kein", "Geld", ",", "ich", "geb", "ihr", "kein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich kaufe ihr ein schwarzes Kleid,", "tokens": ["Ich", "kau\u00b7fe", "ihr", "ein", "schwar\u00b7zes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das soll sie tragen zur Kirch und zum Leid.\u00ab", "tokens": ["Das", "soll", "sie", "tra\u00b7gen", "zur", "Kirch", "und", "zum", "Leid", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Da reist der Knabe ins Niederland,", "tokens": ["Da", "reist", "der", "Kna\u00b7be", "ins", "Nie\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da ward ihm sein Herzallerliebste krank;", "tokens": ["Da", "ward", "ihm", "sein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Botschaft ihm kam: krank auf den Tod,", "tokens": ["Die", "Bot\u00b7schaft", "ihm", "kam", ":", "krank", "auf", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$.", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Drey Tag und drey Nacht redt sie kein Wort.", "tokens": ["Drey", "Tag", "und", "drey", "Nacht", "redt", "sie", "kein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "CARD", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als der Knab die Botschaft h\u00f6rt,", "tokens": ["Und", "als", "der", "Knab", "die", "Bot\u00b7schaft", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sein Herzliebste so krank da w\u00e4r;", "tokens": ["Da\u00df", "sein", "Herz\u00b7liebs\u00b7te", "so", "krank", "da", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VAFIN", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da lie\u00df er gleich sein Hab und Gut,", "tokens": ["Da", "lie\u00df", "er", "gleich", "sein", "Hab", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schaut, was sein Herzallerliebste thut.", "tokens": ["Und", "schaut", ",", "was", "sein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und als er in die Stub hinein kam,", "tokens": ["Und", "als", "er", "in", "die", "Stub", "hin\u00b7ein", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "APZR", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sein Herzallerliebste auf den Tod war krank:", "tokens": ["Sein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "auf", "den", "Tod", "war", "krank", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "\u00bbseyst du mir willkommen getreuer Schatz,", "tokens": ["\u00bb", "seyst", "du", "mir", "will\u00b7kom\u00b7men", "ge\u00b7treu\u00b7er", "Schatz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der Tod will jezt wohnen an deinem Platz.\u00ab", "tokens": ["Der", "Tod", "will", "jezt", "woh\u00b7nen", "an", "dei\u00b7nem", "Platz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "\u00bbgr\u00fc\u00df Gott, gr\u00fc\u00df Gott liebs Sch\u00e4tzelein,", "tokens": ["\u00bb", "gr\u00fc\u00df", "Gott", ",", "gr\u00fc\u00df", "Gott", "liebs", "Sch\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "$,", "VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was machst du hier im Bettelein?\u00ab", "tokens": ["Was", "machst", "du", "hier", "im", "Bet\u00b7tel\u00b7ein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdank Gott, dank Gott, mein lieber Knab,", "tokens": ["\u00bb", "dank", "Gott", ",", "dank", "Gott", ",", "mein", "lie\u00b7ber", "Knab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "APPR", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir wirds heissen fort ins Grab.\u00ab", "tokens": ["Mit", "mir", "wirds", "heis\u00b7sen", "fort", "ins", "Grab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "PTKVZ", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbnicht so, nicht so mein Sch\u00e4tzelein,", "tokens": ["\u00bb", "nicht", "so", ",", "nicht", "so", "mein", "Sch\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADV", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die Lieb und Treu mu\u00df l\u00e4nger seyn;", "tokens": ["Die", "Lieb", "und", "Treu", "mu\u00df", "l\u00e4n\u00b7ger", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht gschwind, geht gschwind und holt ein Licht,", "tokens": ["Geht", "gschwind", ",", "geht", "gschwind", "und", "holt", "ein", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schatz der stirbt, da\u00df niemand sieht.\u00ab", "tokens": ["Mein", "Schatz", "der", "stirbt", ",", "da\u00df", "nie\u00b7mand", "sieht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was zog er aus seiner Tasche mit Flei\u00df,", "tokens": ["Was", "zog", "er", "aus", "sei\u00b7ner", "Ta\u00b7sche", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Aepfelein das war roth und wei\u00df,", "tokens": ["Ein", "A\u00b7e\u00b7pfel\u00b7ein", "das", "war", "roth", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er legts auf ihren weis rothen Mund,", "tokens": ["Er", "legts", "auf", "ih\u00b7ren", "weis", "ro\u00b7then", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "PTKVZ", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sch\u00f6n Sch\u00e4tzl, bist krank, werd wieder gesund.", "tokens": ["Sch\u00f6n", "Sch\u00e4tzl", ",", "bist", "krank", ",", "werd", "wie\u00b7der", "ge\u00b7sund", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Er wollt sie legen in seinen Arm,", "tokens": ["Er", "wollt", "sie", "le\u00b7gen", "in", "sei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie war nicht kalt, sie war nicht warm;", "tokens": ["Sie", "war", "nicht", "kalt", ",", "sie", "war", "nicht", "warm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie thut ihm in seinem Arm verscheiden,", "tokens": ["Sie", "thut", "ihm", "in", "sei\u00b7nem", "Arm", "ver\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie thut eine reine Jungfrau bleiben.", "tokens": ["Sie", "thut", "ei\u00b7ne", "rei\u00b7ne", "Jung\u00b7frau", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Was zog er aus der Tasche sein,", "tokens": ["Was", "zog", "er", "aus", "der", "Ta\u00b7sche", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Seide war es ein T\u00fcchlein fein;", "tokens": ["Von", "Sei\u00b7de", "war", "es", "ein", "T\u00fcch\u00b7lein", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er trocknet damit sein Auge und H\u00e4nd,", "tokens": ["Er", "trock\u00b7net", "da\u00b7mit", "sein", "Au\u00b7ge", "und", "H\u00e4nd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach Gott wann nimmt mein Trauren ein End.", "tokens": ["Ach", "Gott", "wann", "nimmt", "mein", "Trau\u00b7ren", "ein", "End", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PWAV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Er lie\u00df sich machen ein schwarzes Kleid,", "tokens": ["Er", "lie\u00df", "sich", "ma\u00b7chen", "ein", "schwar\u00b7zes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er trugs wegen seiner Traurigkeit,", "tokens": ["Er", "trugs", "we\u00b7gen", "sei\u00b7ner", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wohl sieben Jahr und noch viel mehr,", "tokens": ["Wohl", "sie\u00b7ben", "Jahr", "und", "noch", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "KON", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Trauren das nahm kein Ende mehr.", "tokens": ["Sein", "Trau\u00b7ren", "das", "nahm", "kein", "En\u00b7de", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}