{"textgrid.poem.57186": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hatte mich im Hochgebirg verstiegen.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte mich im Hochgebirg verstiegen.", "tokens": ["Ich", "hat\u00b7te", "mich", "im", "Hoch\u00b7ge\u00b7birg", "ver\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Felsenwelt um mich, sie war wohl sch\u00f6n;", "tokens": ["Die", "Fel\u00b7sen\u00b7welt", "um", "mich", ",", "sie", "war", "wohl", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "doch konnt ich keinen Ausgang mir ersiegen,", "tokens": ["doch", "konnt", "ich", "kei\u00b7nen", "Aus\u00b7gang", "mir", "er\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "noch einen Aufgang nach den lichten H\u00f6hn.", "tokens": ["noch", "ei\u00b7nen", "Auf\u00b7gang", "nach", "den", "lich\u00b7ten", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da traf ich Dich, in \u00e4rgster Not: den Andern!", "tokens": ["Da", "traf", "ich", "Dich", ",", "in", "\u00e4rgs\u00b7ter", "Not", ":", "den", "An\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$.", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Dir vereint, gewann ich frischen Mut.", "tokens": ["Mit", "Dir", "ver\u00b7eint", ",", "ge\u00b7wann", "ich", "fri\u00b7schen", "Mut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von neuem hob ich an, mit Dir, zu wandern,", "tokens": ["Von", "neu\u00b7em", "hob", "ich", "an", ",", "mit", "Dir", ",", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPER", "$,", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und siehe da: Das Schicksal war uns gut.", "tokens": ["und", "sie\u00b7he", "da", ":", "Das", "Schick\u00b7sal", "war", "uns", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "$.", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir fanden einen Pfad, der klar und einsam", "tokens": ["Wir", "fan\u00b7den", "ei\u00b7nen", "Pfad", ",", "der", "klar", "und", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "empor sich zog, bis, wo ein Tempel stand.", "tokens": ["em\u00b7por", "sich", "zog", ",", "bis", ",", "wo", "ein", "Tem\u00b7pel", "stand", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "PRF", "VVFIN", "$,", "KOUS", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Steig war steil, doch wagten wir's gemeinsam ...", "tokens": ["Der", "Steig", "war", "steil", ",", "doch", "wag\u00b7ten", "wir's", "ge\u00b7mein\u00b7sam", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVINF", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und heut noch helfen wir uns, Hand in Hand.", "tokens": ["Und", "heut", "noch", "hel\u00b7fen", "wir", "uns", ",", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mag sein, wir stehn an unsres Lebens Ende", "tokens": ["Mag", "sein", ",", "wir", "stehn", "an", "uns\u00b7res", "Le\u00b7bens", "En\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VAINF", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "noch unterm Ziel, \u2013 genug, der Weg ist klar!", "tokens": ["noch", "un\u00b7term", "Ziel", ",", "\u2013", "ge\u00b7nug", ",", "der", "Weg", "ist", "klar", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "$(", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wir uns trafen, war die gro\u00dfe Wende.", "tokens": ["Da\u00df", "wir", "uns", "tra\u00b7fen", ",", "war", "die", "gro\u00b7\u00dfe", "Wen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus zwei Verirrten ward ein wissend Paar.", "tokens": ["Aus", "zwei", "Ver\u00b7irr\u00b7ten", "ward", "ein", "wis\u00b7send", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}