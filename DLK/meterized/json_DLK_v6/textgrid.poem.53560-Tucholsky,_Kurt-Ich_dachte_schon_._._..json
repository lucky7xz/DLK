{"textgrid.poem.53560": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ich dachte schon . . .", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dachte schon, als Willi t\u00fcrmte,", "tokens": ["Ich", "dach\u00b7te", "schon", ",", "als", "Wil\u00b7li", "t\u00fcrm\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "nun w\u00e4rs f\u00fcr unsereinen aus.", "tokens": ["nun", "w\u00e4rs", "f\u00fcr", "un\u00b7ser\u00b7ei\u00b7nen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mich, ich mu\u00df es sagen, w\u00fcrmte,", "tokens": ["Was", "mich", ",", "ich", "mu\u00df", "es", "sa\u00b7gen", ",", "w\u00fcrm\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "denn gern geht kein Akteur nach Haus.", "tokens": ["denn", "gern", "geht", "kein", "Ak\u00b7teur", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schnallte schon die Harfe ab", "tokens": ["Ich", "schnall\u00b7te", "schon", "die", "Har\u00b7fe", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und wankte in ein fr\u00fches Grab.", "tokens": ["und", "wank\u00b7te", "in", "ein", "fr\u00fc\u00b7hes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch hundert Schritte vorm Portale \u2013", "tokens": ["Doch", "hun\u00b7dert", "Schrit\u00b7te", "vorm", "Por\u00b7ta\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was h\u00f6rt da mein entz\u00fcndet Ohr?", "tokens": ["was", "h\u00f6rt", "da", "mein", "ent\u00b7z\u00fcn\u00b7det", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Phrasenl\u00e4rm mit einem Male", "tokens": ["Aus", "Phra\u00b7sen\u00b7l\u00e4rm", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "schallt ein Kommando frisch hervor.", "tokens": ["schallt", "ein", "Kom\u00b7man\u00b7do", "frisch", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der Vater Noske . . . Ach, zum Speiben . . .", "tokens": ["Der", "Va\u00b7ter", "Nos\u00b7ke", ".", ".", ".", "Ach", ",", "zum", "Spei\u00b7ben", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "NE", "$.", "$.", "$.", "ITJ", "$,", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich dachte mir: Da kannst du bleiben!", "tokens": ["Ich", "dach\u00b7te", "mir", ":", "Da", "kannst", "du", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich blieb, und was ich nun erlebte,", "tokens": ["Ich", "blieb", ",", "und", "was", "ich", "nun", "er\u00b7leb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "gemahnt mich an die alte Zeit . . .", "tokens": ["ge\u00b7mahnt", "mich", "an", "die", "al\u00b7te", "Zeit", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVPP", "PRF", "APPR", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich den Herren eine klebte,", "tokens": ["Wenn", "ich", "den", "Her\u00b7ren", "ei\u00b7ne", "kleb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "geschahs aus liebem Zeitvertreib \u2013", "tokens": ["ge\u00b7schahs", "aus", "lie\u00b7bem", "Zeit\u00b7ver\u00b7treib", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich danke fr\u00f6hlich Gott dem Herrn:", "tokens": ["Ich", "dan\u00b7ke", "fr\u00f6h\u00b7lich", "Gott", "dem", "Herrn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Heut tu ich es noch mal so gern.", "tokens": ["Heut", "tu", "ich", "es", "noch", "mal", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da haben wir den alten Kummer,", "tokens": ["Da", "ha\u00b7ben", "wir", "den", "al\u00b7ten", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "den alten Dreh, den alten Wahn \u2013", "tokens": ["den", "al\u00b7ten", "Dreh", ",", "den", "al\u00b7ten", "Wahn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die beste wilhelminische Nummer", "tokens": ["die", "bes\u00b7te", "wil\u00b7hel\u00b7mi\u00b7ni\u00b7sche", "Num\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "hat mir es nicht so angetan.", "tokens": ["hat", "mir", "es", "nicht", "so", "an\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Weimar singt, grins ich erbaut:", "tokens": ["Wenn", "Wei\u00b7mar", "singt", ",", "grins", "ich", "er\u00b7baut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ist mir dieses Lied vertraut!", "tokens": ["Wie", "ist", "mir", "die\u00b7ses", "Lied", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich dachte schon, ich sei erledigt . . .", "tokens": ["Ich", "dach\u00b7te", "schon", ",", "ich", "sei", "er\u00b7le\u00b7digt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott nahm mich unter seinen Hut.", "tokens": ["Gott", "nahm", "mich", "un\u00b7ter", "sei\u00b7nen", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So eine fette Fastenpredigt", "tokens": ["So", "ei\u00b7ne", "fet\u00b7te", "Fas\u00b7ten\u00b7pre\u00b7digt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ist nach wie vor f\u00fcr viele gut.", "tokens": ["ist", "nach", "wie", "vor", "f\u00fcr", "vie\u00b7le", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "KOKOM", "APPR", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lang ihr diesen Schiebern borgt:", "tokens": ["So", "lang", "ihr", "die\u00b7sen", "Schie\u00b7bern", "borgt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bleibe da \u2013", "tokens": ["Ich", "blei\u00b7be", "da", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "F\u00fcr mich ist ausgesorgt!", "tokens": ["F\u00fcr", "mich", "ist", "aus\u00b7ge\u00b7sorgt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}