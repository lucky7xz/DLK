{"textgrid.poem.66134": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An B\u00f6ckh", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Leben, klein Geschenk, wenn dich mein Geist durchdenket,", "tokens": ["O", "Le\u00b7ben", ",", "klein", "Ge\u00b7schenk", ",", "wenn", "dich", "mein", "Geist", "durch\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir nichts als eine lange Nacht!", "tokens": ["Mir", "nichts", "als", "ei\u00b7ne", "lan\u00b7ge", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein hoffnungsreicher Lenz, der andern Rosen schenket,", "tokens": ["Dein", "hoff\u00b7nungs\u00b7rei\u00b7cher", "Lenz", ",", "der", "an\u00b7dern", "Ro\u00b7sen", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat nichts als Dornen mir gebracht.", "tokens": ["Hat", "nichts", "als", "Dor\u00b7nen", "mir", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Morgen ging hervor, verh\u00fcllt in Finsternissen,", "tokens": ["Mein", "Mor\u00b7gen", "ging", "her\u00b7vor", ",", "ver\u00b7h\u00fcllt", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Mittag ohne Sonnenschein;", "tokens": ["Mein", "Mit\u00b7tag", "oh\u00b7ne", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, Gott, darf ich von da auf meinen Abend schlie\u00dfen,", "tokens": ["Und", ",", "Gott", ",", "darf", "ich", "von", "da", "auf", "mei\u00b7nen", "A\u00b7bend", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VMFIN", "PPER", "APPR", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie tr\u00fcb', wie traurig wird er sein!", "tokens": ["Wie", "tr\u00fcb'", ",", "wie", "trau\u00b7rig", "wird", "er", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie schwer ist's, in der Welt sich G\u00f6nner zu erwecken!", "tokens": ["Wie", "schwer", "ist's", ",", "in", "der", "Welt", "sich", "G\u00f6n\u00b7ner", "zu", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "PRF", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwingt mich ein trauriges Geschick,", "tokens": ["Zwingt", "mich", "ein", "trau\u00b7ri\u00b7ges", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wie Satans Bild krummschleichend Staub zu lecken?", "tokens": ["Wie", "Sa\u00b7tans", "Bild", "krumm\u00b7schlei\u00b7chend", "Staub", "zu", "le\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Grausamer Weg zu meinem Gl\u00fcck.", "tokens": ["Grau\u00b7sa\u00b7mer", "Weg", "zu", "mei\u00b7nem", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Es sch\u00fcttelt jeder Tag von seinen leichten Schwingen", "tokens": ["Es", "sch\u00fct\u00b7telt", "je\u00b7der", "Tag", "von", "sei\u00b7nen", "leich\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Thoren oft ein Gl\u00fcck herab,", "tokens": ["F\u00fcr", "Tho\u00b7ren", "oft", "ein", "Gl\u00fcck", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel l\u00e4\u00dft mich nur brotlose Lieder singen \u2013", "tokens": ["Der", "Him\u00b7mel", "l\u00e4\u00dft", "mich", "nur", "brot\u00b7lo\u00b7se", "Lie\u00b7der", "sin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zeigt mir sp\u00e4ten Trost \u2013 das Grab.", "tokens": ["Und", "zeigt", "mir", "sp\u00e4\u00b7ten", "Trost", "\u2013", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}