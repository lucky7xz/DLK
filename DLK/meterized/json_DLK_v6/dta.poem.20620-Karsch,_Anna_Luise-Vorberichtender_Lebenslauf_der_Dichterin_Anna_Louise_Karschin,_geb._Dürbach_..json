{"dta.poem.20620": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Vorberichtender  \n Lebenslauf  \n der Dichterin  \n Anna Louise Karschin,  \n geb. D\u00fcrbach .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freu\u2019 Dich, Deutschlands Dichterin!", "tokens": ["Freu'", "Dich", ",", "Deutschlands", "Dich\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Freu\u2019 Dich hoch in Deinem Sinn;", "tokens": ["Freu'", "Dich", "hoch", "in", "Dei\u00b7nem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der K\u00f6nig hat befohlen mir,", "tokens": ["Der", "K\u00f6\u00b7nig", "hat", "be\u00b7foh\u00b7len", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Haus zu bauen Dir.", "tokens": ["Ein", "neu\u00b7es", "Haus", "zu", "bau\u00b7en", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}