{"textgrid.poem.50892": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies ist eine heilige Lenzmitternacht, o h\u00f6re:", "tokens": ["Dies", "ist", "ei\u00b7ne", "hei\u00b7li\u00b7ge", "Lenz\u00b7mit\u00b7ter\u00b7nacht", ",", "o", "h\u00f6\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "FM", "VVFIN", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Drangvoll alle Quellen gehn, laut rauschen ihre Ch\u00f6re!", "tokens": ["Drang\u00b7voll", "al\u00b7le", "Quel\u00b7len", "gehn", ",", "laut", "rau\u00b7schen", "ih\u00b7re", "Ch\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Tag und N\u00e4chte gleichen sich, hell zuckt ein Wetterschein,", "tokens": ["Tag", "und", "N\u00e4ch\u00b7te", "glei\u00b7chen", "sich", ",", "hell", "zuckt", "ein", "Wet\u00b7ter\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Lieblich aber, als ob ihn nur Bl\u00fctendunst geb\u00f6re!", "tokens": ["Lieb\u00b7lich", "a\u00b7ber", ",", "als", "ob", "ihn", "nur", "Bl\u00fc\u00b7ten\u00b7dunst", "ge\u00b7b\u00f6\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOKOM", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "L\u00f6se auf dein wallend Haar, weit la\u00df es sich zerstreun,", "tokens": ["L\u00f6\u00b7se", "auf", "dein", "wal\u00b7lend", "Haar", ",", "weit", "la\u00df", "es", "sich", "zer\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$,", "ADJD", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Schw\u00f6re, da\u00df ein jegliches mein nur, o mein geh\u00f6re!", "tokens": ["Schw\u00f6\u00b7re", ",", "da\u00df", "ein", "jeg\u00b7li\u00b7ches", "mein", "nur", ",", "o", "mein", "ge\u00b7h\u00f6\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "PIAT", "PPOSAT", "ADV", "$,", "FM", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "L\u00f6se deine Schleier auf, die Liebesbrust zu weihn,", "tokens": ["L\u00f6\u00b7se", "dei\u00b7ne", "Schlei\u00b7er", "auf", ",", "die", "Lie\u00b7bes\u00b7brust", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Bade sie im Wetterschein; mich treu zu lieben schw\u00f6re!", "tokens": ["Ba\u00b7de", "sie", "im", "Wet\u00b7ter\u00b7schein", ";", "mich", "treu", "zu", "lie\u00b7ben", "schw\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$.", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}