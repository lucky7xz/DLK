{"dta.poem.11575": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Genesene an die Hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Doch schon lag mein Haupt, wie s\u00fc\u00df!", "tokens": ["Doch", "schon", "lag", "mein", "Haupt", ",", "wie", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Hoffnung, dir im Schoos verborgen,", "tokens": ["Hoff\u00b7nung", ",", "dir", "im", "Schoos", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der Sieg gewonnen hie\u00df.", "tokens": ["Bis", "der", "Sieg", "ge\u00b7won\u00b7nen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Opfer bracht' ich allen G\u00f6ttern,", "tokens": ["Op\u00b7fer", "bracht'", "ich", "al\u00b7len", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch vergessen warest du;", "tokens": ["Doch", "ver\u00b7ges\u00b7sen", "wa\u00b7rest", "du", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seitw\u00e4rts von den ew'gen Rettern", "tokens": ["Seit\u00b7w\u00e4rts", "von", "den", "ew'\u00b7gen", "Ret\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sahest du dem Feste zu.", "tokens": ["Sa\u00b7hest", "du", "dem", "Fes\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O vergib, du Vielgetreue!", "tokens": ["O", "ver\u00b7gib", ",", "du", "Viel\u00b7ge\u00b7treu\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt aus deinem D\u00e4mmerlicht,", "tokens": ["Tritt", "aus", "dei\u00b7nem", "D\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich dir in's ewig neue,", "tokens": ["Da\u00df", "ich", "dir", "in's", "e\u00b7wig", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mondenhelle Angesicht", "tokens": ["Mon\u00b7den\u00b7hel\u00b7le", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einmal schaue recht von Herzen,", "tokens": ["Ein\u00b7mal", "schau\u00b7e", "recht", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Kind und sonder Harm;", "tokens": ["Wie", "ein", "Kind", "und", "son\u00b7der", "Harm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, nur Einmal ohne Schmerzen", "tokens": ["Ach", ",", "nur", "Ein\u00b7mal", "oh\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlie\u00dfe mich in deinen Arm!", "tokens": ["Schlie\u00b7\u00dfe", "mich", "in", "dei\u00b7nen", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}