{"textgrid.poem.43872": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Liebe,", "tokens": ["O", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Was vor innig-s\u00fc\u00dfe Triebe", "tokens": ["Was", "vor", "in\u00b7nig\u00b7s\u00fc\u00b7\u00dfe", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hegstu nicht in deiner Brust!", "tokens": ["Hegs\u00b7tu", "nicht", "in", "dei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrden doch nur die Ver\u00e4chter", "tokens": ["W\u00fcr\u00b7den", "doch", "nur", "die", "Ver\u00b7\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Einmahl unsrer Wollust W\u00e4chter,", "tokens": ["Ein\u00b7mahl", "uns\u00b7rer", "Wol\u00b7lust", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schw\u00f6r ich bey Amoenens Gunst,", "tokens": ["Schw\u00f6r", "ich", "bey", "A\u00b7moe\u00b7nens", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sie erstlich selbst nicht w\u00fcsten,", "tokens": ["Da\u00df", "sie", "erst\u00b7lich", "selbst", "nicht", "w\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob der Himmel zeitlich sey,", "tokens": ["Ob", "der", "Him\u00b7mel", "zeit\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und darnach vor Scham und Reu", "tokens": ["Und", "dar\u00b7nach", "vor", "Scham", "und", "Reu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "NN", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nur vom Zusehn sterben m\u00fcsten.", "tokens": ["Nur", "vom", "Zu\u00b7sehn", "ster\u00b7ben", "m\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Das th\u00e4ten sie,", "tokens": ["Das", "th\u00e4\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Das th\u00e4ten deine Triebe,", "tokens": ["Das", "th\u00e4\u00b7ten", "dei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "O Liebe!", "tokens": ["O", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}