{"textgrid.poem.28464": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vorsitz ist den L\u00e4usen f\u00fcr Fl\u00f6hen wol erlaubt,", "tokens": ["Der", "Vor\u00b7sitz", "ist", "den", "L\u00e4u\u00b7sen", "f\u00fcr", "Fl\u00f6\u00b7hen", "wol", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die, wie die Fl\u00f6h, im Busen nicht wohnen, nur ums Haupt.", "tokens": ["Die", ",", "wie", "die", "Fl\u00f6h", ",", "im", "Bu\u00b7sen", "nicht", "woh\u00b7nen", ",", "nur", "ums", "Haupt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "$,", "APPRART", "NN", "PTKNEG", "VVINF", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schmarotzer, die bey Hofe credentzen f\u00fcrstlich Gut,", "tokens": ["Schma\u00b7rot\u00b7zer", ",", "die", "bey", "Ho\u00b7fe", "cre\u00b7dent\u00b7zen", "f\u00fcrst\u00b7lich", "Gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sind f\u00fcr gemeinen Heuchlern befreyt zu gr\u00f6\u00dfrem Mut.", "tokens": ["Sind", "f\u00fcr", "ge\u00b7mei\u00b7nen", "Heuch\u00b7lern", "be\u00b7freyt", "zu", "gr\u00f6\u00df\u00b7rem", "Mut", "."], "token_info": ["word", "word", "word", "word", "word