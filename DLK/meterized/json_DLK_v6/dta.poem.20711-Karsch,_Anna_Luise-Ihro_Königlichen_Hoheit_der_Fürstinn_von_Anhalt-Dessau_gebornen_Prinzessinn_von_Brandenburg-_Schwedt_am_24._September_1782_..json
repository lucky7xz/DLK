{"dta.poem.20711": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ihro K\u00f6niglichen Hoheit  \n der  \n  F\u00fcrstinn von Anhalt-Dessau  \n gebornen Prinzessinn von Brandenburg-  \n Schwedt  \n  am 24. September 1782 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df war vor Zeiten                 ", "tokens": ["Gro\u00df", "war", "vor", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Manch f\u00fcrstlich Weib,", "tokens": ["Manch", "f\u00fcrst\u00b7lich", "Weib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im Kriegesstreiten", "tokens": ["Im", "Krie\u00b7ges\u00b7strei\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und im Betreib", "tokens": ["Und", "im", "Be\u00b7treib"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "++-+", "measure": "iambic.di"}, "line.5": {"text": "Der Throngesch\u00e4fte,", "tokens": ["Der", "Thron\u00b7ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die sie begann,", "tokens": ["Die", "sie", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Voll Geisteskr\u00e4fte,", "tokens": ["Voll", "Geis\u00b7tes\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Als w\u00e4rs ein Mann \u2014", "tokens": ["Als", "w\u00e4rs", "ein", "Mann"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Doch mehr als jene", "tokens": ["Doch", "mehr", "als", "je\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "PDS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bist Du mir gro\u00df!", "tokens": ["Bist", "Du", "mir", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du Seelensch\u00f6ne!", "tokens": ["Du", "See\u00b7len\u00b7sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Du Dein Loos,", "tokens": ["Die", "Du", "Dein", "Loos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Geliebt zu werden,", "tokens": ["Ge\u00b7liebt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "So ganz erf\u00fcllst,", "tokens": ["So", "ganz", "er\u00b7f\u00fcllst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und Grambeschwerden", "tokens": ["Und", "Gram\u00b7be\u00b7schwer\u00b7den"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Der Armen stillst,", "tokens": ["Der", "Ar\u00b7men", "stillst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mit Gottesmilde", "tokens": ["Mit", "Got\u00b7tes\u00b7mil\u00b7de"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Ringsher um Dich \u2014", "tokens": ["Rings\u00b7her", "um", "Dich"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Wie das Gefilde", "tokens": ["Wie", "das", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Im Sommer sich", "tokens": ["Im", "Som\u00b7mer", "sich"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Durch Thau erquicket,", "tokens": ["Durch", "Thau", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Labt sich Dein Land,", "tokens": ["Labt", "sich", "Dein", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Wenn Dich\u2019s erblicket", "tokens": ["Wenn", "Dich's", "er\u00b7bli\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Der sanft regieret,", "tokens": ["Der", "sanft", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Und Vater hei\u00dft,", "tokens": ["Und", "Va\u00b7ter", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Wenn\u2019s Volk ger\u00fchret", "tokens": ["Wenn's", "Volk", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Dich \u201eMutter\u201e preist. \u2014", "tokens": ["Dich", "\u201e", "Mut\u00b7ter", "\u201e", "preist", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "$(", "NN", "$(", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}