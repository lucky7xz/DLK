{"textgrid.poem.64650": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "277. Midas", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gold, und sonst nichts, war ihm des Wunsches werth;", "tokens": ["Gold", ",", "und", "sonst", "nichts", ",", "war", "ihm", "des", "Wun\u00b7sches", "werth", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "PIS", "$,", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Doch f\u00fchlt' er bald, da\u00df Gold nicht n\u00e4hrt.", "tokens": ["Doch", "f\u00fchlt'", "er", "bald", ",", "da\u00df", "Gold", "nicht", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ging es Spanien mit Peru", "tokens": ["So", "ging", "es", "Spa\u00b7ni\u00b7en", "mit", "Pe\u00b7ru"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "APPR", "NE"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Auch kamen die Midasohren dazu.", "tokens": ["Auch", "ka\u00b7men", "die", "Mi\u00b7da\u00b7soh\u00b7ren", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}