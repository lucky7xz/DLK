{"textgrid.poem.53370": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist, gew\u00fcnschter Fr\u00fchling, war,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist, gew\u00fcnschter Fr\u00fchling, war,", "tokens": ["Es", "ist", ",", "ge\u00b7w\u00fcnschter", "Fr\u00fch\u00b7ling", ",", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "$,", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dein Reichthum kr\u00f6hnt das gantze Jahr,", "tokens": ["Dein", "Reicht\u00b7hum", "kr\u00b7\u00f6hnt", "das", "gant\u00b7ze", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Du bist die Lust der Zeiten,", "tokens": ["Du", "bist", "die", "Lust", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Fu\u00dfStapff w\u00e4scht in Oele sich,", "tokens": ["Dein", "Fu\u00df", "Stapff", "w\u00e4scht", "in", "O\u00b7e\u00b7le", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "APPR", "NN", "PRF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dein Kleid kan mehr als K\u00f6niglich", "tokens": ["Dein", "Kleid", "kan", "mehr", "als", "K\u00f6\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Feld Berg und Thal bespreiten.", "tokens": ["Feld", "Berg", "und", "Thal", "be\u00b7sprei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein Morpheus ist so mannigfalt", "tokens": ["Kein", "Mor\u00b7pheus", "ist", "so", "man\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als du in tausend Lust-gestalt,", "tokens": ["Als", "du", "in", "tau\u00b7send", "Lust\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du l\u00e4chelst durch die Sonne,", "tokens": ["Du", "l\u00e4\u00b7chelst", "durch", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch dich ergetzt sich Mensch und Thier,", "tokens": ["Durch", "dich", "er\u00b7getzt", "sich", "Mensch", "und", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist der Glieder newe Zier", "tokens": ["Du", "bist", "der", "Glie\u00b7der", "ne\u00b7we", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd aller Hertzen Wonne.", "tokens": ["Vnd", "al\u00b7ler", "Hert\u00b7zen", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du bist das Leben in dem Meer", "tokens": ["Du", "bist", "das", "Le\u00b7ben", "in", "dem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du singest auff den Zweigen her,", "tokens": ["Du", "sin\u00b7gest", "auff", "den", "Zwei\u00b7gen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd wehest in den L\u00fcfften,", "tokens": ["Vnd", "we\u00b7hest", "in", "den", "L\u00fcff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du regst der B\u00e4ch' und Quellen Fluth,", "tokens": ["Du", "regst", "der", "B\u00e4ch'", "und", "Quel\u00b7len", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd bist das unersch\u00f6pffte Gut", "tokens": ["Vnd", "bist", "das", "un\u00b7er\u00b7sch\u00f6pff\u00b7te", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tieff in der Erden Kl\u00fcfften.", "tokens": ["Tieff", "in", "der", "Er\u00b7den", "Kl\u00fcff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wolan, sey liebreich als du thust,", "tokens": ["Wo\u00b7lan", ",", "sey", "lieb\u00b7reich", "als", "du", "thust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "ADJD", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fcll der Menschen Sinn mit Lust,", "tokens": ["Er\u00b7f\u00fcll", "der", "Men\u00b7schen", "Sinn", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Feld mit sch\u00f6nen Gaben,", "tokens": ["Das", "Feld", "mit", "sch\u00f6\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sey aller Dinge Gn\u00fcg und Schein,", "tokens": ["Sey", "al\u00b7ler", "Din\u00b7ge", "Gn\u00fcg", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die gantze Jahr-Zeit such' allein", "tokens": ["Die", "gant\u00b7ze", "Jahr\u00b7Zeit", "such'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Zum Freunde dich zu haben.", "tokens": ["Zum", "Freun\u00b7de", "dich", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du gleichest doch bey weiten nicht", "tokens": ["Du", "glei\u00b7chest", "doch", "bey", "wei\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem ewig-sch\u00f6nen Vorjahrs Licht", "tokens": ["Dem", "e\u00b7wig\u00b7sch\u00f6\u00b7nen", "Vor\u00b7jahrs", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach diesem schn\u00f6den Leben:", "tokens": ["Nach", "die\u00b7sem", "schn\u00f6\u00b7den", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir lassen s\u00e4mptlich deine Zier", "tokens": ["Wir", "las\u00b7sen", "s\u00e4mpt\u00b7lich", "dei\u00b7ne", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sampt aller Welt, so bald als wir", "tokens": ["Sampt", "al\u00b7ler", "Welt", ",", "so", "bald", "als", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$,", "ADV", "ADV", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von hinnen uns begeben.", "tokens": ["Von", "hin\u00b7nen", "uns", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie leicht k\u00f6mpt dir ein strenger Nort", "tokens": ["Wie", "leicht", "k\u00f6mpt", "dir", "ein", "stren\u00b7ger", "Nort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der f\u00fchret alle Hoffnung fort,", "tokens": ["Der", "f\u00fch\u00b7ret", "al\u00b7le", "Hoff\u00b7nung", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So man zu dir getragen,", "tokens": ["So", "man", "zu", "dir", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo bleibt so manches Hertzeleid", "tokens": ["Wo", "bleibt", "so", "man\u00b7ches", "Hert\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wieder deine gutte Zeit", "tokens": ["Da", "wie\u00b7der", "dei\u00b7ne", "gut\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchaus nicht wird verschlagen?", "tokens": ["Durc\u00b7haus", "nicht", "wird", "ver\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie lang auch wehrt dein gr\u00fcner Pracht?", "tokens": ["Wie", "lang", "auch", "wehrt", "dein", "gr\u00fc\u00b7ner", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vier Wochen werden hingebracht,", "tokens": ["Vier", "Wo\u00b7chen", "wer\u00b7den", "hin\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn k\u00f6mpt des Sommers Hitze,", "tokens": ["Denn", "k\u00f6mpt", "des", "Som\u00b7mers", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Hundsterns Unlust-voller Schein", "tokens": ["Des", "Hunds\u00b7terns", "Un\u00b7lust\u00b7vol\u00b7ler", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die schweren Wetter lass' ich seyn,", "tokens": ["Die", "schwe\u00b7ren", "Wet\u00b7ter", "lass'", "ich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Donner sampt dem Blitze.", "tokens": ["Den", "Don\u00b7ner", "sampt", "dem", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Lentz im Himmel aber wehrt,", "tokens": ["Der", "Lentz", "im", "Him\u00b7mel", "a\u00b7ber", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht durch Verdru\u00df noch Noth beschwert.", "tokens": ["Nicht", "durch", "Ver\u00b7dru\u00df", "noch", "Noth", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ohn alle Ma\u00df und Ende", "tokens": ["Ohn", "al\u00b7le", "Ma\u00df", "und", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleich mit der langen Ewigkeit,", "tokens": ["Gleich", "mit", "der", "lan\u00b7gen", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn da ist weder Ziel noch Zeit", "tokens": ["Denn", "da", "ist", "we\u00b7der", "Ziel", "noch", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch Nacht noch Sonnen-wende.", "tokens": ["Noch", "Nacht", "noch", "Son\u00b7nen\u00b7wen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "F\u00fcr tausend Sonnen ist das Licht", "tokens": ["F\u00fcr", "tau\u00b7send", "Son\u00b7nen", "ist", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst unser Gott, der wancket nicht,", "tokens": ["Selbst", "un\u00b7ser", "Gott", ",", "der", "wan\u00b7cket", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fleusst das Quell der Gnaden,", "tokens": ["Da", "fleusst", "das", "Quell", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In welches Birnstein-hellen Fluth", "tokens": ["In", "wel\u00b7ches", "Birn\u00b7stein\u00b7hel\u00b7len", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Seelen, so durch Christus Blut", "tokens": ["Die", "See\u00b7len", ",", "so", "durch", "Chris\u00b7tus", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gereinigt sind, sich baden.", "tokens": ["Ge\u00b7rei\u00b7nigt", "sind", ",", "sich", "ba\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da h\u00f6ret man den Lobgesang", "tokens": ["Da", "h\u00f6\u00b7ret", "man", "den", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Engel, und der Music Klang", "tokens": ["Der", "En\u00b7gel", ",", "und", "der", "Mu\u00b7sic", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch aller Hertzen dringen,", "tokens": ["Durch", "al\u00b7ler", "Hert\u00b7zen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd Frewden die kein Aug erkant,", "tokens": ["Vnd", "Frew\u00b7den", "die", "kein", "Aug", "er\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "PIAT", "NN", "VVPP", 