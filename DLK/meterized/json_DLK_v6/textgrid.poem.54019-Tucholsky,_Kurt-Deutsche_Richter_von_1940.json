{"textgrid.poem.54019": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Richter von 1940", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir stehen hier im Vereine", "tokens": ["Wir", "ste\u00b7hen", "hier", "im", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "in diesem Lederflaus;", "tokens": ["in", "die\u00b7sem", "Le\u00b7der\u00b7flaus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wie die abgestochenen Schweine", "tokens": ["wie", "die", "ab\u00b7ge\u00b7sto\u00b7che\u00b7nen", "Schwei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "sehn wir aus.", "tokens": ["sehn", "wir", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wir fechten die Kreuz und die Quere", "tokens": ["Wir", "fech\u00b7ten", "die", "Kreuz", "und", "die", "Que\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "mit Schlag und Hieb und Sto\u00df;", "tokens": ["mit", "Schlag", "und", "Hieb", "und", "Sto\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "wir schlachten uns um die Ehre \u2013!", "tokens": ["wir", "schlach\u00b7ten", "uns", "um", "die", "Eh\u00b7re", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Auf die Mensur!", "tokens": ["Auf", "die", "Men\u00b7sur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der deutsche Geist? Hier steht er.", "tokens": ["Der", "deut\u00b7sche", "Geist", "?", "Hier", "steht", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie unsere Tiefquart sitzt!", "tokens": ["Wie", "un\u00b7se\u00b7re", "Tief\u00b7quart", "sitzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir machen Hackepeter,", "tokens": ["Wir", "ma\u00b7chen", "Ha\u00b7cke\u00b7pe\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "da\u00df die rote Suppe spritzt.", "tokens": ["da\u00df", "die", "ro\u00b7te", "Sup\u00b7pe", "spritzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir sind die Bl\u00fcte der Arier", "tokens": ["Wir", "sind", "die", "Bl\u00fc\u00b7te", "der", "A\u00b7rier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und verachten k\u00fchl und grandios", "tokens": ["und", "ver\u00b7ach\u00b7ten", "k\u00fchl", "und", "gran\u00b7di\u00b7os"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "die verrohten Proletarier \u2013", "tokens": ["die", "ver\u00b7roh\u00b7ten", "Pro\u00b7le\u00b7ta\u00b7rier", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Auf die Mensur!", "tokens": ["Auf", "die", "Men\u00b7sur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Gebunden!", "tokens": ["Ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Wir sitzen in zwanzig Jahren", "tokens": ["Wir", "sit\u00b7zen", "in", "zwan\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "mit zerhacktem Angesicht", "tokens": ["mit", "zer\u00b7hack\u00b7tem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in W\u00fcrde und Talaren", "tokens": ["in", "W\u00fcr\u00b7de", "und", "Ta\u00b7la\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00fcber euch zu Gericht.", "tokens": ["\u00fc\u00b7ber", "euch", "zu", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dann werden wirs euch zeigen", "tokens": ["Dann", "wer\u00b7den", "wirs", "euch", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAINF", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "in Sprechstunden und B\u00fcros . . .", "tokens": ["in", "Sprech\u00b7stun\u00b7den", "und", "B\u00fc\u00b7ros", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$.", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "ihr habt euch zu ducken, zu schweigen", "tokens": ["ihr", "habt", "euch", "zu", "du\u00b7cken", ",", "zu", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Auf die Mensur!", "tokens": ["Auf", "die", "Men\u00b7sur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Gebunden!", "tokens": ["Ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Fertig!", "tokens": ["Fer\u00b7tig", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Wie lange, M\u00e4nner und Frauen,", "tokens": ["Wie", "lan\u00b7ge", ",", "M\u00e4n\u00b7ner", "und", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "seht ihr euch das mit an \u2013?", "tokens": ["seht", "ihr", "euch", "das", "mit", "an", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "APPR", "APPR", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sie sich heut selber verhauen:", "tokens": ["Wenn", "sie", "sich", "heut", "sel\u00b7ber", "ver\u00b7hau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Euch fallen sie morgen an!", "tokens": ["Euch", "fal\u00b7len", "sie", "mor\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihr seid das Volk und die Masse", "tokens": ["Ihr", "seid", "das", "Volk", "und", "die", "Mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "von der Etsch bis an den Rhein:", "tokens": ["von", "der", "Etsch", "bis", "an", "den", "Rhein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "soll ", "tokens": ["soll"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "sollen ", "tokens": ["sol\u00b7len"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Fertig! Los! Los!", "tokens": ["Fer\u00b7tig", "!", "Los", "!", "Los", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "NE", "$.", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}