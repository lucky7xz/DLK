{"textgrid.poem.54534": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Danck sey dir! ewig Danck! vor h\u00f6chste Wunderthaten/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Danck sey dir! ewig Danck! vor h\u00f6chste Wunderthaten/", "tokens": ["Danck", "sey", "dir", "!", "e\u00b7wig", "Danck", "!", "vor", "h\u00f6chs\u00b7te", "Wun\u00b7der\u00b7tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$.", "ADJD", "NN", "$.", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Danck sey dir/ Herr/ da\u00df ich dein Wunderwerck an mir", "tokens": ["Danck", "sey", "dir", "/", "Herr", "/", "da\u00df", "ich", "dein", "Wun\u00b7der\u00b7werck", "an", "mir"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$(", "NN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Allmacht deiner Hand und H\u00f6chste Gunst versp\u00fcr/", "tokens": ["Und", "All\u00b7macht", "dei\u00b7ner", "Hand", "und", "H\u00f6chs\u00b7te", "Gunst", "ver\u00b7sp\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dadurch in Mutterleib ich dein Gesch\u00f6pff gerathen.", "tokens": ["Da\u00b7durch", "in", "Mut\u00b7ter\u00b7leib", "ich", "dein", "Ge\u00b7sch\u00f6pff", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du hast mit deinem Licht in den verborgnen Schatten", "tokens": ["Du", "hast", "mit", "dei\u00b7nem", "Licht", "in", "den", "ver\u00b7borg\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Seel in mir entsteckt/ die Glieder die ich f\u00fchr/", "tokens": ["Die", "Seel", "in", "mir", "ent\u00b7steckt", "/", "die", "Glie\u00b7der", "die", "ich", "f\u00fchr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$(", "ART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Beiner Meisterst\u00fcck/ das Fleisch/ der Adern Zier/", "tokens": ["Der", "Bei\u00b7ner", "Meis\u00b7ter\u00b7st\u00fcck", "/", "das", "Fleisch", "/", "der", "A\u00b7dern", "Zier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sind Wunder. Wunder ists was du mir wilst verstatten;", "tokens": ["Sind", "Wun\u00b7der", ".", "Wun\u00b7der", "ists", "was", "du", "mir", "wilst", "ver\u00b7stat\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "NN", "VAFIN", "PWS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch \u00fcber Wunder geht Herr da\u00df ich dich erkenn/", "tokens": ["Doch", "\u00fc\u00b7ber", "Wun\u00b7der", "geht", "Herr", "da\u00df", "ich", "dich", "er\u00b7kenn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "NN", "KOUS", "PPER", "PRF", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df ich/ ich bl\u00f6der Mensch/ dich Gott und Sch\u00f6pfer nenn/", "tokens": ["Da\u00df", "ich", "/", "ich", "bl\u00f6\u00b7der", "Mensch", "/", "dich", "Gott", "und", "Sch\u00f6p\u00b7fer", "nenn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPER", "ADJA", "NN", "$(", "PPER", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df ich mich von der Erd' hinauf in Himmel schwinge.", "tokens": ["Da\u00df", "ich", "mich", "von", "der", "Erd'", "hin\u00b7auf", "in", "Him\u00b7mel", "schwin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mehr \u00fcber Wunder geht da\u00df du dich mir entdeckt/", "tokens": ["Mehr", "\u00fc\u00b7ber", "Wun\u00b7der", "geht", "da\u00df", "du", "dich", "mir", "ent\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "KOUS", "PPER", "PRF", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und Zuversicht zu dir in meinem Geist erweckt/", "tokens": ["Und", "Zu\u00b7ver\u00b7sicht", "zu", "dir", "in", "mei\u00b7nem", "Geist", "er\u00b7