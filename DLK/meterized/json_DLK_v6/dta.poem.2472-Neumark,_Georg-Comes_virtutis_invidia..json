{"dta.poem.2472": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Comes virtutis invidia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.97"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Esel mu\u00df sich stets so lang' er lebet plagen/", "tokens": ["Ein", "E\u00b7sel", "mu\u00df", "sich", "stets", "so", "lang'", "er", "le\u00b7bet", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "ADV", "ADV", "PPER", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Disteln speist man ihn/ mu\u00df t\u00e4glich B\u00fcrden tragen/", "tokens": ["Mit", "Dis\u00b7teln", "speist", "man", "ihn", "/", "mu\u00df", "t\u00e4g\u00b7lich", "B\u00fcr\u00b7den", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PPER", "$(", "VMFIN", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn aber er verrekkt so schlept man ihn hinau\u00df", "tokens": ["Wenn", "a\u00b7ber", "er", "ver\u00b7rekkt", "so", "schlept", "man", "ihn", "hin\u00b7au\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "ADV", "VVFIN", "PIS", "PPER", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nimmt ihm die Bein\u2019 und Haut macht Pfeiff und Drum-", "tokens": ["Nimmt", "ihm", "die", "Bein'", "und", "Haut", "macht", "Pfeiff", "und", "Drum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "NN", "VVFIN", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "meln drau\u00df;", "tokens": ["meln", "drau\u00df", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.6": {"text": "So ist ein Geitzhal\u00df auch so bald er nur gestorben/", "tokens": ["So", "ist", "ein", "Geitz\u00b7hal\u00df", "auch", "so", "bald", "er", "nur", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "ADV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "K\u00f6mmt mancher frischer Sohn/ greifft an was er erwor", "tokens": ["K\u00f6mmt", "man\u00b7cher", "fri\u00b7scher", "Sohn", "/", "greifft", "an", "was", "er", "er\u00b7wor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$(", "VVFIN", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Schlemmt/ demmt/ und ist auff nichts als Uppigkeit be-", "tokens": ["Schlemmt", "/", "demmt", "/", "und", "ist", "auff", "nichts", "als", "Up\u00b7pig\u00b7keit", "be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "KON", "VAFIN", "APPR", "PIS", "KOKOM", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "dacht.", "tokens": ["dacht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Was sch\u00e4ndlich wird erkratzt/ wird sch\u00e4ndlich durchge-", "tokens": ["Was", "sch\u00e4nd\u00b7lich", "wird", "er\u00b7kratzt", "/", "wird", "sch\u00e4nd\u00b7lich", "durch\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "VVPP", "$(", "VAFIN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "bracht.", "tokens": ["bracht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}