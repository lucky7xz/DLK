{"dta.poem.21741": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An den Schatten des Numan.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Numan, dein gedenkt das Herz,", "tokens": ["Nu\u00b7man", ",", "dein", "ge\u00b7denkt", "das", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer werde dein vergessen!", "tokens": ["Nim\u00b7mer", "wer\u00b7de", "dein", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft noch soll der herbe Schmerz", "tokens": ["Oft", "noch", "soll", "der", "her\u00b7be", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir um dich die Wange n\u00e4ssen.", "tokens": ["Mir", "um", "dich", "die", "Wan\u00b7ge", "n\u00e4s\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lange noch sey dir mein Leid,", "tokens": ["Lan\u00b7ge", "noch", "sey", "dir", "mein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lange noch mein Lied geweiht.", "tokens": ["Lan\u00b7ge", "noch", "mein", "Lied", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, den in der grausen Noth", "tokens": ["Du", ",", "den", "in", "der", "grau\u00b7sen", "Noth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir der Gott zum Retter sandte;", "tokens": ["Mir", "der", "Gott", "zum", "Ret\u00b7ter", "sand\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du, der von mir herben Tod,", "tokens": ["Du", ",", "der", "von", "mir", "her\u00b7ben", "Tod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch herbre Schande wandte;", "tokens": ["Und", "noch", "herb\u00b7re", "Schan\u00b7de", "wand\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den der Thaten Edelste", "tokens": ["Den", "der", "Tha\u00b7ten", "E\u00b7dels\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "--+----", "measure": "anapaest.init"}, "line.6": {"text": "Ach sein Herzblut kostete. . . .", "tokens": ["Ach", "sein", "Herz\u00b7blut", "kos\u00b7te\u00b7te", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVFIN", "$.", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dunkles Schicksal, ach warum", "tokens": ["Dunk\u00b7les", "Schick\u00b7sal", ",", "ach", "wa\u00b7rum"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Musste so mein Numan enden?", "tokens": ["Muss\u00b7te", "so", "mein", "Nu\u00b7man", "en\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Giovanni, ach warum", "tokens": ["Gio\u00b7van\u00b7ni", ",", "ach", "wa\u00b7rum"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "XY", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Musstest du den Freund vollenden? . . .", "tokens": ["Muss\u00b7test", "du", "den", "Freund", "voll\u00b7en\u00b7den", "?", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch das Schicksal, ernst und stumm,", "tokens": ["Doch", "das", "Schick\u00b7sal", ",", "ernst", "und", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Achtet st\u00f6rrig kein Warum! . . .", "tokens": ["Ach\u00b7tet", "st\u00f6r\u00b7rig", "kein", "Wa\u00b7rum", "!", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "ADJD", "PIAT", "NN", "$.", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Numan, dein entseelter Staub", "tokens": ["Nu\u00b7man", ",", "dein", "ent\u00b7seel\u00b7ter", "Staub"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt im Schooss des Meers, und modert . . .", "tokens": ["Liegt", "im", "Scho\u00b7oss", "des", "Meers", ",", "und", "mo\u00b7dert", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "$.", "$.", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Oder ward vielleicht der Raub", "tokens": ["O\u00b7der", "ward", "viel\u00b7leicht", "der", "Raub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcstre Fluth dir abgefodert?", "tokens": ["D\u00fcst\u00b7re", "Fluth", "dir", "ab\u00b7ge\u00b7fo\u00b7dert", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat vielleicht am \u00f6den Strand", "tokens": ["Hat", "viel\u00b7leicht", "am", "\u00f6\u00b7den", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihn verscharrt des Fremdlings Hand?", "tokens": ["Ihn", "ver\u00b7scharrt", "des", "Fremd\u00b7lings", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun getrost! dein bessrer Theil,", "tokens": ["Nun", "ge\u00b7trost", "!", "dein", "bess\u00b7rer", "Theil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Numan, bleibet unverlohren.", "tokens": ["Nu\u00b7man", ",", "blei\u00b7bet", "un\u00b7ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward nicht Christus, Aller Heil,", "tokens": ["Ward", "nicht", "Chris\u00b7tus", ",", "Al\u00b7ler", "Heil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Numan, auch f\u00fcr dich gebohren?", "tokens": ["Nu\u00b7man", ",", "auch", "f\u00fcr", "dich", "ge\u00b7boh\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch sollte, rein und sch\u00f6n,", "tokens": ["Den\u00b7noch", "soll\u00b7te", ",", "rein", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Numans Seele untergehn?", "tokens": ["Nu\u00b7mans", "See\u00b7le", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Numan, nein, war fromm und gut.", "tokens": ["Nu\u00b7man", ",", "nein", ",", "war", "fromm", "und", "gut", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKANT", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, auch Numan ward getaufet,", "tokens": ["Nein", ",", "auch", "Nu\u00b7man", "ward", "ge\u00b7tau\u00b7fet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward durch Wasser und durch Blut", "tokens": ["Ward", "durch", "Was\u00b7ser", "und", "durch", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom Verderben losgekaufet.", "tokens": ["Vom", "Ver\u00b7der\u00b7ben", "los\u00b7ge\u00b7kau\u00b7fet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Numan, Numan auch f\u00fcr die", "tokens": ["Nu\u00b7man", ",", "Nu\u00b7man", "auch", "f\u00fcr", "die"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "ADV", "APPR", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Starb, der mich erl\u00f6ste, mich!", "tokens": ["Starb", ",", "der", "mich", "er\u00b7l\u00f6s\u00b7te", ",", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Einstens, wenn die Wage klingt,", "tokens": ["Eins\u00b7tens", ",", "wenn", "die", "Wa\u00b7ge", "klingt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn der ernste Richter richtet;", "tokens": ["Wenn", "der", "erns\u00b7te", "Rich\u00b7ter", "rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Rache blitzbeschwingt,", "tokens": ["Wenn", "die", "Ra\u00b7che", "blitz\u00b7be\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verruchte Schaar vernichtet;", "tokens": ["Die", "ver\u00b7ruch\u00b7te", "Schaar", "ver\u00b7nich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Gnade, blutvers\u00f6hnt,", "tokens": ["Wenn", "die", "Gna\u00b7de", ",", "blut\u00b7ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich und Giovanni kr\u00f6nt;", "tokens": ["Mich", "und", "Gio\u00b7van\u00b7ni", "kr\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "NE", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Giovanni dann und ich", "tokens": ["Gio\u00b7van\u00b7ni", "dann", "und", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "KON", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Fassen Numan in der Mitten,", "tokens": ["Fas\u00b7sen", "Nu\u00b7man", "in", "der", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Richter neiget sich", "tokens": ["Und", "der", "Rich\u00b7ter", "nei\u00b7get", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Huldreich zu der S\u00fcnder Bitten.", "tokens": ["Huld\u00b7reich", "zu", "der", "S\u00fcn\u00b7der", "Bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja der Richter wird vers\u00f6hnt,", "tokens": ["Ja", "der", "Rich\u00b7ter", "wird", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auch Numan wird gekr\u00f6nt.", "tokens": ["Und", "auch", "Nu\u00b7man", "wird", "ge\u00b7kr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Dann mit Numan Arm in Arm", "tokens": ["Dann", "mit", "Nu\u00b7man", "Arm", "in", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird mein Giovanni wallen.", "tokens": ["Wird", "mein", "Gio\u00b7van\u00b7ni", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aller Hader, aller Harm", "tokens": ["Al\u00b7ler", "Ha\u00b7der", ",", "al\u00b7ler", "Harm"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endet sich in Wohlgefallen.", "tokens": ["En\u00b7det", "sich", "in", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Froh wird dann Bianca stehn,", "tokens": ["Froh", "wird", "dann", "Bi\u00b7an\u00b7ca", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "NE", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Froh die Freunde wandeln sehn.", "tokens": ["Froh", "die", "Freun\u00b7de", "wan\u00b7deln", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}