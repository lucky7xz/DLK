{"textgrid.poem.49166": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Christliches Sterblied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steh' in Furcht und Noth,", "tokens": ["Ich", "steh'", "in", "Furcht", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da mein verlangter Tod", "tokens": ["Da", "mein", "ver\u00b7lang\u00b7ter", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich fordert von der Erden;", "tokens": ["Mich", "for\u00b7dert", "von", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es steht auf diesem Blick", "tokens": ["Es", "steht", "auf", "die\u00b7sem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Ungl\u00fcck und mein Gl\u00fcck,", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "und", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das ewig mir soll werden.", "tokens": ["Das", "e\u00b7wig", "mir", "soll", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach Gott, kein Sturm der See", "tokens": ["Ach", "Gott", ",", "kein", "Sturm", "der", "See"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Thut nicht dem Schiff so weh',", "tokens": ["Thut", "nicht", "dem", "Schiff", "so", "weh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hin und her mu\u00df schweben,", "tokens": ["Das", "hin", "und", "her", "mu\u00df", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "KON", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als diese Reise mir,", "tokens": ["Als", "die\u00b7se", "Rei\u00b7se", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denk' ich, was dort und hier", "tokens": ["Denk'", "ich", ",", "was", "dort", "und", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mir noch zu \u00fcberstreben.", "tokens": ["Mir", "noch", "zu", "\u00fc\u00b7bers\u00b7tre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier stellt sich Satan mir", "tokens": ["Hier", "stellt", "sich", "Sa\u00b7tan", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit meinen S\u00fcnden f\u00fcr", "tokens": ["Mit", "mei\u00b7nen", "S\u00fcn\u00b7den", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dr\u00e4ut mir mit der H\u00f6llen;", "tokens": ["Und", "dr\u00e4ut", "mir", "mit", "der", "H\u00f6l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort lockt dein Himmel mich", "tokens": ["Dort", "lockt", "dein", "Him\u00b7mel", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und spricht, ich soll auf dich,", "tokens": ["Und", "spricht", ",", "ich", "soll", "auf", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein Hort, mein' Hoffnung stellen.", "tokens": ["Mein", "Hort", ",", "mein'", "Hoff\u00b7nung", "stel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr Jesu, Gottessohn,", "tokens": ["Herr", "Je\u00b7su", ",", "Got\u00b7tes\u00b7sohn", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der sich von seinem Thron", "tokens": ["Der", "sich", "von", "sei\u00b7nem", "Thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Welt zu Dienst begeben", "tokens": ["Der", "Welt", "zu", "Dienst", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mir zu lieb' und gut", "tokens": ["Und", "mir", "zu", "lieb'", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKZU", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein todtes Fleisch und Blut", "tokens": ["Mein", "tod\u00b7tes", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gew\u00fcrdigt zu beleben,", "tokens": ["Ge\u00b7w\u00fcr\u00b7digt", "zu", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Komm', o mein h\u00f6chstes Gut,", "tokens": ["Komm'", ",", "o", "mein", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und nimm dein Kind in Hut,", "tokens": ["Und", "nimm", "dein", "Kind", "in", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das du f\u00fcr dich erkoren", "tokens": ["Das", "du", "f\u00fcr", "dich", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dir zur lieben Braut,", "tokens": ["Und", "dir", "zur", "lie\u00b7ben", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Liebster, hast vertraut,", "tokens": ["Mein", "Liebs\u00b7ter", ",", "hast", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Eh' da\u00df die Zeit geboren.", "tokens": ["Eh'", "da\u00df", "die", "Zeit", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich bin der Erde satt,", "tokens": ["Ich", "bin", "der", "Er\u00b7de", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von M\u00fch' und Jahren matt;", "tokens": ["Von", "M\u00fch'", "und", "Jah\u00b7ren", "matt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Versammle mein Gebeine", "tokens": ["Ver\u00b7samm\u00b7le", "mein", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinen s\u00fcssen Sand;", "tokens": ["In", "sei\u00b7nen", "s\u00fcs\u00b7sen", "Sand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Geist, dein theures Pfand,", "tokens": ["Mein", "Geist", ",", "dein", "theu\u00b7res", "Pfand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bleib' aber stets der Deine.", "tokens": ["Bleib'", "a\u00b7ber", "stets", "der", "Dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Derselb' ist her von dir", "tokens": ["Der\u00b7selb'", "ist", "her", "von", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und hielt sich dein allhier", "tokens": ["Und", "hielt", "sich", "dein", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wol in Leid als Freuden;", "tokens": ["So", "wol", "in", "Leid", "als", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mu\u00df er jetzt auch dein", "tokens": ["So", "mu\u00df", "er", "jetzt", "auch", "dein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In seiner Heimfahrt sein,", "tokens": ["In", "sei\u00b7ner", "Heim\u00b7fahrt", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dies fordert selbst dein Leiden.", "tokens": ["Dies", "for\u00b7dert", "selbst", "dein", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Tod, den du verschm\u00e4ht,", "tokens": ["Der", "Tod", ",", "den", "du", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In deinem Tod' ert\u00f6dt,", "tokens": ["In", "dei\u00b7nem", "Tod'", "er\u00b7t\u00f6dt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei meines Todes Leben;", "tokens": ["Sei", "mei\u00b7nes", "To\u00b7des", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Grab, das dich umschlo\u00df,", "tokens": ["Das", "Grab", ",", "das", "dich", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lass' mir in seinem Scho\u00df", "tokens": ["Lass'", "mir", "in", "sei\u00b7nem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein sanftes Ruhbett geben.", "tokens": ["Ein", "sanf\u00b7tes", "Ruh\u00b7bett", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Hierauf umfass' ich dich", "tokens": ["Hier\u00b7auf", "um\u00b7fass'", "ich", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und h\u00fclle, Jesu, mich", "tokens": ["Und", "h\u00fcl\u00b7le", ",", "Je\u00b7su", ",", "mich"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "NE", "$,", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deine blut'gen Wunden", "tokens": ["In", "dei\u00b7ne", "blut'\u00b7gen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frei, unbesorgt und froh,", "tokens": ["Frei", ",", "un\u00b7be\u00b7sorgt", "und", "froh", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wei\u00df, da\u00df ich also", "tokens": ["Und", "wei\u00df", ",", "da\u00df", "ich", "al\u00b7so"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dir ewig bin verbunden.", "tokens": ["Dir", "e\u00b7wig", "bin", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ach ja, ich bleibe dein,", "tokens": ["Ach", "ja", ",", "ich", "blei\u00b7be", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da, wo der helle Schein", "tokens": ["Da", ",", "wo", "der", "hel\u00b7le", "Schein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sterne mich umgeben,", "tokens": ["Der", "Ster\u00b7ne", "mich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und, Jesu, du in mir,", "tokens": ["Und", ",", "Je\u00b7su", ",", "du", "in", "mir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und, Jesu, ich in dir", "tokens": ["Und", ",", "Je\u00b7su", ",", "ich", "in", "dir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NE", "$,", "PPER", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ohn' Ende werden leben.", "tokens": ["Ohn'", "En\u00b7de", "wer\u00b7den", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Weg nur f\u00e4llt mir schwer;", "tokens": ["Der", "Weg", "nur", "f\u00e4llt", "mir", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lass' denn, mein Heil, vorher", "tokens": ["Lass'", "denn", ",", "mein", "Heil", ",", "vor\u00b7her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Fleisch und Blut mich weiden,", "tokens": ["Dein", "Fleisch", "und", "Blut", "mich", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies st\u00e4rkt mir Seel' und Muth", "tokens": ["Dies", "st\u00e4rkt", "mir", "Seel'", "und", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und macht die Bahne gut;", "tokens": ["Und", "macht", "die", "Bah\u00b7ne", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So fahr' ich hin mit Freuden.", "tokens": ["So", "fahr'", "ich", "hin", "mit", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}