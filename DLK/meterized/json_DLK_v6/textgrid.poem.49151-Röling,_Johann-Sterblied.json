{"textgrid.poem.49151": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Sterblied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erschrick, o liebste Seele, nicht,", "tokens": ["Er\u00b7schrick", ",", "o", "liebs\u00b7te", "See\u00b7le", ",", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "$,", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn dich des Todes Noth anficht.", "tokens": ["Wenn", "dich", "des", "To\u00b7des", "Noth", "an\u00b7ficht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, worauf ich leb' und sterbe,", "tokens": ["Ich", "wei\u00df", ",", "wo\u00b7rauf", "ich", "leb'", "und", "ster\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Grund des Glaubens stehet fest;", "tokens": ["Mein", "Grund", "des", "Glau\u00b7bens", "ste\u00b7het", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn mich die ganze Welt verl\u00e4sst,", "tokens": ["Wenn", "mich", "die", "gan\u00b7ze", "Welt", "ver\u00b7l\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So werd' ich erst des Himmels Erbe.", "tokens": ["So", "werd'", "ich", "erst", "des", "Him\u00b7mels", "Er\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin des H\u00f6chsten liebstes Kind,", "tokens": ["Ich", "bin", "des", "H\u00f6chs\u00b7ten", "liebs\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat mich, eh' die Welt gegr\u00fcndt,", "tokens": ["Er", "hat", "mich", ",", "eh'", "die", "Welt", "ge\u00b7gr\u00fcndt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Jesu, seinem Sohn, erw\u00e4hlet.", "tokens": ["In", "Je\u00b7su", ",", "sei\u00b7nem", "Sohn", ",", "er\u00b7w\u00e4h\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hierauf steht meine Zuversicht,", "tokens": ["Hier\u00b7auf", "steht", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was mir sein Vaterherz verspricht,", "tokens": ["Was", "mir", "sein", "Va\u00b7ter\u00b7herz", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das bleibt mir ewig ungeschm\u00e4let.", "tokens": ["Das", "bleibt", "mir", "e\u00b7wig", "un\u00b7ge\u00b7schm\u00e4\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was will mir hier zuwider sein?", "tokens": ["Was", "will", "mir", "hier", "zu\u00b7wi\u00b7der", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Leben oder Todespein,", "tokens": ["Nicht", "Le\u00b7ben", "o\u00b7der", "To\u00b7de\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch was hier oder dort zu nennen.", "tokens": ["Noch", "was", "hier", "o\u00b7der", "dort", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein tiefes Leid, noch hohe Pracht,", "tokens": ["Kein", "tie\u00b7fes", "Leid", ",", "noch", "ho\u00b7he", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Engel oder andre Macht", "tokens": ["Kein", "En\u00b7gel", "o\u00b7der", "and\u00b7re", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann mich von Gottes Liebe trennen.", "tokens": ["Kann", "mich", "von", "Got\u00b7tes", "Lie\u00b7be", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drauf halt' ich meinem Sch\u00f6pfer still;", "tokens": ["Drauf", "halt'", "ich", "mei\u00b7nem", "Sch\u00f6p\u00b7fer", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gehe mir dann, wie es will,", "tokens": ["Es", "ge\u00b7he", "mir", "dann", ",", "wie", "es", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Frommen H\u00f6ll' ist auf der Erden.", "tokens": ["Der", "From\u00b7men", "H\u00f6ll'", "ist", "auf", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was kr\u00e4nkt mich diese kurze Zeit,", "tokens": ["Was", "kr\u00e4nkt", "mich", "die\u00b7se", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr welcher Noth die Ewigkeit", "tokens": ["F\u00fcr", "wel\u00b7cher", "Noth", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir soll zu lauter Freude werden.", "tokens": ["Mir", "soll", "zu", "lau\u00b7ter", "Freu\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gieb dich zufrieden, liebe Seel',", "tokens": ["Gieb", "dich", "zu\u00b7frie\u00b7den", ",", "lie\u00b7be", "Seel'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leg' ab gutwillig deine H\u00f6hl',", "tokens": ["Leg'", "ab", "gut\u00b7wil\u00b7lig", "dei\u00b7ne", "H\u00f6hl'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Leib, das Haus so vieler S\u00fcnden.", "tokens": ["Den", "Leib", ",", "das", "Haus", "so", "vie\u00b7ler", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In diesem Fleisch, das jetzt so schwach", "tokens": ["In", "die\u00b7sem", "Fleisch", ",", "das", "jetzt", "so", "schwach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und voll von allem Ungemach,", "tokens": ["Und", "voll", "von", "al\u00b7lem", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird Gott sich selber k\u00fcnftig finden.", "tokens": ["Wird", "Gott", "sich", "sel\u00b7ber", "k\u00fcnf\u00b7tig", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Trotz sei dem Teufel und der Welt,", "tokens": ["Trotz", "sei", "dem", "Teu\u00b7fel", "und", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir so schwer zu tragen f\u00e4llt;", "tokens": ["Die", "mir", "so", "schwer", "zu", "tra\u00b7gen", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mich nichts soll von Gott abscheiden,", "tokens": ["Wenn", "mich", "nichts", "soll", "von", "Gott", "ab\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So will ich gern zufrieden sein,", "tokens": ["So", "will", "ich", "gern", "zu\u00b7frie\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sollt' ich auch derselben Pein", "tokens": ["Und", "sollt'", "ich", "auch", "der\u00b7sel\u00b7ben", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel tausendmal noch schwerer leiden.", "tokens": ["Viel", "tau\u00b7send\u00b7mal", "noch", "schwe\u00b7rer", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}