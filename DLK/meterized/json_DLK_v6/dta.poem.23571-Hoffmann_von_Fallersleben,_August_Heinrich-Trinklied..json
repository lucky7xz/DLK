{"dta.poem.23571": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den St\u00f6psel weg! und schenket ein!", "tokens": ["Den", "St\u00f6p\u00b7sel", "weg", "!", "und", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schenkt ein, da\u00df unser Herz ergl\u00fche,", "tokens": ["Schenkt", "ein", ",", "da\u00df", "un\u00b7ser", "Herz", "er\u00b7gl\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie die Blum' am Sonnenschein,", "tokens": ["Und", "wie", "die", "Blum'", "am", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So an der Glut des Weins erbl\u00fche!", "tokens": ["So", "an", "der", "Glut", "des", "Weins", "er\u00b7bl\u00fc\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den St\u00f6psel weg! dann wird es klar:", "tokens": ["Den", "St\u00f6p\u00b7sel", "weg", "!", "dann", "wird", "es", "klar", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sich in einem Nu gefunden,", "tokens": ["Was", "sich", "in", "ei\u00b7nem", "Nu", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist sogleich f\u00fcr jedes Jahr,", "tokens": ["Das", "ist", "sog\u00b7leich", "f\u00fcr", "je\u00b7des", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja f\u00fcr die Ewigkeit verbunden.", "tokens": ["Ja", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So recht! jetzt werft den St\u00f6psel fort!", "tokens": ["So", "recht", "!", "jetzt", "werft", "den", "St\u00f6p\u00b7sel", "fort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ei, der verfluchte Kerkermeister,", "tokens": ["Ei", ",", "der", "ver\u00b7fluch\u00b7te", "Ker\u00b7ker\u00b7meis\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der wollt' uns zwingherrn Wein und Wort,", "tokens": ["Der", "wollt'", "uns", "zwing\u00b7herrn", "Wein", "und", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trennen alle guten Geister!", "tokens": ["Und", "tren\u00b7nen", "al\u00b7le", "gu\u00b7ten", "Geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der St\u00f6psel war Philisterei,", "tokens": ["Der", "St\u00f6p\u00b7sel", "war", "Phi\u00b7lis\u00b7te\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns nichts Gutes wollte g\u00f6nnen \u2014", "tokens": ["Die", "uns", "nichts", "Gu\u00b7tes", "woll\u00b7te", "g\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Flasch' und unser Herz ist frei,", "tokens": ["Die", "Fla\u00b7sch'", "und", "un\u00b7ser", "Herz", "ist", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und wir, wir zeigen was wir k\u00f6nnen.", "tokens": ["Und", "wir", ",", "wir", "zei\u00b7gen", "was", "wir", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}