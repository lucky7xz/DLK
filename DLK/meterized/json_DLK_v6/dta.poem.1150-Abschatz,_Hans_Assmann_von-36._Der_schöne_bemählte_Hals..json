{"dta.poem.1150": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "36. Der sch\u00f6ne bem\u00e4hlte Hals.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ander sagt vielleicht/ er will der Sch\u00f6nheit darben/ ", "tokens": ["Ein", "an\u00b7der", "sagt", "viel\u00b7leicht", "/", "er", "will", "der", "Sch\u00f6n\u00b7heit", "dar\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "ART", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die/ was ihr \u00fcbel steht/ bald an der Seiten sp\u00fcrt/", "tokens": ["Die", "/", "was", "ihr", "\u00fc\u00b7bel", "steht", "/", "bald", "an", "der", "Sei\u00b7ten", "sp\u00fcrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "ADJD", "VVFIN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey der das edle Glied/ das zum Gesichte f\u00fchrt/", "tokens": ["Bey", "der", "das", "ed\u00b7le", "Glied", "/", "das", "zum", "Ge\u00b7sich\u00b7te", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "$(", "ART", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An glatter Wei\u00dfe statt hegt rauhe Purpur-Farben.", "tokens": ["An", "glat\u00b7ter", "Wei\u00b7\u00dfe", "statt", "hegt", "rau\u00b7he", "Pur\u00b7pur\u00b7Fa\u00b7rben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "D\u00e4mpfft braune R\u00f6the wohl den Prei\u00df der Weitzen-Garben?", "tokens": ["D\u00e4mpfft", "brau\u00b7ne", "R\u00f6\u00b7the", "wohl", "den", "Prei\u00df", "der", "Weit\u00b7zen\u00b7\u00b7Ga\u00b7rben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Der Liebe Pulver-Schlag hat/ Sch\u00f6ne/ dich ger\u00fchrt;", "tokens": ["Der", "Lie\u00b7be", "Pul\u00b7ver\u00b7Schlag", "hat", "/", "Sch\u00f6\u00b7ne", "/", "dich", "ge\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$(", "NN", "$(", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Damit des Bildes Haubt der K\u00fcnstler mehr beziert/", "tokens": ["Da\u00b7mit", "des", "Bil\u00b7des", "Haubt", "der", "K\u00fcnst\u00b7ler", "mehr", "be\u00b7ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gr\u00e4bt er in dessen Fu\u00df viel Wunden/ Stich und Narben.", "tokens": ["Gr\u00e4bt", "er", "in", "des\u00b7sen", "Fu\u00df", "viel", "Wun\u00b7den", "/", "Stich", "und", "Nar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRELAT", "NN", "PIAT", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Seh ich dich eigen an/ schau ich Trajanus Spitze/", "tokens": ["Seh", "ich", "dich", "ei\u00b7gen", "an", "/", "schau", "ich", "Tra\u00b7ja\u00b7nus", "Spit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$(", "VVFIN", "PPER", "NN", "NN", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Mausolens Grab erh\u00f6ht vertiefft durch so viel Ritze", "tokens": ["Mau\u00b7so\u00b7lens", "Grab", "er\u00b7h\u00f6ht", "ver\u00b7tiefft", "durch", "so", "viel", "Rit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVPP", "VVFIN", "APPR", "ADV", "PIAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Und Zeichen/ die allein die Liebe lesen kan.", "tokens": ["Und", "Zei\u00b7chen", "/", "die", "al\u00b7lein", "die", "Lie\u00b7be", "le\u00b7sen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "PRELS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O Sp\u00f6tter/ scheue dich die Fehler dieser Sch\u00f6nen/", "tokens": ["O", "Sp\u00f6t\u00b7ter", "/", "scheu\u00b7e", "dich", "die", "Feh\u00b7ler", "die\u00b7ser", "Sch\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "VVFIN", "PRF", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als Wercke der Natur/ muttwillig zu verh\u00f6nen:", "tokens": ["Als", "Wer\u00b7cke", "der", "Na\u00b7tur", "/", "mutt\u00b7wil\u00b7lig", "zu", "ver\u00b7h\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$(", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der K\u00f6nig r\u00fchret sie ohn Widerwillen an.", "tokens": ["Der", "K\u00f6\u00b7nig", "r\u00fch\u00b7ret", "sie", "ohn", "Wi\u00b7der\u00b7wil\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}