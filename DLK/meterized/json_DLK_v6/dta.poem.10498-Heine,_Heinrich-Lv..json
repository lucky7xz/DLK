{"dta.poem.10498": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte bei dir weilen,", "tokens": ["Ich", "woll\u00b7te", "bei", "dir", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und an deiner Seite ruhn;", "tokens": ["Und", "an", "dei\u00b7ner", "Sei\u00b7te", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du mu\u00dftest von mir eilen,", "tokens": ["Du", "mu\u00df\u00b7test", "von", "mir", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hattest viel zu thun.", "tokens": ["Du", "hat\u00b7test", "viel", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sagte, da\u00df meine Seele", "tokens": ["Ich", "sag\u00b7te", ",", "da\u00df", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dir g\u00e4nzlich ergeben sey;", "tokens": ["Dir", "g\u00e4nz\u00b7lich", "er\u00b7ge\u00b7ben", "sey", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du lachtest aus voller Kehle,", "tokens": ["Du", "lach\u00b7test", "aus", "vol\u00b7ler", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und machtest 'nen Knix dabei.", "tokens": ["Und", "mach\u00b7test", "'nen", "Knix", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du hast noch mehr gesteigert", "tokens": ["Du", "hast", "noch", "mehr", "ge\u00b7stei\u00b7gert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir meinen Liebesverdru\u00df,", "tokens": ["Mir", "mei\u00b7nen", "Lie\u00b7bes\u00b7ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und hast mir sogar verweigert", "tokens": ["Und", "hast", "mir", "so\u00b7gar", "ver\u00b7wei\u00b7gert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Am Ende den Abschiedsku\u00df.", "tokens": ["Am", "En\u00b7de", "den", "Ab\u00b7schieds\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Glaub' nicht, da\u00df ich mich erschie\u00dfe,", "tokens": ["Glaub'", "nicht", ",", "da\u00df", "ich", "mich", "er\u00b7schie\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie schlimm auch die Sachen stehn!", "tokens": ["Wie", "schlimm", "auch", "die", "Sa\u00b7chen", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Alles, meine S\u00fc\u00dfe,", "tokens": ["Das", "Al\u00b7les", ",", "mei\u00b7ne", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mir schon einmal geschehn.", "tokens": ["Ist", "mir", "schon", "ein\u00b7mal", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}