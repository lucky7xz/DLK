{"textgrid.poem.49435": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[ein stiller Ritter mit geschlossenem Visier]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein stiller Ritter mit geschlossenem Visier:", "tokens": ["Ein", "stil\u00b7ler", "Rit\u00b7ter", "mit", "ge\u00b7schlos\u00b7se\u00b7nem", "Vi\u00b7sier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ungl\u00fcck, stach ins Herz mit seiner Lanze mir.", "tokens": ["Das", "Un\u00b7gl\u00fcck", ",", "stach", "ins", "Herz", "mit", "sei\u00b7ner", "Lan\u00b7ze", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem alten Herz entsprang das Blut in tr\u00fcben Fluten,", "tokens": ["Dem", "al\u00b7ten", "Herz", "ent\u00b7sprang", "das", "Blut", "in", "tr\u00fc\u00b7ben", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versiegt auf Blum und Blatt in klaren Sonnengluten.", "tokens": ["Ver\u00b7siegt", "auf", "Blum", "und", "Blatt", "in", "kla\u00b7ren", "Son\u00b7nen\u00b7glu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Auge deckte Nacht, laut schrie ich auf vor Schmerz,", "tokens": ["Mein", "Au\u00b7ge", "deck\u00b7te", "Nacht", ",", "laut", "schrie", "ich", "auf", "vor", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,", "ADJD", "VVFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In wilden Schauern zuckend starb mein altes Herz.", "tokens": ["In", "wil\u00b7den", "Schau\u00b7ern", "zu\u00b7ckend", "starb", "mein", "al\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Ritter Ungl\u00fcck schwang hernieder sich vom Pferde,", "tokens": ["Der", "Rit\u00b7ter", "Un\u00b7gl\u00fcck", "schwang", "her\u00b7nie\u00b7der", "sich", "vom", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich fasste seine Hand mit finsterer Geb\u00e4rde.", "tokens": ["Mich", "fass\u00b7te", "sei\u00b7ne", "Hand", "mit", "fins\u00b7te\u00b7rer", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mit eh'rnem Handschuh griff in meine Wunde er,", "tokens": ["Mit", "eh'r\u00b7nem", "Hand\u00b7schuh", "griff", "in", "mei\u00b7ne", "Wun\u00b7de", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein mitleidlos Gebot ert\u00f6nte hart und schwer.", "tokens": ["Sein", "mit\u00b7leid\u00b7los", "Ge\u00b7bot", "er\u00b7t\u00f6n\u00b7te", "hart", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und es geschah, da rauh sein Finger mich ber\u00fchrte,", "tokens": ["Und", "es", "ge\u00b7schah", ",", "da", "rauh", "sein", "Fin\u00b7ger", "mich", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dass ein erneutes Herz ich stolz und rein versp\u00fcrte.", "tokens": ["Dass", "ein", "er\u00b7neu\u00b7tes", "Herz", "ich", "stolz", "und", "rein", "ver\u00b7sp\u00fcr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und dass von g\u00f6ttlicher Gnade heiss durchbebt", "tokens": ["Und", "dass", "von", "g\u00f6tt\u00b7li\u00b7cher", "Gna\u00b7de", "heiss", "durch\u00b7bebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein junges, tapfres Herz in tiefer Brust mir lebt.", "tokens": ["Ein", "jun\u00b7ges", ",", "tapf\u00b7res", "Herz", "in", "tie\u00b7fer", "Brust", "mir", "lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und voller Ehrfurcht blieb ich, zweifelnd und benommen,", "tokens": ["Und", "vol\u00b7ler", "Ehr\u00b7furcht", "blieb", "ich", ",", "zwei\u00b7felnd", "und", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich einem Menschen, dem Gott selbst im Traum gekommen.", "tokens": ["Gleich", "ei\u00b7nem", "Men\u00b7schen", ",", "dem", "Gott", "selbst", "im", "Traum", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.9": {"line.1": {"text": "Der gute Ritter stieg von neuem auf sein Pferd", "tokens": ["Der", "gu\u00b7te", "Rit\u00b7ter", "stieg", "von", "neu\u00b7em", "auf", "sein", "Pferd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nickte scheidend, wie er von mir sich gekehrt.", "tokens": ["Und", "nick\u00b7te", "schei\u00b7dend", ",", "wie", "er", "von", "mir", "sich", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und schrie, noch immer h\u00f6r die Stimme ich mit Beben:", "tokens": ["Und", "schrie", ",", "noch", "im\u00b7mer", "h\u00f6r", "die", "Stim\u00b7me", "ich", "mit", "Be\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00fct' dich, so milde komm ich einmal nur im Leben.", "tokens": ["H\u00fct'", "dich", ",", "so", "mil\u00b7de", "komm", "ich", "ein\u00b7mal", "nur", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADJD", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}