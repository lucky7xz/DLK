{"dta.poem.11947": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol lebt des Mannes Geist im gro\u00dfen Allgemeinen,", "tokens": ["Wol", "lebt", "des", "Man\u00b7nes", "Geist", "im", "gro\u00b7\u00dfen", "All\u00b7ge\u00b7mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPRART", "ADJA", "ADJA", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch leben will auch sein Gem\u00fct im eignen Kleinen.", "tokens": ["Doch", "le\u00b7ben", "will", "auch", "sein", "Ge\u00b7m\u00fct", "im", "eig\u00b7nen", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ADV", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol will er f\u00fcr die Welt des Sch\u00f6n' und Guten warten,", "tokens": ["Wol", "will", "er", "f\u00fcr", "die", "Welt", "des", "Sch\u00f6n'", "und", "Gu\u00b7ten", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch es auch bl\u00fchen sehn in seinem Haus und Garten.", "tokens": ["Doch", "es", "auch", "bl\u00fc\u00b7hen", "sehn", "in", "sei\u00b7nem", "Haus", "und", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV