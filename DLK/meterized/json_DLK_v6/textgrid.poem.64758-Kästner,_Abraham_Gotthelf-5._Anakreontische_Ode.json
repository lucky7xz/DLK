{"textgrid.poem.64758": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "5. Anakreontische Ode", "genre": "verse", "period": "N.A.", "pub_year": 1738, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcne B\u00fcsche, bunte G\u00e4nge,", "tokens": ["Gr\u00fc\u00b7ne", "B\u00fc\u00b7sche", ",", "bun\u00b7te", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer reizendes Gepr\u00e4nge", "tokens": ["Eu\u00b7er", "rei\u00b7zen\u00b7des", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht der angenehme May,", "tokens": ["Macht", "der", "an\u00b7ge\u00b7neh\u00b7me", "May", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach verjagtem Winter, neu.", "tokens": ["Nach", "ver\u00b7jag\u00b7tem", "Win\u00b7ter", ",", "neu", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doppelt seyd ihr sch\u00f6n zu sch\u00e4tzen,", "tokens": ["Dop\u00b7pelt", "seyd", "ihr", "sch\u00f6n", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doppelt k\u00f6nnt ihr uns erg\u00f6tzen", "tokens": ["Dop\u00b7pelt", "k\u00f6nnt", "ihr", "uns", "er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch der Blumen farbigt Heer,", "tokens": ["Durch", "der", "Blu\u00b7men", "far\u00b7bigt", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Sch\u00f6nen Zahl noch mehr.", "tokens": ["Durch", "der", "Sch\u00f6\u00b7nen", "Zahl", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manchen Schwarm von Schmetterlingen", "tokens": ["Man\u00b7chen", "Schwarm", "von", "Schmet\u00b7ter\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht man sich um jene schwingen;", "tokens": ["Sieht", "man", "sich", "um", "je\u00b7ne", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "PDAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die verfolgt oft zum Verdru\u00df", "tokens": ["Die", "ver\u00b7folgt", "oft", "zum", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Des geputzten Thoren Fu\u00df.", "tokens": ["Des", "ge\u00b7putz\u00b7ten", "Tho\u00b7ren", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stets verj\u00fcngt sich euer Prangen,", "tokens": ["Stets", "ver\u00b7j\u00fcngt", "sich", "eu\u00b7er", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihr das, was euch vergangen,", "tokens": ["Wenn", "ihr", "das", ",", "was", "euch", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was euch jeder Herbst entrei\u00dft,", "tokens": ["Was", "euch", "je\u00b7der", "Herbst", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeden Fr\u00fchling sch\u00f6ner weist.", "tokens": ["Je\u00b7den", "Fr\u00fch\u00b7ling", "sch\u00f6\u00b7ner", "weist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch auf ewig ist dein Prangen,", "tokens": ["Doch", "auf", "e\u00b7wig", "ist", "dein", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebster Ort, f\u00fcr mich vergangen,", "tokens": ["Liebs\u00b7ter", "Ort", ",", "f\u00fcr", "mich", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts erg\u00f6tzt hier meinen Geist,", "tokens": ["Nichts", "er\u00b7g\u00f6tzt", "hier", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sich ", "tokens": ["Wo", "sich"], "token_info": ["word", "word"], "pos": ["PWAV", "PRF"], "meter": "+-", "measure": "trochaic.single"}}}}}