{"textgrid.poem.46485": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Bl\u00e4ue und Wolkenschatten durchdunkelt,", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Bl\u00e4ue und Wolkenschatten durchdunkelt,", "tokens": ["Von", "Bl\u00e4u\u00b7e", "und", "Wol\u00b7ken\u00b7schat\u00b7ten", "durch\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von flirrenden Sternen durchfunkelt,", "tokens": ["Von", "flir\u00b7ren\u00b7den", "Ster\u00b7nen", "durch\u00b7fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schwelt sausend und sacht", "tokens": ["Schwelt", "sau\u00b7send", "und", "sacht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Weltraum-Nacht \u2013", "tokens": ["Die", "Welt\u00b7raum\u00b7Nacht", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da kommt \u00fcber Wolkenwogen", "tokens": ["Da", "kommt", "\u00fc\u00b7ber", "Wol\u00b7ken\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein flimmerndes Singen geflogen,", "tokens": ["Ein", "flim\u00b7mern\u00b7des", "Sin\u00b7gen", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Tropft und tropft Klang um Klang", "tokens": ["Tropft", "und", "tropft", "Klang", "um", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "NN", "APPR", "NN"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ein Sph\u00e4rengesang", "tokens": ["Ein", "Sph\u00e4\u00b7ren\u00b7ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "In die Nacht \u2013", "tokens": ["In", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "\u00bbo' du und wir \u2013 du und wir \u2013", "tokens": ["\u00bb", "o'", "du", "und", "wir", "\u2013", "du", "und", "wir", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "KON", "PPER", "$(", "PPER", "KON", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist in der Nacht und bist doch nicht hier \u2013", "tokens": ["Du", "bist", "in", "der", "Nacht", "und", "bist", "doch", "nicht", "hier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "KON", "VAFIN", "ADV", "PTKNEG", "ADV", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O' Wirbal, du Gottheld der Liebe,", "tokens": ["O'", "Wir\u00b7bal", ",", "du", "Got\u00b7theld", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du All\u00fcberrager,", "tokens": ["Du", "Al\u00b7l\u00fc\u00b7berr\u00b7a\u00b7ger", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wo gl\u00e4nzt dein Auge, wo ist dein Lager?", "tokens": ["Wo", "gl\u00e4nzt", "dein", "Au\u00b7ge", ",", "wo", "ist", "dein", "La\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wir d\u00fcrsten nach dir,", "tokens": ["Wir", "d\u00fcrs\u00b7ten", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "K\u00e4mpfer und Wager,", "tokens": ["K\u00e4mp\u00b7fer", "und", "Wa\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und nach Liebe \u2013 \u00ab", "tokens": ["Und", "nach", "Lie\u00b7be", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "$(", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Da schweben wie mit n\u00e4chtlichen Schwingen,", "tokens": ["Da", "schwe\u00b7ben", "wie", "mit", "n\u00e4cht\u00b7li\u00b7chen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die suchenden Frauen aus d\u00fcsterer Ferne", "tokens": ["Die", "su\u00b7chen\u00b7den", "Frau\u00b7en", "aus", "d\u00fcs\u00b7te\u00b7rer", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "In den Spr\u00fchlichtregen der Sterne \u2013", "tokens": ["In", "den", "Spr\u00fch\u00b7licht\u00b7re\u00b7gen", "der", "Ster\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Da gleitet im Leuchten der leuchtende Chor \u2013 \u2013", "tokens": ["Da", "glei\u00b7tet", "im", "Leuch\u00b7ten", "der", "leuch\u00b7ten\u00b7de", "Chor", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nun wieder ein Singen,", "tokens": ["Nun", "wie\u00b7der", "ein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Einer Stimme Singen:", "tokens": ["Ei\u00b7ner", "Stim\u00b7me", "Sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbo' Wirbal, du, den ich erkor,", "tokens": ["\u00bb", "o'", "Wir\u00b7bal", ",", "du", ",", "den", "ich", "er\u00b7kor", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$,", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Den ich am Anfang besa\u00df \u2013", "tokens": ["Den", "ich", "am", "An\u00b7fang", "be\u00b7sa\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wieder verlor, wie m\u00fcd ist mein Tanz,", "tokens": ["Und", "wie\u00b7der", "ver\u00b7lor", ",", "wie", "m\u00fcd", "ist", "mein", "Tanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie leer meine Nacht,", "tokens": ["Wie", "leer", "mei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Kein Glutblick, der mir lacht,", "tokens": ["Kein", "Glut\u00b7blick", ",", "der", "mir", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kein Arm, der mein Bl\u00fchen entfacht,", "tokens": ["Kein", "Arm", ",", "der", "mein", "Bl\u00fc\u00b7hen", "ent\u00b7facht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Nicht du, allgro\u00dfer Glanz \u2013", "tokens": ["Nicht", "du", ",", "all\u00b7gro\u00b7\u00dfer", "Glanz", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "O' sternlichtbetauter,", "tokens": ["O'", "stern\u00b7licht\u00b7be\u00b7tau\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Nachtlockenumblauter", "tokens": ["Nacht\u00b7lo\u00b7cke\u00b7num\u00b7blau\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.10": {"text": "Glanzgott", "tokens": ["Glanz\u00b7gott"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Komm!\u00ab", "tokens": ["Komm", "!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["VVFIN", "$.", "$("], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Die Klage verirrt \u2013 \u2013 \u2013", "tokens": ["Die", "Kla\u00b7ge", "ver\u00b7irrt", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ein Spr\u00fchkomet schwirrt", "tokens": ["Ein", "Spr\u00fch\u00b7ko\u00b7met", "schwirrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hochoben \u2013 \u2013 \u2013", "tokens": ["Hoch\u00b7o\u00b7ben", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NN", "$(", "$(", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Sieh! da kommt ein Feuer!", "tokens": ["Sieh", "!", "da", "kommt", "ein", "Feu\u00b7er", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "H\u00f6r, da kommt ein Fauchen!", "tokens": ["H\u00f6r", ",", "da", "kommt", "ein", "Fau\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da kommt ein Neuer!", "tokens": ["Da", "kommt", "ein", "Neu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Seinem Reitro\u00df rauchen", "tokens": ["Sei\u00b7nem", "Rei\u00b7tro\u00df", "rau\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die N\u00fcstern \u2013", "tokens": ["Die", "N\u00fcs\u00b7tern", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wirbal!", "tokens": ["Wir\u00b7bal", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Wirbal!", "tokens": ["Wir\u00b7bal", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "Die Jungfrauen fl\u00fcstern \u2013", "tokens": ["Die", "Jung\u00b7frau\u00b7en", "fl\u00fcs\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Da beginnt in glutschwerem Ba\u00df", "tokens": ["Da", "be\u00b7ginnt", "in", "glutsc\u00b7hwe\u00b7rem", "Ba\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der riesige Reiter zu singen:", "tokens": ["Der", "rie\u00b7si\u00b7ge", "Rei\u00b7ter", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbdu Eine, die du riefst,", "tokens": ["\u00bb", "du", "Ei\u00b7ne", ",", "die", "du", "riefst", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die du sangst und nicht schliefst,", "tokens": ["Die", "du", "sangst", "und", "nicht", "schliefst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich bin der \u00fcbern\u00e4chtige", "tokens": ["Ich", "bin", "der", "\u00fc\u00b7bern\u00b7\u00e4ch\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liebesm\u00e4chtige,", "tokens": ["Lie\u00b7bes\u00b7m\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den du suchst.", "tokens": ["Den", "du", "suchst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Komm du Tanzschm\u00e4chtige,", "tokens": ["Komm", "du", "Tanz\u00b7schm\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von Sehnsucht verst\u00f6rt,", "tokens": ["Von", "Sehn\u00b7sucht", "ver\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Dein Lied ist erh\u00f6rt,", "tokens": ["Dein", "Lied", "ist", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Erl\u00f6sung wird dein.\u00ab", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "wird", "dein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Singt wieder die Eine allein:", "tokens": ["Singt", "wie\u00b7der", "die", "Ei\u00b7ne", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ART", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "\u00bbo' Wirbal, Wirbal ich kann nicht mehr singen,", "tokens": ["\u00bb", "o'", "Wir\u00b7bal", ",", "Wir\u00b7bal", "ich", "kann", "nicht", "mehr", "sin\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$,", "KOUS", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mein Herz will springen, \u2013", "tokens": ["Mein", "Herz", "will", "sprin\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mein Herz will ich dir bringen \u2013 \u2013", "tokens": ["Mein", "Herz", "will", "ich", "dir", "brin\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Speer gl\u00fcht so rot \u2013 \u00ab", "tokens": ["Dein", "Speer", "gl\u00fcht", "so", "rot", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "\u00bbdu bist liebesstark, sei bereit,", "tokens": ["\u00bb", "du", "bist", "lie\u00b7bes\u00b7stark", ",", "s