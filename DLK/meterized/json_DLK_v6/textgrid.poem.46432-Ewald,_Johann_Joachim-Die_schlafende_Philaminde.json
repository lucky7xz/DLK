{"textgrid.poem.46432": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Die schlafende Philaminde", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sanfte schl\u00e4ft in Blumen hier,", "tokens": ["Wie", "sanf\u00b7te", "schl\u00e4ft", "in", "Blu\u00b7men", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VVFIN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die unschuldsvolle Philaminde!", "tokens": ["Die", "un\u00b7schulds\u00b7vol\u00b7le", "Phi\u00b7la\u00b7min\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie l\u00e4chelt selbst im Schlaf, o weckt sie nicht ihr Winde!", "tokens": ["Sie", "l\u00e4\u00b7chelt", "selbst", "im", "Schlaf", ",", "o", "weckt", "sie", "nicht", "ihr", "Win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "FM", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie tr\u00e4umt vielleicht von mir!", "tokens": ["Sie", "tr\u00e4umt", "viel\u00b7leicht", "von", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}