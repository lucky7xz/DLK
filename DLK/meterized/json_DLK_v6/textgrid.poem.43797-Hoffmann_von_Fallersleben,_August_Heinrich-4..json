{"textgrid.poem.43797": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man braucht mich aller Orten,", "tokens": ["Man", "braucht", "mich", "al\u00b7ler", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil man mich n\u00f6thig hat;", "tokens": ["Weil", "man", "mich", "n\u00f6\u00b7thig", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch folget meinen Worten", "tokens": ["Doch", "fol\u00b7get", "mei\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht immer nach die That.", "tokens": ["Nicht", "im\u00b7mer", "nach", "die", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei manchem Titel trab' ich", "tokens": ["Bei", "man\u00b7chem", "Ti\u00b7tel", "trab'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bescheiden hinten an;", "tokens": ["Be\u00b7schei\u00b7den", "hin\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oft zum Genossen hab' ich,", "tokens": ["Oft", "zum", "Ge\u00b7nos\u00b7sen", "hab'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der mich nie brauchen kann", "tokens": ["Der", "mich", "nie", "brau\u00b7chen", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Will ich mich selber pflegen,", "tokens": ["Will", "ich", "mich", "sel\u00b7ber", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So setz' ich mich in mich;", "tokens": ["So", "setz'", "ich", "mich", "in", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann werd' ich oft verlegen,", "tokens": ["Dann", "werd'", "ich", "oft", "ver\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fehlt mir mein eigen Ich.", "tokens": ["Fehlt", "mir", "mein", "ei\u00b7gen", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "PPER", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Hast du mich nun errathen,", "tokens": ["Hast", "du", "mich", "nun", "er\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So geb' ich dir den Rath:", "tokens": ["So", "geb'", "ich", "dir", "den", "Rath", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Brauch' mich bei allen Thaten", "tokens": ["Brauch'", "mich", "bei", "al\u00b7len", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Guten fr\u00fch und spat!", "tokens": ["Zum", "Gu\u00b7ten", "fr\u00fch", "und", "spat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}