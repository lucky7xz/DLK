{"textgrid.poem.55278": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wasser holen geht die reine,", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wasser holen geht die reine,", "tokens": ["Was\u00b7ser", "ho\u00b7len", "geht", "die", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Frau des hohen Brahmen,", "tokens": ["Sch\u00f6\u00b7ne", "Frau", "des", "ho\u00b7hen", "Brah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Des verehrten, fehlerlosen,", "tokens": ["Des", "ver\u00b7ehr\u00b7ten", ",", "feh\u00b7ler\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ernstester Gerechtigkeit.", "tokens": ["Erns\u00b7tes\u00b7ter", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00e4glich von dem heiligen Flusse", "tokens": ["T\u00e4g\u00b7lich", "von", "dem", "hei\u00b7li\u00b7gen", "Flus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Holt sie k\u00f6stlichstes Erquicken; \u2013", "tokens": ["Holt", "sie", "k\u00f6st\u00b7lichs\u00b7tes", "Er\u00b7qui\u00b7cken", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber wo ist Krug und Eimer?", "tokens": ["A\u00b7ber", "wo", "ist", "Krug", "und", "Ei\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie bedarf derselben nicht.", "tokens": ["Sie", "be\u00b7darf", "der\u00b7sel\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Seligem Herzen, frommen H\u00e4nden", "tokens": ["Se\u00b7li\u00b7gem", "Her\u00b7zen", ",", "from\u00b7men", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Ballt sich die bewegte Welle", "tokens": ["Ballt", "sich", "die", "be\u00b7weg\u00b7te", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Herrlich zu kristallner Kugel;", "tokens": ["Herr\u00b7lich", "zu", "kris\u00b7tall\u00b7ner", "Ku\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.12": {"text": "Diese tr\u00e4gt sie frohen Busens,", "tokens": ["Die\u00b7se", "tr\u00e4gt", "sie", "fro\u00b7hen", "Bu\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Reiner Sitte, holden Wandelns", "tokens": ["Rei\u00b7ner", "Sit\u00b7te", ",", "hol\u00b7den", "Wan\u00b7delns"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Vor den Gatten in das Haus.", "tokens": ["Vor", "den", "Gat\u00b7ten", "in", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute kommt die morgendliche", "tokens": ["Heu\u00b7te", "kommt", "die", "mor\u00b7gend\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Gebet zu Ganges' Fluten,", "tokens": ["Im", "Ge\u00b7bet", "zu", "Gan\u00b7ge\u00b7s'", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Beugt sich zu der klaren Fl\u00e4che \u2013", "tokens": ["Beugt", "sich", "zu", "der", "kla\u00b7ren", "Fl\u00e4\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich \u00fcberraschend spiegelt", "tokens": ["Pl\u00f6tz\u00b7lich", "\u00fc\u00b7berr\u00b7a\u00b7schend", "spie\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus des h\u00f6chsten Himmels Breiten", "tokens": ["Aus", "des", "h\u00f6chs\u00b7ten", "Him\u00b7mels", "Brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber ihr vor\u00fcbereilend", "tokens": ["\u00dc\u00b7ber", "ihr", "vor\u00b7\u00fc\u00b7be\u00b7rei\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Allerlieblichste Gestalt", "tokens": ["Al\u00b7ler\u00b7lieb\u00b7lichs\u00b7te", "Ge\u00b7stalt"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hehren J\u00fcnglings, den des Gottes", "tokens": ["Heh\u00b7ren", "J\u00fcng\u00b7lings", ",", "den", "des", "Got\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Uranf\u00e4nglich sch\u00f6nes Denken", "tokens": ["U\u00b7ran\u00b7f\u00e4ng\u00b7lich", "sch\u00f6\u00b7nes", "Den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aus dem ew'gen Busen schuf;", "tokens": ["Aus", "dem", "ew'\u00b7gen", "Bu\u00b7sen", "schuf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Solchen schauend, f\u00fchlt ergriffen", "tokens": ["Sol\u00b7chen", "schau\u00b7end", ",", "f\u00fchlt", "er\u00b7grif\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJD", "$,", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Von verwirrenden Gef\u00fchlen", "tokens": ["Von", "ver\u00b7wir\u00b7ren\u00b7den", "Ge\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sie das innere, tiefste Leben,", "tokens": ["Sie", "das", "in\u00b7ne\u00b7re", ",", "tiefs\u00b7te", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-----+-+-", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Will verharren in dem Anschaun,", "tokens": ["Will", "ver\u00b7har\u00b7ren", "in", "dem", "An\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weist es weg, da kehrt es wieder,", "tokens": ["Weist", "es", "weg", ",", "da", "kehrt", "es", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verworren strebt sie flutw\u00e4rts,", "tokens": ["Und", "ver\u00b7wor\u00b7ren", "strebt", "sie", "flut\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit unsichrer Hand zu sch\u00f6pfen;", "tokens": ["Mit", "un\u00b7sich\u00b7rer", "Hand", "zu", "sch\u00f6p\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber ach! sie sch\u00f6pft nicht mehr!", "tokens": ["A\u00b7ber", "ach", "!", "sie", "sch\u00f6pft", "nicht", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn des Wassers heilige Welle", "tokens": ["Denn", "des", "Was\u00b7sers", "hei\u00b7li\u00b7ge", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Scheint zu fliehn, sich zu entfernen,", "tokens": ["Scheint", "zu", "fliehn", ",", "sich", "zu", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie erblickt nur hohler Wirbel", "tokens": ["Sie", "er\u00b7blickt", "nur", "hoh\u00b7ler", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Grause Tiefen unter sich.", "tokens": ["Grau\u00b7se", "Tie\u00b7fen", "un\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Arme sinken, Tritte straucheln,", "tokens": ["Ar\u00b7me", "sin\u00b7ken", ",", "Trit\u00b7te", "strau\u00b7cheln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's denn auch der Pfad nach Hause?", "tokens": ["Ist's", "denn", "auch", "der", "Pfad", "nach", "Hau\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll sie zaudern? soll sie fliehen?", "tokens": ["Soll", "sie", "zau\u00b7dern", "?", "soll", "sie", "flie\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will sie denken, wo Gedanke,", "tokens": ["Will", "sie", "den\u00b7ken", ",", "wo", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rat und H\u00fclfe gleich versagt? \u2013", "tokens": ["Rat", "und", "H\u00fcl\u00b7fe", "gleich", "ver\u00b7sagt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und so tritt sie vor den Gatten;", "tokens": ["Und", "so", "tritt", "sie", "vor", "den", "Gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Er erblickt sie, Blick ist Urteil,", "tokens": ["Er", "er\u00b7blickt", "sie", ",", "Blick", "ist", "Ur\u00b7teil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hohen Sinns ergreift das Schwert er,", "tokens": ["Ho\u00b7hen", "Sinns", "er\u00b7greift", "das", "Schwert", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Schleppt sie zu dem Totenh\u00fcgel,", "tokens": ["Schleppt", "sie", "zu", "dem", "To\u00b7ten\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo Verbrecher b\u00fc\u00dfend bluten.", "tokens": ["Wo", "Ver\u00b7bre\u00b7cher", "b\u00fc\u00b7\u00dfend", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "W\u00fc\u00dfte sie zu widerstreben?", "tokens": ["W\u00fc\u00df\u00b7te", "sie", "zu", "wi\u00b7der\u00b7stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "W\u00fc\u00dfte sie sich zu entschuld'gen,", "tokens": ["W\u00fc\u00df\u00b7te", "sie", "sich", "zu", "ent\u00b7schuld'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Schuldig, keiner Schuld bewu\u00dft?", "tokens": ["Schul\u00b7dig", ",", "kei\u00b7ner", "Schuld", "be\u00b7wu\u00dft", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und er kehrt mit blutigem Schwerte", "tokens": ["Und", "er", "kehrt", "mit", "blu\u00b7ti\u00b7gem", "Schwer\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sinnend zu der stillen Wohnung;", "tokens": ["Sin\u00b7nend", "zu", "der", "stil\u00b7len", "Woh\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da entgegnet ihm der Sohn:", "tokens": ["Da", "ent\u00b7geg\u00b7net", "ihm", "der", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "\u00bbwessen Blut ist's? Vater! Vater!\u00ab", "tokens": ["\u00bb", "wes\u00b7sen", "Blut", "ist's", "?", "Va\u00b7ter", "!", "Va\u00b7ter", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "VAFIN", "$.", "NN", "$.", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbder Verbrecherin!\u00ab \u2013 \u00bbMitnichten!", "tokens": ["\u00bb", "der", "Ver\u00b7bre\u00b7che\u00b7rin", "!", "\u00ab", "\u2013", "\u00bb", "Mit\u00b7nich\u00b7ten", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "$(", "$(", "$(", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn es starret nicht am Schwerte", "tokens": ["Denn", "es", "star\u00b7ret", "nicht", "am", "Schwer\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie verbrecherische Tropfen,", "tokens": ["Wie", "ver\u00b7bre\u00b7che\u00b7ri\u00b7sche", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Flie\u00dft wie aus der Wunde frisch.", "tokens": ["Flie\u00dft", "wie", "aus", "der", "Wun\u00b7de", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mutter, Mutter! tri