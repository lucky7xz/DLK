"token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nt das grausenvolle Wort:", "tokens": ["T\u00f6nt", "das", "grau\u00b7sen\u00b7vol\u00b7le", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbsohn, o Sohn! welch \u00dcbereilen!", "tokens": ["\u00bb", "sohn", ",", "o", "Sohn", "!", "welch", "\u00dc\u00b7be\u00b7rei\u00b7len", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "FM", "NN", "$.", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner Mutter Leichnam dorten,", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", "Leich\u00b7nam", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Neben ihm das freche Haupt", "tokens": ["Ne\u00b7ben", "ihm", "das", "fre\u00b7che", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Verbrecherin, des Opfers", "tokens": ["Der", "Ver\u00b7bre\u00b7che\u00b7rin", ",", "des", "Op\u00b7fers"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Waltender Gerechtigkeit!", "tokens": ["Wal\u00b7ten\u00b7der", "Ge\u00b7rech\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mich nun hast du ihrem K\u00f6rper", "tokens": ["Mich", "nun", "hast", "du", "ih\u00b7rem", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eingeimpft auf ewige Tage;", "tokens": ["Ein\u00b7ge\u00b7impft", "auf", "e\u00b7wi\u00b7ge", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Weisen Wollens, wilden Handelns", "tokens": ["Wei\u00b7sen", "Wol\u00b7lens", ",", "wil\u00b7den", "Han\u00b7delns"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd ich unter G\u00f6ttern sein.", "tokens": ["Werd", "ich", "un\u00b7ter", "G\u00f6t\u00b7tern", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, des Himmelsknaben Bildnis", "tokens": ["Ja", ",", "des", "Him\u00b7mels\u00b7kna\u00b7ben", "Bild\u00b7nis"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Webt so sch\u00f6n vor Stirn und Auge;", "tokens": ["Webt", "so", "sch\u00f6n", "vor", "Stirn", "und", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Senkt sich's in das Herz herunter,", "tokens": ["Senkt", "sich's", "in", "das", "Herz", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Regt es tolle Wutbegier.", "tokens": ["Regt", "es", "tol\u00b7le", "Wut\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Immer wird es wiederkehren,", "tokens": ["Im\u00b7mer", "wird", "es", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer steigen, immer sinken,", "tokens": ["Im\u00b7mer", "stei\u00b7gen", ",", "im\u00b7mer", "sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich verd\u00fcstern, sich verkl\u00e4ren,", "tokens": ["Sich", "ver\u00b7d\u00fcs\u00b7tern", ",", "sich", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So hat Brahma dies gewollt.", "tokens": ["So", "hat", "Brah\u00b7ma", "dies", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PDS", "VMPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er gebot ja buntem Fittich,", "tokens": ["Er", "ge\u00b7bot", "ja", "bun\u00b7tem", "Fit\u00b7tich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klarem Antlitz, schlanken Gliedern,", "tokens": ["Kla\u00b7rem", "Ant\u00b7litz", ",", "schlan\u00b7ken", "Glie\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "G\u00f6ttlich-einzigem Erscheinen,", "tokens": ["G\u00f6tt\u00b7lich\u00b7ein\u00b7zi\u00b7gem", "Er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Mich zu pr\u00fcfen, zu verf\u00fchren;", "tokens": ["Mich", "zu", "pr\u00fc\u00b7fen", ",", "zu", "ver\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn von oben kommt Verf\u00fchrung,", "tokens": ["Denn", "von", "o\u00b7ben", "kommt", "Ver\u00b7f\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn's den G\u00f6ttern so beliebt.", "tokens": ["Wenn's", "den", "G\u00f6t\u00b7tern", "so", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und so soll ich, die Brahmane,", "tokens": ["Und", "so", "soll", "ich", ",", "die", "Brah\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mit dem Haupt im Himmel weilend,", "tokens": ["Mit", "dem", "Haupt", "im", "Him\u00b7mel", "wei\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "F\u00fchlen, Paria, dieser Erde", "tokens": ["F\u00fch\u00b7len", ",", "Pa\u00b7ria", ",", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Niederziehende Gewalt.", "tokens": ["Nie\u00b7der\u00b7zie\u00b7hen\u00b7de", "Ge\u00b7walt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.12": {"line.1": {"text": "Sohn, ich sende dich dem Vater!", "tokens": ["Sohn", ",", "ich", "sen\u00b7de", "dich", "dem", "Va\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00f6ste! \u2013 Nicht ein traurig B\u00fc\u00dfen,", "tokens": ["Tr\u00f6s\u00b7te", "!", "\u2013", "Nicht", "ein", "trau\u00b7rig", "B\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PTKNEG", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stumpfes Harren, stolz Verdienen", "tokens": ["Stump\u00b7fes", "Har\u00b7ren", ",", "stolz", "Ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halt euch in der Wildnis fest;", "tokens": ["Halt", "euch", "in", "der", "Wild\u00b7nis", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wandert aus durch alle Welten,", "tokens": ["Wan\u00b7dert", "aus", "durch", "al\u00b7le", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandelt hin durch alle Zeiten,", "tokens": ["Wan\u00b7delt", "hin", "durch", "al\u00b7le", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verk\u00fcndet auch Geringstem:", "tokens": ["Und", "ver\u00b7k\u00fcn\u00b7det", "auch", "Ge\u00b7rings\u00b7tem", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ihn Brahma droben h\u00f6rt!", "tokens": ["Da\u00df", "ihn", "Brah\u00b7ma", "dro\u00b7ben", "h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ihm ist keiner der Geringste \u2013", "tokens": ["Ihm", "ist", "kei\u00b7ner", "der", "Ge\u00b7rings\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer sich mit gel\u00e4hmten Gliedern,", "tokens": ["Wer", "sich", "mit", "ge\u00b7l\u00e4hm\u00b7ten", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich mit wild zerst\u00f6rtem Geiste,", "tokens": ["Sich", "mit", "wild", "zer\u00b7st\u00f6r\u00b7tem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcster, ohne H\u00fclf und Rettung,", "tokens": ["D\u00fcs\u00b7ter", ",", "oh\u00b7ne", "H\u00fclf", "und", "Ret\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei er Brahma, sei er Paria,", "tokens": ["Sei", "er", "Brah\u00b7ma", ",", "sei", "er", "Pa\u00b7ria", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$,", "VAFIN", "PPER", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mit dem Blick nach oben kehrt,", "tokens": ["Mit", "dem", "Blick", "nach", "o\u00b7ben", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird's empfinden, wird's erfahren:", "tokens": ["Wird's", "emp\u00b7fin\u00b7den", ",", "wird's", "er\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dort ergl\u00fchen tausend Augen,", "tokens": ["Dort", "er\u00b7gl\u00fc\u00b7hen", "tau\u00b7send", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ruhend lauschen tausend Ohren,", "tokens": ["Ru\u00b7hend", "lau\u00b7schen", "tau\u00b7send", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denen nichts verborgen bleibt.", "tokens": ["De\u00b7nen", "nichts", "ver\u00b7bor\u00b7gen", "bleibt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Heb ich mich zu seinem Throne,", "tokens": ["Heb", "ich", "mich", "zu", "sei\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut er mich, die Grausenhafte,", "tokens": ["Schaut", "er", "mich", ",", "die", "Grau\u00b7sen\u00b7haf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die er gr\u00e4\u00dflich umgeschaffen,", "tokens": ["Die", "er", "gr\u00e4\u00df\u00b7lich", "um\u00b7ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df er ewig mich bejammern,", "tokens": ["Mu\u00df", "er", "e\u00b7wig", "mich", "be\u00b7jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Euch zugute komme das.", "tokens": ["Euch", "zu\u00b7gu\u00b7te", "kom\u00b7me", "das", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich werd ihn freundlich mahnen,", "tokens": ["Und", "ich", "werd", "ihn", "freund\u00b7lich", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich werd ihm w\u00fctend sagen,", "tokens": ["Und", "ich", "werd", "ihm", "w\u00fc\u00b7tend", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie es mir der Sinn gebietet,", "tokens": ["Wie", "es", "mir", "der", "Sinn", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie es mir im Busen schwellet.", "tokens": ["Wie", "es", "mir", "im", "Bu\u00b7sen", "schwel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Was ich denke, was ich f\u00fchle \u2013", "tokens": ["Was", "ich", "den\u00b7ke", ",", "was", "ich", "f\u00fch\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ein Geheimnis bleibe das.\u00ab", "tokens": ["Ein", "Ge\u00b7heim\u00b7nis", "blei\u00b7be", "das", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}