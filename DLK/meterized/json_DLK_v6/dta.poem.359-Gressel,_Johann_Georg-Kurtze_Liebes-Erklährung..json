{"dta.poem.359": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Kurtze Liebes-Erkl\u00e4hrung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Rubell' ich liebe dich/ ich ehre deine Pracht", "tokens": ["Ru\u00b7bell'", "ich", "lie\u00b7be", "dich", "/", "ich", "eh\u00b7re", "dei\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gef\u00e4llt dir dieser Schlu\u00df/ behaget dir mein Lieben?", "tokens": ["Ge\u00b7f\u00e4llt", "dir", "die\u00b7ser", "Schlu\u00df", "/", "be\u00b7ha\u00b7get", "dir", "mein", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$(", "VVFIN", "PPER", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So werde mir dein Will\u2019 hinwieder zugeschrieben", "tokens": ["So", "wer\u00b7de", "mir", "dein", "Will'", "hin\u00b7wie\u00b7der", "zu\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit Vereinigung ein vestes B\u00fcndni\u00df macht.", "tokens": ["Da\u00b7mit", "Ver\u00b7ei\u00b7ni\u00b7gung", "ein", "ves\u00b7tes", "B\u00fcn\u00b7dni\u00df", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rubelle traue mir/ mein Hertze ist dein eigen/", "tokens": ["Ru\u00b7bel\u00b7le", "trau\u00b7e", "mir", "/", "mein", "Hert\u00b7ze", "ist", "dein", "ei\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$(", "PPOSAT", "VVFIN", "VAFIN", "PPOSAT", "ADJD", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Drum scheue dich nur nicht auch deines mir zu zeigen.", "tokens": ["Drum", "scheu\u00b7e", "dich", "nur", "nicht", "auch", "dei\u00b7nes", "mir", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "PPOSAT", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}