{"textgrid.poem.45598": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Eckensteher", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnante, was ist in dich gefahren?", "tokens": ["\u00bb", "nan\u00b7te", ",", "was", "ist", "in", "dich", "ge\u00b7fah\u00b7ren", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du g\u00f6nnst einem Landsmann ja kein Wort!\u00ab", "tokens": ["Du", "g\u00f6nnst", "ei\u00b7nem", "Lands\u00b7mann", "ja", "kein", "Wort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sonst dient ich einem russischen Bojaren,", "tokens": ["Sonst", "dient", "ich", "ei\u00b7nem", "rus\u00b7si\u00b7schen", "Bo\u00b7ja\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch jetzt einem englischen Lord.", "tokens": ["Doch", "jetzt", "ei\u00b7nem", "eng\u00b7li\u00b7schen", "Lord", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}