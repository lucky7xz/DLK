{"dta.poem.23634": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Philister.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Philistervolk auf allen Wegen,", "tokens": ["Phi\u00b7lis\u00b7ter\u00b7volk", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Philister vor und hinter mir,", "tokens": ["Phi\u00b7lis\u00b7ter", "vor", "und", "hin\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Sonnenschein, im Schnee und Regen,", "tokens": ["Im", "Son\u00b7nen\u00b7schein", ",", "im", "Schnee", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Philister dort, Philister hier!", "tokens": ["Phi\u00b7lis\u00b7ter", "dort", ",", "Phi\u00b7lis\u00b7ter", "hier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du noch Beine, so enteile!", "tokens": ["Hast", "du", "noch", "Bei\u00b7ne", ",", "so", "ent\u00b7ei\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar ist gewi\u00df du stirbst einmal \u2014", "tokens": ["Zwar", "ist", "ge\u00b7wi\u00df", "du", "stirbst", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist ein Tod vor Langerweile", "tokens": ["Doch", "ist", "ein", "Tod", "vor", "Lan\u00b7ger\u00b7wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon hier auf Erden H\u00f6llenqual.", "tokens": ["Schon", "hier", "auf", "Er\u00b7den", "H\u00f6l\u00b7len\u00b7qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So dacht' ich, und es klopft so eben,", "tokens": ["So", "dacht'", "ich", ",", "und", "es", "klopft", "so", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ein Philister stellt sich ein,", "tokens": ["Und", "ein", "Phi\u00b7lis\u00b7ter", "stellt", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umarmt mich, k\u00fcsst mich, \u2014 gottergeben", "tokens": ["Um\u00b7armt", "mich", ",", "k\u00fcsst", "mich", ",", "got\u00b7ter\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "$(", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Geh' ich in meinen Tod hinein.", "tokens": ["Geh'", "ich", "in", "mei\u00b7nen", "Tod", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}