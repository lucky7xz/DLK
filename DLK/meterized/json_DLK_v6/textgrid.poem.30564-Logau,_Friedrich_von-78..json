{"textgrid.poem.30564": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer zu loben von viel Sachen,", "tokens": ["Wer", "zu", "lo\u00b7ben", "von", "viel", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da wil Lob sich schwerer machen", "tokens": ["Da", "wil", "Lob", "sich", "schwe\u00b7rer", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "PRF", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als bey dem, wo nichts sich weiset,", "tokens": ["Als", "bey", "dem", ",", "wo", "nichts", "sich", "wei\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das man f\u00fcglich r\u00fchmt und preiset;", "tokens": ["Das", "man", "f\u00fcg\u00b7lich", "r\u00fchmt", "und", "prei\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn dort mangelts an den Worten,", "tokens": ["Denn", "dort", "man\u00b7gelts", "an", "den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die man darff zu so viel Orten;", "tokens": ["Die", "man", "darff", "zu", "so", "viel", "Or\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hier ermangelts an den Dingen,", "tokens": ["Hier", "er\u00b7man\u00b7gelts", "an", "den", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Da\u00df man blo\u00df mu\u00df Worte bringen.", "tokens": ["Da\u00df", "man", "blo\u00df", "mu\u00df", "Wor\u00b7te", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}