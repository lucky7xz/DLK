{"textgrid.poem.28938": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du brauchest deine Zung als wie der Fuchs den Schwantz;", "tokens": ["Du", "brau\u00b7chest", "dei\u00b7ne", "Zung", "als", "wie", "der", "Fuchs", "den", "Schwantz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOUS", "KOKOM", "ART", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach, da\u00df du, Palpo, so sie m\u00fcssest brauchen gantz!", "tokens": ["Ach", ",", "da\u00df", "du", ",", "Pal\u00b7po", ",", "so", "sie", "m\u00fcs\u00b7sest", "brau\u00b7chen", "gantz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "$,", "NE", "$,", "ADV", "PPER", "VMFIN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil seinen Schwantz der Fuchs mit Wasser offt befeucht,", "tokens": ["Weil", "sei\u00b7nen", "Schwantz", "der", "Fuchs", "mit", "Was\u00b7ser", "offt", "be\u00b7feucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NE", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ihn zwar viel nicht kost, zum besten doch nicht reucht.", "tokens": ["Da\u00df", "ihn", "zwar", "viel", "nicht", "kost", ",", "zum", "bes\u00b7ten", "doch", "nicht", "reucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "$,", "APPRART", "VVFIN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}