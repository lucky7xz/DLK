{"textgrid.poem.58115": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "153. Verleugnung aus reiner Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur dem Herren zu behagen,", "tokens": ["Nur", "dem", "Her\u00b7ren", "zu", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich gern mir selbst entsagen", "tokens": ["Will", "ich", "gern", "mir", "selbst", "ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch aller Kreatur;", "tokens": ["Und", "auch", "al\u00b7ler", "Kre\u00b7a\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollt' auch Leib und Seel' verschmachten,", "tokens": ["Sollt'", "auch", "Leib", "und", "Seel'", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will ich solches doch nicht achten,", "tokens": ["Will", "ich", "sol\u00b7ches", "doch", "nicht", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich ihm gefalle nur.", "tokens": ["Wenn", "ich", "ihm", "ge\u00b7fal\u00b7le", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}