{"textgrid.poem.38276": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Druck und Gegendruck", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n klar einstmal die Sonne", "tokens": ["Sch\u00f6n", "klar", "einst\u00b7mal", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leuchtet mit ihrem Schein,", "tokens": ["Leuch\u00b7tet", "mit", "ih\u00b7rem", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Als ich nach Herzens Wonne", "tokens": ["Als", "ich", "nach", "Her\u00b7zens", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spazieren gieng allein,", "tokens": ["Spa\u00b7zie\u00b7ren", "gieng", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In gr\u00fcnen Wald am Morgen,", "tokens": ["In", "gr\u00fc\u00b7nen", "Wald", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darin fand ich verborgen", "tokens": ["Da\u00b7rin", "fand", "ich", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ein sch\u00f6ns Jungfr\u00e4ulein voll Sorgen;", "tokens": ["Ein", "sch\u00f6ns", "Jung\u00b7fr\u00e4u\u00b7lein", "voll", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drum fragt ich es bald in Geheim,", "tokens": ["Drum", "fragt", "ich", "es", "bald", "in", "Ge\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Auf wen sie wartet hier allein.", "tokens": ["Auf", "wen", "sie", "war\u00b7tet", "hier", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sprach: ich liebt im Herzen", "tokens": ["Sie", "sprach", ":", "ich", "liebt", "im", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein J\u00fcngling tugendvoll;", "tokens": ["Ein", "J\u00fcng\u00b7ling", "tu\u00b7gend\u00b7voll", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er aber th\u00e4t nur scherzen,", "tokens": ["Er", "a\u00b7ber", "th\u00e4t", "nur", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lohnte mir nit wohl,", "tokens": ["Und", "lohn\u00b7te", "mir", "nit", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum will ich hier verderben.", "tokens": ["Drum", "will", "ich", "hier", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich sprach: Ihr sollt nit sterben,", "tokens": ["Ich", "sprach", ":", "Ihr", "sollt", "nit", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00dft mich euer Gunst erwerben,", "tokens": ["La\u00dft", "mich", "eu\u00b7er", "Gunst", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dr\u00fcckt mich an ihr Herz hinan,", "tokens": ["Und", "dr\u00fcckt", "mich", "an", "ihr", "Herz", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df mir vor Lieb das mein zersprang.", "tokens": ["Da\u00df", "mir", "vor", "Lieb", "das", "mein", "zer\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}