{"textgrid.poem.67105": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gebet f\u00fcr die Unheilbaren", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eil, o zaudernde Zeit, sie ans Ungereimte zu f\u00fchren,", "tokens": ["Eil", ",", "o", "zau\u00b7dern\u00b7de", "Zeit", ",", "sie", "ans", "Un\u00b7ge\u00b7reim\u00b7te", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$,", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Anders belehrest du sie nie, wie verst\u00e4ndig sie sind.", "tokens": ["An\u00b7ders", "be\u00b7leh\u00b7rest", "du", "sie", "nie", ",", "wie", "ver\u00b7st\u00e4n\u00b7dig", "sie", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Eile, verderbe sie ganz, und f\u00fchr ans furchtbare Nichts sie,", "tokens": ["Ei\u00b7le", ",", "ver\u00b7der\u00b7be", "sie", "ganz", ",", "und", "f\u00fchr", "ans", "furcht\u00b7ba\u00b7re", "Nichts", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN", "PPER", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Anders glauben sie dir nie, wie verdorben sie sind.", "tokens": ["An\u00b7ders", "glau\u00b7ben", "sie", "dir", "nie", ",", "wie", "ver\u00b7dor\u00b7ben", "sie", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "VVPP", "PPER", "VAFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Diese Toren bekehren sich nie, wenn ihnen nicht schwindelt,", "tokens": ["Die\u00b7se", "To\u00b7ren", "be\u00b7keh\u00b7ren", "sich", "nie", ",", "wenn", "ih\u00b7nen", "nicht", "schwin\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PRF", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Diese... sich nie, wenn sie Verwesung nicht sehn.", "tokens": ["Die\u00b7se", "...", "sich", "nie", ",", "wenn", "sie", "Ver\u00b7we\u00b7sung", "nicht", "sehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$(", "PRF", "ADV", "$,", "KOUS", "PPER", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}}}}