{"dta.poem.224": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Brunetten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich meine Meynung sagen/", "tokens": ["Soll", "ich", "mei\u00b7ne", "Mey\u00b7nung", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mein Hertze von euch hegt/", "tokens": ["Die", "mein", "Hert\u00b7ze", "von", "euch", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So verwerff ich mich zu retten", "tokens": ["So", "ver\u00b7werff", "ich", "mich", "zu", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den angenehmen Ketten/", "tokens": ["Von", "den", "an\u00b7ge\u00b7neh\u00b7men", "Ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ihr mir habt angelegt/", "tokens": ["Die", "ihr", "mir", "habt", "an\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wil sie best\u00e4ndig tragen.", "tokens": ["Und", "wil", "sie", "be\u00b7st\u00e4n\u00b7dig", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Eur beliebtes Angesichte/", "tokens": ["Eur", "be\u00b7lieb\u00b7tes", "An\u00b7ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So mit holden L\u00e4cheln spielt/", "tokens": ["So", "mit", "hol\u00b7den", "L\u00e4\u00b7cheln", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht mit unentwandten Blicken", "tokens": ["Sucht", "mit", "un\u00b7ent\u00b7wand\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Freyheit zubestricken;", "tokens": ["Mei\u00b7ne", "Frey\u00b7heit", "zu\u00b7be\u00b7stri\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn es blitzend auf mich zielt", "tokens": ["Wenn", "es", "blit\u00b7zend", "auf", "mich", "zielt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht mein freyer Sinn zu nichte.", "tokens": ["Geht", "mein", "frey\u00b7er", "Sinn", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die anmuths-vollen Br\u00fcste", "tokens": ["Wenn", "die", "an\u00b7muths\u00b7vol\u00b7len", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich bald auf bald nieder bl\u00e4hn/", "tokens": ["Sich", "bald", "auf", "bald", "nie\u00b7der", "bl\u00e4hn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADV", "PTKVZ", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df in diesen sch\u00f6nen Wellen", "tokens": ["Mu\u00df", "in", "die\u00b7sen", "sch\u00f6\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Freyheit-Schiff zerschellen", "tokens": ["Mei\u00b7ner", "Frey\u00b7heit\u00b7Schiff", "zer\u00b7schel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und darauf zu Grunde gehn;", "tokens": ["Und", "da\u00b7rauf", "zu", "Grun\u00b7de", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Uberschwemmt durch so viel L\u00fcste.", "tokens": ["U\u00b7ber\u00b7schwemmt", "durch", "so", "viel", "L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kan ich aber in dem Haven/", "tokens": ["Kan", "ich", "a\u00b7ber", "in", "dem", "Ha\u00b7ven", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Liebe Hoffstadt h\u00e4lt/", "tokens": ["Wo", "die", "Lie\u00b7be", "Hoff\u00b7stadt", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Schiffbruch ohne Stranden", "tokens": ["Oh\u00b7ne", "Schiff\u00b7bruch", "oh\u00b7ne", "Stran\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen euren H\u00fcfften landen/", "tokens": ["Zwi\u00b7schen", "eu\u00b7ren", "H\u00fcff\u00b7ten", "lan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und beschreitt ich diesen Belt", "tokens": ["Und", "be\u00b7schreitt", "ich", "die\u00b7sen", "Belt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mach ich mich zu eurem Schlaven.", "tokens": ["Mach", "ich", "mich", "zu", "eu\u00b7rem", "Schla\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Meine Seel ist nun eur eigen/", "tokens": ["Mei\u00b7ne", "Seel", "ist", "nun", "eur", "ei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Hertze h\u00f6rt euch zu/", "tokens": ["Und", "mein", "Hert\u00b7ze", "h\u00f6rt", "euch", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "VVFIN", "PPER", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich aus so sch\u00f6nen Gr\u00fcnden", "tokens": ["Weil", "ich", "aus", "so", "sch\u00f6\u00b7nen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnen meinen Ancker winden.", "tokens": ["K\u00f6n\u00b7nen", "mei\u00b7nen", "An\u00b7cker", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich mache/ was ich thu", "tokens": ["Was", "ich", "ma\u00b7che", "/", "was", "ich", "thu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$(", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df euch meine Ehr-Furcht zeigen.", "tokens": ["Mu\u00df", "euch", "mei\u00b7ne", "Ehr\u00b7Furcht", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unvergleichliche ", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7li\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Dis ist meine Danckbarkeit/", "tokens": ["Dis", "ist", "mei\u00b7ne", "Dan\u00b7ck\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ja ich wil zum Angedencken", "tokens": ["Ja", "ich", "wil", "zum", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich euch gantz zu eigen schencken.", "tokens": ["Mich", "euch", "gantz", "zu", "ei\u00b7gen", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Dienste sind bereit/", "tokens": ["Mei\u00b7ne", "Diens\u00b7te", "sind", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So am Tage als zu Bette.", "tokens": ["So", "am", "Ta\u00b7ge", "als", "zu", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}