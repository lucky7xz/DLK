{"textgrid.poem.43079": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lebend unter Niedern und Rohen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebend unter Niedern und Rohen", "tokens": ["Le\u00b7bend", "un\u00b7ter", "Nie\u00b7dern", "und", "Ro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zieht's mich m\u00e4chtig empor zum Hohen;", "tokens": ["Zieht's", "mich", "m\u00e4ch\u00b7tig", "em\u00b7por", "zum", "Ho\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Doch die Fl\u00fcgel beschwert mit Steinen,", "tokens": ["Doch", "die", "Fl\u00fc\u00b7gel", "be\u00b7schwert", "mit", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sink' ich auf's neue herab zum Gemeinen.", "tokens": ["Sink'", "ich", "auf's", "neu\u00b7e", "her\u00b7ab", "zum", "Ge\u00b7mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "ADJA", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "M\u00fcde des Eklen und Kleinen", "tokens": ["M\u00fc\u00b7de", "des", "Ek\u00b7len", "und", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Eil' ich zu Orgien aus bitterer Noth \u2013", "tokens": ["Eil'", "ich", "zu", "Or\u00b7gi\u00b7en", "aus", "bit\u00b7te\u00b7rer", "Noth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Und so, begeistert vom Reinen,", "tokens": ["Und", "so", ",", "be\u00b7geis\u00b7tert", "vom", "Rei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Erstick' ich noch im Koth!", "tokens": ["Er\u00b7stick'", "ich", "noch", "im", "Koth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}