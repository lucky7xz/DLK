{"textgrid.poem.62203": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[parteilichkeit, Parteienha\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Parteilichkeit, Parteienha\u00df,", "tokens": ["Par\u00b7tei\u00b7lich\u00b7keit", ",", "Par\u00b7tei\u00b7en\u00b7ha\u00df", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schaut so gr\u00fcn und wird so bla\u00df \u2013", "tokens": ["Das", "schaut", "so", "gr\u00fcn", "und", "wird", "so", "bla\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Schlang' und Nesseln ein Gew\u00fchl! \u2013", "tokens": ["Von", "Schlang'", "und", "Nes\u00b7seln", "ein", "Ge\u00b7w\u00fchl", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Welch unnat\u00fcrliches Gef\u00fchl!", "tokens": ["Welch", "un\u00b7na\u00b7t\u00fcr\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch unnat\u00fcrliches Gef\u00fchl!", "tokens": ["Welch", "un\u00b7na\u00b7t\u00fcr\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O kurze Zeit des Lebens Zeit,", "tokens": ["O", "kur\u00b7ze", "Zeit", "des", "Le\u00b7bens", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch k\u00fcrzer durch Parteilichkeit", "tokens": ["Noch", "k\u00fcr\u00b7zer", "durch", "Par\u00b7tei\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Konfession und Politik:", "tokens": ["In", "Kon\u00b7fes\u00b7si\u00b7on", "und", "Po\u00b7li\u00b7tik", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Parteienha\u00df hat keinen Schick!", "tokens": ["Par\u00b7tei\u00b7en\u00b7ha\u00df", "hat", "kei\u00b7nen", "Schick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Parteienha\u00df hat keinen Schick!", "tokens": ["Par\u00b7tei\u00b7en\u00b7ha\u00df", "hat", "kei\u00b7nen", "Schick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}