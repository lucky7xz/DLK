{"textgrid.poem.55072": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Lustigen von Weimar", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Donnerstag nach Belvedere,", "tokens": ["Don\u00b7ners\u00b7tag", "nach", "Bel\u00b7ve\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freitag geht's nach Jena fort:", "tokens": ["Frei\u00b7tag", "geht's", "nach", "Je\u00b7na", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn das ist, bei meiner Ehre,", "tokens": ["Denn", "das", "ist", ",", "bei", "mei\u00b7ner", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ein allerliebster Ort!", "tokens": ["Doch", "ein", "al\u00b7ler\u00b7liebs\u00b7ter", "Ort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Samstag ist's, worauf wir zielen,", "tokens": ["Sams\u00b7tag", "ist's", ",", "wo\u00b7rauf", "wir", "zie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonntag rutscht man auf das Land;", "tokens": ["Sonn\u00b7tag", "rutscht", "man", "auf", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zw\u00e4zen, Burgau, Schneidem\u00fchlen", "tokens": ["Zw\u00e4\u00b7zen", ",", "Bur\u00b7gau", ",", "Schnei\u00b7de\u00b7m\u00fch\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind uns alle wohlbekannt.", "tokens": ["Sind", "uns", "al\u00b7le", "wohl\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Montag reizet uns die B\u00fchne;", "tokens": ["Mon\u00b7tag", "rei\u00b7zet", "uns", "die", "B\u00fch\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dienstag schleicht dann auch herbei,", "tokens": ["Diens\u00b7tag", "schleicht", "dann", "auch", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er bringt zu stiller S\u00fchne", "tokens": ["Doch", "er", "bringt", "zu", "stil\u00b7ler", "S\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Rapuschchen frank und frei.", "tokens": ["Ein", "Ra\u00b7pu\u00b7schchen", "frank", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mittwoch fehlt es nicht an R\u00fchrung:", "tokens": ["Mitt\u00b7woch", "fehlt", "es", "nicht", "an", "R\u00fch\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn es gibt ein gutes St\u00fcck;", "tokens": ["Denn", "es", "gibt", "ein", "gu\u00b7tes", "St\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Donnerstag lenkt die Verf\u00fchrung", "tokens": ["Don\u00b7ners\u00b7tag", "lenkt", "die", "Ver\u00b7f\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Uns nach Belveder' zur\u00fcck.", "tokens": ["Uns", "nach", "Bel\u00b7ve\u00b7der'", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es schlingt ununterbrochen", "tokens": ["Und", "es", "schlingt", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer sich der Freudenkreis", "tokens": ["Im\u00b7mer", "sich", "der", "Freu\u00b7den\u00b7kreis"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die zweiundfunfzig Wochen,", "tokens": ["Durch", "die", "zwei\u00b7und\u00b7funf\u00b7zig", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man's recht zu f\u00fchren wei\u00df.", "tokens": ["Wenn", "man's", "recht", "zu", "f\u00fch\u00b7ren", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Spiel und Tanz, Gespr\u00e4ch, Theater,", "tokens": ["Spiel", "und", "Tanz", ",", "Ge\u00b7spr\u00e4ch", ",", "The\u00b7a\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie erfrischen unser Blut;", "tokens": ["Sie", "er\u00b7fri\u00b7schen", "un\u00b7ser", "Blut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00dft den Wienern ihren Prater;", "tokens": ["La\u00dft", "den", "Wie\u00b7nern", "ih\u00b7ren", "Pra\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weimar, Jena, da ist's gut!", "tokens": ["Wei\u00b7mar", ",", "Je\u00b7na", ",", "da", "ist's", "gut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}}}}