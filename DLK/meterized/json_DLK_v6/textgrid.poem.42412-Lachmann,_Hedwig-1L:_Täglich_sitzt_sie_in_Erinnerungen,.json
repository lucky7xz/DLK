{"textgrid.poem.42412": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: T\u00e4glich sitzt sie in Erinnerungen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00e4glich sitzt sie in Erinnerungen,", "tokens": ["T\u00e4g\u00b7lich", "sitzt", "sie", "in", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihre H\u00e4nde auf den Knien verschlungen.", "tokens": ["Ih\u00b7re", "H\u00e4n\u00b7de", "auf", "den", "Kni\u00b7en", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Eingesponnen in ihr Traumgewebe,", "tokens": ["Ein\u00b7ge\u00b7spon\u00b7nen", "in", "ihr", "Traum\u00b7ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schaukelnd auf versunkner Zeiten Schwebe.", "tokens": ["Schau\u00b7kelnd", "auf", "ver\u00b7sun\u00b7kner", "Zei\u00b7ten", "Schwe\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ihre Quellen haben sich geschlossen,", "tokens": ["Ih\u00b7re", "Quel\u00b7len", "ha\u00b7ben", "sich", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sind ins Innere zur\u00fcckgeflossen;", "tokens": ["Sind", "ins", "In\u00b7ne\u00b7re", "zu\u00b7r\u00fcck\u00b7ge\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Auf dem dunklen Seelenspiegel jagen", "tokens": ["Auf", "dem", "dunk\u00b7len", "See\u00b7len\u00b7spie\u00b7gel", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich die Schatten von gelebten Tagen.", "tokens": ["Sich", "die", "Schat\u00b7ten", "von", "ge\u00b7leb\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Immer weht es aus dem Unsichtbaren:", "tokens": ["Im\u00b7mer", "weht", "es", "aus", "dem", "Un\u00b7sicht\u00b7ba\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist sie nicht die Gleiche wie vor Jahren?", "tokens": ["Ist", "sie", "nicht", "die", "Glei\u00b7che", "wie", "vor", "Jah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "In dem unterirdischen Verstecke", "tokens": ["In", "dem", "un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "Ver\u00b7ste\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sprengt ihr fr\u00fches Selbst die leichte Decke,", "tokens": ["Sprengt", "ihr", "fr\u00fc\u00b7hes", "Selbst", "die", "leich\u00b7te", "De\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Dr\u00e4ngt mit ungebrochnen Schmerzgewalten", "tokens": ["Dr\u00e4ngt", "mit", "un\u00b7ge\u00b7broch\u00b7nen", "Schmerz\u00b7ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich im Bild der Seele zu gestalten.", "tokens": ["Sich", "im", "Bild", "der", "See\u00b7le", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Scheu verschlossne Sehnsuchtstriebe springen,", "tokens": ["Scheu", "ver\u00b7schloss\u00b7ne", "Sehn\u00b7sucht\u00b7strie\u00b7be", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zitternd, sich aufs neue darzubringen.", "tokens": ["Zit\u00b7ternd", ",", "sich", "aufs", "neu\u00b7e", "dar\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "APPRART", "ADJA", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "In dem Sternenglanz der Allmacht spiegelt", "tokens": ["In", "dem", "Ster\u00b7nen\u00b7glanz", "der", "All\u00b7macht", "spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich ihr Sein vollendet und entsiegelt.", "tokens": ["Sich", "ihr", "Sein", "voll\u00b7en\u00b7det", "und", "ent\u00b7sie\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}