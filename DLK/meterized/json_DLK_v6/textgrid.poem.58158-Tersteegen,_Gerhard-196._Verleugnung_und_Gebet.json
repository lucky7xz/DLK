{"textgrid.poem.58158": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "196. Verleugnung und Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verleugnung und Gebet sind Schwestern, die sich lieben,", "tokens": ["Ver\u00b7leug\u00b7nung", "und", "Ge\u00b7bet", "sind", "Schwes\u00b7tern", ",", "die", "sich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer eine von sich st\u00f6\u00dft, hat beide schon vertrieben,", "tokens": ["Wer", "ei\u00b7ne", "von", "sich", "st\u00f6\u00dft", ",", "hat", "bei\u00b7de", "schon", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "APPR", "PRF", "VVFIN", "$,", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer eine sucht, der mach' der andern sich gemein,", "tokens": ["Wer", "ei\u00b7ne", "sucht", ",", "der", "mach'", "der", "an\u00b7dern", "sich", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "ADJA", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer beide liebt und \u00fcbt, wird bald geheiligt sein.", "tokens": ["Wer", "bei\u00b7de", "liebt", "und", "\u00fcbt", ",", "wird", "bald", "ge\u00b7hei\u00b7ligt", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}