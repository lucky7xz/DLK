{"textgrid.poem.67478": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Schmetterling und die Rose", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe den Schmetterling hier! Er k\u00fc\u00dft die bl\u00fchende Rose;", "tokens": ["Sie\u00b7he", "den", "Schmet\u00b7ter\u00b7ling", "hier", "!", "Er", "k\u00fc\u00dft", "die", "bl\u00fc\u00b7hen\u00b7de", "Ro\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bald ist der Schmetterling nicht, bald auch die Rose nicht mehr.", "tokens": ["Bald", "ist", "der", "Schmet\u00b7ter\u00b7ling", "nicht", ",", "bald", "auch", "die", "Ro\u00b7se", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "$,", "ADV", "ADV", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}