{"dta.poem.1203": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "AnEMONS  \n und  \n  ADONIS  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.85", "en:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich rede nicht wie vor so frey/", "tokens": ["Ich", "re\u00b7de", "nicht", "wie", "vor", "so", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "APPR", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Auge klebt der Erden an/", "tokens": ["Mein", "Au\u00b7ge", "klebt", "der", "Er\u00b7den", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und findet sich mit Furcht herbey/", "tokens": ["Und", "fin\u00b7det", "sich", "mit", "Furcht", "her\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man dich/ Nimphe/ schauen kan;", "tokens": ["Wo", "man", "dich", "/", "Nim\u00b7phe", "/", "schau\u00b7en", "kan", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "$(", "NE", "$(", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verbrochne Seufftzer und gestohlne Blicke", "tokens": ["Ver\u00b7broch\u00b7ne", "Seufft\u00b7zer", "und", "ge\u00b7stohl\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sinds/ die ich dir/ mein Kind/ entgegen schicke.", "tokens": ["Sinds", "/", "die", "ich", "dir", "/", "mein", "Kind", "/", "ent\u00b7ge\u00b7gen", "schi\u00b7cke", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "PRELS", "PPER", "PPER", "$(", "PPOSAT", "NN", "$(", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der strengen Auff sicht scharffe Wacht/", "tokens": ["Der", "stren\u00b7gen", "Auff", "sicht", "scharf\u00b7fe", "Wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Neyd und Eyfer um uns stellt/", "tokens": ["Die", "Neyd", "und", "Ey\u00b7fer", "um", "uns", "stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimmt ein iedweders Wort in acht", "tokens": ["Nimmt", "ein", "ied\u00b7we\u00b7ders", "Wort", "in", "acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "PIAT", "NN", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So uns von ungef\u00e4hr entf\u00e4llt/", "tokens": ["So", "uns", "von", "un\u00b7ge\u00b7f\u00e4hr", "ent\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Heist unsre Unschuld stets in Sorgen stehen/", "tokens": ["Heist", "uns\u00b7re", "Un\u00b7schuld", "stets", "in", "Sor\u00b7gen", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und zwischen Dorn und Ei\u00df behutsam gehen.", "tokens": ["Und", "zwi\u00b7schen", "Dorn", "und", "Ei\u00df", "be\u00b7hut\u00b7sam", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die schlimme Welt denckt/ Ich und Du", "tokens": ["Die", "schlim\u00b7me", "Welt", "denckt", "/", "Ich", "und", "Du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00fc\u00df ihr an Bo\u00dfheit gleiche seyn/", "tokens": ["M\u00fc\u00df", "ihr", "an", "Bo\u00df\u00b7heit", "glei\u00b7che", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dringt sich mit sch\u00e4lem Aug herzu/", "tokens": ["Dringt", "sich", "mit", "sch\u00e4\u00b7lem", "Aug", "her\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "ADV", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Greifft unsern keuschen Freuden ein/", "tokens": ["Greifft", "un\u00b7sern", "keu\u00b7schen", "Freu\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wolte gern/ was sie nicht kan gen\u00fcssen/", "tokens": ["Und", "wol\u00b7te", "gern", "/", "was", "sie", "nicht", "kan", "ge\u00b7n\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "$(", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch andern ohne Schuld verboten wissen.", "tokens": ["Auch", "an\u00b7dern", "oh\u00b7ne", "Schuld", "ver\u00b7bo\u00b7ten", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zwar wehe thut der schwere Zwang/", "tokens": ["Zwar", "we\u00b7he", "thut", "der", "schwe\u00b7re", "Zwang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dem man uns verbinden will;", "tokens": ["Zu", "dem", "man", "uns", "ver\u00b7bin\u00b7den", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch wird solcher Uberdrang", "tokens": ["Je\u00b7doch", "wird", "sol\u00b7cher", "U\u00b7ber\u00b7drang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch haben sein gestecktes Ziel.", "tokens": ["Auch", "ha\u00b7ben", "sein", "ge\u00b7steck\u00b7tes", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tugend reines Kleid kan nichts beflecken;", "tokens": ["Der", "Tu\u00b7gend", "rei\u00b7nes", "Kleid", "kan", "nichts", "be\u00b7fle\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Zeit wird unser Recht der Welt entdecken.", "tokens": ["Die", "Zeit", "wird", "un\u00b7ser", "Recht", "der", "Welt", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der beste Rath ist hier Gedult:", "tokens": ["Der", "bes\u00b7te", "Rath", "ist", "hier", "Ge\u00b7dult", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleib\u2019 mir best\u00e4ndig/ wie du bist/", "tokens": ["Bleib'", "mir", "be\u00b7st\u00e4n\u00b7dig", "/", "wie", "du", "bist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$(", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lebe dir in stillem hold/", "tokens": ["Ich", "le\u00b7be", "dir", "in", "stil\u00b7lem", "hold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So brechen wir der Feinde List.", "tokens": ["So", "bre\u00b7chen", "wir", "der", "Fein\u00b7de", "List", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Redligkeit sich kan zun Sternen heben/", "tokens": ["Wenn", "Red\u00b7lig\u00b7keit", "sich", "kan", "zun", "Ster\u00b7nen", "he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mu\u00df der Verleumder Maul im Kothe kleben.", "tokens": ["Mu\u00df", "der", "Ver\u00b7leum\u00b7der", "Maul", "im", "Ko\u00b7the", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}