{"textgrid.poem.61036": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lang ist's doch, da\u00df ich nicht sang?", "tokens": ["Wie", "lang", "ist's", "doch", ",", "da\u00df", "ich", "nicht", "sang", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl Monden sind dahingegangen -", "tokens": ["Wohl", "Mon\u00b7den", "sind", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "PAV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein langer Winter tr\u00fcb und bang", "tokens": ["Ein", "lan\u00b7ger", "Win\u00b7ter", "tr\u00fcb", "und", "bang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hielt mir zuletzt den Sinn befangen.", "tokens": ["Hielt", "mir", "zu\u00b7letzt", "den", "Sinn", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er brachte mir des Bittern viel;", "tokens": ["Er", "brach\u00b7te", "mir", "des", "Bit\u00b7tern", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es waren da viel falsche Zungen,", "tokens": ["Es", "wa\u00b7ren", "da", "viel", "fal\u00b7sche", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die trieben gar ein schlimmes Spiel,", "tokens": ["Die", "trie\u00b7ben", "gar", "ein", "schlim\u00b7mes", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So da\u00df mir fast das Herz zersprungen.", "tokens": ["So", "da\u00df", "mir", "fast", "das", "Herz", "zer\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu fremder Torheit eigne Schuld", "tokens": ["Zu", "frem\u00b7der", "Tor\u00b7heit", "eig\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versehrte mich mit gift'gen Pfeilen -", "tokens": ["Ver\u00b7sehr\u00b7te", "mich", "mit", "gift'\u00b7gen", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nun Geduld, o Herz, Geduld!", "tokens": ["Doch", "nun", "Ge\u00b7duld", ",", "o", "Herz", ",", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "FM", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fr\u00fchling kommt, er wird dich heilen.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", ",", "er", "wird", "dich", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die ersten Knospen werden wach,", "tokens": ["Die", "ers\u00b7ten", "Knos\u00b7pen", "wer\u00b7den", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bach entrauscht in schnellen Wogen;", "tokens": ["Der", "Bach", "ent\u00b7rauscht", "in", "schnel\u00b7len", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein dumpfes Gr\u00e4men rauscht ihm nach -", "tokens": ["Mein", "dum\u00b7pfes", "Gr\u00e4\u00b7men", "rauscht", "ihm", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frisch auf, und in die Welt gezogen!", "tokens": ["Frisch", "auf", ",", "und", "in", "die", "Welt", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}