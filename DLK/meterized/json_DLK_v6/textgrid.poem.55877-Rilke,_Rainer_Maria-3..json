{"textgrid.poem.55877": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seine Diener f\u00fcttern mit mehr und mehr", "tokens": ["Sei\u00b7ne", "Die\u00b7ner", "f\u00fct\u00b7tern", "mit", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "KON", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ein Rudel von jenen wilden Ger\u00fcchten,", "tokens": ["ein", "Ru\u00b7del", "von", "je\u00b7nen", "wil\u00b7den", "Ge\u00b7r\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "die auch noch Er sind, alles noch Er.", "tokens": ["die", "auch", "noch", "Er", "sind", ",", "al\u00b7les", "noch", "Er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "VAFIN", "$,", "PIS", "ADV", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Seine G\u00fcnstlinge fl\u00fcchten vor ihm her.", "tokens": ["Sei\u00b7ne", "G\u00fcnst\u00b7lin\u00b7ge", "fl\u00fcch\u00b7ten", "vor", "ihm", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-++-+-+-+", "measure": "zehnsilber"}}, "stanza.3": {"line.1": {"text": "Und seine Frauen fl\u00fcstern und stiften", "tokens": ["Und", "sei\u00b7ne", "Frau\u00b7en", "fl\u00fcs\u00b7tern", "und", "stif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "B\u00fcnde. Und er h\u00f6rt sie ganz innen", "tokens": ["B\u00fcn\u00b7de", ".", "Und", "er", "h\u00f6rt", "sie", "ganz", "in\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "in ihren Gem\u00e4chern mit Dienerinnen,", "tokens": ["in", "ih\u00b7ren", "Ge\u00b7m\u00e4\u00b7chern", "mit", "Die\u00b7ne\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "die sich scheu umsehn, sprechen von Giften.", "tokens": ["die", "sich", "scheu", "um\u00b7sehn", ",", "spre\u00b7chen", "von", "Gif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADJD", "VVINF", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Alle W\u00e4nde sind hohl von Schr\u00e4nken und F\u00e4chern,", "tokens": ["Al\u00b7le", "W\u00e4n\u00b7de", "sind", "hohl", "von", "Schr\u00e4n\u00b7ken", "und", "F\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "M\u00f6rder ducken unter den D\u00e4chern", "tokens": ["M\u00f6r\u00b7der", "du\u00b7cken", "un\u00b7ter", "den", "D\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "und spielen M\u00f6nche mit viel Geschick.", "tokens": ["und", "spie\u00b7len", "M\u00f6n\u00b7che", "mit", "viel", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und er hat nichts als einen Blick", "tokens": ["Und", "er", "hat", "nichts", "als", "ei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dann und wann; als den leisen", "tokens": ["dann", "und", "wann", ";", "als", "den", "lei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KON", "PWAV", "$.", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schritt auf den Treppen die kreisen;", "tokens": ["Schritt", "auf", "den", "Trep\u00b7pen", "die", "krei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "nichts als das Eisen an seinem Stock.", "tokens": ["nichts", "als", "das", "Ei\u00b7sen", "an", "sei\u00b7nem", "Stock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Nichts als den d\u00fcrftigen B\u00fc\u00dferrock", "tokens": ["Nichts", "als", "den", "d\u00fcrf\u00b7ti\u00b7gen", "B\u00fc\u00b7\u00dfer\u00b7rock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "(durch den die K\u00e4lte aus den Fliesen", "tokens": ["(", "durch", "den", "die", "K\u00e4l\u00b7te", "aus", "den", "Flie\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "an ihm hinaufkriecht wie mit Krallen)", "tokens": ["an", "ihm", "hin\u00b7auf\u00b7kriecht", "wie", "mit", "Kral\u00b7len", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nichts, was er zu rufen wagt,", "tokens": ["nichts", ",", "was", "er", "zu", "ru\u00b7fen", "wagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "nichts als die Angst vor allen diesen,", "tokens": ["nichts", "als", "die", "Angst", "vor", "al\u00b7len", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "APPR", "PIAT", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "nichts als die t\u00e4gliche Angst vor Allen,", "tokens": ["nichts", "als", "die", "t\u00e4g\u00b7li\u00b7che", "Angst", "vor", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "die ihn jagt durch diese gejagten", "tokens": ["die", "ihn", "jagt", "durch", "die\u00b7se", "ge\u00b7jag\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PDAT", "ADJA"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gesichter, an dunklen ungefragten", "tokens": ["Ge\u00b7sich\u00b7ter", ",", "an", "dunk\u00b7len", "un\u00b7ge\u00b7frag\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "vielleicht schuldigen H\u00e4nden entlang.", "tokens": ["viel\u00b7leicht", "schul\u00b7di\u00b7gen", "H\u00e4n\u00b7den", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Manchmal packt er Einen im Gang", "tokens": ["Manch\u00b7mal", "packt", "er", "Ei\u00b7nen", "im", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "grade noch an des Mantels Falten,", "tokens": ["gra\u00b7de", "noch", "an", "des", "Man\u00b7tels", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "und er zerrt ihn zornig her;", "tokens": ["und", "er", "zerrt", "ihn", "zor\u00b7nig", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "aber im Fenster wei\u00df er nicht mehr:", "tokens": ["a\u00b7ber", "im", "Fens\u00b7ter", "wei\u00df", "er", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "wer ist Haltender? Wer ist gehalten?", "tokens": ["wer", "ist", "Hal\u00b7ten\u00b7der", "?", "Wer", "ist", "ge\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "PWS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wer bin ich und wer ist der?", "tokens": ["Wer", "bin", "ich", "und", "wer", "ist", "der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KON", "PWS", "VAFIN", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}