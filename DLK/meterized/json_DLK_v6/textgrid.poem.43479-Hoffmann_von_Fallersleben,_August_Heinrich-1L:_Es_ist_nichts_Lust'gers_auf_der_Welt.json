{"textgrid.poem.43479": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist nichts Lust'gers auf der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nichts Lust'gers auf der Welt", "tokens": ["Es", "ist", "nichts", "Lust'\u00b7gers", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nichts ist so geschwind,", "tokens": ["Und", "nichts", "ist", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Als wir Husaren in dem Feld", "tokens": ["Als", "wir", "Hu\u00b7sa\u00b7ren", "in", "dem", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und vor dem Feinde sind.", "tokens": ["Und", "vor", "dem", "Fein\u00b7de", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir schw\u00e4rmen wie ein Bienenschwarm", "tokens": ["Wir", "schw\u00e4r\u00b7men", "wie", "ein", "Bie\u00b7nen\u00b7schwarm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rings um den Feind herum,", "tokens": ["Rings", "um", "den", "Feind", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer nicht l\u00e4uft, da\u00df Gott erbarm'!", "tokens": ["Und", "wer", "nicht", "l\u00e4uft", ",", "da\u00df", "Gott", "er\u00b7barm'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den hau'n wir blitzschnell um.", "tokens": ["Den", "hau'n", "wir", "blitz\u00b7schnell", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und steht der Feind auch felsenfest,", "tokens": ["Und", "steht", "der", "Feind", "auch", "fel\u00b7sen\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hei\u00dft es: Drauf! juchhe!", "tokens": ["So", "hei\u00dft", "es", ":", "Drauf", "!", "juch\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PAV", "$.", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da dringen wir ihm in sein Nest", "tokens": ["Da", "drin\u00b7gen", "wir", "ihm", "in", "sein", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprengen sein ", "tokens": ["Und", "spren\u00b7gen", "sein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Das hat der ", "tokens": ["Das", "hat", "der"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Dem Feind' oft beigebracht;", "tokens": ["Dem", "Feind'", "oft", "bei\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hat der ", "tokens": ["Das", "hat", "der"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Gezeigt in mancher Schlacht.", "tokens": ["Ge\u00b7zeigt", "in", "man\u00b7cher", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn wir siegreich nicht bestehn,", "tokens": ["Und", "wenn", "wir", "sieg\u00b7reich", "nicht", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bleibt's uns unverwehrt:", "tokens": ["So", "bleibt's", "uns", "un\u00b7ver\u00b7wehrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So la\u00dft uns k\u00e4mpfend untergehn,", "tokens": ["So", "la\u00dft", "uns", "k\u00e4mp\u00b7fend", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann sind wir noch geehrt", "tokens": ["Dann", "sind", "wir", "noch", "ge\u00b7ehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So sei's und bleib' es immerdar,", "tokens": ["So", "sei's", "und", "bleib'", "es", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die rechte Art,", "tokens": ["Das", "ist", "die", "rech\u00b7te", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang' ein preu\u00dfischer Husar", "tokens": ["So", "lang'", "ein", "preu\u00b7\u00dfi\u00b7scher", "Hu\u00b7sar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich streichet seinen Bart.", "tokens": ["Sich", "strei\u00b7chet", "sei\u00b7nen", "Bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum singen wir mit frohem Schall:", "tokens": ["Drum", "sin\u00b7gen", "wir", "mit", "fro\u00b7hem", "Schall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hurrah! Trompetentusch!", "tokens": ["Hur\u00b7rah", "!", "Trom\u00b7pe\u00b7ten\u00b7tusch", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hoch ", "tokens": ["Hoch"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Hoch ", "tokens": ["Hoch"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}}}}