{"dta.poem.10469": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umte: traurig schaute der Mond,", "tokens": ["Mir", "tr\u00e4um\u00b7te", ":", "trau\u00b7rig", "schau\u00b7te", "der", "Mond", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und traurig schienen die Sterne;", "tokens": ["Und", "trau\u00b7rig", "schie\u00b7nen", "die", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es trug mich zur Stadt, wo Liebchen wohnt,", "tokens": ["Es", "trug", "mich", "zur", "Stadt", ",", "wo", "Lieb\u00b7chen", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Viel hundert Meilen ferne.", "tokens": ["Viel", "hun\u00b7dert", "Mei\u00b7len", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat mich zu ihrem Hause gef\u00fchrt,", "tokens": ["Es", "hat", "mich", "zu", "ih\u00b7rem", "Hau\u00b7se", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich k\u00fc\u00dfte die Steine der Treppe,", "tokens": ["Ich", "k\u00fc\u00df\u00b7te", "die", "Stei\u00b7ne", "der", "Trep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die oft ihr kleiner Fu\u00df ber\u00fchrt,", "tokens": ["Die", "oft", "ihr", "klei\u00b7ner", "Fu\u00df", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihres Kleides Schleppe.", "tokens": ["Und", "ih\u00b7res", "Klei\u00b7des", "Schlep\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Nacht war lang, die Nacht war kalt,", "tokens": ["Die", "Nacht", "war", "lang", ",", "die", "Nacht", "war", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es waren so kalt die Steine;", "tokens": ["Es", "wa\u00b7ren", "so", "kalt", "die", "Stei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es lugt' aus dem Fenster die blasse Gestalt,", "tokens": ["Es", "lugt'", "aus", "dem", "Fens\u00b7ter", "die", "blas\u00b7se", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Beleuchtet vom Mondenscheine.", "tokens": ["Be\u00b7leuch\u00b7tet", "vom", "Mon\u00b7den\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}