{"textgrid.poem.52810": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfe deutsche Literaturballade", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gegen Abend in der Abendr\u00f6the,", "tokens": ["Ge\u00b7gen", "A\u00b7bend", "in", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ferne von der Menschen rohem Schwarm,", "tokens": ["Fer\u00b7ne", "von", "der", "Men\u00b7schen", "ro\u00b7hem", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wandelten der Schiller und der G\u00f6the", "tokens": ["Wan\u00b7del\u00b7ten", "der", "Schil\u00b7ler", "und", "der", "G\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oft spazieren Arm in Arm.", "tokens": ["Oft", "spa\u00b7zie\u00b7ren", "Arm", "in", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie betrachteten die sch\u00f6ne Landschaft,", "tokens": ["Sie", "be\u00b7trach\u00b7te\u00b7ten", "die", "sch\u00f6\u00b7ne", "Land\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dr\u00fcckten sich die gro\u00dfen edlen H\u00e4nd',", "tokens": ["Dr\u00fcck\u00b7ten", "sich", "die", "gro\u00b7\u00dfen", "ed\u00b7len", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Gl\u00fccklich im Gef\u00fchl der Wahlverwandtschaft", "tokens": ["Gl\u00fcck\u00b7lich", "im", "Ge\u00b7f\u00fchl", "der", "Wahl\u00b7ver\u00b7wandt\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unterhielten sie sich excellent.", "tokens": ["Un\u00b7ter\u00b7hiel\u00b7ten", "sie", "sich", "ex\u00b7cel\u00b7lent", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dieser war schon etwas grau von Haaren,", "tokens": ["Die\u00b7ser", "war", "schon", "et\u00b7was", "grau", "von", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jener zwar nicht weit vom fr\u00fchen Grab,", "tokens": ["Je\u00b7ner", "zwar", "nicht", "weit", "vom", "fr\u00fc\u00b7hen", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "PTKNEG", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber grad in seinen besten Jahren", "tokens": ["A\u00b7ber", "grad", "in", "sei\u00b7nen", "bes\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als ein Dichter und geborner Schwab.", "tokens": ["Als", "ein", "Dich\u00b7ter", "und", "ge\u00b7bor\u00b7ner", "Schwab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Keiner that dem Andern was verhehlen,", "tokens": ["Kei\u00b7ner", "that", "dem", "An\u00b7dern", "was", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "PWS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sie vertauschten ihre Lorbeerkr\u00e4nz',", "tokens": ["Sie", "ver\u00b7tauschten", "ih\u00b7re", "Lor\u00b7beer\u00b7kr\u00e4nz'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und die sch\u00f6ne Harmonie der Seelen", "tokens": ["Und", "die", "sch\u00f6\u00b7ne", "Har\u00b7mo\u00b7nie", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Tr\u00fcbte nicht der Wahn der Convenienz.", "tokens": ["Tr\u00fcb\u00b7te", "nicht", "der", "Wahn", "der", "Con\u00b7ve\u00b7ni\u00b7enz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sehen Sie, so redete der G\u00f6the,", "tokens": ["Se\u00b7hen", "Sie", ",", "so", "re\u00b7de\u00b7te", "der", "G\u00f6\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dort die edle Pflanze in dem Gras,", "tokens": ["Dort", "die", "ed\u00b7le", "Pflan\u00b7ze", "in", "dem", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jenes Steingebilde, diese Kr\u00f6te,", "tokens": ["Je\u00b7nes", "Stein\u00b7ge\u00b7bil\u00b7de", ",", "die\u00b7se", "Kr\u00f6\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dort den Schmetterling und dies und das,", "tokens": ["Dort", "den", "Schmet\u00b7ter\u00b7ling", "und", "dies", "und", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "PDS", "KON", "PDS", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und \u2013 die Sonn', erwiederte verwundert", "tokens": ["Und", "\u2013", "die", "Sonn'", ",", "er\u00b7wie\u00b7der\u00b7te", "ver\u00b7wun\u00b7dert"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$(", "ART", "NN", "$,", "VVFIN", "VVFIN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Drauf der Schiller, sehen Sie, o Freund,", "tokens": ["Drauf", "der", "Schil\u00b7ler", ",", "se\u00b7hen", "Sie", ",", "o", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Eben, seh'n Sie, eben geht sie unter!", "tokens": ["E\u00b7ben", ",", "seh'n", "Sie", ",", "e\u00b7ben", "geht", "sie", "un\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "APPR", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "So hab' ich's im R\u00e4uber Moor gemeint.", "tokens": ["So", "hab'", "ich's", "im", "R\u00e4u\u00b7ber", "Moor", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ein andermal begann der Schiller,", "tokens": ["Und", "ein", "an\u00b7der\u00b7mal", "be\u00b7gann", "der", "Schil\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als sie wandelten am Wiesenbach,", "tokens": ["Als", "sie", "wan\u00b7del\u00b7ten", "am", "Wie\u00b7sen\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der G\u00f6the wurde immer stiller,", "tokens": ["Und", "der", "G\u00f6\u00b7the", "wur\u00b7de", "im\u00b7mer", "stil\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4hrend der entz\u00fcckte Schiller sprach:", "tokens": ["W\u00e4h\u00b7rend", "der", "ent\u00b7z\u00fcck\u00b7te", "Schil\u00b7ler", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sehen Sie, wie diese Wellen flie\u00dfen,", "tokens": ["Se\u00b7hen", "Sie", ",", "wie", "die\u00b7se", "Wel\u00b7len", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ohne Ruh und ohne Rast dahin,", "tokens": ["Oh\u00b7ne", "Ruh", "und", "oh\u00b7ne", "Rast", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie die Menschen alle wandern m\u00fc\u00dfen,", "tokens": ["Wie", "die", "Men\u00b7schen", "al\u00b7le", "wan\u00b7dern", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "VVFIN", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und die Zeiten unaufhaltsam flieh'n!", "tokens": ["Und", "die", "Zei\u00b7ten", "un\u00b7auf\u00b7halt\u00b7sam", "flieh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Herzlich ist, was Sie mir da bemerkten,", "tokens": ["Herz\u00b7lich", "ist", ",", "was", "Sie", "mir", "da", "be\u00b7merk\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "PPER", "PPER", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gab der G\u00f6the seinem Freund zur\u00fcck:", "tokens": ["Gab", "der", "G\u00f6\u00b7the", "sei\u00b7nem", "Freund", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seyn Sie \u00fcberzeugt, da\u00df Sie best\u00e4rkten", "tokens": ["Seyn", "Sie", "\u00fc\u00b7berz\u00b7eugt", ",", "da\u00df", "Sie", "be\u00b7st\u00e4rk\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Meinung von des Menschen Gl\u00fcck.", "tokens": ["Mei\u00b7ne", "Mei\u00b7nung", "von", "des", "Men\u00b7schen", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alles seh ich gleichsam in dem Wasser,", "tokens": ["Al\u00b7les", "seh", "ich", "gleich\u00b7sam", "in", "dem", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Form und Ordnung, Ma\u00dfstab und Bezug,", "tokens": ["Form", "und", "Ord\u00b7nung", ",", "Ma\u00df\u00b7stab", "und", "Be\u00b7zug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Vieles Trefflichen bin ich Verfasser,", "tokens": ["Vie\u00b7les", "Treff\u00b7li\u00b7chen", "bin", "ich", "Ver\u00b7fas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Doch am Ende sey's gerad genug.", "tokens": ["Doch", "am", "En\u00b7de", "sey's", "ge\u00b7rad", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbalexander und Homerus starben,", "tokens": ["\u00bb", "a\u00b7lex\u00b7an\u00b7der", "und", "Ho\u00b7me\u00b7rus", "star\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KON", "NE", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dieses ist das Loos von Allem fast.\u00ab", "tokens": ["Die\u00b7ses", "ist", "das", "Loos", "von", "Al\u00b7lem", "fast", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PIS", "ADV", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und was sagen Sie denn von den ", "tokens": ["Und", "was", "sa\u00b7gen", "Sie", "denn", "von", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welchen ich so sorgsam aufgepa\u00dft?\u00ab", "tokens": ["Wel\u00b7chen", "ich", "so", "sorg\u00b7sam", "auf\u00b7ge\u00b7pa\u00dft", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u00bbgeht es Ihnen auch so sehr zu Herzen,", "tokens": ["\u00bb", "geht", "es", "Ih\u00b7nen", "auch", "so", "sehr", "zu", "Her\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Herr Geheimerath, das Ideal?\u00ab", "tokens": ["Herr", "Ge\u00b7hei\u00b7me\u00b7rath", ",", "das", "I\u00b7deal", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mich ergreift, ich wei\u00df nicht darf ich scherzen,", "tokens": ["Mich", "er\u00b7greift", ",", "ich", "wei\u00df", "nicht", "darf", "ich", "scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Himmlisches Behagen auf einmal'!", "tokens": ["Himm\u00b7li\u00b7sches", "Be\u00b7ha\u00b7gen", "auf", "ein\u00b7mal'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIS", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Unter solchen g\u00f6ttlichen Gespr\u00e4chen", "tokens": ["Un\u00b7ter", "sol\u00b7chen", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7spr\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schritten die verkl\u00e4rten Dichter oft", "tokens": ["Schrit\u00b7ten", "die", "ver\u00b7kl\u00e4r\u00b7ten", "Dich\u00b7ter", "oft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In des Waldes unbetretnen Schl\u00e4gen,", "tokens": ["In", "des", "Wal\u00b7des", "un\u00b7be\u00b7tret\u00b7nen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis es dunkel wurde unverhofft.", "tokens": ["Bis", "es", "dun\u00b7kel", "wur\u00b7de", "un\u00b7ver\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und die weltber\u00fchmtesten der Verse", "tokens": ["Und", "die", "welt\u00b7be\u00b7r\u00fchm\u00b7tes\u00b7ten", "der", "Ver\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Machten miteinander unterwegs", "tokens": ["Mach\u00b7ten", "mi\u00b7tein\u00b7an\u00b7der", "un\u00b7ter\u00b7wegs"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "So der Dichter Tell's und der des Lerse,", "tokens": ["So", "der", "Dich\u00b7ter", "Tell's", "und", "der", "des", "Ler\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "KON", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Eingedenk des gro\u00dfen K\u00fcnstlerzwecks.", "tokens": ["Ein\u00b7ge\u00b7denk", "des", "gro\u00b7\u00dfen", "K\u00fcnst\u00b7ler\u00b7zwecks", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Zum Exempel jene Prachtballaden", "tokens": ["Zum", "Ex\u00b7em\u00b7pel", "je\u00b7ne", "Pracht\u00b7bal\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PDAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von dem frommen Knechte Fridolin,", "tokens": ["Von", "dem", "from\u00b7men", "Knech\u00b7te", "Fri\u00b7do\u00b7lin", ","], "token_info": ["wo