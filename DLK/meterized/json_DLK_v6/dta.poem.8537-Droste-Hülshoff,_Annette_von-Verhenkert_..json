{"dta.poem.8537": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Verhenkert .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie du gehst und wie du stehst,", "tokens": ["Wie", "du", "gehst", "und", "wie", "du", "stehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was du sprichst und beginnest,", "tokens": ["Und", "was", "du", "sprichst", "und", "be\u00b7gin\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gift\u2019ge Pfeile die Worte sind,", "tokens": ["Gift'\u00b7ge", "Pfei\u00b7le", "die", "Wor\u00b7te", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wie Nattern deine Geberden,", "tokens": ["Wie", "Nat\u00b7tern", "dei\u00b7ne", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.5": {"text": "An dem Pfahle, da ist dein Platz,", "tokens": ["An", "dem", "Pfah\u00b7le", ",", "da", "ist", "dein", "Platz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und auf der luftigen Spindel,", "tokens": ["Und", "auf", "der", "luf\u00b7ti\u00b7gen", "Spin\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wo der Rabe dich gr\u00fc\u00dfen mag,", "tokens": ["Wo", "der", "Ra\u00b7be", "dich", "gr\u00fc\u00b7\u00dfen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Der unges\u00e4ttigte Vogel.", "tokens": ["Der", "un\u00b7ge\u00b7s\u00e4t\u00b7tig\u00b7te", "Vo\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}