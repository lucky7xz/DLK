{"textgrid.poem.49943": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf die N\u00fcrtinger Schule", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Genius hast du der Welt in Schelling erzogen;", "tokens": ["Ei\u00b7nen", "Ge\u00b7nius", "hast", "du", "der", "Welt", "in", "Schel\u00b7ling", "er\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dessen ber\u00fchmest du dich, wackere Schule, mit Recht.", "tokens": ["Des\u00b7sen", "be\u00b7r\u00fch\u00b7mest", "du", "dich", ",", "wa\u00b7cke\u00b7re", "Schu\u00b7le", ",", "mit", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "H\u00e4tte dir Schwaben nur mehr von solcherlei Samen zu senden,", "tokens": ["H\u00e4t\u00b7te", "dir", "Schwa\u00b7ben", "nur", "mehr", "von", "sol\u00b7cher\u00b7lei", "Sa\u00b7men", "zu", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "ADV", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nicht am G\u00e4rtner f\u00fcrwahr,da\u00df er dir bl\u00fchte, gebricht's.", "tokens": ["Nicht", "am", "G\u00e4rt\u00b7ner", "f\u00fcr\u00b7wahr", ",", "da\u00df", "er", "dir", "bl\u00fch\u00b7te", ",", "ge\u00b7bricht'", "s."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["PTKNEG", "APPRART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "NE"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}}}}