{"textgrid.poem.39918": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Kepplers Adelsbrief", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich ist der Brief gefunden,", "tokens": ["End\u00b7lich", "ist", "der", "Brief", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n von Schreibers Hand gemalt,", "tokens": ["Sch\u00f6n", "von", "Schrei\u00b7bers", "Hand", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Seidenschnur umwunden,", "tokens": ["Von", "der", "Sei\u00b7den\u00b7schnur", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vom Siegel rot bestralt!", "tokens": ["Und", "vom", "Sie\u00b7gel", "rot", "be\u00b7stralt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist du fr\u00f6hlich, alter, grauer", "tokens": ["Bist", "du", "fr\u00f6h\u00b7lich", ",", "al\u00b7ter", ",", "grau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00fcrgermeister Weils der Stadt,", "tokens": ["B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "Weils", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun dein Sohn, obwohl ein Bauer,", "tokens": ["Nun", "dein", "Sohn", ",", "ob\u00b7wohl", "ein", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Wieg' ein Kn\u00e4blein hat?", "tokens": ["In", "der", "Wieg'", "ein", "Kn\u00e4\u00b7blein", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wei\u00dft du doch: des Urahns Gl\u00fccke", "tokens": ["Wei\u00dft", "du", "doch", ":", "des", "Ur\u00b7ahns", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauschte Gru\u00df der Tiberstrom,", "tokens": ["Rauschte", "Gru\u00df", "der", "Ti\u00b7ber\u00b7strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ihn Sigmund auf der Br\u00fccke", "tokens": ["Als", "ihn", "Sig\u00b7mund", "auf", "der", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlug zum Ritter einst in Rom.", "tokens": ["Schlug", "zum", "Rit\u00b7ter", "einst", "in", "Rom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mach' den Enkel nur zum Grafen,", "tokens": ["Mach'", "den", "En\u00b7kel", "nur", "zum", "Gra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt er gleich im Baurenstroh!", "tokens": ["Liegt", "er", "gleich", "im", "Bau\u00b7ren\u00b7stroh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sebald Keppler, geh nun schlafen,", "tokens": ["Se\u00b7bald", "Kepp\u00b7ler", ",", "geh", "nun", "schla\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Adelsbriefes froh.", "tokens": ["Dei\u00b7nes", "A\u00b7dels\u00b7brie\u00b7fes", "froh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Siehst du nicht, wie Sternenschimmer", "tokens": ["Siehst", "du", "nicht", ",", "wie", "Ster\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf das Pergament schon blinkt? \u2013", "tokens": ["Auf", "das", "Per\u00b7ga\u00b7ment", "schon", "blinkt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Alte liest noch immer,", "tokens": ["Doch", "der", "Al\u00b7te", "liest", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Bis das Augenlid ihm sinkt.", "tokens": ["Bis", "das", "Au\u00b7gen\u00b7lid", "ihm", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ei, was wird der Brief im Traume?", "tokens": ["Ei", ",", "was", "wird", "der", "Brief", "im", "Trau\u00b7me", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, er w\u00e4chst zum Firmament,", "tokens": ["Sieh", ",", "er", "w\u00e4chst", "zum", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo auf ungemessnem Raume", "tokens": ["Wo", "auf", "un\u00b7ge\u00b7mess\u00b7nem", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonne statt des Siegels brennt.", "tokens": ["Son\u00b7ne", "statt", "des", "Sie\u00b7gels", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aehnlichs hat er nie gesehen,", "tokens": ["A\u00b7ehn\u00b7lichs", "hat", "er", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O welch wunderbar Gesicht!", "tokens": ["O", "welch", "wun\u00b7der\u00b7bar", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Sonne bleibt ja stehen,", "tokens": ["Die\u00b7se", "Son\u00b7ne", "bleibt", "ja", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Sonne schreitet nicht!", "tokens": ["Die\u00b7se", "Son\u00b7ne", "schrei\u00b7tet", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ohne Scheu, am Tage, treten", "tokens": ["Oh\u00b7ne", "Scheu", ",", "am", "Ta\u00b7ge", ",", "tre\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Statt der Lettern Sterne vor,", "tokens": ["Statt", "der", "Let\u00b7tern", "Ster\u00b7ne", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es kreisen die Planeten", "tokens": ["Und", "es", "krei\u00b7sen", "die", "Pla\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Um das goldne Licht im Chor.", "tokens": ["Um", "das", "gold\u00b7ne", "Licht", "im", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie so s\u00fc\u00df erklingt, wie t\u00f6net", "tokens": ["Wie", "so", "s\u00fc\u00df", "er\u00b7klingt", ",", "wie", "t\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jede Sph\u00e4re dieser Welt!", "tokens": ["Je\u00b7de", "Sph\u00e4\u00b7re", "die\u00b7ser", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Umgekehrt und doch versch\u00f6net", "tokens": ["Um\u00b7ge\u00b7kehrt", "und", "doch", "ver\u00b7sch\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lacht das ganze Himmelszelt!", "tokens": ["Lacht", "das", "gan\u00b7ze", "Him\u00b7mels\u00b7zelt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Endlich sieht er einen braunen", "tokens": ["End\u00b7lich", "sieht", "er", "ei\u00b7nen", "brau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Chorherrn sich im Traume nahn.", "tokens": ["Chor\u00b7herrn", "sich", "im", "Trau\u00b7me", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der spricht freundlich: \u00bbLa\u00df dein Staunen,", "tokens": ["Der", "spricht", "freund\u00b7lich", ":", "\u00bb", "La\u00df", "dein", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$.", "$(", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solches ist der Sterne Bahn!", "tokens": ["Sol\u00b7ches", "ist", "der", "Ster\u00b7ne", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Merke dir den echten Himmel,", "tokens": ["Mer\u00b7ke", "dir", "den", "ech\u00b7ten", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn das ist sein Gu\u00df und Flu\u00df;", "tokens": ["Denn", "das", "ist", "sein", "Gu\u00df", "und", "Flu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ordnung bracht ich in's Gewimmel,", "tokens": ["Ord\u00b7nung", "bracht", "ich", "in's", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00df' ich ja Copernikus!", "tokens": ["Hei\u00df'", "ich", "ja", "Co\u00b7per\u00b7ni\u00b7kus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Doch so sauber in dem Blauen", "tokens": ["Doch", "so", "sau\u00b7ber", "in", "dem", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich einst es nicht entdeckt.", "tokens": ["Hab'", "ich", "einst", "es", "nicht", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mann! dein Enkel, der wird schauen,", "tokens": ["Mann", "!", "dein", "En\u00b7kel", ",", "der", "wird", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mir selber blieb versteckt.", "tokens": ["Was", "mir", "sel\u00b7ber", "blieb", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Siehst du dort den roten flimmern?", "tokens": ["Siehst", "du", "dort", "den", "ro\u00b7ten", "flim\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist Mars, der sichre Stern:", "tokens": ["Das", "ist", "Mars", ",", "der", "sich\u00b7re", "Stern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Dem, von allen Wandelschimmern,", "tokens": ["Dem", ",", "von", "al\u00b7len", "Wan\u00b7del\u00b7schim\u00b7mern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgt er um den Sonnenkern.", "tokens": ["Folgt", "er", "um", "den", "Son\u00b7nen\u00b7kern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und bald mi\u00dft er ihre Bahnen,", "tokens": ["Und", "bald", "mi\u00dft", "er", "ih\u00b7re", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Scharfblick irret nie.", "tokens": ["Und", "sein", "Scharf\u00b7blick", "ir\u00b7ret", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre G\u00e4nge durft' ich ahnen", "tokens": ["Ih\u00b7re", "G\u00e4n\u00b7ge", "durft'", "ich", "ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber Er berechnet sie!", "tokens": ["A\u00b7ber", "Er", "be\u00b7rech\u00b7net", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Drum durchsp\u00e4h' die Himmelsferne,", "tokens": ["Drum", "durch\u00b7sp\u00e4h'", "die", "Him\u00b7mels\u00b7fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Modern la\u00df des Kaisers Wort.", "tokens": ["Mo\u00b7dern", "la\u00df", "des",