{"dta.poem.12077": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Spruch: Erkenne dich! sollst du nicht \u00fcbertreiben;", "tokens": ["Den", "Spruch", ":", "Er\u00b7ken\u00b7ne", "dich", "!", "sollst", "du", "nicht", "\u00fc\u00b7bert\u00b7rei\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df immer unbekannt dir in dir etwas bleiben.", "tokens": ["La\u00df", "im\u00b7mer", "un\u00b7be\u00b7kannt", "dir", "in", "dir", "et\u00b7was", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "PPER", "APPR", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Grund, aus welchem quillt dein Daseyn, mu\u00dft du f\u00fchlen;", "tokens": ["Den", "Grund", ",", "aus", "wel\u00b7chem", "quillt", "dein", "Da\u00b7seyn", ",", "mu\u00dft", "du", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "VVFIN", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zerst\u00f6ren wirst du ihn, wenn du ihn auf willst w\u00fchlen.", "tokens": ["Zer\u00b7st\u00f6\u00b7ren", "wirst", "du", "ihn", ",", "wenn", "du", "ihn", "auf", "willst", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die reine Quelle wird, frech aufgew\u00fchlt, ein Sumpf;", "tokens": ["Die", "rei\u00b7ne", "Quel\u00b7le", "wird", ",", "frech", "auf\u00b7ge\u00b7w\u00fchlt", ",", "ein", "Sumpf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ADJD", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wer sie nicht erkennt, wer sich nicht f\u00fchlt ist dumpf.", "tokens": ["Nicht", "wer", "sie", "nicht", "er\u00b7kennt", ",", "wer", "sich", "nicht", "f\u00fchlt", "ist", "dumpf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "PWS", "PRF", "PTKNEG", "VVFIN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}