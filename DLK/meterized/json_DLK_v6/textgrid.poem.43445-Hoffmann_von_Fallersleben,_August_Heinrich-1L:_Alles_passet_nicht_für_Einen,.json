{"textgrid.poem.43445": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles passet nicht f\u00fcr Einen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles passet nicht f\u00fcr Einen,", "tokens": ["Al\u00b7les", "pas\u00b7set", "nicht", "f\u00fcr", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber Eines pa\u00dft f\u00fcr Jeden,", "tokens": ["A\u00b7ber", "Ei\u00b7nes", "pa\u00dft", "f\u00fcr", "Je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eins mu\u00df euch als Recht erscheinen:", "tokens": ["Eins", "mu\u00df", "euch", "als", "Recht", "er\u00b7schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freies Denken, freies Reden.", "tokens": ["Frei\u00b7es", "Den\u00b7ken", ",", "frei\u00b7es", "Re\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft ihr dieses euch beschr\u00e4nken,", "tokens": ["La\u00dft", "ihr", "die\u00b7ses", "euch", "be\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Habt ihr Alles weggegeben \u2013", "tokens": ["Habt", "ihr", "Al\u00b7les", "weg\u00b7ge\u00b7ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leben ist nur freies Denken,", "tokens": ["Le\u00b7ben", "ist", "nur", "frei\u00b7es", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freies Denken nur ist Leben.", "tokens": ["Frei\u00b7es", "Den\u00b7ken", "nur", "ist", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}