{"textgrid.poem.55889": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied der Witwe", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Anfang war mir das Leben gut.", "tokens": ["Am", "An\u00b7fang", "war", "mir", "das", "Le\u00b7ben", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es hielt mich warm, es machte mir Mut", "tokens": ["Es", "hielt", "mich", "warm", ",", "es", "mach\u00b7te", "mir", "Mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "PPER", "VVFIN", "PPER", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da\u00df es das allen Jungen tut,", "tokens": ["Da\u00df", "es", "das", "al\u00b7len", "Jun\u00b7gen", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie konnt ich das damals wissen.", "tokens": ["wie", "konnt", "ich", "das", "da\u00b7mals", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PDS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich wu\u00dfte nicht, was das Leben war \u2013,", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", ",", "was", "das", "Le\u00b7ben", "war", "\u2013", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "NN", "VAFIN", "$(", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "auf einmal war es nur Jahr und Jahr,", "tokens": ["auf", "ein\u00b7mal", "war", "es", "nur", "Jahr", "und", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "nicht mehr gut, nicht mehr neu, nicht mehr wunderbar,", "tokens": ["nicht", "mehr", "gut", ",", "nicht", "mehr", "neu", ",", "nicht", "mehr", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "PTKNEG", "ADV", "ADJD", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "wie mitten entzwei gerissen.", "tokens": ["wie", "mit\u00b7ten", "ent\u00b7zwei", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKVZ", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das war nicht Seine, nicht meine Schuld;", "tokens": ["Das", "war", "nicht", "Sei\u00b7ne", ",", "nicht", "mei\u00b7ne", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NE", "$,", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wir hatten beide nichts als Geduld,", "tokens": ["wir", "hat\u00b7ten", "bei\u00b7de", "nichts", "als", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "aber der Tod hat keine.", "tokens": ["a\u00b7ber", "der", "Tod", "hat", "kei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIAT", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich sah ihn kommen (wie schlecht er kam),", "tokens": ["Ich", "sah", "ihn", "kom\u00b7men", "(", "wie", "schlecht", "er", "kam", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$(", "PWAV", "ADJD", "PPER", "VVFIN", "$(", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und ich schaute ihm zu wie er nahm und nahm:", "tokens": ["und", "ich", "schau\u00b7te", "ihm", "zu", "wie", "er", "nahm", "und", "nahm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "es war ja gar nicht das Meine.", "tokens": ["es", "war", "ja", "gar", "nicht", "das", "Mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "ART", "PPOSAT", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was war denn das Meine; Meines, Mein?", "tokens": ["Was", "war", "denn", "das", "Mei\u00b7ne", ";", "Mei\u00b7nes", ",", "Mein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "PPOSAT", "$.", "PPOSAT", "$,", "PPOSAT", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "War mir nicht selbst mein Elendsein", "tokens": ["War", "mir", "nicht", "selbst", "mein", "E\u00b7lend\u00b7sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "nur vom Schicksal geliehn?", "tokens": ["nur", "vom", "Schick\u00b7sal", "ge\u00b7liehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das Schicksal will nicht nur das Gl\u00fcck,", "tokens": ["Das", "Schick\u00b7sal", "will", "nicht", "nur", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "es will die Pein und das Schrein zur\u00fcck", "tokens": ["es", "will", "die", "Pein", "und", "das", "Schrein", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "KON", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "und es kauft f\u00fcr alt den Ruin.", "tokens": ["und", "es", "kauft", "f\u00fcr", "alt", "den", "Ru\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Schicksal war da und erwarb f\u00fcr ein Nichts", "tokens": ["Das", "Schick\u00b7sal", "war", "da", "und", "er\u00b7warb", "f\u00fcr", "ein", "Nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "jeden Ausdruck meines Gesichts", "tokens": ["je\u00b7den", "Aus\u00b7druck", "mei\u00b7nes", "Ge\u00b7sichts"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "bis auf die Art zu gehn.", "tokens": ["bis", "auf", "die", "Art", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das war ein t\u00e4glicher Ausverkauf", "tokens": ["Das", "war", "ein", "t\u00e4g\u00b7li\u00b7cher", "Aus\u00b7ver\u00b7kauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und als ich leer war, gab es mich auf", "tokens": ["und", "als", "ich", "leer", "war", ",", "gab", "es", "mich", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PRF", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "und lie\u00df mich offen stehn.", "tokens": ["und", "lie\u00df", "mich", "of\u00b7fen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}