{"textgrid.poem.41277": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Eine, vor dem Jahre 1732, seltene Sache", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es herrschet \u00fcberall ein d\u00fcrft'ger, stolzer Neid,", "tokens": ["Es", "herr\u00b7schet", "\u00fc\u00b7be\u00b7rall", "ein", "d\u00fcrft'\u00b7ger", ",", "stol\u00b7zer", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das l\u00e4cherlichste Loos der l\u00e4cherlichen Zeit,", "tokens": ["Das", "l\u00e4\u00b7cher\u00b7lichs\u00b7te", "Loos", "der", "l\u00e4\u00b7cher\u00b7li\u00b7chen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ob das gro\u00dfe Gut, Unsterblichkeit und Ehre,", "tokens": ["Als", "ob", "das", "gro\u00b7\u00dfe", "Gut", ",", "U\u00b7nsterb\u00b7lich\u00b7keit", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur Eines Eigenthum, und nicht zu theilen w\u00e4re.", "tokens": ["Nur", "Ei\u00b7nes", "Ei\u00b7gen\u00b7thum", ",", "und", "nicht", "zu", "thei\u00b7len", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KON", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch, wo regieret mehr Parteilichkeit und Ha\u00df,", "tokens": ["Doch", ",", "wo", "re\u00b7gie\u00b7ret", "mehr", "Par\u00b7tei\u00b7lich\u00b7keit", "und", "Ha\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als auf dem heutigen Parna\u00df?", "tokens": ["Als", "auf", "dem", "heu\u00b7ti\u00b7gen", "Par\u00b7na\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Viel eher findet man bei so verg\u00e4llten Trieben", "tokens": ["Viel", "e\u00b7her", "fin\u00b7det", "man", "bei", "so", "ver\u00b7g\u00e4ll\u00b7ten", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drei Helden, die sich gern in gleicher Gr\u00f6\u00dfe sehn,", "tokens": ["Drei", "Hel\u00b7den", ",", "die", "sich", "gern", "in", "glei\u00b7cher", "Gr\u00f6\u00b7\u00dfe", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drei Sch\u00f6ne, die sich nie, aus Mi\u00dfgunst, hintergehn,", "tokens": ["Drei", "Sch\u00f6\u00b7ne", ",", "die", "sich", "nie", ",", "aus", "Mi\u00df\u00b7gunst", ",", "hin\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PRF", "ADV", "$,", "APPR", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als zween Dichter, die sich lieben.", "tokens": ["Als", "zween", "Dich\u00b7ter", ",", "die", "sich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}