{"textgrid.poem.67832": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4. Der ungl\u00fcckliche Weidenbaum", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, mein Pferd, mein Pferdchen,", "tokens": ["Ei", ",", "mein", "Pferd", ",", "mein", "Pferd\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Du, mein lieber Brauner,", "tokens": ["Du", ",", "mein", "lie\u00b7ber", "Brau\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Du, warum nicht fressen", "tokens": ["Du", ",", "wa\u00b7rum", "nicht", "fres\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PWAV", "PTKNEG", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Reinen, sch\u00f6nen Haber?", "tokens": ["Rei\u00b7nen", ",", "sch\u00f6\u00b7nen", "Ha\u00b7ber", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wird dir wohl zu sauer", "tokens": ["Wird", "dir", "wohl", "zu", "sau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKA", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Diese weite Reise,", "tokens": ["Die\u00b7se", "wei\u00b7te", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Diese weite Reise,", "tokens": ["Die\u00b7se", "wei\u00b7te", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zweimalhundert Meilen?", "tokens": ["Zwei\u00b7mal\u00b7hun\u00b7dert", "Mei\u00b7len", "?"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Neun Gew\u00e4sser sind wir", "tokens": ["Neun", "Ge\u00b7w\u00e4s\u00b7ser", "sind", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schwimmend durchgeschwommen;", "tokens": ["Schwim\u00b7mend", "durch\u00b7ge\u00b7schwom\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Noch in diesen zehnten", "tokens": ["Noch", "in", "die\u00b7sen", "zehn\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "La\u00df hinein uns tauchen!", "tokens": ["La\u00df", "hin\u00b7ein", "uns", "tau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Pferdchen schwamm ans Ufer,", "tokens": ["Pferd\u00b7chen", "schwamm", "ans", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bruderchen sank unter,", "tokens": ["Bru\u00b7der\u00b7chen", "sank", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Bruder hielt im sinken", "tokens": ["Bru\u00b7der", "hielt", "im", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Einen Weidbaum feste.", "tokens": ["Ei\u00b7nen", "Weid\u00b7baum", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ei du Weidbaum, Weidbaum,", "tokens": ["Ei", "du", "Weid\u00b7baum", ",", "Weid\u00b7baum", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Stehst du noch und gr\u00fcnest?", "tokens": ["Stehst", "du", "noch", "und", "gr\u00fc\u00b7nest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sollst nicht l\u00e4nger gr\u00fcnen", "tokens": ["Sollst", "nicht", "l\u00e4n\u00b7ger", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Als den Sommer \u00fcber.", "tokens": ["Als", "den", "Som\u00b7mer", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ja, ich will dich f\u00e4llen,", "tokens": ["Ja", ",", "ich", "will", "dich", "f\u00e4l\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Deine Zweige kappen,", "tokens": ["Dei\u00b7ne", "Zwei\u00b7ge", "kap\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Will aus deinem Stamme", "tokens": ["Will", "aus", "dei\u00b7nem", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bretter schneiden lassen,", "tokens": ["Bret\u00b7ter", "schnei\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kleine weisse Bretter.", "tokens": ["Klei\u00b7ne", "weis\u00b7se", "Bret\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Davon will ich bauen", "tokens": ["Da\u00b7von", "will", "ich", "bau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "VVINF"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.2": {"text": "Kleine weisse Wiege", "tokens": ["Klei\u00b7ne", "weis\u00b7se", "Wie\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fcr mein junges M\u00e4dchen;", "tokens": ["F\u00fcr", "mein", "jun\u00b7ges", "M\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und aus deinen Aesten", "tokens": ["Und", "aus", "dei\u00b7nen", "A\u00b7es\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Will ich diehlen lassen", "tokens": ["Will", "ich", "dieh\u00b7len", "las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Meiner Pferde Schauer.", "tokens": ["Mei\u00b7ner", "Pfer\u00b7de", "Schau\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}