{"textgrid.poem.24012": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ein Menuett", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nestwarmweiche Lagerst\u00e4tte,", "tokens": ["Nest\u00b7warm\u00b7wei\u00b7che", "La\u00b7ger\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelblaues Himmelbette,", "tokens": ["Him\u00b7mel\u00b7blau\u00b7es", "Him\u00b7mel\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seidenkissen, Spitzenzier,", "tokens": ["Sei\u00b7den\u00b7kis\u00b7sen", ",", "Spit\u00b7zen\u00b7zier", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosawolken, mullgebauschte,", "tokens": ["Ro\u00b7sa\u00b7wol\u00b7ken", ",", "mull\u00b7ge\u00b7bauschte", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hinter denen Amor lauschte,", "tokens": ["Hin\u00b7ter", "de\u00b7nen", "A\u00b7mor", "lauschte", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsrer Liebe, dir und mir,", "tokens": ["Uns\u00b7rer", "Lie\u00b7be", ",", "dir", "und", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kr\u00e4uselte der Tapezier.", "tokens": ["Kr\u00e4u\u00b7sel\u00b7te", "der", "Ta\u00b7pe\u00b7zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Aus der Ampel quillt in hellen", "tokens": ["Aus", "der", "Am\u00b7pel", "quillt", "in", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgenr\u00f6tenrosenwellen", "tokens": ["Mor\u00b7gen\u00b7r\u00f6\u00b7ten\u00b7ro\u00b7sen\u00b7wel\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmeichelweiches Liebeslicht.", "tokens": ["Schmei\u00b7chel\u00b7wei\u00b7ches", "Lie\u00b7bes\u00b7licht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie in einem Rosenhaine,", "tokens": ["Wie", "in", "ei\u00b7nem", "Ro\u00b7sen\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rose selber, ruht die Meine,", "tokens": ["Ro\u00b7se", "sel\u00b7ber", ",", "ruht", "die", "Mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und von Rosen ein Gedicht", "tokens": ["Und", "von", "Ro\u00b7sen", "ein", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihres Busens Heben spricht.", "tokens": ["Ih\u00b7res", "Bu\u00b7sens", "He\u00b7ben", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leise, leise, ihren roten", "tokens": ["Lei\u00b7se", ",", "lei\u00b7se", ",", "ih\u00b7ren", "ro\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lippen Morgengru\u00df geboten.", "tokens": ["Lip\u00b7pen", "Mor\u00b7gen\u00b7gru\u00df", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Augen auf. Bon jour Madam'!", "tokens": ["Au\u00b7gen", "auf", ".", "Bon", "jour", "Ma\u00b7dam'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "NN", "NE", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Zweier Sonnen hell Erwachen,", "tokens": ["Zwei\u00b7er", "Son\u00b7nen", "hell", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zweier Sonnen selig Lachen ...", "tokens": ["Zwei\u00b7er", "Son\u00b7nen", "se\u00b7lig", "La\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ich in den Arm sie nahm,", "tokens": ["Als", "ich", "in", "den", "Arm", "sie", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Amor aus der Wolke kam.", "tokens": ["A\u00b7mor", "aus", "der", "Wol\u00b7ke", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}