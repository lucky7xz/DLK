{"dta.poem.18990": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "17.  \n  Schaiden vnd Lieb vnsterblich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.85", "af:0.14"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Mv\u00df es geschaiden sein? Ist dises dan die stund/", "tokens": ["Mv\u00df", "es", "ge\u00b7schai\u00b7den", "sein", "?", "Ist", "di\u00b7ses", "dan", "die", "stund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "VAINF", "$.", "VAFIN", "PDS", "ADV", "ART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die stund/ ach nein/ die wund/ die vns will haben", "tokens": ["Die", "stund", "/", "ach", "nein", "/", "die", "wund", "/", "die", "vns", "will", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "XY", "PTKANT", "$(", "ART", "NN", "$(", "PRELS", "PPER", "VMFIN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schaiden?", "tokens": ["schai\u00b7den", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wie! schaiden mu\u00df ich dan? Ach nein/ Ich mu\u00df", "tokens": ["Wie", "!", "schai\u00b7den", "mu\u00df", "ich", "dan", "?", "Ach", "nein", "/", "Ich", "mu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$.", "VVINF", "VMFIN", "PPER", "ADV", "$.", "NN", "PTKANT", "$(", "PPER", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "verschaiden/", "tokens": ["ver\u00b7schai\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Dan ja zu gro\u00df mein schmertz/ vnd zu tieff meine", "tokens": ["Dan", "ja", "zu", "gro\u00df", "mein", "schmertz", "/", "vnd", "zu", "tieff", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKA", "ADJD", "PPOSAT", "NN", "$(", "KON", "PTKA", "ADJD", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "wund.", "tokens": ["wund", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Zwar nicht mein aigen laid/ sondern/ mein Rosen-", "tokens": ["Zwar", "nicht", "mein", "ai\u00b7gen", "laid", "/", "son\u00b7dern", "/", "mein", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PTKNEG", "PPOSAT", "ADJA", "NN", "$(", "KON", "$(", "PPOSAT", "TRUNC"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "mund", "tokens": ["mund"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "(mund/ dessen s\u00fcsse k\u00fc\u00df mein hertz gantz g\u00f6tlich", "tokens": ["(", "mund", "/", "des\u00b7sen", "s\u00fcs\u00b7se", "k\u00fc\u00df", "mein", "hertz", "gantz", "g\u00f6t\u00b7lich"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "$(", "PDS", "VVFIN", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "waiden)", "tokens": ["wai\u00b7den", ")"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Dein seuftzen/ weinen/ klag mich zu dem tod be-", "tokens": ["Dein", "seuft\u00b7zen", "/", "wei\u00b7nen", "/", "klag", "mich", "zu", "dem", "tod", "be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VVINF", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schaiden/", "tokens": ["schai\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Vnd machen deinen tod mir/ meinen tod dir/ k\u0169d.", "tokens": ["Vnd", "ma\u00b7chen", "dei\u00b7nen", "tod", "mir", "/", "mei\u00b7nen", "tod", "dir", "/", "k\u0169d", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "$(", "PPOSAT", "NN", "PPER", "$(", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So la\u00df mich nu von dir/ thu du von mir/ empfangen", "tokens": ["So", "la\u00df", "mich", "nu", "von", "dir", "/", "thu", "du", "von", "mir", "/", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPER", "$(", "VVFIN", "PPER", "APPR", "PPER", "$(", "VVPP"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den letzten letzin-ku\u00df. O s\u00fcsser tod! Ach nein/", "tokens": ["Den", "letz\u00b7ten", "let\u00b7zin\u00b7ku\u00df", ".", "O", "s\u00fcs\u00b7ser", "tod", "!", "Ach", "nein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "ADJA", "NN", "$.", "NN", "PTKANT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O newe lebens-krafft/ die wir zu gleich erlangen!", "tokens": ["O", "ne\u00b7we", "le\u00b7bens\u00b7krafft", "/", "die", "wir", "zu", "gleich", "er\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "PRELS", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Dan meine sehl in dich/ in mich dein sehlelein", "tokens": ["Dan", "mei\u00b7ne", "sehl", "in", "dich", "/", "in", "mich", "dein", "seh\u00b7lel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER", "$(", "APPR", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(verwech\u00dflend) haben sich durch disen ku\u00df ver-", "tokens": ["(", "ver\u00b7wech\u00df\u00b7lend", ")", "ha\u00b7ben", "sich", "durch", "di\u00b7sen", "ku\u00df", "ver"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "$(", "VAFIN", "PRF", "APPR", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "gangen/", "tokens": ["gan\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Da\u00df vnser tod vnd lieb nu mu\u00df vnsterblich sein.", "tokens": ["Da\u00df", "vn\u00b7ser", "tod", "vnd", "lieb", "nu", "mu\u00df", "vns\u00b7terb\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "ADJD", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}