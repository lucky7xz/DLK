{"textgrid.poem.52369": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "19. Snider-Begn\u00e4ugen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dor was mal eins en l\u00fctten Mann,", "tokens": ["Dor", "was", "mal", "eins", "en", "l\u00fct\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PIS", "VVINF", "VVFIN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hadd Hosen an,", "tokens": ["Hadd", "Ho\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hadd kunterbunte Hosen an,", "tokens": ["Hadd", "kun\u00b7ter\u00b7bun\u00b7te", "Ho\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "En fipprig R\u00f6ckschen un so wider,", "tokens": ["En", "fipp\u00b7rig", "R\u00f6ck\u00b7schen", "un", "so", "wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Un was von Profe\u00dfschon en Snider,", "tokens": ["Un", "was", "von", "Pro\u00b7fe\u00df\u00b7schon", "en", "Sni\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NE", "NE", "NE", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Un sporsam was hei hellschen.", "tokens": ["Un", "spor\u00b7sam", "was", "hei", "hell\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "De seggt tau sinen Jungen: \u00bbHal", "tokens": ["De", "seggt", "tau", "si\u00b7nen", "Jun\u00b7gen", ":", "\u00bb", "Hal"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Uns doch enmal", "tokens": ["Uns", "doch", "en\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ADV"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Den Hiringsschwanz von'n B\u00e4n hendal,", "tokens": ["Den", "Hi\u00b7rings\u00b7schwanz", "von'n", "B\u00e4n", "hen\u00b7dal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00f6r mi en Finzel, di en Finzel,", "tokens": ["F\u00f6r", "mi", "en", "Fin\u00b7zel", ",", "di", "en", "Fin\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$,", "FM", "FM", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Un mine Fru hal ok en Finzel,", "tokens": ["Un", "mi\u00b7ne", "Fru", "hal", "ok", "en", "Fin\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Un'n Finzel, den lat liggen.\u00ab", "tokens": ["Un'n", "Fin\u00b7zel", ",", "den", "lat", "lig\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "De drei, de sitten \u00fcm den Disch.", "tokens": ["De", "drei", ",", "de", "sit\u00b7ten", "\u00fcm", "den", "Disch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "CARD", "$,", "FM", "FM", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Jung' will frisch", "tokens": ["De", "Jung'", "will", "frisch"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "VMFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Inhauen up sin Finzel Fisch,", "tokens": ["In\u00b7hau\u00b7en", "up", "sin", "Fin\u00b7zel", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dunn ritt de Meister mit de Gabel", "tokens": ["Dunn", "ritt", "de", "Meis\u00b7ter", "mit", "de", "Ga\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "NE", "APPR", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "De H\u00e4lft em weg v\u00f6r sinen Snabel.", "tokens": ["De", "H\u00e4lft", "em", "weg", "v\u00f6r", "si\u00b7nen", "Sna\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bb't k\u00fcnn up de Nacht di schaden!", "tokens": ["\u00bb", "'t", "k\u00fcnn", "up", "de", "Nacht", "di", "scha\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "NN", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du frettst di ganz ut Rick und Schick,", "tokens": ["Du", "frettst", "di", "ganz", "ut", "Rick", "und", "Schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wardst tau dick.\u00ab", "tokens": ["Du", "wardst", "tau", "dick", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Fru Meistern nimmt dat anner St\u00fcck.", "tokens": ["Fru", "Meis\u00b7tern", "nimmt", "dat", "an\u00b7ner", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbdu frettst di noch ut Rand un Band,", "tokens": ["\u00bb", "du", "frettst", "di", "noch", "ut", "Rand", "un", "Band", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NE", "ADV", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bringst Hungersnot noch in dat Land.", "tokens": ["Bringst", "Hun\u00b7gers\u00b7not", "noch", "in", "dat", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wi will'n kein Fettswin m\u00e4sten.\u00ab", "tokens": ["Wi", "will'n", "kein", "Fetts\u00b7win", "m\u00e4s\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PIAT", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "De Jung' steiht trurig up un sch\u00fcwwt", "tokens": ["De", "Jung'", "steiht", "tru\u00b7rig", "up", "un", "sch\u00fcwwt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADJD", "FM", "FM", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit eine T\u00fcft", "tokens": ["Mit", "ei\u00b7ne", "T\u00fcft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nah'n B\u00e4n herup un sitt un riwwt", "tokens": ["Nah'n", "B\u00e4n", "he\u00b7rup", "un", "sitt", "un", "riwwt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An't Schapp, wo noch de Finzel steiht,", "tokens": ["An't", "Schapp", ",", "wo", "noch", "de", "Fin\u00b7zel", "steiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "ADV", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit sine T\u00fcft. \u00bbWer weit? Wer weit?", "tokens": ["Mit", "si\u00b7ne", "T\u00fcft", ".", "\u00bb", "Wer", "weit", "?", "Wer", "weit", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "PWS", "ADJD", "$.", "PWS", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sei k\u00fcnn doch dornah smecken.\u00ab", "tokens": ["Sei", "k\u00fcnn", "doch", "dor\u00b7nah", "sme\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}