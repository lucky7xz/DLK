{"textgrid.poem.56253": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Josua", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Juchhei! das war ein Schlagen,", "tokens": ["Juch\u00b7hei", "!", "das", "war", "ein", "Schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Schlachten bei Gibeon;", "tokens": ["Ein", "Schlach\u00b7ten", "bei", "Gi\u00b7be\u00b7on", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Tag gebrach den W\u00fcrgern,", "tokens": ["Der", "Tag", "ge\u00b7brach", "den", "W\u00fcr\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es neigte die Sonne sich schon.", "tokens": ["Es", "neig\u00b7te", "die", "Son\u00b7ne", "sich", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sprach Josua zur Sonne:", "tokens": ["Sprach", "Jo\u00b7sua", "zur", "Son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u00bbdu, steh am Himmel fest!\u00ab", "tokens": ["\u00bb", "du", ",", "steh", "am", "Him\u00b7mel", "fest", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie stand, da gab er gem\u00e4chlich", "tokens": ["Sie", "stand", ",", "da", "gab", "er", "ge\u00b7m\u00e4ch\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den \u00dcberwundnen den Rest.", "tokens": ["Den", "\u00dc\u00b7berw\u00b7und\u00b7nen", "den", "Rest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Das war ein Tag der Frommen,", "tokens": ["Das", "war", "ein", "Tag", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie nie ein andrer getagt,", "tokens": ["Wie", "nie", "ein", "an\u00b7drer", "ge\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie nie ein andrer wird tagen,", "tokens": ["Wie", "nie", "ein", "an\u00b7drer", "wird", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "VAFIN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das wird ausdr\u00fccklich gesagt.", "tokens": ["Das", "wird", "aus\u00b7dr\u00fcck\u00b7lich", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Das war ein feines Kunstst\u00fcck,", "tokens": ["Das", "war", "ein", "fei\u00b7nes", "Kunst\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie mancher erachten mag,", "tokens": ["Wie", "man\u00b7cher", "e\u00b7rach\u00b7ten", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der wohl die Nacht uns w\u00fcnschte", "tokens": ["Der", "wohl", "die", "Nacht", "uns", "w\u00fcnschte"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu jenem unendlichen Tag.", "tokens": ["Zu", "je\u00b7nem", "un\u00b7end\u00b7li\u00b7chen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Sie beten und schimpfen und sch\u00f6pfen", "tokens": ["Sie", "be\u00b7ten", "und", "schimp\u00b7fen", "und", "sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In S\u00e4cke das Sonnenlicht,", "tokens": ["In", "S\u00e4\u00b7cke", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es tief in das Meer zu versenken \u2013", "tokens": ["Es", "tief", "in", "das", "Meer", "zu", "ver\u00b7sen\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Tag verdunkeln sie nicht.", "tokens": ["Den", "Tag", "ver\u00b7dun\u00b7keln", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "La\u00dft dieses nicht euch k\u00fcmmern,", "tokens": ["La\u00dft", "die\u00b7ses", "nicht", "euch", "k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "PTKNEG", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Welt ist kugelrund,", "tokens": ["Die", "Welt", "ist", "ku\u00b7gel\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rollt von Westen gen Osten", "tokens": ["Und", "rollt", "von", "Wes\u00b7ten", "gen", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verst\u00e4ndig zu aller Stund.", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", "zu", "al\u00b7ler", "Stund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und der das Lied euch gesungen,", "tokens": ["Und", "der", "das", "Lied", "euch", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat auch die Welt sich beschaut;", "tokens": ["Hat", "auch", "die", "Welt", "sich", "be\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PRF", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Er hat bei den Wilden gehauset,", "tokens": ["Er", "hat", "bei", "den", "Wil\u00b7den", "ge\u00b7hau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und sich mit ihnen erbaut.", "tokens": ["Und", "sich", "mit", "ih\u00b7nen", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}