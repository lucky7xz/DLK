{"textgrid.poem.40104": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da steht, da steht sie im Gewimmel!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da steht, da steht sie im Gewimmel!", "tokens": ["Da", "steht", ",", "da", "steht", "sie", "im", "Ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "an ihrem Busen in der Rechten,", "tokens": ["an", "ih\u00b7rem", "Bu\u00b7sen", "in", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wie Nachtgew\u00f6lke ruhn am Himmel,", "tokens": ["wie", "Nacht\u00b7ge\u00b7w\u00f6l\u00b7ke", "ruhn", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die aufgerafften dunklen Flechten, \u2013", "tokens": ["die", "auf\u00b7ge\u00b7raff\u00b7ten", "dunk\u00b7len", "Flech\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "umstricken meinen Blick wie Schlangen,", "tokens": ["um\u00b7stri\u00b7cken", "mei\u00b7nen", "Blick", "wie", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mir tr\u00e4umt von Paradiesesn\u00e4chten ...", "tokens": ["mir", "tr\u00e4umt", "von", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7n\u00e4ch\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was schl\u00e4gst du pl\u00f6tzlich so voll Bangen", "tokens": ["Was", "schl\u00e4gst", "du", "pl\u00f6tz\u00b7lich", "so", "voll", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den Mantel, Weib, um deine Flechten?!", "tokens": ["den", "Man\u00b7tel", ",", "Weib", ",", "um", "dei\u00b7ne", "Flech\u00b7ten", "?!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}