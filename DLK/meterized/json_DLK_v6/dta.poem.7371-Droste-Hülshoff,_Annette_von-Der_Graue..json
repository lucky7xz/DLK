{"dta.poem.7371": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Graue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Er war tief in die Nacht hinein,", "tokens": ["Er", "war", "tief", "in", "die", "Nacht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drau\u00dfen heulte noch der Sturm,", "tokens": ["Und", "drau\u00b7\u00dfen", "heul\u00b7te", "noch", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schnob zischend an dem Fensterstein", "tokens": ["Schnob", "zi\u00b7schend", "an", "dem", "Fens\u00b7ter\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und drillt den Glockenstrang am Thurm.", "tokens": ["Und", "drillt", "den", "Glo\u00b7cken\u00b7strang", "am", "Thurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In seinem Bette Waller lag,", "tokens": ["In", "sei\u00b7nem", "Bet\u00b7te", "Wal\u00b7ler", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und las so scharf im Ivanhoe,", "tokens": ["Und", "las", "so", "scharf", "im", "I\u00b7van\u00b7hoe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df man gedacht, bevor es Tag", "tokens": ["Da\u00df", "man", "ge\u00b7dacht", ",", "be\u00b7vor", "es", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVPP", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sey Englands K\u00f6nigreich in Ruh.", "tokens": ["Sey", "En\u00b7glands", "K\u00f6\u00b7nig\u00b7reich", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er sah nicht, da\u00df die Kerze tief", "tokens": ["Er", "sah", "nicht", ",", "da\u00df", "die", "Ker\u00b7ze", "tief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich brannte in der Flasche Rand,", "tokens": ["Sich", "brann\u00b7te", "in", "der", "Fla\u00b7sche", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Talg in schweren Tropfen lief,", "tokens": ["Der", "Talg", "in", "schwe\u00b7ren", "Trop\u00b7fen", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und drunten eine Lache stand.", "tokens": ["Und", "drun\u00b7ten", "ei\u00b7ne", "La\u00b7che", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie tr\u00e4umend h\u00f6rt' er das Geknarr", "tokens": ["Wie", "tr\u00e4u\u00b7mend", "h\u00f6rt'", "er", "das", "Ge\u00b7knarr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fenster, vom Rouleau ged\u00e4mpft,", "tokens": ["Der", "Fens\u00b7ter", ",", "vom", "Rou\u00b7le\u00b7au", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und wie die Th\u00fcre mit Geschnarr", "tokens": ["Und", "wie", "die", "Th\u00fc\u00b7re", "mit", "Ge\u00b7schnarr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In ihren Angeln zuckt und k\u00e4mpft.", "tokens": ["In", "ih\u00b7ren", "An\u00b7geln", "zuckt", "und", "k\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sehr freut er sich am Bruder Tuck,", "tokens": ["Sehr", "freut", "er", "sich", "am", "Bru\u00b7der", "Tuck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2014 Die Sehne schwirrt, es rauscht der Hain \u2014", "tokens": ["Die", "Seh\u00b7ne", "schwirrt", ",", "es", "rauscht", "der", "Hain"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da pl\u00f6tzlich ein gewalt'ger Ruck,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "ein", "ge\u00b7walt'\u00b7ger", "Ruck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und, hui! die Scheibe klirrt hinein.", "tokens": ["Und", ",", "hui", "!", "die", "Schei\u00b7be", "klirrt", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM.la", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er fuhr empor, \u2014 weg war der Traum \u2014", "tokens": ["Er", "fuhr", "em\u00b7por", ",", "weg", "war", "der", "Traum"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "$(", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und deckte mit der Hand das Licht,", "tokens": ["Und", "deck\u00b7te", "mit", "der", "Hand", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ha! wie so w\u00fcst des Zimmers Raum!", "tokens": ["Ha", "!", "wie", "so", "w\u00fcst", "des", "Zim\u00b7mers", "Raum", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Selbst ein romantisches Gedicht!", "tokens": ["Selbst", "ein", "ro\u00b7man\u00b7ti\u00b7sches", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Der Sessel feudalistisch Gold \u2014", "tokens": ["Der", "Ses\u00b7sel", "feu\u00b7da\u00b7lis\u00b7tisch", "Gold"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Marmortisch die Greifenklau' \u2014", "tokens": ["Am", "Mar\u00b7mor\u00b7tisch", "die", "Grei\u00b7fen\u00b7klau'"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber'm Spiegel flatternd rollt,", "tokens": ["Und", "\u00fc\u00b7ber'm", "Spie\u00b7gel", "flat\u00b7ternd", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Banner, der Tapete Blau,", "tokens": ["Ein", "Ban\u00b7ner", ",", "der", "Ta\u00b7pe\u00b7te", "Blau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Zug der durch die L\u00fccke schnaubt;", "tokens": ["Im", "Zug", "der", "durch", "die", "L\u00fc\u00b7cke", "schnaubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ahnenbilder leben fast,", "tokens": ["Die", "Ah\u00b7nen\u00b7bil\u00b7der", "le\u00b7ben", "fast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sch\u00fctteln ihr behelmtes Haupt", "tokens": ["Und", "sch\u00fct\u00b7teln", "ihr", "be\u00b7helm\u00b7tes", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ergrimmt ob dem plebejen Gast.", "tokens": ["Er\u00b7grimmt", "ob", "dem", "ple\u00b7be\u00b7jen", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Der blonde Waller machte gern", "tokens": ["Der", "blon\u00b7de", "Wal\u00b7ler", "mach\u00b7te", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich selber einen kleinen Graus,", "tokens": ["Sich", "sel\u00b7ber", "ei\u00b7nen", "klei\u00b7nen", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So nickt er sp\u00f6ttisch gen die Herrn,", "tokens": ["So", "nickt", "er", "sp\u00f6t\u00b7tisch", "gen", "die", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als fordert' er sie keck heraus.", "tokens": ["Als", "for\u00b7dert'", "er", "sie", "keck", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Glocke summt \u2014 schon Eins f\u00fcrwahr!", "tokens": ["Die", "Glo\u00b7cke", "summt", "schon", "Eins", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie eine Boa dehnt' er sich,", "tokens": ["Wie", "ei\u00b7ne", "Boa", "dehnt'", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Und sah nach dem Pistolenpaar,", "tokens": ["Und", "sah", "nach", "dem", "Pis\u00b7to\u00b7len\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann r\u00fcstet er zum Schlafe sich.", "tokens": ["Dann", "r\u00fcs\u00b7tet", "er", "zum", "Schla\u00b7fe", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Flasche hob er einmal noch", "tokens": ["Die", "Fla\u00b7sche", "hob", "er", "ein\u00b7mal", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leuchtete die W\u00e4nde an,", "tokens": ["Und", "leuch\u00b7te\u00b7te", "die", "W\u00e4n\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz wie 'ne alte Halle doch", "tokens": ["Ganz", "wie", "'ne", "al\u00b7te", "Hal\u00b7le", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus einem Scottischen Roman!", "tokens": ["Aus", "ei\u00b7nem", "Scot\u00b7ti\u00b7schen", "Ro\u00b7man", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und \u2014 ist das Nebel oder Rauch,", "tokens": ["Und", "ist", "das", "Ne\u00b7bel", "o\u00b7der", "Rauch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was durch der Th\u00fcre Spalten quillt,", "tokens": ["Was", "durch", "der", "Th\u00fc\u00b7re", "Spal\u00b7ten", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, wirbelnd in des Zuges Hauch,", "tokens": ["Und", ",", "wir\u00b7belnd", "in", "des", "Zu\u00b7ges", "Hauch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die dunstigen Paneele f\u00fcllt?", "tokens": ["Die", "duns\u00b7ti\u00b7gen", "Pa\u00b7nee\u00b7le", "f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Ding \u2014 ein Ding \u2014 wie Grau in Grau,", "tokens": ["Ein", "Ding", "ein", "Ding", "wie", "Grau", "in", "Grau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "PWAV", "ADJD", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Formen schwanken \u2014 sonderbar! \u2014", "tokens": ["Die", "For\u00b7men", "schwan\u00b7ken", "son\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, ob der Blick sich sch\u00e4rft? den Bau", "tokens": ["Doch", ",", "ob", "der", "Blick", "sich", "sch\u00e4rft", "?", "den", "Bau"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Gliedern nimmt er m\u00e4hlig wahr.", "tokens": ["Von", "Glie\u00b7dern", "nimmt", "er", "m\u00e4h\u00b7lig", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie \u00fcber'm Eisenhammer, schwer", "tokens": ["Wie", "\u00fc\u00b7ber'm", "Ei\u00b7sen\u00b7ham\u00b7mer", ",", "schwer"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "APPRART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schwarz, des Rauches S\u00e4ule wallt;", "tokens": ["Und", "schwarz", ",", "des", "Rau\u00b7ches", "S\u00e4u\u00b7le", "wallt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Zucken flattert dr\u00fcben her,", "tokens": ["Ein", "Zu\u00b7cken", "flat\u00b7tert", "dr\u00fc\u00b7ben", "her", ","], "token_info": ["wor