{"dta.poem.22359": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "162. Hinein kehr deine Strahlen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ach kehrt nur meine Seel jhr Flammen umb und ein!", "tokens": ["Ach", "kehrt", "nur", "mei\u00b7ne", "Seel", "jhr", "Flam\u00b7men", "umb", "und", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird sie mit dem Blitz/ bald Blitz und Eines sein.", "tokens": ["So", "wird", "sie", "mit", "dem", "Blitz", "/", "bald", "Blitz", "und", "Ei\u00b7nes", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$(", "ADV", "NN", "KON", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}