{"textgrid.poem.48304": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2. Seydlitz und der B\u00fcrgermeister von Ohlau", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Ohlau der B\u00fcrgermeister der Stadt", "tokens": ["In", "Oh\u00b7lau", "der", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "NN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine wei\u00dfe Zippelm\u00fctze hat;", "tokens": ["Ei\u00b7ne", "wei\u00b7\u00dfe", "Zip\u00b7pel\u00b7m\u00fct\u00b7ze", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gegen\u00fcber im Kommandantenhaus", "tokens": ["Ge\u00b7gen\u00b7\u00fc\u00b7ber", "im", "Kom\u00b7man\u00b7dan\u00b7ten\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sieht ", "tokens": ["Sieht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Und jeden Morgen, unentwegt,", "tokens": ["Und", "je\u00b7den", "Mor\u00b7gen", ",", "un\u00b7ent\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich auch Zippelm\u00fctz' ins Fenster legt,", "tokens": ["Sich", "auch", "Zip\u00b7pel\u00b7m\u00fctz'", "ins", "Fens\u00b7ter", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und wenn der Seydlitz dr\u00fcben schmaucht,", "tokens": ["Und", "wenn", "der", "Seyd\u00b7litz", "dr\u00fc\u00b7ben", "schmaucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Zippelm\u00fctze sein Pfeifchen raucht,", "tokens": ["Auch", "Zip\u00b7pel\u00b7m\u00fct\u00b7ze", "sein", "Pfei\u00b7fchen", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wenn der Seydlitz zum R\u00e4uspern ruckt,", "tokens": ["Und", "wenn", "der", "Seyd\u00b7litz", "zum", "R\u00e4us\u00b7pern", "ruckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hat Zippelm\u00fctze schon ausgespuckt.", "tokens": ["Hat", "Zip\u00b7pel\u00b7m\u00fct\u00b7ze", "schon", "aus\u00b7ge\u00b7spuckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Das \u00e4rgert den Seydlitz. \u00bbPhilistergesicht.", "tokens": ["Das", "\u00e4r\u00b7gert", "den", "Seyd\u00b7litz", ".", "\u00bb", "Phi\u00b7lis\u00b7ter\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NE", "$.", "$(", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Affront dazu; das lieb' ich nicht.\u00ab", "tokens": ["A\u00b7ffront", "da\u00b7zu", ";", "das", "lieb'", "ich", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PAV", "$.", "PDS", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und er nimmt Pistolen links von der Wand,", "tokens": ["Und", "er", "nimmt", "Pis\u00b7to\u00b7len", "links", "von", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Zielt hin\u00fcber mit sichrer Hand,", "tokens": ["Zielt", "hin\u00b7\u00fc\u00b7ber", "mit", "sich\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Zielt und schie\u00dft auf drei\u00dfig Schritt,", "tokens": ["Zielt", "und", "schie\u00dft", "auf", "drei\u00b7\u00dfig", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine zweite Kugel und nun eine dritt',", "tokens": ["Ei\u00b7ne", "zwei\u00b7te", "Ku\u00b7gel", "und", "nun", "ei\u00b7ne", "dritt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "ART", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Es spritzt der Kalk \u2013 der dr\u00fcben heiter", "tokens": ["Es", "spritzt", "der", "Kalk", "\u2013", "der", "dr\u00fc\u00b7ben", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zieht seine M\u00fctze, raucht aber weiter,", "tokens": ["Zieht", "sei\u00b7ne", "M\u00fct\u00b7ze", ",", "raucht", "a\u00b7ber", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und Seydlitz lacht: \u00bbVerfluchte Visage.", "tokens": ["Und", "Seyd\u00b7litz", "lacht", ":", "\u00bb", "Ver\u00b7fluch\u00b7te", "Vi\u00b7sa\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aber der Kerl hat Courage.\u00ab", "tokens": ["A\u00b7ber", "der", "Kerl", "hat", "Cou\u00b7ra\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Das war im Frieden. Nun steht die Schlacht:", "tokens": ["Das", "war", "im", "Frie\u00b7den", ".", "Nun", "steht", "die", "Schlacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seydlitz wartet und Seydlitz wacht,", "tokens": ["Seyd\u00b7litz", "war\u00b7tet", "und", "Seyd\u00b7litz", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "NE", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "An strahlt ihn der Ruhm, er steigt zu Pferde,", "tokens": ["An", "strahlt", "ihn", "der", "Ruhm", ",", "er", "steigt", "zu", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hundert Schwadronen, es donnert die Erde;", "tokens": ["Hun\u00b7dert", "Schwad\u00b7ro\u00b7nen", ",", "es", "don\u00b7nert", "die", "Er\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Gestern in Ohlau im Fenster liegen,", "tokens": ["Ge\u00b7stern", "in", "Oh\u00b7lau", "im", "Fens\u00b7ter", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Heute bei Zorndorf siegen, siegen \u2013", "tokens": ["Heu\u00b7te", "bei", "Zorn\u00b7dorf", "sie\u00b7gen", ",", "sie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVINF", "$,", "VVFIN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wie kam der Wandel! Fragt nicht ", "tokens": ["Wie", "kam", "der", "Wan\u00b7del", "!", "Fragt", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "VVFIN", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Klein im Kleinen, im Gro\u00dfen Genie.", "tokens": ["Klein", "im", "Klei\u00b7nen", ",", "im", "Gro\u00b7\u00dfen", "Ge\u00b7nie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}