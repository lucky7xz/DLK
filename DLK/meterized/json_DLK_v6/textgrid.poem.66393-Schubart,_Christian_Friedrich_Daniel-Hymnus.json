{"textgrid.poem.66393": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Hymnus", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige Tonkunst, g\u00f6ttlichen Stammes!", "tokens": ["Hei\u00b7li\u00b7ge", "Ton\u00b7kunst", ",", "g\u00f6tt\u00b7li\u00b7chen", "Stam\u00b7mes", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gespielin der Engel, Vertraute des Himmels!", "tokens": ["Ge\u00b7spie\u00b7lin", "der", "En\u00b7gel", ",", "Ver\u00b7trau\u00b7te", "des", "Him\u00b7mels", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die gefallene Menschheit klagte;", "tokens": ["Die", "ge\u00b7fal\u00b7le\u00b7ne", "Menschheit", "klag\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Des Lebens Dornenpfad verwundet' ihre Sohle,", "tokens": ["Des", "Le\u00b7bens", "Dor\u00b7nen\u00b7pfad", "ver\u00b7wun\u00b7det'", "ih\u00b7re", "Soh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eine blutige Thr\u00e4ne fiel auf die sengende Nessel:", "tokens": ["Ei\u00b7ne", "blu\u00b7ti\u00b7ge", "Thr\u00e4\u00b7ne", "fiel", "auf", "die", "sen\u00b7gen\u00b7de", "Nes\u00b7sel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Da trat'st du, Himmlische, im Schwanenkleide", "tokens": ["Da", "trat'st", "du", ",", "Himm\u00b7li\u00b7sche", ",", "im", "Schwa\u00b7nen\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vor sie hin und hauchtest ihr ", "tokens": ["Vor", "sie", "hin", "und", "hauch\u00b7test", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nun klang die Saite unter dem ziehenden Bogen,", "tokens": ["Nun", "klang", "die", "Sai\u00b7te", "un\u00b7ter", "dem", "zie\u00b7hen\u00b7den", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Nun klang das Goldgeweb der Harfe;", "tokens": ["Nun", "klang", "das", "Gold\u00b7ge\u00b7web", "der", "Har\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nun klang der Lyra Silbergewebe;", "tokens": ["Nun", "klang", "der", "Ly\u00b7ra", "Sil\u00b7ber\u00b7ge\u00b7we\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Nun schmetterte Trompetenklang,", "tokens": ["Nun", "schmet\u00b7ter\u00b7te", "Trom\u00b7pe\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Und es wieherte das Streitro\u00df d'rein.", "tokens": ["Und", "es", "wie\u00b7her\u00b7te", "das", "Strei\u00b7tro\u00df", "d'\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Nun t\u00f6nte das schallende Horn,", "tokens": ["Nun", "t\u00f6n\u00b7te", "das", "schal\u00b7len\u00b7de", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Nun flisterte die weiche, lydische Fl\u00f6te;", "tokens": ["Nun", "flis\u00b7ter\u00b7te", "die", "wei\u00b7che", ",", "ly\u00b7di\u00b7sche", "Fl\u00f6\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Nun wirbelte der Tanz,", "tokens": ["Nun", "wir\u00b7bel\u00b7te", "der", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Nun schmolz der J\u00fcngling in Liebe,", "tokens": ["Nun", "schmolz", "der", "J\u00fcng\u00b7ling", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "\u2013 Zerflo\u00df das bleichere M\u00e4dchen in Liebe.", "tokens": ["\u2013", "Zer\u00b7flo\u00df", "das", "blei\u00b7che\u00b7re", "M\u00e4d\u00b7chen", "in", "Lie\u00b7be", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Im Tempel scholl Jehovahs Lob;", "tokens": ["Im", "Tem\u00b7pel", "scholl", "Je\u00b7ho\u00b7vahs", "Lob", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Hallposaune t\u00f6nte d'rein,", "tokens": ["Die", "Hall\u00b7po\u00b7sau\u00b7ne", "t\u00f6n\u00b7te", "d'\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Und die Asoor und die Githit und die schallende Cymbal.", "tokens": ["Und", "die", "A\u00b7soor", "und", "die", "Git\u00b7hit", "und", "die", "schal\u00b7len\u00b7de", "Cym\u00b7bal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Der Donner des ", "tokens": ["Der", "Don\u00b7ner", "des"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.22": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Und Jehovah l\u00e4chelte Gnade!", "tokens": ["Und", "Je\u00b7ho\u00b7vah", "l\u00e4\u00b7chel\u00b7te", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "NN", "$."], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df mich dich, g\u00f6ttliche ", "tokens": ["La\u00df", "mich", "dich", ",", "g\u00f6tt\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "PPER", "$,", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "\u2013 Denn auch mich hast du in den Stunden der Weihe besucht:", "tokens": ["\u2013", "Denn", "auch", "mich", "hast", "du", "in", "den", "Stun\u00b7den", "der", "Wei\u00b7he", "be\u00b7sucht", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du gabst mir m\u00e4nnlichen Gesang und Fl\u00fcgelspiel,", "tokens": ["Du", "gabst", "mir", "m\u00e4nn\u00b7li\u00b7chen", "Ge\u00b7sang", "und", "Fl\u00fc\u00b7gel\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich gebiete der Thr\u00e4ne des H\u00f6rers zu flie\u00dfen;", "tokens": ["Da\u00df", "ich", "ge\u00b7bie\u00b7te", "der", "Thr\u00e4\u00b7ne", "des", "H\u00f6\u00b7rers", "zu", "flie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da\u00df ich f\u00e4rbe das Antlitz des f\u00fchlenden J\u00fcnglings", "tokens": ["Da\u00df", "ich", "f\u00e4r\u00b7be", "das", "Ant\u00b7litz", "des", "f\u00fch\u00b7len\u00b7den", "J\u00fcng\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Mit der Begeisterung Glut;", "tokens": ["Mit", "der", "Be\u00b7geis\u00b7te\u00b7rung", "Glut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Da\u00df ich dem lauschenden M\u00e4dchen", "tokens": ["Da\u00df", "ich", "dem", "lau\u00b7schen\u00b7den", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Seufzer der Lieb' entlocke;", "tokens": ["Seuf\u00b7zer", "der", "Lieb'", "ent\u00b7lo\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Da\u00df ich durch Wodansgesang schwelle den Busen des Mannes \u2013", "tokens": ["Da\u00df", "ich", "durch", "Wo\u00b7dans\u00b7ge\u00b7sang", "schwel\u00b7le", "den", "Bu\u00b7sen", "des", "Man\u00b7nes", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+-+---+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "O la\u00df mich dich, g\u00f6ttliche Polyhymnia,", "tokens": ["O", "la\u00df", "mich", "dich", ",", "g\u00f6tt\u00b7li\u00b7che", "Po\u00b7ly\u00b7hym\u00b7nia", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und deines Geschenkes himmlischen Werth nie entweihen!", "tokens": ["Und", "dei\u00b7nes", "Ge\u00b7schen\u00b7kes", "himm\u00b7li\u00b7schen", "Werth", "nie", "ent\u00b7wei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "La\u00df mich singen Jehovah \u2013", "tokens": ["La\u00df", "mich", "sin\u00b7gen", "Je\u00b7ho\u00b7vah", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "NE", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Der ist, der war, und der kommt!", "tokens": ["Der", "ist", ",", "der", "war", ",", "und", "der", "kommt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "VAFIN", "$,", "KON", "ART", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "\u2013 Dir, o Tugend, dir, fr\u00f6mmere Liebe,", "tokens": ["\u2013", "Dir", ",", "o", "Tu\u00b7gend", ",", "dir", ",", "fr\u00f6m\u00b7me\u00b7re", "Lie\u00b7be", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "FM", "NN", "$,", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Dir, traulicher Scherz bei unentweihten Pokalen,", "tokens": ["Dir", ",", "trau\u00b7li\u00b7cher", "Scherz", "bei", "un\u00b7ent\u00b7weih\u00b7ten", "Po\u00b7ka\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und, ach dir, o Vaterland, Vaterland,", "tokens": ["Und", ",", "ach", "dir", ",", "o", "Va\u00b7ter\u00b7land", ",", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "XY", "PPER", "$,", "FM", "NN", "$,", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Das ich liebe, wie der J\u00fcngling die Braut \u2013", "tokens": ["Das", "ich", "lie\u00b7be", ",", "wie", "der", "J\u00fcng\u00b7ling", "die", "Braut", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Dir, o Vaterland der Helden und der Feuerseelen,", "tokens": ["Dir", ",", "o", "Va\u00b7ter\u00b7land", "der", "Hel\u00b7den", "und", "der", "Feu\u00b7er\u00b7see\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "Weih' ich mein Fl\u00fcgelspiel und meinen Sang!", "tokens": ["Weih'", "ich", "mein", "Fl\u00fc\u00b7gel\u00b7spiel", "und", "mei\u00b7nen", "Sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wenn ich einst schlummere nach meines Lebens M\u00fchen,", "tokens": ["Wenn", "ich", "einst", "schlum\u00b7me\u00b7re", "nach", "mei\u00b7nes", "Le\u00b7bens", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn \u00fcber meinem Gebein sich der Grabh\u00fcgel th\u00fcrmt,", "tokens": ["Wenn", "\u00fc\u00b7ber", "mei\u00b7nem", "Ge\u00b7bein", "sich", "der", "Grab\u00b7h\u00fc\u00b7gel", "th\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Wenn ich meiner Ketten Last", "tokens": ["Wenn", "ich", "mei\u00b7ner", "Ket\u00b7ten", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Am Grabgekl\u00fcft zur\u00fccke lie\u00df:", "tokens": ["Am", "Grab\u00b7ge\u00b7kl\u00fcft", "zu\u00b7r\u00fc\u00b7cke", "lie\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So weil' ein z\u00e4rtlicher J\u00fcngling am Grabe,", "tokens": ["So", "weil'", "ein", "z\u00e4rt\u00b7li\u00b7cher", "J\u00fcng\u00b7ling", "am", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "So weil' ein f\u00fchlendes M\u00e4dchen am Grabe;", "tokens": ["So", "weil'", "ein", "f\u00fch\u00b7len\u00b7des", "M\u00e4d\u00b7chen", "am", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Sie schauen himmelan und sprechen", "tokens": ["Sie", "schau\u00b7en", "him\u00b7me\u00b7lan", "und", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Mit dem Schimmerblick d