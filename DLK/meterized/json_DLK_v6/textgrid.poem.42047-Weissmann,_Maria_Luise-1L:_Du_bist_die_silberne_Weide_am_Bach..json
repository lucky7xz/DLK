{"textgrid.poem.42047": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist die silberne Weide am Bach.", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist die silberne Weide am Bach.", "tokens": ["Du", "bist", "die", "sil\u00b7ber\u00b7ne", "Wei\u00b7de", "am", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schatten der Wolke Du schwimmend.", "tokens": ["Schat\u00b7ten", "der", "Wol\u00b7ke", "Du", "schwim\u00b7mend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Du gehst \u00fcber die mondenen Wege.", "tokens": ["Du", "gehst", "\u00fc\u00b7ber", "die", "mon\u00b7de\u00b7nen", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-++--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die St\u00e4dte-Stra\u00dfen kennen Dich.", "tokens": ["Die", "St\u00e4d\u00b7te\u00b7Stra\u00b7\u00dfen", "ken\u00b7nen", "Dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Tiere sp\u00fcrten Deiner F\u00e4hrte all.", "tokens": ["Tie\u00b7re", "sp\u00fcr\u00b7ten", "Dei\u00b7ner", "F\u00e4hr\u00b7te", "all", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPOSAT", "NN", "PIAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nun suchen Waller, steile, Dich gebetvoll.", "tokens": ["Nun", "su\u00b7chen", "Wal\u00b7ler", ",", "stei\u00b7le", ",", "Dich", "ge\u00b7bet\u00b7voll", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da rot mein Fu\u00df ging \u2013 Deine Ferne brannte! \u2013", "tokens": ["Da", "rot", "mein", "Fu\u00df", "ging", "\u2013", "Dei\u00b7ne", "Fer\u00b7ne", "brann\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Liebend erkannten sich die Wandernden.", "tokens": ["Lie\u00b7bend", "er\u00b7kann\u00b7ten", "sich", "die", "Wan\u00b7dern\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}}}}}