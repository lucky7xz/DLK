{"textgrid.poem.32849": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man r\u00fchmt an Chloen jederzeit,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man r\u00fchmt an Chloen jederzeit,", "tokens": ["Man", "r\u00fchmt", "an", "Chloen", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie tugendhaft sie sey:", "tokens": ["Wie", "tu\u00b7gend\u00b7haft", "sie", "sey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcr die Ungerechtigkeit,", "tokens": ["Und", "f\u00fcr", "die", "Un\u00b7ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gst du doch keine Scheu?", "tokens": ["Tr\u00e4gst", "du", "doch", "kei\u00b7ne", "Scheu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit sch\u00f6nem Zorne drohst du mir,", "tokens": ["Mit", "sch\u00f6\u00b7nem", "Zor\u00b7ne", "drohst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stampfst mit dem kleinen Fu\u00df,", "tokens": ["Stampfst", "mit", "dem", "klei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und st\u00f6\u00dfest mich erz\u00fcrnt von dir,", "tokens": ["Und", "st\u00f6\u00b7\u00dfest", "mich", "er\u00b7z\u00fcrnt", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdiente die\u00df ein Ku\u00df?", "tokens": ["Ver\u00b7dien\u00b7te", "die\u00df", "ein", "Ku\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gut! da\u00df du wei\u00dft, auf welch Versehn,", "tokens": ["Gut", "!", "da\u00df", "du", "wei\u00dft", ",", "auf", "welch", "Ver\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "KOUS", "PPER", "VVFIN", "$,", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dergleichen Rache blitzt;", "tokens": ["Derg\u00b7lei\u00b7chen", "Ra\u00b7che", "blitzt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So la\u00df mich es geschwind begehn,", "tokens": ["So", "la\u00df", "mich", "es", "ge\u00b7schwind", "be\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann strafe so wie itzt!", "tokens": ["Dann", "stra\u00b7fe", "so", "wie", "itzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}