{"textgrid.poem.37501": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Schreihals", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbda, Lina, zieh ihm 's Nachtzeug an,", "tokens": ["\u00bb", "da", ",", "Li\u00b7na", ",", "zieh", "ihm", "'s", "Nacht\u00b7zeug", "an", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NE", "$,", "VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df ich die Flasche w\u00e4rmen kann.\u00ab", "tokens": ["Da\u00df", "ich", "die", "Fla\u00b7sche", "w\u00e4r\u00b7men", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Mutter geht, und eh' sie scheidet,", "tokens": ["Die", "Mut\u00b7ter", "geht", ",", "und", "eh'", "sie", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird Willi schon des Hemds entkleidet.", "tokens": ["Wird", "Wil\u00b7li", "schon", "des", "Hemds", "ent\u00b7klei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die W\u00e4scherei gef\u00e4llt ihm nicht,", "tokens": ["Die", "W\u00e4\u00b7sche\u00b7rei", "ge\u00b7f\u00e4llt", "ihm", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor allen Dingen im Gesicht.", "tokens": ["Vor", "al\u00b7len", "Din\u00b7gen", "im", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch schreit er nicht und h\u00e4lt ganz still", "tokens": ["Doch", "schreit", "er", "nicht", "und", "h\u00e4lt", "ganz", "still"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4\u00dft sich pudern, wo man will.", "tokens": ["Und", "l\u00e4\u00dft", "sich", "pu\u00b7dern", ",", "wo", "man", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVINF", "$,", "PWAV", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kaum aber schn\u00fcret man ihn ein,", "tokens": ["Kaum", "a\u00b7ber", "schn\u00fc\u00b7ret", "man", "ihn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So f\u00e4ngt er auch schon an zu schrein.", "tokens": ["So", "f\u00e4ngt", "er", "auch", "schon", "an", "zu", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hab\u00e4h! \u2013 So t\u00f6nt sein Wehgeschrei", "tokens": ["Ha\u00b7b\u00e4h", "!", "\u2013", "So", "t\u00f6nt", "sein", "Weh\u00b7ge\u00b7schrei"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lockt den Vater selbst herbei.", "tokens": ["Und", "lockt", "den", "Va\u00b7ter", "selbst", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbhier, halt ihn eben mal, Papa!", "tokens": ["\u00bb", "hier", ",", "halt", "ihn", "e\u00b7ben", "mal", ",", "Pa\u00b7pa", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich geh' und rufe die Mama!\u00ab", "tokens": ["Ich", "geh'", "und", "ru\u00b7fe", "die", "Ma\u00b7ma", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Vater trommelt an den Scheiben,", "tokens": ["Der", "Va\u00b7ter", "trom\u00b7melt", "an", "den", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um Willis Trauer zu vertreiben.", "tokens": ["Um", "Wil\u00b7lis", "Trau\u00b7er", "zu", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er l\u00e4\u00dft ihn in den Spiegel schaun. \u2013", "tokens": ["Er", "l\u00e4\u00dft", "ihn", "in", "den", "Spie\u00b7gel", "schaun", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Willi schreit, bis da\u00df er braun.", "tokens": ["Der", "Wil\u00b7li", "schreit", ",", "bis", "da\u00df", "er", "braun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "KON", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbhorch, Willi, horch, die Ticktackuhr!\u00ab", "tokens": ["\u00bb", "horch", ",", "Wil\u00b7li", ",", "horch", ",", "die", "Tick\u00b7ta\u00b7ckuhr", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$,", "NE", "$,", "VVIMP", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Willi schreit noch \u00e4rger nur.", "tokens": ["Der", "Wil\u00b7li", "schreit", "noch", "\u00e4r\u00b7ger", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbsusu, mein Herz! Schlaf ein, schlaf ein!\u00ab \u2013", "tokens": ["\u00bb", "su\u00b7su", ",", "mein", "Herz", "!", "Schlaf", "ein", ",", "schlaf", "ein", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "NE", "$,", "PPOSAT", "NN", "$.", "NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "$(", "$("], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er f\u00e4ngt noch lauter an zu schrein.", "tokens": ["Er", "f\u00e4ngt", "noch", "lau\u00b7ter", "an", "zu", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Mit List zeigt er die Zipfelhauben \u2013", "tokens": ["Mit", "List", "zeigt", "er", "die", "Zip\u00b7fel\u00b7hau\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umsonst!\u2013der Willi will's nicht glauben.", "tokens": ["Um\u00b7sonst", "!", "\u2013", "der", "Wil\u00b7li", "will's", "nicht", "glau\u00b7ben", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "ART", "NE", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Jetzt macht er einen Butzemann. \u2013", "tokens": ["Jetzt", "macht", "er", "ei\u00b7nen", "But\u00b7ze\u00b7mann", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O weh! \u2013 Nun geht's noch schlimmer an.", "tokens": ["O", "weh", "!", "\u2013", "Nun", "geht's", "noch", "schlim\u00b7mer", "an", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die Mutter \u00f6ffnet grad die T\u00fcr:", "tokens": ["Die", "Mut\u00b7ter", "\u00f6ff\u00b7net", "grad", "die", "T\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmein Herz! Was machen sie mit dir?!!\u00ab", "tokens": ["\u00bb", "mein", "Herz", "!", "Was", "ma\u00b7chen", "sie", "mit", "dir", "?!!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Die Mutter macht ein ernst Gesicht:", "tokens": ["Die", "Mut\u00b7ter", "macht", "ein", "ernst", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbja, was ist das? \u2013 Auch dieses nicht?!\u00ab \u2013", "tokens": ["\u00bb", "ja", ",", "was", "ist", "das", "?", "\u2013", "Auch", "die\u00b7ses", "nicht", "?!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "PWS", "VAFIN", "PDS", "$.", "$(", "ADV", "PDS", "PTKNEG", "$.", "$(", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.16": {"line.1": {"text": "Grad kommt die Tante auf Visite", "tokens": ["Grad", "kommt", "die", "Tan\u00b7te", "auf", "Vi\u00b7si\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ruft erschreckt: \u00bbDu meine G\u00fcte!!\u00ab \u2013", "tokens": ["Und", "ruft", "er\u00b7schreckt", ":", "\u00bb", "Du", "mei\u00b7ne", "G\u00fc\u00b7te", "!!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$.", "$(", "PPER", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Voll Weisheit \u00f6ffnet sie den Bund. \u2013", "tokens": ["Voll", "Weis\u00b7heit", "\u00f6ff\u00b7net", "sie", "den", "Bund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da haben wir's! \u2013 Das war der Grund! \u2013", "tokens": ["Da", "ha\u00b7ben", "wir's", "!", "\u2013", "Das", "war", "der", "Grund", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$.", "$(", "PDS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Und Willi, der von Schmerz befreit,", "tokens": ["Und", "Wil\u00b7li", ",", "der", "von", "Schmerz", "be\u00b7freit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lacht laut vor lauter Heiterkeit.", "tokens": ["Lacht", "laut", "vor", "lau\u00b7ter", "Hei\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}