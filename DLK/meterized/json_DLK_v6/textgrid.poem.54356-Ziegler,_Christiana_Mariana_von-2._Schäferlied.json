{"textgrid.poem.54356": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "2. Sch\u00e4ferlied", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thyrsis! fragst du wo ich bin,", "tokens": ["Thyr\u00b7sis", "!", "fragst", "du", "wo", "ich", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Qu\u00e4lt dich Sehnsucht und Verlangen;", "tokens": ["Qu\u00e4lt", "dich", "Sehn\u00b7sucht", "und", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klagst du mit best\u00fcrztem Sinn,", "tokens": ["Klagst", "du", "mit", "be\u00b7st\u00fcrz\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df dir Doris ist entgangen?", "tokens": ["Da\u00df", "dir", "Do\u00b7ris", "ist", "ent\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja sie floh, doch steht sie hier,", "tokens": ["Ja", "sie", "floh", ",", "doch", "steht", "sie", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir von weitem nach zu blicken,", "tokens": ["Dir", "von", "wei\u00b7tem", "nach", "zu", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIS", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da mich das Verh\u00e4ngni\u00df dir", "tokens": ["Da", "mich", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Will aus Fluhr und Augen r\u00fccken.", "tokens": ["Will", "aus", "Fluhr", "und", "Au\u00b7gen", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Frage nur den Wiederschall,", "tokens": ["Fra\u00b7ge", "nur", "den", "Wie\u00b7der\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der durch Th\u00e4l und W\u00e4lder dringet,", "tokens": ["Der", "durch", "Th\u00e4l", "und", "W\u00e4l\u00b7der", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die W\u00f6rter \u00fcberall", "tokens": ["Und", "die", "W\u00f6r\u00b7ter", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er f\u00e4ngt, zusammen bringet.", "tokens": ["Die", "er", "f\u00e4ngt", ",", "zu\u00b7sam\u00b7men", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frag ihn nur, ob Doris klagt,", "tokens": ["Frag", "ihn", "nur", ",", "ob", "Do\u00b7ris", "klagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Abschied schuldig blieben,", "tokens": ["Die", "den", "Ab\u00b7schied", "schul\u00b7dig", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was sie dem entfernet sagt,", "tokens": ["Was", "sie", "dem", "ent\u00b7fer\u00b7net", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PDS", "VVFIN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Den sie noch mu\u00df z\u00e4rtlich lieben.", "tokens": ["Den", "sie", "noch", "mu\u00df", "z\u00e4rt\u00b7lich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Holde Gegend, fettes Gras!", "tokens": ["Hol\u00b7de", "Ge\u00b7gend", ",", "fet\u00b7tes", "Gras", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Augen sch\u00f6nste Weyde,", "tokens": ["Mei\u00b7ner", "Au\u00b7gen", "sch\u00f6ns\u00b7te", "Wey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich oft mit Thyrsis sa\u00df,", "tokens": ["Wo", "ich", "oft", "mit", "Thyr\u00b7sis", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O was sp\u00fchrten wir f\u00fcr Freude!", "tokens": ["O", "was", "sp\u00fchr\u00b7ten", "wir", "f\u00fcr", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, wie s\u00fcsse roch der Klee,", "tokens": ["Ach", ",", "wie", "s\u00fcs\u00b7se", "roch", "der", "Klee", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der die bunten Wiesen zieret;", "tokens": ["Der", "die", "bun\u00b7ten", "Wie\u00b7sen", "zie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Itzo hat ein herbes Weh", "tokens": ["It\u00b7zo", "hat", "ein", "her\u00b7bes", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich und meine Brust ger\u00fchret.", "tokens": ["Mich", "und", "mei\u00b7ne", "Brust", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stolzer Bach! was rauschest du?", "tokens": ["Stol\u00b7zer", "Bach", "!", "was", "rau\u00b7schest", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dein Rieseln stille schweigen.", "tokens": ["La\u00df", "dein", "Rie\u00b7seln", "stil\u00b7le", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6re mir, der Doris zu,", "tokens": ["H\u00f6\u00b7re", "mir", ",", "der", "Do\u00b7ris", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie erkiest dich hier zum Zeugen.", "tokens": ["Sie", "er\u00b7kiest", "dich", "hier", "zum", "Zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sage meinem Sch\u00e4fer an,", "tokens": ["Sa\u00b7ge", "mei\u00b7nem", "Sch\u00e4\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was du siehst f\u00fcr Thr\u00e4nen fliessen,", "tokens": ["Was", "du", "siehst", "f\u00fcr", "Thr\u00e4\u00b7nen", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die dein Umfang gar nicht kann", "tokens": ["Die", "dein", "Um\u00b7fang", "gar", "nicht", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV", "PTKNEG", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit in seine Quelle schliessen.", "tokens": ["Mit", "in", "sei\u00b7ne", "Quel\u00b7le", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Still, hier h\u00f6r ich einen Klang,", "tokens": ["Still", ",", "hier", "h\u00f6r", "ich", "ei\u00b7nen", "Klang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es verdoppelt sich mein Klagen.", "tokens": ["Es", "ver\u00b7dop\u00b7pelt", "sich", "mein", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht vielleicht in diesem Gang", "tokens": ["Sucht", "viel\u00b7leicht", "in", "die\u00b7sem", "Gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich mein Thyrsis auszufragen?", "tokens": ["Mich", "mein", "Thyr\u00b7sis", "aus\u00b7zu\u00b7fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4um ich, oder, h\u00f6r ich ihn,", "tokens": ["Tr\u00e4um", "ich", ",", "o\u00b7der", ",", "h\u00f6r", "ich", "ihn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ein Waldgeist mich verf\u00fchren,", "tokens": ["Will", "ein", "Wald\u00b7geist", "mich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Feige Doris willst du fliehn?", "tokens": ["Fei\u00b7ge", "Do\u00b7ris", "willst", "du", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nein! ich such ihn auszusp\u00fchren.", "tokens": ["Nein", "!", "ich", "such", "ihn", "aus\u00b7zu\u00b7sp\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eilt ihr Sch\u00e4fchen! kommt mit mir,", "tokens": ["Eilt", "ihr", "Sch\u00e4f\u00b7chen", "!", "kommt", "mit", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft uns ihm entgegen gehen,", "tokens": ["La\u00dft", "uns", "ihm", "ent\u00b7ge\u00b7gen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Denn ich weis gewi\u00df, da\u00df ihr", "tokens": ["Denn", "ich", "weis", "ge\u00b7wi\u00df", ",", "da\u00df", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "PTKVZ", "ADV", "$,", "KOUS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Stimme m\u00fc\u00dft verstehen.", "tokens": ["Sei\u00b7ne", "Stim\u00b7me", "m\u00fc\u00dft", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wi\u00dft ihr nicht, wie manches Lied", "tokens": ["Wi\u00dft", "ihr", "nicht", ",", "wie", "man\u00b7ches", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir, und euch vielleicht, gefallen,", "tokens": ["Mir", ",", "und", "euch", "viel\u00b7leicht", ",", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "KON", "PPER", "ADV", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das mein Sch\u00e4fer sonst bem\u00fcht", "tokens": ["Das", "mein", "Sch\u00e4\u00b7fer", "sonst", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lie\u00df auf eurer Weid erschallen.", "tokens": ["Lie\u00df", "auf", "eu\u00b7rer", "Weid", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schaut, dort k\u00f6mmt er in der That,", "tokens": ["Schaut", ",", "dort", "k\u00f6mmt", "er", "in", "der", "That", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ich seh ihn schon von weiten;", "tokens": ["Ach", ",", "ich", "seh", "ihn", "schon", "von", "wei\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O du h\u00f6chst begl\u00fcckter Pfad!", "tokens": ["O", "du", "h\u00f6chst", "be\u00b7gl\u00fcck\u00b7ter", "Pfad", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den mein Thyrsis will beschreiten.", "tokens": ["Den", "mein", "Thyr\u00b7sis", "will", "be\u00b7schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, dies hab ich wohl gedacht,", "tokens": ["Ja", ",", "dies", "hab", "ich", "wohl", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df mein Ach! ihn m\u00fcste r\u00fchren,", "tokens": ["Da\u00df", "mein", "Ach", "!", "ihn", "m\u00fcs\u00b7te", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auch bey der finstren Nacht", "tokens": ["Und", "auch", "bey", "der", "finst\u00b7ren", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Uber Stock und Steine f\u00fchren.", "tokens": ["U\u00b7ber", "Stock", "und", "Stei\u00b7ne", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Holder Sch\u00e4fer, z\u00fcrne nicht,", "tokens": ["Hol\u00b7der", "Sch\u00e4\u00b7fer", ",", "z\u00fcr\u00b7ne", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du mich entfernt gefunden.", "tokens": ["Wenn", "du", "mich", "ent\u00b7fernt", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wem das Schicksal wiederspricht,", "tokens": ["Wem", "das", "Schick\u00b7sal", "wie\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem ist Hand und Fu\u00df gebunden.", "tokens": ["Dem", "ist", "Hand", "und", "Fu\u00df", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses ri\u00df mich zwar von dir,", "tokens": ["Die\u00b7ses", "ri\u00df", "mich", "zwar", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber dich nicht aus dem Herzen,", "tokens": ["A\u00b7ber", "dich", "nicht", "aus", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Doch vergi\u00df es, weil es hier", "tokens": ["Doch", "ver\u00b7gi\u00df", "es", ",", "weil", "es", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns vergn\u00fcgt l\u00e4\u00dft wieder scherzen.", "tokens": ["Uns", "ver\u00b7gn\u00fcgt", "l\u00e4\u00dft", "wie\u00b7der", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sage mir, du, den der Pan", "tokens": ["Sa\u00b7ge", "mir", ",", "du", ",", "den", "der", "Pan"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Als den Preis von unsern Hirten", "tokens": ["Als", "den", "Preis", "von", "un\u00b7sern", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gar nicht satt bekr\u00f6nen kann,", "tokens": ["Gar", "nicht", "satt", "be\u00b7kr\u00f6\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwer es mir bey seinen Myrrthen!", "tokens": ["Schwer", "es", "mir", "bey", "sei\u00b7nen", "Myrrt\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du, da man uns gest\u00f6hrt,", "tokens": ["Bist", "du", ",", "da", "man", "uns", "ge\u00b7st\u00f6hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seit der Zeit auch treu geblieben?", "tokens": ["Seit", "der", "Zeit", "auch", "treu", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat was Fremdes, das entehrt,", "tokens": ["Hat", "was", "Frem\u00b7des", ",", "das", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was das Gl\u00fcck mir zugeschrieben?", "tokens": ["Was", "das", "Gl\u00fcck", "mir", "zu\u00b7ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch, mein matter Sch\u00e4fer gleicht", "tokens": ["Doch", ",", "mein", "mat\u00b7ter", "Sch\u00e4\u00b7fer", "gleicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stummen B\u00e4umen in den Th\u00e4lern,", "tokens": ["Stum\u00b7men", "B\u00e4u\u00b7men", "in", "den", "Th\u00e4\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht ein Satyr hier vielleicht", "tokens": ["Sucht", "ein", "Sa\u00b7tyr", "hier", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Lieb und Treu zu schm\u00e4hlern?", "tokens": ["Mei\u00b7ne", "Lieb", "und", "Treu", "zu", "schm\u00e4h\u00b7lern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rede doch, Gespiele, sprich,", "tokens": ["Re\u00b7de", "doch", ",", "Ge\u00b7spie\u00b7le", ",", "sprich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat ein Feind dich aufgehetzet,", "tokens": ["Hat", "ein", "Feind", "dich", "auf\u00b7ge\u00b7het\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df du deine Freundin, mich,", "tokens": ["Da\u00df", "du", "dei\u00b7ne", "Freun\u00b7din", ",", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hast so schnell hindan gesetzet?", "tokens": ["Hast", "so", "schnell", "hin\u00b7dan", "ge\u00b7set\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach ihr Sterne! was geschicht,", "tokens": ["Ach", "ihr", "Ster\u00b7ne", "!", "was", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$.", "PWS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es m\u00f6glich, da\u00df ein Schatten,", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", ",", "da\u00df", "ein", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich, die doch kein Schlaf anficht,", "tokens": ["Mich", ",", "die", "doch", "kein", "Schlaf", "an\u00b7ficht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft mit meinem Thyrsis gatten!", "tokens": ["L\u00e4\u00dft", "mit", "mei\u00b7nem", "Thyr\u00b7sis", "gat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wahrlich, es ist nur ein Traum", "tokens": ["Wahr\u00b7lich", ",", "es", "ist", "nur", "ein", "Traum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Und ein blosser Schein gewesen,", "tokens": ["Und", "ein", "blos\u00b7ser", "Schein", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich zuckers\u00fcssen Schaum", "tokens": ["Da\u00df", "ich", "zu\u00b7cker\u00b7s\u00fcs\u00b7sen", "Schaum"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Von des Sch\u00e4fers Mund gelesen.", "tokens": ["Von", "des", "Sch\u00e4\u00b7fers", "Mund", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Falscher Nachtgott qu\u00e4le doch", "tokens": ["Fal\u00b7scher", "Nacht\u00b7gott", "qu\u00e4\u00b7le", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich nicht mit dergleichen Bildern!", "tokens": ["Mich", "nicht", "mit", "derg\u00b7lei\u00b7chen", "Bil\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PIS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du bey so schwerem Joch", "tokens": ["Willst", "du", "bey", "so", "schwe\u00b7rem", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir noch meinen Abgott schildern?", "tokens": ["Mir", "noch", "mei\u00b7nen", "Ab\u00b7gott", "schil\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den ich, da die Hoffnung tr\u00fcgt,", "tokens": ["Den", "ich", ",", "da", "die", "Hoff\u00b7nung", "tr\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leider mu\u00df nunmehr vermissen;", "tokens": ["Lei\u00b7der", "mu\u00df", "nun\u00b7mehr", "ver\u00b7mis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was mich wachend nicht vergn\u00fcgt,", "tokens": ["Was", "mich", "wa\u00b7chend", "nicht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mag ich auch im Traum nicht k\u00fcssen.", "tokens": ["Mag", "ich", "auch", "im", "Traum", "nicht", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Rede Doris nicht zuviel,", "tokens": ["Re\u00b7de", "Do\u00b7ris", "nicht", "zu\u00b7viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da dich Zorn und Eifer treibet,", "tokens": ["Da", "dich", "Zorn", "und", "Ei\u00b7fer", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil auch bey dem Schattenspiel", "tokens": ["Weil", "auch", "bey", "dem", "Schat\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Thyrsis doch dein Liebling bleibet!", "tokens": ["Thyr\u00b7sis", "doch", "dein", "Lieb\u00b7ling", "blei\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohl, ich will zufrieden seyn,", "tokens": ["Wohl", ",", "ich", "will", "zu\u00b7frie\u00b7den", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Find ich ihn nicht in der N\u00e4he,", "tokens": ["Find", "ich", "ihn", "nicht", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Wenn ich nur von ihm den Schein", "tokens": ["Wenn", "ich", "nur", "von", "ihm", "den", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tr\u00e4umend in dem Schlafe sehe.", "tokens": ["Tr\u00e4u\u00b7mend", "in", "dem", "Schla\u00b7fe", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Braune Nacht! verl\u00e4ngre dich,", "tokens": ["Brau\u00b7ne", "Nacht", "!", "ver\u00b7l\u00e4ng\u00b7re", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df den Hesper lange wachen,", "tokens": ["La\u00df", "den", "Hes\u00b7per", "lan\u00b7ge", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich ihn, mein ander ich", "tokens": ["Da\u00df", "ich", "ihn", ",", "mein", "an\u00b7der", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "PPOSAT", "ADJD", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh im Geist und Bilde lachen.", "tokens": ["Seh", "im", "Geist", "und", "Bil\u00b7de", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat man doch wohl ehr gesehn,", "tokens": ["Hat", "man", "doch", "wohl", "ehr", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ein Traum was prophezeyet.", "tokens": ["Da\u00df", "ein", "Traum", "was", "pro\u00b7phe\u00b7ze\u00b7yet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PWS", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Hoffe fest, es kann geschehn,", "tokens": ["Hof\u00b7fe", "fest", ",", "es", "kann", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df die That dich bald erfreuet.", "tokens": ["Da\u00df", "die", "That", "dich", "bald", "er\u00b7freu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}