{"textgrid.poem.32724": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Trauerlied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchles Grab, o n\u00e4hmest du", "tokens": ["K\u00fch\u00b7les", "Grab", ",", "o", "n\u00e4h\u00b7mest", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "FM", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich in deine stille Ruh!", "tokens": ["Mich", "in", "dei\u00b7ne", "stil\u00b7le", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Liebevolle, Reine,", "tokens": ["Denn", "die", "Lie\u00b7be\u00b7vol\u00b7le", ",", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00df mich auf der Welt alleine.", "tokens": ["Lie\u00df", "mich", "auf", "der", "Welt", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fchles Grab, o n\u00e4hmest du", "tokens": ["K\u00fch\u00b7les", "Grab", ",", "o", "n\u00e4h\u00b7mest", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "FM", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich in deine stille Ruh.", "tokens": ["Mich", "in", "dei\u00b7ne", "stil\u00b7le", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, ich sehe rings umher;", "tokens": ["Ach", ",", "ich", "se\u00b7he", "rings", "um\u00b7her", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber sie ist nirgends mehr!", "tokens": ["A\u00b7ber", "sie", "ist", "nir\u00b7gends", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Pl\u00e4tzchen dieser Wiese", "tokens": ["Je\u00b7des", "Pl\u00e4tz\u00b7chen", "die\u00b7ser", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mahnet deiner mich, Elise!", "tokens": ["Mah\u00b7net", "dei\u00b7ner", "mich", ",", "E\u00b7li\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "PPER", "$,", "NE", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Ach, ich sehe rings umher;", "tokens": ["Ach", ",", "ich", "se\u00b7he", "rings", "um\u00b7her", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber du bist nirgends mehr!", "tokens": ["A\u00b7ber", "du", "bist", "nir\u00b7gends", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Engel! hier am Blumenrain", "tokens": ["En\u00b7gel", "!", "hier", "am", "Blu\u00b7men\u00b7rain"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sa\u00dfen wir so oft allein;", "tokens": ["Sa\u00b7\u00dfen", "wir", "so", "oft", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Konnten nichts vor Freuden sagen,", "tokens": ["Konn\u00b7ten", "nichts", "vor", "Freu\u00b7den", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die Augen niederschlagen.", "tokens": ["Nur", "die", "Au\u00b7gen", "nie\u00b7der\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Engel! hier am Blumenrain", "tokens": ["En\u00b7gel", "!", "hier", "am", "Blu\u00b7men\u00b7rain"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sa\u00dfen wir so oft allein.", "tokens": ["Sa\u00b7\u00dfen", "wir", "so", "oft", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unser ganzes Hab und Gut", "tokens": ["Un\u00b7ser", "gan\u00b7zes", "Hab", "und", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War ein keuscher, froher Mut.", "tokens": ["War", "ein", "keu\u00b7scher", ",", "fro\u00b7her", "Mut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese kleine Blumenwiese", "tokens": ["Die\u00b7se", "klei\u00b7ne", "Blu\u00b7men\u00b7wie\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schufst du mir zum Paradiese.", "tokens": ["Schufst", "du", "mir", "zum", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber, ach! ein Paradies,", "tokens": ["A\u00b7ber", ",", "ach", "!", "ein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo mein Engel mich verlie\u00df!", "tokens": ["Wo", "mein", "En\u00b7gel", "mich", "ver\u00b7lie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}