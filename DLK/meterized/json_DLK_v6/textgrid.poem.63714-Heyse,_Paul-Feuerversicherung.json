{"textgrid.poem.63714": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Feuerversicherung", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nun sprich, wie soll ich's machen,", "tokens": ["Und", "nun", "sprich", ",", "wie", "soll", "ich's", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier des Lebens froh zu sein,", "tokens": ["Hier", "des", "Le\u00b7bens", "froh", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn so recht von Herzen lachen", "tokens": ["Denn", "so", "recht", "von", "Her\u00b7zen", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ein Mensch doch nur zu Zwein.", "tokens": ["Kann", "ein", "Mensch", "doch", "nur", "zu", "Zwein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar es trennt die flachen D\u00e4cher", "tokens": ["Zwar", "es", "trennt", "die", "fla\u00b7chen", "D\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein verw\u00fcnschtes M\u00e4uerchen,", "tokens": ["Ein", "ver\u00b7w\u00fcnschtes", "M\u00e4u\u00b7er\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch dar\u00fcber sprang in frecher", "tokens": ["Doch", "da\u00b7r\u00fc\u00b7ber", "sprang", "in", "fre\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "APPR", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schadenlust das Feuerchen.", "tokens": ["Scha\u00b7den\u00b7lust", "das", "Feu\u00b7er\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Als du mir dein warmes H\u00e4ndchen", "tokens": ["Als", "du", "mir", "dein", "war\u00b7mes", "H\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichtest \u00fcber jene Wand,", "tokens": ["Reich\u00b7test", "\u00fc\u00b7ber", "je\u00b7ne", "Wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Anfangs zuckt' in mir ein Br\u00e4ndchen,", "tokens": ["An\u00b7fangs", "zuckt'", "in", "mir", "ein", "Br\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es wuchs und ward ein Brand.", "tokens": ["Doch", "es", "wuchs", "und", "ward", "ein", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nun sage, willst du's hindern,", "tokens": ["Und", "nun", "sa\u00b7ge", ",", "willst", "du's", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Klettr' ich dir zum Dach hinein?", "tokens": ["Klett\u00b7r'", "ich", "dir", "zum", "Dach", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, ein solches Feuer lindern", "tokens": ["Ach", ",", "ein", "sol\u00b7ches", "Feu\u00b7er", "lin\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ein Mensch doch nur zu Zwein!", "tokens": ["Kann", "ein", "Mensch", "doch", "nur", "zu", "Zwein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}