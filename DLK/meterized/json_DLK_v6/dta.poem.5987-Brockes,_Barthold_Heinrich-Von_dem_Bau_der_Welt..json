{"dta.poem.5987": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf eine Zeit verliert ein Stern sein Feuer;", "tokens": ["Auf", "ei\u00b7ne", "Zeit", "ver\u00b7liert", "ein", "Stern", "sein", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald drauf zerreisset er den duncklen Schleyer,", "tokens": ["Bald", "drauf", "zer\u00b7reis\u00b7set", "er", "den", "dunck\u00b7len", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wird an seiner ersten Stelle,", "tokens": ["Und", "wird", "an", "sei\u00b7ner", "ers\u00b7ten", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs neu gebohren, wieder helle.", "tokens": ["Aufs", "neu", "ge\u00b7boh\u00b7ren", ",", "wie\u00b7der", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VVPP", "$,", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein andrer dort verliert auf immer", "tokens": ["Ein", "an\u00b7drer", "dort", "ver\u00b7liert", "auf", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den bis daher besessuen Schimmer;", "tokens": ["Den", "bis", "da\u00b7her", "be\u00b7ses\u00b7su\u00b7en", "Schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PAV", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er \u00e4ndert die Natur; und der verschwundne Schein", "tokens": ["Er", "\u00e4n\u00b7dert", "die", "Na\u00b7tur", ";", "und", "der", "ver\u00b7schwund\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "H\u00fcllt sich in dicht-und dunckle Rinden ein.", "tokens": ["H\u00fcllt", "sich", "in", "dicht\u00b7\u00b7und", "dunck\u00b7le", "Rin\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}