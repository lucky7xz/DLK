{"textgrid.poem.42196": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Bajazzo", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seltsam sind des Gl\u00fcckes Launen,", "tokens": ["Selt\u00b7sam", "sind", "des", "Gl\u00fc\u00b7ckes", "Lau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie kein Hirn sie noch ersann,", "tokens": ["Wie", "kein", "Hirn", "sie", "noch", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich meist vor lauter Staunen", "tokens": ["Da\u00df", "ich", "meist", "vor", "lau\u00b7ter", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lachen nicht noch weinen kann!", "tokens": ["La\u00b7chen", "nicht", "noch", "wei\u00b7nen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber freilich steht auf festen", "tokens": ["A\u00b7ber", "frei\u00b7lich", "steht", "auf", "fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fc\u00dfen selbst der Himmel kaum,", "tokens": ["F\u00fc\u00b7\u00dfen", "selbst", "der", "Him\u00b7mel", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum schl\u00e4gt auch der Mensch am besten", "tokens": ["Drum", "schl\u00e4gt", "auch", "der", "Mensch", "am", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "APPRART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "T\u00e4glich seinen Purzelbaum.", "tokens": ["T\u00e4g\u00b7lich", "sei\u00b7nen", "Pur\u00b7zel\u00b7baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wem die Beine noch geschmeidig,", "tokens": ["Wem", "die", "Bei\u00b7ne", "noch", "ge\u00b7schmei\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch die Arme schmiegsam sind,", "tokens": ["Noch", "die", "Ar\u00b7me", "schmieg\u00b7sam", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den stimmt Unheil auch so freudig,", "tokens": ["Den", "stimmt", "Un\u00b7heil", "auch", "so", "freu\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er's innig liebgewinnt!", "tokens": ["Da\u00df", "er's", "in\u00b7nig", "lieb\u00b7ge\u00b7winnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}