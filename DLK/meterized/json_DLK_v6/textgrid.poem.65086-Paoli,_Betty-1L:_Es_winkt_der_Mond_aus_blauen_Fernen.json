{"textgrid.poem.65086": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es winkt der Mond aus blauen Fernen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es winkt der Mond aus blauen Fernen", "tokens": ["Es", "winkt", "der", "Mond", "aus", "blau\u00b7en", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hernieder seinen Geistergru\u00df,", "tokens": ["Her\u00b7nie\u00b7der", "sei\u00b7nen", "Geis\u00b7ter\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Erde schickt den Himmelssternen", "tokens": ["Die", "Er\u00b7de", "schickt", "den", "Him\u00b7melss\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In duft'gen Seufzern Ku\u00df auf Ku\u00df.", "tokens": ["In", "duft'\u00b7gen", "Seuf\u00b7zern", "Ku\u00df", "auf", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In solcher Nacht war's, wo die H\u00fclle", "tokens": ["In", "sol\u00b7cher", "Nacht", "wa\u00b7r's", ",", "wo", "die", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir von dem jungen Auge fiel,", "tokens": ["Mir", "von", "dem", "jun\u00b7gen", "Au\u00b7ge", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich der Liebeswonnen F\u00fclle", "tokens": ["Wo", "ich", "der", "Lie\u00b7bes\u00b7won\u00b7nen", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuerst getr\u00e4umt als Lebensziel,", "tokens": ["Zu\u00b7erst", "ge\u00b7tr\u00e4umt", "als", "Le\u00b7bens\u00b7ziel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ein gestaltlos hei\u00dfes Ahnen,", "tokens": ["Wo", "ein", "ge\u00b7stalt\u00b7los", "hei\u00b7\u00dfes", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Tief mit geheimni\u00dfreichen Mahnen,", "tokens": ["Tief", "mit", "ge\u00b7heim\u00b7ni\u00df\u00b7rei\u00b7chen", "Mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Seele mir zuerst durchfacht", "tokens": ["Die", "See\u00b7le", "mir", "zu\u00b7erst", "durch\u00b7facht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In solcher Nacht.", "tokens": ["In", "sol\u00b7cher", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "In solcher Nacht war's, wo ich, trunken,", "tokens": ["In", "sol\u00b7cher", "Nacht", "wa\u00b7r's", ",", "wo", "ich", ",", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADJD", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zuerst an deiner Brust gegl\u00fcht,", "tokens": ["Zu\u00b7erst", "an", "dei\u00b7ner", "Brust", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo deine Schw\u00fcre Gottesfunken", "tokens": ["Wo", "dei\u00b7ne", "Schw\u00fc\u00b7re", "Got\u00b7tes\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In's tiefste Wesen mir gespr\u00fcht,", "tokens": ["In's", "tiefs\u00b7te", "We\u00b7sen", "mir", "ge\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo, um im Herzen mir zu liegen,", "tokens": ["Wo", ",", "um", "im", "Her\u00b7zen", "mir", "zu", "lie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUI", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom ew'gen Thron herabgestiegen", "tokens": ["Vom", "ew'\u00b7gen", "Thron", "her\u00b7ab\u00b7ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Seligkeiten reichste Macht", "tokens": ["Der", "Se\u00b7lig\u00b7kei\u00b7ten", "reichs\u00b7te", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In solcher Nacht.", "tokens": ["In", "sol\u00b7cher", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "In solcher Nacht ist's nun, da\u00df, tr\u00fcbe", "tokens": ["In", "sol\u00b7cher", "Nacht", "ist's", "nun", ",", "da\u00df", ",", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ADV", "$,", "KOUS", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Geist der Sch\u00e4tze all gedenkt,", "tokens": ["Mein", "Geist", "der", "Sch\u00e4t\u00b7ze", "all", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Gl\u00fcck's, des Hoffens und der Liebe,", "tokens": ["Des", "Gl\u00fc\u00b7ck's", ",", "des", "Hof\u00b7fens", "und", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die l\u00e4ngst ins Meer der Zeit versenkt.", "tokens": ["Die", "l\u00e4ngst", "ins", "Meer", "der", "Zeit", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ich geahnt, was ich empfunden,", "tokens": ["Was", "ich", "ge\u00b7ahnt", ",", "was", "ich", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich besa\u00df, es ist verschwunden", "tokens": ["Was", "ich", "be\u00b7sa\u00df", ",", "es", "ist", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bis auf den Schmerz, der einsam wacht", "tokens": ["Bis", "auf", "den", "Schmerz", ",", "der", "ein\u00b7sam", "wacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In solcher Nacht.", "tokens": ["In", "sol\u00b7cher", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}