{"dta.poem.21528": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Osterpsalm .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nun feiert vom Werke! Des Alltags Gel\u00fcst,", "tokens": ["Nun", "fei\u00b7ert", "vom", "Wer\u00b7ke", "!", "Des", "All\u00b7tags", "Ge\u00b7l\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nun bannt es aus Sinnen und Herzen!", "tokens": ["Nun", "bannt", "es", "aus", "Sin\u00b7nen", "und", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und von der Sonne der Liebe gek\u00fc\u00dft", "tokens": ["Und", "von", "der", "Son\u00b7ne", "der", "Lie\u00b7be", "ge\u00b7k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "La\u00dft flammen die Freudenkerzen!", "tokens": ["La\u00dft", "flam\u00b7men", "die", "Freu\u00b7den\u00b7ker\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir haben gerungen mit schwieliger Hand \u2014", "tokens": ["Wir", "ha\u00b7ben", "ge\u00b7run\u00b7gen", "mit", "schwie\u00b7li\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Im Alltagsstaube geschmachtet! \u2014", "tokens": ["Im", "All\u00b7tags\u00b7stau\u00b7be", "ge\u00b7schmach\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nun la\u00dft uns zerbrechen den leeren Tand,", "tokens": ["Nun", "la\u00dft", "uns", "zer\u00b7bre\u00b7chen", "den", "lee\u00b7ren", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Nun la\u00dft uns z\u00fcnden den Opferbrand,", "tokens": ["Nun", "la\u00dft", "uns", "z\u00fcn\u00b7den", "den", "Op\u00b7fer\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und der Liebe, die lang\u2019 wir verachtet \u2014", "tokens": ["Und", "der", "Lie\u00b7be", ",", "die", "lang'", "wir", "ver\u00b7ach\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Die an\u2019s Kreuz wir geschlagen in frevelndem Wahn,", "tokens": ["Die", "an's", "Kreuz", "wir", "ge\u00b7schla\u00b7gen", "in", "fre\u00b7veln\u00b7dem", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Gekr\u00f6nt mit Dornengewinden:", "tokens": ["Ge\u00b7kr\u00f6nt", "mit", "Dor\u00b7nen\u00b7ge\u00b7win\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Wir geben uns heute ihr unterthan,", "tokens": ["Wir", "ge\u00b7ben", "uns", "heu\u00b7te", "ihr", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Auf da\u00df Erl\u00f6sung wir finden!", "tokens": ["Auf", "da\u00df", "Er\u00b7l\u00f6\u00b7sung", "wir", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "Und der Liebe, die lang\u2019 wir verspottet, verh\u00f6hnt:", "tokens": ["Und", "der", "Lie\u00b7be", ",", "die", "lang'", "wir", "ver\u00b7spot\u00b7tet", ",", "ver\u00b7h\u00f6hnt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.15": {"text": "Geeint und vers\u00f6hnt", "tokens": ["Ge\u00b7eint", "und", "ver\u00b7s\u00f6hnt"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Erschlie\u00dfen wir heute die ", "tokens": ["Er\u00b7schlie\u00b7\u00dfen", "wir", "heu\u00b7te", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Und wie im jungen M\u00e4rzen", "tokens": ["Und", "wie", "im", "jun\u00b7gen", "M\u00e4r\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der Lenz mit allm\u00e4chtigem Werdeton", "tokens": ["Der", "Lenz", "mit", "all\u00b7m\u00e4ch\u00b7ti\u00b7gem", "Wer\u00b7de\u00b7ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Durch die Lande ruft, der Sonnensohn,", "tokens": ["Durch", "die", "Lan\u00b7de", "ruft", ",", "der", "Son\u00b7nen\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Und die Welt in donnerndem Siegesgesang", "tokens": ["Und", "die", "Welt", "in", "don\u00b7nern\u00b7dem", "Sie\u00b7ges\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Ihm zujauchzt, da\u00df nun die Kette zersprang,", "tokens": ["Ihm", "zu\u00b7jauchzt", ",", "da\u00df", "nun", "die", "Ket\u00b7te", "zer\u00b7sprang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Die der Winter ihr wand um die Glieder:", "tokens": ["Die", "der", "Win\u00b7ter", "ihr", "wand", "um", "die", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.23": {"text": "Also auch wieder", "tokens": ["Al\u00b7so", "auch", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Werfen ", "tokens": ["Wer\u00b7fen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Der Seele Pforten: zu Hauf nun, zu Hauf,", "tokens": ["Der", "See\u00b7le", "Pfor\u00b7ten", ":", "zu", "Hauf", "nun", ",", "zu", "Hauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "APPR", "NN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Sammelt euch, Lichtgedanken!", "tokens": ["Sam\u00b7melt", "euch", ",", "Licht\u00b7ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.27": {"text": "Jungbl\u00fchender Liebe Osterpracht,", "tokens": ["Jung\u00b7bl\u00fc\u00b7hen\u00b7der", "Lie\u00b7be", "Os\u00b7ter\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "In Flammen und Gluthen zum Leben erwacht,", "tokens": ["In", "Flam\u00b7men", "und", "Glu\u00b7then", "zum", "Le\u00b7ben", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.29": {"text": "Nach bleischwer lastender Winternacht,", "tokens": ["Nach", "blei\u00b7schwer", "las\u00b7ten\u00b7der", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.30": {"text": "Heile die M\u00fcden und Kranken!", "tokens": ["Hei\u00b7le", "die", "M\u00fc\u00b7den", "und", "Kran\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.31": {"text": "Und wenn wir gebangt, gezagt und geklagt,", "tokens": ["Und", "wenn", "wir", "ge\u00b7bangt", ",", "ge\u00b7zagt", "und", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Die Seele zerrissen von Schmerzen \u2014", "tokens": ["Die", "See\u00b7le", "zer\u00b7ris\u00b7sen", "von", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.33": {"text": "Wir wissen es Alle: Es tagt, es tagt", "tokens": ["Wir", "wis\u00b7sen", "es", "Al\u00b7le", ":", "Es", "tagt", ",", "es", "tagt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und in lichtgr\u00fcnem Gekr\u00e4nz\u2019", "tokens": ["Und", "in", "licht\u00b7gr\u00fc\u00b7nem", "Ge\u00b7kr\u00e4nz'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Wandelt der Lenz,", "tokens": ["Wan\u00b7delt", "der", "Lenz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.36": {"text": "Der heilige, selige ", "tokens": ["Der", "hei\u00b7li\u00b7ge", ",", "se\u00b7li\u00b7ge"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Heut\u2019 durch die Lande und ", "tokens": ["Heut'", "durch", "die", "Lan\u00b7de", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON"], "meter": "++-+-+", "measure": "iambic.tri"}}}}}