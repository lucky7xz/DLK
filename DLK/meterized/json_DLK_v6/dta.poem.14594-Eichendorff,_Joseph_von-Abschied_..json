{"dta.poem.14594": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Abendlich schon rauscht der Wald", "tokens": ["A\u00b7bend\u00b7lich", "schon", "rauscht", "der", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus den tiefen Gr\u00fcnden,", "tokens": ["Aus", "den", "tie\u00b7fen", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Droben wird der Herr nun bald", "tokens": ["Dro\u00b7ben", "wird", "der", "Herr", "nun", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An die Sterne z\u00fcnden,", "tokens": ["An", "die", "Ster\u00b7ne", "z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie so stille in den Schl\u00fcnden,", "tokens": ["Wie", "so", "stil\u00b7le", "in", "den", "Schl\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Abendlich nur rauscht der Wald.", "tokens": ["A\u00b7bend\u00b7lich", "nur", "rauscht", "der", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART"