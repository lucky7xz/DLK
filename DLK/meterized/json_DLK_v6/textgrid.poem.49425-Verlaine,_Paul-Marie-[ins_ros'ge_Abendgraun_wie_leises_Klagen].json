{"textgrid.poem.49425": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[ins ros'ge Abendgraun wie leises Klagen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins ros'ge Abendgraun wie leises Klagen", "tokens": ["Ins", "ros'\u00b7ge", "A\u00b7bend\u00b7graun", "wie", "lei\u00b7ses", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "T\u00f6nt das Klavier, gek\u00fcsst von schmaler Hand.", "tokens": ["T\u00f6nt", "das", "Kla\u00b7vier", ",", "ge\u00b7k\u00fcsst", "von", "schma\u00b7ler", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und wie mit schwachem, lindem Fl\u00fcgelschlagen", "tokens": ["Und", "wie", "mit", "schwa\u00b7chem", ",", "lin\u00b7dem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4lt eine alte Weise mich gebannt,", "tokens": ["H\u00e4lt", "ei\u00b7ne", "al\u00b7te", "Wei\u00b7se", "mich", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die schmeichlerisch und zaghaft zu mir fand,", "tokens": ["Die", "schmeich\u00b7le\u00b7risch", "und", "zag\u00b7haft", "zu", "mir", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Raum, des Duft von ihr noch schien zu sagen.", "tokens": ["Im", "Raum", ",", "des", "Duft", "von", "ihr", "noch", "schien", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "APPR", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sanft die Wiege auf und nieder geht,", "tokens": ["Wie", "sanft", "die", "Wie\u00b7ge", "auf", "und", "nie\u00b7der", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die mild einlullt mein Herz, das seufzt und leidet!", "tokens": ["Die", "mild", "ein\u00b7lullt", "mein", "Herz", ",", "das", "seufzt", "und", "lei\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was willst du, Lied, das spielend mich umweht?", "tokens": ["Was", "willst", "du", ",", "Lied", ",", "das", "spie\u00b7lend", "mich", "um\u00b7weht", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was meinst du, Klang, der schwindet und vergeht,", "tokens": ["Was", "meinst", "du", ",", "Klang", ",", "der", "schwin\u00b7det", "und", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der nach dem Fenster hin verl\u00f6schend scheidet,", "tokens": ["Der", "nach", "dem", "Fens\u00b7ter", "hin", "ver\u00b7l\u00f6\u00b7schend", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das nach dem kleinen Garten offen steht?", "tokens": ["Das", "nach", "dem", "klei\u00b7nen", "Gar\u00b7ten", "of\u00b7fen", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}