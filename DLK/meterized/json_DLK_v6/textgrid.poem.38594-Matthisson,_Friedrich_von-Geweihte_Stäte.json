{"textgrid.poem.38594": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Geweihte St\u00e4te", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00fctzt allg\u00fctig, ihr Nymphen, dies heilige Pl\u00e4tzchen der Liebe!", "tokens": ["Sch\u00fctzt", "all\u00b7g\u00fc\u00b7tig", ",", "ihr", "Nym\u00b7phen", ",", "dies", "hei\u00b7li\u00b7ge", "Pl\u00e4tz\u00b7chen", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PPOSAT", "NN", "$,", "PDS", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Nimmer bekr\u00e4nze der Faun hier der M\u00e4nade den Kelch.", "tokens": ["Nim\u00b7mer", "be\u00b7kr\u00e4n\u00b7ze", "der", "Faun", "hier", "der", "M\u00e4\u00b7na\u00b7de", "den", "Kelch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Aber den Grazien spendet, beim Reigen, die duftenden Gl\u00f6ckchen,", "tokens": ["A\u00b7ber", "den", "Gra\u00b7zi\u00b7en", "spen\u00b7det", ",", "beim", "Rei\u00b7gen", ",", "die", "duf\u00b7ten\u00b7den", "Gl\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Welche die Schl\u00e4fe des Mais, schimmernd wie Silber umbl\u00fchn.", "tokens": ["Wel\u00b7che", "die", "Schl\u00e4\u00b7fe", "des", "Mais", ",", "schim\u00b7mernd", "wie", "Sil\u00b7ber", "um\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "$,", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}