{"textgrid.poem.62889": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein junges Weib aus Tivoli", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junges Weib aus Tivoli", "tokens": ["Ein", "jun\u00b7ges", "Weib", "aus", "Ti\u00b7vo\u00b7li"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.2": {"text": "In Algier Sklavin ward.", "tokens": ["In", "Al\u00b7gier", "Skla\u00b7vin", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein rascher Aga kaufte sie", "tokens": ["Ein", "ra\u00b7scher", "A\u00b7ga", "kauf\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und strich sich froh den Bart.", "tokens": ["Und", "strich", "sich", "froh", "den", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbkind, sprach er, trotz den Houris sch\u00f6n,", "tokens": ["\u00bb", "kind", ",", "sprach", "er", ",", "trotz", "den", "Hou\u00b7ris", "sch\u00f6n", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "PPER", "$,", "APPR", "ART", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hemm deiner Thr\u00e4nen Lauf;", "tokens": ["Hemm", "dei\u00b7ner", "Thr\u00e4\u00b7nen", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst heut mit mir schlafen gehn;", "tokens": ["Du", "sollst", "heut", "mit", "mir", "schla\u00b7fen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm diesen Ku\u00df darauf.\u00ab", "tokens": ["Nimm", "die\u00b7sen", "Ku\u00df", "da\u00b7rauf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PDAT", "NN", "PAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Susette war der Heilgen gleich,", "tokens": ["Su\u00b7set\u00b7te", "war", "der", "Heil\u00b7gen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr den Namen gab:", "tokens": ["Die", "ihr", "den", "Na\u00b7men", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie zieht ihm einen Backenstreich", "tokens": ["Sie", "zieht", "ihm", "ei\u00b7nen", "Ba\u00b7cken\u00b7streich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wischt den Ku\u00df sich ab.", "tokens": ["Und", "wischt", "den", "Ku\u00df", "sich", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbdas leid ich nicht, beym Mahomet!\u00ab", "tokens": ["\u00bb", "das", "leid", "ich", "nicht", ",", "beym", "Ma\u00b7ho\u00b7met", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "PTKNEG", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rief ihr der T\u00fcrke zu.", "tokens": ["Rief", "ihr", "der", "T\u00fcr\u00b7ke", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbha! schluchzt die Sch\u00f6ne, dein Prophet", "tokens": ["\u00bb", "ha", "!", "schluchzt", "die", "Sch\u00f6\u00b7ne", ",", "dein", "Pro\u00b7phet"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ITJ", "$.", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War just ein Bock wie du.\u00ab", "tokens": ["War", "just", "ein", "Bock", "wie", "du", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ART", "NE", "PWAV", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein wilder Derwisch stand dabey", "tokens": ["Ein", "wil\u00b7der", "Der\u00b7wisch", "stand", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und h\u00f6rt die Blasphemie;", "tokens": ["Und", "h\u00f6rt", "die", "Blas\u00b7phe\u00b7mie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schleppt sie sch\u00e4umend vor den Dey", "tokens": ["Er", "schleppt", "sie", "sch\u00e4u\u00b7mend", "vor", "den", "Dey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klaget wider sie.", "tokens": ["Und", "kla\u00b7get", "wi\u00b7der", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Dey war leider nicht mehr jung,", "tokens": ["Der", "Dey", "war", "lei\u00b7der", "nicht", "mehr", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sprach mit frommem Graus:", "tokens": ["Er", "sprach", "mit", "from\u00b7mem", "Graus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man schneide f\u00fcr die L\u00e4sterung", "tokens": ["Man", "schnei\u00b7de", "f\u00fcr", "die", "L\u00e4s\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die frevle Zung ihr aus.", "tokens": ["Die", "frev\u00b7le", "Zung", "ihr", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gesagt, gethan. Susette war", "tokens": ["Ge\u00b7sagt", ",", "ge\u00b7than", ".", "Su\u00b7set\u00b7te", "war"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "$.", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun eine M\u00e4rtyrin,", "tokens": ["Nun", "ei\u00b7ne", "M\u00e4r\u00b7ty\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und oben drein verdammt ein Jahr", "tokens": ["Und", "o\u00b7ben", "drein", "ver\u00b7dammt", "ein", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schellenkarrn zu ziehn.", "tokens": ["Den", "Schel\u00b7len\u00b7karrn", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie bog geduldig sich ins Joch,", "tokens": ["Sie", "bog", "ge\u00b7dul\u00b7dig", "sich", "ins", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trug wie ein Lamm ihr Kreutz,", "tokens": ["Trug", "wie", "ein", "Lamm", "ihr", "Kreutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer sie sah, den r\u00fchrte noch", "tokens": ["Und", "wer", "sie", "sah", ",", "den", "r\u00fchr\u00b7te", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Zwilchgewand ihr Reitz.", "tokens": ["Im", "Zwilch\u00b7ge\u00b7wand", "ihr", "Reitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das Jahr verstrich; durch Quaal und Spott", "tokens": ["Das", "Jahr", "ver\u00b7strich", ";", "durch", "Qua\u00b7al", "und", "Spott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gieng es die tr\u00e4ge Bahn:", "tokens": ["Gieng", "es", "die", "tr\u00e4\u00b7ge", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nun flog ein Maschinengott", "tokens": ["Und", "nun", "flog", "ein", "Ma\u00b7schi\u00b7nen\u00b7gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu ihrem Trost heran.", "tokens": ["Zu", "ih\u00b7rem", "Trost", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr Mann, der auch die Kette trug,", "tokens": ["Ihr", "Mann", ",", "der", "auch", "die", "Ket\u00b7te", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schriebs an den Pabst nach Rom.", "tokens": ["Schriebs", "an", "den", "Pabst", "nach", "Rom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der heil'ge Vater ohn Verzug", "tokens": ["Der", "heil'\u00b7ge", "Va\u00b7ter", "ohn", "Ver\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Berief den M\u00f6nch Pachom.", "tokens": ["Be\u00b7rief", "den", "M\u00f6nch", "Pa\u00b7chom", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Er z\u00e4hlte tausend Kronen baar", "tokens": ["Er", "z\u00e4hl\u00b7te", "tau\u00b7send", "Kro\u00b7nen", "baar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Pater in den Schoo\u00df.", "tokens": ["Dem", "Pa\u00b7ter", "in", "den", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh, sprach er, mach das fromme Paar", "tokens": ["Geh", ",", "sprach", "er", ",", "mach", "das", "from\u00b7me", "Paar"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Joch der T\u00fcrken los.", "tokens": ["Vom", "Joch", "der", "T\u00fcr\u00b7ken", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dreymal b\u00fcckt sich der Pater tief,", "tokens": ["Drey\u00b7mal", "b\u00fcckt", "sich", "der", "Pa\u00b7ter", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und schnell wie Habakuk", "tokens": ["Und", "schnell", "wie", "Ha\u00b7ba\u00b7kuk"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eilt er auf ein Ankonerschiff,", "tokens": ["Eilt", "er", "auf", "ein", "An\u00b7ko\u00b7ner\u00b7schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Genannt Sankt Nepomuk.", "tokens": ["Ge\u00b7nannt", "Sankt", "Ne\u00b7po\u00b7muk", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Pachom lief ohne F\u00e4hrlichkeit", "tokens": ["Pa\u00b7chom", "lief", "oh\u00b7ne", "F\u00e4hr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Port von Algier ein,", "tokens": ["Im", "Port", "von", "Al\u00b7gier", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fand den Weg in kurzer Zeit,", "tokens": ["Und", "fand", "den", "Weg", "in", "kur\u00b7zer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ehpaar zu befreyn.", "tokens": ["Das", "Eh\u00b7paar", "zu", "be\u00b7freyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Von Hut und Haube bis zum Strumpf", "tokens": ["Von", "Hut", "und", "Hau\u00b7be", "bis", "zum", "Strumpf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Neumodisch ausstaffiert,", "tokens": ["Neu\u00b7mo\u00b7disch", "aus\u00b7staf\u00b7fiert", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Pater sie nun im Triumph", "tokens": ["Der", "Pa\u00b7ter", "sie", "nun", "im", "Tri\u00b7umph"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Zum guten Urban f\u00fchrt.", "tokens": ["Zum", "gu\u00b7ten", "Ur\u00b7ban", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Sie k\u00fcssen weinend ihm die Schuh", "tokens": ["Sie", "k\u00fcs\u00b7sen", "wei\u00b7nend", "ihm", "die", "Schuh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll heisser Dankbegier.", "tokens": ["Voll", "heis\u00b7ser", "Dank\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ger\u00fchrt sieht er dem Weiblein zu", "tokens": ["Ge\u00b7r\u00fchrt", "sieht", "er", "dem", "Weib\u00b7lein", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und giebt den Segen ihr.", "tokens": ["Und", "giebt", "