{"textgrid.poem.43720": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonne sank", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sank, der Abend naht", "tokens": ["Die", "Son\u00b7ne", "sank", ",", "der", "A\u00b7bend", "naht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stiller wird's auf Stra\u00df' und Pfad,", "tokens": ["Und", "stil\u00b7ler", "wird's", "auf", "Stra\u00df'", "und", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und s\u00fc\u00dfer Friede, Ruh und Rast", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "Frie\u00b7de", ",", "Ruh", "und", "Rast"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Folgt auf des Tages Sorg' und Last.", "tokens": ["Folgt", "auf", "des", "Ta\u00b7ges", "Sor\u00b7g'", "und", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Es schweigt der Wald, es schweigt das Thal:", "tokens": ["Es", "schweigt", "der", "Wald", ",", "es", "schweigt", "das", "Thal", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00f6gel schlafen allzumal,", "tokens": ["Die", "V\u00f6\u00b7gel", "schla\u00b7fen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sogar die Blume nicket ein", "tokens": ["So\u00b7gar", "die", "Blu\u00b7me", "ni\u00b7cket", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlummert bis zum Tag hinein.", "tokens": ["Und", "schlum\u00b7mert", "bis", "zum", "Tag", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon rieselt nieder k\u00fchler Thau", "tokens": ["Schon", "rie\u00b7selt", "nie\u00b7der", "k\u00fch\u00b7ler", "Thau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Halm und Blatt in Feld und Au,", "tokens": ["Auf", "Halm", "und", "Blatt", "in", "Feld", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Laube spielet frische Luft,", "tokens": ["Im", "Lau\u00b7be", "spie\u00b7let", "fri\u00b7sche", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Bl\u00fcth' und Blume spendet Duft.", "tokens": ["Und", "Bl\u00fcth'", "und", "Blu\u00b7me", "spen\u00b7det", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Abendstern mit g\u00fcldnem Schein", "tokens": ["Der", "A\u00b7bends\u00b7tern", "mit", "g\u00fcld\u00b7nem", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Blickt in die stille Welt hinein,", "tokens": ["Blickt", "in", "die", "stil\u00b7le", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als rief' er jedem Herzen zu:", "tokens": ["Als", "rie\u00b7f'", "er", "je\u00b7dem", "Her\u00b7zen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sei still, sei still, und schlaf' auch du!", "tokens": ["Sei", "still", ",", "sei", "still", ",", "und", "schlaf'", "auch", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}