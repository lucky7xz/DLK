{"textgrid.poem.46223": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Herren Grafen Ernsten", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prinz, held, Mars in dem feld, von dem des feinds unzahl", "tokens": ["Prinz", ",", "held", ",", "Mars", "in", "dem", "feld", ",", "von", "dem", "des", "feinds", "un\u00b7zahl"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "niemal ohn schweren sold und leichte zahl entgangen.", "tokens": ["nie\u00b7mal", "ohn", "schwe\u00b7ren", "sold", "und", "leich\u00b7te", "zahl", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "held, dessen wunder rat, Mars, dessen dunderstral", "tokens": ["held", ",", "des\u00b7sen", "wun\u00b7der", "rat", ",", "Mars", ",", "des\u00b7sen", "dun\u00b7der\u00b7stral"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PRELAT", "NN", "VVFIN", "$,", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "verk\u00fcrzet unsers feinds zu langen stolz und prangen:", "tokens": ["ver\u00b7k\u00fcr\u00b7zet", "un\u00b7sers", "feinds", "zu", "lan\u00b7gen", "stolz", "und", "pran\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wan einmal des reichs st\u00e4nd, so jetz in hoher qual", "tokens": ["Wan", "ein\u00b7mal", "des", "reichs", "st\u00e4nd", ",", "so", "jetz", "in", "ho\u00b7her", "qual"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "durch der tyrannen t\u00fcck mit ihrem strick gefangen,", "tokens": ["durch", "der", "ty\u00b7ran\u00b7nen", "t\u00fcck", "mit", "ih\u00b7rem", "strick", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "genie\u00dfend deiner hand und hauptsmacht aus drangsal", "tokens": ["ge\u00b7nie\u00b7\u00dfend", "dei\u00b7ner", "hand", "und", "haupts\u00b7macht", "aus", "dran\u00b7gsal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN", "KON", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "widrum der freiheit liecht und alten ruhm erlangen,", "tokens": ["wid\u00b7rum", "der", "frei\u00b7heit", "liecht", "und", "al\u00b7ten", "ruhm", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Alsdan soll dir zu lob, o einig werter held,", "tokens": ["Als\u00b7dan", "soll", "dir", "zu", "lob", ",", "o", "ei\u00b7nig", "wer\u00b7ter", "held", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "$,", "FM", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "frolockend durch den mund deren, die dir verbunden,", "tokens": ["fro\u00b7lo\u00b7ckend", "durch", "den", "mund", "de\u00b7ren", ",", "die", "dir", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "mit stetem widerhall erschallen alle welt:", "tokens": ["mit", "ste\u00b7tem", "wi\u00b7der\u00b7hall", "er\u00b7schal\u00b7len", "al\u00b7le", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nu hat des h\u00f6chsten gnad geheilet unsre wunden", "tokens": ["Nu", "hat", "des", "h\u00f6chs\u00b7ten", "gnad", "ge\u00b7hei\u00b7let", "uns\u00b7re", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und mit gerechtem Ernst von des besten Mans feld", "tokens": ["und", "mit", "ge\u00b7rech\u00b7tem", "Ernst", "von", "des", "bes\u00b7ten", "Mans", "feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "durch eines helden that die w\u00fctrich \u00fcberwunden.", "tokens": ["durch", "ei\u00b7nes", "hel\u00b7den", "that", "die", "w\u00fct\u00b7rich", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}