{"textgrid.poem.53303": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Herr, wie ist so gar", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Herr, wie ist so gar", "tokens": ["Ach", "Herr", ",", "wie", "ist", "so", "gar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PWAV", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnz\u00e4hlich gro\u00df die Schaar", "tokens": ["Vn\u00b7z\u00e4h\u00b7lich", "gro\u00df", "die", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Feinde, die mich neiden!", "tokens": ["Der", "Fein\u00b7de", ",", "die", "mich", "nei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie viel stehn wieder mich!", "tokens": ["Wie", "viel", "stehn", "wie\u00b7der", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie mu\u00df von vielen sich", "tokens": ["Wie", "mu\u00df", "von", "vie\u00b7len", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "PIAT", "PRF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mein' arme Seele leiden!", "tokens": ["Mein'", "ar\u00b7me", "See\u00b7le", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es wird jhr nachgesagt,", "tokens": ["Es", "wird", "jhr", "nach\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sie sey an Gott verzagt,", "tokens": ["Sie", "sey", "an", "Gott", "ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "M\u00fc\u00df' H\u00fclff- und Trost-lo\u00df stehen:", "tokens": ["M\u00fc\u00df'", "H\u00fclff", "und", "Trost\u00b7lo\u00df", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Jedoch mein Schild und Ruh", "tokens": ["Je\u00b7doch", "mein", "Schild", "und", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Vnd Ehre, Gott, bist Du,", "tokens": ["Vnd", "Eh\u00b7re", ",", "Gott", ",", "bist", "Du", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "VAFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Du wirst mein H\u00e4upt erh\u00f6hen.", "tokens": ["Du", "wirst", "mein", "H\u00e4upt", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich pflege mit Geschrey", "tokens": ["Ich", "pfle\u00b7ge", "mit", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd Thr\u00e4nen mancherley", "tokens": ["Vnd", "Thr\u00e4\u00b7nen", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "PIAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Herren anzuflehen,", "tokens": ["Den", "Her\u00b7ren", "an\u00b7zu\u00b7fle\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So neigt er sich zu mir", "tokens": ["So", "neigt", "er", "sich", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Sions heilgen Zier", "tokens": ["Von", "Si\u00b7ons", "heil\u00b7gen", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mich gn\u00e4dig anzusehen.", "tokens": ["Mich", "gn\u00e4\u00b7dig", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich schlaffe sicher ein,", "tokens": ["Ich", "schlaf\u00b7fe", "si\u00b7cher", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Vnd kan auch wache seyn,", "tokens": ["Vnd", "kan", "auch", "wa\u00b7che", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJA", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Denn Gott ist mir zur Seiten,", "tokens": ["Denn", "Gott", "ist", "mir", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ich seh' ohn Schrecken an", "tokens": ["Ich", "seh'", "ohn", "Schre\u00b7cken", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Viel hundert tausent Mann,", "tokens": ["Viel", "hun\u00b7dert", "tau\u00b7sent", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die mich vmbher bestreiten.", "tokens": ["Die", "mich", "vm\u00b7bher", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Herr auff! vnd hilff, mein Gott,", "tokens": ["Herr", "auff", "!", "vnd", "hilff", ",", "mein", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "KON", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir wieder diese Rott!", "tokens": ["Mir", "wie\u00b7der", "die\u00b7se", "Rott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vmbgib mich allerwegen!", "tokens": ["Vmb\u00b7gib", "mich", "al\u00b7ler\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast, o h\u00f6chster Freund,", "tokens": ["Du", "hast", ",", "o", "h\u00f6chs\u00b7ter", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die s\u00e4mptlich, so mir feind,", "tokens": ["Die", "s\u00e4mpt\u00b7lich", ",", "so", "mir", "feind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADV", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gestrafft mit Backen-Schl\u00e4gen.", "tokens": ["Ge\u00b7strafft", "mit", "Ba\u00b7cken\u00b7Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der S\u00fcnder b\u00f6sen Schaar", "tokens": ["Der", "S\u00fcn\u00b7der", "b\u00f6\u00b7sen", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Hast du die Z\u00e4hne gar", "tokens": ["Hast", "du", "die", "Z\u00e4h\u00b7ne", "gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zermalmt vnd auffgerieben:", "tokens": ["Zer\u00b7malmt", "vnd", "auff\u00b7ge\u00b7rie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Gott ist, der H\u00fclff vnd Rath", "tokens": ["Gott", "ist", ",", "der", "H\u00fclff", "vnd", "Rath"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Vnd reichen Segen hat", "tokens": ["Vnd", "rei\u00b7chen", "Se\u00b7gen", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "F\u00fcr alle, die Ihn lieben.", "tokens": ["F\u00fcr", "al\u00b7le", ",", "die", "Ihn", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}