, "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eich nehme Frau und Mann!", "tokens": ["\u201e", "ich", "neh\u00b7me", "Frau", "und", "Mann", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201edie Kinderlein zieh ich herf\u00fcr,", "tokens": ["\u201e", "die", "Kin\u00b7der\u00b7lein", "zieh", "ich", "her\u00b7f\u00fcr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eein jedes mu\u00df mir folgen,", "tokens": ["\u201e", "ein", "je\u00b7des", "mu\u00df", "mir", "fol\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ewenn ich klopf an die Th\u00fcr.\u201c", "tokens": ["\u201e", "wenn", "ich", "klopf", "an", "die", "Th\u00fcr.", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["$(", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.16": {"line.1": {"text": "Er nahm sie in der Mitten,", "tokens": ["Er", "nahm", "sie", "in", "der", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sie am schw\u00e4chsten was,", "tokens": ["Da", "sie", "am", "schw\u00e4chs\u00b7ten", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "PWS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es half bey ihm kein Bitten,", "tokens": ["Es", "half", "bey", "ihm", "kein", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er warf sie in das Gra\u00df,", "tokens": ["Er", "warf", "sie", "in", "das", "Gra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und r\u00fchrte an ihr junges Herz", "tokens": ["Und", "r\u00fchr\u00b7te", "an", "ihr", "jun\u00b7ges", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da liegt das M\u00e4gdlein zarte,", "tokens": ["Da", "liegt", "das", "M\u00e4gd\u00b7lein", "zar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Voll bittrer Angst und Schmerz.", "tokens": ["Voll", "bit\u00b7trer", "Angst", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ihr Farb that sie verwandlen,", "tokens": ["Ihr", "Farb", "that", "sie", "ver\u00b7wand\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Aeuglein sie verkehrt", "tokens": ["Ihr", "A\u00b7e\u00b7u\u00b7glein", "sie", "ver\u00b7kehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einer Seit zur andern", "tokens": ["Von", "ei\u00b7ner", "Seit", "zur", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warf sie sich auf der Erd,", "tokens": ["Warf", "sie", "sich", "auf", "der", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "All Wollust ihr vergangen war,", "tokens": ["All", "Wol\u00b7lust", "ihr", "ver\u00b7gan\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Bl\u00fcmlein mehr wollt holen", "tokens": ["Kein", "Bl\u00fcm\u00b7lein", "mehr", "wollt", "ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohl aus dem gr\u00fcnen Gra\u00df.", "tokens": ["Wohl", "aus", "dem", "gr\u00fc\u00b7nen", "Gra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}