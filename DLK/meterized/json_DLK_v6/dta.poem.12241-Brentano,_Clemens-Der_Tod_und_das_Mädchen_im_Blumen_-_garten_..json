{"dta.poem.12241": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod und das M\u00e4dchen im Blumen -  \n  garten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein M\u00e4gdlein zarte               ", "tokens": ["Es", "ging", "ein", "M\u00e4gd\u00b7lein", "zar\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fr\u00fch in der Morgenstund", "tokens": ["Fr\u00fch", "in", "der", "Mor\u00b7gen\u00b7stund"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "In einen Blumengarten,", "tokens": ["In", "ei\u00b7nen", "Blu\u00b7men\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frisch, fr\u00f6hlich und gesund,", "tokens": ["Frisch", ",", "fr\u00f6h\u00b7lich", "und", "ge\u00b7sund", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Bl\u00fcmlein es viel brechen wollt,", "tokens": ["Der", "Bl\u00fcm\u00b7lein", "es", "viel", "bre\u00b7chen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daraus ein Kranz zu machen,", "tokens": ["Da\u00b7raus", "ein", "Kranz", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Silber und von Gold.", "tokens": ["Von", "Sil\u00b7ber", "und", "von", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da kam herzu geschlichen", "tokens": ["Da", "kam", "her\u00b7zu", "ge\u00b7schli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein gar erschrecklich Mann,", "tokens": ["Ein", "gar", "er\u00b7schreck\u00b7lich", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Farb war ihm verblichen,", "tokens": ["Die", "Farb", "war", "ihm", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein' Kleider hatt' er an,", "tokens": ["Kein'", "Klei\u00b7der", "hatt'", "er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er hatt' kein Fleisch, kein Blut, kein Haar,", "tokens": ["Er", "hatt'", "kein", "Fleisch", ",", "kein", "Blut", ",", "kein", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es war an ihm verdorret", "tokens": ["Es", "war", "an", "ihm", "ver\u00b7dor\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Haut, und Flechsen gar.", "tokens": ["Sein", "Haut", ",", "und", "Flech\u00b7sen", "gar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gar h\u00e4\u00dflich th\u00e4t er sehen,", "tokens": ["Gar", "h\u00e4\u00df\u00b7lich", "th\u00e4t", "er", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Scheu\u00dflich war sein Gesicht,", "tokens": ["Scheu\u00df\u00b7lich", "war", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er weiset seine Z\u00e4hne", "tokens": ["Er", "wei\u00b7set", "sei\u00b7ne", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und that noch einen Schritt,", "tokens": ["Und", "that", "noch", "ei\u00b7nen", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl zu dem M\u00e4gdlein zart,", "tokens": ["Wohl", "zu", "dem", "M\u00e4gd\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das schier f\u00fcr gro\u00dfen Aengsten,", "tokens": ["Das", "schier", "f\u00fcr", "gro\u00b7\u00dfen", "A\u00b7engs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Des grimmen Todes ward.", "tokens": ["Des", "grim\u00b7men", "To\u00b7des", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201enun schick dich M\u00e4gdlein, schick dich,", "tokens": ["\u201e", "nun", "schick", "dich", "M\u00e4gd\u00b7lein", ",", "schick", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edu must mit mir an Tanz!", "tokens": ["\u201e", "du", "must", "mit", "mir", "an", "Tanz", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich will dir bald aufsetzen,", "tokens": ["\u201e", "ich", "will", "dir", "bald", "auf\u00b7set\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eein wundersch\u00f6nen Kranz,", "tokens": ["\u201e", "ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Kranz", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eder wird dir nicht gebunden sein", "tokens": ["\u201e", "der", "wird", "dir", "nicht", "ge\u00b7bun\u00b7den", "sein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201evon wohlriechenden Kr\u00e4utern,", "tokens": ["\u201e", "von", "wohl\u00b7rie\u00b7chen\u00b7den", "Kr\u00e4u\u00b7tern", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "\u201eund zarten Bl\u00fcmelein.", "tokens": ["\u201e", "und", "zar\u00b7ten", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201eder Kranz, den ich aufsetze,", "tokens": ["\u201e", "der", "Kranz", ",", "den", "ich", "auf\u00b7set\u00b7ze", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eder hei\u00dft die Sterblichkeit;", "tokens": ["\u201e", "der", "hei\u00dft", "die", "Sterb\u00b7lich\u00b7keit", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edu wirst nicht seyn die letzte,", "tokens": ["\u201e", "du", "wirst", "nicht", "seyn", "die", "letz\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "VAINF", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edie ihn tr\u00e4gt auf dem Haupt;", "tokens": ["\u201e", "die", "ihn", "tr\u00e4gt", "auf", "dem", "Haupt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "\u201ewie viel allhie gebohren seyn,", "tokens": ["\u201e", "wie", "viel", "all\u00b7hie", "ge\u00b7boh\u00b7ren", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edie m\u00fcssen mit mir tanzen", "tokens": ["\u201e", "die", "m\u00fcs\u00b7sen", "mit", "mir", "tan\u00b7zen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ewohl um das Kr\u00e4nzelein.", "tokens": ["\u201e", "wohl", "um", "das", "Kr\u00e4n\u00b7zel\u00b7ein", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201eder W\u00fcrmer in der Erde", "tokens": ["\u201e", "der", "W\u00fcr\u00b7mer", "in", "der", "Er\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eist eine gro\u00dfe Zahl,", "tokens": ["\u201e", "ist", "ei\u00b7ne", "gro\u00b7\u00dfe", "Zahl", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edie werden dir verzehren", "tokens": ["\u201e", "die", "wer\u00b7den", "dir", "ver\u00b7zeh\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edein Sch\u00f6nheit allzumahl,", "tokens": ["\u201e", "dein", "Sch\u00f6n\u00b7heit", "all\u00b7zu\u00b7mahl", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201esie werden deine Bl\u00fcmlein seyn,", "tokens": ["\u201e", "sie", "wer\u00b7den", "dei\u00b7ne", "Bl\u00fcm\u00b7lein", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edas Gold, und auch die Perlen,", "tokens": ["\u201e", "das", "Gold", ",", "und", "auch", "die", "Per\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201esilber und Edelstein.", "tokens": ["\u201e", "sil\u00b7ber", "und", "E\u00b7del\u00b7stein", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "\u201ewillst du mich gerne kennen", "tokens": ["\u201e", "willst", "du", "mich", "ger\u00b7ne", "ken\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eund wissen, wer ich sey?", "tokens": ["\u201e", "und", "wis\u00b7sen", ",", "wer", "ich", "sey", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVINF", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eso h\u00f6r mein Nahmen nennen,", "tokens": ["\u201e", "so", "h\u00f6r", "mein", "Nah\u00b7men", "nen\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-----", "measure": "dactylic.init"}, "line.4": {"text": "\u201ewill dir ihn sagen frey:", "tokens": ["\u201e", "will", "dir", "ihn", "sa\u00b7gen", "frey", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u201eder grimme Tod werd ich genannt,", "tokens": ["\u201e", "der", "grim\u00b7me", "Tod", "werd", "ich", "ge\u00b7nannt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eund bin in allen Landen,", "tokens": ["\u201e", "und", "bin", "in", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201egar weit und breit bekannt.", "tokens": ["\u201e", "gar", "weit", "und", "breit", "be\u00b7kannt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u201edie Sense ist mein Wappen,", "tokens": ["\u201e", "die", "Sen\u00b7se", "ist", "mein", "Wap\u00b7pen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edas ich mit Rechte f\u00fchr,", "tokens": ["\u201e", "das", "ich", "mit", "Rech\u00b7te", "f\u00fchr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edamit thu ich anklopfen", "tokens": ["\u201e", "da\u00b7mit", "thu", "ich", "an\u00b7klop\u00b7fen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PAV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ejedem an seine Th\u00fcr,", "tokens": ["\u201e", "je\u00b7dem", "an", "sei\u00b7ne", "Th\u00fcr", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "\u201eund wenn sein Zeit ist kommen schon,", "tokens": ["\u201e", "und", "wenn", "sein", "Zeit", "ist", "kom\u00b7men", "schon", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPOSAT", "NN", "VAFIN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201esp\u00e4t, fr\u00fch, und in der Mitten,", "tokens": ["\u201e", "sp\u00e4t", ",", "fr\u00fch", ",", "und", "in", "der", "Mit\u00b7ten", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201e'S hilft nichts, er mu\u00df davon!\u201c", "tokens": ["\u201e", "'s", "hilft", "nichts", ",", "er", "mu\u00df", "da\u00b7von", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "VVFIN", "PIS", "$,", "PPER", "VMFIN", "PAV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Das M\u00e4gdlein voller Schmerzen,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "vol\u00b7ler", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll bittrer