{"textgrid.poem.52936": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "4. Abschied von Wien", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bleich, wie hold, wie schmachtend hingegossen", "tokens": ["Wie", "bleich", ",", "wie", "hold", ",", "wie", "schmach\u00b7tend", "hin\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie daliegt, die gef\u00e4hrliche Sirene,", "tokens": ["Sie", "da\u00b7liegt", ",", "die", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "Si\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die dunklen Augen tr\u00e4umerisch geschlossen,", "tokens": ["Die", "dunk\u00b7len", "Au\u00b7gen", "tr\u00e4u\u00b7me\u00b7risch", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Haupt geneigt an ihrer Berge Lehne!", "tokens": ["Das", "Haupt", "ge\u00b7neigt", "an", "ih\u00b7rer", "Ber\u00b7ge", "Leh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es geht ein s\u00fc\u00dfes, winkendes Erwarten", "tokens": ["Es", "geht", "ein", "s\u00fc\u00b7\u00dfes", ",", "win\u00b7ken\u00b7des", "Er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Nachtigallen-Locken durch die Flur,", "tokens": ["Wie", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7Lo\u00b7cken", "durch", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Brunnen murmeln heimlich in den Garten,", "tokens": ["Die", "Brun\u00b7nen", "mur\u00b7meln", "heim\u00b7lich", "in", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Zweige lallen: Komm, o komm doch nur!", "tokens": ["Die", "Zwei\u00b7ge", "lal\u00b7len", ":", "Komm", ",", "o", "komm", "doch", "nur", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "VVFIN", "$,", "FM", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entschlafen sind Sankt Stephans W\u00e4chtersorgen,", "tokens": ["Ent\u00b7schla\u00b7fen", "sind", "Sankt", "Ste\u00b7phans", "W\u00e4ch\u00b7ter\u00b7sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstummt die Mahnungen des treuen Flusses;", "tokens": ["Ver\u00b7stummt", "die", "Mah\u00b7nun\u00b7gen", "des", "treu\u00b7en", "Flus\u00b7ses", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie fern der n\u00fcchterne, der strenge Morgen,", "tokens": ["Wie", "fern", "der", "n\u00fcch\u00b7ter\u00b7ne", ",", "der", "stren\u00b7ge", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie lang die Nacht entfesselten Genusses!", "tokens": ["Wie", "lang", "die", "Nacht", "ent\u00b7fes\u00b7sel\u00b7ten", "Ge\u00b7nus\u00b7ses", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Nun hat sie abgestreift die letzte H\u00fclle,", "tokens": ["Nun", "hat", "sie", "ab\u00b7ge\u00b7streift", "die", "letz\u00b7te", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den gr\u00fcnen G\u00fcrtel der Glacis gel\u00f6st,", "tokens": ["Den", "gr\u00fc\u00b7nen", "G\u00fcr\u00b7tel", "der", "Gla\u00b7cis", "ge\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Frei gl\u00e4nzt und nackt der Schultern Marmorf\u00fclle", "tokens": ["Frei", "gl\u00e4nzt", "und", "nackt", "der", "Schul\u00b7tern", "Mar\u00b7mor\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Arm und Busen jedem Wunsch entbl\u00f6\u00dft.", "tokens": ["Und", "Arm", "und", "Bu\u00b7sen", "je\u00b7dem", "Wunsch", "ent\u00b7bl\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh, durch verhangne Fenster schimmert l\u00fcstern", "tokens": ["Sieh", ",", "durch", "ver\u00b7hang\u00b7ne", "Fens\u00b7ter", "schim\u00b7mert", "l\u00fcs\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mond, im Laube rauscht's wie Regentropfen,", "tokens": ["Der", "Mond", ",", "im", "Lau\u00b7be", "rauscht's", "wie", "Re\u00b7gen\u00b7trop\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verbotne Schritte rascheln, K\u00fcsse fl\u00fcstern,", "tokens": ["Ver\u00b7bot\u00b7ne", "Schrit\u00b7te", "ra\u00b7scheln", ",", "K\u00fcs\u00b7se", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Herz am Herzen h\u00f6rt sich gl\u00fchend klopfen!", "tokens": ["Und", "Herz", "am", "Her\u00b7zen", "h\u00f6rt", "sich", "gl\u00fc\u00b7hend", "klop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Meer von Liebe schl\u00e4gt in hei\u00dfen Wogen", "tokens": ["Ein", "Meer", "von", "Lie\u00b7be", "schl\u00e4gt", "in", "hei\u00b7\u00dfen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hoch \u00fcber dem entz\u00fcckten Tale hin,", "tokens": ["Hoch", "\u00fc\u00b7ber", "dem", "ent\u00b7z\u00fcck\u00b7ten", "Ta\u00b7le", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum Vorhang wandelt sich des Himmels Bogen,", "tokens": ["Zum", "Vor\u00b7hang", "wan\u00b7delt", "sich", "des", "Him\u00b7mels", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ganz Wien in eine Venus-Priesterin!", "tokens": ["Ganz", "Wi\u00b7en", "in", "ei\u00b7ne", "Ve\u00b7nus\u00b7Pries\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Buhldirne Du, die hinter der Gardine", "tokens": ["Buhl\u00b7dir\u00b7ne", "Du", ",", "die", "hin\u00b7ter", "der", "Gar\u00b7di\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Alln\u00e4chtlich ihre Phallos-Feste feiert,", "tokens": ["All\u00b7n\u00e4cht\u00b7lich", "ih\u00b7re", "Phal\u00b7los\u00b7Fes\u00b7te", "fei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und morgens fr\u00fch mit Magdalenen-Miene", "tokens": ["Und", "mor\u00b7gens", "fr\u00fch", "mit", "Mag\u00b7da\u00b7le\u00b7nen\u00b7Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Beichtstuhl heuchelnd ihr \u00bbAbsolve\u00ab leiert;", "tokens": ["Im", "Beicht\u00b7stuhl", "heu\u00b7chelnd", "ihr", "\u00bb", "Ab\u00b7sol\u00b7ve", "\u00ab", "lei\u00b7ert", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "PPER", "$(", "NN", "$(", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kannst Du mit Wollust nur ein Leben w\u00fcrzen,", "tokens": ["Kannst", "Du", "mit", "Wol\u00b7lust", "nur", "ein", "Le\u00b7ben", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem jede geist'ge Kraft und Weihe fehlt,", "tokens": ["Dem", "je\u00b7de", "geist'\u00b7ge", "Kraft", "und", "Wei\u00b7he", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und nur in des Genusses Abgrund st\u00fcrzen,", "tokens": ["Und", "nur", "in", "des", "Ge\u00b7nus\u00b7ses", "Ab\u00b7grund", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von keinem heiligeren Drang beseelt?", "tokens": ["Von", "kei\u00b7nem", "hei\u00b7li\u00b7ge\u00b7ren", "Drang", "be\u00b7seelt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ja, Du bist sch\u00f6n in Deinem Rosenkranze,", "tokens": ["Ja", ",", "Du", "bist", "sch\u00f6n", "in", "Dei\u00b7nem", "Ro\u00b7sen\u00b7kran\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bl\u00fcte der Verhei\u00dfung auf den Wangen,", "tokens": ["Die", "Bl\u00fc\u00b7te", "der", "Ver\u00b7hei\u00b7\u00dfung", "auf", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Du vor\u00fcberfliegst im wilden Tanze,", "tokens": ["Wenn", "Du", "vor\u00b7\u00fc\u00b7berf\u00b7liegst", "im", "wil\u00b7den", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begehrlich von der M\u00e4nner Brunst umfangen!", "tokens": ["Be\u00b7gehr\u00b7lich", "von", "der", "M\u00e4n\u00b7ner", "Brunst", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In Deinem Scho\u00df sich welt-vergessen wiegen,", "tokens": ["In", "Dei\u00b7nem", "Scho\u00df", "sich", "welt\u00b7ver\u00b7ges\u00b7sen", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Versinken gehn in weicher Arme Bucht,", "tokens": ["Ver\u00b7sin\u00b7ken", "gehn", "in", "wei\u00b7cher", "Ar\u00b7me", "Bucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Deinem Zauber taumelgleich erliegen, \u2013", "tokens": ["Und", "Dei\u00b7nem", "Zau\u00b7ber", "tau\u00b7mel\u00b7gleich", "er\u00b7lie\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wohl ist's ein Ziel, das G\u00f6tter selbst versucht.", "tokens": ["Wohl", "ist's", "ein", "Ziel", ",", "das", "G\u00f6t\u00b7ter", "selbst", "ver\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich fliehe, Weib, um nicht vor Dir zu knien,", "tokens": ["Ich", "flie\u00b7he", ",", "Weib", ",", "um", "nicht", "vor", "Dir", "zu", "kni\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "KOUI", "PTKNEG", "APPR", "PPER", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch einer von den Proselyten-Scharen;", "tokens": ["Auch", "ei\u00b7ner", "von", "den", "Pro\u00b7se\u00b7ly\u00b7ten\u00b7Scha\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wirst mich nicht auf Deinen Purpur ziehen,", "tokens": ["Du", "wirst", "mich", "nicht", "auf", "Dei\u00b7nen", "Pur\u00b7pur", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weib Potiphars, \u2013 la\u00df meinen Mantel fahren!", "tokens": ["Weib", "Po\u00b7ti\u00b7phars", ",", "\u2013", "la\u00df", "mei\u00b7nen", "Man\u00b7tel", "fah\u00b7ren", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "$(", "VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor meinen Blicken schwebt in keuschem Lichte", "tokens": ["Vor", "mei\u00b7nen", "Bli\u00b7cken", "schwebt", "in", "keu\u00b7schem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein andres Bild, das meiner Seelen-Braut,", "tokens": ["Ein", "and\u00b7res", "Bild", ",", "das", "mei\u00b7ner", "See\u00b7len\u00b7Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der hab' ich mich im Leben, im Gedichte", "tokens": ["Der", "hab'", "ich", "mich", "im", "Le\u00b7ben", ",", "im", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PRF", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit deutschem Wort auf ewig angetraut.", "tokens": ["Mit", "deut\u00b7schem", "Wort", "auf", "e\u00b7wig", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ihr Aug' ist sch\u00f6n, ob minder sch\u00f6n, als Deines,", "tokens": ["Ihr", "Aug'", "ist", "sch\u00f6n", ",", "ob", "min\u00b7der", "sch\u00f6n", ",", "als", "Dei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KOUS", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es strahlt nur Frieden, Deines flammt Entz\u00fccken,", "tokens": ["Es", "strahlt", "nur", "Frie\u00b7den", ",", "Dei\u00b7nes", "flammt", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "PPOSAT", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Ku\u00df ist Glut, der ihre nur ein reines,", "tokens": ["Dein", "Ku\u00df", "ist", "Glut", ",", "der", "ih\u00b7re", "nur", "ein", "rei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PRELS", "PPOSAT", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein hauchendes und fl\u00fcchtiges Begl\u00fccken;", "tokens": ["Ein", "hau\u00b7chen\u00b7des", "und", "fl\u00fcch\u00b7ti\u00b7ges", "Be\u00b7gl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du neigst Dich ganz in dulde