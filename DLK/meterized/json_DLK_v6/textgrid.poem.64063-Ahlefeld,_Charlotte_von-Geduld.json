{"textgrid.poem.64063": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Geduld", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umstarrt vom Eis des Norden", "tokens": ["Um\u00b7starrt", "vom", "Eis", "des", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In tr\u00fcber Einsamkeit,", "tokens": ["In", "tr\u00fc\u00b7ber", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist mir ein Bl\u00fcmchen worden", "tokens": ["Ist", "mir", "ein", "Bl\u00fcm\u00b7chen", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das duftend mich erfreut.", "tokens": ["Das", "duf\u00b7tend", "mich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Thaue bittrer Thr\u00e4nen", "tokens": ["Im", "Thau\u00b7e", "bit\u00b7trer", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entfaltete es sich,", "tokens": ["Ent\u00b7fal\u00b7te\u00b7te", "es", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und heilte von dem Sehnen", "tokens": ["Und", "heil\u00b7te", "von", "dem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach bessrer Zukunft mich.", "tokens": ["Nach", "bess\u00b7rer", "Zu\u00b7kunft", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Tief trag ich es verborgen", "tokens": ["Tief", "trag", "ich", "es", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In der verschwiegnen Brust.", "tokens": ["In", "der", "ver\u00b7schwieg\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wandelt's meine Sorgen", "tokens": ["Da", "wan\u00b7delt's", "mei\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In stiller Wehmuth Lust.", "tokens": ["In", "stil\u00b7ler", "Weh\u00b7muth", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Um mein Geschick zu tragen", "tokens": ["Um", "mein", "Ge\u00b7schick", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gab mir's des Himmels Huld.", "tokens": ["Gab", "mir's", "des", "Him\u00b7mels", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie heisst es? wirst Du fragen.", "tokens": ["Wie", "heisst", "es", "?", "wirst", "Du", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Bl\u00fcmchen heisst \u2013 \u2013 Geduld.", "tokens": ["Das", "Bl\u00fcm\u00b7chen", "heisst", "\u2013", "\u2013", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "$(", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}