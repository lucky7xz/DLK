{"dta.poem.204": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Arlinden,  da sie ihn liebkosete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Arlinde, liebstes Kind/ dein holdes Augen-wincken/", "tokens": ["Ar\u00b7lin\u00b7de", ",", "liebs\u00b7tes", "Kind", "/", "dein", "hol\u00b7des", "Au\u00b7gen\u00b7win\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein lachend Angesicht macht meinen Geist erfreut/", "tokens": ["Dein", "la\u00b7chend", "An\u00b7ge\u00b7sicht", "macht", "mei\u00b7nen", "Geist", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein angenehmer Mund so s\u00fcsses Zucker streut/", "tokens": ["Dein", "an\u00b7ge\u00b7neh\u00b7mer", "Mund", "so", "s\u00fcs\u00b7ses", "Zu\u00b7cker", "streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df mir die Galle wird als ", "tokens": ["Da\u00df", "mir", "die", "Gal\u00b7le", "wird", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "KOKOM"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Augen Blitzen wird zu einen sanfften Blincken/", "tokens": ["Der", "Au\u00b7gen", "Blit\u00b7zen", "wird", "zu", "ei\u00b7nen", "sanff\u00b7ten", "Blin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dein Erquickungs-Strahl nicht mit ", "tokens": ["Und", "dein", "Er\u00b7qui\u00b7ckungs\u00b7Strahl", "nicht", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein heller ", "tokens": ["Ein", "hel\u00b7ler"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Der seinen Anmuths Glantz l\u00e4st auf mein Hertze sincken.", "tokens": ["Der", "sei\u00b7nen", "An\u00b7muths", "Glantz", "l\u00e4st", "auf", "mein", "Hert\u00b7ze", "sin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sehe schon den Port da angenehm zu landen/", "tokens": ["Ich", "se\u00b7he", "schon", "den", "Port", "da", "an\u00b7ge\u00b7nehm", "zu", "lan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NE", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Wo an der Br\u00fcste-Fels kein Schiffmann pflegt zu strandem", "tokens": ["Wo", "an", "der", "Br\u00fcs\u00b7te\u00b7Fels", "kein", "Schiff\u00b7mann", "pflegt", "zu", "stran\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und vielmehr an das Land der gr\u00f6\u00dften L\u00fcste steigt.", "tokens": ["Und", "viel\u00b7mehr", "an", "das", "Land", "der", "gr\u00f6\u00df\u00b7ten", "L\u00fcs\u00b7te", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Schenckt nun dein Augen-Glantz mir ferner sanfften Schein", "tokens": ["Schenckt", "nun", "dein", "Au\u00b7gen\u00b7Glantz", "mir", "fer\u00b7ner", "sanff\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So treibe ich gew\u00fcnscht in deinen Haven ein.", "tokens": ["So", "trei\u00b7be", "ich", "ge\u00b7w\u00fcnscht", "in", "dei\u00b7nen", "Ha\u00b7ven", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}