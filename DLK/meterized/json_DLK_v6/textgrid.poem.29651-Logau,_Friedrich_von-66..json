{"textgrid.poem.29651": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Simplex ist ein grober Mann; was er sagt, das pflegt zu seyn;", "tokens": ["Sim\u00b7plex", "ist", "ein", "gro\u00b7ber", "Mann", ";", "was", "er", "sagt", ",", "das", "pflegt", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Duplex ist ein Hofe-Mann; was er sagt, hat blossen Schein;", "tokens": ["Du\u00b7plex", "ist", "ein", "Ho\u00b7fe\u00b7Mann", ";", "was", "er", "sagt", ",", "hat", "blos\u00b7sen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Demnach acht ich Grobheit viel, H\u00f6fligkeit hingegen klein.", "tokens": ["Dem\u00b7nach", "acht", "ich", "Grob\u00b7heit", "viel", ",", "H\u00f6f\u00b7lig\u00b7keit", "hin\u00b7ge\u00b7gen", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "CARD", "PPER", "NN", "ADV", "$,", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}