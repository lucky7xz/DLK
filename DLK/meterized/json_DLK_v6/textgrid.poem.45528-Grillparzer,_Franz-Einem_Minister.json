{"textgrid.poem.45528": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Einem Minister", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du dirigierst unsre Bretterwelt", "tokens": ["Du", "di\u00b7ri\u00b7gierst", "uns\u00b7re", "Bret\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und hast den Erfolg im Sack.", "tokens": ["Und", "hast", "den", "Er\u00b7folg", "im", "Sack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo irgend auftritt dein erster Held,", "tokens": ["Wo", "ir\u00b7gend", "auf\u00b7tritt", "dein", "ers\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sorgst du f\u00fcr eine Claque.", "tokens": ["Sorgst", "du", "f\u00fcr", "ei\u00b7ne", "Cla\u00b7que", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}