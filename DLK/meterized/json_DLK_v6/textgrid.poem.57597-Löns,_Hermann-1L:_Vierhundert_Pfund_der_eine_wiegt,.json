{"textgrid.poem.57597": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vierhundert Pfund der eine wiegt,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vierhundert Pfund der eine wiegt,", "tokens": ["Vier\u00b7hun\u00b7dert", "Pfund", "der", "ei\u00b7ne", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dreihundert zieren den andern,", "tokens": ["Drei\u00b7hun\u00b7dert", "zie\u00b7ren", "den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich sehe sie t\u00e4glich langsamen Schritts", "tokens": ["Ich", "se\u00b7he", "sie", "t\u00e4g\u00b7lich", "lang\u00b7sa\u00b7men", "Schritts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00dcber den Domplatz wandern.", "tokens": ["\u00dc\u00b7ber", "den", "Dom\u00b7platz", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Zwei Vollmonde, breit und w\u00fcrdevoll", "tokens": ["Zwei", "Voll\u00b7mon\u00b7de", ",", "breit", "und", "w\u00fcr\u00b7de\u00b7voll"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die rosenbl\u00fchsamen Gesichter,", "tokens": ["Die", "ro\u00b7sen\u00b7bl\u00fch\u00b7sa\u00b7men", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4re ich leider kein Dichter schon,", "tokens": ["Und", "w\u00e4\u00b7re", "ich", "lei\u00b7der", "kein", "Dich\u00b7ter", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Begeisterte dies mich zum Dichter!", "tokens": ["Be\u00b7geis\u00b7ter\u00b7te", "dies", "mich", "zum", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und diese B\u00e4uche! Voll Staunen bleib'", "tokens": ["Und", "die\u00b7se", "B\u00e4u\u00b7che", "!", "Voll", "Stau\u00b7nen", "bleib'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$.", "ADJD", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich ehrerbietig stehen \u2013", "tokens": ["Ich", "ehr\u00b7er\u00b7bie\u00b7tig", "ste\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seit zwei Jahrzehnten k\u00f6nnen sie nicht", "tokens": ["Seit", "zwei", "Jahr\u00b7zehn\u00b7ten", "k\u00f6n\u00b7nen", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihre eigenen Beine mehr sehen.", "tokens": ["Ih\u00b7re", "ei\u00b7ge\u00b7nen", "Bei\u00b7ne", "mehr", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Was gibt das f\u00fcr reizende Engelchen,", "tokens": ["Was", "gibt", "das", "f\u00fcr", "rei\u00b7zen\u00b7de", "En\u00b7gel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nur mu\u00df sich der Herrgott bequemen,", "tokens": ["Nur", "mu\u00df", "sich", "der", "Herr\u00b7gott", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Statt Taubenfl\u00fcgel die Fittiche", "tokens": ["Statt", "Tau\u00b7ben\u00b7fl\u00fc\u00b7gel", "die", "Fit\u00b7ti\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vom Vogel Greif zu nehmen.", "tokens": ["Vom", "Vo\u00b7gel", "Greif", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und da\u00df man trotzdem nicht an Wunder glaubt,", "tokens": ["Und", "da\u00df", "man", "trotz\u00b7dem", "nicht", "an", "Wun\u00b7der", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PAV", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ist es, was mich wundert \u2013", "tokens": ["Das", "ist", "es", ",", "was", "mich", "wun\u00b7dert", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor diesen Begnadeten fall aufs Knie,", "tokens": ["Vor", "die\u00b7sen", "Be\u00b7gna\u00b7de\u00b7ten", "fall", "aufs", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ungl\u00e4ubiges Jahrhundert.", "tokens": ["Un\u00b7gl\u00e4u\u00b7bi\u00b7ges", "Jahr\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}