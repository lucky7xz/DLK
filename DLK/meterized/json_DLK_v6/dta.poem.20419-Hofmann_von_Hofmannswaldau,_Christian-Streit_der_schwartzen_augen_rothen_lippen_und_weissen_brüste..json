{"dta.poem.20419": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchmt/ schwestern/ was ihr wolt/ den ruhm von unsern flammen", "tokens": ["R\u00fchmt", "/", "schwes\u00b7tern", "/", "was", "ihr", "wolt", "/", "den", "ruhm", "von", "un\u00b7sern", "flam\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "VVINF", "$(", "PWS", "PPER", "VMFIN", "$(", "ART", "NN", "APPR", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat keine zeit verletzt/ kein winter abgethan;", "tokens": ["Hat", "kei\u00b7ne", "zeit", "ver\u00b7letzt", "/", "kein", "win\u00b7ter", "ab\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$(", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier steht die lieblichkeit und auch die krafft beysammen/", "tokens": ["Hier", "steht", "die", "lieb\u00b7lich\u00b7keit", "und", "auch", "die", "krafft", "bey\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dencken auff ein band/ das hertzen fangen kan.", "tokens": ["Und", "den\u00b7cken", "auff", "ein", "band", "/", "das", "hert\u00b7zen", "fan\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die schl\u00fcssel hengen hier zu tausend m\u00e4nner hertzen/", "tokens": ["Die", "schl\u00fcs\u00b7sel", "hen\u00b7gen", "hier", "zu", "tau\u00b7send", "m\u00e4n\u00b7ner", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die liebe hat bey uns das zeughaus ihrer macht;", "tokens": ["Die", "lie\u00b7be", "hat", "bey", "uns", "das", "zeug\u00b7haus", "ih\u00b7rer", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "PPER", "ART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Cupido holet hier das feuer zu den kertzen;", "tokens": ["Cu\u00b7pi\u00b7do", "ho\u00b7let", "hier", "das", "feu\u00b7er", "zu", "den", "kert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja/ lieben haben wir auff diese welt gebracht.", "tokens": ["Ja", "/", "lie\u00b7ben", "ha\u00b7ben", "wir", "auff", "die\u00b7se", "welt", "ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VVINF", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}