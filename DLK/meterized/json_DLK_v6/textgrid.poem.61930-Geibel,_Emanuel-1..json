{"textgrid.poem.61930": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Fluren dahin im Schneesturm wandelt der Winter,", "tokens": ["\u00dc\u00b7ber", "die", "Flu\u00b7ren", "da\u00b7hin", "im", "Schnee\u00b7sturm", "wan\u00b7delt", "der", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Mit eint\u00f6nigem Wei\u00df deckt er die Farben des Jahrs;", "tokens": ["Mit", "ein\u00b7t\u00f6\u00b7ni\u00b7gem", "Wei\u00df", "deckt", "er", "die", "Far\u00b7ben", "des", "Jahrs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Statt der Rosen im Garten erbl\u00fchn Eisblumen am Fenster,", "tokens": ["Statt", "der", "Ro\u00b7sen", "im", "Gar\u00b7ten", "er\u00b7bl\u00fchn", "Eis\u00b7blu\u00b7men", "am", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVINF", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und am Herde den Platz r\u00e4umt der Betrachtung das Lied.", "tokens": ["Und", "am", "Her\u00b7de", "den", "Platz", "r\u00e4umt", "der", "Be\u00b7trach\u00b7tung", "das", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Nicht die Empfindung allein, auch was in ernster Erfahrung", "tokens": ["Nicht", "die", "Emp\u00b7fin\u00b7dung", "al\u00b7lein", ",", "auch", "was", "in", "erns\u00b7ter", "Er\u00b7fah\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ADV", "$,", "ADV", "PWS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ihn das Leben gelehrt, spreche der Lyriker aus,", "tokens": ["Ihn", "das", "Le\u00b7ben", "ge\u00b7lehrt", ",", "spre\u00b7che", "der", "Ly\u00b7ri\u00b7ker", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Aber am Herzen gereift zum Herzen rede die Weisheit,", "tokens": ["A\u00b7ber", "am", "Her\u00b7zen", "ge\u00b7reift", "zum", "Her\u00b7zen", "re\u00b7de", "die", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber im Strom des Gef\u00fchls sei der Gedanke gel\u00f6st.", "tokens": ["A\u00b7ber", "im", "Strom", "des", "Ge\u00b7f\u00fchls", "sei", "der", "Ge\u00b7dan\u00b7ke", "ge\u00b7l\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wie aus Jupiters Stirn einst Pallas Athene, so sprang aus", "tokens": ["Wie", "aus", "Ju\u00b7pi\u00b7ters", "Stirn", "einst", "Pal\u00b7las", "A\u00b7the\u00b7ne", ",", "so", "sprang", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADV", "NE", "NE", "$,", "ADV", "VVFIN", "PTKVZ"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bismarcks Haupte das Reich waffenger\u00fcstet hervor.", "tokens": ["Bis\u00b7marcks", "Haup\u00b7te", "das", "Reich", "waf\u00b7fen\u00b7ge\u00b7r\u00fcs\u00b7tet", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Tu es der G\u00f6ttin gleich, Germania! Pflanze den \u00d6lbaum,", "tokens": ["Tu", "es", "der", "G\u00f6t\u00b7tin", "gleich", ",", "Ger\u00b7ma\u00b7nia", "!", "Pflan\u00b7ze", "den", "\u00d6l\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADV", "$,", "NE", "$.", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sei dem Gedanken ein Hort, bleibe gewaffnet, wie sie!", "tokens": ["Sei", "dem", "Ge\u00b7dan\u00b7ken", "ein", "Hort", ",", "blei\u00b7be", "ge\u00b7waff\u00b7net", ",", "wie", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "VVPP", "$,", "PWAV", "PPER", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ruhig, sicher und fest, wie das Himmelsgew\u00f6lbe der Atlas,", "tokens": ["Ru\u00b7hig", ",", "si\u00b7cher", "und", "fest", ",", "wie", "das", "Him\u00b7mels\u00b7ge\u00b7w\u00f6l\u00b7be", "der", "At\u00b7las", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "PTKVZ", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Auf der Schulter von Erz tr\u00e4gst du die S\u00e4ulen des Reichs.", "tokens": ["Auf", "der", "Schul\u00b7ter", "von", "Erz", "tr\u00e4gst", "du", "die", "S\u00e4u\u00b7len", "des", "Reichs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "M\u00f6ge der Tag fern sein, der einst von der B\u00fcrde dich abruft,", "tokens": ["M\u00f6\u00b7ge", "der", "Tag", "fern", "sein", ",", "der", "einst", "von", "der", "B\u00fcr\u00b7de", "dich", "ab\u00b7ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VAINF", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+--+--++", "measure": "iambic.septa.invert"}, "line.4": {"text": "Denn kein Zweiter f\u00fcrwahr lebt, der sie tr\u00fcge wie du.", "tokens": ["Denn", "kein", "Zwei\u00b7ter", "f\u00fcr\u00b7wahr", "lebt", ",", "der", "sie", "tr\u00fc\u00b7ge", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}