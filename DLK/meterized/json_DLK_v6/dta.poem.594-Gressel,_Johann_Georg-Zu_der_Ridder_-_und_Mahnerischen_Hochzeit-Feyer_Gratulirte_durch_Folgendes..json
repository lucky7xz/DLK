{"dta.poem.594": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Zu der  \n Ridder - und Mahnerischen  \n Hochzeit-Feyer/  \n  Gratulirte  durch Folgendes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Tichter/ deren Geist der Himmel selber treibet/", "tokens": ["Ihr", "Tich\u00b7ter", "/", "de\u00b7ren", "Geist", "der", "Him\u00b7mel", "sel\u00b7ber", "trei\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELAT", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er durch heilges Feur bl\u00e4st eure Sinnen an/", "tokens": ["Wenn", "er", "durch", "heil\u00b7ges", "Feur", "bl\u00e4st", "eu\u00b7re", "Sin\u00b7nen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sagt/ da ihr offter mahls von Liebes-Flammen schreibet/", "tokens": ["Sagt", "/", "da", "ihr", "off\u00b7ter", "mahls", "von", "Lie\u00b7bes\u00b7Flam\u00b7men", "schrei\u00b7bet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Womit man sie zuletzt noch wol vergleichen kan?", "tokens": ["Wo\u00b7mit", "man", "sie", "zu\u00b7letzt", "noch", "wol", "ver\u00b7glei\u00b7chen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der eine sagt/ es sey ein Zucker vor die Seelen:", "tokens": ["Der", "ei\u00b7ne", "sagt", "/", "es", "sey", "ein", "Zu\u00b7cker", "vor", "die", "See\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$(", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein ander aber nennt sie der Gem\u00fcther Band.", "tokens": ["Ein", "an\u00b7der", "a\u00b7ber", "nennt", "sie", "der", "Ge\u00b7m\u00fc\u00b7ther", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der heist sie einen Grind/ der nicht wol zu verhelen:", "tokens": ["Der", "heist", "sie", "ei\u00b7nen", "Grind", "/", "der", "nicht", "wol", "zu", "ver\u00b7he\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "Und dieser spricht/ sie sey nichts als ein heisser Brand.", "tokens": ["Und", "die\u00b7ser", "spricht", "/", "sie", "sey", "nichts", "als", "ein", "heis\u00b7ser", "Brand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$(", "PPER", "VAFIN", "PIS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein ihr irret noch/ ein einger hats getroffen/", "tokens": ["Al\u00b7lein", "ihr", "ir\u00b7ret", "noch", "/", "ein", "ein\u00b7ger", "hats", "ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$(", "ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der sagt/ da\u00df sie mit recht dem J\u00e4ger-Garne gleicht/", "tokens": ["Der", "sagt", "/", "da\u00df", "sie", "mit", "recht", "dem", "J\u00e4\u00b7ger\u00b7Gar\u00b7ne", "gleicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "PPER", "APPR", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Darinn schon manches Wild vergn\u00fcgt hinein geloffen", "tokens": ["Da\u00b7rinn", "schon", "man\u00b7ches", "Wild", "ver\u00b7gn\u00fcgt", "hin\u00b7ein", "ge\u00b7lof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PIS", "NE", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn es der schlaue Flei\u00df des ", "tokens": ["Wenn", "es", "der", "schlau\u00b7e", "Flei\u00df", "des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.13": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Dabey der Augen-Blitz die erste Lock-Pfeiff\u2019 ist.", "tokens": ["Da\u00b7bey", "der", "Au\u00b7gen\u00b7Blitz", "die", "ers\u00b7te", "Lock\u00b7Pfeiff'", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die K\u00f6rner sind alsdann der Erden falsche G\u00fcter/", "tokens": ["Die", "K\u00f6r\u00b7ner", "sind", "als\u00b7dann", "der", "Er\u00b7den", "fal\u00b7sche", "G\u00fc\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Woran sich mancher Mensch mit Lust zu tode frist.", "tokens": ["Wo\u00b7ran", "sich", "man\u00b7cher", "Mensch", "mit", "Lust", "zu", "to\u00b7de", "frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Stangen sind hieran die Regungen des Blutes/", "tokens": ["Die", "Stan\u00b7gen", "sind", "hie\u00b7ran", "die", "Re\u00b7gun\u00b7gen", "des", "Blu\u00b7tes", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "ART", "NN", "ART", "NN", "$("], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Wo bey so J\u00e4ger als auch Wildpret wird ger\u00fchrt/", "tokens": ["Wo", "bey", "so", "J\u00e4\u00b7ger", "als", "auch", "Wild\u00b7pret", "wird", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADV", "NN", "KOKOM", "ADV", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er ist bey selbigen vergn\u00fcgt und gutes Muhtes", "tokens": ["Er", "ist", "bey", "sel\u00b7bi\u00b7gen", "ver\u00b7gn\u00fcgt", "und", "gu\u00b7tes", "Muh\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "VVPP", "KON", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Da dieser Witterung sein Vogel nachgesp\u00fchrt.", "tokens": ["Da", "die\u00b7ser", "Wit\u00b7te\u00b7rung", "sein", "Vo\u00b7gel", "nach\u00b7ge\u00b7sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Allein dis alles hilfft noch nichts vor hohe Sinnen/", "tokens": ["Al\u00b7lein", "dis", "al\u00b7les", "hilfft", "noch", "nichts", "vor", "ho\u00b7he", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PIS", "VVFIN", "ADV", "PIS", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bey denen mu\u00df allein die Tugend-J\u00e4ger seyn/", "tokens": ["Bey", "de\u00b7nen", "mu\u00df", "al\u00b7lein", "die", "Tu\u00b7gen\u00b7dJ\u00e4\u00b7ger", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VMFIN", "ADV", "ART", "NN", "VAINF", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.23": {"text": "Das Steck-Garn das Vertraun/ so kan hier nichts entrinnen/", "tokens": ["Das", "Steck\u00b7Garn", "das", "Ver\u00b7traun", "/", "so", "kan", "hier", "nichts", "ent\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ADV", "VMFIN", "ADV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Denn alles laufft mit Lust in diese Lappen ein.", "tokens": ["Denn", "al\u00b7les", "laufft", "mit", "Lust", "in", "die\u00b7se", "Lap\u00b7pen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und diese Tugend ist bey ihm das Garn gewesen/", "tokens": ["Und", "die\u00b7se", "Tu\u00b7gend", "ist", "bey", "ihm", "das", "Garn", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "VAPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Herr Vetter/ darinn er vorl\u00e4ngst dis Wildpret fing/", "tokens": ["Herr", "Vet\u00b7ter", "/", "da\u00b7rinn", "er", "vor\u00b7l\u00e4ngst", "dis", "Wild\u00b7pret", "fing", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PAV", "PPER", "VVFIN", "PDS", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Da\u00df dieses Garn allein zu seinem Fang erlesen", "tokens": ["Da\u00df", "die\u00b7ses", "Garn", "al\u00b7lein", "zu", "sei\u00b7nem", "Fang", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ob gleich sonst manches Netz ihm um das Lager hing.", "tokens": ["Ob", "gleich", "sonst", "man\u00b7ches", "Netz", "ihm", "um", "das", "La\u00b7ger", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die frisch belebte Stirn die weiset sein Gem\u00fchte/", "tokens": ["Die", "frisch", "be\u00b7leb\u00b7te", "Stirn", "die", "wei\u00b7set", "sein", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Helenens Br\u00fcder sind daran verliebter Blitz/", "tokens": ["He\u00b7le\u00b7nens", "Br\u00fc\u00b7der", "sind", "da\u00b7ran", "ver\u00b7lieb\u00b7ter", "Blitz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PAV", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.31": {"text": "Der Wangen Purpur zeugt von seiner Seelen G\u00fcte", "tokens": ["Der", "Wan\u00b7gen", "Pur\u00b7pur", "zeugt", "von", "sei\u00b7ner", "See\u00b7len", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Auf der beschneeten Brust hat ", "tokens": ["Auf", "der", "be\u00b7schnee\u00b7ten", "Brust", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Jedoch ich will nicht mehr verbl\u00fchmt und dunckel schreiben/", "tokens": ["Je\u00b7doch", "ich", "will", "nicht", "mehr", "ver\u00b7bl\u00fchmt", "und", "dun\u00b7ckel", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es ist mit einem Wort die sch\u00f6ne ", "tokens": ["Es", "ist", "mit", "ei\u00b7nem", "Wort", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Ich w\u00fcnsche nur/ da mich Pflicht und Gehorsahm treiben/", "tokens": ["Ich", "w\u00fcn\u00b7sche", "nur", "/", "da", "mich", "Pflicht", "und", "Ge\u00b7hor\u00b7sahm", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Nehmt aus des H\u00f6chsten Hand viel tausend Seegen hin.", "tokens": ["Nehmt", "aus", "des", "H\u00f6chs\u00b7ten", "Hand", "viel", "tau\u00b7send", "See\u00b7gen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}