{"textgrid.poem.24529": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Einladung an seine Liebste zu einer Schlittenfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein g\u00fctiges Geh\u00f6r/ und gieb den Willen drein/", "tokens": ["Ein", "g\u00fc\u00b7ti\u00b7ges", "Ge\u00b7h\u00f6r", "/", "und", "gieb", "den", "Wil\u00b7len", "drein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df bey gefallnen Schnee auf einen leichten Schlitten/", "tokens": ["Da\u00df", "bey", "ge\u00b7fall\u00b7nen", "Schnee", "auf", "ei\u00b7nen", "leich\u00b7ten", "Schlit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein gantz ergebner Knecht dein F\u00fchrer m\u00f6ge seyn.", "tokens": ["Dein", "gantz", "er\u00b7geb\u00b7ner", "Knecht", "dein", "F\u00fch\u00b7rer", "m\u00f6\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bald der Abend komt/ und ", "tokens": ["So", "bald", "der", "A\u00b7bend", "komt", "/", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$(", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch ihren hellen Stern am Himmel l\u00e4sset sehn/", "tokens": ["Durch", "ih\u00b7ren", "hel\u00b7len", "Stern", "am", "Him\u00b7mel", "l\u00e4s\u00b7set", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So hol ich Sch\u00f6nste dich erfreut von deinem Zimmer/", "tokens": ["So", "hol", "ich", "Sch\u00f6ns\u00b7te", "dich", "er\u00b7freut", "von", "dei\u00b7nem", "Zim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Glantz der Anmuth kan wohl bey der ", "tokens": ["Dein", "Glantz", "der", "An\u00b7muth", "kan", "wohl", "bey", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VMFIN", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df mich der Zeit zum Trotz/ indem es itzt will frieren/", "tokens": ["La\u00df", "mich", "der", "Zeit", "zum", "Trotz", "/", "in\u00b7dem", "es", "itzt", "will", "frie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPRART", "NN", "$(", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jung/ sch\u00f6n und feurig Kind/ durch dich den Fr\u00fchling f\u00fchren.", "tokens": ["Jung", "/", "sch\u00f6n", "und", "feu\u00b7rig", "Kind", "/", "durch", "dich", "den", "Fr\u00fch\u00b7ling", "f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJD", "KON", "ADJD", "NN", "$(", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}