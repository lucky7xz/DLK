{"textgrid.poem.66299": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Meinem Erl\u00f6ser", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, den ich sonst, wie unter tr\u00fcber H\u00fclle", "tokens": ["Du", ",", "den", "ich", "sonst", ",", "wie", "un\u00b7ter", "tr\u00fc\u00b7ber", "H\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Mondes Antlitz d\u00e4mmern sah;", "tokens": ["Des", "Mon\u00b7des", "Ant\u00b7litz", "d\u00e4m\u00b7mern", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie bist du mir in deiner Gottesf\u00fclle,", "tokens": ["Wie", "bist", "du", "mir", "in", "dei\u00b7ner", "Got\u00b7tes\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erl\u00f6ser, nun so nah;", "tokens": ["Er\u00b7l\u00f6\u00b7ser", ",", "nun", "so", "nah", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich im Morgenstrahle mich erhebe,", "tokens": ["Wenn", "ich", "im", "Mor\u00b7gen\u00b7strah\u00b7le", "mich", "er\u00b7he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So f\u00fchl' ich's, da\u00df du um mich bist,", "tokens": ["So", "f\u00fchl'", "ich's", ",", "da\u00df", "du", "um", "mich", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich gr\u00fc\u00df' die Sonne, die die St\u00e4be", "tokens": ["Ich", "gr\u00fc\u00df'", "die", "Son\u00b7ne", ",", "die", "die", "St\u00e4\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von meinem Gitter k\u00fc\u00dft.", "tokens": ["Von", "mei\u00b7nem", "Git\u00b7ter", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6r' dich s\u00e4useln in der L\u00fcfte Freie", "tokens": ["Ich", "h\u00f6r'", "dich", "s\u00e4u\u00b7seln", "in", "der", "L\u00fcf\u00b7te", "Frei\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fchl' dein Wehen um mich her;", "tokens": ["Und", "f\u00fchl'", "dein", "We\u00b7hen", "um", "mich", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh' dich in des Himmels Bl\u00e4ue", "tokens": ["Ich", "seh'", "dich", "in", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in des Lichtes Meer;", "tokens": ["Und", "in", "des", "Lich\u00b7tes", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seh' im Mittag, wenn kleine Wolken ziehen,", "tokens": ["Seh'", "im", "Mit\u00b7tag", ",", "wenn", "klei\u00b7ne", "Wol\u00b7ken", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dem Zuge mit Entz\u00fccken zu,", "tokens": ["Dem", "Zu\u00b7ge", "mit", "Ent\u00b7z\u00fc\u00b7cken", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seh' den Abendhimmel gl\u00fchen", "tokens": ["Und", "seh'", "den", "A\u00b7bend\u00b7him\u00b7mel", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und denke: dort bist du!", "tokens": ["Und", "den\u00b7ke", ":", "dort", "bist", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Seh' dich am Sternenhimmel flimmern,", "tokens": ["Seh'", "dich", "am", "Ster\u00b7nen\u00b7him\u00b7mel", "flim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ihn dein Kleid mit Licht beflammt,", "tokens": ["Wenn", "ihn", "dein", "Kleid", "mit", "Licht", "be\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh' dich im nassen Grase schimmern,", "tokens": ["Seh'", "dich", "im", "nas\u00b7sen", "Gra\u00b7se", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auf des Veilchens Sammt.", "tokens": ["Wie", "auf", "des", "Veil\u00b7chens", "Sammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie lieb sind mir die Menschen, meine Br\u00fcder!", "tokens": ["Wie", "lieb", "sind", "mir", "die", "Men\u00b7schen", ",", "mei\u00b7ne", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich schaur' in s\u00fc\u00dfer Sympathie;", "tokens": ["Ich", "schaur'", "in", "s\u00fc\u00b7\u00dfer", "Sym\u00b7pa\u00b7thie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Glieder sind sie, Jesu Glieder;", "tokens": ["Denn", "Glie\u00b7der", "sind", "sie", ",", "Je\u00b7su", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Blut besprengt sind sie.", "tokens": ["Mit", "Blut", "be\u00b7sprengt", "sind", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch n\u00e4her noch, noch unaussprechlich n\u00e4her,", "tokens": ["Doch", "n\u00e4\u00b7her", "noch", ",", "noch", "un\u00b7aus\u00b7sprech\u00b7lich", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erl\u00f6ser, bist du mir;", "tokens": ["Er\u00b7l\u00f6\u00b7ser", ",", "bist", "du", "mir", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz, du wei\u00dft es, Herzenssp\u00e4her,", "tokens": ["Mein", "Herz", ",", "du", "wei\u00dft", "es", ",", "Her\u00b7zens\u00b7sp\u00e4\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz ist voll von dir.", "tokens": ["Mein", "Herz", "ist", "voll", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du bist um mich in meiner \u00f6den Zelle,", "tokens": ["Du", "bist", "um", "mich", "in", "mei\u00b7ner", "\u00f6\u00b7den", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umgibst mich fr\u00fch und sp\u00e4t,", "tokens": ["Um\u00b7gibst", "mich", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Machst meine stummen W\u00e4nde helle", "tokens": ["Machst", "mei\u00b7ne", "stum\u00b7men", "W\u00e4n\u00b7de", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sonnest meinen Pfad.", "tokens": ["Und", "son\u00b7nest", "mei\u00b7nen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn Einsamkeit um mich die Fl\u00fcgel breitet", "tokens": ["Wenn", "Ein\u00b7sam\u00b7keit", "um", "mich", "die", "Fl\u00fc\u00b7gel", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mich in schwarze Schatten h\u00fcllt,", "tokens": ["Und", "mich", "in", "schwar\u00b7ze", "Schat\u00b7ten", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Sehnsucht auf den Wangen gleitet", "tokens": ["Wenn", "Sehn\u00b7sucht", "auf", "den", "Wan\u00b7gen", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Augen f\u00fcllt;", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen", "f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn ich mein Weib im Wittwenschlei'r erblicke", "tokens": ["Wenn", "ich", "mein", "Weib", "im", "Witt\u00b7wen\u00b7schlei'r", "er\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schling' den Arm um sie herum", "tokens": ["Und", "schling'", "den", "Arm", "um", "sie", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ach! nur Duft an meinen Busen dr\u00fccke", "tokens": ["Und", "ach", "!", "nur", "Duft", "an", "mei\u00b7nen", "Bu\u00b7sen", "dr\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und starrend steh' und stumm;", "tokens": ["Und", "star\u00b7rend", "steh'", "und", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wenn Kinder mir wie unterm Flor erscheinen,", "tokens": ["Wenn", "Kin\u00b7der", "mir", "wie", "un\u00b7term", "Flor", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "KOKOM", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Mutter, Bruder, Freund mich schreckt", "tokens": ["Wenn", "Mut\u00b7ter", ",", "Bru\u00b7der", ",", "Freund", "mich", "schreckt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mich ihr fernes dumpfes Weinen", "tokens": ["Und", "mich", "ihr", "fer\u00b7nes", "dum\u00b7pfes", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Jammerklage weckt:", "tokens": ["Zur", "Jam\u00b7mer\u00b7kla\u00b7ge", "weckt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann seh' ich dich, erbarmender Erl\u00f6ser,", "tokens": ["Dann", "seh'", "ich", "dich", ",", "er\u00b7bar\u00b7men\u00b7der", "Er\u00b7l\u00f6\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie du voll sanftes Mitleids bist;", "tokens": ["Wie", "du", "voll", "sanf\u00b7tes", "Mit\u00b7leids", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deine Liebe wird mir gr\u00f6\u00dfer,", "tokens": ["Und", "dei\u00b7ne", "Lie\u00b7be", "wird", "mir", "gr\u00f6\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Erdenliebe ist.", "tokens": ["Als", "Er\u00b7den\u00b7lie\u00b7be", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und wenn ich Nachts am sterngestickten Himmel", "tokens": ["Und", "wenn", "ich", "Nachts", "am", "stern\u00b7ge\u00b7stick\u00b7ten", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem vollen Mond ins Antlitz seh',", "tokens": ["Dem", "vol\u00b7len", "Mond", "ins", "Ant\u00b7litz", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ach! im st\u00fcrmischen Gewimmel", "tokens": ["Und", "ach", "!", "im", "st\u00fcr\u00b7mi\u00b7schen", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Qualen fast vergeh';", "tokens": ["Der", "Qua\u00b7len", "fast", "ver\u00b7geh'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und wenn ich oft im Innersten empfinde", "tokens": ["Und", "wenn", "ich", "oft", "im", "In\u00b7ners\u00b7ten", "emp\u00b7fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line