{"dta.poem.20546": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "5.  \n  Scharnhorst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df ein Invalide,", "tokens": ["Es", "sa\u00df", "ein", "In\u00b7va\u00b7li\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl etwas matt und m\u00fcde", "tokens": ["Wohl", "et\u00b7was", "matt", "und", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "(zur Stadt war weit sein Gang.)", "tokens": ["(", "zur", "Stadt", "war", "weit", "sein", "Gang", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf eines Dorfes Bank.", "tokens": ["Auf", "ei\u00b7nes", "Dor\u00b7fes", "Bank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sprach vom K\u00f6nig ", "tokens": ["Er", "sprach", "vom", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Keith, Dessau, Seidelitz,", "tokens": ["Keith", ",", "Des\u00b7sau", ",", "Sei\u00b7de\u00b7litz", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erz\u00e4hlte tausend Suiten", "tokens": ["Er\u00b7z\u00e4hl\u00b7te", "tau\u00b7send", "Sui\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom alten Gen\u2019ral ", "tokens": ["Vom", "al\u00b7ten", "Gen'\u00b7ral"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Beim Namen: Graf ", "tokens": ["Beim", "Na\u00b7men", ":", "Graf"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$.", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "That ihm die Wange gl\u00fch\u2019n", "tokens": ["That", "ihm", "die", "Wan\u00b7ge", "gl\u00fch'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "VVINF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Von allen mitgemachten", "tokens": ["Von", "al\u00b7len", "mit\u00b7ge\u00b7mach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Belagerungen, Schlachten,", "tokens": ["Be\u00b7la\u00b7ge\u00b7run\u00b7gen", ",", "Schlach\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Trug er Geschichten vor,", "tokens": ["Trug", "er", "Ge\u00b7schich\u00b7ten", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "Die in das offne Ohr", "tokens": ["Die", "in", "das", "off\u00b7ne", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Der dicken Bauernrangen,", "tokens": ["Der", "di\u00b7cken", "Bau\u00b7ern\u00b7ran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die ihn umliefen, drangen.", "tokens": ["Die", "ihn", "um\u00b7lie\u00b7fen", ",", "dran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Doch stimmt, wie sonst wohl Keinen,", "tokens": ["Doch", "stimmt", ",", "wie", "sonst", "wohl", "Kei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "ADV", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Es froh besonders Einen,", "tokens": ["Es", "froh", "be\u00b7son\u00b7ders", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Der Tags, ja selbst bei Nacht,", "tokens": ["Der", "Tags", ",", "ja", "selbst", "bei", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Ernstlich drob nachgedacht.", "tokens": ["Ernst\u00b7lich", "drob", "nach\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.21": {"text": "Er hofft im Traum: W\u00fcrd\u2019 mal", "tokens": ["Er", "hofft", "im", "Traum", ":", "W\u00fcrd'", "mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Ich doch ein Korporal! \u2014", "tokens": ["Ich", "doch", "ein", "Kor\u00b7po\u00b7ral", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "So ward er denn Gen\u2019ral,", "tokens": ["So", "ward", "er", "denn", "Gen'\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Der nicht nur das, was nah,", "tokens": ["Der", "nicht", "nur", "das", ",", "was", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "PDS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Nein, in die Zukunft sah.", "tokens": ["Nein", ",", "in", "die", "Zu\u00b7kunft", "sah", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Scharnhorst, der, als durch Schmach", "tokens": ["Scharn\u00b7horst", ",", "der", ",", "als", "durch", "Schmach"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Das Heer darnieder lag,", "tokens": ["Das", "Heer", "dar\u00b7nie\u00b7der", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Jhm wu\u00dfte neues Leben", "tokens": ["Jhm", "wu\u00df\u00b7te", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Und inn\u2019re Kraft zu geben.", "tokens": ["Und", "inn'\u00b7re", "Kraft", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Er ist\u2019s, der Preu\u00dfens Waffen", "tokens": ["Er", "ist's", ",", "der", "Preu\u00b7\u00dfens", "Waf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Fast g\u00e4nzlich umgeschaffen;", "tokens": ["Fast", "g\u00e4nz\u00b7lich", "um\u00b7ge\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Kr\u00e4ft\u2019gend die inn\u2019re Ehr\u2019", "tokens": ["Kr\u00e4ft'\u00b7gend", "die", "inn'\u00b7re", "Ehr'"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.33": {"text": "Vom Stand der Kriegeswehr.", "tokens": ["Vom", "Stand", "der", "Krie\u00b7ges\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}