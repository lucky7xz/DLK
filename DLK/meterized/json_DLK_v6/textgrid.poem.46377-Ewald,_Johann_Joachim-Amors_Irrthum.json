{"textgrid.poem.46377": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Amors Irrthum", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sah ich Phyllis tanzen", "tokens": ["J\u00fcngst", "sah", "ich", "Phyl\u00b7lis", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und fieng sie an zu lieben.", "tokens": ["Und", "fi\u00b7eng", "sie", "an", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich rief den nahen Amor", "tokens": ["Ich", "rief", "den", "na\u00b7hen", "A\u00b7mor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach: Verwunde Phyllis", "tokens": ["Und", "sprach", ":", "Ver\u00b7wun\u00b7de", "Phyl\u00b7lis"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "NN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Damit mich Phyllis liebe!", "tokens": ["Da\u00b7mit", "mich", "Phyl\u00b7lis", "lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Blinde nahm den Bogen", "tokens": ["Der", "Blin\u00b7de", "nahm", "den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und scho\u00df und traf die Mutter;", "tokens": ["Und", "scho\u00df", "und", "traf", "die", "Mut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nun liebt mich ach! die Mutter!", "tokens": ["Nun", "liebt", "mich", "ach", "!", "die", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}