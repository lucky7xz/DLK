{"dta.poem.7801": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df Lob gefiele Saul, er nahm es g\u00fctig auf,", "tokens": ["Die\u00df", "Lob", "ge\u00b7fie\u00b7le", "Saul", ",", "er", "nahm", "es", "g\u00fc\u00b7tig", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "NE", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thalmais gr\u00fcste er und der Matronen Hauf", "tokens": ["Thal\u00b7mais", "gr\u00fcs\u00b7te", "er", "und", "der", "Mat\u00b7ro\u00b7nen", "Hauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ward freundlich angesehn von ihrem frohen K\u00f6nig.", "tokens": ["Ward", "freund\u00b7lich", "an\u00b7ge\u00b7sehn", "von", "ih\u00b7rem", "fro\u00b7hen", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indeme Michal nun verzogen noch ein wenig,", "tokens": ["In\u00b7de\u00b7me", "Mic\u00b7hal", "nun", "ver\u00b7zo\u00b7gen", "noch", "ein", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "ADV", "ART", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hebt an ihr Saitenspiel, Saul siehet nach ihr hin,", "tokens": ["Hebt", "an", "ihr", "Sai\u00b7ten\u00b7spiel", ",", "Saul", "sie\u00b7het", "nach", "ihr", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "NE", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Begierig da\u00df er auch von ihr ein Lob gewinn.", "tokens": ["Be\u00b7gie\u00b7rig", "da\u00df", "er", "auch", "von", "ihr", "ein", "Lob", "ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie aber f\u00e4het an. Seynd tausend Mann geblieben", "tokens": ["Sie", "a\u00b7ber", "f\u00e4\u00b7het", "an", ".", "Seynd", "tau\u00b7send", "Mann", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$.", "VAIMP", "CARD", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch unsers K\u00f6nigs Hand, so hat noch mehr vertrieben", "tokens": ["Durch", "un\u00b7sers", "K\u00f6\u00b7nigs", "Hand", ",", "so", "hat", "noch", "mehr", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ADV", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Held von Bethlehem; du Sieger sey gegr\u00fcst,", "tokens": ["Der", "Held", "von", "Beth\u00b7le\u00b7hem", ";", "du", "Sie\u00b7ger", "sey", "ge\u00b7gr\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "PPER", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dich hat Jsraels Gott zu unserm Heil erkiest.", "tokens": ["Dich", "hat", "Js\u00b7raels", "Gott", "zu", "un\u00b7serm", "Heil", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Willkommen k\u00fchner Held, was solln wir von dir sagen,", "tokens": ["Will\u00b7kom\u00b7men", "k\u00fch\u00b7ner", "Held", ",", "was", "solln", "wir", "von", "dir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du edler David hast zehn tausend Mann erschlagen.", "tokens": ["Du", "ed\u00b7ler", "Da\u00b7vid", "hast", "zehn", "tau\u00b7send", "Mann", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "VAFIN", "CARD", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}