{"textgrid.poem.54507": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilst du die Gnaden Sonn/ den Herren Jesum seyn?", "tokens": ["Wilst", "du", "die", "Gna\u00b7den", "Sonn", "/", "den", "Her\u00b7ren", "Je\u00b7sum", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "$(", "ART", "NN", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df das Z\u00f6lner Haus die Werckst\u00e4dt tr\u00fcber S\u00fcnden.", "tokens": ["So", "la\u00df", "das", "Z\u00f6l\u00b7ner", "Haus", "die", "Werck\u00b7st\u00e4dt", "tr\u00fc\u00b7ber", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er l\u00e4st sich nirgend sonst/ den auff der Creutzbahn finden/", "tokens": ["Er", "l\u00e4st", "sich", "nir\u00b7gend", "sonst", "/", "den", "auff", "der", "Creutz\u00b7bahn", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "$(", "ART", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff/ auff! la\u00df Hoff vnd Stadt la\u00df eigen Gutt vnd Lehn", "tokens": ["Auff", "/", "auff", "!", "la\u00df", "Hoff", "vnd", "Stadt", "la\u00df", "ei\u00b7gen", "Gutt", "vnd", "Lehn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "$(", "PTKVZ", "$.", "VVIMP", "VVFIN", "KON", "NN", "VVFIN", "ADJD", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob Glaub vnd Fleisch wol klein/ ob gleich die Heuchler schmehn/", "tokens": ["Ob", "Glaub", "vnd", "Fleisch", "wol", "klein", "/", "ob", "gleich", "die", "Heuch\u00b7ler", "schmehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "ADJD", "$(", "KOUS", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Steig seinen Creutz-Baum auff/ bald wird was hindert schwinden/", "tokens": ["Steig", "sei\u00b7nen", "Creutz\u00b7Baum", "auff", "/", "bald", "wird", "was", "hin\u00b7dert", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "$(", "ADV", "VAFIN", "PIS", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald wird er dich anschreyn vnd aller Schuld entbinden/", "tokens": ["Bald", "wird", "er", "dich", "an\u00b7schreyn", "vnd", "al\u00b7ler", "Schuld", "ent\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "KON", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bald wird an deinem Haus vnd Seelen Heil geschehn/", "tokens": ["Bald", "wird", "an", "dei\u00b7nem", "Haus", "vnd", "See\u00b7len", "Heil", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch must du voll von Reu/ was vnrecht gantz weglegen/", "tokens": ["Doch", "must", "du", "voll", "von", "Reu", "/", "was", "vn\u00b7recht", "gantz", "weg\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "APPR", "NE", "$(", "PWS", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.10": {"text": "Vnd von Betrug abstehn wofern sein reicher Segen", "tokens": ["Vnd", "von", "Be\u00b7trug", "ab\u00b7stehn", "wo\u00b7fern", "sein", "rei\u00b7cher", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVINF", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sol geben was dir fehlt/ vnd abthun was dich kr\u00e4nckt.", "tokens": ["Sol", "ge\u00b7ben", "was", "dir", "fehlt", "/", "vnd", "ab\u00b7thun", "was", "dich", "kr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PWS", "PPER", "VVFIN", "$(", "KON", "VVINF", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Schau doch O Lebens Licht auff meine Bu\u00df vnd Zehren/", "tokens": ["Schau", "doch", "O", "Le\u00b7bens", "Licht", "auff", "mei\u00b7ne", "Bu\u00df", "vnd", "Zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "NN", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Eyl in di\u00df enge Hau\u00df de\u00df Hertzens einzukehren", "tokens": ["Eyl", "in", "di\u00df", "en\u00b7ge", "Hau\u00df", "de\u00df", "Hert\u00b7zens", "ein\u00b7zu\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDS", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Darauff kein W\u00e4chter acht vnd Abraham nicht denckt.", "tokens": ["Dar\u00b7auff", "kein", "W\u00e4ch\u00b7ter", "acht", "vnd", "Ab\u00b7ra\u00b7ham", "nicht", "denckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "CARD", "KON", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}