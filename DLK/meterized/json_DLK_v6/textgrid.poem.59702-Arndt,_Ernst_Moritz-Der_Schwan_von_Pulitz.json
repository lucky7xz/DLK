{"textgrid.poem.59702": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Der Schwan von Pulitz", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schneewei\u00dfer Schwan, wo fliegst, wo klingst du her?", "tokens": ["Schnee\u00b7wei\u00b7\u00dfer", "Schwan", ",", "wo", "fliegst", ",", "wo", "klingst", "du", "her", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Wo kommst du Fr\u00fchlingsklinger hergeflogen?", "tokens": ["Wo", "kommst", "du", "Fr\u00fch\u00b7lings\u00b7klin\u00b7ger", "her\u00b7ge\u00b7flo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus meiner gr\u00fcnen Insel stillem Meer?", "tokens": ["Aus", "mei\u00b7ner", "gr\u00fc\u00b7nen", "In\u00b7sel", "stil\u00b7lem", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus Pulitz' sturmgeschirmten Wogen?", "tokens": ["Aus", "Pu\u00b7litz'", "sturm\u00b7ge\u00b7schirm\u00b7ten", "Wo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Flogst du aus seinen stillen Buchten her?", "tokens": ["Flogst", "du", "aus", "sei\u00b7nen", "stil\u00b7len", "Buch\u00b7ten", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und tr\u00e4gst im goldnen Schnabel goldne M\u00e4r?", "tokens": ["Und", "tr\u00e4gst", "im", "gold\u00b7nen", "Schna\u00b7bel", "gold\u00b7ne", "M\u00e4r", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hast du die kleinern Inseln auch besehn?", "tokens": ["Hast", "du", "die", "klei\u00b7nern", "In\u00b7seln", "auch", "be\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die steile Oi, vom Vilm die stolzen Buchen?", "tokens": ["Die", "stei\u00b7le", "O\u00b7i", ",", "vom", "Vilm", "die", "stol\u00b7zen", "Bu\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Den Rugard, Putbus' waldbekr\u00e4nzte H\u00f6hn,", "tokens": ["Den", "Ru\u00b7gard", ",", "Put\u00b7bus'", "wald\u00b7be\u00b7kr\u00e4nz\u00b7te", "H\u00f6hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo Reiz und Sch\u00f6nheit Aug' und Herz versuchen?", "tokens": ["Wo", "Reiz", "und", "Sch\u00f6n\u00b7heit", "Aug'", "und", "Herz", "ver\u00b7su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O klinge mir den s\u00fc\u00dfen Heimatklang!", "tokens": ["O", "klin\u00b7ge", "mir", "den", "s\u00fc\u00b7\u00dfen", "Hei\u00b7mat\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein greises Haupt, es neigt zum Schwanensang.", "tokens": ["Mein", "grei\u00b7ses", "Haupt", ",", "es", "neigt", "zum", "Schwa\u00b7nen\u00b7sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbzum Schwanensang? F\u00fcr diesen kam ich nicht,", "tokens": ["\u00bb", "zum", "Schwa\u00b7nen\u00b7sang", "?", "F\u00fcr", "die\u00b7sen", "kam", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$.", "APPR", "PDAT", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr diesen regt' ich nicht zum Rhein die Fl\u00fcgel,", "tokens": ["F\u00fcr", "die\u00b7sen", "regt'", "ich", "nicht", "zum", "Rhein", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PTKNEG", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr diesen flog ich schnell wie Lieb' und Licht", "tokens": ["F\u00fcr", "die\u00b7sen", "flog", "ich", "schnell", "wie", "Lieb'", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So weiten Flug nicht \u00fcber Tal und H\u00fcgel. \u2013", "tokens": ["So", "wei\u00b7ten", "Flug", "nicht", "\u00fc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du wei\u00dft, still schaurig klingt der Schwanensang,", "tokens": ["Du", "wei\u00dft", ",", "still", "schau\u00b7rig", "klingt", "der", "Schwa\u00b7nen\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Heut kling' ich eitel hellen Freudenklang.", "tokens": ["Heut", "kling'", "ich", "ei\u00b7tel", "hel\u00b7len", "Freu\u00b7den\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Heut kling' ich Klang der Himmelsnachtigall,", "tokens": ["Heut", "kling'", "ich", "Klang", "der", "Him\u00b7mels\u00b7nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Lieb' und Lenz in Putbus' Hainen singet,", "tokens": ["Die", "Lieb'", "und", "Lenz", "in", "Put\u00b7bus'", "Hai\u00b7nen", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heut kling' ich nach den s\u00fc\u00dfen Wunderschall,", "tokens": ["Heut", "kling'", "ich", "nach", "den", "s\u00fc\u00b7\u00dfen", "Wun\u00b7der\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der wie aus h\u00f6herm Himmel niederklinget,", "tokens": ["Der", "wie", "aus", "h\u00f6\u00b7herm", "Him\u00b7mel", "nie\u00b7der\u00b7klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich klinge nach \u2013 o k\u00f6nnt' ich's recht und ganz! \u2013", "tokens": ["Ich", "klin\u00b7ge", "nach", "\u2013", "o", "k\u00f6nnt'", "ich's", "recht", "und", "ganz", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "$(", "FM", "VMFIN", "PIS", "ADJD", "KON", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du kennest Klang und Wonne, Licht und Glanz.", "tokens": ["Du", "ken\u00b7nest", "Klang", "und", "Won\u00b7ne", ",", "Licht", "und", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nimm Klang und Gru\u00df!\u00ab \u2013 Und horch'! Der Fl\u00fcgel rauscht,", "tokens": ["Nimm", "Klang", "und", "Gru\u00df", "!", "\u00ab", "\u2013", "Und", "horch'", "!", "Der", "Fl\u00fc\u00b7gel", "rauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$.", "$(", "$(", "KON", "VVIMP", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ehe Aug' und Ohr sich noch besinnen,", "tokens": ["Und", "e\u00b7he", "Aug'", "und", "Ohr", "sich", "noch", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "KON", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie man im Traum auf Bild und Stimme lauscht", "tokens": ["Wie", "man", "im", "Traum", "auf", "Bild", "und", "Stim\u00b7me", "lauscht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPRART", "NN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und fassen will, ist Schwan und Fl\u00fcgel hinnen,", "tokens": ["Und", "fas\u00b7sen", "will", ",", "ist", "Schwan", "und", "Fl\u00fc\u00b7gel", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,", "VAFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie aus Fernen klingt ein s\u00fc\u00dfer Schall,", "tokens": ["Und", "wie", "aus", "Fer\u00b7nen", "klingt", "ein", "s\u00fc\u00b7\u00dfer", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Himmelsstimme, Putbus' Nachtigall.", "tokens": ["Die", "Him\u00b7mels\u00b7stim\u00b7me", ",", "Put\u00b7bus'", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}