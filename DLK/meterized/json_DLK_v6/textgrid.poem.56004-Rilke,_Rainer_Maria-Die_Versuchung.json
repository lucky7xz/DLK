{"textgrid.poem.56004": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Versuchung", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, es half nicht, da\u00df er sich die scharfen", "tokens": ["Nein", ",", "es", "half", "nicht", ",", "da\u00df", "er", "sich", "die", "schar\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stacheln einhieb in das geile Fleisch;", "tokens": ["Sta\u00b7cheln", "ein\u00b7hieb", "in", "das", "gei\u00b7le", "Fleisch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "alle seine tr\u00e4chtigen Sinne warfen", "tokens": ["al\u00b7le", "sei\u00b7ne", "tr\u00e4ch\u00b7ti\u00b7gen", "Sin\u00b7ne", "war\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "unter krei\u00dfendem Gekreisch", "tokens": ["un\u00b7ter", "krei\u00b7\u00dfen\u00b7dem", "Ge\u00b7kreisch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00fchgeburten: schiefe, hingeschielte", "tokens": ["Fr\u00fch\u00b7ge\u00b7bur\u00b7ten", ":", "schie\u00b7fe", ",", "hin\u00b7ge\u00b7schiel\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$.", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "kriechende und fliegende Gesichte,", "tokens": ["krie\u00b7chen\u00b7de", "und", "flie\u00b7gen\u00b7de", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Nichte, deren nur auf ihn erpichte", "tokens": ["Nich\u00b7te", ",", "de\u00b7ren", "nur", "auf", "ihn", "er\u00b7pich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "ADV", "APPR", "PPER", "VVFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Bosheit sich verband und mit ihm spielte.", "tokens": ["Bos\u00b7heit", "sich", "ver\u00b7band", "und", "mit", "ihm", "spiel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und schon hatten seine Sinne Enkel;", "tokens": ["Und", "schon", "hat\u00b7ten", "sei\u00b7ne", "Sin\u00b7ne", "En\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "denn das Pack war fruchtbar in der Nacht", "tokens": ["denn", "das", "Pack", "war", "frucht\u00b7bar", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und in immer bunterem Gesprenkel", "tokens": ["und", "in", "im\u00b7mer", "bun\u00b7te\u00b7rem", "Ge\u00b7spren\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "hingehudelt und verhundertfacht.", "tokens": ["hin\u00b7ge\u00b7hu\u00b7delt", "und", "ver\u00b7hun\u00b7dert\u00b7facht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aus dem Ganzen ward ein Trank gemacht:", "tokens": ["Aus", "dem", "Gan\u00b7zen", "ward", "ein", "Trank", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "seine H\u00e4nde griffen lauter Henkel,", "tokens": ["sei\u00b7ne", "H\u00e4n\u00b7de", "grif\u00b7fen", "lau\u00b7ter", "Hen\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und der Schatten schob sich auf wie Schenkel", "tokens": ["und", "der", "Schat\u00b7ten", "schob", "sich", "auf", "wie", "Schen\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "KOKOM", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "warm und zu Umarmungen erwacht \u2013.", "tokens": ["warm", "und", "zu", "Um\u00b7ar\u00b7mun\u00b7gen", "er\u00b7wacht", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "VVPP", "$(", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und da schrie er nach dem Engel, schrie:", "tokens": ["Und", "da", "schrie", "er", "nach", "dem", "En\u00b7gel", ",", "schrie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Engel kam in seinem Schein", "tokens": ["Und", "der", "En\u00b7gel", "kam", "in", "sei\u00b7nem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und war da: und jagte sie", "tokens": ["und", "war", "da", ":", "und", "jag\u00b7te", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "$.", "KON", "VVFIN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "wieder in den Heiligen hinein,", "tokens": ["wie\u00b7der", "in", "den", "Hei\u00b7li\u00b7gen", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "da\u00df er mit Geteufel und Getier", "tokens": ["da\u00df", "er", "mit", "Ge\u00b7teu\u00b7fel", "und", "Ge\u00b7tier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "in sich weiterringe wie seit Jahren", "tokens": ["in", "sich", "wei\u00b7ter\u00b7rin\u00b7ge", "wie", "seit", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "VVFIN", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und sich Gott, den lange noch nicht klaren,", "tokens": ["und", "sich", "Gott", ",", "den", "lan\u00b7ge", "noch", "nicht", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "NN", "$,", "PRELS", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "innen aus dem J\u00e4sen destillier.", "tokens": ["in\u00b7nen", "aus", "dem", "J\u00e4\u00b7sen", "des\u00b7til\u00b7lier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}