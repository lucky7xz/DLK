text": "In seinen Fu\u00df; da liebt' er uns nicht minder,", "tokens": ["In", "sei\u00b7nen", "Fu\u00df", ";", "da", "liebt'", "er", "uns", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.69": {"text": "Doch ward er b\u00f6s auf uns zur selben Stunde.", "tokens": ["Doch", "ward", "er", "b\u00f6s", "auf", "uns", "zur", "sel\u00b7ben", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.70": {"text": "Da trieb er mit Kanonen uns geschwinder,", "tokens": ["Da", "trieb", "er", "mit", "Ka\u00b7no\u00b7nen", "uns", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.71": {"text": "Weil tr\u00e4g er uns und ungelehrig fand;", "tokens": ["Weil", "tr\u00e4g", "er", "uns", "und", "un\u00b7ge\u00b7leh\u00b7rig", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPER", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.72": {"text": "Und dennoch liebt' er seine roten Kinder. \u2013", "tokens": ["Und", "den\u00b7noch", "liebt'", "er", "sei\u00b7ne", "ro\u00b7ten", "Kin\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.73": {"text": "Wie un