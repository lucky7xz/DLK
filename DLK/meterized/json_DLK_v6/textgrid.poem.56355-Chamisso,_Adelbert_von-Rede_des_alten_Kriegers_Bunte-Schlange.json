{"textgrid.poem.56355": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Rede des alten Kriegers Bunte-Schlange", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.74", "nl:0.25"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Rat der Creek-Indianer ward der Bote", "tokens": ["Im", "Rat", "der", "Creek\u00b7In\u00b7di\u00b7a\u00b7ner", "ward", "der", "Bo\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Pr\u00e4sidenten Jackson vorgelassen;", "tokens": ["Des", "Pr\u00e4\u00b7si\u00b7den\u00b7ten", "Jack\u00b7son", "vor\u00b7ge\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Brief, den er verlas, enthielt Gebote.", "tokens": ["Der", "Brief", ",", "den", "er", "ver\u00b7las", ",", "ent\u00b7hielt", "Ge\u00b7bo\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Landmark, welche diesseits sie besa\u00dfen", "tokens": ["Die", "Land\u00b7mark", ",", "wel\u00b7che", "dies\u00b7seits", "sie", "be\u00b7sa\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Mississippi, sollten gleich sie r\u00e4umen,", "tokens": ["Des", "Mis\u00b7sis\u00b7sip\u00b7pi", ",", "soll\u00b7ten", "gleich", "sie", "r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und der Entschlu\u00df blieb ihnen nur zu fassen.", "tokens": ["Und", "der", "Ent\u00b7schlu\u00df", "blieb", "ih\u00b7nen", "nur", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und starr und stumm beharrten, wie in Tr\u00e4umen,", "tokens": ["Und", "starr", "und", "stumm", "be\u00b7harr\u00b7ten", ",", "wie", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Oberh\u00e4upter, man vernahm noch lange", "tokens": ["Die", "O\u00b7ber\u00b7h\u00e4up\u00b7ter", ",", "man", "ver\u00b7nahm", "noch", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PIS", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das S\u00e4useln nur des Windes in den B\u00e4umen.", "tokens": ["Das", "S\u00e4u\u00b7seln", "nur", "des", "Win\u00b7des", "in", "den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da hob sich aus der M\u00e4nner erstem Range", "tokens": ["Da", "hob", "sich", "aus", "der", "M\u00e4n\u00b7ner", "ers\u00b7tem", "Ran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der hundertj\u00e4hr'ge waffenm\u00fcde Greis,", "tokens": ["Der", "hun\u00b7dert\u00b7j\u00e4hr'\u00b7ge", "waf\u00b7fen\u00b7m\u00fc\u00b7de", "Greis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ein Nestor seines Volks, der Bunte-Schlange.", "tokens": ["Ein", "Nes\u00b7tor", "sei\u00b7nes", "Volks", ",", "der", "Bun\u00b7te\u00b7Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Er trat gest\u00fctzt von zweien in den Kreis,", "tokens": ["Er", "trat", "ge\u00b7st\u00fctzt", "von", "zwei\u00b7en", "in", "den", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "CARD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und wie gespannt ein jeder auf ihn sah,", "tokens": ["Und", "wie", "ge\u00b7spannt", "ein", "je\u00b7der", "auf", "ihn", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ART", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Begann er seine Rede klug und weis:", "tokens": ["Be\u00b7gann", "er", "sei\u00b7ne", "Re\u00b7de", "klug", "und", "weis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u00bbihr, meine Br\u00fcder, h\u00f6ret selber ja,", "tokens": ["\u00bb", "ihr", ",", "mei\u00b7ne", "Br\u00fc\u00b7der", ",", "h\u00f6\u00b7ret", "sel\u00b7ber", "ja", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Was unsers gro\u00dfen Vaters Meinung ist;", "tokens": ["Was", "un\u00b7sers", "gro\u00b7\u00dfen", "Va\u00b7ters", "Mei\u00b7nung", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Er liebet seine roten Kinder ja.", "tokens": ["Er", "lie\u00b7bet", "sei\u00b7ne", "ro\u00b7ten", "Kin\u00b7der", "ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Er ist sehr gut, \u2013 ihr, meine Br\u00fcder, wi\u00dft,", "tokens": ["Er", "ist", "sehr", "gut", ",", "\u2013", "ihr", ",", "mei\u00b7ne", "Br\u00fc\u00b7der", ",", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "$(", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ich habe fr\u00fcher oft sein Wort vernommen \u2013", "tokens": ["Ich", "ha\u00b7be", "fr\u00fc\u00b7her", "oft", "sein", "Wort", "ver\u00b7nom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Er ist sehr gut, wohl ohne Falsch und List.", "tokens": ["Er", "ist", "sehr", "gut", ",", "wohl", "oh\u00b7ne", "Falsch", "und", "List", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wie erst vom gro\u00dfen Wasser er gekommen,", "tokens": ["Wie", "erst", "vom", "gro\u00b7\u00dfen", "Was\u00b7ser", "er", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Er war sehr klein, er trug ein rotes Kleid,", "tokens": ["Er", "war", "sehr", "klein", ",", "er", "trug", "ein", "ro\u00b7tes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Es mocht ihm l\u00e4nger nicht im Boote frommen.", "tokens": ["Es", "mocht", "ihm", "l\u00e4n\u00b7ger", "nicht", "im", "Boo\u00b7te", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKNEG", "APPRART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Der wei\u00dfe Mann tat unsern Br\u00fcdern leid;", "tokens": ["Der", "wei\u00b7\u00dfe", "Mann", "tat", "un\u00b7sern", "Br\u00fc\u00b7dern", "leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Er bat um Land, sein Feuer anzuz\u00fcnden,", "tokens": ["Er", "bat", "um", "Land", ",", "sein", "Feu\u00b7er", "an\u00b7zu\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und wartete geruhig auf Bescheid.", "tokens": ["Und", "war\u00b7te\u00b7te", "ge\u00b7ru\u00b7hig", "auf", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Er wollte, gab er vor, uns blo\u00df verk\u00fcnden,", "tokens": ["Er", "woll\u00b7te", ",", "gab", "er", "vor", ",", "uns", "blo\u00df", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Was vieles wir zu unserm Gl\u00fccke brauchten;", "tokens": ["Was", "vie\u00b7les", "wir", "zu", "un\u00b7serm", "Gl\u00fc\u00b7cke", "brauch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Wir aber wollten uns mit ihm verb\u00fcnden.", "tokens": ["Wir", "a\u00b7ber", "woll\u00b7ten", "uns", "mit", "ihm", "ver\u00b7b\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Am Ufer des Savannah-Stromes rauchten", "tokens": ["Am", "U\u00b7fer", "des", "Sa\u00b7van\u00b7nah\u00b7Stro\u00b7mes", "rauch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Die Muskotshihs mit ihm die Friedenspfeife;", "tokens": ["Die", "Mus\u00b7kots\u00b7hihs", "mit", "ihm", "die", "Frie\u00b7den\u00b7spfei\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Dort war's, wo in den Wind den Rauch sie hauchten.", "tokens": ["Dort", "wa\u00b7r's", ",", "wo", "in", "den", "Wind", "den", "Rauch", "sie", "hauch\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.34": {"text": "Sie machten ihm ein Feuer an; die Steife", "tokens": ["Sie", "mach\u00b7ten", "ihm", "ein", "Feu\u00b7er", "an", ";", "die", "Stei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Der Glieder w\u00e4rmte da der wei\u00dfe Mann;", "tokens": ["Der", "Glie\u00b7der", "w\u00e4rm\u00b7te", "da", "der", "wei\u00b7\u00dfe", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Sie gaben Land ihm, wo nach Wild er schweife.", "tokens": ["Sie", "ga\u00b7ben", "Land", "ihm", ",", "wo", "nach", "Wild", "er", "schwei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "$,", "PWAV", "APPR", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Er war sehr klein; es feindeten ihn an", "tokens": ["Er", "war", "sehr", "klein", ";", "es", "fein\u00b7de\u00b7ten", "ihn", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Des S\u00fcdens blasse M\u00e4nner, die um Beute", "tokens": ["Des", "S\u00fc\u00b7dens", "blas\u00b7se", "M\u00e4n\u00b7ner", ",", "die", "um", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Sich wider ihn erhoben; Krieg begann.", "tokens": ["Sich", "wi\u00b7der", "ihn", "er\u00b7ho\u00b7ben", ";", "Krieg", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVPP", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "F\u00fcr ihn ergriffen unsre jungen Leute", "tokens": ["F\u00fcr", "ihn", "er\u00b7grif\u00b7fen", "uns\u00b7re", "jun\u00b7gen", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Den Tomahawk, und gaben nicht ihn blo\u00df", "tokens": ["Den", "To\u00b7ma\u00b7hawk", ",", "und", "ga\u00b7ben", "nicht", "ihn", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Dem Messer zu skalpieren, das er scheute.", "tokens": ["Dem", "Mes\u00b7ser", "zu", "skal\u00b7pie\u00b7ren", ",", "das", "er", "scheu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und wie darauf er, seines Feindes los,", "tokens": ["Und", "wie", "da\u00b7rauf", "er", ",", "sei\u00b7nes", "Fein\u00b7des", "los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PAV", "PPER", "$,", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Sich unter uns erw\u00e4rmet und gen\u00e4hrt,", "tokens": ["Sich", "un\u00b7ter", "uns", "er\u00b7w\u00e4r\u00b7met", "und", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Da wuchs er auf, da ward er riesengro\u00df;", "tokens": ["Da", "wuchs", "er", "auf", ",", "da", "ward", "er", "rie\u00b7sen\u00b7gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Da hat sein Tritt das Jagdrevier verheert,", "tokens": ["Da", "hat", "sein", "Tritt", "das", "Jagd\u00b7re\u00b7vier", "ver\u00b7heert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Da hat er \u00fcberholt die fernsten Horden,", "tokens": ["Da", "hat", "er", "\u00fc\u00b7berh\u00b7olt", "die", "ferns\u00b7ten", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Und Wald und Flur und See f\u00fcr sich begehrt.", "tokens": ["Und", "Wald", "und", "Flur", "und", "See", "f\u00fcr", "sich", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Nach S\u00fcden reichte seine Hand und Norden,", "tokens": ["Nach", "S\u00fc\u00b7den", "reich\u00b7te", "sei\u00b7ne", "Hand", "und", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Und seine Stirne zu des Mondes Schild;", "tokens": ["Und", "sei\u00b7ne", "Stir\u00b7ne", "zu", "des", "Mon\u00b7des", "Schild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Da ist er unser gro\u00dfer Vater worden.", "tokens": ["Da", "ist", "er", "un\u00b7ser", "gro\u00b7\u00dfer", "Va\u00b7ter", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Zu seinen roten Kindern sprach er mild, \u2013", "tokens": ["Zu", "sei\u00b7nen", "ro\u00b7ten", "Kin\u00b7dern", "sprach", "er", "mild", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "Er liebt sie ja: \u203aGeht weiter, weiter! h\u00f6rt!", "tokens": ["Er", "liebt", "sie", "ja", ":", "\u203a", "Geht", "wei\u00b7ter", ",", "wei\u00b7ter", "!", "h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "VVFIN", "PTKVZ", "$,", "ADV", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "Sonst tret ich euch, so wie im Forst das Wild.\u2039", "tokens": ["Sonst", "tret", "ich", "euch", ",", "so", "wie", "im", "Forst", "das", "Wild", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "KOKOM", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.55": {"text": "Er stie\u00df sie mit dem Fu\u00dfe, unerh\u00f6rt!", "tokens": ["Er", "stie\u00df", "sie", "mit", "dem", "Fu\u00b7\u00dfe", ",", "un\u00b7er\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.56": {"text": "Den Oconih hin\u00fcber; dann zertrat er", "tokens": ["Den", "O\u00b7co\u00b7nih", "hin\u00b7\u00fc\u00b7ber", ";", "dann", "zer\u00b7trat", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "Die Gr\u00e4ber ihrer V\u00e4ter ungest\u00f6rt.", "tokens": ["Die", "Gr\u00e4\u00b7ber", "ih\u00b7rer", "V\u00e4\u00b7ter", "un\u00b7ge\u00b7s