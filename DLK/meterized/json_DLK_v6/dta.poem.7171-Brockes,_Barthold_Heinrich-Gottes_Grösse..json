{"dta.poem.7171": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch der kl\u00fcg\u2019sten Geister Schrancken", "tokens": ["Auch", "der", "kl\u00fcg'\u00b7sten", "Geis\u00b7ter", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind zu eng, zu kurtz ihr Ziel.", "tokens": ["Sind", "zu", "eng", ",", "zu", "kurtz", "ihr", "Ziel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den gesch\u00e4rfftesten Gedancken", "tokens": ["Den", "ge\u00b7sch\u00e4rff\u00b7tes\u00b7ten", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Nichts schon viel zu viel:", "tokens": ["Ist", "ein", "Nichts", "schon", "viel", "zu", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIS", "ADV", "ADV", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer will sich denn tr\u00e4umen lassen,", "tokens": ["Wer", "will", "sich", "denn", "tr\u00e4u\u00b7men", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein unendlichs All zu fassen,", "tokens": ["Ein", "un\u00b7end\u00b7lichs", "All", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das, was Erd\u2019 und Himmel hegt,", "tokens": ["Das", ",", "was", "Erd'", "und", "Him\u00b7mel", "hegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In sich selbst begreifft und tr\u00e4gt?", "tokens": ["In", "sich", "selbst", "be\u00b7greifft", "und", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}