{"dta.poem.13240": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewahr ists, die Feinde seynd so m\u00e4chtig, als erbost;", "tokens": ["\u201e", "wahr", "ists", ",", "die", "Fein\u00b7de", "seynd", "so", "m\u00e4ch\u00b7tig", ",", "als", "er\u00b7bost", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "130\u201dDas aber schreckt mich nicht; GOtt und das Recht gibt Trost.", "tokens": ["\"", "Das", "a\u00b7ber", "schreckt", "mich", "nicht", ";", "Gott", "und", "das", "Recht", "gibt", "Trost", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "KON", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201eob wir schon oft Verlust und Uebermacht gelitten,", "tokens": ["\u201e", "ob", "wir", "schon", "oft", "Ver\u00b7lust", "und", "Ue\u00b7ber\u00b7macht", "ge\u00b7lit\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ehat doch der Feind f\u00fcr sich nur Ungemach erstritten.", "tokens": ["\u201e", "hat", "doch", "der", "Feind", "f\u00fcr", "sich", "nur", "Un\u00b7ge\u00b7mach", "er\u00b7strit\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "APPR", "PRF", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas halffen Schwert und Feur? was n\u00fczte Blut und Sieg?", "tokens": ["\u201e", "was", "half\u00b7fen", "Schwert", "und", "Feur", "?", "was", "n\u00fcz\u00b7te", "Blut", "und", "Sieg", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJA", "NN", "KON", "NN", "$.", "PWS", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eo Landverderblicher! O Herrschsucht-voller Krieg!", "tokens": ["\u201e", "o", "Land\u00b7ver\u00b7derb\u00b7li\u00b7cher", "!", "O", "Herrschsucht\u00b7vol\u00b7ler", "Krieg", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "135\u201dNichts, als da\u00df mancher Wall vor deiner Wuth gezittert;", "tokens": ["\"", "Nichts", ",", "als", "da\u00df", "man\u00b7cher", "Wall", "vor", "dei\u00b7ner", "Wuth", "ge\u00b7zit\u00b7tert", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "$,", "KOKOM", "KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eund die Gerechtigkeit sich wieder dich erbittert.", "tokens": ["\u201e", "und", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "sich", "wie\u00b7der", "dich", "er\u00b7bit\u00b7tert", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "PRF", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}