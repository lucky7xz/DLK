{"textgrid.poem.45973": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[merkt, o g\u00fct'ge Freunde meines Klanges]", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Merkt, o g\u00fct'ge Freunde meines Klanges,", "tokens": ["Merkt", ",", "o", "g\u00fct'\u00b7ge", "Freun\u00b7de", "mei\u00b7nes", "Klan\u00b7ges", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn des Liedes Traum Sturmschwingen regt,", "tokens": ["Wenn", "des", "Lie\u00b7des", "Traum", "Sturm\u00b7schwin\u00b7gen", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ein Schwanensang hier hohen Ranges", "tokens": ["Da\u00df", "ein", "Schwa\u00b7nen\u00b7sang", "hier", "ho\u00b7hen", "Ran\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Adlerfl\u00fcgel-Klang zur Sonne tr\u00e4gt.", "tokens": ["Ad\u00b7ler\u00b7fl\u00fc\u00b7gel\u00b7Klang", "zur", "Son\u00b7ne", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ist ihm, ja es selbst sich unterlegt.", "tokens": ["Ist", "ihm", ",", "ja", "es", "selbst", "sich", "un\u00b7ter\u00b7legt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "PPER", "ADV", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Habe ", "tokens": ["Ha\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und so wird nach Weisen es gesungen,", "tokens": ["Und", "so", "wird", "nach", "Wei\u00b7sen", "es", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Deren Lichtbahn es zu singen strebt;", "tokens": ["De\u00b7ren", "Licht\u00b7bahn", "es", "zu", "sin\u00b7gen", "strebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ob dem Dichter, ob dem S\u00e4nger mehr gelungen", "tokens": ["Ob", "dem", "Dich\u00b7ter", ",", "ob", "dem", "S\u00e4n\u00b7ger", "mehr", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Ob das Wort, der Klang lichttrunkner schwebt? \u2013", "tokens": ["Ob", "das", "Wort", ",", "der", "Klang", "licht\u00b7trunk\u00b7ner", "schwebt", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Pr\u00fcft den Kranz, ", "tokens": ["Pr\u00fcft", "den", "Kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Da\u00df ihr ", "tokens": ["Da\u00df", "ihr"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Und den S\u00e4nger gr\u00fc\u00dft mit ", "tokens": ["Und", "den", "S\u00e4n\u00b7ger", "gr\u00fc\u00dft", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Die in gleicher Ehre stumm geworden.", "tokens": ["Die", "in", "glei\u00b7cher", "Eh\u00b7re", "stumm", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}