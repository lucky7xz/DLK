{"textgrid.poem.62101": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[die Fenster sind gefroren]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fenster sind gefroren,", "tokens": ["Die", "Fens\u00b7ter", "sind", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie eis'ge Bl\u00fcmlein, schau:", "tokens": ["Wie", "eis'\u00b7ge", "Bl\u00fcm\u00b7lein", ",", "schau", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das sind die falschen Menschen,", "tokens": ["Das", "sind", "die", "fal\u00b7schen", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf menschlich sch\u00f6ner Au!", "tokens": ["Auf", "menschlich", "sch\u00f6\u00b7ner", "Au", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}