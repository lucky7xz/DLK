{"textgrid.poem.24753": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zerrissnen Tones, \u00fcberlauter Rede", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerrissnen Tones, \u00fcberlauter Rede", "tokens": ["Zer\u00b7riss\u00b7nen", "To\u00b7nes", ",", "\u00fc\u00b7berl\u00b7au\u00b7ter", "Re\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verf\u00e4nglich Blendwerk mu\u00df vergessen sein:", "tokens": ["Ver\u00b7f\u00e4ng\u00b7lich", "Blend\u00b7werk", "mu\u00df", "ver\u00b7ges\u00b7sen", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Allein den bunten schmerzverzognen Lippen", "tokens": ["Al\u00b7lein", "den", "bun\u00b7ten", "schmerz\u00b7ver\u00b7zog\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entrollte, unverweslicher als Perlen", "tokens": ["Ent\u00b7roll\u00b7te", ",", "un\u00b7ver\u00b7wes\u00b7li\u00b7cher", "als", "Per\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und leuchtender, zuweilen ein Gebild:", "tokens": ["Und", "leuch\u00b7ten\u00b7der", ",", "zu\u00b7wei\u00b7len", "ein", "Ge\u00b7bild", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das traget am lebendigen Leib, und nie", "tokens": ["Das", "tra\u00b7get", "am", "le\u00b7ben\u00b7di\u00b7gen", "Leib", ",", "und", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Verliert es seinen innern feuchten Glanz.", "tokens": ["Ver\u00b7liert", "es", "sei\u00b7nen", "in\u00b7nern", "feuch\u00b7ten", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}