{"textgrid.poem.47190": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoffte, da\u00df du solltest bei mir bleiben,", "tokens": ["Hoff\u00b7te", ",", "da\u00df", "du", "soll\u00b7test", "bei", "mir", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nie verlassen, T\u00f6chterchen, den Vater,", "tokens": ["Nie", "ver\u00b7las\u00b7sen", ",", "T\u00f6ch\u00b7ter\u00b7chen", ",", "den", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn die Knaben aus dem Hause liefen,", "tokens": ["Wenn", "die", "Kna\u00b7ben", "aus", "dem", "Hau\u00b7se", "lie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In der Welt ihr eignes Gl\u00fcck zu suchen,", "tokens": ["In", "der", "Welt", "ihr", "eig\u00b7nes", "Gl\u00fcck", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Losgerissen von der Eltern Herzen;", "tokens": ["Los\u00b7ge\u00b7ris\u00b7sen", "von", "der", "El\u00b7tern", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "W\u00fcrdest du am stillen Herde walten,", "tokens": ["W\u00fcr\u00b7dest", "du", "am", "stil\u00b7len", "Her\u00b7de", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wo du spielend jetzt dich um die Mutter", "tokens": ["Wo", "du", "spie\u00b7lend", "jetzt", "dich", "um", "die", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "PRF", "APPR", "ART", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "M\u00fchst, in ihre Stell' im Ernste treten,", "tokens": ["M\u00fchst", ",", "in", "ih\u00b7re", "Stell'", "im", "Erns\u00b7te", "tre\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wohlvers\u00fc\u00dft den Kaffee selbst mir bringen,", "tokens": ["Wohl\u00b7ver\u00b7s\u00fc\u00dft", "den", "Kaf\u00b7fee", "selbst", "mir", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wie sie jetzt ihn bringt, von dir begleitet,", "tokens": ["Wie", "sie", "jetzt", "ihn", "bringt", ",", "von", "dir", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und nun bringst du diesen bittern Trank mir!", "tokens": ["Und", "nun", "bringst", "du", "die\u00b7sen", "bit\u00b7tern", "Trank", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ihn mir zu vers\u00fc\u00dfen, mu\u00df ich sagen:", "tokens": ["Ihn", "mir", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", ",", "mu\u00df", "ich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Ewig konntest du mir doch nicht bleiben;", "tokens": ["E\u00b7wig", "konn\u00b7test", "du", "mir", "doch", "nicht", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Unversehens klopfet an ein Freier,", "tokens": ["Un\u00b7ver\u00b7se\u00b7hens", "klop\u00b7fet", "an", "ein", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Und entgegen klopfet ihm dein Herzchen,", "tokens": ["Und", "ent\u00b7ge\u00b7gen", "klop\u00b7fet", "ihm", "dein", "Herz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Und: Herein! werd' ich wohl sagen m\u00fcssen.", "tokens": ["Und", ":", "Her\u00b7ein", "!", "werd'", "ich", "wohl", "sa\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "PTKVZ", "$.", "VAFIN", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Und die junge Gattin wird den Gatten", "tokens": ["Und", "die", "jun\u00b7ge", "Gat\u00b7tin", "wird", "den", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Lieber haben als den alten Vater,", "tokens": ["Lie\u00b7ber", "ha\u00b7ben", "als", "den", "al\u00b7ten", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Und die Kinder lieber dann als beide.", "tokens": ["Und", "die", "Kin\u00b7der", "lie\u00b7ber", "dann", "als", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "KOUS", "PIS", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Denn da\u00df \u00fcber alles man ein Kind liebt,", "tokens": ["Denn", "da\u00df", "\u00fc\u00b7ber", "al\u00b7les", "man", "ein", "Kind", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PIS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Lern' ich eben, da ich dich verloren.", "tokens": ["Lern'", "ich", "e\u00b7ben", ",", "da", "ich", "dich", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Nun ersparst du diese Eifersucht mir,", "tokens": ["Nun", "er\u00b7sparst", "du", "die\u00b7se", "Ei\u00b7fer\u00b7sucht", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "PPER", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "T\u00f6chterchen, nun kannst du deinen Vater", "tokens": ["T\u00f6ch\u00b7ter\u00b7chen", ",", "nun", "kannst", "du", "dei\u00b7nen", "Va\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Einzig lieb, wie er dich selbst, behalten.", "tokens": ["Ein\u00b7zig", "lieb", ",", "wie", "er", "dich", "selbst", ",", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "PWAV", "PPER", "PRF", "ADV", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}