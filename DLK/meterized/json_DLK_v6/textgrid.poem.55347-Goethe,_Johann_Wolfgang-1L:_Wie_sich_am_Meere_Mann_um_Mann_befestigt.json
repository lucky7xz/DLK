{"textgrid.poem.55347": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sich am Meere Mann um Mann befestigt", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich am Meere Mann um Mann befestigt", "tokens": ["Wie", "sich", "am", "Mee\u00b7re", "Mann", "um", "Mann", "be\u00b7fes\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPRART", "ADJA", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und am Gestade Schiffer \u00fcberl\u00e4stigt,", "tokens": ["Und", "am", "Ge\u00b7sta\u00b7de", "Schif\u00b7fer", "\u00fc\u00b7berl\u00b7\u00e4s\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die engen Pfade v\u00f6llig weglos macht,", "tokens": ["Die", "en\u00b7gen", "Pfa\u00b7de", "v\u00f6l\u00b7lig", "weg\u00b7los", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf Sicherheit, mehr auf Gewalt bedacht,", "tokens": ["Auf", "Si\u00b7cher\u00b7heit", ",", "mehr", "auf", "Ge\u00b7walt", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald Recht, bald Plackerei, sein selbst gewi\u00df,", "tokens": ["Bald", "Recht", ",", "bald", "Pla\u00b7cke\u00b7rei", ",", "sein", "selbst", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,", "PPOSAT", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sei's, wie es sei, und immer Hindernis,", "tokens": ["Sei's", ",", "wie", "es", "sei", ",", "und", "im\u00b7mer", "Hin\u00b7der\u00b7nis", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VAFIN", "$,", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So Tag und Nacht den Reisenden zur Last:", "tokens": ["So", "Tag", "und", "Nacht", "den", "Rei\u00b7sen\u00b7den", "zur", "Last", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}