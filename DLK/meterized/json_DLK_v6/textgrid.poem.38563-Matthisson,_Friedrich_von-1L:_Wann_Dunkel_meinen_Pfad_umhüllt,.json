{"textgrid.poem.38563": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann Dunkel meinen Pfad umh\u00fcllt,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann Dunkel meinen Pfad umh\u00fcllt,", "tokens": ["Wann", "Dun\u00b7kel", "mei\u00b7nen", "Pfad", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Werd' ich mit heiligem Entz\u00fccken", "tokens": ["Werd'", "ich", "mit", "hei\u00b7li\u00b7gem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ahndungsvoller Ruh', o Bild,", "tokens": ["Und", "ahn\u00b7dungs\u00b7vol\u00b7ler", "Ruh'", ",", "o", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An die beklommne Brust dich dr\u00fccken!", "tokens": ["An", "die", "be\u00b7klomm\u00b7ne", "Brust", "dich", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann wird, wie Fr\u00fchlingsmorgenschein,", "tokens": ["Dann", "wird", ",", "wie", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Glaubens Klarheit mich umgeben,", "tokens": ["Des", "Glau\u00b7bens", "Klar\u00b7heit", "mich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00e4chtiger durch mein Gebein", "tokens": ["Und", "m\u00e4ch\u00b7ti\u00b7ger", "durch", "mein", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Himmels Vorempfindung beben.", "tokens": ["Des", "Him\u00b7mels", "Vor\u00b7emp\u00b7fin\u00b7dung", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich soll einst, o geliebtes Bild,", "tokens": ["Dich", "soll", "einst", ",", "o", "ge\u00b7lieb\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In der Verwesung stillen Gr\u00fcnden,", "tokens": ["In", "der", "Ver\u00b7we\u00b7sung", "stil\u00b7len", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In meines Herzens Staub geh\u00fcllt,", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Staub", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Auferstehung Morgen finden!", "tokens": ["Der", "Auf\u00b7er\u00b7ste\u00b7hung", "Mor\u00b7gen", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}