{"textgrid.poem.43134": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Urewiger!", "tokens": ["U\u00b7re\u00b7wi\u00b7ger", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Unendlicher!", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Du h\u00f6rst das Schreien", "tokens": ["Du", "h\u00f6rst", "das", "Schrei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der ringenden Seele.", "tokens": ["Der", "rin\u00b7gen\u00b7den", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu Dir gefl\u00fcchtet", "tokens": ["Zu", "Dir", "ge\u00b7fl\u00fcch\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bin ich in Stunden,", "tokens": ["Bin", "ich", "in", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wo Dir entfremdet", "tokens": ["Wo", "Dir", "ent\u00b7frem\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und Dich verh\u00f6hnend,", "tokens": ["Und", "Dich", "ver\u00b7h\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "In Schmutz und S\u00fcnde", "tokens": ["In", "Schmutz", "und", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Sich Jene w\u00e4lzten,", "tokens": ["Sich", "Je\u00b7ne", "w\u00e4lz\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PDS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Die gestern lobpriesen", "tokens": ["Die", "ge\u00b7stern", "lob\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Dein heiliges Wort,", "tokens": ["Dein", "hei\u00b7li\u00b7ges", "Wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Die morgen wieder", "tokens": ["Die", "mor\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Vor Deinem Kreuze", "tokens": ["Vor", "Dei\u00b7nem", "Kreu\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Im Staub sich winden,", "tokens": ["Im", "Staub", "sich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Ein heiliges Antlitz", "tokens": ["Ein", "hei\u00b7li\u00b7ges", "Ant\u00b7litz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Und heilige Sitten", "tokens": ["Und", "hei\u00b7li\u00b7ge", "Sit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Fromml\u00e4chelnd zeigen. \u2013", "tokens": ["Fromm\u00b7l\u00e4\u00b7chelnd", "zei\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVINF", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "O ewiges Wesen", "tokens": ["O", "e\u00b7wi\u00b7ges", "We\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Barmherzig bist Du,", "tokens": ["Barm\u00b7her\u00b7zig", "bist", "Du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Du bist milde,", "tokens": ["Du", "bist", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "G\u00f6ttlich, g\u00fctig! \u2013", "tokens": ["G\u00f6tt\u00b7lich", ",", "g\u00fc\u00b7tig", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Ich glaube an Dich,", "tokens": ["Ich", "glau\u00b7be", "an", "Dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "Ich hoffe auf Dich,", "tokens": ["Ich", "hof\u00b7fe", "auf", "Dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Und wenn auch versinkend,", "tokens": ["Und", "wenn", "auch", "ver\u00b7sin\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VVPP", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.26": {"text": "Ruf ich zu Dir!", "tokens": ["Ruf", "ich", "zu", "Dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Du h\u00f6rst dies Rufen .....", "tokens": ["Du", "h\u00f6rst", "dies", "Ru\u00b7fen", "....."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Der Kr\u00e4merseelen", "tokens": ["Der", "Kr\u00e4\u00b7mer\u00b7see\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Erb\u00e4rmlich Winseln", "tokens": ["Er\u00b7b\u00e4rm\u00b7lich", "Win\u00b7seln"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Dringt nicht an Dein Ohr:", "tokens": ["Dringt", "nicht", "an", "Dein", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.31": {"text": "Doch dort, wo Jammer", "tokens": ["Doch", "dort", ",", "wo", "Jam\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "Und gro\u00dfe Schuld", "tokens": ["Und", "gro\u00b7\u00dfe", "Schuld"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Vor Dir sich beugen", "tokens": ["Vor", "Dir", "sich", "beu\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "In schmerzlicher Reue,", "tokens": ["In", "schmerz\u00b7li\u00b7cher", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Dort, wo beladen", "tokens": ["Dort", ",", "wo", "be\u00b7la\u00b7den"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Mit menschlichem Elend,", "tokens": ["Mit", "menschli\u00b7chem", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Von Dir ein Wesen,", "tokens": ["Von", "Dir", "ein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "S\u00fcndenm\u00fcde,", "tokens": ["S\u00fcn\u00b7den\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.39": {"text": "Lebensm\u00fcde,", "tokens": ["Le\u00b7bens\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.40": {"text": "Erl\u00f6sung heischt,", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "heischt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.41": {"text": "Dort wirst Du h\u00f6ren, \u2013", "tokens": ["Dort", "wirst", "Du", "h\u00f6\u00b7ren", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Denn Du bist Gott!", "tokens": ["Denn", "Du", "bist", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}