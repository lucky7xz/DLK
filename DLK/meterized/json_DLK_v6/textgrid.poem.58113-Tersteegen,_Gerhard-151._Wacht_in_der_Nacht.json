{"textgrid.poem.58113": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "151. Wacht in der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dunkeln Wegen harre nur", "tokens": ["In", "dun\u00b7keln", "We\u00b7gen", "har\u00b7re", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kehr nicht aus zur Kreatur,", "tokens": ["Und", "kehr", "nicht", "aus", "zur", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Jungfrau, Gottes Braut;", "tokens": ["O", "Jung\u00b7frau", ",", "Got\u00b7tes", "Braut", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denk nicht, dein Br\u00e4ut'gam sei dann weit!", "tokens": ["Denk", "nicht", ",", "dein", "Br\u00e4ut'\u00b7gam", "sei", "dann", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "PPOSAT", "NE", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer sich h\u00e4lt wachend und bereit,", "tokens": ["Wer", "sich", "h\u00e4lt", "wa\u00b7chend", "und", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Mitternacht ihn schaut.", "tokens": ["Zur", "Mit\u00b7ter\u00b7nacht", "ihn", "schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}