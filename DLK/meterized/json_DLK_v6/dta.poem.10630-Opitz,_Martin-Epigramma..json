{"dta.poem.10630": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ach schicke mir doch zu ein K\u00fcssichin/ mein Leben/", "tokens": ["Ach", "schi\u00b7cke", "mir", "doch", "zu", "ein", "K\u00fcs\u00b7si\u00b7chin", "/", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcrchstu/ da\u00df auff dem Weg es jemandt m\u00f6cht auffheben?", "tokens": ["F\u00fcrch\u00b7stu", "/", "da\u00df", "auff", "dem", "Weg", "es", "je\u00b7mandt", "m\u00f6cht", "auff\u00b7he\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "APPR", "ART", "NN", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ey druck auff meinen Mundt dein zartes Mundelein/", "tokens": ["Ey", "druck", "auff", "mei\u00b7nen", "Mundt", "dein", "zar\u00b7tes", "Mun\u00b7del\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wirdt es vor Gefahr der Diebe sicher sein.", "tokens": ["So", "wirdt", "es", "vor", "Ge\u00b7fahr", "der", "Die\u00b7be", "si\u00b7cher", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}