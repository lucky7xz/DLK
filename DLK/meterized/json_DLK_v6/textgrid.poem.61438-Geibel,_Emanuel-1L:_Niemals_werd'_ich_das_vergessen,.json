{"textgrid.poem.61438": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Niemals werd' ich das vergessen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemals werd' ich das vergessen,", "tokens": ["Nie\u00b7mals", "werd'", "ich", "das", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie dein Arm mich noch umfing,", "tokens": ["Wie", "dein", "Arm", "mich", "noch", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Wort beim bangen Pressen", "tokens": ["Je\u00b7des", "Wort", "beim", "ban\u00b7gen", "Pres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir in Tr\u00e4nen unterging.", "tokens": ["Dir", "in", "Tr\u00e4\u00b7nen", "un\u00b7ter\u00b7ging", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, wir lernten erst im Scheiden", "tokens": ["Ach", ",", "wir", "lern\u00b7ten", "erst", "im", "Schei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Liebe ganz verstehn,", "tokens": ["Uns\u00b7re", "Lie\u00b7be", "ganz", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und doch war's uns beiden, beiden:", "tokens": ["Und", "doch", "wa\u00b7r's", "uns", "bei\u00b7den", ",", "bei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PIAT", "$,", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "'s ist auf Nimmerwiedersehn!", "tokens": ["'s", "ist", "auf", "Nim\u00b7mer\u00b7wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seit der Stunde jener Schmerzen", "tokens": ["Seit", "der", "Stun\u00b7de", "je\u00b7ner", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch den Druck von deiner Hand", "tokens": ["Noch", "den", "Druck", "von", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchl' ich k\u00fchl auf meinem Herzen,", "tokens": ["F\u00fchl'", "ich", "k\u00fchl", "auf", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich damals ihn empfand.", "tokens": ["Wie", "ich", "da\u00b7mals", "ihn", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn alles schweigt um mich,", "tokens": ["Und", "wenn", "al\u00b7les", "schweigt", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir aufs Bett die Sterne scheinen,", "tokens": ["Mir", "aufs", "Bett", "die", "Ster\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist mir oft, ich h\u00f6re dich", "tokens": ["Ist", "mir", "oft", ",", "ich", "h\u00f6\u00b7re", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Ferne weinen.", "tokens": ["In", "der", "Fer\u00b7ne", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}