{"dta.poem.20567": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "23.  \n  Knesebeck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Schlacht von Bautzen war geschlagen,", "tokens": ["Schlacht", "von", "Baut\u00b7zen", "war", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Muthvoll noch war Bundesheer.", "tokens": ["Muth\u00b7voll", "noch", "war", "Bun\u00b7des\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neuen Kampf gleich m\u00f6cht es wagen,", "tokens": ["Neu\u00b7en", "Kampf", "gleich", "m\u00f6cht", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch manch\u2019 F\u00fchrer w\u00fcnscht dies sehr.", "tokens": ["Auch", "man\u00b7ch'", "F\u00fch\u00b7rer", "w\u00fcnscht", "dies", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ueberz\u00e4hlig war solch\u2019 Meinung", "tokens": ["Ue\u00b7ber\u00b7z\u00e4h\u00b7lig", "war", "solch'", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der Schlacht im Kriegesrath.", "tokens": ["Nach", "der", "Schlacht", "im", "Krie\u00b7ges\u00b7rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch von Einem kam Verneinung,", "tokens": ["Doch", "von", "Ei\u00b7nem", "kam", "Ver\u00b7nei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der mit Recht schob auf die That.", "tokens": ["Der", "mit", "Recht", "schob", "auf", "die", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Erw\u00e4hnte war ein Dichter.", "tokens": ["Der", "Er\u00b7w\u00e4hn\u00b7te", "war", "ein", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "\u201edichter sind beschr\u00e4nkte Leut\u2019,\u201c", "tokens": ["\u201e", "dich\u00b7ter", "sind", "be\u00b7schr\u00e4nk\u00b7te", "Leut'", ",", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dachte manch\u2019 erhab\u2019ner Richter", "tokens": ["Dach\u00b7te", "man\u00b7ch'", "er\u00b7hab'\u00b7ner", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vormals, viele denken\u2019s heut. \u2014", "tokens": ["Vor\u00b7mals", ",", "vie\u00b7le", "den\u00b7ken's", "heut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PIAT", "PDS", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja es richten die Poeten", "tokens": ["Ja", "es", "rich\u00b7ten", "die", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Stets nur an Confusion,", "tokens": ["Stets", "nur", "an", "Con\u00b7fu\u00b7si\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und beim Kampf sie nur trompeten,", "tokens": ["Und", "beim", "Kampf", "sie", "nur", "trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meinte schon ", "tokens": ["Mein\u00b7te", "schon"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Der besonnen war und stark.", "tokens": ["Der", "be\u00b7son\u00b7nen", "war", "und", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leider weilt Er schon dort droben!", "tokens": ["Lei\u00b7der", "weilt", "Er", "schon", "dort", "dro\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "(schwager Jhm ist ", "tokens": ["(", "schwa\u00b7ger", "Jhm", "ist"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ADJD", "PPER", "VAFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit der Feder, mit dem Degen", "tokens": ["Mit", "der", "Fe\u00b7der", ",", "mit", "dem", "De\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Focht mit ", "tokens": ["Focht", "mit"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Drum soll stets und von Rechtswegen", "tokens": ["Drum", "soll", "stets", "und", "von", "Rechts\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ADV", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Knesebeck gefeiert sein!", "tokens": ["Kne\u00b7se\u00b7beck", "ge\u00b7fei\u00b7ert", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}