{"textgrid.poem.55316": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Verm\u00e4chtnis", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Wesen kann zu Nichts zerfallen!", "tokens": ["Kein", "We\u00b7sen", "kann", "zu", "Nichts", "zer\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ew'ge regt sich fort in allen,", "tokens": ["Das", "Ew'\u00b7ge", "regt", "sich", "fort", "in", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "PTKVZ", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Sein erhalte dich begl\u00fcckt!", "tokens": ["Am", "Sein", "er\u00b7hal\u00b7te", "dich", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Sein ist ewig: denn Gesetze", "tokens": ["Das", "Sein", "ist", "e\u00b7wig", ":", "denn", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bewahren die lebend'gen Sch\u00e4tze,", "tokens": ["Be\u00b7wah\u00b7ren", "die", "le\u00b7ben\u00b7d'\u00b7gen", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aus welchen sich das All geschm\u00fcckt.", "tokens": ["Aus", "wel\u00b7chen", "sich", "das", "All", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Wahre war schon l\u00e4ngst gefunden,", "tokens": ["Das", "Wah\u00b7re", "war", "schon", "l\u00e4ngst", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat edle Geisterschaft verbunden;", "tokens": ["Hat", "ed\u00b7le", "Geis\u00b7ter\u00b7schaft", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte Wahre, fa\u00df es an! Verdank es,", "tokens": ["Das", "al\u00b7te", "Wah\u00b7re", ",", "fa\u00df", "es", "an", "!", "Ver\u00b7dank", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erdensohn, dem Weisen,", "tokens": ["Er\u00b7den\u00b7sohn", ",", "dem", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der ihr, die Sonne zu umkreisen,", "tokens": ["Der", "ihr", ",", "die", "Son\u00b7ne", "zu", "um\u00b7krei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dem Geschwister wies die Bahn.", "tokens": ["Und", "dem", "Ge\u00b7schwis\u00b7ter", "wies", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sofort nun wende dich nach innen,", "tokens": ["So\u00b7fort", "nun", "wen\u00b7de", "dich", "nach", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Zentrum findest du dadrinnen,", "tokens": ["Das", "Zent\u00b7rum", "fin\u00b7dest", "du", "dad\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Woran kein Edler zweifeln mag.", "tokens": ["Wo\u00b7ran", "kein", "Ed\u00b7ler", "zwei\u00b7feln", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirst keine Regel da vermissen:", "tokens": ["Wirst", "kei\u00b7ne", "Re\u00b7gel", "da", "ver\u00b7mis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn das selbst\u00e4ndige Gewissen", "tokens": ["Denn", "das", "selb\u00b7st\u00e4n\u00b7di\u00b7ge", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Sonne deinem Sittentag.", "tokens": ["Ist", "Son\u00b7ne", "dei\u00b7nem", "Sit\u00b7ten\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Sinnen hast du dann zu trauen,", "tokens": ["Den", "Sin\u00b7nen", "hast", "du", "dann", "zu", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Falsches lassen sie dich schauen,", "tokens": ["Kein", "Fal\u00b7sches", "las\u00b7sen", "sie", "dich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dein Verstand dich wach erh\u00e4lt.", "tokens": ["Wenn", "dein", "Ver\u00b7stand", "dich", "wach", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit frischem Blick bemerke freudig,", "tokens": ["Mit", "fri\u00b7schem", "Blick", "be\u00b7mer\u00b7ke", "freu\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wandle sicher wie geschmeidig", "tokens": ["Und", "wand\u00b7le", "si\u00b7cher", "wie", "ge\u00b7schmei\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KOKOM", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Auen reichbegabter Welt.", "tokens": ["Durch", "Au\u00b7en", "reich\u00b7be\u00b7gab\u00b7ter", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Genie\u00dfe m\u00e4\u00dfig F\u00fcll und Segen,", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "m\u00e4\u00b7\u00dfig", "F\u00fcll", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vernunft sei \u00fcberall zugegen,", "tokens": ["Ver\u00b7nunft", "sei", "\u00fc\u00b7be\u00b7rall", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Leben sich des Lebens freut.", "tokens": ["Wo", "Le\u00b7ben", "sich", "des", "Le\u00b7bens", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ist Vergangenheit best\u00e4ndig,", "tokens": ["Dann", "ist", "Ver\u00b7gan\u00b7gen\u00b7heit", "be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das K\u00fcnftige voraus lebendig,", "tokens": ["Das", "K\u00fcnf\u00b7ti\u00b7ge", "vo\u00b7raus", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Augenblick ist Ewigkeit.", "tokens": ["Der", "Au\u00b7gen\u00b7blick", "ist", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und war es endlich dir gelungen,", "tokens": ["Und", "war", "es", "end\u00b7lich", "dir", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bist du vom Gef\u00fchl durchdrungen:", "tokens": ["Und", "bist", "du", "vom", "Ge\u00b7f\u00fchl", "durch\u00b7drun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was fruchtbar ist, allein ist wahr \u2013", "tokens": ["Was", "frucht\u00b7bar", "ist", ",", "al\u00b7lein", "ist", "wahr", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "ADV", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du pr\u00fcfst das allgemeine Walten,", "tokens": ["Du", "pr\u00fcfst", "das", "all\u00b7ge\u00b7mei\u00b7ne", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es wird nach seiner Weise schalten,", "tokens": ["Es", "wird", "nach", "sei\u00b7ner", "Wei\u00b7se", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geselle dich zur kleinsten Schar.", "tokens": ["Ge\u00b7sel\u00b7le", "dich", "zur", "kleins\u00b7ten", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und wie von alters her im stillen", "tokens": ["Und", "wie", "von", "al\u00b7ters", "her", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADV", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Liebewerk nach eignem Willen", "tokens": ["Ein", "Lie\u00b7be\u00b7werk", "nach", "eig\u00b7nem", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Philosoph, der Dichter schuf,", "tokens": ["Der", "Phi\u00b7lo\u00b7soph", ",", "der", "Dich\u00b7ter", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wirst du sch\u00f6nste Gunst erzielen:", "tokens": ["So", "wirst", "du", "sch\u00f6ns\u00b7te", "Gunst", "er\u00b7zie\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn edlen Seelen vorzuf\u00fchlen", "tokens": ["Denn", "ed\u00b7len", "See\u00b7len", "vor\u00b7zu\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist w\u00fcnschenswertester Beruf.", "tokens": ["Ist", "w\u00fcn\u00b7schens\u00b7wer\u00b7tes\u00b7ter", "Be\u00b7ruf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}