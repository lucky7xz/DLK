{"textgrid.poem.58091": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "129. Der blinden Liebe Art", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die blinde Liebe acht't es nicht,", "tokens": ["Die", "blin\u00b7de", "Lie\u00b7be", "acht't", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob's gleich ihr selbst an Kraft gebricht;", "tokens": ["Ob's", "gleich", "ihr", "selbst", "an", "Kraft", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wei\u00df: Gott alles kann.", "tokens": ["Sie", "wei\u00df", ":", "Gott", "al\u00b7les", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "PIS", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die blinde Liebe auch nicht denkt,", "tokens": ["Die", "blin\u00b7de", "Lie\u00b7be", "auch", "nicht", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob dies erquickt und jenes kr\u00e4nkt;", "tokens": ["Ob", "dies", "er\u00b7quickt", "und", "je\u00b7nes", "kr\u00e4nkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVPP", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie siehet Gott nur an.", "tokens": ["Sie", "sie\u00b7het", "Gott", "nur", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die blinde Liebe will nicht sehn,", "tokens": ["Die", "blin\u00b7de", "Lie\u00b7be", "will", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie folget, ohne zu verstehn,", "tokens": ["Sie", "fol\u00b7get", ",", "oh\u00b7ne", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die blinde Liebe sich ergibt", "tokens": ["Die", "blin\u00b7de", "Lie\u00b7be", "sich", "er\u00b7gibt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auf ewig ganz dem, den sie liebt,", "tokens": ["Auf", "e\u00b7wig", "ganz", "dem", ",", "den", "sie", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Sich selbst in ihm verliert.", "tokens": ["Sich", "selbst", "in", "ihm", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}