{"textgrid.poem.42664": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Strahl der Klugheit, des Verstandes!", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Strahl der Klugheit, des Verstandes!", "tokens": ["Strahl", "der", "Klug\u00b7heit", ",", "des", "Ver\u00b7stan\u00b7des", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leitstern unsers Bruderbandes!", "tokens": ["Leits\u00b7tern", "un\u00b7sers", "Bru\u00b7der\u00b7ban\u00b7des", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottesgabe! Geist der Ruh!", "tokens": ["Got\u00b7tes\u00b7ga\u00b7be", "!", "Geist", "der", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steig herab auf unsre Br\u00fcder!", "tokens": ["Steig", "her\u00b7ab", "auf", "uns\u00b7re", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer dich liebt, den liebst du wieder:", "tokens": ["Wer", "dich", "liebt", ",", "den", "liebst", "du", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ART", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer dir folgt, den leitest du.", "tokens": ["Wer", "dir", "folgt", ",", "den", "lei\u00b7test", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ART", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Himmelstochter! deinem Schimmer", "tokens": ["Him\u00b7mel\u00b7stoch\u00b7ter", "!", "dei\u00b7nem", "Schim\u00b7mer"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weicht des reinsten Goldes Flimmer:", "tokens": ["Weicht", "des", "reins\u00b7ten", "Gol\u00b7des", "Flim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Still und friedlich ist dein Pfad.", "tokens": ["Still", "und", "fried\u00b7lich", "ist", "dein", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rechts an deinem Throne winken", "tokens": ["Rechts", "an", "dei\u00b7nem", "Thro\u00b7ne", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Recht und Wahrheit, und zur Linken", "tokens": ["Recht", "und", "Wahr\u00b7heit", ",", "und", "zur", "Lin\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sitzen Vorsicht, Lieb' und Rath.", "tokens": ["Sit\u00b7zen", "Vor\u00b7sicht", ",", "Lieb'", "und", "Rath", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer von Wissgier flammt, der eile", "tokens": ["Wer", "von", "Wiss\u00b7gier", "flammt", ",", "der", "ei\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "PRELS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Muthig hin zur rechten S\u00e4ule", "tokens": ["Mut\u00b7hig", "hin", "zur", "rech\u00b7ten", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um den Lohn, den er verdient!", "tokens": ["Um", "den", "Lohn", ",", "den", "er", "ver\u00b7dient", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm, o Weisheit, streng zu pr\u00fcfen,", "tokens": ["Komm", ",", "o", "Weis\u00b7heit", ",", "streng", "zu", "pr\u00fc\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob auch unsers Herzens Tiefen", "tokens": ["Ob", "auch", "un\u00b7sers", "Her\u00b7zens", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rein, wie unsre Kellen, sind!", "tokens": ["Rein", ",", "wie", "uns\u00b7re", "Kel\u00b7len", ",", "sind", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "$,", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}