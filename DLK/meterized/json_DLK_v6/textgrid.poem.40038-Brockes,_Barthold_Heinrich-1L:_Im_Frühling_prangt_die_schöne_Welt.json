{"textgrid.poem.40038": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Fr\u00fchling prangt die sch\u00f6ne Welt", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fr\u00fchling prangt die sch\u00f6ne Welt", "tokens": ["Im", "Fr\u00fch\u00b7ling", "prangt", "die", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem fast Smaragden Schein.", "tokens": ["In", "ei\u00b7nem", "fast", "Sma\u00b7rag\u00b7den", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Sommer gl\u00e4ntzt das reife Feld,", "tokens": ["Im", "Som\u00b7mer", "gl\u00e4ntzt", "das", "rei\u00b7fe", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und scheint dem Glode gleich zu seyn.", "tokens": ["Und", "scheint", "dem", "Glo\u00b7de", "gleich", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Herbste sieht man, als Opalen,", "tokens": ["Im", "Herbs\u00b7te", "sieht", "man", ",", "als", "O\u00b7pa\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der B\u00e4ume bunte Bl\u00e4tter strahlen.", "tokens": ["Der", "B\u00e4u\u00b7me", "bun\u00b7te", "Bl\u00e4t\u00b7ter", "strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Winter schm\u00fcckt ein Schein, wie Diamant", "tokens": ["Im", "Win\u00b7ter", "schm\u00fcckt", "ein", "Schein", ",", "wie", "Di\u00b7a\u00b7mant"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und reines Silber, Fluth und Land.", "tokens": ["Und", "rei\u00b7nes", "Sil\u00b7ber", ",", "Fluth", "und", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja kurtz, wenn wir die Welt aufmercksam sehn,", "tokens": ["Ja", "kurtz", ",", "wenn", "wir", "die", "Welt", "auf\u00b7merck\u00b7sam", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist sie zu allen Zeit sch\u00f6n.", "tokens": ["Ist", "sie", "zu", "al\u00b7len", "Zeit", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}