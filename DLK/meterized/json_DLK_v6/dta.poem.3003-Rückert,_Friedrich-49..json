{"dta.poem.3003": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist nicht gemacht, zum Himmel aufzufliegen;", "tokens": ["Der", "Mensch", "ist", "nicht", "ge\u00b7macht", ",", "zum", "Him\u00b7mel", "auf\u00b7zu\u00b7flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Fl\u00fcgel fehlen ihm, sich vogelgleich zu wiegen,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "feh\u00b7len", "ihm", ",", "sich", "vo\u00b7gel\u00b7gleich", "zu", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und h\u00e4tt' er Fl\u00fcgel auch, und fehlt' ihm nichts am Schwunge,", "tokens": ["Und", "h\u00e4tt'", "er", "Fl\u00fc\u00b7gel", "auch", ",", "und", "fehlt'", "ihm", "nichts", "am", "Schwun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "ADV", "$,", "KON", "VVFIN", "PPER", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Vogel w\u00fcrd' er doch mit seiner Menschenlunge.", "tokens": ["Kein", "Vo\u00b7gel", "w\u00fcrd'", "er", "doch", "mit", "sei\u00b7ner", "Men\u00b7schen\u00b7lun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf hohen Bergen schon geht ihm der Athem aus,", "tokens": ["Auf", "ho\u00b7hen", "Ber\u00b7gen", "schon", "geht", "ihm", "der", "A\u00b7them", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Behaglich ist er nur auf mittlern H\u00f6hn zuhaus.", "tokens": ["Be\u00b7hag\u00b7lich", "ist", "er", "nur", "auf", "mitt\u00b7lern", "H\u00f6hn", "zu\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und f\u00fcllt er seinen Ball mit L\u00fcften oder Feuern,", "tokens": ["Und", "f\u00fcllt", "er", "sei\u00b7nen", "Ball", "mit", "L\u00fcf\u00b7ten", "o\u00b7der", "Feu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lernt durchs Meer der Luft alswie durchs andre steuern;", "tokens": ["Und", "lernt", "durchs", "Meer", "der", "Luft", "als\u00b7wie", "durchs", "and\u00b7re", "steu\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "KOKOM", "APPRART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was hilfts ihm, wenn er auch nicht f\u00fcllen zum Verbrauch", "tokens": ["Was", "hilfts", "ihm", ",", "wenn", "er", "auch", "nicht", "f\u00fcl\u00b7len", "zum", "Ver\u00b7brauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Luftfart kann mit Luft zum Athmen einen Schlauch?", "tokens": ["Der", "Luft\u00b7fart", "kann", "mit", "Luft", "zum", "Ath\u00b7men", "ei\u00b7nen", "Schlauch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Alswie ein Schiffer, eh er auf die bittern Bronnen", "tokens": ["Als\u00b7wie", "ein", "Schif\u00b7fer", ",", "eh", "er", "auf", "die", "bit\u00b7tern", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hinaus sich wagt, zuvor mit s\u00fc\u00dfen f\u00fcllt die Tonnen,", "tokens": ["Hin\u00b7aus", "sich", "wagt", ",", "zu\u00b7vor", "mit", "s\u00fc\u00b7\u00dfen", "f\u00fcllt", "die", "Ton\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "$,", "ADV", "APPR", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Bis er sein Schifflein legt an einem Eiland an,", "tokens": ["Bis", "er", "sein", "Schif\u00b7flein", "legt", "an", "ei\u00b7nem", "Ei\u00b7land", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie jener an dem Rand des Mondes seinen Kahn!", "tokens": ["Wie", "je\u00b7ner", "an", "dem", "Rand", "des", "Mon\u00b7des", "sei\u00b7nen", "Kahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Drum lieber lasset uns von fern des Mondes Nachen", "tokens": ["Drum", "lie\u00b7ber", "las\u00b7set", "uns", "von", "fern", "des", "Mon\u00b7des", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "APPR", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschauen in der Nacht, wann wir gerade wachen,", "tokens": ["Be\u00b7schau\u00b7en", "in", "der", "Nacht", ",", "wann", "wir", "ge\u00b7ra\u00b7de", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und wann wir schlafen, uns, gefittiget vom Traum,", "tokens": ["Und", "wann", "wir", "schla\u00b7fen", ",", "uns", ",", "ge\u00b7fit\u00b7ti\u00b7get", "vom", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "PPER", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwingen empor zu ihm und jedem h\u00f6hern Raum.", "tokens": ["Schwin\u00b7gen", "em\u00b7por", "zu", "ihm", "und", "je\u00b7dem", "h\u00f6\u00b7hern", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "PPER", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}