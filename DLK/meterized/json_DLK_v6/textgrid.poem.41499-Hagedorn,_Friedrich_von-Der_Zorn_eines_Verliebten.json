{"textgrid.poem.41499": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Zorn eines Verliebten", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brief und Wink verhie\u00dfen mir", "tokens": ["Brief", "und", "Wink", "ver\u00b7hie\u00b7\u00dfen", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon um Zwei die liebste Sch\u00f6ne;", "tokens": ["Schon", "um", "Zwei", "die", "liebs\u00b7te", "Sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Zeiger ging auf Vier,", "tokens": ["Doch", "der", "Zei\u00b7ger", "ging", "auf", "Vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mir fehlte noch Climene.", "tokens": ["Und", "mir", "fehl\u00b7te", "noch", "Cli\u00b7me\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NE", "$."], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "So Geduld als Zeit verstrich", "tokens": ["So", "Ge\u00b7duld", "als", "Zeit", "ver\u00b7strich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KOUS", "NN", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und ich schwur, den Trug zu r\u00e4chen;", "tokens": ["Und", "ich", "schwur", ",", "den", "Trug", "zu", "r\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber endlich wies sie sich,", "tokens": ["A\u00b7ber", "end\u00b7lich", "wies", "sie", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich hielt sie ihr Versprechen.", "tokens": ["End\u00b7lich", "hielt", "sie", "ihr", "Ver\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie so sch\u00f6n, sagt' ich aus Hohn,", "tokens": ["Wie", "so", "sch\u00f6n", ",", "sagt'", "ich", "aus", "Hohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du alles wahrgenommen!", "tokens": ["Hast", "du", "al\u00b7les", "wahr\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zwo Stunden wart' ich schon:", "tokens": ["Nur", "zwo", "Stun\u00b7den", "wart'", "ich", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Konntest du nicht sp\u00e4ter kommen?", "tokens": ["Konn\u00b7test", "du", "nicht", "sp\u00e4\u00b7ter", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eines Frauenzimmers Uhr", "tokens": ["Ei\u00b7nes", "Frau\u00b7en\u00b7zim\u00b7mers", "Uhr"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Braucht nicht Ziffer, braucht nicht R\u00e4der:", "tokens": ["Braucht", "nicht", "Zif\u00b7fer", ",", "braucht", "nicht", "R\u00e4\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "$,", "VVFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schm\u00fcckt sie Kett' und Siegel nur,", "tokens": ["Schm\u00fcckt", "sie", "Kett'", "und", "Sie\u00b7gel", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was bedarf sie dann der Feder?", "tokens": ["Was", "be\u00b7darf", "sie", "dann", "der", "Fe\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da mein Eifer Raum gewann,", "tokens": ["Da", "mein", "Ei\u00b7fer", "Raum", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollt' ich sie noch sch\u00e4rfer lehren;", "tokens": ["Wollt'", "ich", "sie", "noch", "sch\u00e4r\u00b7fer", "leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, was l\u00e4rmst du? hub sie an:", "tokens": ["Doch", ",", "was", "l\u00e4rmst", "du", "?", "hub", "sie", "an", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird man mich denn auch nicht h\u00f6ren?", "tokens": ["Wird", "man", "mich", "denn", "auch", "nicht", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! was hab' ich jetzt vor Schmerz", "tokens": ["Ach", "!", "was", "hab'", "ich", "jetzt", "vor", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Rosenknosp' erlitten,", "tokens": ["Von", "der", "Ro\u00b7sen\u00b7knosp'", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mir, recht bis an das Herz,", "tokens": ["Die", "mir", ",", "recht", "bis", "an", "das", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Brust hinabgeglitten!", "tokens": ["Von", "der", "Brust", "hin\u00b7ab\u00b7ge\u00b7glit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O wie dr\u00fcckt mich's! Himmel, wie!", "tokens": ["O", "wie", "dr\u00fcckt", "mich's", "!", "Him\u00b7mel", ",", "wie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PIS", "$.", "NN", "$,", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier, hier, in der linken Seite.", "tokens": ["Hier", ",", "hier", ",", "in", "der", "lin\u00b7ken", "Sei\u00b7te", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sieh nur selbst: mir glaubst du nie;", "tokens": ["Sieh", "nur", "selbst", ":", "mir", "glaubst", "du", "nie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch was glaubt ihr klugen Leute!", "tokens": ["Doch", "was", "glaubt", "ihr", "klu\u00b7gen", "Leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie entbl\u00f6\u00dfte Hals und Brust,", "tokens": ["Sie", "ent\u00b7bl\u00f6\u00df\u00b7te", "Hals", "und", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir der Knospe Druck zu zeigen:", "tokens": ["Mir", "der", "Knos\u00b7pe", "Druck", "zu", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00f6tzlich hie\u00df der Sitz der Lust", "tokens": ["Pl\u00f6tz\u00b7lich", "hie\u00df", "der", "Sitz", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich und die Verweise schweigen.", "tokens": ["Mich", "und", "die", "Ver\u00b7wei\u00b7se", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}