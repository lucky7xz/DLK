{"textgrid.poem.46154": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einig s\u00fc\u00dfes m\u00fcndelein,", "tokens": ["Ei\u00b7nig", "s\u00fc\u00b7\u00dfes", "m\u00fcn\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "r\u00f6ter dan ein r\u00f6selein,", "tokens": ["r\u00f6\u00b7ter", "dan", "ein", "r\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "das die sonn durch ihr ansehen", "tokens": ["das", "die", "sonn", "durch", "ihr", "an\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADV", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "macht aufgehen;", "tokens": ["macht", "auf\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "lefzen \u00fcbertreffend weit", "tokens": ["lef\u00b7zen", "\u00fc\u00b7bert\u00b7ref\u00b7fend", "weit"], "token_info": ["word", "word", "word"], "pos": ["VVINF", "VVPP", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "den thau, so die erden netzet,", "tokens": ["den", "thau", ",", "so", "die", "er\u00b7den", "net\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "und mit fruchtbarkeit ergetzet", "tokens": ["und", "mit", "frucht\u00b7bar\u00b7keit", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "in der s\u00fc\u00dfen fr\u00fchlingszeit.", "tokens": ["in", "der", "s\u00fc\u00b7\u00dfen", "fr\u00fch\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein liebreiches sch\u00e4tzelein,", "tokens": ["Mein", "lieb\u00b7rei\u00b7ches", "sch\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gib mir so vil schm\u00e4tzelein,", "tokens": ["gib", "mir", "so", "vil", "schm\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "so vil du gibst meinem herzen", "tokens": ["so", "vil", "du", "gibst", "mei\u00b7nem", "her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "pein und schmerzen,", "tokens": ["pein", "und", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "so viel pfeil der fliegend got", "tokens": ["so", "viel", "pfeil", "der", "flie\u00b7gend", "got"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wider mein herz abgeschossen,", "tokens": ["wi\u00b7der", "mein", "herz", "ab\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "so vil ich leid unverdrossen", "tokens": ["so", "vil", "ich", "leid", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "jamer, tr\u00fcbsal, angst und spot.", "tokens": ["ja\u00b7mer", ",", "tr\u00fcb\u00b7sal", ",", "angst", "und", "spot", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So vil man wol k\u00f6rnlein sands", "tokens": ["So", "vil", "man", "wol", "k\u00f6rn\u00b7lein", "sands"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADV", "NE", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "am ufer des Morenlands,", "tokens": ["am", "u\u00b7fer", "des", "Mo\u00b7ren\u00b7lands", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "so vil gras in dem feld stehen", "tokens": ["so", "vil", "gras", "in", "dem", "feld", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "man kan sehen;", "tokens": ["man", "kan", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "so vil tropfen in dem meer,", "tokens": ["so", "vil", "trop\u00b7fen", "in", "dem", "meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "so vil fisch die wasser bringen,", "tokens": ["so", "vil", "fisch", "die", "was\u00b7ser", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "v\u00f6gel durch den luft sich schwingen", "tokens": ["v\u00f6\u00b7gel", "durch", "den", "luft", "sich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "und so vil der herbst weinbeer.", "tokens": ["und", "so", "vil", "der", "herbst", "wein\u00b7beer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So vil sch\u00f6ne lieblichkeit,", "tokens": ["So", "vil", "sch\u00f6\u00b7ne", "lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schmollende holdseligkeit,", "tokens": ["schmol\u00b7len\u00b7de", "hold\u00b7se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "so vil h\u00f6flichkeit und lachen", "tokens": ["so", "vil", "h\u00f6f\u00b7lich\u00b7keit", "und", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "lieblich machen", "tokens": ["lieb\u00b7lich", "ma\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "deinen theuren purpurmund;", "tokens": ["dei\u00b7nen", "theu\u00b7ren", "pur\u00b7pur\u00b7mund", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wie vil rosen deine wangen,", "tokens": ["wie", "vil", "ro\u00b7sen", "dei\u00b7ne", "wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "wie vil lilgen machen prangen", "tokens": ["wie", "vil", "lil\u00b7gen", "ma\u00b7chen", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "deinen busen steif und rund.", "tokens": ["dei\u00b7nen", "bu\u00b7sen", "steif", "und", "rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So oft k\u00fc\u00df mich, Nymfelein,", "tokens": ["So", "oft", "k\u00fc\u00df", "mich", ",", "Nym\u00b7fel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "so oft schm\u00e4tz mich schimpfelein,", "tokens": ["so", "oft", "schm\u00e4tz", "mich", "schimp\u00b7fe\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "la\u00df uns miteinander scherzen", "tokens": ["la\u00df", "uns", "mi\u00b7tein\u00b7an\u00b7der", "scher\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und uns herzen,", "tokens": ["und", "uns", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "bis ich sag: \u00bbmein frid, mein freid,", "tokens": ["bis", "ich", "sag", ":", "\u00bb", "mein", "frid", ",", "mein", "freid", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ich kan nicht mehr, la\u00df mich gehen!\u00ab", "tokens": ["ich", "kan", "nicht", "mehr", ",", "la\u00df", "mich", "ge\u00b7hen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$,", "VVIMP", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "so solt du ein weil abstehen,", "tokens": ["so", "solt", "du", "ein", "weil", "ab\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "KOUS", "VVINF", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "da\u00df ich seufzend halb verscheid.", "tokens": ["da\u00df", "ich", "seuf\u00b7zend", "halb", "ver\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Darnach k\u00fc\u00df mich widerum,", "tokens": ["Dar\u00b7nach", "k\u00fc\u00df", "mich", "wi\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df noch gr\u00f6\u00dfer werd die sum,", "tokens": ["da\u00df", "noch", "gr\u00f6\u00b7\u00dfer", "werd", "die", "sum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VAFIN", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "st\u00fcpf mich auch mit deiner zungen", "tokens": ["st\u00fcpf", "mich", "auch", "mit", "dei\u00b7ner", "zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ungezwungen,", "tokens": ["un\u00b7ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "die so s\u00fc\u00df als honig ist:", "tokens": ["die", "so", "s\u00fc\u00df", "als", "ho\u00b7nig", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "also la\u00df uns kurzweil f\u00fchren", "tokens": ["al\u00b7so", "la\u00df", "uns", "kurz\u00b7weil", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "damit wir ja nicht verlieren", "tokens": ["da\u00b7mit", "wir", "ja", "nicht", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "der jugend einige frist.", "tokens": ["der", "ju\u00b7gend", "ei\u00b7ni\u00b7ge", "frist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "La\u00df uns nach der lieb willkur", "tokens": ["La\u00df", "uns", "nach", "der", "lieb", "will\u00b7kur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wandlen auf der jugend spur,", "tokens": ["wand\u00b7len", "auf", "der", "ju\u00b7gend", "spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "bis das alter krum gebogen", "tokens": ["bis", "das", "al\u00b7ter", "krum", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kom gezogen", "tokens": ["kom", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word"], "pos": ["VVFIN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "mit k\u00e4lt, zittern, forcht und graus,", "tokens": ["mit", "k\u00e4lt", ",", "zit\u00b7tern", ",", "forcht", "und", "graus", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "welches mit sich auf dem rucken", "tokens": ["wel\u00b7ches", "mit", "sich", "auf", "dem", "ru\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "vil leids bringet, uns zu drucken,", "tokens": ["vil", "leids", "brin\u00b7get", ",", "uns", "zu", "dru\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "bis es uns macht den garaus.", "tokens": ["bis", "es", "uns", "macht", "den", "ga\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}