{"textgrid.poem.39267": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das deutsche Lied in dunkle Nacht verschlagen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das deutsche Lied in dunkle Nacht verschlagen", "tokens": ["Das", "deut\u00b7sche", "Lied", "in", "dunk\u00b7le", "Nacht", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat lang geschlummert, bis sein Strahl erwacht,", "tokens": ["Hat", "lang", "ge\u00b7schlum\u00b7mert", ",", "bis", "sein", "Strahl", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis mit des Geistes g\u00f6ttlich hoher Macht", "tokens": ["Bis", "mit", "des", "Geis\u00b7tes", "g\u00f6tt\u00b7lich", "ho\u00b7her", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein bl\u00fchend Kind es an das Licht getragen.", "tokens": ["Ein", "bl\u00fc\u00b7hend", "Kind", "es", "an", "das", "Licht", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das war die M\u00e4r' von alten Heldensagen,", "tokens": ["Das", "war", "die", "M\u00e4r'", "von", "al\u00b7ten", "Hel\u00b7den\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die hat's zur G\u00f6tterflamme angefacht,", "tokens": ["Die", "hat's", "zur", "G\u00f6t\u00b7ter\u00b7flam\u00b7me", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis es erbl\u00fcht' in unerreichte Pracht", "tokens": ["Bis", "es", "er\u00b7bl\u00fcht'", "in", "un\u00b7er\u00b7reich\u00b7te", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und flog empor auf goldnem Sonnenwagen.", "tokens": ["Und", "flog", "em\u00b7por", "auf", "gold\u00b7nem", "Son\u00b7nen\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbda zog einher mein m\u00e4chtig Kampfest\u00f6nen", "tokens": ["\u00bb", "da", "zog", "ein\u00b7her", "mein", "m\u00e4ch\u00b7tig", "Kamp\u00b7fes\u00b7t\u00f6\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "ADJD", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchs deutsche Land von heil'ger Kraft erf\u00fcllt,", "tokens": ["Durchs", "deut\u00b7sche", "Land", "von", "heil'\u00b7ger", "Kraft", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Obgleich kein Mei\u00dfel gl\u00e4ttend mich gefeilt,", "tokens": ["Ob\u00b7gleich", "kein", "Mei\u00b7\u00dfel", "gl\u00e4t\u00b7tend", "mich", "ge\u00b7feilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und immer t\u00f6n' ich noch der Dichtkunst S\u00f6hnen,", "tokens": ["Und", "im\u00b7mer", "t\u00f6n'", "ich", "noch", "der", "Dicht\u00b7kunst", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das erste Kind, das deutscher Sang belebt,", "tokens": ["Das", "ers\u00b7te", "Kind", ",", "das", "deut\u00b7scher", "Sang", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das zu der Sonne stolz vorangeeilt.\u00ab", "tokens": ["Das", "zu", "der", "Son\u00b7ne", "stolz", "vor\u00b7an\u00b7ge\u00b7eilt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}