{"textgrid.poem.58482": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "520. Das Reich Gottes ist in euch!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00df nur dein' \u00c4uglein zu, entsink dir selbst zu Grunde", "tokens": ["Schlie\u00df", "nur", "dein'", "\u00c4ug\u00b7lein", "zu", ",", "ent\u00b7sink", "dir", "selbst", "zu", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gelassen, still und blo\u00df ins gegenw\u00e4rt'ge Nu,", "tokens": ["Ge\u00b7las\u00b7sen", ",", "still", "und", "blo\u00df", "ins", "ge\u00b7gen\u00b7w\u00e4rt'\u00b7ge", "Nu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADV", "APPRART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So find'st du wie ein Kind da, wo du bist, zur Stunde", "tokens": ["So", "fin\u00b7d'st", "du", "wie", "ein", "Kind", "da", ",", "wo", "du", "bist", ",", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,", "APPRART", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das inn're K\u00f6nigreich, Gott selbst und seine Ruh!", "tokens": ["Das", "inn'\u00b7re", "K\u00f6\u00b7nig\u00b7reich", ",", "Gott", "selbst", "und", "sei\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "ADV", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}