{"dta.poem.9491": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Auf H. M. Michael Hellers etc. und  \n Jungf. Sophien Volckmarin/  \n Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war nun Hesperus dem Tage fast gewichen/", "tokens": ["Es", "war", "nun", "Hes\u00b7pe\u00b7rus", "dem", "Ta\u00b7ge", "fast", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sonnen Kammer-Magd kam allgemach ge-", "tokens": ["Der", "Son\u00b7nen", "Kam\u00b7mer\u00b7Magd", "kam", "all\u00b7ge\u00b7mach", "ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schlichen/", "tokens": ["schli\u00b7chen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "und bahnete den Weg der Tagesmeisterin/", "tokens": ["und", "bah\u00b7ne\u00b7te", "den", "Weg", "der", "Ta\u00b7ges\u00b7meis\u00b7te\u00b7rin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der F\u00fcrstin des Gestirns/ und nahm den Nebel hin:", "tokens": ["Der", "F\u00fcrs\u00b7tin", "des", "Ge\u00b7stirns", "/", "und", "nahm", "den", "Ne\u00b7bel", "hin", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Spricht Pallas/ lieber Sohn/ steh\u2019 auf un\u0303 la\u00df erschalle\u0303", "tokens": ["Spricht", "Pal\u00b7las", "/", "lie\u00b7ber", "Sohn", "/", "steh'", "auf", "u\u00f1", "la\u00df", "er\u00b7schall\u1ebd"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$(", "ADV", "NN", "$(", "VVFIN", "APPR", "KON", "XY", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein lieblich Hochzeit-Lied/ dann jtzund soll vor allen", "tokens": ["Ein", "lieb\u00b7lich", "Hoch\u00b7zeit\u00b7Lied", "/", "dann", "jt\u00b7zund", "soll", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$(", "ADV", "ADV", "VMFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Herr Heller laben sich; Drauf stim\u0303t ich alsobald", "tokens": ["Herr", "Hel\u00b7ler", "la\u00b7ben", "sich", ";", "Drauf", "stim\u0303t", "ich", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "PRF", "$.", "PAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des Pindars Seiten an/ da\u00df thal und Feld erschallt.", "tokens": ["Des", "Pin\u00b7dars", "Sei\u00b7ten", "an", "/", "da\u00df", "thal", "und", "Feld", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$(", "KOUS", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}