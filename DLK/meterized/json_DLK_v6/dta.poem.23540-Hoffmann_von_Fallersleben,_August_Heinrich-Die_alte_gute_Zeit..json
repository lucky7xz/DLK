{"dta.poem.23540": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die alte gute Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O lernet doch ihr armen Knecht' und Wichte,", "tokens": ["O", "ler\u00b7net", "doch", "ihr", "ar\u00b7men", "Knecht'", "und", "Wich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O lernt doch unsers deutschen Volks Geschichte,", "tokens": ["O", "lernt", "doch", "un\u00b7sers", "deut\u00b7schen", "Volks", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und preist nicht gro\u00df und herrlich jene Zeit,", "tokens": ["Und", "preist", "nicht", "gro\u00df", "und", "herr\u00b7lich", "je\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "KON", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Zeit der niedrigsten Erb\u00e4rmlichkeit!", "tokens": ["Die", "Zeit", "der", "nied\u00b7rigs\u00b7ten", "Er\u00b7b\u00e4rm\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch nein, ihr bleibt bei eurem dummen Schw\u00e4tzen,", "tokens": ["Doch", "nein", ",", "ihr", "bleibt", "bei", "eu\u00b7rem", "dum\u00b7men", "Schw\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr wollt der guten Zeit ein Denkmal setzen \u2014", "tokens": ["Ihr", "wollt", "der", "gu\u00b7ten", "Zeit", "ein", "Denk\u00b7mal", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohlan, so gie\u00dft die Zopfzeit dann in Erz,", "tokens": ["Wo\u00b7hlan", ",", "so", "gie\u00dft", "die", "Zopf\u00b7zeit", "dann", "in", "Erz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gie\u00dft hinein des deutschen Volkes Schmerz!", "tokens": ["Und", "gie\u00dft", "hin\u00b7ein", "des", "deut\u00b7schen", "Vol\u00b7kes", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}