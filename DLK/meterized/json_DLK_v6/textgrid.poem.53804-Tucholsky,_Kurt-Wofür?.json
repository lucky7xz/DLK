{"textgrid.poem.53804": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Wof\u00fcr?", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich Kindern la\u00dft ihr euch betr\u00fcgen,", "tokens": ["Gleich", "Kin\u00b7dern", "la\u00dft", "ihr", "euch", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis ihr zu sp\u00e4t erkennt, o weh! \u2013", "tokens": ["Bis", "ihr", "zu", "sp\u00e4t", "er\u00b7kennt", ",", "o", "weh", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "PTKA", "ADJD", "VVFIN", "$,", "FM", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wacht am Rhein wird nicht gen\u00fcgen,", "tokens": ["Die", "Wacht", "am", "Rhein", "wird", "nicht", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der schlimmste Feind steht an der Spree.", "tokens": ["Der", "schlimms\u00b7te", "Feind", "steht", "an", "der", "Spree", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}