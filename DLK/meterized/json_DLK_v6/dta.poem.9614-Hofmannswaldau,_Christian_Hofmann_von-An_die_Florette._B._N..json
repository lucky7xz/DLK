{"dta.poem.9614": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die Florette.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Florette soll ich denn in flammen untergehn?", "tokens": ["Flo\u00b7ret\u00b7te", "soll", "ich", "denn", "in", "flam\u00b7men", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nichts als kalter schnee in deiner brust zu finden?", "tokens": ["Ist", "nichts", "als", "kal\u00b7ter", "schnee", "in", "dei\u00b7ner", "brust", "zu", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan sich dein purpur-munb mit hitze nicht verbinden?", "tokens": ["Kan", "sich", "dein", "pur\u00b7pur\u00b7munb", "mit", "hit\u00b7ze", "nicht", "ver\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sieht man auch den neyd auff reinen wangen stehn?", "tokens": ["Und", "sieht", "man", "auch", "den", "neyd", "auff", "rei\u00b7nen", "wan\u00b7gen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! schaue dich doch selbst mit andern augen an!", "tokens": ["Ach", "!", "schau\u00b7e", "dich", "doch", "selbst", "mit", "an\u00b7dern", "au\u00b7gen", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sch\u00f6nheit f\u00fcrni\u00df kan nicht ewig farbe fassen/", "tokens": ["Der", "sch\u00f6n\u00b7heit", "f\u00fcr\u00b7ni\u00df", "kan", "nicht", "e\u00b7wig", "far\u00b7be", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PTKNEG", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein schimrend wa\u00dfer wird/ wie tr\u00fcbe glut/ erblassen;", "tokens": ["Dein", "schim\u00b7rend", "wa\u00b7\u00dfer", "wird", "/", "wie", "tr\u00fc\u00b7be", "glut", "/", "er\u00b7blas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJD", "VAFIN", "$(", "KOKOM", "ADJA", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn iede stunde zeigt/ wie sie dir trotzen kan.", "tokens": ["Denn", "ie\u00b7de", "stun\u00b7de", "zeigt", "/", "wie", "sie", "dir", "trot\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VVFIN", "$(", "PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was heute carmasin und scharlach \u00fcberdeckt/", "tokens": ["Was", "heu\u00b7te", "car\u00b7ma\u00b7sin", "und", "schar\u00b7lach", "\u00fc\u00b7berd\u00b7eckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo muschel und corall um alabaster gl\u00e4ntzen/", "tokens": ["Wo", "mu\u00b7schel", "und", "co\u00b7rall", "um", "a\u00b7la\u00b7bas\u00b7ter", "gl\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "KON", "NN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was himmel und natur mit rosen-schein umkr\u00e4ntzen/", "tokens": ["Was", "him\u00b7mel", "und", "na\u00b7tur", "mit", "ro\u00b7sen\u00b7schein", "um\u00b7kr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wird morgen durch das gifft der tr\u00fcben zeit befleckt.", "tokens": ["Wird", "mor\u00b7gen", "durch", "das", "gifft", "der", "tr\u00fc\u00b7ben", "zeit", "be\u00b7fleckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der fr\u00fchling wird dir nicht stets um die lippen stehn/", "tokens": ["Der", "fr\u00fch\u00b7ling", "wird", "dir", "nicht", "stets", "um", "die", "lip\u00b7pen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der beste nelcken-strauch wird endlich hingeri\u00dfen/", "tokens": ["Der", "bes\u00b7te", "nel\u00b7cken\u00b7strauch", "wird", "end\u00b7lich", "hin\u00b7ge\u00b7ri\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und deine blumen sind nur fl\u00fcchtige narei\u00dfen/", "tokens": ["Und", "dei\u00b7ne", "blu\u00b7men", "sind", "nur", "fl\u00fcch\u00b7ti\u00b7ge", "nar\u00b7ei\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die heute pr\u00e4chtig bl\u00fchn/ und morgen untergehn.", "tokens": ["Die", "heu\u00b7te", "pr\u00e4ch\u00b7tig", "bl\u00fchn", "/", "und", "mor\u00b7gen", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVINF", "$(", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dem sommer folgt der herbst/ dem herbste winters-zeit/", "tokens": ["Dem", "som\u00b7mer", "folgt", "der", "herbst", "/", "dem", "herbs\u00b7te", "win\u00b7ter\u00b7szeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ART", "ADV", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Florette dencke selbst/ wie deine fr\u00fcchte reiffen;", "tokens": ["Flo\u00b7ret\u00b7te", "den\u00b7cke", "selbst", "/", "wie", "dei\u00b7ne", "fr\u00fcch\u00b7te", "reif\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$(", "KOKOM", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach! la\u00df sie/ werthe/ doch den winter nicht ergreiffen/", "tokens": ["Ach", "!", "la\u00df", "sie", "/", "wert\u00b7he", "/", "doch", "den", "win\u00b7ter", "nicht", "er\u00b7greif\u00b7fen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "$(", "VVFIN", "$(", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein frischer apfel giebt die beste liebligkeit.", "tokens": ["Ein", "fri\u00b7scher", "ap\u00b7fel", "giebt", "die", "bes\u00b7te", "lieb\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Schleu\u00df deinen garten auff/ weil dich der himmel liebt/", "tokens": ["Schleu\u00df", "dei\u00b7nen", "gar\u00b7ten", "auff", "/", "weil", "dich", "der", "him\u00b7mel", "liebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "APPR", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein rebenstock ist ja f\u00fcr menschen aufgescho\u00dfen/", "tokens": ["Ein", "re\u00b7ben\u00b7stock", "ist", "ja", "f\u00fcr", "men\u00b7schen", "auf\u00b7ge\u00b7scho\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Was hilfft ein s\u00fcsser trunck/ der keinen mund beflo\u00dfen?", "tokens": ["Was", "hilfft", "ein", "s\u00fcs\u00b7ser", "trunck", "/", "der", "kei\u00b7nen", "mund", "be\u00b7flo\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was n\u00fctzt granaten-frucht/ die keine kerne giebt?", "tokens": ["Was", "n\u00fctzt", "gra\u00b7na\u00b7ten\u00b7frucht", "/", "die", "kei\u00b7ne", "ker\u00b7ne", "giebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein weib das fleisch und blut in ihrem busen tr\u00e4gt/", "tokens": ["Ein", "weib", "das", "fleisch", "und", "blut", "in", "ih\u00b7rem", "bu\u00b7sen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und doch die jungfrauschafft will lebendig begraben/", "tokens": ["Und", "doch", "die", "jung\u00b7frausc\u00b7hafft", "will", "le\u00b7ben\u00b7dig", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.27": {"text": "Ist werth/ wenn schlang und wurm den leib gefressen haben/", "tokens": ["Ist", "werth", "/", "wenn", "schlang", "und", "wurm", "den", "leib", "ge\u00b7fres\u00b7sen", "ha\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "KOUS", "ADJD", "KON", "NN", "ART", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df man ihr diese schrifft auf ihren leich-stein pr\u00e4gt:", "tokens": ["Da\u00df", "man", "ihr", "die\u00b7se", "schrifft", "auf", "ih\u00b7ren", "leich\u00b7stein", "pr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hier lieget pech und stroh/ das keine flammen fing/", "tokens": ["Hier", "lie\u00b7get", "pech", "und", "stroh", "/", "das", "kei\u00b7ne", "flam\u00b7men", "fing", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$(", "ART", "PIAT", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein schweflicht wesen/ das beym feuer nicht entbrante/", "tokens": ["Ein", "schwef\u00b7licht", "we\u00b7sen", "/", "das", "beym", "feu\u00b7er", "nicht", "ent\u00b7bran\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$(", "ART", "APPRART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein zunder-voller leib/ der keine glut erkante/", "tokens": ["Ein", "zun\u00b7der\u00b7vol\u00b7ler", "leib", "/", "der", "kei\u00b7ne", "glut", "er\u00b7kan\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein glantz/ der in der lufft wie tr\u00fcber rauch verging:", "tokens": ["Ein", "glantz", "/", "der", "in", "der", "lufft", "wie", "tr\u00fc\u00b7ber", "rauch", "ver\u00b7ging", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "ART", "NN", "KOKOM", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein brunn der seine quell in feste mauren schlo\u00df/", "tokens": ["Ein", "brunn", "der", "sei\u00b7ne", "quell", "in", "fes\u00b7te", "mau\u00b7ren", "schlo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein balsam/ den wir nur von weitem angerochen.", "tokens": ["Ein", "bal\u00b7sam", "/", "den", "wir", "nur", "von", "wei\u00b7tem", "an\u00b7ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ART", "PPER", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein s\u00fcsser citronat/ den keine faust gebrochen.", "tokens": ["Ein", "s\u00fcs\u00b7ser", "cit\u00b7ro\u00b7nat", "/", "den", "kei\u00b7ne", "faust", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein artzt/ der seine krafft auff kalte t\u00fccher go\u00df.", "tokens": ["Ein", "artzt", "/", "der", "sei\u00b7ne", "krafft", "auff", "kal\u00b7te", "t\u00fc\u00b7cher", "go\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein bunter blumen-platz/ den ieder nur beschaut.", "tokens": ["Ein", "bun\u00b7ter", "blu\u00b7men\u00b7platz", "/", "den", "ie\u00b7der", "nur", "be\u00b7schaut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIAT", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein feld/ das weder pflug noch schare durchgeschnitten.", "tokens": ["Ein", "feld", "/", "das", "we\u00b7der", "pflug", "noch", "scha\u00b7re", "durch\u00b7ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "KON", "VVFIN", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein wolgeziertes pferd/ das keinen sporn erlitten/", "tokens": ["Ein", "wol\u00b7ge\u00b7zier\u00b7tes", "pferd", "/", "das", "kei\u00b7nen", "sporn", "er\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und wie Bucephalus dem schatten kaum getraut.", "tokens": ["Und", "wie", "Bu\u00b7ce\u00b7pha\u00b7lus", "dem", "schat\u00b7ten", "kaum", "ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "ART", "VVFIN", "ADV", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Ein himmel/ der nur schmertz und keinen trost gebahr.", "tokens": ["Ein", "him\u00b7mel", "/", "der", "nur", "schmertz", "und", "kei\u00b7nen", "trost", "ge\u00b7bahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADJD", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein amber-voller kram/ der keinem feil gewesen/", "tokens": ["Ein", "am\u00b7ber\u00b7vol\u00b7ler", "kram", "/", "der", "kei\u00b7nem", "feil", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und will man alles hier aus einer zeile lesen:", "tokens": ["Und", "will", "man", "al\u00b7les", "hier", "aus", "ei\u00b7ner", "zei\u00b7le", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PIS", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Hier liegt ein todes mensch/ das keine menschin war.", "tokens": ["Hier", "liegt", "ein", "to\u00b7des", "mensch", "/", "das", "kei\u00b7ne", "men\u00b7schin", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Jm leben wolte sie die Juno selber seyn/", "tokens": ["Jm", "le\u00b7ben", "wol\u00b7te", "sie", "die", "Ju\u00b7no", "sel\u00b7ber", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VMFIN", "PPER", "ART", "NN", "ADV", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Jhr mund verm\u00e4hlte sich mit schimmrenden rubinen/", "tokens": ["Ihr", "mund", "ver\u00b7m\u00e4hl\u00b7te", "sich", "mit", "schimm\u00b7ren\u00b7den", "ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Jtzt mu\u00df ein kahler sarg zum trauer-zimmer dienen/", "tokens": ["Jtzt", "mu\u00df", "ein", "kah\u00b7ler", "sarg", "zum", "trau\u00b7e\u00b7rzim\u00b7mer", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und schliest den gantzen rest in diese breter ein.", "tokens": ["Und", "schliest", "den", "gant\u00b7zen", "rest", "in", "die\u00b7se", "bre\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "VVFIN", "APPR", "PDAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Florette schreckt dich nun die schwartze grabes-schrifft/", "tokens": ["Flo\u00b7ret\u00b7te", "schreckt", "dich", "nun", "die", "schwart\u00b7ze", "gra\u00b7bes\u00b7schrifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "So lerne/ sch\u00f6nste/ dich doch selber recht erkennen/", "tokens": ["So", "ler\u00b7ne", "/", "sch\u00f6ns\u00b7te", "/", "dich", "doch", "sel\u00b7ber", "recht", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "$(", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "La\u00df auch die liebes-glut in deiner brust entbrennen/", "tokens": ["La\u00df", "auch", "die", "lie\u00b7bes\u00b7glut", "in", "dei\u00b7ner", "brust", "ent\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und meide mit bedacht das tr\u00fcbe todtes-gifft.", "tokens": ["Und", "mei\u00b7de", "mit", "be\u00b7dacht", "das", "tr\u00fc\u00b7be", "tod\u00b7tes\u00b7gifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Das bette steht dir mehr als schwartze bahren an;", "tokens": ["Das", "bet\u00b7te", "steht", "dir", "mehr", "als", "schwart\u00b7ze", "bah\u00b7ren", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "PIAT", "KOKOM", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Der kittel wird dich nicht so wie die federn zieren/", "tokens": ["Der", "kit\u00b7tel", "wird", "dich", "nicht", "so", "wie", "die", "fe\u00b7dern", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Das alter soll den tod/ die jugend l\u00fcste sp\u00fcren/", "tokens": ["Das", "al\u00b7ter", "soll", "den", "tod", "/", "die", "ju\u00b7gend", "l\u00fcs\u00b7te", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ART", "NN", "$(", "ART", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Weil sich der sommer nicht mit k\u00e4lte schwistern kan.", "tokens": ["Weil", "sich", "der", "som\u00b7mer", "nicht", "mit", "k\u00e4l\u00b7te", "schwis\u00b7tern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADV", "PTKNEG", "APPR", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ein schatten volles grab hegt schlechte lebens-lufft/", "tokens": ["Ein", "schat\u00b7ten", "vol\u00b7les", "grab", "hegt", "schlech\u00b7te", "le\u00b7bens\u00b7lufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Man kan der lippen ruhm nicht vom gerippe lesen:", "tokens": ["Man", "kan", "der", "lip\u00b7pen", "ruhm", "nicht", "vom", "ge\u00b7rip\u00b7pe", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und was dein purpur-mund und deine pracht gewesen/", "tokens": ["Und", "was", "dein", "pur\u00b7pur\u00b7mund", "und", "dei\u00b7ne", 