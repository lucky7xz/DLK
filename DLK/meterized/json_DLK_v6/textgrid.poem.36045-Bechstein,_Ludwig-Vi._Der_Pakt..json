{"textgrid.poem.36045": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Vi. Der Pakt.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In stiller Zelle weilet Faustus mit d\u00fcsterm Blick;", "tokens": ["In", "stil\u00b7ler", "Zel\u00b7le", "wei\u00b7let", "Faus\u00b7tus", "mit", "d\u00fcs\u00b7term", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wird nun erf\u00fcllt sein Hoffen? Erfasst er nun sein Gl\u00fcck?", "tokens": ["Wird", "nun", "er\u00b7f\u00fcllt", "sein", "Hof\u00b7fen", "?", "Er\u00b7fasst", "er", "nun", "sein", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Tr\u00fcgt ihn auch nicht der D\u00e4mon, den er aus Nacht beschwor?", "tokens": ["Tr\u00fcgt", "ihn", "auch", "nicht", "der", "D\u00e4\u00b7mon", ",", "den", "er", "aus", "Nacht", "be\u00b7schwor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Weh, wenn er an ein Trugbild die Seligkeit verlor!", "tokens": ["Weh", ",", "wenn", "er", "an", "ein", "Trug\u00b7bild", "die", "Se\u00b7lig\u00b7keit", "ver\u00b7lor", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Die Stunden schleichen \u2013 d\u00fcnkt ihm die Zeit doch fl\u00fcgellahm,", "tokens": ["Die", "Stun\u00b7den", "schlei\u00b7chen", "\u2013", "d\u00fcnkt", "ihm", "die", "Zeit", "doch", "fl\u00fc\u00b7gel\u00b7lahm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "VVFIN", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht sehnlicher die Brautnacht erharrt ein Br\u00e4utigam,", "tokens": ["Nicht", "sehn\u00b7li\u00b7cher", "die", "Braut\u00b7nacht", "er\u00b7harrt", "ein", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als Faustus bang, voll Unruh, des H\u00f6llenboten harrt,", "tokens": ["Als", "Faus\u00b7tus", "bang", ",", "voll", "Un\u00b7ruh", ",", "des", "H\u00f6l\u00b7len\u00b7bo\u00b7ten", "harrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "$,", "ADJD", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da k\u00fcndet Schattenwallen ihm Geistergegenwart.", "tokens": ["Da", "k\u00fcn\u00b7det", "Schat\u00b7ten\u00b7wal\u00b7len", "ihm", "Geis\u00b7ter\u00b7ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Bald schwimmt die Nebelbildung vor seinem Blick, wie Flor;", "tokens": ["Bald", "schwimmt", "die", "Ne\u00b7bel\u00b7bil\u00b7dung", "vor", "sei\u00b7nem", "Blick", ",", "wie", "Flor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bald steigt's, gleich Irrlichtfl\u00e4mmchen, hoch zum Gew\u00f6lb empor.", "tokens": ["Bald", "steigt's", ",", "gleich", "Irr\u00b7licht\u00b7fl\u00e4mm\u00b7chen", ",", "hoch", "zum", "Ge\u00b7w\u00f6lb", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "NN", "$,", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bald scheint ein Menschenantlitz todtbleich vor ihm zu stehn,", "tokens": ["Bald", "scheint", "ein", "Men\u00b7schen\u00b7ant\u00b7litz", "todt\u00b7bleich", "vor", "ihm", "zu", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bald wieder, Duft und Rauch gleich, in Wirbeln zu verwehn.", "tokens": ["Bald", "wie\u00b7der", ",", "Duft", "und", "Rauch", "gleich", ",", "in", "Wir\u00b7beln", "zu", "ver\u00b7wehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "KON", "NN", "ADV", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Drauf fasst der Zaubrer zornig das Buch, das Geister bannt;", "tokens": ["Drauf", "fasst", "der", "Zaub\u00b7rer", "zor\u00b7nig", "das", "Buch", ",", "das", "Geis\u00b7ter", "bannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADJD", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit Qualen neuer Formeln droht wild der Nekromant.", "tokens": ["Mit", "Qua\u00b7len", "neu\u00b7er", "For\u00b7meln", "droht", "wild", "der", "Ne\u00b7kro\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da wird zur Feuerflamme, zur Lohe schier das Haus,", "tokens": ["Da", "wird", "zur", "Feu\u00b7er\u00b7flam\u00b7me", ",", "zur", "Lo\u00b7he", "schier", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$,", "APPRART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die breitet Drachenfl\u00fcgel nach allen Seiten aus.", "tokens": ["Die", "brei\u00b7tet", "Dra\u00b7chen\u00b7fl\u00fc\u00b7gel", "nach", "al\u00b7len", "Sei\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und von den Flammenschwingen umleuchtet und umloht", "tokens": ["Und", "von", "den", "Flam\u00b7men\u00b7schwin\u00b7gen", "um\u00b7leuch\u00b7tet", "und", "um\u00b7loht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zeigt sich ein Riesenantlitz grimmvoll und blutigroth;", "tokens": ["Zeigt", "sich", "ein", "Rie\u00b7sen\u00b7ant\u00b7litz", "grimm\u00b7voll", "und", "blu\u00b7ti\u00b7groth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem Antlitz gleich des Tigers, der in die Ketten beisst,", "tokens": ["Dem", "Ant\u00b7litz", "gleich", "des", "Ti\u00b7gers", ",", "der", "in", "die", "Ket\u00b7ten", "beisst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wuthblickend, wie das Grabthier, das am Gegitter reisst.", "tokens": ["Wuth\u00b7bli\u00b7ckend", ",", "wie", "das", "Grab\u00b7thier", ",", "das", "am", "Ge\u00b7git\u00b7ter", "reisst", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch Faustus, in der Rechten das Buch erhoben, spricht:", "tokens": ["Doch", "Faus\u00b7tus", ",", "in", "der", "Rech\u00b7ten", "das", "Buch", "er\u00b7ho\u00b7ben", ",", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbich rief nach meinen Knechten, Dein Gaukeln schreckt mich nicht!", "tokens": ["\u00bb", "ich", "rief", "nach", "mei\u00b7nen", "Knech\u00b7ten", ",", "Dein", "Gau\u00b7keln", "schreckt", "mich", "nicht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sprich mir, was Du verlangest, dass mir Dein Dienst geh\u00f6rt:", "tokens": ["Sprich", "mir", ",", "was", "Du", "ver\u00b7lan\u00b7gest", ",", "dass", "mir", "Dein", "Dienst", "ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Dass Du den Pakt empfangest, den Dir mein Mund beschw\u00f6rt!\u00ab", "tokens": ["Dass", "Du", "den", "Pakt", "emp\u00b7fan\u00b7gest", ",", "den", "Dir", "mein", "Mund", "be\u00b7schw\u00f6rt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und furchtbar rollt die Stimme, wie Donner, durchs Gemach:", "tokens": ["Und", "furcht\u00b7bar", "rollt", "die", "Stim\u00b7me", ",", "wie", "Don\u00b7ner", ",", "durchs", "Ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "Soll ich den Knecht Dir senden aus meiner Diener Schaar,", "tokens": ["Soll", "ich", "den", "Knecht", "Dir", "sen\u00b7den", "aus", "mei\u00b7ner", "Die\u00b7ner", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "So h\u00f6re mein Bedingniss und nimm es treulich wahr:\u00ab", "tokens": ["So", "h\u00f6\u00b7re", "mein", "Be\u00b7ding\u00b7niss", "und", "nimm", "es", "treu\u00b7lich", "wahr", ":", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVIMP", "PPER", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbverfluche den, der neidisch uns einst gest\u00fcrzt vom Thron!", "tokens": ["\u00bb", "ver\u00b7flu\u00b7che", "den", ",", "der", "nei\u00b7disch", "uns", "einst", "ge\u00b7st\u00fcrzt", "vom", "Thron", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "$,", "PRELS", "ADJD", "PPER", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Abschw\u00f6re Dich dem Irrwahn von der Maria Sohn!", "tokens": ["Ab\u00b7schw\u00f6\u00b7re", "Dich", "dem", "Irr\u00b7wahn", "von", "der", "Ma\u00b7ria", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "NN", "APPR", "ART", "NE", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und bleibe Pfaffen abhold, gleich ihm, ohn' Unterlass!", "tokens": ["Und", "blei\u00b7be", "Pfaf\u00b7fen", "ab\u00b7hold", ",", "gleich", "ihm", ",", "ohn'", "Un\u00b7ter\u00b7lass", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADJD", "$,", "ADV", "PPER", "$,", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schw\u00f6r' allem Guten Feindschaft, und aller Liebe Hass!\u00ab", "tokens": ["Schw\u00f6r'", "al\u00b7lem", "Gu\u00b7ten", "Feind\u00b7schaft", ",", "und", "al\u00b7ler", "Lie\u00b7be", "Hass", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "$,", "KON", "PIAT", "NN", "NE", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbverfluche jede Kirche, wie jedes Sakrament!", "tokens": ["\u00bb", "ver\u00b7flu\u00b7che", "je\u00b7de", "Kir\u00b7che", ",", "wie", "je\u00b7des", "Sak\u00b7ra\u00b7ment", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du wirst nie frein, nie lesen im neuen Testament!", "tokens": ["Du", "wirst", "nie", "frein", ",", "nie", "le\u00b7sen", "im", "neu\u00b7en", "Tes\u00b7ta\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Hast Du dess Lust und Muthes? Wohl, schreib's mit eigner Hand,", "tokens": ["Hast", "Du", "dess", "Lust", "und", "Mu\u00b7thes", "?", "Wohl", ",", "schreib's", "mit", "eig\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "NN", "$.", "ADV", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit Purpur Deines Blutes f\u00fcr uns zum Unterpfand!\u00ab", "tokens": ["Mit", "Pur\u00b7pur", "Dei\u00b7nes", "Blu\u00b7tes", "f\u00fcr", "uns", "zum", "Un\u00b7ter\u00b7pfand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "APPR", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbdann magst Du frei gebieten, Faust, dann ist alles Dein;", "tokens": ["\u00bb", "dann", "magst", "Du", "frei", "ge\u00b7bie\u00b7ten", ",", "Faust", ",", "dann", "ist", "al\u00b7les", "Dein", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,", "NN", "$,", "ADV", "VAFIN", "PIS", "PPOSAT", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Sch\u00e4tze, die wir h\u00fcten, die Kraft, die wir verleihn!", "tokens": ["Die", "Sch\u00e4t\u00b7ze", ",", "die", "wir", "h\u00fc\u00b7ten", ",", "die", "Kraft", ",", "die", "wir", "ver\u00b7leihn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was Deinen Sinn erfreue, was Deiner Lust behagt,", "tokens": ["Was", "Dei\u00b7nen", "Sinn", "er\u00b7freu\u00b7e", ",", "was", "Dei\u00b7ner", "Lust", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sei Dir mit fester Treue gelobt, und zugesagt!\u00ab \u2013", "tokens": ["Sei", "Dir", "mit", "fes\u00b7ter", "Treu\u00b7e", "ge\u00b7lobt", ",", "und", "zu\u00b7ge\u00b7sagt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,", "KON", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Und Faust: \u00bbWas Du begehrest, erf\u00fcll' ich willig Dir,", "tokens": ["Und", "Faust", ":", "\u00bb", "Was", "Du", "be\u00b7geh\u00b7rest", ",", "er\u00b7f\u00fcll'", "ich", "wil\u00b7lig", "Dir", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "$(", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sofern Du treu gew\u00e4hrest, was Du geboten mir!", "tokens": ["So\u00b7fern", "Du", "treu", "ge\u00b7w\u00e4h\u00b7rest", ",", "was", "Du", "ge\u00b7bo\u00b7ten", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$,", "PWS", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wenn vier und zwanzig Iahre mir Deine Macht gehorcht,", "tokens": ["Wenn", "vier", "und", "zwan\u00b7zig", "I\u00b7ah\u00b7re", "mir", "Dei\u00b7ne", "Macht", "ge\u00b7horcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "KON", "CARD", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Will ich dem Himmel trotzen, und Dein sein unbesorgt!\u00ab", "tokens": ["Will", "ich", "dem", "Him\u00b7mel", "trot\u00b7zen", ",", "und", "Dein", "sein", "un\u00b7be\u00b7sorgt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "KON", "PPOSAT", "VAINF", "ADJD", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbverflucht der K\u00f6hlerglaube, der Thoren Bande schlingt!", "tokens": ["\u00bb", "ver\u00b7flucht", "der", "K\u00f6h\u00b7ler\u00b7glau\u00b7be", ",", "der", "Tho\u00b7ren", "Ban\u00b7de", "schlingt", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verflucht das Ammenm\u00e4hrchen, das von Erl\u00f6sung singt!", "tokens": ["Ver\u00b7flucht", "das", "Am\u00b7men\u00b7m\u00e4hr\u00b7chen", ",", "das", "von", "Er\u00b7l\u00f6\u00b7sung", "singt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hab' auch gehofft, gebetet \u2013 blieb es nicht, wie zuvor?", "tokens": ["Hab'", "auch", "ge\u00b7hofft", ",", "ge\u00b7be\u00b7tet", "\u2013", "blieb", "es", "nicht", ",", "wie", "zu\u00b7vor", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "VVPP", "$(", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Ich hatte Glauben, Liebe \u2013 war ein betrogner Thor!\u00ab", "tokens": ["Ich", "hat\u00b7te", "Glau\u00b7ben", ",", "Lie\u00b7be", "\u2013", "war", "ein", "be\u00b7trog\u00b7ner", "Thor", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "$(", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbabschw\u00f6r' ich Pfaffengemeinschaft sammt ihrem heilgen Quark!", "tokens": ["\u00bb", "ab\u00b7schw\u00f6r'", "ich", "Pfaf\u00b7fen\u00b7ge\u00b7mein\u00b7schaft", "sammt", "ih\u00b7rem", "heil\u00b7gen", "Quark", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+----+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Abschw\u00f6r' ich Lieb' und Freundschaft, ", "tokens": ["Ab\u00b7schw\u00f6r'", "ich", "Lieb'", "und", "Freund\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Verflucht sei jede Kirche, die sich mit Formen spreizt!", "tokens": ["Ver\u00b7flucht", "sei", "je\u00b7de", "Kir\u00b7che", ",", "die", "sich", "mit", "For\u00b7men", "spreizt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verflucht sei jedes Eh'band, es hat mich nie gereizt!\u00ab \u2013", "tokens": ["Ver\u00b7flucht", "sei", "je\u00b7des", "Eh'\u00b7band", ",", "es", "hat", "mich", "nie", "ge\u00b7reizt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$.", "$(", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Er ruft's mit bittrem Lachen, zur Feder greift er schnell;", "tokens": ["Er", "ruft's", "mit", "bitt\u00b7rem", "La\u00b7chen", ",", "zur", "Fe\u00b7der", "greift", "er", "schnell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und gleich aus offner Ader str\u00f6mt seines Blutes Quell.", "tokens": ["Und", "gleich", "aus", "off\u00b7ner", "A\u00b7der", "str\u00f6mt", "sei\u00b7nes", "Blu\u00b7tes", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er schreibt, was er gesprochen, von wilder Gluth durchw\u00fchlt,", "tokens": ["Er", "schreibt", ",", "was", "er", "ge\u00b7spro\u00b7chen", ",", "von", "wil\u00b7der", "Gluth", "durch\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und hat das Band zerbrochen, das ihn am Himmel hielt.", "tokens": ["Und", "hat", "das", "Band", "zer\u00b7bro\u00b7chen", ",", "das", "ihn", "am", "Him\u00b7mel", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Und als das Blatt betrachtend, er d\u00fcster sinnend stand,", "tokens": ["Und", "als", "das", "Blatt", "be\u00b7trach\u00b7tend", ",", "er", "d\u00fcs\u00b7ter", "sin\u00b7nend", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Braust durchs Gemach ein Windstoss, und reisst's ihm aus der Hand.", "tokens": ["Braust", "durchs", "Ge\u00b7mach", "ein", "Winds\u00b7toss", ",", "und", "reisst's", "ihm", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und eine Flamme lodert, und ein Gel\u00e4chter gellt,", "tokens": ["Und", "ei\u00b7ne", "Flam\u00b7me", "lo\u00b7dert", ",", "und", "ein", "Ge\u00b7l\u00e4ch\u00b7ter", "gellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als jauchze triumphirend der F\u00fcrst der Unterwelt.", "tokens": ["Als", "jauch\u00b7ze", "tri\u00b7um\u00b7phi\u00b7rend", "der", "F\u00fcrst", "der", "Un\u00b7ter\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Faustus weilt in der Zelle mit unmuthd\u00fcsterm Blick;", "tokens": ["Faus\u00b7tus", "weilt", "in", "der", "Zel\u00b7le", "mit", "un\u00b7muth\u00b7d\u00fcs\u00b7term", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wird nun erf\u00e4llt sein Hoffen? Erfasst er nun sein Gl\u00fcck?", "tokens": ["Wird", "nun", "er\u00b7f\u00e4llt", "sein", "Hof\u00b7fen", "?", "Er\u00b7fasst", "er", "nun", "sein", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Lacht nicht der D\u00e4mon h\u00f6hnend, den er aus Nacht beschwor?", "tokens": ["Lacht", "nicht", "der", "D\u00e4\u00b7mon", "h\u00f6h\u00b7nend", ",", "den", "er", "aus", "Nacht", "be\u00b7schwor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weh, wenn er an ein Wahnbild die Seligkeit verlor! \u2013", "tokens": ["Weh", ",", "wenn", "er", "an", "ein", "Wahn\u00b7bild", "die", "Se\u00b7lig\u00b7keit", "ver\u00b7lor", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}}}}