{"textgrid.poem.56068": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Anfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War in des Wagens Wendung dieser Schwung?", "tokens": ["War", "in", "des", "Wa\u00b7gens", "Wen\u00b7dung", "die\u00b7ser", "Schwung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War er im Blick, mit dem man die barocken", "tokens": ["War", "er", "im", "Blick", ",", "mit", "dem", "man", "die", "ba\u00b7ro\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,", "APPR", "PRELS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Engelfiguren, die bei blauen Glocken", "tokens": ["En\u00b7gel\u00b7fi\u00b7gu\u00b7ren", ",", "die", "bei", "blau\u00b7en", "Glo\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "im Felde standen voll Erinnerung,", "tokens": ["im", "Fel\u00b7de", "stan\u00b7den", "voll", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "annahm und hielt und wieder lie\u00df, bevor", "tokens": ["an\u00b7nahm", "und", "hielt", "und", "wie\u00b7der", "lie\u00df", ",", "be\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "ADV", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Schlo\u00dfpark schlie\u00dfend um die Fahrt sich dr\u00e4ngte,", "tokens": ["der", "Schlo\u00df\u00b7park", "schlie\u00b7\u00dfend", "um", "die", "Fahrt", "sich", "dr\u00e4ng\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "an die er streifte, die er \u00fcberh\u00e4ngte", "tokens": ["an", "die", "er", "streif\u00b7te", ",", "die", "er", "\u00fc\u00b7ber\u00b7h\u00e4ng\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und pl\u00f6tzlich freigab: denn da war das Tor,", "tokens": ["und", "pl\u00f6tz\u00b7lich", "frei\u00b7gab", ":", "denn", "da", "war", "das", "Tor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$.", "KON", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "das nun, als h\u00e4tte es sie angerufen,", "tokens": ["das", "nun", ",", "als", "h\u00e4t\u00b7te", "es", "sie", "an\u00b7ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die lange Front zu einer Schwenkung zwang,", "tokens": ["die", "lan\u00b7ge", "Front", "zu", "ei\u00b7ner", "Schwen\u00b7kung", "zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nach der sie stand. Aufgl\u00e4nzend ging ein Gleiten", "tokens": ["nach", "der", "sie", "stand", ".", "Auf\u00b7gl\u00e4n\u00b7zend", "ging", "ein", "Glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$.", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "die Glast\u00fcr abw\u00e4rts; und ein Windhund drang", "tokens": ["die", "Glas\u00b7t\u00fcr", "ab\u00b7w\u00e4rts", ";", "und", "ein", "Wind\u00b7hund", "drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus ihrem Aufgehn, seine nahen Seiten", "tokens": ["aus", "ih\u00b7rem", "Auf\u00b7gehn", ",", "sei\u00b7ne", "na\u00b7hen", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "heruntertragend von den flachen Stufen.", "tokens": ["her\u00b7un\u00b7ter\u00b7tra\u00b7gend", "von", "den", "fla\u00b7chen", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}