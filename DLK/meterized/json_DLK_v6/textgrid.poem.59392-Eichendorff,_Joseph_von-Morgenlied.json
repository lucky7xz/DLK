{"textgrid.poem.59392": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Stimmlein noch schallt von allen", "tokens": ["Kein", "Stimm\u00b7lein", "noch", "schallt", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "APPR", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In fr\u00fchester Morgenstund,", "tokens": ["In", "fr\u00fc\u00b7hes\u00b7ter", "Mor\u00b7gen\u00b7stund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie still ist's noch in den Hallen", "tokens": ["Wie", "still", "ist's", "noch", "in", "den", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch den weiten Waldesgrund.", "tokens": ["Durch", "den", "wei\u00b7ten", "Wal\u00b7des\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich stehe hoch \u00fcberm Tale", "tokens": ["Ich", "ste\u00b7he", "hoch", "\u00fc\u00b7berm", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Stille vor gro\u00dfer Lust,", "tokens": ["Stil\u00b7le", "vor", "gro\u00b7\u00dfer", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und schau nach dem ersten Strahle,", "tokens": ["Und", "schau", "nach", "dem", "ers\u00b7ten", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "K\u00fchl schauernd in tiefster Brust.", "tokens": ["K\u00fchl", "schau\u00b7ernd", "in", "tiefs\u00b7ter", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie sieht da zu dieser Stunde", "tokens": ["Wie", "sieht", "da", "zu", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So anders das Land herauf,", "tokens": ["So", "an\u00b7ders", "das", "Land", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nichts h\u00f6r ich da in der Runde", "tokens": ["Nichts", "h\u00f6r", "ich", "da", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als von fern der Str\u00f6me Lauf.", "tokens": ["Als", "von", "fern", "der", "Str\u00f6\u00b7me", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ehe sich alle erhoben", "tokens": ["Und", "e\u00b7he", "sich", "al\u00b7le", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "PIS", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Tages Freuden und Weh,", "tokens": ["Des", "Ta\u00b7ges", "Freu\u00b7den", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Will ich, Herr Gott, dich loben", "tokens": ["Will", "ich", ",", "Herr", "Gott", ",", "dich", "lo\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "NN", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier einsam in stiller H\u00f6h. \u2013", "tokens": ["Hier", "ein\u00b7sam", "in", "stil\u00b7ler", "H\u00f6h", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun rauschen schon st\u00e4rker die W\u00e4lder,", "tokens": ["Nun", "rau\u00b7schen", "schon", "st\u00e4r\u00b7ker", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Morgenlicht funkelt herauf,", "tokens": ["Mor\u00b7gen\u00b7licht", "fun\u00b7kelt", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Lerche singt \u00fcber den Feldern,", "tokens": ["Die", "Ler\u00b7che", "singt", "\u00fc\u00b7ber", "den", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sch\u00f6ne Erde, nun wache auf!", "tokens": ["Sch\u00f6\u00b7ne", "Er\u00b7de", ",", "nun", "wa\u00b7che", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJA", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}