{"textgrid.poem.32830": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leider! bin ich sehr zerstreut!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leider! bin ich sehr zerstreut!", "tokens": ["Lei\u00b7der", "!", "bin", "ich", "sehr", "zer\u00b7streut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich thu zu mancher Zeit", "tokens": ["Und", "ich", "thu", "zu", "man\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht, was ich wohl soll!", "tokens": ["Nicht", ",", "was", "ich", "wohl", "soll", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWS", "PPER", "ADV", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Will mein Nachbar billig seyn,", "tokens": ["Will", "mein", "Nach\u00b7bar", "bil\u00b7lig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So mu\u00df er mir die\u00df verzeihn:", "tokens": ["So", "mu\u00df", "er", "mir", "die\u00df", "ver\u00b7zeihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PDS", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Doch der Mann ist toll!", "tokens": ["Doch", "der", "Mann", "ist", "toll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst kam ich zu ihm gerennt:", "tokens": ["J\u00fcngst", "kam", "ich", "zu", "ihm", "ge\u00b7rennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eben schickt ihm ein Client", "tokens": ["E\u00b7ben", "schickt", "ihm", "ein", "Client"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein paar Flaschen Wein;", "tokens": ["Ein", "paar", "Fla\u00b7schen", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Voll Zerstreuung leer ich aus:", "tokens": ["Voll", "Zer\u00b7streu\u00b7ung", "leer", "ich", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn er selbst war nicht zu Haus,", "tokens": ["Denn", "er", "selbst", "war", "nicht", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sein Weib allein.", "tokens": ["Und", "sein", "Weib", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Die\u00df verlohnte sich der M\u00fch:", "tokens": ["Die\u00df", "ver\u00b7lohn\u00b7te", "sich", "der", "M\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Voll Zerstreuung sah ich sie", "tokens": ["Voll", "Zer\u00b7streu\u00b7ung", "sah", "ich", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr mein Weibgen an:", "tokens": ["F\u00fcr", "mein", "Weib\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eben kam der Narr zur\u00fcck;", "tokens": ["E\u00b7ben", "kam", "der", "Narr", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wer wei\u00df, es war sein Gl\u00fcck!", "tokens": ["Und", "wer", "wei\u00df", ",", "es", "war", "sein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich sonst gethan!", "tokens": ["Was", "ich", "sonst", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}