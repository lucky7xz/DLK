{"textgrid.poem.50355": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Versichre dich, ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Versichre dich, ", "tokens": ["Ver\u00b7sich\u00b7re", "dich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was ich nicht in dem Hertzen trage,", "tokens": ["Was", "ich", "nicht", "in", "dem", "Hert\u00b7zen", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch dass ein ", "tokens": ["Noch", "dass", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Mit Liebe weiss ich nicht zu schertzen:", "tokens": ["Mit", "Lie\u00b7be", "weiss", "ich", "nicht", "zu", "schert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jedes ", "tokens": ["Ein", "je\u00b7des"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}}}}