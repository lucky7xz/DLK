{"textgrid.poem.54373": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "5. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? will die bange Furcht die Hand noch l\u00e4nger st\u00e4mmen?", "tokens": ["Wie", "?", "will", "die", "ban\u00b7ge", "Furcht", "die", "Hand", "noch", "l\u00e4n\u00b7ger", "st\u00e4m\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entrei\u00df dich ihr mein Kiel, verfolge deinen Lauf,", "tokens": ["Ent\u00b7rei\u00df", "dich", "ihr", "mein", "Kiel", ",", "ver\u00b7fol\u00b7ge", "dei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und la\u00df ihn ferner nicht durch bl\u00f6den Einwurf hemmen;", "tokens": ["Und", "la\u00df", "ihn", "fer\u00b7ner", "nicht", "durch", "bl\u00f6\u00b7den", "Ein\u00b7wurf", "hem\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verbanne Sorg und Qual, gieb allen Zweifel auf.", "tokens": ["Ver\u00b7ban\u00b7ne", "Sorg", "und", "Qual", ",", "gieb", "al\u00b7len", "Zwei\u00b7fel", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "VVIMP", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun ists nicht l\u00e4nger Zeit die Leidenschaft zu bergen,", "tokens": ["Nun", "ists", "nicht", "l\u00e4n\u00b7ger", "Zeit", "die", "Lei\u00b7den\u00b7schaft", "zu", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADJD", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Entsch\u00fctte dich der Last nunmehr, verzagter Geist!", "tokens": ["Ent\u00b7sch\u00fct\u00b7te", "dich", "der", "Last", "nun\u00b7mehr", ",", "ver\u00b7zag\u00b7ter", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du stellst dir Riesen vor, und k\u00e4mpfst doch nur mit Zwergen,", "tokens": ["Du", "stellst", "dir", "Rie\u00b7sen", "vor", ",", "und", "k\u00e4mpfst", "doch", "nur", "mit", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was hilft es, wenn man gleich den Schmerzen lang verbei\u00dft;", "tokens": ["Was", "hilft", "es", ",", "wenn", "man", "gleich", "den", "Schmer\u00b7zen", "lang", "ver\u00b7bei\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Annehmlichster der Welt! mein Caunus, mein Verlangen,", "tokens": ["An\u00b7nehm\u00b7lichs\u00b7ter", "der", "Welt", "!", "mein", "Cau\u00b7nus", ",", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Entsiegle dieses Blat, das dir mein Leiden klagt.", "tokens": ["Ent\u00b7sieg\u00b7le", "die\u00b7ses", "Blat", ",", "das", "dir", "mein", "Lei\u00b7den", "klagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich bin dir, doch mit Scheu, schon lange nachgegangen,", "tokens": ["Ich", "bin", "dir", ",", "doch", "mit", "Scheu", ",", "schon", "lan\u00b7ge", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADV", "APPR", "NN", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun ist die Furcht verbannt, nun hab ichs doch gewagt.", "tokens": ["Nun", "ist", "die", "Furcht", "ver\u00b7bannt", ",", "nun", "hab", "ichs", "doch", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Geist reicht nicht mehr zu, die Sehnsucht zu verhelen", "tokens": ["Mein", "Geist", "reicht", "nicht", "mehr", "zu", ",", "die", "Sehn\u00b7sucht", "zu", "ver\u00b7he\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die mich, ach glaub es mir, geliebter, Tag und Nacht,", "tokens": ["Die", "mich", ",", "ach", "glaub", "es", "mir", ",", "ge\u00b7lieb\u00b7ter", ",", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,", "ADJA", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Auf das empfindlichste best\u00e4ndig sucht zu qu\u00e4len,", "tokens": ["Auf", "das", "emp\u00b7find\u00b7lichs\u00b7te", "be\u00b7st\u00e4n\u00b7dig", "sucht", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und meinen K\u00f6rper recht zum Schattenbilde macht.", "tokens": ["Und", "mei\u00b7nen", "K\u00f6r\u00b7per", "recht", "zum", "Schat\u00b7ten\u00b7bil\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die zuckers\u00fcsse Kost will mir zu Myrrhen werden,", "tokens": ["Die", "zu\u00b7cker\u00b7s\u00fcs\u00b7se", "Kost", "will", "mir", "zu", "Myr\u00b7rhen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der beste Rebensaft schmeckt mir wie Aloe.", "tokens": ["Der", "bes\u00b7te", "Re\u00b7ben\u00b7saft", "schmeckt", "mir", "wie", "A\u00b7loe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "KOKOM", "NE", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Selbst meine Wohnung ist ein Zeughaus der Beschwerden,", "tokens": ["Selbst", "mei\u00b7ne", "Woh\u00b7nung", "ist", "ein", "Zeug\u00b7haus", "der", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Brust tr\u00e4gt \u00fcberall sich mit dem herben Weh.", "tokens": ["Die", "Brust", "tr\u00e4gt", "\u00fc\u00b7be\u00b7rall", "sich", "mit", "dem", "her\u00b7ben", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was aber ist es denn das mir den Lenz der Jahre", "tokens": ["Was", "a\u00b7ber", "ist", "es", "denn", "das", "mir", "den", "Lenz", "der", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "ADV", "ART", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Im d\u00fcstern Winter schon durch Harm und Gram verkehrt,", "tokens": ["Im", "d\u00fcs\u00b7tern", "Win\u00b7ter", "schon", "durch", "Harm", "und", "Gram", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So da\u00df der innere Schmerz mich zeitig auf die Bahre", "tokens": ["So", "da\u00df", "der", "in\u00b7ne\u00b7re", "Schmerz", "mich", "zei\u00b7tig", "auf", "die", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Beklagensw\u00fcrdig streckt, wofern man mich nicht h\u00f6rt?", "tokens": ["Be\u00b7kla\u00b7gens\u00b7w\u00fcr\u00b7dig", "streckt", ",", "wo\u00b7fern", "man", "mich", "nicht", "h\u00f6rt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "PIS", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach Bruder! darf ich dich den sch\u00f6nen M\u00f6rder nennen,", "tokens": ["Ach", "Bru\u00b7der", "!", "darf", "ich", "dich", "den", "sch\u00f6\u00b7nen", "M\u00f6r\u00b7der", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VMFIN", "PPER", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der, eh ichs recht gewust, mir Mark und Blut entf\u00fchrt?", "tokens": ["Der", ",", "eh", "ichs", "recht", "ge\u00b7wust", ",", "mir", "Mark", "und", "Blut", "ent\u00b7f\u00fchrt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "ADJD", "VVPP", "$,", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich bin in dich entbrannt, la\u00df mich es frey bekennen,", "tokens": ["Ich", "bin", "in", "dich", "ent\u00b7brannt", ",", "la\u00df", "mich", "es", "frey", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Du hast, Anmuthigster, mir Seel und Herz ger\u00fchrt.", "tokens": ["Du", "hast", ",", "An\u00b7mut\u00b7higs\u00b7ter", ",", "mir", "Seel", "und", "Herz", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Kein andrer kann so leicht wie du die Sehnsucht lindern,", "tokens": ["Kein", "an\u00b7drer", "kann", "so", "leicht", "wie", "du", "die", "Sehn\u00b7sucht", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VMFIN", "ADV", "ADJD", "KOKOM", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wenn er dem Narzi\u00df und Paris \u00e4hnlich w\u00e4r;", "tokens": ["Und", "wenn", "er", "dem", "Nar\u00b7zi\u00df", "und", "Pa\u00b7ris", "\u00e4hn\u00b7lich", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "KON", "NE", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Auch G\u00f6tter sind zu schwach die Neigung zu verhindern,", "tokens": ["Auch", "G\u00f6t\u00b7ter", "sind", "zu", "schwach", "die", "Nei\u00b7gung", "zu", "ver\u00b7hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PTKA", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie steiget gegen dich noch immer mehr und mehr,", "tokens": ["Sie", "stei\u00b7get", "ge\u00b7gen", "dich", "noch", "im\u00b7mer", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Warum verf\u00e4rbst du dich? wie? fluchst du meinen Flammen?", "tokens": ["Wa\u00b7rum", "ver\u00b7f\u00e4rbst", "du", "dich", "?", "wie", "?", "fluchst", "du", "mei\u00b7nen", "Flam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$.", "PWAV", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie sind ja mehr als rein; ach Caunus, glaube mir", "tokens": ["Sie", "sind", "ja", "mehr", "als", "rein", ";", "ach", "Cau\u00b7nus", ",", "glau\u00b7be", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "KOKOM", "ADJD", "$.", "XY", "XY", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df selbige gewi\u00df von etwas hohem stammen,", "tokens": ["Da\u00df", "sel\u00b7bi\u00b7ge", "ge\u00b7wi\u00df", "von", "et\u00b7was", "ho\u00b7hem", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "APPR", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "O! stelle dir nur selbst die Macht der Liebe f\u00fcr.", "tokens": ["O", "!", "stel\u00b7le", "dir", "nur", "selbst", "die", "Macht", "der", "Lie\u00b7be", "f\u00fcr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du weist was f\u00fcr Gewalt die Herscherinn besitzet,", "tokens": ["Du", "weist", "was", "f\u00fcr", "Ge\u00b7walt", "die", "Her\u00b7sche\u00b7rinn", "be\u00b7sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hier gilt kein Widerstand; denn ihrer Reizung Schein,", "tokens": ["Hier", "gilt", "kein", "Wi\u00b7der\u00b7stand", ";", "denn", "ih\u00b7rer", "Rei\u00b7zung", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$.", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der allen Sterblichen in Herz und Augen blitzet,", "tokens": ["Der", "al\u00b7len", "Sterb\u00b7li\u00b7chen", "in", "Herz", "und", "Au\u00b7gen", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambi