, "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Erl\u00f6se Bibliden von ihren schweren Ketten,", "tokens": ["Er\u00b7l\u00f6\u00b7se", "Bib\u00b7li\u00b7den", "von", "ih\u00b7ren", "schwe\u00b7ren", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Sie gleicht Andromeden, und du sollst Perseus seyn.", "tokens": ["Sie", "gleicht", "And\u00b7ro\u00b7me\u00b7den", ",", "und", "du", "sollst", "Per\u00b7seus", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KON", "PPER", "VMFIN", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Der Werkstatt der Natur, so uns verschlossen hegte,", "tokens": ["Der", "Werk\u00b7statt", "der", "Na\u00b7tur", ",", "so", "uns", "ver\u00b7schlos\u00b7sen", "heg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Stellt uns als Zwillinge zugleich ans Licht der Welt;", "tokens": ["Stellt", "uns", "als", "Zwil\u00b7lin\u00b7ge", "zu\u00b7gleich", "ans", "Licht", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.67": {"text": "Durch welche Nachbarschaft sie uns ins Herze pr\u00e4gte,", "tokens": ["Durch", "wel\u00b7che", "Nach\u00b7bar\u00b7schaft", "sie", "uns", "ins", "Her\u00b7ze", "pr\u00e4g\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Da\u00df das, was sich recht liebt, auch stets zusammen h\u00e4lt.", "tokens": ["Da\u00df", "das", ",", "was", "sich", "recht", "liebt", ",", "auch", "stets", "zu\u00b7sam\u00b7men", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Bespiegle dich doch nur an dem gestirnten Bogen,", "tokens": ["Be\u00b7spieg\u00b7le", "dich", "doch", "nur", "an", "dem", "ge\u00b7stirn\u00b7ten", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.70": {"text": "Der statt der Vorschrift uns das Zwillingszeichen weist.", "tokens": ["Der", "statt", "der", "Vor\u00b7schrift", "uns", "das", "Zwil\u00b7lings\u00b7zei\u00b7chen", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Umarmen sie sich nicht vertraut und recht gewogen?", "tokens": ["Um\u00b7ar\u00b7men", "sie", "sich", "nicht", "ver\u00b7traut", "und", "recht", "ge\u00b7wo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Wer ist der dieses Paar nicht unzertrennlich hei\u00dft!", "tokens": ["Wer", "ist", "der", "die\u00b7ses", "Paar", "nicht", "un\u00b7zer\u00b7trenn\u00b7lich", "hei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PDAT", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Ergreife den Entschlu\u00df, und la\u00df mich, liebster, wissen,", "tokens": ["Er\u00b7grei\u00b7fe", "den", "Ent\u00b7schlu\u00df", ",", "und", "la\u00df", "mich", ",", "liebs\u00b7ter", ",", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVIMP", "PPER", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Ob Biblide dein Herz und dich nach Wunsch besiegt?", "tokens": ["Ob", "Bib\u00b7li\u00b7de", "dein", "Herz", "und", "dich", "nach", "Wunsch", "be\u00b7siegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "KON", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Wie z\u00e4rtlich werd ich dich so dann, mein Bruder k\u00fcssen!", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "werd", "ich", "dich", "so", "dann", ",", "mein", "Bru\u00b7der", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PRF", "ADV", "ADV", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Ich schmecke schon voraus, wie mich die Lust vergn\u00fcgt,", "tokens": ["Ich", "schme\u00b7cke", "schon", "vo\u00b7raus", ",", "wie", "mich", "die", "Lust", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Doch will dein Eigensinn mir kein Geh\u00f6re geben,", "tokens": ["Doch", "will", "dein", "Ei\u00b7gen\u00b7sinn", "mir", "kein", "Ge\u00b7h\u00f6\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "So glaube, da\u00df der Gram mein banges Herze bricht", "tokens": ["So", "glau\u00b7be", ",", "da\u00df", "der", "Gram", "mein", "ban\u00b7ges", "Her\u00b7ze", "bricht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Denn, Sch\u00f6nster, ohne dich vermag ich nicht zu leben;", "tokens": ["Denn", ",", "Sch\u00f6ns\u00b7ter", ",", "oh\u00b7ne", "dich", "ver\u00b7mag", "ich", "nicht", "zu", "le\u00b7ben", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "KOUI", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Mein Sinn ist nur auf dich, auf keinen sonst gericht.", "tokens": ["Mein", "Sinn", "ist", "nur", "auf", "dich", ",", "auf", "kei\u00b7nen", "sonst", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "$,", "APPR", "PIAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Ich m\u00fcste, fl\u00f6hst du mich, in Z\u00e4hren ganz zerfliessen,", "tokens": ["Ich", "m\u00fcs\u00b7te", ",", "fl\u00f6hst", "du", "mich", ",", "in", "Z\u00e4h\u00b7ren", "ganz", "zer\u00b7flies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "PRF", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Mein Auge w\u00fcrde sich bey stetem Weh und Ach,", "tokens": ["Mein", "Au\u00b7ge", "w\u00fcr\u00b7de", "sich", "bey", "ste\u00b7tem", "Weh", "und", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "In Thr\u00e4nen ohne Zahl gleich einem Strohm ergiessen", "tokens": ["In", "Thr\u00e4\u00b7nen", "oh\u00b7ne", "Zahl", "gleich", "ei\u00b7nem", "Strohm", "er\u00b7gies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Drum Bruder, h\u00f6re mich, gieb meiner Liebe nach.", "tokens": ["Drum", "Bru\u00b7der", ",", "h\u00f6\u00b7re", "mich", ",", "gieb", "mei\u00b7ner", "Lie\u00b7be", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "VVFIN", "PPER", "$,", "VVIMP"