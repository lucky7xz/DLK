{"textgrid.poem.28292": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil Welt die Redligkeit verjagt und duldet nicht,", "tokens": ["Weil", "Welt", "die", "Red\u00b7lig\u00b7keit", "ver\u00b7jagt", "und", "dul\u00b7det", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVPP", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sey du, der du hier f\u00fcr\u00fcber gehst, bericht,", "tokens": ["So", "sey", "du", ",", "der", "du", "hier", "f\u00fc\u00b7r\u00fc\u00b7ber", "gehst", ",", "be\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df nicht ein schlechter Theil, da\u00df grosser Schatz von ihr", "tokens": ["Da\u00df", "nicht", "ein", "schlech\u00b7ter", "Theil", ",", "da\u00df", "gros\u00b7ser", "Schatz", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat unter diesen Stein sich wie verborgen hier.", "tokens": ["Hat", "un\u00b7ter", "die\u00b7sen", "Stein", "sich", "wie", "ver\u00b7bor\u00b7gen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "PRF", "KOKOM", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wofern du redlich bist, so seuffze, da\u00df ein Stein", "tokens": ["Wo\u00b7fern", "du", "red\u00b7lich", "bist", ",", "so", "seuff\u00b7ze", ",", "da\u00df", "ein", "Stein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Soll w\u00fcrdiger, als wir, di\u00df Gut zu haben seyn.", "tokens": ["Soll", "w\u00fcr\u00b7di\u00b7ger", ",", "als", "wir", ",", "di\u00df", "Gut", "zu", "ha\u00b7ben", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "$,", "KOUS", "PPER", "$,", "PDS", "ADJD", "PTKZU", "VAINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}