{"textgrid.poem.41295": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Hygin, einen gesunden Alten", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hygin, du bist von sechzig Jahren,", "tokens": ["Hy\u00b7gin", ",", "du", "bist", "von", "sech\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nur im Kr\u00e4nkeln unerfahren.", "tokens": ["Und", "nur", "im", "Kr\u00e4n\u00b7keln", "un\u00b7er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Podagra, der Krampf, die Gicht", "tokens": ["Das", "Po\u00b7da\u00b7gra", ",", "der", "Krampf", ",", "die", "Gicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbittern dir den Steinwein nicht.", "tokens": ["Ver\u00b7bit\u00b7tern", "dir", "den", "Stein\u00b7wein", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich kann kein Arzt zu Elixiren,", "tokens": ["Dich", "kann", "kein", "Arzt", "zu", "E\u00b7li\u00b7xi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Lebens\u00f6l, zum Salz verf\u00fchren:", "tokens": ["Zum", "Le\u00b7bens\u00b7\u00f6l", ",", "zum", "Salz", "ver\u00b7f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Macht er dir Aphorismos kund,", "tokens": ["Macht", "er", "dir", "A\u00b7pho\u00b7ris\u00b7mos", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "NE", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "So lachst du, bist und bleibst gesund.", "tokens": ["So", "lachst", "du", ",", "bist", "und", "bleibst", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein andrer z\u00e4hle seine Tage,", "tokens": ["Ein", "an\u00b7drer", "z\u00e4h\u00b7le", "sei\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und rechne nicht die Zeit der Plage,", "tokens": ["Und", "rech\u00b7ne", "nicht", "die", "Zeit", "der", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Noch was vom Leben \u00fcberhaupt", "tokens": ["Noch", "was", "vom", "Le\u00b7ben", "\u00fc\u00b7ber\u00b7haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Schmerz, Krankheit oder Kummer raubt;", "tokens": ["Schmerz", ",", "Krank\u00b7heit", "o\u00b7der", "Kum\u00b7mer", "raubt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So scheinen ihm die Jahre minder:", "tokens": ["So", "schei\u00b7nen", "ihm", "die", "Jah\u00b7re", "min\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wir hei\u00dfen alt, und sind noch Kinder.", "tokens": ["Wir", "hei\u00b7\u00dfen", "alt", ",", "und", "sind", "noch", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dem, der mir Nestors Dauer preist,", "tokens": ["Dem", ",", "der", "mir", "Nes\u00b7tors", "Dau\u00b7er", "preist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und Priams Alter trefflich hei\u00dft,", "tokens": ["Und", "Pri\u00b7ams", "Al\u00b7ter", "treff\u00b7lich", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dem werd' ich nimmer Beifall geben:", "tokens": ["Dem", "werd'", "ich", "nim\u00b7mer", "Bei\u00b7fall", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Nur die Gesundheit ist das Leben.", "tokens": ["Nur", "die", "Ge\u00b7sund\u00b7heit", "ist", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}