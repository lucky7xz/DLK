{"dta.poem.10093": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Schiff-Fahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem grossen Schiff, das unl\u00e4ngst erst gebaut,", "tokens": ["In", "ei\u00b7nem", "gros\u00b7sen", "Schiff", ",", "das", "un\u00b7l\u00e4ngst", "erst", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hatt\u2019 ich mich j\u00fcngst den wilden Wellen,", "tokens": ["Hatt'", "ich", "mich", "j\u00fcngst", "den", "wil\u00b7den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Ampts-Gesch\u00e4fften, anvertraut.", "tokens": ["In", "Ampt\u00b7sGe\u00b7sch\u00e4ff\u00b7ten", ",", "an\u00b7ver\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Indem nun gleich darauf die hohlen Segel schwellen,", "tokens": ["In\u00b7dem", "nun", "gleich", "da\u00b7rauf", "die", "hoh\u00b7len", "Se\u00b7gel", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Nord-Wind schnaubt und saust,", "tokens": ["Der", "Nord\u00b7Wind", "schnaubt", "und", "saust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das Wasser sch\u00e4umt und braust;", "tokens": ["Das", "Was\u00b7ser", "sch\u00e4umt", "und", "braust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Brach, nach versuncknem Sonnen-Schein,", "tokens": ["Brach", ",", "nach", "ver\u00b7sunck\u00b7nem", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Nacht mit duncklen Schatten ein:", "tokens": ["Die", "Nacht", "mit", "dunck\u00b7len", "Schat\u00b7ten", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und, wie es sp\u00e4t ward, legt\u2019 ein ieder,", "tokens": ["Und", ",", "wie", "es", "sp\u00e4t", "ward", ",", "legt'", "ein", "ie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht aller Furcht entohniget, sich nieder,", "tokens": ["Nicht", "al\u00b7ler", "Furcht", "en\u00b7toh\u00b7ni\u00b7get", ",", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VVFIN", "$,", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Auf Betten, die (wie es der Brauch", "tokens": ["Auf", "Bet\u00b7ten", ",", "die", "(", "wie", "es", "der", "Brauch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "$(", "PWAV", "PPER", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "In Schiffen) in des Schiffes Bauch.", "tokens": ["In", "Schif\u00b7fen", ")", "in", "des", "Schif\u00b7fes", "Bauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Indem ich nun, da sich der Sturm vermehrte,", "tokens": ["In\u00b7dem", "ich", "nun", ",", "da", "sich", "der", "Sturm", "ver\u00b7mehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "PRF", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gantz nah\u2019 an meinem Ohr, der Fluthen Brausen h\u00f6rte,", "tokens": ["Gantz", "nah'", "an", "mei\u00b7nem", "Ohr", ",", "der", "Flut\u00b7hen", "Brau\u00b7sen", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gedacht ich bey mir selbst, indem die andern schlieffen:", "tokens": ["Ge\u00b7dacht", "ich", "bey", "mir", "selbst", ",", "in\u00b7dem", "die", "an\u00b7dern", "schlief\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "ADV", "$,", "KOUS", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie nahe sind mir ietzt die finstern Tieffen!", "tokens": ["Wie", "na\u00b7he", "sind", "mir", "ietzt", "die", "fins\u00b7tern", "Tief\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein GOTT! ein Holtz, nur wenig Daumen dick,", "tokens": ["Mein", "GoTT", "!", "ein", "Holtz", ",", "nur", "we\u00b7nig", "Dau\u00b7men", "dick", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "$,", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist blo\u00df der Zwischen-Stand,", "tokens": ["Ist", "blo\u00df", "der", "Zwi\u00b7schen\u00b7Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und h\u00e4lt den Schwall der duncklen Fluth", "tokens": ["Und", "h\u00e4lt", "den", "Schwall", "der", "dunck\u00b7len", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von mir, und von dem Pfuhl des Abgrunds mich, zur\u00fcck,", "tokens": ["Von", "mir", ",", "und", "von", "dem", "Pfuhl", "des", "Ab\u00b7grunds", "mich", ",", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "APPR", "ART", "NN", "ART", "NN", "PPER", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In welchem ungezehlte Heere", "tokens": ["In", "wel\u00b7chem", "un\u00b7ge\u00b7zehl\u00b7te", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von Taumlern, Kabeljauen, St\u00f6re,", "tokens": ["Von", "Taum\u00b7lern", ",", "Ka\u00b7bel\u00b7jau\u00b7en", ",", "St\u00f6\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und andre Wasser-Thiere schw\u00e4rmen.", "tokens": ["Und", "and\u00b7re", "Was\u00b7ser\u00b7Thie\u00b7re", "schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das brausende Ger\u00e4usch der Fluth kam meinem Ohr,", "tokens": ["Das", "brau\u00b7sen\u00b7de", "Ge\u00b7r\u00e4usch", "der", "Fluth", "kam", "mei\u00b7nem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem es so nahe war, nicht anders vor,", "tokens": ["Dem", "es", "so", "na\u00b7he", "war", ",", "nicht", "an\u00b7ders", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ob so gar das Heer der nahen Fische", "tokens": ["Als", "ob", "so", "gar", "das", "Heer", "der", "na\u00b7hen", "Fi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit der besch\u00e4umten Fluthen l\u00e4rmen", "tokens": ["Mit", "der", "be\u00b7sch\u00e4um\u00b7ten", "Flut\u00b7hen", "l\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr schnaubendes Get\u00f6se mische.", "tokens": ["Ihr", "schnau\u00b7ben\u00b7des", "Ge\u00b7t\u00f6\u00b7se", "mi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo auf der Welt ein Stand, der uns, auf GOTT", "tokens": ["Wo", "auf", "der", "Welt", "ein", "Stand", ",", "der", "uns", ",", "auf", "GoTT"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "$,", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Seel\u2019 und Sinn auf Seine Huld zu lencken,", "tokens": ["Und", "Seel'", "und", "Sinn", "auf", "Sei\u00b7ne", "Huld", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Recht bewegen sollt, so ist es dieser wol,", "tokens": ["Mit", "Recht", "be\u00b7we\u00b7gen", "sollt", ",", "so", "ist", "es", "die\u00b7ser", "wol", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "PDS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da zwischen Tod und Leben", "tokens": ["Da", "zwi\u00b7schen", "Tod", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur wenig Zoll", "tokens": ["Nur", "we\u00b7nig", "Zoll"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Uns eine Scheide-Wand so gar zerbrechlich, geben.", "tokens": ["Uns", "ei\u00b7ne", "Schei\u00b7de\u00b7Wand", "so", "gar", "zer\u00b7brech\u00b7lich", ",", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "ADV", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Indem ich also lag und dachte,", "tokens": ["In\u00b7dem", "ich", "al\u00b7so", "lag", "und", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlieff ich gelassen ein.", "tokens": ["Schlieff", "ich", "ge\u00b7las\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie ich fr\u00fch erwachte,", "tokens": ["Und", "wie", "ich", "fr\u00fch", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und meinem ", "tokens": ["Und", "mei\u00b7nem"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Macht\u2019 ich mir, was ich sah\u2019, aufs neu zu Nutz.", "tokens": ["Macht'", "ich", "mir", ",", "was", "ich", "sah'", ",", "aufs", "neu", "zu", "Nutz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "APPRART", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich sahe denn darauf die weite Wasser-Welt,", "tokens": ["Ich", "sa\u00b7he", "denn", "da\u00b7rauf", "die", "wei\u00b7te", "Was\u00b7ser\u00b7Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wie ein blau unabzusehend Feld,", "tokens": ["Als", "wie", "ein", "blau", "un\u00b7ab\u00b7zu\u00b7se\u00b7hend", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit dessen weit entlegnen Grentzen", "tokens": ["Mit", "des\u00b7sen", "weit", "ent\u00b7leg\u00b7nen", "Grent\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel selbst, dem Schein nach, sich verband.", "tokens": ["Der", "Him\u00b7mel", "selbst", ",", "dem", "Schein", "nach", ",", "sich", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "PTKVZ", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sah dies rege blau vom hellen Sonnen-Licht,", "tokens": ["Ich", "sah", "dies", "re\u00b7ge", "blau", "vom", "hel\u00b7len", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADJA", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das sich mit schnellem Blitz an hohlen Wolcken bricht,", "tokens": ["Das", "sich", "mit", "schnel\u00b7lem", "Blitz", "an", "hoh\u00b7len", "Wol\u00b7cken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An manchem Ort, wie fliessend Silber, gl\u00e4ntzen.", "tokens": ["An", "man\u00b7chem", "Ort", ",", "wie", "flies\u00b7send", "Sil\u00b7ber", ",", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "ADJD", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich sah\u2019 auf dieser Fl\u00e4ch\u2019 ein\u2019 ungezehlte Menge", "tokens": ["Ich", "sah'", "auf", "die\u00b7ser", "Fl\u00e4ch'", "ein'", "un\u00b7ge\u00b7zehl\u00b7te", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschwollner Segel mancher Art,", "tokens": ["Ge\u00b7schwoll\u00b7ner", "Se\u00b7gel", "man\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als pfl\u00fcgten sie das Feld der Fluthen hin und her,", "tokens": ["Als", "pfl\u00fcg\u00b7ten", "sie", "das", "Feld", "der", "Flut\u00b7hen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald in die Qwer,", "tokens": ["Bald", "in", "die", "Qwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bald in die L\u00e4nge,", "tokens": ["Bald", "in", "die", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit halber theils, und theils mit gantzer Fahrt,", "tokens": ["Mit", "hal\u00b7ber", "theils", ",", "und", "theils", "mit", "gant\u00b7zer", "Fahrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPO", "ADV", "$,", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die durch den schnellen Druck besch\u00e4umte Wellen theilen,", "tokens": ["Die", "durch", "den", "schnel\u00b7len", "Druck", "be\u00b7sch\u00e4um\u00b7te", "Wel\u00b7len", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und \u00f6ffters, wie ein Pfeil, bey mir vor\u00fcber eilen.", "tokens": ["Und", "\u00f6ff\u00b7ters", ",", "wie", "ein", "Pfeil", ",", "bey", "mir", "vor\u00b7\u00fc\u00b7ber", "ei\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "NN", "$,", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.h