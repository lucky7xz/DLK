"pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit hohlen Segeln, hohen Masten,", "tokens": ["Mit", "hoh\u00b7len", "Se\u00b7geln", ",", "ho\u00b7hen", "Mas\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Von einem zu dem andern Ort,", "tokens": ["Von", "ei\u00b7nem", "zu", "dem", "an\u00b7dern", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So leicht, bequem, geschwinde,", "tokens": ["So", "leicht", ",", "be\u00b7quem", ",", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Durch H\u00fclffe wolgetheilter Winde,", "tokens": ["Durch", "H\u00fclf\u00b7fe", "wol\u00b7ge\u00b7theil\u00b7ter", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "So f\u00fcglich fortzubringen wissen;", "tokens": ["So", "f\u00fcg\u00b7lich", "fort\u00b7zu\u00b7brin\u00b7gen", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Da\u00df offt ein einzger Mann", "tokens": ["Da\u00df", "offt", "ein", "einz\u00b7ger", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Mit einer Hand das Schiff regieren kann,", "tokens": ["Mit", "ei\u00b7ner", "Hand", "das", "Schiff", "re\u00b7gie\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und w\u00e4r\u2019 es noch so gro\u00df.", "tokens": ["Und", "w\u00e4r'", "es", "noch", "so", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Dies zeiget eine Gr\u00f6ss\u2019 in unserm Geist,", "tokens": ["Dies", "zei\u00b7get", "ei\u00b7ne", "Gr\u00f6ss'", "in", "un\u00b7serm", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Die wunderbar, die nicht begreifflich ist.", "tokens": ["Die", "wun\u00b7der\u00b7bar", ",", "die", "nicht", "be\u00b7greif\u00b7flich", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PRELS", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Indem mein Geist nun dieses recht ermisst,", "tokens": ["In\u00b7dem", "mein", "Geist", "nun", "die\u00b7ses", "recht", "er\u00b7misst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PDAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und diese Hoheit der Gedancken", "tokens": ["Und", "die\u00b7se", "Ho\u00b7heit", "der", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich fast der Menschlichkeit entreisst;", "tokens": ["Mich", "fast", "der", "Menschlich\u00b7keit", "en\u00b7treisst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "So lenckt\u2019 ein anderer mich wieder", "tokens": ["So", "len\u00b7ckt'", "ein", "an\u00b7de\u00b7rer", "mich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In die uns zugemessne Schrancken.", "tokens": ["In", "die", "uns", "zu\u00b7ge\u00b7mess\u00b7ne", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er zeigte mir, indem die Menschen weder Wind,", "tokens": ["Er", "zeig\u00b7te", "mir", ",", "in\u00b7dem", "die", "Men\u00b7schen", "we\u00b7der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch Wetter, im geringsten nicht", "tokens": ["Noch", "Wet\u00b7ter", ",", "im", "ge\u00b7rings\u00b7ten", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "APPRART", "ADJA", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu \u00e4ndern m\u00e4chtig sind;", "tokens": ["Zu", "\u00e4n\u00b7dern", "m\u00e4ch\u00b7tig", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.