{"textgrid.poem.41892": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo die Minne herrscht in dem holden Gebiet,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die Minne herrscht in dem holden Gebiet,", "tokens": ["Wo", "die", "Min\u00b7ne", "herrscht", "in", "dem", "hol\u00b7den", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sch\u00f6nste der K\u00f6niginnen,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "der", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo nimmer das singende V\u00f6glein flieht,", "tokens": ["Wo", "nim\u00b7mer", "das", "sin\u00b7gen\u00b7de", "V\u00f6\u00b7glein", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wo ewig der duftende Fr\u00fchling bl\u00fcht,", "tokens": ["Wo", "e\u00b7wig", "der", "duf\u00b7ten\u00b7de", "Fr\u00fch\u00b7ling", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und die B\u00e4chlein nimmer verrinnen:", "tokens": ["Und", "die", "B\u00e4ch\u00b7lein", "nim\u00b7mer", "ver\u00b7rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dort wohnt' ich im hellen, lustigen Hain", "tokens": ["Dort", "wohnt'", "ich", "im", "hel\u00b7len", ",", "lus\u00b7ti\u00b7gen", "Hain"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und diente der Stolzen mit langer Pein", "tokens": ["Und", "dien\u00b7te", "der", "Stol\u00b7zen", "mit", "lan\u00b7ger", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Jahr aus, Jahr ein,", "tokens": ["Jahr", "aus", ",", "Jahr", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und konnte sie nimmer gewinnen.", "tokens": ["Und", "konn\u00b7te", "sie", "nim\u00b7mer", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Da wand ich erz\u00fcrnt von der Kette mich los", "tokens": ["Da", "wand", "ich", "er\u00b7z\u00fcrnt", "von", "der", "Ket\u00b7te", "mich", "los"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und dachte sie ewig zu meiden.", "tokens": ["Und", "dach\u00b7te", "sie", "e\u00b7wig", "zu", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und ich barg mich tief in des Waldes Schoo\u00df", "tokens": ["Und", "ich", "barg", "mich", "tief", "in", "des", "Wal\u00b7des", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und warf mich seufzend in's duftige Moos", "tokens": ["Und", "warf", "mich", "seuf\u00b7zend", "in's", "duf\u00b7ti\u00b7ge", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und rief im heimlichen Leiden:", "tokens": ["Und", "rief", "im", "heim\u00b7li\u00b7chen", "Lei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "O Hain, wie spielet das V\u00f6glein hier", "tokens": ["O", "Hain", ",", "wie", "spie\u00b7let", "das", "V\u00f6\u00b7glein", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So still und friedlich im gr\u00fcnen Revier!", "tokens": ["So", "still", "und", "fried\u00b7lich", "im", "gr\u00fc\u00b7nen", "Re\u00b7vier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sprich, wird auch mir", "tokens": ["Sprich", ",", "wird", "auch", "mir"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "VAFIN", "ADV", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dein Schatten wohl Ruhe bescheiden?", "tokens": ["Dein", "Schat\u00b7ten", "wohl", "Ru\u00b7he", "be\u00b7schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und s\u00e4uselnd bebte der weite Hain", "tokens": ["Und", "s\u00e4u\u00b7selnd", "beb\u00b7te", "der", "wei\u00b7te", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sprach mit k\u00fchligem Wehen:", "tokens": ["Und", "sprach", "mit", "k\u00fch\u00b7li\u00b7gem", "We\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tief h\u00fcll' ich in d\u00e4mm'rige Lauben dich ein;", "tokens": ["Tief", "h\u00fcll'", "ich", "in", "d\u00e4m\u00b7m'\u00b7ri\u00b7ge", "Lau\u00b7ben", "dich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht sollst du mir ferner in z\u00f6gernder Pein", "tokens": ["Nicht", "sollst", "du", "mir", "fer\u00b7ner", "in", "z\u00f6\u00b7gern\u00b7der", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Vor dem Blicke der Strengen vergehen. \u2013", "tokens": ["Vor", "dem", "Bli\u00b7cke", "der", "Stren\u00b7gen", "ver\u00b7ge\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "O Hain, du tr\u00f6stest mit schlimmem Rath;", "tokens": ["O", "Hain", ",", "du", "tr\u00f6s\u00b7test", "mit", "schlim\u00b7mem", "Rath", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Leicht findet ihr Bild durch die Nacht den Pfad.", "tokens": ["Leicht", "fin\u00b7det", "ihr", "Bild", "durch", "die", "Nacht", "den", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Wer ihr genaht,", "tokens": ["Wer", "ihr", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mu\u00df immer und immer sie sehen!", "tokens": ["Mu\u00df", "im\u00b7mer", "und", "im\u00b7mer", "sie", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "KON", "ADV", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und ich klomm in dem finsteren Wald empor,", "tokens": ["Und", "ich", "klomm", "in", "dem", "fins\u00b7te\u00b7ren", "Wald", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wo wilder die Berge sich heben;", "tokens": ["Wo", "wil\u00b7der", "die", "Ber\u00b7ge", "sich", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da brauste mit Macht aus dem Felsenthor", "tokens": ["Da", "braus\u00b7te", "mit", "Macht", "aus", "dem", "Fel\u00b7sen\u00b7thor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Lautwogend ein sprudelnder Strom hervor,", "tokens": ["Laut\u00b7wo\u00b7gend", "ein", "spru\u00b7deln\u00b7der", "Strom", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Der sollte die Kunde mir geben:", "tokens": ["Der", "soll\u00b7te", "die", "Kun\u00b7de", "mir", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "O Strom, du rauschest so wild vorbey", "tokens": ["O", "Strom", ",", "du", "rau\u00b7schest", "so", "wild", "vor\u00b7bey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und tr\u00e4gst vor Klippen und Sturm nicht Scheu;", "tokens": ["Und", "tr\u00e4gst", "vor", "Klip\u00b7pen", "und", "Sturm", "nicht", "Scheu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gern z\u00f6g' ich frey", "tokens": ["Gern", "z\u00f6g'", "ich", "frey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und muthig, wie du, durch das Leben!", "tokens": ["Und", "mut\u00b7hig", ",", "wie", "du", ",", "durch", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und aufw\u00e4rts schallt' es mit dumpfem Gebraus,", "tokens": ["Und", "auf\u00b7w\u00e4rts", "schallt'", "es", "mit", "dum\u00b7pfem", "Ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie die Wellen sich heben und senken:", "tokens": ["Wie", "die", "Wel\u00b7len", "sich", "he\u00b7ben", "und", "sen\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Weit roll' ich in's nebliche Meer hinaus;", "tokens": ["Weit", "roll'", "ich", "in's", "neb\u00b7li\u00b7che", "Meer", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wo das Schweigen wohnt in dem k\u00fchlen Haus,", "tokens": ["Wo", "das", "Schwei\u00b7gen", "wohnt", "in", "dem", "k\u00fch\u00b7len", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Soll nichts dich erfreun und dich kr\u00e4nken. \u2013", "tokens": ["Soll", "nichts", "dich", "er\u00b7freun", "und", "dich", "kr\u00e4n\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PIS", "PRF", "VVINF", "KON", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "O Strom, nicht lockst du mich niederw\u00e4rts,", "tokens": ["O", "Strom", ",", "nicht", "lockst", "du", "mich", "nie\u00b7der\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PTKNEG", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-++-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Denn hab' ich im Leben auch Noth und Schmerz,", "tokens": ["Denn", "hab'", "ich", "im", "Le\u00b7ben", "auch", "Noth", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Stets will mein Herz", "tokens": ["Stets", "will", "mein", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "An die minnige Freundin gedenken!", "tokens": ["An", "die", "min\u00b7ni\u00b7ge", "Freun\u00b7din", "ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und als sich in Nacht das Gebirge geh\u00fcllt,", "tokens": ["Und", "als", "sich", "in", "Nacht", "das", "Ge\u00b7bir\u00b7ge", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da tobte der Sturm in den Eichen,", "tokens": ["Da", "tob\u00b7te", "der", "Sturm", "in", "den", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und er schwang durch den Himmel sich rasch und wild,", "tokens": ["Und", "er", "schwang", "durch", "den", "Him\u00b7mel", "sich", "rasch", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und fl\u00fcchtig begann manch Wolkengebild,", "tokens": ["Und", "fl\u00fcch\u00b7tig", "be\u00b7gann", "manch", "Wol\u00b7ken\u00b7ge\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vor dem Monde vor\u00fcberzustreichen.", "tokens": ["Vor", "dem", "Mon\u00b7de", "vor\u00b7\u00fc\u00b7berz\u00b7u\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Und ich rief empor in die sausende Jagd:", "tokens": ["Und", "ich", "rief", "em\u00b7por", "in", "die", "sau\u00b7sen\u00b7de", "Jagd", ":"], "token_info": ["wo