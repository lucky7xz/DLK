{"dta.poem.20548": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "7.  \n  Des K\u00f6nigs Aufruf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "In Seiner Braven Mitte", "tokens": ["In", "Sei\u00b7ner", "Bra\u00b7ven", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df auf der V\u00e4ter Thron", "tokens": ["Sa\u00df", "auf", "der", "V\u00e4\u00b7ter", "Thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Friedrich Wilhelm der Dritte,", "tokens": ["Fried\u00b7rich", "Wil\u00b7helm", "der", "Drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der Herr der Zollernkron\u2019.", "tokens": ["Der", "Herr", "der", "Zol\u00b7lern\u00b7kron'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sprach: Die Kund ist kommen,", "tokens": ["Er", "sprach", ":", "Die", "Kund", "ist", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gott dr\u00e4ut dem Zw\u00e4nger: Halt!", "tokens": ["Gott", "dr\u00e4ut", "dem", "Zw\u00e4n\u00b7ger", ":", "Halt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur Krieg kann jetzo frommen,", "tokens": ["Nur", "Krieg", "kann", "jet\u00b7zo", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erhebt Euch, Jung und Alt!", "tokens": ["Er\u00b7hebt", "Euch", ",", "Jung", "und", "Alt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schnell vorw\u00e4rts war gedrungen", "tokens": ["Schnell", "vor\u00b7w\u00e4rts", "war", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des K\u00f6nigs Donnerwort.", "tokens": ["Des", "K\u00f6\u00b7nigs", "Don\u00b7ner\u00b7wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begeisternd war\u2019s erklungen", "tokens": ["Be\u00b7geis\u00b7ternd", "wa\u00b7r's", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VAFIN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hin bis zum kleinsten Ort;", "tokens": ["Hin", "bis", "zum", "kleins\u00b7ten", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verlassen ward die Schule,", "tokens": ["Ver\u00b7las\u00b7sen", "ward", "die", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Amt-, Kunst-, Kauf-, Herbergs-Tisch.", "tokens": ["Am\u00b7t", ",", "Kunst", ",", "Kauf", ",", "Her\u00b7bergs\u00b7Tisch", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Recken-Sinn in Thule", "tokens": ["Der", "Re\u00b7cken\u00b7Sinn", "in", "Thu\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "War fast so stark und frisch!", "tokens": ["War", "fast", "so", "stark", "und", "frisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da bl\u00e4st\u2019s: \u201eFrisch auf zum Jagen,", "tokens": ["Da", "bl\u00e4st's", ":", "\u201e", "Frisch", "auf", "zum", "Ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "$(", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ees ist schon an der Zeit,", "tokens": ["\u201e", "es", "ist", "schon", "an", "der", "Zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ees f\u00e4ngt schon an zu tagen,", "tokens": ["\u201e", "es", "f\u00e4ngt", "schon", "an", "zu", "ta\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eder Kampf ist nicht mehr weit.", "tokens": ["\u201e", "der", "Kampf", "ist", "nicht", "mehr", "weit", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eauf, la\u00dft die Faulen liegen,", "tokens": ["\u201e", "auf", ",", "la\u00dft", "die", "Fau\u00b7len", "lie\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201ela\u00dft sie in ihrer Ruh\u2019!", "tokens": ["\u201e", "la\u00dft", "sie", "in", "ih\u00b7rer", "Ruh'", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201ewir r\u00fccken mit Vergn\u00fcgen", "tokens": ["\u201e", "wir", "r\u00fc\u00b7cken", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201edem lieben K\u00f6nig zu.", "tokens": ["\u201e", "dem", "lie\u00b7ben", "K\u00f6\u00b7nig", "zu", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201edie Mehrsten zieh\u2019n einst wieder", "tokens": ["\u201e", "die", "Mehrs\u00b7ten", "zieh'n", "einst", "wie\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ezur\u00fcck in Siegesreih\u2019n.", "tokens": ["\u201e", "zu\u00b7r\u00fcck", "in", "Sie\u00b7ges\u00b7reih'", "n."], "token_info": ["punct", "word", "word", "word", "abbreviation"], "pos": ["$(", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edann t\u00f6nen Jubellieder,", "tokens": ["\u201e", "dann", "t\u00f6\u00b7nen", "Ju\u00b7bel\u00b7lie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edas wird \u2019ne Freude sein!", "tokens": ["\u201e", "das", "wird", "'ne", "Freu\u00b7de", "sein", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ewie gl\u00fch\u2019n davon die Herzen", "tokens": ["\u201e", "wie", "gl\u00fch'n", "da\u00b7von", "die", "Her\u00b7zen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADJD", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eso froh und stark und weich!", "tokens": ["\u201e", "so", "froh", "und", "stark", "und", "weich", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201ewer f\u00e4llt, der kann\u2019s verschmerzen,", "tokens": ["\u201e", "wer", "f\u00e4llt", ",", "der", "kann's", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201eder hat das Himmelreich!\u201c", "tokens": ["\u201e", "der", "hat", "das", "Him\u00b7mel\u00b7reich", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So klang\u2019s von Rott\u2019 zu Rotte,", "tokens": ["So", "klang's", "von", "Rott'", "zu", "Rot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es lief von Dorf zu Stadt", "tokens": ["Es", "lief", "von", "Dorf", "zu", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Lied, das de la Motte", "tokens": ["Das", "Lied", ",", "das", "de", "la", "Mot\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "FM", "FM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fouqu", "tokens": ["Fou\u00b7qu"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Es dringt durch Raum und Zeiten", "tokens": ["Es", "dringt", "durch", "Raum", "und", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Lied vom Sieg und Tod!", "tokens": ["Das", "Lied", "vom", "Sieg", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es that mit vorbereiten", "tokens": ["Es", "that", "mit", "vor\u00b7be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Freiheit Morgenroth!", "tokens": ["Der", "Frei\u00b7heit", "Mor\u00b7gen\u00b7roth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Kirchhof lag gebreitet,", "tokens": ["Ein", "Kirch\u00b7hof", "lag", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlo\u00df schon den Dichter ein.", "tokens": ["Schlo\u00df", "schon", "den", "Dich\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein H\u00fcgel ward bereitet,", "tokens": ["Ein", "H\u00fc\u00b7gel", "ward", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch wohl ein Leichenstein.", "tokens": ["Auch", "wohl", "ein", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn selbst auch brannten Kerzen,", "tokens": ["Wenn", "selbst", "auch", "brann\u00b7ten", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So gl\u00fcht doch heller fort", "tokens": ["So", "gl\u00fcht", "doch", "hel\u00b7ler", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In allen Deutschen Herzen", "tokens": ["In", "al\u00b7len", "Deut\u00b7schen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des S\u00e4ngers Heldenwort!", "tokens": ["Des", "S\u00e4n\u00b7gers", "Hel\u00b7den\u00b7wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}