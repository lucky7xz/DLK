{"dta.poem.23327": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Interessen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit jedem neuen Anlehn mehret", "tokens": ["Mit", "je\u00b7dem", "neu\u00b7en", "An\u00b7lehn", "meh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Staat nun seine Schulden zwar,", "tokens": ["Der", "Staat", "nun", "sei\u00b7ne", "Schul\u00b7den", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wird er immer mehr geehret", "tokens": ["Doch", "wird", "er", "im\u00b7mer", "mehr", "ge\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mehr geliebt von Jahr zu Jahr.", "tokens": ["Und", "mehr", "ge\u00b7liebt", "von", "Jahr", "zu", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das lassen wir uns gern gefallen \u2014", "tokens": ["Das", "las\u00b7sen", "wir", "uns", "gern", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Staat ist ja ein gutes Haus,", "tokens": ["Der", "Staat", "ist", "ja", "ein", "gu\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang' er immerfort uns allen", "tokens": ["So", "lang'", "er", "im\u00b7mer\u00b7fort", "uns", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "PPER", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zinsen zahlet richtig aus.", "tokens": ["Die", "Zin\u00b7sen", "zah\u00b7let", "rich\u00b7tig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch sollte sich dies Haus nicht halten", "tokens": ["Doch", "soll\u00b7te", "sich", "dies", "Haus", "nicht", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "PDS", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht es auch einmal Bankrott,", "tokens": ["Und", "macht", "es", "auch", "ein\u00b7mal", "Bank\u00b7rott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann wird die Liebe schnell erkalten,", "tokens": ["Dann", "wird", "die", "Lie\u00b7be", "schnell", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ade, o Staat, dir gnade Gott!", "tokens": ["A\u00b7de", ",", "o", "Staat", ",", "dir", "gna\u00b7de", "Gott", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Versprich das Weltall in Decreten,", "tokens": ["Ver\u00b7sprich", "das", "Wel\u00b7tall", "in", "De\u00b7cre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du f\u00e4ngst kein Unterthanenherz;", "tokens": ["Du", "f\u00e4ngst", "kein", "Un\u00b7ter\u00b7tha\u00b7nen\u00b7herz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast du nicht Taschen voll Moneten,", "tokens": ["Hast", "du", "nicht", "Ta\u00b7schen", "voll", "Mo\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So dreht dir jeder zu den Sterz.", "tokens": ["So", "dreht", "dir", "je\u00b7der", "zu", "den", "Sterz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}