{"textgrid.poem.43825": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schaut der Mond mit g\u00fcldenem Schein", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schaut der Mond mit g\u00fcldenem Schein", "tokens": ["Es", "schaut", "der", "Mond", "mit", "g\u00fcl\u00b7de\u00b7nem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So ernst in die stille Welt hinein.", "tokens": ["So", "ernst", "in", "die", "stil\u00b7le", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es ist so eigen ihm zu Sinn,", "tokens": ["Es", "ist", "so", "ei\u00b7gen", "ihm", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wandelt und plaudert so vor sich hin.", "tokens": ["Er", "wan\u00b7delt", "und", "plau\u00b7dert", "so", "vor", "sich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wo m\u00f6gen die lieben Sterne sein?", "tokens": ["Wo", "m\u00f6\u00b7gen", "die", "lie\u00b7ben", "Ster\u00b7ne", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich wandle nicht gerne so allein.", "tokens": ["Ich", "wand\u00b7le", "nicht", "ger\u00b7ne", "so", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir k\u00f6nnten uns heute frei ergeh'n,", "tokens": ["Wir", "k\u00f6nn\u00b7ten", "uns", "heu\u00b7te", "frei", "er\u00b7geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es l\u00e4\u00dft sich kein einziges W\u00f6lkchen seh'n.", "tokens": ["Es", "l\u00e4\u00dft", "sich", "kein", "ein\u00b7zi\u00b7ges", "W\u00f6lk\u00b7chen", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Allein zu sein ist Trauer und Pein", "tokens": ["Al\u00b7lein", "zu", "sein", "ist", "Trau\u00b7er", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VAINF", "VAFIN", "NN", "KON", "NN"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bei allem Glanz und g\u00fcldenem Schein;", "tokens": ["Bei", "al\u00b7lem", "Glanz", "und", "g\u00fcl\u00b7de\u00b7nem", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Allein zu sein ist allezeit", "tokens": ["Al\u00b7lein", "zu", "sein", "ist", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VAINF", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur halbe Freud' und doppelt Leid.", "tokens": ["Nur", "hal\u00b7be", "Freud'", "und", "dop\u00b7pelt", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das h\u00f6rt ein Stern wohl Wort f\u00fcr Wort,", "tokens": ["Das", "h\u00f6rt", "ein", "Stern", "wohl", "Wort", "f\u00fcr", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sagt es dem n\u00e4chsten Stern sofort;", "tokens": ["Er", "sagt", "es", "dem", "n\u00e4chs\u00b7ten", "Stern", "so\u00b7fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So sagt es ein Stern dem andern Stern,", "tokens": ["So", "sagt", "es", "ein", "Stern", "dem", "an\u00b7dern", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bald wissen es alle nah' und fern.", "tokens": ["Bald", "wis\u00b7sen", "es", "al\u00b7le", "nah'", "und", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie kommen wie zum Reigentanz,", "tokens": ["Sie", "kom\u00b7men", "wie", "zum", "Rei\u00b7gen\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kommen in ihrem silbernen Glanz,", "tokens": ["Sie", "kom\u00b7men", "in", "ih\u00b7rem", "sil\u00b7ber\u00b7nen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und alle, alle sind bereit,", "tokens": ["Und", "al\u00b7le", ",", "al\u00b7le", "sind", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Monde zu geben das Geleit.", "tokens": ["Dem", "Mon\u00b7de", "zu", "ge\u00b7ben", "das", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+------+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "\u00bbwillkommen, o Mond, in deiner Pracht!", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", ",", "o", "Mond", ",", "in", "dei\u00b7ner", "Pracht", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Willkommen in Winter- und Sommernacht!", "tokens": ["Will\u00b7kom\u00b7men", "in", "Win\u00b7ter", "und", "Som\u00b7mer\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Du sollst nie sein in der Welt allein:", "tokens": ["Du", "sollst", "nie", "sein", "in", "der", "Welt", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wir wollen dir treue Begleiter sein.\u00ab", "tokens": ["Wir", "wol\u00b7len", "dir", "treu\u00b7e", "Be\u00b7glei\u00b7ter", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Nun lassen sie jede Nacht sich seh'n,", "tokens": ["Nun", "las\u00b7sen", "sie", "je\u00b7de", "Nacht", "sich", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem lieben Monde spazieren zu geh'n;", "tokens": ["Mit", "dem", "lie\u00b7ben", "Mon\u00b7de", "spa\u00b7zie\u00b7ren", "zu", "geh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und wenn er nicht kommt, so warten sie sein,", "tokens": ["Und", "wenn", "er", "nicht", "kommt", ",", "so", "war\u00b7ten", "sie", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "VAINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie lassen ihn niemals wieder allein.", "tokens": ["Sie", "las\u00b7sen", "ihn", "nie\u00b7mals", "wie\u00b7der", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "O ging' es doch mir wie dem Monde so gut,", "tokens": ["O", "ging'", "es", "doch", "mir", "wie", "dem", "Mon\u00b7de", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PPER", "KOKOM", "ART", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wie w\u00e4r' ich fr\u00f6hlich und wohlgemuth!", "tokens": ["Wie", "w\u00e4r'", "ich", "fr\u00f6h\u00b7lich", "und", "wohl\u00b7ge\u00b7muth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann h\u00e4tt' ich immer in Freud' und Leid", "tokens": ["Dann", "h\u00e4tt'", "ich", "im\u00b7mer", "in", "Freud'", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein ganzes Leben ein freundlich Geleit!", "tokens": ["Mein", "gan\u00b7zes", "Le\u00b7ben", "ein", "freund\u00b7lich", "Ge\u00b7leit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}