{"textgrid.poem.63450": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Achter de Hock", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Min Sei\u00df un Strick legg ick", "tokens": ["Min", "Sei\u00df", "un", "Strick", "legg", "ick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "dor achter an't Sleet;", "tokens": ["dor", "ach\u00b7ter", "an't", "Sleet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "nu kumm man, kumm, segg ick,", "tokens": ["nu", "kumm", "man", ",", "kumm", ",", "segg", "ick", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "$,", "ADJD", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "hier's Oewerwind, Gret!", "tokens": ["hier's", "O\u00b7e\u00b7wer\u00b7wind", ",", "Gret", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NE", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "De Kiep lang eens, Dirn, her", "tokens": ["De", "Kiep", "lang", "e\u00b7ens", ",", "Dirn", ",", "her"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NE", "ADJD", "ADV", "$,", "NN", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "un't Lechel dorto", "tokens": ["un't", "Le\u00b7chel", "dor\u00b7to"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "NE", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "un huk di man hier her", "tokens": ["un", "huk", "di", "man", "hier", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "PIS", "ADV", "APZR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "uppe Garw, as ick do!", "tokens": ["up\u00b7pe", "Garw", ",", "as", "ick", "do", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Giff Speck, Wust un Schinken,", "tokens": ["Giff", "Speck", ",", "Wust", "un", "Schin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "FM", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "giff Pimpkes' un K\u00f6st!", "tokens": ["giff", "Pimp\u00b7kes'", "un", "K\u00f6st", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "FM", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lat \u00fcns \u00e4ten un drinken,", "tokens": ["Lat", "\u00fcns", "\u00e4\u00b7ten", "un", "drin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "giff her, wat du hest!", "tokens": ["giff", "her", ",", "wat", "du", "hest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Min Lif dat was hellig,", "tokens": ["Min", "Lif", "dat", "was", "hel\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PWS", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "nu is dat min Hart;", "tokens": ["nu", "is", "dat", "min", "Hart", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "ART", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "denn w\u00e4s' so gef\u00e4llig", "tokens": ["denn", "w\u00e4s'", "so", "ge\u00b7f\u00e4l\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "un giff em sin Part.", "tokens": ["un", "giff", "em", "sin", "Part", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Denn w\u00e4s' nich so drus, Dirn,", "tokens": ["Denn", "w\u00e4s'", "nich", "so", "drus", ",", "Dirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "PTKNEG", "ADV", "PAV", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "un nimm nicks nich kr\u00fcmm!", "tokens": ["un", "nimm", "nicks", "nich", "kr\u00fcmm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Un giff mi'n Pu\u00df, Dirn,", "tokens": ["Un", "giff", "mi'n", "Pu\u00df", ",", "Dirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "un fat mi eens \u00fcm.", "tokens": ["un", "fat", "mi", "e\u00b7ens", "\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hier h\u00fcrt \u00fcns keen Uhr nich,", "tokens": ["Hier", "h\u00fcrt", "\u00fcns", "ke\u00b7en", "Uhr", "nich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "keen Og s\u00fcht \u00fcns, Kind \u2013", "tokens": ["ke\u00b7en", "Og", "s\u00fcht", "\u00fcns", ",", "Kind", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "NE", "$,", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "de Hock von Natur nich,", "tokens": ["de", "Hock", "von", "Na\u00b7tur", "nich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "de's dof jo un blind.", "tokens": ["de's", "dof", "jo", "un", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Un s\u00fcht dat de L\u00fcnk ok,", "tokens": ["Un", "s\u00fcht", "dat", "de", "L\u00fcnk", "ok", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NE", "NE", "NE", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "de lacht sick dorbi", "tokens": ["de", "lacht", "sick", "dor\u00b7bi"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "un denkt sacht: Dat k\u00fcnn'ck ok", "tokens": ["un", "denkt", "sacht", ":", "Dat", "k\u00fcnn'ck", "ok"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "VVFIN", "ADJD", "$.", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "noch flinker as ji!", "tokens": ["noch", "flin\u00b7ker", "as", "ji", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}