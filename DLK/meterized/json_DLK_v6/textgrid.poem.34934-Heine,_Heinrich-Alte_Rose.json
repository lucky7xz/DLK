{"textgrid.poem.34934": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Alte Rose", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Rosenknospe war", "tokens": ["Ei\u00b7ne", "Ro\u00b7sen\u00b7knos\u00b7pe", "war"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie, f\u00fcr die mein Herze gl\u00fchte;", "tokens": ["Sie", ",", "f\u00fcr", "die", "mein", "Her\u00b7ze", "gl\u00fch\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Doch sie wuchs, und wunderbar", "tokens": ["Doch", "sie", "wuchs", ",", "und", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Scho\u00df sie auf in voller Bl\u00fcte.", "tokens": ["Scho\u00df", "sie", "auf", "in", "vol\u00b7ler", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ward die sch\u00f6nste Ros' im Land,", "tokens": ["Ward", "die", "sch\u00f6ns\u00b7te", "Ros'", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich wollt die Rose brechen,", "tokens": ["Und", "ich", "wollt", "die", "Ro\u00b7se", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie wu\u00dfte mich pikant", "tokens": ["Doch", "sie", "wu\u00df\u00b7te", "mich", "pi\u00b7kant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Dornen fortzustechen.", "tokens": ["Mit", "den", "Dor\u00b7nen", "fort\u00b7zu\u00b7ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt, wo sie verwelkt, zerfetzt", "tokens": ["Jetzt", ",", "wo", "sie", "ver\u00b7welkt", ",", "zer\u00b7fetzt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVPP", "$,", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und verklatscht von Wind und Regen \u2013", "tokens": ["Und", "ver\u00b7klatscht", "von", "Wind", "und", "Re\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbliebster Heinrich\u00ab bin ich jetzt,", "tokens": ["\u00bb", "liebs\u00b7ter", "Hein\u00b7rich", "\u00ab", "bin", "ich", "jetzt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NE", "$(", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebend kommt sie mir entgegen.", "tokens": ["Lie\u00b7bend", "kommt", "sie", "mir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heinrich hinten, Heinrich vorn,", "tokens": ["Hein\u00b7rich", "hin\u00b7ten", ",", "Hein\u00b7rich", "vorn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klingt es jetzt mit s\u00fc\u00dfen T\u00f6nen;", "tokens": ["Klingt", "es", "jetzt", "mit", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sticht mich jetzt etwa ein Dorn,", "tokens": ["Sticht", "mich", "jetzt", "et\u00b7wa", "ein", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es an dem Kinn der Sch\u00f6nen.", "tokens": ["Ist", "es", "an", "dem", "Kinn", "der", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Allzu hart die Borsten sind,", "tokens": ["All\u00b7zu", "hart", "die", "Bors\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die des Kinnes W\u00e4rzchen zieren \u2013", "tokens": ["Die", "des", "Kin\u00b7nes", "W\u00e4rz\u00b7chen", "zie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh ins Kloster, liebes Kind,", "tokens": ["Geh", "ins", "Klos\u00b7ter", ",", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder lasse dich rasieren.", "tokens": ["O\u00b7der", "las\u00b7se", "dich", "ra\u00b7sie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}