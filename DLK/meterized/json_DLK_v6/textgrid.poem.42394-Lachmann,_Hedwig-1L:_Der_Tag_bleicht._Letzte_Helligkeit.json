{"textgrid.poem.42394": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Tag bleicht. Letzte Helligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag bleicht. Letzte Helligkeit", "tokens": ["Der", "Tag", "bleicht", ".", "Letz\u00b7te", "Hel\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Quillt aus dem ebenm\u00e4ssigen Gew\u00f6lk.", "tokens": ["Quillt", "aus", "dem", "e\u00b7ben\u00b7m\u00e4s\u00b7si\u00b7gen", "Ge\u00b7w\u00f6lk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Erde trocken und befreit", "tokens": ["Die", "Er\u00b7de", "tro\u00b7cken", "und", "be\u00b7freit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Schnee; nur hie und da die Spur", "tokens": ["Von", "Schnee", ";", "nur", "hie", "und", "da", "die", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "ADV", "ADV", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von d\u00fcnnem Eise, wie Glasur.", "tokens": ["Von", "d\u00fcn\u00b7nem", "Ei\u00b7se", ",", "wie", "Gla\u00b7sur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Dunkelheit w\u00e4chst sanft und st\u00e4t;", "tokens": ["Die", "Dun\u00b7kel\u00b7heit", "w\u00e4chst", "sanft", "und", "st\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Licht, das aufblitzt, glimmt noch matt;", "tokens": ["Ein", "Licht", ",", "das", "auf\u00b7blitzt", ",", "glimmt", "noch", "matt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kinder spielen noch so sp\u00e4t,", "tokens": ["Die", "Kin\u00b7der", "spie\u00b7len", "noch", "so", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tagesfreuden nimmer satt.", "tokens": ["Der", "Ta\u00b7ges\u00b7freu\u00b7den", "nim\u00b7mer", "satt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Menschen schreiten s\u00e4umig, wie verf\u00fchrt;", "tokens": ["Die", "Men\u00b7schen", "schrei\u00b7ten", "s\u00e4u\u00b7mig", ",", "wie", "ver\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und atmend heben sie das Kinn", "tokens": ["Und", "at\u00b7mend", "he\u00b7ben", "sie", "das", "Kinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So an die Luft, als l\u00e4ge drin", "tokens": ["So", "an", "die", "Luft", ",", "als", "l\u00e4\u00b7ge", "drin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr sie ein Etwas, das den Sinn", "tokens": ["F\u00fcr", "sie", "ein", "Et\u00b7was", ",", "das", "den", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADV", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie eine wahre Seligkeit ber\u00fchrt.", "tokens": ["Wie", "ei\u00b7ne", "wah\u00b7re", "Se\u00b7lig\u00b7keit", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}