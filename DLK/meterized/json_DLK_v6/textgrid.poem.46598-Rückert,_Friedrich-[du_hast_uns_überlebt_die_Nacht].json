{"textgrid.poem.46598": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du hast uns \u00fcberlebt die Nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast uns \u00fcberlebt die Nacht,", "tokens": ["Du", "hast", "uns", "\u00fc\u00b7ber\u00b7lebt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wiewohl in Todesschmerzen;", "tokens": ["Wie\u00b7wohl", "in", "To\u00b7des\u00b7schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und auch daf\u00fcr sei dargebracht", "tokens": ["Und", "auch", "da\u00b7f\u00fcr", "sei", "dar\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "VAFIN", "VVPP"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ein Dank von unsern Herzen;", "tokens": ["Ein", "Dank", "von", "un\u00b7sern", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn uns auch nur noch einen Tag", "tokens": ["Wenn", "uns", "auch", "nur", "noch", "ei\u00b7nen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein s\u00fc\u00dfes Leben bliebe;", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "blie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So lang als m\u00f6glich halten mag", "tokens": ["So", "lang", "als", "m\u00f6g\u00b7lich", "hal\u00b7ten", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, was sie liebt, die Liebe.", "tokens": ["Ja", ",", "was", "sie", "liebt", ",", "die", "Lie\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf lange fest ja halten mag", "tokens": ["Auf", "lan\u00b7ge", "fest", "ja", "hal\u00b7ten", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJD", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts, was sie liebt, die Liebe;", "tokens": ["Nichts", ",", "was", "sie", "liebt", ",", "die", "Lie\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Genug, da\u00df nur von Tag zu Tag", "tokens": ["Ge\u00b7nug", ",", "da\u00df", "nur", "von", "Tag", "zu", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich der Verlust verschiebe.", "tokens": ["Sich", "der", "Ver\u00b7lust", "ver\u00b7schie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}