{"textgrid.poem.40774": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcnfzig Jahre sind's, da riefen unsre Eltern zu den Waffen:", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnfzig Jahre sind's, da riefen unsre Eltern zu den Waffen:", "tokens": ["F\u00fcnf\u00b7zig", "Jah\u00b7re", "sin\u00b7d's", ",", "da", "rie\u00b7fen", "uns\u00b7re", "El\u00b7tern", "zu", "den", "Waf\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Krieg und Kampf den dicken, plumpen, kugelrunden, feisten Pfaffen!", "tokens": ["Krieg", "und", "Kampf", "den", "di\u00b7cken", ",", "plum\u00b7pen", ",", "ku\u00b7gel\u00b7run\u00b7den", ",", "feis\u00b7ten", "Pfaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Auch in Waffen steh'n wir Enkel; jetzt doch mu\u00df die Losung sein:", "tokens": ["Auch", "in", "Waf\u00b7fen", "steh'n", "wir", "En\u00b7kel", ";", "jetzt", "doch", "mu\u00df", "die", "Lo\u00b7sung", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "NN", "$.", "ADV", "ADV", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Krieg und Kampf den d\u00fcnnen, magern, spindelhagern Pf\u00e4ffelein!", "tokens": ["Krieg", "und", "Kampf", "den", "d\u00fcn\u00b7nen", ",", "ma\u00b7gern", ",", "spin\u00b7del\u00b7ha\u00b7gern", "Pf\u00e4f\u00b7fe\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "$,", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Aber wo gab's gr\u00f6\u00dfre Arbeit, welcher Kampf bot mehr Gefahren?", "tokens": ["A\u00b7ber", "wo", "gab's", "gr\u00f6\u00df\u00b7re", "Ar\u00b7beit", ",", "wel\u00b7cher", "Kampf", "bot", "mehr", "Ge\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "ADJA", "NN", "$,", "PWAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wo galt's fester auszudauern, wo galt's kl\u00fcger sich zu wahren?", "tokens": ["Wo", "galt's", "fes\u00b7ter", "aus\u00b7zu\u00b7dau\u00b7ern", ",", "wo", "galt's", "kl\u00fc\u00b7ger", "sich", "zu", "wah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJD", "VVINF", "$,", "PWAV", "NE", "ADJD", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Lauthin schnaubt die plumpe Wildsau, wenn sie durch das Dickicht keucht,", "tokens": ["Laut\u00b7hin", "schnaubt", "die", "plum\u00b7pe", "Wild\u00b7sau", ",", "wenn", "sie", "durch", "das", "Di\u00b7ckicht", "keucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber leise kriecht die Viper, die nach deinen Fersen schleicht!", "tokens": ["A\u00b7ber", "lei\u00b7se", "kriecht", "die", "Vi\u00b7per", ",", "die", "nach", "dei\u00b7nen", "Fer\u00b7sen", "schleicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Einst verschnarchten dicke Pfaffen ganze Tag' in s\u00fc\u00dfem Schl\u00e4flein,", "tokens": ["Einst", "ver\u00b7schnarch\u00b7ten", "di\u00b7cke", "Pfaf\u00b7fen", "gan\u00b7ze", "Tag'", "in", "s\u00fc\u00b7\u00dfem", "Schl\u00e4f\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jetzt doch liegen auf der Lauer immer wach die d\u00fcnnen Pf\u00e4fflein;", "tokens": ["Jetzt", "doch", "lie\u00b7gen", "auf", "der", "Lau\u00b7er", "im\u00b7mer", "wach", "die", "d\u00fcn\u00b7nen", "Pf\u00e4f\u00b7flein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jene br\u00fcllten ihre Inbrunst heulend in die Welt hinein,", "tokens": ["Je\u00b7ne", "br\u00fcll\u00b7ten", "ih\u00b7re", "In\u00b7brunst", "heu\u00b7lend", "in", "die", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Diese winseln ihren Jammer, Katern gleich im M\u00e4rz, so fein.", "tokens": ["Die\u00b7se", "win\u00b7seln", "ih\u00b7ren", "Jam\u00b7mer", ",", "Ka\u00b7tern", "gleich", "im", "M\u00e4rz", ",", "so", "fein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "NN", "ADV", "APPRART", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "M\u00e4cht'gen, schweren Folianten glichen einstens jene Dicken,", "tokens": ["M\u00e4cht'\u00b7gen", ",", "schwe\u00b7ren", "Fo\u00b7li\u00b7an\u00b7ten", "gli\u00b7chen", "eins\u00b7tens", "je\u00b7ne", "Di\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bballgemeines gro\u00dfes Kochbuch\u00ab stand als Inschrift auf dem R\u00fccken;", "tokens": ["\u00bb", "all\u00b7ge\u00b7mei\u00b7nes", "gro\u00b7\u00dfes", "Koch\u00b7buch", "\u00ab", "stand", "als", "In\u00b7schrift", "auf", "dem", "R\u00fc\u00b7cken", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "ADJA", "NN", "$(", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Einem schmalen kleinen B\u00fcchlein sind die D\u00fcnnen gleich, f\u00fcrwahr,", "tokens": ["Ei\u00b7nem", "schma\u00b7len", "klei\u00b7nen", "B\u00fcch\u00b7lein", "sind", "die", "D\u00fcn\u00b7nen", "gleich", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "$,", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "\u00bbkurzgefa\u00dfte Gaunerst\u00fccklein\u00ab beut das Titelblatt euch dar.", "tokens": ["\u00bb", "kurz\u00b7ge\u00b7fa\u00df\u00b7te", "Gau\u00b7ner\u00b7st\u00fcck\u00b7lein", "\u00ab", "beut", "das", "Ti\u00b7tel\u00b7blatt", "euch", "dar", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$(", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Mit der Grobheit und der Dummheit hattet einst den Kampf, ihr Alten,", "tokens": ["Mit", "der", "Grob\u00b7heit", "und", "der", "Dumm\u00b7heit", "hat\u00b7tet", "einst", "den", "Kampf", ",", "ihr", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch der Artigkeit und Schlauheit m\u00fcssen wir die Stange halten!", "tokens": ["Doch", "der", "Ar\u00b7tig\u00b7keit", "und", "Schlau\u00b7heit", "m\u00fcs\u00b7sen", "wir", "die", "Stan\u00b7ge", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Einstens rannten euch die Dicken mit dem Wanst die Th\u00fcren ein,", "tokens": ["Eins\u00b7tens", "rann\u00b7ten", "euch", "die", "Di\u00b7cken", "mit", "dem", "Wanst", "die", "Th\u00fc\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch es kriechen jetzt die D\u00fcnnen uns durchs Schl\u00fcsselloch herein.", "tokens": ["Doch", "es", "krie\u00b7chen", "jetzt", "die", "D\u00fcn\u00b7nen", "uns", "durchs", "Schl\u00fcs\u00b7sel\u00b7loch", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "L\u00e4ngst schon hat ein tapfrer Ritter k\u00fchn der Dicken Heer geb\u00e4ndigt,", "tokens": ["L\u00e4ngst", "schon", "hat", "ein", "tapf\u00b7rer", "Rit\u00b7ter", "k\u00fchn", "der", "Di\u00b7cken", "Heer", "ge\u00b7b\u00e4n\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und als goldner Stern des Tages jene finstre Nacht geendigt,", "tokens": ["Und", "als", "gold\u00b7ner", "Stern", "des", "Ta\u00b7ges", "je\u00b7ne", "finst\u00b7re", "Nacht", "ge\u00b7en\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJA", "NN", "ART", "NN", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Joseph hie\u00df der Stern und Ritter! Wien, du kannst sein Denkmal seh'n", "tokens": ["Jo\u00b7se\u00b7ph", "hie\u00df", "der", "Stern", "und", "Rit\u00b7ter", "!", "Wi\u00b7en", ",", "du", "kannst", "sein", "Denk\u00b7mal", "seh'n"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "KON", "NN", "$.", "NE", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "+----+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ach und will denn gen die D\u00fcnnen nimmer solch ein Held ersteh'n?", "tokens": ["Ach", "und", "will", "denn", "gen", "die", "D\u00fcn\u00b7nen", "nim\u00b7mer", "solch", "ein", "Held", "er\u00b7steh'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "VMFIN", "ADV", "APPR", "ART", "NN", "ADV", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "O so steigt ihr Dicken wieder lebend aus der Todesurne!", "tokens": ["O", "so", "steigt", "ihr", "Di\u00b7cken", "wie\u00b7der", "le\u00b7bend", "aus", "der", "To\u00b7de\u00b7sur\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch mit altem gutem Magen! Werdet christliche Saturne!", "tokens": ["Doch", "mit", "al\u00b7tem", "gu\u00b7tem", "Ma\u00b7gen", "!", "Wer\u00b7det", "christ\u00b7li\u00b7che", "Sa\u00b7tur\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "$.", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und verschlingt den magern Nachwuchs, o dann sind wir beider los,", "tokens": ["Und", "ver\u00b7schlingt", "den", "ma\u00b7gern", "Nach\u00b7wuchs", ",", "o", "dann", "sind", "wir", "bei\u00b7der", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "FM", "ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Denn nicht lange mehr kann leben, wer solch' gift'ge Kost geno\u00df!", "tokens": ["Denn", "nicht", "lan\u00b7ge", "mehr", "kann", "le\u00b7ben", ",", "wer", "solch'", "gift'\u00b7ge", "Kost", "ge\u00b7no\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "VMFIN", "VVINF", "$,", "PWS", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}