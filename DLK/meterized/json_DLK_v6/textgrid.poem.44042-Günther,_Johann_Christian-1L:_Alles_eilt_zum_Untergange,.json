{"textgrid.poem.44042": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles eilt zum Untergange,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles eilt zum Untergange,", "tokens": ["Al\u00b7les", "eilt", "zum", "Un\u00b7ter\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur mein hart Verh\u00e4ngn\u00fc\u00df nicht.", "tokens": ["Nur", "mein", "hart", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJD", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Harter Himmel, ach wie lange", "tokens": ["Har\u00b7ter", "Him\u00b7mel", ",", "ach", "wie", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "KOKOM", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigstu sein erschr\u00f6cklich Licht?", "tokens": ["Zeigs\u00b7tu", "sein", "er\u00b7schr\u00f6ck\u00b7lich", "Licht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll er mir jezund erscheinen,", "tokens": ["Soll", "er", "mir", "je\u00b7zund", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O so gieb ihm bald sein Amt,", "tokens": ["O", "so", "gieb", "ihm", "bald", "sein", "Amt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eh mich ein verzweiflend Weinen", "tokens": ["Eh", "mich", "ein", "ver\u00b7zwei\u00b7flend", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch zu gr\u00f6\u00dfrer Noth verdammt.", "tokens": ["Noch", "zu", "gr\u00f6\u00df\u00b7rer", "Noth", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich, ein Mensch von schlechtem Zeuge,", "tokens": ["Ich", ",", "ein", "Mensch", "von", "schlech\u00b7tem", "Zeu\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kan mir selbst nicht widerstehn,", "tokens": ["Kan", "mir", "selbst", "nicht", "wi\u00b7der\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich kaum gela\u00dfen schweige,", "tokens": ["Da\u00df", "ich", "kaum", "ge\u00b7la\u00b7\u00dfen", "schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Wellen h\u00f6her gehn.", "tokens": ["Wenn", "die", "Wel\u00b7len", "h\u00f6\u00b7her", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fleisch und Blut beh\u00e4lt im Schmerzen", "tokens": ["Fleisch", "und", "Blut", "be\u00b7h\u00e4lt", "im", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber die Vernunft das Feld,", "tokens": ["\u00dc\u00b7ber", "die", "Ver\u00b7nunft", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Hofnung steckt im Herzen,", "tokens": ["Und", "die", "Hof\u00b7nung", "steckt", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welches keinen Mensch erh\u00e4lt.", "tokens": ["Wel\u00b7ches", "kei\u00b7nen", "Mensch", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt ich Bo\u00dfheit im Gem\u00fcthe", "tokens": ["H\u00e4tt", "ich", "Bo\u00df\u00b7heit", "im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder an den Lastern Lust,", "tokens": ["O\u00b7der", "an", "den", "Las\u00b7tern", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So verzieh ich mich der G\u00fcte", "tokens": ["So", "ver\u00b7zieh", "ich", "mich", "der", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner treuen Vaterbrust.", "tokens": ["Dei\u00b7ner", "treu\u00b7en", "Va\u00b7ter\u00b7brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ach, so wirstu finden,", "tokens": ["A\u00b7ber", "ach", ",", "so", "wirs\u00b7tu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pr\u00fcfe Mienen, Herz und Sinn,", "tokens": ["Pr\u00fc\u00b7fe", "Mie\u00b7nen", ",", "Herz", "und", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich bei den Schwachheitss\u00fcnden", "tokens": ["Da\u00df", "ich", "bei", "den", "Schwach\u00b7heits\u00b7s\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch nicht sonder Bu\u00dfe bin.", "tokens": ["Doch", "nicht", "son\u00b7der", "Bu\u00b7\u00dfe", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar sind, die noch \u00e4rger leben", "tokens": ["Zwar", "sind", ",", "die", "noch", "\u00e4r\u00b7ger", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Lastern Schaden thun,", "tokens": ["Und", "mit", "Las\u00b7tern", "Scha\u00b7den", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die l\u00e4st du im Gl\u00fccke schweben", "tokens": ["Die", "l\u00e4st", "du", "im", "Gl\u00fc\u00b7cke", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und in seinem Schoo\u00dfe ruhn;", "tokens": ["Und", "in", "sei\u00b7nem", "Schoo\u00b7\u00dfe", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie verschwenden deinen Seegen", "tokens": ["Sie", "ver\u00b7schwen\u00b7den", "dei\u00b7nen", "See\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur zu Troz auf meinen Fall,", "tokens": ["Nur", "zu", "Troz", "auf", "mei\u00b7nen", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Handeln, wie die Thoren pflegen,", "tokens": ["Han\u00b7deln", ",", "wie", "die", "Tho\u00b7ren", "pfle\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch ger\u00e4th es \u00fcberall.", "tokens": ["Doch", "ge\u00b7r\u00e4\u00b7th", "es", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich bezwinge mich hingegen,", "tokens": ["Ich", "be\u00b7zwin\u00b7ge", "mich", "hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brauche des Verstandes Kraft", "tokens": ["Brau\u00b7che", "des", "Ver\u00b7stan\u00b7des", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bem\u00fch mich nach Verm\u00f6gen", "tokens": ["Und", "be\u00b7m\u00fch", "mich", "nach", "Ver\u00b7m\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um Gedult und Wi\u00dfenschaft;", "tokens": ["Um", "Ge\u00b7dult", "und", "Wi\u00b7\u00dfen\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleichwohl ist in allen Sachen", "tokens": ["Gleich\u00b7wohl", "ist", "in", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch mein bester Rathschlu\u00df blind,", "tokens": ["Auch", "mein", "bes\u00b7ter", "Rath\u00b7schlu\u00df", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sich's die zu Nuze machen,", "tokens": ["Da\u00df", "sich's", "die", "zu", "Nu\u00b7ze", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die mir feind und sch\u00e4dlich sind.", "tokens": ["Die", "mir", "feind", "und", "sch\u00e4d\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}