{"textgrid.poem.54359": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies ist der sch\u00f6ne Tag, da Seladon gebohren;", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies ist der sch\u00f6ne Tag, da Seladon gebohren;", "tokens": ["Dies", "ist", "der", "sch\u00f6\u00b7ne", "Tag", ",", "da", "Se\u00b7la\u00b7don", "ge\u00b7boh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tag, der zu meiner Lust vor andern auserkohren;", "tokens": ["Tag", ",", "der", "zu", "mei\u00b7ner", "Lust", "vor", "an\u00b7dern", "au\u00b7ser\u00b7koh\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "APPR", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tag, der mir in der Welt der allerliebste bleibt;", "tokens": ["Tag", ",", "der", "mir", "in", "der", "Welt", "der", "al\u00b7ler\u00b7liebs\u00b7te", "bleibt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Tag, den die Liebe selbst in mein Ged\u00e4chtnis schreibt;", "tokens": ["Tag", ",", "den", "die", "Lie\u00b7be", "selbst", "in", "mein", "Ge\u00b7d\u00e4cht\u00b7nis", "schreibt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Tag, den ich nimmermehr kann hoch genug verehren;", "tokens": ["Tag", ",", "den", "ich", "nim\u00b7mer\u00b7mehr", "kann", "hoch", "ge\u00b7nug", "ver\u00b7eh\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "ADJD", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Tag, der die reine Gluth kann in der Brust vermehren;", "tokens": ["Tag", ",", "der", "die", "rei\u00b7ne", "Gluth", "kann", "in", "der", "Brust", "ver\u00b7meh\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Tag, der den Geist besiegt, das Herz empfindlich macht;", "tokens": ["Tag", ",", "der", "den", "Geist", "be\u00b7siegt", ",", "das", "Herz", "emp\u00b7find\u00b7lich", "macht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Tag, der die Seele letzt, an dem mein Gl\u00fccke lacht.", "tokens": ["Tag", ",", "der", "die", "See\u00b7le", "letzt", ",", "an", "dem", "mein", "Gl\u00fc\u00b7cke", "lacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADV", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Du bleibst mein einig All, mein Paradies auf Erden!", "tokens": ["Du", "bleibst", "mein", "ei\u00b7nig", "All", ",", "mein", "Pa\u00b7ra\u00b7dies", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und sollst deswegen auch mit Lust besungen werden.", "tokens": ["Und", "sollst", "des\u00b7we\u00b7gen", "auch", "mit", "Lust", "be\u00b7sun\u00b7gen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "ADV", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auf Sch\u00e4fer! kommt herbey, helft diesen Tag begehn!", "tokens": ["Auf", "Sch\u00e4\u00b7fer", "!", "kommt", "her\u00b7bey", ",", "helft", "die\u00b7sen", "Tag", "be\u00b7gehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "VVFIN", "PTKVZ", "$,", "VVFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00dft Mopsen unterde\u00df auf Huth und Wache stehn;", "tokens": ["La\u00dft", "Mop\u00b7sen", "un\u00b7ter\u00b7de\u00df", "auf", "Huth", "und", "Wa\u00b7che", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Legt euren Sch\u00e4ferschurz um die so schlanken Lenden;", "tokens": ["Legt", "eu\u00b7ren", "Sch\u00e4\u00b7fer\u00b7schurz", "um", "die", "so", "schlan\u00b7ken", "Len\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich will euch Huth und Strau\u00df und allen Ausputz senden.", "tokens": ["Ich", "will", "euch", "Huth", "und", "Strau\u00df", "und", "al\u00b7len", "Aus\u00b7putz", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "KON", "NE", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Kommt, weil der Morgenthau noch auf den Matten ist,", "tokens": ["Kommt", ",", "weil", "der", "Mor\u00b7gent\u00b7hau", "noch", "auf", "den", "Mat\u00b7ten", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil sich das Fl\u00fcgelheer noch in den Nestern k\u00fc\u00dft.", "tokens": ["Weil", "sich", "das", "Fl\u00fc\u00b7gel\u00b7heer", "noch", "in", "den", "Nes\u00b7tern", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Pfl\u00fcckt Blum und Kr\u00e4uter ab, brecht Bl\u00e4tter von den Zweigen,", "tokens": ["Pfl\u00fcckt", "Blum", "und", "Kr\u00e4u\u00b7ter", "ab", ",", "brecht", "Bl\u00e4t\u00b7ter", "von", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "PTKVZ", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denkt nur auf Scherz und Lust; kein einiges soll schweigen.", "tokens": ["Denkt", "nur", "auf", "Scherz", "und", "Lust", ";", "kein", "ei\u00b7ni\u00b7ges", "soll", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$.", "PIAT", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die St\u00f6cke m\u00fcssen frisch und wohl beb\u00e4ndert seyn,", "tokens": ["Die", "St\u00f6\u00b7cke", "m\u00fcs\u00b7sen", "frisch", "und", "wohl", "be\u00b7b\u00e4n\u00b7dert", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Becher ausgesp\u00fchlt. Nun schenk sich jeder ein,", "tokens": ["Die", "Be\u00b7cher", "aus\u00b7ge\u00b7sp\u00fchlt", ".", "Nun", "schenk", "sich", "je\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "PRF", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Weil ich den sch\u00f6nsten Kranz mit eignen H\u00e4nden winde,", "tokens": ["Weil", "ich", "den", "sch\u00f6ns\u00b7ten", "Kranz", "mit", "eig\u00b7nen", "H\u00e4n\u00b7den", "win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vor meinen Seladon, zu seinem Angebinde.", "tokens": ["Vor", "mei\u00b7nen", "Se\u00b7la\u00b7don", ",", "zu", "sei\u00b7nem", "An\u00b7ge\u00b7bin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So sprach j\u00fcngst Sylvia: bald kam der ganze Chor,", "tokens": ["So", "sprach", "j\u00fcngst", "Syl\u00b7via", ":", "bald", "kam", "der", "gan\u00b7ze", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Und jeder pfiff ein Lied auf seinem Haberrohr;", "tokens": ["Und", "je\u00b7der", "pfiff", "ein", "Lied", "auf", "sei\u00b7nem", "Ha\u00b7berr\u00b7ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da nahten sich zugleich die holden Sch\u00e4ferinnen,", "tokens": ["Da", "nah\u00b7ten", "sich", "zu\u00b7gleich", "die", "hol\u00b7den", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es suchte jegliche was neues auszusinnen,", "tokens": ["Es", "such\u00b7te", "jeg\u00b7li\u00b7che", "was", "neu\u00b7es", "aus\u00b7zu\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PWS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Damit die Lust vermehrt, das Fest vollkommner ward.", "tokens": ["Da\u00b7mit", "die", "Lust", "ver\u00b7mehrt", ",", "das", "Fest", "voll\u00b7komm\u00b7ner", "ward", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nur fehlte Seladon mit seiner Gegenwart.", "tokens": ["Nur", "fehl\u00b7te", "Se\u00b7la\u00b7don", "mit", "sei\u00b7ner", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kaum, da\u00df die Sch\u00e4ferzunft die Cythern anger\u00fchret,", "tokens": ["Kaum", ",", "da\u00df", "die", "Sch\u00e4\u00b7fer\u00b7zunft", "die", "Cy\u00b7thern", "an\u00b7ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So ward schon ein Ger\u00e4usch im n\u00e4chsten Busch versp\u00fchret;", "tokens": ["So", "ward", "schon", "ein", "Ge\u00b7r\u00e4usch", "im", "n\u00e4chs\u00b7ten", "Busch", "ver\u00b7sp\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die muntre Nachtigall fiel in den Ton mit ein,", "tokens": ["Die", "mun\u00b7tre", "Nach\u00b7ti\u00b7gall", "fiel", "in", "den", "Ton", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Die wilde Wachtel kam, und wollte Zeuge seyn.", "tokens": ["Die", "wil\u00b7de", "Wach\u00b7tel", "kam", ",", "und", "woll\u00b7te", "Zeu\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Kukuck lies sich auch mit seiner Stimme h\u00f6ren;", "tokens": ["Der", "Ku\u00b7kuck", "lies", "sich", "auch", "mit", "sei\u00b7ner", "Stim\u00b7me", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er lie\u00df sich weder Tanz, Gesang, noch Leyer st\u00f6hren;", "tokens": ["Er", "lie\u00df", "sich", "we\u00b7der", "Tanz", ",", "Ge\u00b7sang", ",", "noch", "Le\u00b7yer", "st\u00f6h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "NN", "$,", "NN", "$,", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Schwalbe zwitscherte; der Bienen schw\u00e4rmend Heer", "tokens": ["Die", "Schwal\u00b7be", "zwit\u00b7scher\u00b7te", ";", "der", "Bie\u00b7nen", "schw\u00e4r\u00b7mend", "Heer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Verliessen ihr Gefach, und flogen hin und her;", "tokens": ["Ver\u00b7lies\u00b7sen", "ihr", "Ge\u00b7fach", ",", "und", "flo\u00b7gen", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Taube sehnte sich nach ihrem frommen Gatten,", "tokens": ["Die", "Tau\u00b7be", "sehn\u00b7te", "sich", "nach", "ih\u00b7rem", "from\u00b7men", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das M\u00e4nnchen kam zu ihr, sie setzten sich im Schatten;", "tokens": ["Das", "M\u00e4nn\u00b7chen", "kam", "zu", "ihr", ",", "sie", "setz\u00b7ten", "sich", "im", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Schaafe z\u00e4ckten sich; dort sprangen Zieg und Bock", "tokens": ["Die", "Schaa\u00b7fe", "z\u00e4ck\u00b7ten", "sich", ";", "dort", "spran\u00b7gen", "Zieg", "und", "Bock"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "ADV", "VVFIN", "NN", "KON", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Den H\u00fcgel auf und ab, und \u00fcber Berg und Stock;", "tokens": ["Den", "H\u00fc\u00b7gel", "auf", "und", "ab", ",", "und", "\u00fc\u00b7ber", "Berg", "und", "Stock", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "wor