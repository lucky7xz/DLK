{"textgrid.poem.52022": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist kein Wunder nicht, da\u00df faule Motten kommen,", "tokens": ["Es", "ist", "kein", "Wun\u00b7der", "nicht", ",", "da\u00df", "fau\u00b7le", "Mot\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und richten dann ein Mahl aus euern Versen zu:", "tokens": ["Und", "rich\u00b7ten", "dann", "ein", "Mahl", "aus", "eu\u00b7ern", "Ver\u00b7sen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr last des Lesers Ohr doch gar zu sehr zur Ruh,", "tokens": ["Ihr", "last", "des", "Le\u00b7sers", "Ohr", "doch", "gar", "zu", "sehr", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADV", "ADV", "PTKA", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Sinn verliert das Buch, eh' es die Hand genommen.", "tokens": ["Der", "Sinn", "ver\u00b7liert", "das", "Buch", ",", "eh'", "es", "die", "Hand", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als lang am Himmel sich die goldnen Sph\u00e4ren waltzen,", "tokens": ["Als", "lang", "am", "Him\u00b7mel", "sich", "die", "gold\u00b7nen", "Sph\u00e4\u00b7ren", "walt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lange wird ein Blat durch kluge H\u00e4nde gehn,", "tokens": ["So", "lan\u00b7ge", "wird", "ein", "Blat", "durch", "klu\u00b7ge", "H\u00e4n\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff welchem so viel Wort als s\u00fcsse Stacheln stehn,", "tokens": ["Auff", "wel\u00b7chem", "so", "viel", "Wort", "als", "s\u00fcs\u00b7se", "Sta\u00b7cheln", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PIAT", "NN", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fragt ihr, an was es fehlt? Ihr m\u00fcst die Verse saltzen.", "tokens": ["Fragt", "ihr", ",", "an", "was", "es", "fehlt", "?", "Ihr", "m\u00fcst", "die", "Ver\u00b7se", "salt\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}