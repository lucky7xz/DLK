{"dta.poem.4043": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Emilie an Klara .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Himmlischen! das war er. Kannst du mir", "tokens": ["Ihr", "Himm\u00b7li\u00b7schen", "!", "das", "war", "er", ".", "Kannst", "du", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PDS", "VAFIN", "PPER", "$.", "VMFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es glauben? \u2012 Beste! \u2012 w\u00e4rst du bei mir! \u2012 Er!", "tokens": ["Es", "glau\u00b7ben", "?", "\u2012", "Bes\u00b7te", "!", "\u2012", "w\u00e4rst", "du", "bei", "mir", "!", "\u2012", "Er", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "$(", "NN", "$.", "$(", "VAFIN", "PPER", "APPR", "PPER", "$.", "$(", "PPER", "$."], "meter": "-+--+-+---+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Hohe, der Gef\u00fcrchtete, Geliebte! \u2012", "tokens": ["Der", "Ho\u00b7he", ",", "der", "Ge\u00b7f\u00fcrch\u00b7te\u00b7te", ",", "Ge\u00b7lieb\u00b7te", "!", "\u2012"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein bebend Herz, hast du so viel gewollt?", "tokens": ["Mein", "be\u00b7bend", "Herz", ",", "hast", "du", "so", "viel", "ge\u00b7wollt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "VMPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Da gieng ich so zur\u00fcck mit meinen Blumen,", "tokens": ["Da", "gieng", "ich", "so", "zu\u00b7r\u00fcck", "mit", "mei\u00b7nen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah auf den Pfad, den abendr\u00f6thlichen,", "tokens": ["Sah", "auf", "den", "Pfad", ",", "den", "a\u00b7bend\u00b7r\u00f6th\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In meiner Stille nieder, und es schlief", "tokens": ["In", "mei\u00b7ner", "Stil\u00b7le", "nie\u00b7der", ",", "und", "es", "schlief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir sanft im Busen das Vergangene,", "tokens": ["Mir", "sanft", "im", "Bu\u00b7sen", "das", "Ver\u00b7gan\u00b7ge\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Ein kindlich Hoffen athmete mir auf;", "tokens": ["Ein", "kind\u00b7lich", "Hof\u00b7fen", "ath\u00b7me\u00b7te", "mir", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie wenn uns zwischen s\u00fc\u00dfem Schlaf und Wachen", "tokens": ["Wie", "wenn", "uns", "zwi\u00b7schen", "s\u00fc\u00b7\u00dfem", "Schlaf", "und", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Augen halb ge\u00f6ffnet sind, so war", "tokens": ["Die", "Au\u00b7gen", "halb", "ge\u00b7\u00f6ff\u00b7net", "sind", ",", "so", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "VAFIN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich Blinde. Sieh! da stand er vor mir mein", "tokens": ["Ich", "Blin\u00b7de", ".", "Sieh", "!", "da", "stand", "er", "vor", "mir", "mein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$.", "NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Heroe und ich Arme war, wie todt,", "tokens": ["He\u00b7roe", "und", "ich", "Ar\u00b7me", "war", ",", "wie", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "NN", "VAFIN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und ihm, dem Br\u00fcderlichen, \u00fcbergl\u00e4nzte", "tokens": ["Und", "ihm", ",", "dem", "Br\u00fc\u00b7der\u00b7li\u00b7chen", ",", "\u00fc\u00b7ber\u00b7gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das Angesicht, wie einem Gott, die Freude.", "tokens": ["Das", "An\u00b7ge\u00b7sicht", ",", "wie", "ei\u00b7nem", "Gott", ",", "die", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u201eemilie!\u201c \u2014 das war sein frommer Gru\u00df,", "tokens": ["\u201e", "e\u00b7mi\u00b7lie", "!", "\u201c", "das", "war", "sein", "from\u00b7mer", "Gru\u00df", ","], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$.", "$(", "$(", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ach! alles Sehnen weckte mir und all", "tokens": ["Ach", "!", "al\u00b7les", "Seh\u00b7nen", "weck\u00b7te", "mir", "und", "all"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PIAT", "NN", "VVFIN", "PPER", "KON", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das liebe Leiden, so ich eingewiegt,", "tokens": ["Das", "lie\u00b7be", "Lei\u00b7den", ",", "so", "ich", "ein\u00b7ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der goldne Ton des J\u00fcnglings wieder auf!", "tokens": ["Der", "gold\u00b7ne", "Ton", "des", "J\u00fcng\u00b7lings", "wie\u00b7der", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht aufsehn durft' ich! keine Sylbe durft'", "tokens": ["Nicht", "auf\u00b7sehn", "durft'", "ich", "!", "kei\u00b7ne", "Syl\u00b7be", "durft'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$.", "PIAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich sagen! O, was h\u00e4tt' ich ihm gesagt!", "tokens": ["Ich", "sa\u00b7gen", "!", "O", ",", "was", "h\u00e4tt'", "ich", "ihm", "ge\u00b7sagt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "NE", "$,", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was mein' ich denn, du Gute? \u2014 la\u00df mich nur!", "tokens": ["Was", "mein'", "ich", "denn", ",", "du", "Gu\u00b7te", "?", "la\u00df", "mich", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "$.", "$(", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun darf ich ja, nun ists so th\u00f6richt nimmer,", "tokens": ["Nun", "darf", "ich", "ja", ",", "nun", "ists", "so", "th\u00f6\u00b7richt", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sch\u00f6n ist's, wenn der Schmerz mit seiner", "tokens": ["Und", "sch\u00f6n", "ist's", ",", "wenn", "der", "Schmerz", "mit", "sei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwester", "tokens": ["Schwes\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der Wonne sich vers\u00f6hnt, noch eh' er weggeht.", "tokens": ["Der", "Won\u00b7ne", "sich", "ver\u00b7s\u00f6hnt", ",", "noch", "eh'", "er", "weg\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O Wiedersehn! das ist noch mehr, du Liebe!", "tokens": ["O", "Wie\u00b7der\u00b7sehn", "!", "das", "ist", "noch", "mehr", ",", "du", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PDS", "VAFIN", "ADV", "ADV", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wenn die B\u00e4ume wieder bl\u00fchn, und Quellen", "tokens": ["Als", "wenn", "die", "B\u00e4u\u00b7me", "wie\u00b7der", "bl\u00fchn", ",", "und", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "VVINF", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von neuem fr\u00f6hlich rauschen \u2014", "tokens": ["Von", "neu\u00b7em", "fr\u00f6h\u00b7lich", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ja! ich hab'", "tokens": ["Ja", "!", "ich", "hab'"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ihn oft gesucht und ernstlich oft es mir", "tokens": ["Ihn", "oft", "ge\u00b7sucht", "und", "ernst\u00b7lich", "oft", "es", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "KON", "ADJD", "ADV", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versagt, doch wollt' ich sein Ged\u00e4chtni\u00df ehren.", "tokens": ["Ver\u00b7sagt", ",", "doch", "wollt'", "ich", "sein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "eh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Bilder der Gespielen, die mit mir", "tokens": ["Die", "Bil\u00b7der", "der", "Ge\u00b7spie\u00b7len", ",", "die", "mit", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf gr\u00fcner Erd' in stummer Kindheit sa\u00dfen,", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Erd'", "in", "stum\u00b7mer", "Kind\u00b7heit", "sa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie d\u00e4mmern ja um meine Seele mir,", "tokens": ["Sie", "d\u00e4m\u00b7mern", "ja", "um", "mei\u00b7ne", "See\u00b7le", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dieser edle Schatte, sollt' er nicht?", "tokens": ["Und", "die\u00b7ser", "ed\u00b7le", "Schat\u00b7te", ",", "sollt'", "er", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Herz im Busen, das unsterbliche,", "tokens": ["Das", "Herz", "im", "Bu\u00b7sen", ",", "das", "uns\u00b7terb\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Kann nicht vergessen, sieh! und \u00f6fters bringt", "tokens": ["Kann", "nicht", "ver\u00b7ges\u00b7sen", ",", "sieh", "!", "und", "\u00f6f\u00b7ters", "bringt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "VVPP", "$,", "VVIMP", "$.", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein guter Genius die Liebenden", "tokens": ["Ein", "gu\u00b7ter", "Ge\u00b7nius", "die", "Lie\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zusammen, da\u00df ein neuer Tag beginnt,", "tokens": ["Zu\u00b7sam\u00b7men", ",", "da\u00df", "ein", "neu\u00b7er", "Tag", "be\u00b7ginnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und ihren Mai die Seele wieder feiert.", "tokens": ["Und", "ih\u00b7ren", "Mai", "die", "See\u00b7le", "wie\u00b7der", "fei\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O wunderbar ist mir! auch er! \u2014 da\u00df du", "tokens": ["O", "wun\u00b7der\u00b7bar", "ist", "mir", "!", "auch", "er", "!", "da\u00df", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "ADJD", "VAFIN", "PPER", "$.", "ADV", "PPER", "$.", "$(", "KOUS", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hinunter mu\u00dftest, Lieber! ehe dir", "tokens": ["Hin\u00b7un\u00b7ter", "mu\u00df\u00b7test", ",", "Lie\u00b7ber", "!", "e\u00b7he", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "$,", "ADJD", "$.", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das deine ward, und dich die frohe Braut", "tokens": ["Das", "dei\u00b7ne", "ward", ",", "und", "dich", "die", "fro\u00b7he", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "VAFIN", "$,", "KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum M\u00e4nnerruhme segnete! Doch starbst", "tokens": ["Zum", "M\u00e4n\u00b7ner\u00b7ruh\u00b7me", "seg\u00b7ne\u00b7te", "!", "Doch", "starbst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$.", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du sch\u00f6n, und oft hab' ich geh\u00f6rt, es fallen", "tokens": ["Du", "sch\u00f6n", ",", "und", "oft", "hab'", "ich", "ge\u00b7h\u00f6rt", ",", "es", "fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "$,", "KON", "ADV", "VAFIN", "PPER", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Lieblinge des Himmels fr\u00fch, damit", "tokens": ["Die", "Lieb\u00b7lin\u00b7ge", "des", "Him\u00b7mels", "fr\u00fch", ",", "da\u00b7mit"], "token_info": ["word", "word", 