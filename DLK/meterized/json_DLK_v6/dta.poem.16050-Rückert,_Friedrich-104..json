{"dta.poem.16050": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df \u00fcber dich ergehn, was du nicht kanst abhalten,", "tokens": ["La\u00df", "\u00fc\u00b7ber", "dich", "er\u00b7gehn", ",", "was", "du", "nicht", "kanst", "ab\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "VVINF", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Zeitensturmes Wehn, der Schicksalsm\u00e4chte Walten.", "tokens": ["Des", "Zei\u00b7ten\u00b7stur\u00b7mes", "Wehn", ",", "der", "Schick\u00b7sals\u00b7m\u00e4ch\u00b7te", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie haben dir herbei gewehet mancherlei,", "tokens": ["Sie", "ha\u00b7ben", "dir", "her\u00b7bei", "ge\u00b7we\u00b7het", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wehen es hinweg, alsob nicht dein es sei.", "tokens": ["Und", "we\u00b7hen", "es", "hin\u00b7weg", ",", "al\u00b7sob", "nicht", "dein", "es", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PTKNEG", "PPOSAT", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie haben selber dich geblasen her, von wannen?", "tokens": ["Sie", "ha\u00b7ben", "sel\u00b7ber", "dich", "ge\u00b7bla\u00b7sen", "her", ",", "von", "wan\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "VVFIN", "PTKVZ", "$,", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rasten nicht bis sie dich hauchten auch von dannen.", "tokens": ["Und", "ras\u00b7ten", "nicht", "bis", "sie", "dich", "hauch\u00b7ten", "auch", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPER", "PPER", "VVFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Von deines Lebens Laub ist Blatt auf Blatt entzittert,", "tokens": ["Von", "dei\u00b7nes", "Le\u00b7bens", "Laub", "ist", "Blatt", "auf", "Blatt", "ent\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VAFIN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und endlich ist der Stamm der morsche selbst zersplittert.", "tokens": ["Und", "end\u00b7lich", "ist", "der", "Stamm", "der", "mor\u00b7sche", "selbst", "zer\u00b7split\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}