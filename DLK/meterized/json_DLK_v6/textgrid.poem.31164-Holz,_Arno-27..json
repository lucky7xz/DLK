{"textgrid.poem.31164": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wozu dies F\u00e4ltchen heut, mein S\u00fcsschen,", "tokens": ["Wo\u00b7zu", "dies", "F\u00e4lt\u00b7chen", "heut", ",", "mein", "S\u00fcs\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dies F\u00e4ltchen unter deinem Hut?", "tokens": ["Dies", "F\u00e4lt\u00b7chen", "un\u00b7ter", "dei\u00b7nem", "Hut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meinst du, das Scharren mit den F\u00fcsschen", "tokens": ["Meinst", "du", ",", "das", "Schar\u00b7ren", "mit", "den", "F\u00fcs\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Thut deinen Stiefels\u00f6hlchen gut?", "tokens": ["Thut", "dei\u00b7nen", "Stie\u00b7fel\u00b7s\u00f6hlc\u00b7hen", "gut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein rothes Sonnenschirmchen zittert,", "tokens": ["Dein", "ro\u00b7thes", "Son\u00b7nen\u00b7schirm\u00b7chen", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein H\u00e4ndchen fiebert brennend heiss,", "tokens": ["Dein", "H\u00e4nd\u00b7chen", "fie\u00b7bert", "bren\u00b7nend", "heiss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesteh's nur ein, du bist erbittert", "tokens": ["Ge\u00b7steh's", "nur", "ein", ",", "du", "bist", "er\u00b7bit\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und denkst, sein Herz ist kalt wie Eis.", "tokens": ["Und", "denkst", ",", "sein", "Herz", "ist", "kalt", "wie", "Eis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O nein! Sein Herz hat tausend F\u00fchler", "tokens": ["O", "nein", "!", "Sein", "Herz", "hat", "tau\u00b7send", "F\u00fch\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schl\u00e4gt genau so warm wie deins;", "tokens": ["Und", "schl\u00e4gt", "ge\u00b7nau", "so", "warm", "wie", "deins", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur denkt sein Kopf ein wenig k\u00fchler", "tokens": ["Nur", "denkt", "sein", "Kopf", "ein", "we\u00b7nig", "k\u00fch\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kennt genau das Einmaleins.", "tokens": ["Und", "kennt", "ge\u00b7nau", "das", "Ein\u00b7mal\u00b7eins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wollte wohl, dass ich es w\u00fcsste,", "tokens": ["Ich", "woll\u00b7te", "wohl", ",", "dass", "ich", "es", "w\u00fcss\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie rosenroth dein kleiner Zeh,", "tokens": ["Wie", "ro\u00b7sen\u00b7roth", "dein", "klei\u00b7ner", "Zeh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie milchweiss deine kleinen Br\u00fcste", "tokens": ["Wie", "milch\u00b7weiss", "dei\u00b7ne", "klei\u00b7nen", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie diskret dein Negligee.", "tokens": ["Und", "wie", "dis\u00b7kret", "dein", "Neg\u00b7li\u00b7gee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nach Indien w\u00fcrd ich mit dir fliehen,", "tokens": ["Nach", "In\u00b7di\u00b7en", "w\u00fcrd", "ich", "mit", "dir", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In Heinrich Heine'schem Geschmack,", "tokens": ["In", "Hein\u00b7rich", "Hei\u00b7ne'\u00b7schem", "Ge\u00b7schmack", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wenn du willst, auch vor dir knieen,", "tokens": ["Und", "wenn", "du", "willst", ",", "auch", "vor", "dir", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "$,", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ein neuer Don Quixote im Frack.", "tokens": ["Ein", "neu\u00b7er", "Don", "Qui\u00b7xo\u00b7te", "im", "Frack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Doch dir Bonbons und Ringe kaufen?", "tokens": ["Doch", "dir", "Bon\u00b7bons", "und", "Rin\u00b7ge", "kau\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Casus, Kind, nehm ich dir krumm.", "tokens": ["Den", "Ca\u00b7sus", ",", "Kind", ",", "nehm", "ich", "dir", "krumm", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Beste, w\u00e4r's, du l\u00e4sst mich laufen", "tokens": ["Das", "Bes\u00b7te", ",", "w\u00e4r's", ",", "du", "l\u00e4sst", "mich", "lau\u00b7fen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und siehst dich \u2013 anderweitig um!", "tokens": ["Und", "siehst", "dich", "\u2013", "an\u00b7der\u00b7wei\u00b7tig", "um", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}