{"textgrid.poem.46454": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er sa\u00df im Urgebraus am Chaosmeer", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er sa\u00df im Urgebraus am Chaosmeer", "tokens": ["Er", "sa\u00df", "im", "Ur\u00b7ge\u00b7braus", "am", "Chaos\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Nacht, zur Nacht,", "tokens": ["Zur", "Nacht", ",", "zur", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sein Auge war von allen Dingen schwer:", "tokens": ["Sein", "Au\u00b7ge", "war", "von", "al\u00b7len", "Din\u00b7gen", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Voll Zeugungsnacht \u2013", "tokens": ["Voll", "Zeu\u00b7gungs\u00b7nacht", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da warf er seinen Becher in die Flut,", "tokens": ["Da", "warf", "er", "sei\u00b7nen", "Be\u00b7cher", "in", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Flut war schwarz und tief und tief,", "tokens": ["Die", "Flut", "war", "schwarz", "und", "tief", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er hob ihn wieder: voll von rotem Blut", "tokens": ["Er", "hob", "ihn", "wie\u00b7der", ":", "voll", "von", "ro\u00b7tem", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und trank und warf ihn wieder tief \u2013", "tokens": ["Und", "trank", "und", "warf", "ihn", "wie\u00b7der", "tief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er trank sich voll und \u00fcbervoll", "tokens": ["Er", "trank", "sich", "voll", "und", "\u00fc\u00b7ber\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bis ihm die Seele \u00fcberschwoll:", "tokens": ["Bis", "ihm", "die", "See\u00b7le", "\u00fc\u00b7bersc\u00b7hwoll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da str\u00f6mte wild aus seiner Kehle", "tokens": ["Da", "str\u00f6m\u00b7te", "wild", "aus", "sei\u00b7ner", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Flutgesang:", "tokens": ["Ein", "Flut\u00b7ge\u00b7sang", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Von Erd- und Leibespracht,", "tokens": ["Von", "Erd", "und", "Lei\u00b7be\u00b7spracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Von Mensch- und Welten-Zeugungsnacht,", "tokens": ["Von", "Men\u00b7sch", "und", "Wel\u00b7ten\u00b7Zeu\u00b7gungs\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Vom Hirn und von der gro\u00dfen Liebesseele \u2013", "tokens": ["Vom", "Hirn", "und", "von", "der", "gro\u00b7\u00dfen", "Lie\u00b7bes\u00b7see\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann kam die allertiefste Nacht", "tokens": ["Dann", "kam", "die", "al\u00b7ler\u00b7tiefs\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwer der All-Schlaf.", "tokens": ["Und", "schwer", "der", "All\u00b7Schlaf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}