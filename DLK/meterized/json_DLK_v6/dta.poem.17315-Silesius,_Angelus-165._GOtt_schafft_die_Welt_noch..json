{"dta.poem.17315": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "165.  GOtt schafft die Welt noch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott schafft die Welt annoch: komt dir di\u00df Fremde f\u00fcr?", "tokens": ["Gott", "schafft", "die", "Welt", "an\u00b7noch", ":", "komt", "dir", "di\u00df", "Frem\u00b7de", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$.", "VVFIN", "PPER", "PDS", "NN", "APPR", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So wiss\u2019 es ist bey jhm kein Vor noch nach/ wie hier.", "tokens": ["So", "wiss'", "es", "ist", "bey", "jhm", "kein", "Vor", "noch", "nach", "/", "wie", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VAFIN", "APPR", "PPER", "PIAT", "NN", "ADV", "APPR", "$(", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}