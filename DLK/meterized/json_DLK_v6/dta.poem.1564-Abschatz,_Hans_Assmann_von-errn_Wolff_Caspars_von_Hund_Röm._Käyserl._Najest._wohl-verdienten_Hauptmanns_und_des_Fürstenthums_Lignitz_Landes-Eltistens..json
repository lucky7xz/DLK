{"dta.poem.1564": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "errn Wolff Caspars von Hund/ R\u00f6m. K\u00e4yserl.  \n Najest. wohl-verdienten Hauptmanns/ und des  \n F\u00fcrstenthums Lignitz Landes-Eltistens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "wey Dinge sind/ die sich im Grabe nicht begraben/", "tokens": ["wey", "Din\u00b7ge", "sind", "/", "die", "sich", "im", "Gra\u00b7be", "nicht", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$(", "PRELS", "PRF", "APPRART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die/ wann der Leib verdirbt/ vom Tode bleiben frey:", "tokens": ["Die", "/", "wann", "der", "Leib", "ver\u00b7dirbt", "/", "vom", "To\u00b7de", "blei\u00b7ben", "frey", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAV", "ART", "NN", "VVFIN", "$(", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Nach-Ruff/ welchen uns die Tugend leget bey/", "tokens": ["Der", "Nach\u00b7Ruff", "/", "wel\u00b7chen", "uns", "die", "Tu\u00b7gend", "le\u00b7get", "bey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAT", "PPER", "ART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Seele/ welche wir von GOtt empfangen haben.", "tokens": ["Die", "See\u00b7le", "/", "wel\u00b7che", "wir", "von", "Gott", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein unverzehrlich Tacht/ kein immer-brennend Licht", "tokens": ["Kein", "un\u00b7ver\u00b7zehr\u00b7lich", "Tacht", "/", "kein", "im\u00b7mer\u00b7bren\u00b7nend", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "$(", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gleicht dieser Himmels-Flamm und hellem Zunder nicht;", "tokens": ["Gleicht", "die\u00b7ser", "Him\u00b7mels\u00b7Flamm", "und", "hel\u00b7lem", "Zun\u00b7der", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "KON", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wen\u0303 gleich solt Oel und Schein erbla\u00dfter Sternen schwinden/", "tokens": ["We\u00f1", "gleich", "solt", "O\u00b7el", "und", "Schein", "er\u00b7bla\u00df\u00b7ter", "Ster\u00b7nen", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VMFIN", "NN", "KON", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "L\u00e4st sich der beyden Glantz noch unvertunckelt finden.", "tokens": ["L\u00e4st", "sich", "der", "bey\u00b7den", "Glantz", "noch", "un\u00b7ver\u00b7tun\u00b7ckelt", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "PIAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Unverwe\u00dflich-seyn sucht Morgenland vergebens/", "tokens": ["Das", "Un\u00b7ver\u00b7we\u00df\u00b7lich\u00b7seyn", "sucht", "Mor\u00b7gen\u00b7land", "ver\u00b7ge\u00b7bens", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die theurste Specerey ist selbst verderblich Gutt/", "tokens": ["Die", "theurs\u00b7te", "Spe\u00b7ce\u00b7rey", "ist", "selbst", "ver\u00b7derb\u00b7lich", "Gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ertz/ Steine/ S\u00e4ulen zwingt Zeit/ Sturm/ Wind oder Glutt", "tokens": ["Ertz", "/", "Stei\u00b7ne", "/", "S\u00e4u\u00b7len", "zwingt", "Zeit", "/", "Sturm", "/", "Wind", "o\u00b7der", "Glutt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "VVFIN", "NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der beste Balsam ist der Nachruhm reines Lebens/", "tokens": ["Der", "bes\u00b7te", "Bal\u00b7sam", "ist", "der", "Nach\u00b7ruhm", "rei\u00b7nes", "Le\u00b7bens", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer dieses Kleinod zu der Ahnen Schilde legt/", "tokens": ["Wer", "die\u00b7ses", "Klei\u00b7nod", "zu", "der", "Ah\u00b7nen", "Schil\u00b7de", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Bild der Gottesfurcht in seine Seele pr\u00e4gt/", "tokens": ["Das", "Bild", "der", "Got\u00b7tes\u00b7furcht", "in", "sei\u00b7ne", "See\u00b7le", "pr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Lebt in und ob der Welt/ und wenn sich die begr\u00e4bet/", "tokens": ["Lebt", "in", "und", "ob", "der", "Welt", "/", "und", "wenn", "sich", "die", "be\u00b7gr\u00e4\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "KON", "KOUS", "ART", "NN", "$(", "KON", "KOUS", "PRF", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wei\u00df er/ da\u00df er doch die Zeit selbst \u00fcberlebet.", "tokens": ["So", "wei\u00df", "er", "/", "da\u00df", "er", "doch", "die", "Zeit", "selbst", "\u00fc\u00b7ber\u00b7le\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier liegt der edle Geist verstrickt in seinen Banden/", "tokens": ["Hier", "liegt", "der", "ed\u00b7le", "Geist", "ver\u00b7strickt", "in", "sei\u00b7nen", "Ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das allzu schwere Fleisch/ der Eitelkeiten Koth/", "tokens": ["Das", "all\u00b7zu", "schwe\u00b7re", "Fleisch", "/", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Koth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PTKA", "ADJA", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Klebt seinen Fl\u00fcgeln an/ doch kennt er keinen Todt:", "tokens": ["Klebt", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "an", "/", "doch", "kennt", "er", "kei\u00b7nen", "Todt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann die gew\u00fcnschte Zeit der Freyheit ist verhanden/", "tokens": ["Wann", "die", "ge\u00b7w\u00fcnschte", "Zeit", "der", "Frey\u00b7heit", "ist", "ver\u00b7han\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Im Fall ihn nicht die Last der Laster unterdr\u00fcckt/", "tokens": ["Im", "Fall", "ihn", "nicht", "die", "Last", "der", "Las\u00b7ter", "un\u00b7ter\u00b7dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in das schwartze Reich betr\u00fcbter N\u00e4chte schickt/", "tokens": ["Und", "in", "das", "schwart\u00b7ze", "Reich", "be\u00b7tr\u00fcb\u00b7ter", "N\u00e4ch\u00b7te", "schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So klimmt er an das Licht/ und \u00fcbersteigt die Sonne/", "tokens": ["So", "klimmt", "er", "an", "das", "Licht", "/", "und", "\u00fc\u00b7bers\u00b7teigt", "die", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bewohnt bey GOttes Thron das Reich der steten Wonne.", "tokens": ["Be\u00b7wohnt", "bey", "Got\u00b7tes", "Thron", "das", "Reich", "der", "ste\u00b7ten", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die D\u00fcnste dieser Welt umnebeln das Gesichte/", "tokens": ["Die", "D\u00fcns\u00b7te", "die\u00b7ser", "Welt", "um\u00b7ne\u00b7beln", "das", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein neidig Auge streicht der Sonne Flecken an.", "tokens": ["Ein", "nei\u00b7dig", "Au\u00b7ge", "streicht", "der", "Son\u00b7ne", "Fle\u00b7cken", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man mindert oder mehrt was der und du gethan/", "tokens": ["Man", "min\u00b7dert", "o\u00b7der", "mehrt", "was", "der", "und", "du", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "KON", "VVFIN", "PWS", "ART", "KON", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn unser Lebens-Licht verl\u00f6scht/ so wird es lichte.", "tokens": ["Wenn", "un\u00b7ser", "Le\u00b7bens\u00b7Licht", "ver\u00b7l\u00f6scht", "/", "so", "wird", "es", "lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$(", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn Lust und Nutz entweicht/ kein Zwang uns mehr umgiebt", "tokens": ["Wenn", "Lust", "und", "Nutz", "ent\u00b7weicht", "/", "kein", "Zwang", "uns", "mehr", "um\u00b7giebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$(", "PIAT", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So r\u00fchmt und schilt die Welt/ was ieder ausge\u00fcbt.", "tokens": ["So", "r\u00fchmt", "und", "schilt", "die", "Welt", "/", "was", "ie\u00b7der", "aus\u00b7ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$(", "PWS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der F\u00fcrnis springet ab/ die wahren Lebens-Farben", "tokens": ["Der", "F\u00fcr\u00b7nis", "sprin\u00b7get", "ab", "/", "die", "wah\u00b7ren", "Le\u00b7bens\u00b7Fa\u00b7rben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Erheben unsre Zier/ entbl\u00f6\u00dfen unsre Narben.", "tokens": ["Er\u00b7he\u00b7ben", "uns\u00b7re", "Zier", "/", "ent\u00b7bl\u00f6\u00b7\u00dfen", "uns\u00b7re", "Nar\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir sencken zwar nunmehr die abgelebten Glieder", "tokens": ["Wir", "sen\u00b7cken", "zwar", "nun\u00b7mehr", "die", "ab\u00b7ge\u00b7leb\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Edelen von Hund mit Thr\u00e4nen in die Grufft:", "tokens": ["Des", "E\u00b7de\u00b7len", "von", "Hund", "mit", "Thr\u00e4\u00b7nen", "in", "die", "Grufft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das beste Theil von ihm hat GOtt zu sich gerufft/", "tokens": ["Das", "bes\u00b7te", "Theil", "von", "ihm", "hat", "Gott", "zu", "sich", "ge\u00b7rufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VAFIN", "NN", "APPR", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein wohl verdienter Ruhm schallt aus dem Grabe wider.", "tokens": ["Sein", "wohl", "ver\u00b7dien\u00b7ter", "Ruhm", "schallt", "aus", "dem", "Gra\u00b7be", "wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Plagen dieser Welt/ das Siechthum dieser Zeit", "tokens": ["Die", "Pla\u00b7gen", "die\u00b7ser", "Welt", "/", "das", "Siech\u00b7thum", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$(", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ersetzet ihm nunmehr die Schos der Ewigkeit:", "tokens": ["Er\u00b7set\u00b7zet", "ihm", "nun\u00b7mehr", "die", "Schos", "der", "E\u00b7wig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der m\u00fcde Leib verschl\u00e4fft/ der Geist darff nimmer f\u00fchlen", "tokens": ["Der", "m\u00fc\u00b7de", "Leib", "ver\u00b7schl\u00e4fft", "/", "der", "Geist", "darff", "nim\u00b7mer", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "ART", "NN", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die St\u00fcrme/ welche noch auff unsre H\u00e4ubter zielen.", "tokens": ["Die", "St\u00fcr\u00b7me", "/", "wel\u00b7che", "noch", "auff", "uns\u00b7re", "H\u00e4ub\u00b7ter", "zie\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alex