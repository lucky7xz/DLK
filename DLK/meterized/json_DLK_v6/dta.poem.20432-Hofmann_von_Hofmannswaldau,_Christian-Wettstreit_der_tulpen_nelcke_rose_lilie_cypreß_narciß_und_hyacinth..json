{"dta.poem.20432": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit der tulpen/ nelcke/ rose/  \n lilie/ cypre\u00df/ narci\u00df und  \n hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht meinen purpur an/ ihr blumen/ wie er plitzt?", "tokens": ["Seht", "mei\u00b7nen", "pur\u00b7pur", "an", "/", "ihr", "blu\u00b7men", "/", "wie", "er", "plitzt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "PTKVZ", "$(", "PPOSAT", "NN", "$(", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein k\u00f6niglich gewand hat nicht die herrlichkeiten:", "tokens": ["Ein", "k\u00f6\u00b7nig\u00b7lich", "ge\u00b7wand", "hat", "nicht", "die", "herr\u00b7lich\u00b7kei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAPP", "VAFIN", "PTKNEG", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu dem/ so mag ich nicht blo\u00df um die farbe streiten.", "tokens": ["Zu", "dem", "/", "so", "mag", "ich", "nicht", "blo\u00df", "um", "die", "far\u00b7be", "strei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Welch balsam reucht so sch\u00f6n den Pal\u00e4stina schwitzt/", "tokens": ["Welch", "bal\u00b7sam", "reucht", "so", "sch\u00f6n", "den", "Pa\u00b7l\u00e4s\u00b7ti\u00b7na", "schwitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVFIN", "ADV", "ADJD", "ART", "NE", "VVFIN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Als mein beliebt geruch/ kein zimmet-oele spritzt", "tokens": ["Als", "mein", "be\u00b7liebt", "ge\u00b7ruch", "/", "kein", "zim\u00b7met\u00b7o\u00b7ele", "spritzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "ADV", "$(", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So s\u00fcssen athem aus/ als wenn bey sommers zeiten", "tokens": ["So", "s\u00fcs\u00b7sen", "at\u00b7hem", "aus", "/", "als", "wenn", "bey", "som\u00b7mers", "zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$(", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich meine knopf entschlie\u00df/ und tausend lieblichkeiten", "tokens": ["Ich", "mei\u00b7ne", "knopf", "ent\u00b7schlie\u00df", "/", "und", "tau\u00b7send", "lieb\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$(", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den g\u00e4rten theile zu/ die Flora hegt und sch\u00fctzt.", "tokens": ["Den", "g\u00e4r\u00b7ten", "thei\u00b7le", "zu", "/", "die", "Flo\u00b7ra", "hegt", "und", "sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "$(", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Welch ambra kan die seel und geister so erqvicken?", "tokens": ["Welch", "am\u00b7bra", "kan", "die", "seel", "und", "geis\u00b7ter", "so", "er\u00b7qvi\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN", "KON", "ADJA", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Welch Araber zieht mich hier seinen saffran vor?", "tokens": ["Welch", "A\u00b7ra\u00b7ber", "zieht", "mich", "hier", "sei\u00b7nen", "saf\u00b7fran", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Zu dem/ so kan ich mich mit pracht vielf\u00e4ltig schm\u00fccken/", "tokens": ["Zu", "dem", "/", "so", "kan", "ich", "mich", "mit", "pracht", "viel\u00b7f\u00e4l\u00b7tig", "schm\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auff ieden bl\u00e4ttern schwebt der farben gantzes chor.", "tokens": ["Auff", "ie\u00b7den", "bl\u00e4t\u00b7tern", "schwebt", "der", "far\u00b7ben", "gant\u00b7zes", "chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So darff ich endlich auch mich gar unsterblich r\u00fchren/", "tokens": ["So", "darff", "ich", "end\u00b7lich", "auch", "mich", "gar", "uns\u00b7terb\u00b7lich", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil meine sch\u00f6nheit mu\u00df den winter-krantz bebl\u00fcmen.", "tokens": ["Weil", "mei\u00b7ne", "sch\u00f6n\u00b7heit", "mu\u00df", "den", "win\u00b7ter\u00b7krantz", "be\u00b7bl\u00fc\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}