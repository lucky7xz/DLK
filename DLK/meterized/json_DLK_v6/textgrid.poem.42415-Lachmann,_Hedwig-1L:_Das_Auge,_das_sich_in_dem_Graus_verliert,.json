{"textgrid.poem.42415": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Auge, das sich in dem Graus verliert,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Auge, das sich in dem Graus verliert,", "tokens": ["Das", "Au\u00b7ge", ",", "das", "sich", "in", "dem", "Graus", "ver\u00b7liert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der langsam um den Erdball rast,", "tokens": ["Der", "lang\u00b7sam", "um", "den", "Erd\u00b7ball", "rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird vor Entsetzen irre und gefriert,", "tokens": ["Wird", "vor", "Ent\u00b7set\u00b7zen", "ir\u00b7re", "und", "ge\u00b7friert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie wenn im Tod es brechend sich verglast.", "tokens": ["Wie", "wenn", "im", "Tod", "es", "bre\u00b7chend", "sich", "ver\u00b7glast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPRART", "NN", "PPER", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Weh ohne Mass, ins unbegrenzte All", "tokens": ["Weh", "oh\u00b7ne", "Mass", ",", "ins", "un\u00b7be\u00b7grenz\u00b7te", "All"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie ein emp\u00f6rtes Meer hinausgeschnellt,", "tokens": ["Wie", "ein", "em\u00b7p\u00f6r\u00b7tes", "Meer", "hin\u00b7aus\u00b7ge\u00b7schnellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo es mit millionenfachem Prall", "tokens": ["Wo", "es", "mit", "mil\u00b7li\u00b7o\u00b7nen\u00b7fa\u00b7chem", "Prall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An starrer Luftschicht wesenlos zerschellt!", "tokens": ["An", "star\u00b7rer", "Luft\u00b7schicht", "we\u00b7sen\u00b7los", "zer\u00b7schellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist der Erbfluch unausrottbar z\u00e4h,", "tokens": ["Das", "ist", "der", "Erb\u00b7fluch", "un\u00b7aus\u00b7rott\u00b7bar", "z\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der das Geschlecht mit seinem Bann umfing,", "tokens": ["Der", "das", "Ge\u00b7schlecht", "mit", "sei\u00b7nem", "Bann", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als in verworrnem Urtrieb dumpf und j\u00e4h", "tokens": ["Als", "in", "ver\u00b7worr\u00b7nem", "Ur\u00b7trieb", "dumpf", "und", "j\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum ersten Mal sich Blut am Blut verging.", "tokens": ["Zum", "ers\u00b7ten", "Mal", "sich", "Blut", "am", "Blut", "ver\u00b7ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aus euren Tr\u00e4umen wuchs der wilde Geist,", "tokens": ["Aus", "eu\u00b7ren", "Tr\u00e4u\u00b7men", "wuchs", "der", "wil\u00b7de", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von H\u00f6llenlicht umlodert und umqualmt,", "tokens": ["Von", "H\u00f6l\u00b7len\u00b7licht", "um\u00b7lo\u00b7dert", "und", "um\u00b7qualmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den mit verst\u00f6rten Sinnen ihr umkreist,", "tokens": ["Den", "mit", "ver\u00b7st\u00f6r\u00b7ten", "Sin\u00b7nen", "ihr", "um\u00b7kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und den ihr Gott nennt, weil er euch zermalmt.", "tokens": ["Und", "den", "ihr", "Gott", "nennt", ",", "weil", "er", "euch", "zer\u00b7malmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "F\u00fchllos und ohne Ohr f\u00fcr euer Flehn,", "tokens": ["F\u00fchl\u00b7los", "und", "oh\u00b7ne", "Ohr", "f\u00fcr", "eu\u00b7er", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tut er mit Tod und Grauen euch Bescheid", "tokens": ["Tut", "er", "mit", "Tod", "und", "Grau\u00b7en", "euch", "Be\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4sst er ohne Ende blind geschehn,", "tokens": ["Und", "l\u00e4sst", "er", "oh\u00b7ne", "En\u00b7de", "blind", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dass ihr die Opferer und Opfer seid.", "tokens": ["Dass", "ihr", "die", "Op\u00b7fe\u00b7rer", "und", "Op\u00b7fer", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}