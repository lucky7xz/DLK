{"textgrid.poem.56889": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wahrt euer Mitleid f\u00fcr euch,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrt euer Mitleid f\u00fcr euch,", "tokens": ["Wahrt", "eu\u00b7er", "Mit\u00b7leid", "f\u00fcr", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.2": {"text": "gutherzige Menschlein!", "tokens": ["gu\u00b7ther\u00b7zi\u00b7ge", "Mensc\u00b7hlein", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auch der d\u00fcstersten Leidenschaft", "tokens": ["Auch", "der", "d\u00fcs\u00b7ters\u00b7ten", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "bitterster Seufzer", "tokens": ["bit\u00b7ters\u00b7ter", "Seuf\u00b7zer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "ist k\u00f6stlicher noch,", "tokens": ["ist", "k\u00f6st\u00b7li\u00b7cher", "noch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "als was ihr uns bieten k\u00f6nntet!", "tokens": ["als", "was", "ihr", "uns", "bie\u00b7ten", "k\u00f6nn\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In unserm Schmerz,", "tokens": ["In", "un\u00b7serm", "Schmerz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Zorn, Ha\u00df, Einsamsein \u2013", "tokens": ["Zorn", ",", "Ha\u00df", ",", "Ein\u00b7sam\u00b7sein", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "wie viel gl\u00fccklicher sind wir", "tokens": ["wie", "viel", "gl\u00fcck\u00b7li\u00b7cher", "sind", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "als ihr!", "tokens": ["als", "ihr", "!"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Hinweg mit dem Leichnam,", "tokens": ["Hin\u00b7weg", "mit", "dem", "Leich\u00b7nam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "den, trostbeflissen,", "tokens": ["den", ",", "trost\u00b7be\u00b7flis\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "eu'r Eifer heranschleppt!", "tokens": ["eu'r", "Ei\u00b7fer", "her\u00b7an\u00b7schleppt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Fort mit der Mumie!", "tokens": ["Fort", "mit", "der", "Mu\u00b7mie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Was soll", "tokens": ["Was", "soll"], "token_info": ["word", "word"], "pos": ["PWS", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "den lebendigen G\u00f6ttern", "tokens": ["den", "le\u00b7ben\u00b7di\u00b7gen", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "der tote!", "tokens": ["der", "to\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}