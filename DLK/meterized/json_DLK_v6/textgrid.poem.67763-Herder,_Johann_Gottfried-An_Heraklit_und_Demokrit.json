{"textgrid.poem.67763": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Heraklit und Demokrit", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt kannst Du weinen, Heraklit;", "tokens": ["Jetzt", "kannst", "Du", "wei\u00b7nen", ",", "Her\u00b7a\u00b7klit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn unsre Zeit wird immer schlimmer.", "tokens": ["Denn", "uns\u00b7re", "Zeit", "wird", "im\u00b7mer", "schlim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt kannst Du lachen, Demokrit;", "tokens": ["Jetzt", "kannst", "Du", "la\u00b7chen", ",", "De\u00b7mo\u00b7krit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn l\u00e4cherlicher wird das Leben immer.", "tokens": ["Denn", "l\u00e4\u00b7cher\u00b7li\u00b7cher", "wird", "das", "Le\u00b7ben", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich aber seh' Euch Beide an", "tokens": ["Ich", "a\u00b7ber", "seh'", "Euch", "Bei\u00b7de", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und denke, wie ich mit Euch immer", "tokens": ["Und", "den\u00b7ke", ",", "wie", "ich", "mit", "Euch", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "APPR", "PPER", "ADV"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Mitlachen und mitweinen kann.", "tokens": ["Mit\u00b7la\u00b7chen", "und", "mit\u00b7wei\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}