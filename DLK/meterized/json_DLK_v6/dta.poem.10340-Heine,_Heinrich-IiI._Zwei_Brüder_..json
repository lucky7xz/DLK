{"dta.poem.10340": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Zwei Br\u00fcder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Oben auf der Bergesspitze", "tokens": ["O\u00b7ben", "auf", "der", "Ber\u00b7ges\u00b7spit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt das Schlo\u00df in Nacht geh\u00fcllt;", "tokens": ["Liegt", "das", "Schlo\u00df", "in", "Nacht", "ge\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch im Thale leuchten Blitze,", "tokens": ["Doch", "im", "Tha\u00b7le", "leuch\u00b7ten", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Helle Schwerter klirren wild.", "tokens": ["Hel\u00b7le", "Schwer\u00b7ter", "klir\u00b7ren", "wild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das sind Br\u00fcder, die dort fechten", "tokens": ["Das", "sind", "Br\u00fc\u00b7der", ",", "die", "dort", "fech\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Grimmen Zweikampf, wuthentbrannt.", "tokens": ["Grim\u00b7men", "Zwei\u00b7kampf", ",", "wut\u00b7hent\u00b7brannt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprich, warum die Br\u00fcder rechten", "tokens": ["Sprich", ",", "wa\u00b7rum", "die", "Br\u00fc\u00b7der", "rech\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Schwerte in der Hand?", "tokens": ["Mit", "dem", "Schwer\u00b7te", "in", "der", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gr\u00e4fin Laura's Augenfunken", "tokens": ["Gr\u00e4\u00b7fin", "Lau\u00b7ra's", "Au\u00b7gen\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00fcndeten den Br\u00fcderstreit;", "tokens": ["Z\u00fcn\u00b7de\u00b7ten", "den", "Br\u00fc\u00b7der\u00b7streit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Beide gl\u00fchen liebestrunken", "tokens": ["Bei\u00b7de", "gl\u00fc\u00b7hen", "lie\u00b7be\u00b7strun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr die adlig holde Maid.", "tokens": ["F\u00fcr", "die", "ad\u00b7lig", "hol\u00b7de", "Maid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Welchem aber von den beiden", "tokens": ["Wel\u00b7chem", "a\u00b7ber", "von", "den", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ART", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wendet sich ihr Herze zu?", "tokens": ["Wen\u00b7det", "sich", "ihr", "Her\u00b7ze", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Ergr\u00fcbeln kann's entscheiden, \u2014", "tokens": ["Kein", "Er\u00b7gr\u00fc\u00b7beln", "kann's", "ent\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwert heraus, entscheide du!", "tokens": ["Schwert", "he\u00b7raus", ",", "ent\u00b7schei\u00b7de", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sie fechten k\u00fchn verwegen,", "tokens": ["Und", "sie", "fech\u00b7ten", "k\u00fchn", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hieb auf Hiebe niederkracht's.", "tokens": ["Hieb", "auf", "Hie\u00b7be", "nie\u00b7der\u00b7kracht'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fctet euch, Ihr wilden Degen,", "tokens": ["H\u00fc\u00b7tet", "euch", ",", "Ihr", "wil\u00b7den", "De\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grausig Blendwerk schleichet Nachts.", "tokens": ["Grau\u00b7sig", "Blend\u00b7werk", "schlei\u00b7chet", "Nachts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wehe! Wehe! blut'ge Br\u00fcder!", "tokens": ["We\u00b7he", "!", "We\u00b7he", "!", "blut'\u00b7ge", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehe! Wehe! blut'ges Thal!", "tokens": ["We\u00b7he", "!", "We\u00b7he", "!", "blut'\u00b7ges", "Thal", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Beide K\u00e4mpfer st\u00fcrzen nieder,", "tokens": ["Bei\u00b7de", "K\u00e4mp\u00b7fer", "st\u00fcr\u00b7zen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer in des andern Stahl. \u2014", "tokens": ["Ei\u00b7ner", "in", "des", "an\u00b7dern", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Viel Jahrhunderte verwehen\u201a", "tokens": ["Viel", "Jahr\u00b7hun\u00b7der\u00b7te", "ver\u00b7we\u00b7hen", "\u201a"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Viel Geschlechter deckt das Grab;", "tokens": ["Viel", "Ge\u00b7schlech\u00b7ter", "deckt", "das", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig von des Berges H\u00f6hen", "tokens": ["Trau\u00b7rig", "von", "des", "Ber\u00b7ges", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaut das \u00f6de Schlo\u00df herab.", "tokens": ["Schaut", "das", "\u00f6\u00b7de", "Schlo\u00df", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber Nachts, im Thalesgrunde,", "tokens": ["A\u00b7ber", "Nachts", ",", "im", "Tha\u00b7les\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelt's heimlich, wunderbar\u201a", "tokens": ["Wan\u00b7delt's", "heim\u00b7lich", ",", "wun\u00b7der\u00b7bar", "\u201a"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn da kommt die zw\u00f6lfte Stunde,", "tokens": ["Wenn", "da", "kommt", "die", "zw\u00f6lf\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00e4mpfet dort das Br\u00fcderpaar.", "tokens": ["K\u00e4mp\u00b7fet", "dort", "das", "Br\u00fc\u00b7der\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}