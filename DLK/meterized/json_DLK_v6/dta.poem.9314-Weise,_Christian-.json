{"dta.poem.9314": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lobe das gl\u00fccke/ was nutzet die tugend?", "tokens": ["Ich", "lo\u00b7be", "das", "gl\u00fc\u00b7cke", "/", "was", "nut\u00b7zet", "die", "tu\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "$(", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dieweil das gl\u00fccke stets", "tokens": ["Die\u00b7weil", "das", "gl\u00fc\u00b7cke", "stets"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit s\u00fcsser freude lacht;", "tokens": ["Mit", "s\u00fcs\u00b7ser", "freu\u00b7de", "lacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die tugend aber nicht", "tokens": ["Die", "tu\u00b7gend", "a\u00b7ber", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel gute tage macht.", "tokens": ["Viel", "gu\u00b7te", "ta\u00b7ge", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das gl\u00fccke macht mich gro\u00df; die tugend ist ein licht", "tokens": ["Das", "gl\u00fc\u00b7cke", "macht", "mich", "gro\u00df", ";", "die", "tu\u00b7gend", "ist", "ein", "licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das wenig glantz ertheilt;", "tokens": ["Das", "we\u00b7nig", "glantz", "er\u00b7theilt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Drum selig ist der mann/", "tokens": ["Drum", "se\u00b7lig", "ist", "der", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der das gute gl\u00fcck ereilt", "tokens": ["Der", "das", "gu\u00b7te", "gl\u00fcck", "er\u00b7eilt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und in warheit sprechen kan:", "tokens": ["Und", "in", "war\u00b7heit", "spre\u00b7chen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ich lobe das gl\u00fcck/ was nutzet die tugend?", "tokens": ["Ich", "lo\u00b7be", "das", "gl\u00fcck", "/", "was", "nut\u00b7zet", "die", "tu\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}