{"textgrid.poem.33910": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Nachtgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mich Abends entkleide,", "tokens": ["Wenn", "ich", "mich", "A\u00b7bends", "ent\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gemachsam, St\u00fcck f\u00fcr St\u00fcck,", "tokens": ["Ge\u00b7mach\u00b7sam", ",", "St\u00fcck", "f\u00fcr", "St\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So tragen die m\u00fcden Gedanken", "tokens": ["So", "tra\u00b7gen", "die", "m\u00fc\u00b7den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mich vorw\u00e4rts oder zur\u00fcck.", "tokens": ["Mich", "vor\u00b7w\u00e4rts", "o\u00b7der", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich denke der alten Tage,", "tokens": ["Ich", "den\u00b7ke", "der", "al\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da zog die Mutter mich aus;", "tokens": ["Da", "zog", "die", "Mut\u00b7ter", "mich", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie legte mich still in die Wiege,", "tokens": ["Sie", "leg\u00b7te", "mich", "still", "in", "die", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Winde braus'ten um's Haus.", "tokens": ["Die", "Win\u00b7de", "braus'\u00b7ten", "um's", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ich denke der letzten Stunde,", "tokens": ["Ich", "den\u00b7ke", "der", "letz\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da werden's die Nachbarn thun;", "tokens": ["Da", "wer\u00b7den's", "die", "Nach\u00b7barn", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie senken mich still in die Erde,", "tokens": ["Sie", "sen\u00b7ken", "mich", "still", "in", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann werd' ich lange ruh'n.", "tokens": ["Dann", "werd'", "ich", "lan\u00b7ge", "ruh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schlie\u00dft nun der Schlaf mein Auge,", "tokens": ["Schlie\u00dft", "nun", "der", "Schlaf", "mein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie tr\u00e4um' ich so oftmals das:", "tokens": ["Wie", "tr\u00e4um'", "ich", "so", "oft\u00b7mals", "das", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "PDS", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es w\u00e4re Eins von Beidem,", "tokens": ["Es", "w\u00e4\u00b7re", "Eins", "von", "Bei\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur w\u00fc\u00dft' ich selber nicht, was.", "tokens": ["Nur", "w\u00fc\u00dft'", "ich", "sel\u00b7ber", "nicht", ",", "was", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}