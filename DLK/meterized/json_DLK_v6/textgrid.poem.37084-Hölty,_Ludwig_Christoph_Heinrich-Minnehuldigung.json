{"textgrid.poem.37084": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Minnehuldigung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allbereits im Fl\u00fcgelkleide", "tokens": ["All\u00b7be\u00b7reits", "im", "Fl\u00fc\u00b7gel\u00b7klei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Waren minnigliche Fraun", "tokens": ["Wa\u00b7ren", "min\u00b7nig\u00b7li\u00b7che", "Fraun"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Miene liebste Augenweide,", "tokens": ["Mie\u00b7ne", "liebs\u00b7te", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Konnte nimmer satt mich schaun.", "tokens": ["Konn\u00b7te", "nim\u00b7mer", "satt", "mich", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich verga\u00df der Vogelnester,", "tokens": ["Ich", "ver\u00b7ga\u00df", "der", "Vo\u00b7gel\u00b7nes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warf mein Steckenpferd ins Gras,", "tokens": ["Warf", "mein", "Ste\u00b7cken\u00b7pferd", "ins", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann bey meiner lieben Schwester", "tokens": ["Wann", "bey", "mei\u00b7ner", "lie\u00b7ben", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine sch\u00f6ne Dirne sa\u00df.", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Dir\u00b7ne", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freute mich der sch\u00f6nen Dirne,", "tokens": ["Freu\u00b7te", "mich", "der", "sch\u00f6\u00b7nen", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres rothen Wangenpaars,", "tokens": ["Ih\u00b7res", "ro\u00b7then", "Wan\u00b7gen\u00b7paars", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihres Mundes, ihrer Stirne,", "tokens": ["Ih\u00b7res", "Mun\u00b7des", ",", "ih\u00b7rer", "Stir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres blonden Lockenhaars.", "tokens": ["Ih\u00b7res", "blon\u00b7den", "Lo\u00b7cken\u00b7haars", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lie\u00df Virgilen, lie\u00df Oviden,", "tokens": ["Lie\u00df", "Vir\u00b7gi\u00b7len", ",", "lie\u00df", "O\u00b7vi\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieng ein M\u00e4dchen auf dem Plan,", "tokens": ["Gieng", "ein", "M\u00e4d\u00b7chen", "auf", "dem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhen, traun, in gutem Frieden,", "tokens": ["Ru\u00b7hen", ",", "traun", ",", "in", "gu\u00b7tem", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich der prei\u00dflichen zu nahn.", "tokens": ["Mich", "der", "prei\u00df\u00b7li\u00b7chen", "zu", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ADJA", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ich weiland that als Knabe,", "tokens": ["Was", "ich", "wei\u00b7land", "that", "als", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd ich wahrlich immer thun,", "tokens": ["Werd", "ich", "wahr\u00b7lich", "im\u00b7mer", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ich werd', im k\u00fchlen Grabe,", "tokens": ["Bis", "ich", "werd'", ",", "im", "k\u00fch\u00b7len", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neben meinen V\u00e4tern ruhn.", "tokens": ["Ne\u00b7ben", "mei\u00b7nen", "V\u00e4\u00b7tern", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Immer meine besten Weisen", "tokens": ["Im\u00b7mer", "mei\u00b7ne", "bes\u00b7ten", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Minniglichen Frauen weihn,", "tokens": ["Min\u00b7nig\u00b7li\u00b7chen", "Frau\u00b7en", "weihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Immer Minn' und Weiber preisen,", "tokens": ["Im\u00b7mer", "Minn'", "und", "Wei\u00b7ber", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mich ihrer Sch\u00f6ne freun.", "tokens": ["Und", "mich", "ih\u00b7rer", "Sch\u00f6\u00b7ne", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}