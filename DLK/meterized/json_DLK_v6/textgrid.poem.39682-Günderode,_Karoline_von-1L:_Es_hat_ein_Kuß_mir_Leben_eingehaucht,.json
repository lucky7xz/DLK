{"textgrid.poem.39682": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es hat ein Ku\u00df mir Leben eingehaucht,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat ein Ku\u00df mir Leben eingehaucht,", "tokens": ["Es", "hat", "ein", "Ku\u00df", "mir", "Le\u00b7ben", "ein\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gestillet meines Busens tiefstes Schmachten,", "tokens": ["Ge\u00b7stil\u00b7let", "mei\u00b7nes", "Bu\u00b7sens", "tiefs\u00b7tes", "Schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Komm, Dunkelheit! mich traulich zu umnachten", "tokens": ["Komm", ",", "Dun\u00b7kel\u00b7heit", "!", "mich", "trau\u00b7lich", "zu", "um\u00b7nach\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$.", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df neue Wonne meine Lippe saugt.", "tokens": ["Da\u00df", "neu\u00b7e", "Won\u00b7ne", "mei\u00b7ne", "Lip\u00b7pe", "saugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In Tr\u00e4ume war solch Leben eingetaucht,", "tokens": ["In", "Tr\u00e4u\u00b7me", "war", "solch", "Le\u00b7ben", "ein\u00b7ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drum leb' ich, ewig Tr\u00e4ume zu betrachten,", "tokens": ["Drum", "leb'", "ich", ",", "e\u00b7wig", "Tr\u00e4u\u00b7me", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kann aller andern Freuden Glanz verachten,", "tokens": ["Kann", "al\u00b7ler", "an\u00b7dern", "Freu\u00b7den", "Glanz", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil nur die Nacht so s\u00fc\u00dfen Balsam haucht.", "tokens": ["Weil", "nur", "die", "Nacht", "so", "s\u00fc\u00b7\u00dfen", "Bal\u00b7sam", "haucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Tag ist karg an liebes\u00fc\u00dfen Wonnen,", "tokens": ["Der", "Tag", "ist", "karg", "an", "lie\u00b7be\u00b7s\u00fc\u00b7\u00dfen", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es schmerzt mich seines Lichtes eitles Prangen", "tokens": ["Es", "schmerzt", "mich", "sei\u00b7nes", "Lich\u00b7tes", "eit\u00b7les", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mich verzehren seiner Sonne Gluthen.", "tokens": ["Und", "mich", "ver\u00b7zeh\u00b7ren", "sei\u00b7ner", "Son\u00b7ne", "Glu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum birg dich Aug' dem Glanze irr'dscher Sonnen!", "tokens": ["Drum", "birg", "dich", "Aug'", "dem", "Glan\u00b7ze", "irr'd\u00b7scher", "Son\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "H\u00fcll' dich in Nacht, sie stillet dein Verlangen", "tokens": ["H\u00fcll'", "dich", "in", "Nacht", ",", "sie", "stil\u00b7let", "dein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und heilt den Schmerz, wie Lethes k\u00fchle Fluthen.", "tokens": ["Und", "heilt", "den", "Schmerz", ",", "wie", "Le\u00b7thes", "k\u00fch\u00b7le", "Flut\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}