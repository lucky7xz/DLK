{"textgrid.poem.53647": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sorrent", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Tage so golden verfliegen,", "tokens": ["Wie", "die", "Ta\u00b7ge", "so", "gol\u00b7den", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "wie die Nacht sich so selig vertr\u00e4umt \u2013", "tokens": ["wie", "die", "Nacht", "sich", "so", "se\u00b7lig", "ver\u00b7tr\u00e4umt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "ADJD", "VVPP", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "wenn am Abend bechiffonte Ziegen", "tokens": ["wenn", "am", "A\u00b7bend", "be\u00b7ch\u00b7if\u00b7fon\u00b7te", "Zie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "vor der Theke sich wogen und wiegen \u2013", "tokens": ["vor", "der", "The\u00b7ke", "sich", "wo\u00b7gen", "und", "wie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "wo der Sekt Gottbeh\u00fcte noch sch\u00e4umt . . .", "tokens": ["wo", "der", "Sekt", "Gott\u00b7be\u00b7h\u00fc\u00b7te", "noch", "sch\u00e4umt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "VVFIN", "$.", "$.", "$."], "meter": "+-++-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Wo im Schleier \u2013 ich danke, Herr Franke \u2013", "tokens": ["Wo", "im", "Schlei\u00b7er", "\u2013", "ich", "dan\u00b7ke", ",", "Herr", "Fran\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$(", "PPER", "VVFIN", "$,", "NN", "NE", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "junge Nutten den Beifox vollziehn . . .", "tokens": ["jun\u00b7ge", "Nut\u00b7ten", "den", "Bei\u00b7fox", "voll\u00b7ziehn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.8": {"text": "O du schimmernde Bl\u00fcte der Panke!", "tokens": ["O", "du", "schim\u00b7mern\u00b7de", "Bl\u00fc\u00b7te", "der", "Pan\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Sei gegr\u00fc\u00dft, du mein sch\u00f6nes Berlin \u2013!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "mein", "sch\u00f6\u00b7nes", "Ber\u00b7lin", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "ADJA", "NE", "$(", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und die Nacht, wenn bei Rotters sie toben,", "tokens": ["Und", "die", "Nacht", ",", "wenn", "bei", "Rot\u00b7ters", "sie", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "dem Claqueure der Handschuh zerplatzt \u2013", "tokens": ["dem", "Cla\u00b7que\u00b7u\u00b7re", "der", "Hand\u00b7schuh", "zer\u00b7platzt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wenn Annoncen, so bilderdurchwoben,", "tokens": ["wenn", "An\u00b7non\u00b7cen", ",", "so", "bil\u00b7der\u00b7durch\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ihre Herren preisen und loben \u2013", "tokens": ["ih\u00b7re", "Her\u00b7ren", "prei\u00b7sen", "und", "lo\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "wenn die Loge futtert und schmatzt . . .", "tokens": ["wenn", "die", "Lo\u00b7ge", "fut\u00b7tert", "und", "schmatzt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "$.", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "\u00bbwat is denn det hier forn Jestanke?", "tokens": ["\u00bb", "wat", "is", "denn", "det", "hier", "forn", "Jes\u00b7tan\u00b7ke", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "KON", "PDS", "ADV", "NE", "NE", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wer e\u00dft hier K\u00e4se? Ham Sien?\u00ab . . .", "tokens": ["Wer", "e\u00dft", "hier", "K\u00e4\u00b7se", "?", "Ham", "Sien", "?", "\u00ab", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "ADV", "NN", "$.", "NE", "NE", "$.", "$(", "$.", "$.", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "O du schimmernde Bl\u00fcte der Panke!", "tokens": ["O", "du", "schim\u00b7mern\u00b7de", "Bl\u00fc\u00b7te", "der", "Pan\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Sei gegr\u00fc\u00dft, du mein sch\u00f6nes Berlin \u2013!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "mein", "sch\u00f6\u00b7nes", "Ber\u00b7lin", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "ADJA", "NE", "$(", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo mit m\u00fcde verzogenen Lippen", "tokens": ["Wo", "mit", "m\u00fc\u00b7de", "ver\u00b7zo\u00b7ge\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJD", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "junger Gent kalten Schleichh\u00e4ndler fri\u00dft \u2013", "tokens": ["jun\u00b7ger", "Gent", "kal\u00b7ten", "Schleich\u00b7h\u00e4nd\u00b7ler", "fri\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "wo Chauffeure die schweinernen Rippen", "tokens": ["wo", "Chauf\u00b7feu\u00b7re", "die", "schwei\u00b7ner\u00b7nen", "Rip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "in die fettige Brihsuppe stippen \u2013", "tokens": ["in", "die", "fet\u00b7ti\u00b7ge", "Brih\u00b7sup\u00b7pe", "stip\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "wo der Fahrgast die Taxe vergi\u00dft . . .", "tokens": ["wo", "der", "Fahr\u00b7gast", "die", "Ta\u00b7xe", "ver\u00b7gi\u00dft", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "+-++-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Da begrabt mich mit Efeugeranke,", "tokens": ["Da", "be\u00b7grabt", "mich", "mit", "E\u00b7feu\u00b7ge\u00b7ran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "mit Ranunkeln und wei\u00dfem Jasmin \u2013 \u2013", "tokens": ["mit", "Ra\u00b7nun\u00b7keln", "und", "wei\u00b7\u00dfem", "Jas\u00b7min", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$(", "$("], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Hier leben? Mensch, welch Gedanke!", "tokens": ["Hier", "le\u00b7ben", "?", "Mensch", ",", "welch", "Ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$.", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "O du schimmernde Bl\u00fcte der Panke!", "tokens": ["O", "du", "schim\u00b7mern\u00b7de", "Bl\u00fc\u00b7te", "der", "Pan\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Sei gegr\u00fc\u00dft, du mein sch\u00f6nes Berlin \u2013!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "mein", "sch\u00f6\u00b7nes", "Ber\u00b7lin", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "ADJA", "NE", "$(", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}