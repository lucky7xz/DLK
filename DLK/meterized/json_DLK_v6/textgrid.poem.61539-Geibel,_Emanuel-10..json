{"textgrid.poem.61539": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Setzt mir, soll ich heiter schl\u00fcrfen,", "tokens": ["Setzt", "mir", ",", "soll", "ich", "hei\u00b7ter", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht den schm\u00e4cht'gen Schoppen her;", "tokens": ["Nicht", "den", "schm\u00e4cht'\u00b7gen", "Schop\u00b7pen", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag ich auch nicht mehr bed\u00fcrfen,", "tokens": ["Mag", "ich", "auch", "nicht", "mehr", "be\u00b7d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch empfinden will ich mehr.", "tokens": ["Doch", "emp\u00b7fin\u00b7den", "will", "ich", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Flaschen la\u00dft mich auf dem Tische,", "tokens": ["Fla\u00b7schen", "la\u00dft", "mich", "auf", "dem", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4sser an den W\u00e4nden sehn.", "tokens": ["F\u00e4s\u00b7ser", "an", "den", "W\u00e4n\u00b7den", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mich gr\u00fcndlich was erfrische,", "tokens": ["Da\u00df", "mich", "gr\u00fcnd\u00b7lich", "was", "er\u00b7fri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PWS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df es aus dem Vollen gehn.", "tokens": ["Mu\u00df", "es", "aus", "dem", "Vol\u00b7len", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}