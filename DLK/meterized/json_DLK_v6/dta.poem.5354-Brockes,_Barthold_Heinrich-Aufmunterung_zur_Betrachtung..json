{"dta.poem.5354": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aufmunterung zur Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wilst du denn, lieber Mensch, der Ueberlegung Kraft,", "tokens": ["Wilst", "du", "denn", ",", "lie\u00b7ber", "Mensch", ",", "der", "Ue\u00b7ber\u00b7le\u00b7gung", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "ADV", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eigentlich der Kern, die n\u00f6thigst\u2019 Eigenschaft", "tokens": ["Die", "ei\u00b7gent\u00b7lich", "der", "Kern", ",", "die", "n\u00f6\u00b7thigst'", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der dir von GOtt geschenckten Seelen,", "tokens": ["Der", "dir", "von", "Gott", "ge\u00b7schenck\u00b7ten", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinen Sinnen nie verm\u00e4hlen?", "tokens": ["Mit", "dei\u00b7nen", "Sin\u00b7nen", "nie", "ver\u00b7m\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit GOtt, in den Gesch\u00f6pfen, nie verbinden?", "tokens": ["Mit", "Gott", ",", "in", "den", "Ge\u00b7sch\u00f6p\u00b7fen", ",", "nie", "ver\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verlangest du in GOttes Wercken", "tokens": ["Ver\u00b7lan\u00b7gest", "du", "in", "Got\u00b7tes", "Wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht seine weise Macht und Liebe zu bemercken?", "tokens": ["Nicht", "sei\u00b7ne", "wei\u00b7se", "Macht", "und", "Lie\u00b7be", "zu", "be\u00b7mer\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Sch\u00f6pfer im Gesch\u00f6pf zu finden?", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "im", "Ge\u00b7sch\u00f6pf", "zu", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er legt viel tausend Herrlichkeiten,", "tokens": ["Er", "legt", "viel", "tau\u00b7send", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Viel Millionen Seltenheiten", "tokens": ["Viel", "Mil\u00b7lion\u00b7en", "Sel\u00b7ten\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf Erden, in der Luft, und in der Fluth dir vor.", "tokens": ["Auf", "Er\u00b7den", ",", "in", "der", "Luft", ",", "und", "in", "der", "Fluth", "dir", "vor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er giebt dir Nas\u2019 und Zung\u2019 und Hand und Aug\u2019 und Ohr.", "tokens": ["Er", "giebt", "dir", "Nas'", "und", "Zung'", "und", "Hand", "und", "Aug'", "und", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er schenckt dir eine Seel\u2019, und eine F\u00e4higkeit,", "tokens": ["Er", "schenckt", "dir", "ei\u00b7ne", "Seel'", ",", "und", "ei\u00b7ne", "F\u00e4\u00b7hig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch h\u00f6ren, riechen, sehn, durch schmecken, und durch", "tokens": ["Durch", "h\u00f6\u00b7ren", ",", "rie\u00b7chen", ",", "sehn", ",", "durch", "schme\u00b7cken", ",", "und", "durch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,", "APPR", "VVINF", "$,", "KON", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "f\u00fchlen", "tokens": ["f\u00fch\u00b7len"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Gedancken und Jdeen zu erzielen.", "tokens": ["Ge\u00b7dan\u00b7cken", "und", "Jdeen", "zu", "er\u00b7zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Da nun des Sch\u00f6pfers Huld so \u00fcberschwenglich ist,", "tokens": ["Da", "nun", "des", "Sch\u00f6p\u00b7fers", "Huld", "so", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und, da das, was dich hier so wol, als dort, vergn\u00fcget,", "tokens": ["Und", ",", "da", "das", ",", "was", "dich", "hier", "so", "wol", ",", "als", "dort", ",", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PDS", "$,", "PWS", "PPER", "ADV", "ADV", "ADV", "$,", "KOUS", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein in der Betrachtung lieget;", "tokens": ["Al\u00b7lein", "in", "der", "Be\u00b7trach\u00b7tung", "lie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Wie, da\u00df du gegen dich denn selbst so grausam bist?", "tokens": ["Wie", ",", "da\u00df", "du", "ge\u00b7gen", "dich", "denn", "selbst", "so", "grau\u00b7sam", "bist", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie, da\u00df du deinen Geist nicht zu den Sinnen f\u00fcgest,", "tokens": ["Wie", ",", "da\u00df", "du", "dei\u00b7nen", "Geist", "nicht", "zu", "den", "Sin\u00b7nen", "f\u00fc\u00b7gest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und durch die\u00df holde Band nicht Seel\u2019 und Leib vergn\u00fcgest?", "tokens": ["Und", "durch", "die\u00df", "hol\u00b7de", "Band", "nicht", "Seel'", "und", "Leib", "ver\u00b7gn\u00fc\u00b7gest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "ADJA", "NN", "PTKNEG", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bestrebe dich forthin, die Wercke zu betrachten,", "tokens": ["Be\u00b7stre\u00b7be", "dich", "for\u00b7thin", ",", "die", "Wer\u00b7cke", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und ihres Sch\u00f6pfers Macht in ihnen zu erh\u00f6hn;", "tokens": ["Und", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Macht", "in", "ih\u00b7nen", "zu", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denn, sonder Dencken sie zu h\u00f6ren und zu sehn;", "tokens": ["Denn", ",", "son\u00b7der", "Den\u00b7cken", "sie", "zu", "h\u00f6\u00b7ren", "und", "zu", "sehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Hei\u00dft G\u00f6ttlichen Befehl, Natur, und GOtt verachten.", "tokens": ["Hei\u00dft", "G\u00f6tt\u00b7li\u00b7chen", "Be\u00b7fehl", ",", "Na\u00b7tur", ",", "und", "Gott", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}