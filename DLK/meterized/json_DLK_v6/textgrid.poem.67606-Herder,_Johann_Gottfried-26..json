{"textgrid.poem.67606": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein stolzer Widder, seiner Heerden", "tokens": ["Ein", "stol\u00b7zer", "Wid\u00b7der", ",", "sei\u00b7ner", "Heer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schutzherr und Zier,", "tokens": ["Schutz\u00b7herr", "und", "Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Wollt' auch am Stier", "tokens": ["Wollt'", "auch", "am", "Stier"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Zum Ritter werden.", "tokens": ["Zum", "Rit\u00b7ter", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie ging's dem sch\u00f6nen Thier?", "tokens": ["Wie", "ging's", "dem", "sch\u00f6\u00b7nen", "Thier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Widder ward nachher der zw\u00f6lfte Karl genannt,", "tokens": ["Der", "Wid\u00b7der", "ward", "nach\u00b7her", "der", "zw\u00f6lf\u00b7te", "Karl", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Stier war K\u00f6nig-Elephant,", "tokens": ["Der", "Stier", "war", "K\u00f6\u00b7nig\u00b7E\u00b7le\u00b7phant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Ein Berg voll Geist, der Erden in sich trug", "tokens": ["Ein", "Berg", "voll", "Geist", ",", "der", "Er\u00b7den", "in", "sich", "trug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und jenen Widder schlug.", "tokens": ["Und", "je\u00b7nen", "Wid\u00b7der", "schlug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}