{"textgrid.poem.61140": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Zur Einleitung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Bl\u00fcten sah ich Tal und H\u00fcgel prangen", "tokens": ["In", "Bl\u00fc\u00b7ten", "sah", "ich", "Tal", "und", "H\u00fc\u00b7gel", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tief im Gr\u00fcn die Spur des Winters schwinden,", "tokens": ["Und", "tief", "im", "Gr\u00fcn", "die", "Spur", "des", "Win\u00b7ters", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ist auch mir mein Denken und Empfinden,", "tokens": ["Da", "ist", "auch", "mir", "mein", "Den\u00b7ken", "und", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lust, Zorn und Lieb' in Liedern aufgegangen.", "tokens": ["Lust", ",", "Zorn", "und", "Lieb'", "in", "Lie\u00b7dern", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft lie\u00df ich auch die Laut' am Aste hangen;", "tokens": ["Oft", "lie\u00df", "ich", "auch", "die", "Laut'", "am", "As\u00b7te", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da kam der Lenz und harfte mit den Winden", "tokens": ["Da", "kam", "der", "Lenz", "und", "harf\u00b7te", "mit", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein St\u00fcck dazwischen, eins von seinen linden,", "tokens": ["Ein", "St\u00fcck", "da\u00b7zwi\u00b7schen", ",", "eins", "von", "sei\u00b7nen", "lin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "PIS", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die wundermild das Menschenohr befangen.", "tokens": ["Die", "wun\u00b7der\u00b7mild", "das", "Men\u00b7schen\u00b7ohr", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Lieder alle hab' ich hier gereiht:", "tokens": ["Die", "Lie\u00b7der", "al\u00b7le", "hab'", "ich", "hier", "ge\u00b7reiht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es ward ein Kranz - ich wand ihn leicht und lose -", "tokens": ["Es", "ward", "ein", "Kranz", "ich", "wand", "ihn", "leicht", "und", "lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "ADJD", "KON", "FM", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bunt wie mein Herz und bunt wie diese Zeit.", "tokens": ["Bunt", "wie", "mein", "Herz", "und", "bunt", "wie", "die\u00b7se", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "KON", "ADJD", "KOKOM", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die hei\u00dfe Tulpe flammt bei dunklem Moose,", "tokens": ["Die", "hei\u00b7\u00dfe", "Tul\u00b7pe", "flammt", "bei", "dunk\u00b7lem", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beim Bl\u00fctenschnee tr\u00e4gt die Zypresse Leid,", "tokens": ["Beim", "Bl\u00fc\u00b7ten\u00b7schnee", "tr\u00e4gt", "die", "Zyp\u00b7res\u00b7se", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und unter wilden Nesseln lauscht die Rose.", "tokens": ["Und", "un\u00b7ter", "wil\u00b7den", "Nes\u00b7seln", "lauscht", "die", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}