{"textgrid.poem.40379": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Dachdecker", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Dachdecker werden,", "tokens": ["Ich", "kann", "ein", "Dach\u00b7de\u00b7cker", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "denn ich bin schwindelfrei.", "tokens": ["denn", "ich", "bin", "schwin\u00b7del\u00b7frei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kletter bis auf den Kirchturmhahn,", "tokens": ["Ich", "klet\u00b7ter", "bis", "auf", "den", "Kirch\u00b7turm\u00b7hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und die Dohlen und Kr\u00e4hn schrein: ei,", "tokens": ["und", "die", "Doh\u00b7len", "und", "Kr\u00e4hn", "schrein", ":", "ei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "PTKVZ", "$.", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "was will der Herr denn hier?", "tokens": ["was", "will", "der", "Herr", "denn", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der will die Kircht\u00fcrme flicken,", "tokens": ["Der", "will", "die", "Kirch\u00b7t\u00fcr\u00b7me", "fli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "++-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "es tut schon lange not!", "tokens": ["es", "tut", "schon", "lan\u00b7ge", "not", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Glocken, wenn mein Fahrstuhl kommt,", "tokens": ["Die", "Glo\u00b7cken", ",", "wenn", "mein", "Fahr\u00b7stuhl", "kommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "brummen: \u00dfapperlot,", "tokens": ["brum\u00b7men", ":", "\u00dfap\u00b7per\u00b7lot", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVINF", "$.", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "da baumelt 'ne Himmelsleiter!", "tokens": ["da", "bau\u00b7melt", "'ne", "Him\u00b7mels\u00b7lei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und unten kribbeln die Leutchen,", "tokens": ["Und", "un\u00b7ten", "krib\u00b7beln", "die", "Leut\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und steigt kein Laut mir nach.", "tokens": ["und", "steigt", "kein", "Laut", "mir", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Blos mein Freund, der Schornsteinfeger,", "tokens": ["Blos", "mein", "Freund", ",", "der", "Schorn\u00b7stein\u00b7fe\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ruft manchmal vom n\u00e4chsten Dach:", "tokens": ["ruft", "manch\u00b7mal", "vom", "n\u00e4chs\u00b7ten", "Dach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Komm, Bruder, es gibt ein Gewitter!", "tokens": ["Komm", ",", "Bru\u00b7der", ",", "es", "gibt", "ein", "Ge\u00b7wit\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Aber dann bleib ich lieber", "tokens": ["A\u00b7ber", "dann", "bleib", "ich", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "ruhig auf meinem Sitz", "tokens": ["ru\u00b7hig", "auf", "mei\u00b7nem", "Sitz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und h\u00f6r, wie der Donner losbr\u00fcllt:", "tokens": ["und", "h\u00f6r", ",", "wie", "der", "Don\u00b7ner", "los\u00b7br\u00fcllt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Bravo! Sieh, Bruder Blitz,", "tokens": ["Bra\u00b7vo", "!", "Sieh", ",", "Bru\u00b7der", "Blitz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "$,", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "das ist ein kleiner Held!", "tokens": ["das", "ist", "ein", "klei\u00b7ner", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}