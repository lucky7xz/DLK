{"textgrid.poem.43537": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das arme V\u00f6glein", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Vogel ruft im Walde,", "tokens": ["Ein", "Vo\u00b7gel", "ruft", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df es wohl, wonach?", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "wo\u00b7nach", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er will ein H\u00e4uschen haben,", "tokens": ["Er", "will", "ein", "H\u00e4usc\u00b7hen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein gr\u00fcnes laubig Dach.", "tokens": ["Ein", "gr\u00fc\u00b7nes", "lau\u00b7big", "Dach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er rufet alle Tage,", "tokens": ["Er", "ru\u00b7fet", "al\u00b7le", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und flattert hin und her,", "tokens": ["Und", "flat\u00b7tert", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem ganzen Walde", "tokens": ["Und", "in", "dem", "gan\u00b7zen", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6rt Keiner sein Begehr.", "tokens": ["H\u00f6rt", "Kei\u00b7ner", "sein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und endlich h\u00f6rt's der Fr\u00fchling,", "tokens": ["Und", "end\u00b7lich", "h\u00f6rt's", "der", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Freund der ganzen Welt,", "tokens": ["Der", "Freund", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der giebt dem armen V\u00f6glein", "tokens": ["Der", "giebt", "dem", "ar\u00b7men", "V\u00f6\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein schattig Laubgezelt.", "tokens": ["Ein", "schat\u00b7tig", "Laub\u00b7ge\u00b7zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer singt im hohen Baume", "tokens": ["Wer", "singt", "im", "ho\u00b7hen", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So froh vom gr\u00fcnen Ast?", "tokens": ["So", "froh", "vom", "gr\u00fc\u00b7nen", "Ast", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das thut das arme V\u00f6glein", "tokens": ["Das", "thut", "das", "ar\u00b7me", "V\u00f6\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus seinem Laubpalast.", "tokens": ["Aus", "sei\u00b7nem", "Laub\u00b7pa\u00b7last", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es singet Dank dem Fr\u00fchling", "tokens": ["Es", "sin\u00b7get", "Dank", "dem", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr das, was er beschied,", "tokens": ["F\u00fcr", "das", ",", "was", "er", "be\u00b7schied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und singt, so lang er weilet,", "tokens": ["Und", "singt", ",", "so", "lang", "er", "wei\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm jeden Tag ein Lied.", "tokens": ["Ihm", "je\u00b7den", "Tag", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}