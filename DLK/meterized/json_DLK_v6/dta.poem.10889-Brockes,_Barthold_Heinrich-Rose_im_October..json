{"dta.poem.10889": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Rose im October.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es r\u00fchret mich, o sp\u00e4tes R\u00f6sgen, dein r\u00f6thlicher Rubinen-\nSchein,", "tokens": ["Es", "r\u00fch\u00b7ret", "mich", ",", "o", "sp\u00e4\u00b7tes", "R\u00f6s\u00b7gen", ",", "dein", "r\u00f6th\u00b7li\u00b7cher", "Ru\u00b7bi\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "TRUNC", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Nicht durch die sch\u00f6ne Farb und Form, auch lieblichen Geruch", "tokens": ["Nicht", "durch", "die", "sch\u00f6\u00b7ne", "Farb", "und", "Form", ",", "auch", "lieb\u00b7li\u00b7chen", "Ge\u00b7ruch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ich werde, da dein edler Balsam, und sch\u00f6ner Schmuck mich", "tokens": ["Ich", "wer\u00b7de", ",", "da", "dein", "ed\u00b7ler", "Bal\u00b7sam", ",", "und", "sch\u00f6\u00b7ner", "Schmuck", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zu dem der dich so sch\u00f6n erschaffen, und dich f\u00fcr mich hervor-", "tokens": ["Zu", "dem", "der", "dich", "so", "sch\u00f6n", "er\u00b7schaf\u00b7fen", ",", "und", "dich", "f\u00fcr", "mich", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "PPER", "ADV", "ADJD", "VVPP", "$,", "KON", "PRF", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Auch mir die Kraft, an ihn zu denken, geschenket, seine Lieb,", "tokens": ["Auch", "mir", "die", "Kraft", ",", "an", "ihn", "zu", "den\u00b7ken", ",", "ge\u00b7schen\u00b7ket", ",", "sei\u00b7ne", "Lieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Und Weisheit fr\u00f6lich zu bewundern, und Jhn zu preisen, an-", "tokens": ["Und", "Weis\u00b7heit", "fr\u00f6\u00b7lich", "zu", "be\u00b7wun\u00b7dern", ",", "und", "Jhn", "zu", "prei\u00b7sen", ",", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "ADJD", "PTKZU", "VVINF", "$,", "KON", "PPER", "PTKZU", "VVINF", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}