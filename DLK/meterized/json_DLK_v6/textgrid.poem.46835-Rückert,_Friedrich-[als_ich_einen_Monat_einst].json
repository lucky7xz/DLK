{"textgrid.poem.46835": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[als ich einen Monat einst]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich einen Monat einst", "tokens": ["Als", "ich", "ei\u00b7nen", "Mo\u00b7nat", "einst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War verreist, und wieder kam,", "tokens": ["War", "ver\u00b7reist", ",", "und", "wie\u00b7der", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie freudig wahr ich nahm,", "tokens": ["O", "wie", "freu\u00b7dig", "wahr", "ich", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie du dich entfaltet!", "tokens": ["Wie", "du", "dich", "ent\u00b7fal\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und wie du mir nun erscheinst,", "tokens": ["Und", "wie", "du", "mir", "nun", "er\u00b7scheinst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Noch ist nicht der Monat gar", "tokens": ["Noch", "ist", "nicht", "der", "Mo\u00b7nat", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PTKNEG", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit du schiedest, nehm ich wahr,", "tokens": ["Seit", "du", "schie\u00b7dest", ",", "nehm", "ich", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du gr\u00f6\u00dfer bist gestaltet.", "tokens": ["Da\u00df", "du", "gr\u00f6\u00b7\u00dfer", "bist", "ge\u00b7stal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dank! ich wei\u00df, wie du es meinst;", "tokens": ["Dank", "!", "ich", "wei\u00df", ",", "wie", "du", "es", "meinst", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachsen willst du mir, als wenn", "tokens": ["Wach\u00b7sen", "willst", "du", "mir", ",", "als", "wenn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "KOKOM", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du mir lebtest; wachse denn,", "tokens": ["Du", "mir", "leb\u00b7test", ";", "wach\u00b7se", "denn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$.", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer \u00e4lter, nie gealtet!", "tokens": ["Im\u00b7mer", "\u00e4l\u00b7ter", ",", "nie", "ge\u00b7al\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn du Beides so vereinst,", "tokens": ["Wenn", "du", "Bei\u00b7des", "so", "ver\u00b7einst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fortgang der Entwickelung,", "tokens": ["Fort\u00b7gang", "der", "Ent\u00b7wi\u00b7cke\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "++--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Und ein Bleiben ewig jung,", "tokens": ["Und", "ein", "Blei\u00b7ben", "e\u00b7wig", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat dein Schicksal sch\u00f6n gewaltet.", "tokens": ["Hat", "dein", "Schick\u00b7sal", "sch\u00f6n", "ge\u00b7wal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn du mich so dereinst", "tokens": ["Und", "wenn", "du", "mich", "so", "de\u00b7reinst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort empf\u00e4ngst, mit holder Scheu,", "tokens": ["Dort", "emp\u00b7f\u00e4ngst", ",", "mit", "hol\u00b7der", "Scheu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchl' ich dich mir alt und neu,", "tokens": ["F\u00fchl'", "ich", "dich", "mir", "alt", "und", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unentfremdet noch erkaltet.", "tokens": ["Un\u00b7en\u00b7tfrem\u00b7det", "noch", "er\u00b7kal\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}