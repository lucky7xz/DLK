{"textgrid.poem.59680": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Goldschwingen trugst du \u2013 o wie goldne Schwingen! \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldschwingen trugst du \u2013 o wie goldne Schwingen! \u2013", "tokens": ["Gold\u00b7schwin\u00b7gen", "trugst", "du", "\u2013", "o", "wie", "gold\u00b7ne", "Schwin\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "FM", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein V\u00f6glein, das so fr\u00fche mir entflogen;", "tokens": ["Mein", "V\u00f6\u00b7glein", ",", "das", "so", "fr\u00fc\u00b7he", "mir", "ent\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJA", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum hat von dir der Glanz sich weggezogen,", "tokens": ["Drum", "hat", "von", "dir", "der", "Glanz", "sich", "weg\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "PPER", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum mu\u00df ich fernhin lauschen ihrem Klingen;", "tokens": ["Drum", "mu\u00df", "ich", "fern\u00b7hin", "lau\u00b7schen", "ih\u00b7rem", "Klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach! fernhin, wo, in sel'gen Lichtes Wogen", "tokens": ["Ach", "!", "fern\u00b7hin", ",", "wo", ",", "in", "sel'\u00b7gen", "Lich\u00b7tes", "Wo\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PTKVZ", "$,", "PWAV", "$,", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Engel badend ", "tokens": ["Die", "En\u00b7gel", "ba\u00b7dend"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ach! fernhin \u2013 Mag so hoch ein Schu\u00df gelingen,", "tokens": ["Ach", "!", "fern\u00b7hin", "\u2013", "Mag", "so", "hoch", "ein", "Schu\u00df", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$(", "VMFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Schmerz und Sehnsucht tun vom Herzensbogen?", "tokens": ["Den", "Schmerz", "und", "Sehn\u00b7sucht", "tun", "vom", "Her\u00b7zens\u00b7bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Meist kommt der Pfeil zur\u00fcck, der nicht getroffen,", "tokens": ["Meist", "kommt", "der", "Pfeil", "zu\u00b7r\u00fcck", ",", "der", "nicht", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Herz und Augen Tr\u00e4nen mir verdunkeln,", "tokens": ["Da\u00df", "Herz", "und", "Au\u00b7gen", "Tr\u00e4\u00b7nen", "mir", "ver\u00b7dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df mir die Ohren wie voll Glocken klingen.", "tokens": ["Da\u00df", "mir", "die", "Oh\u00b7ren", "wie", "voll", "Glo\u00b7cken", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOKOM", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Himmel, wann stehn deine Pforten offen,", "tokens": ["O", "Him\u00b7mel", ",", "wann", "stehn", "dei\u00b7ne", "Pfor\u00b7ten", "of\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da\u00df meine Geister mir entgegenfunkeln,", "tokens": ["Da\u00df", "mei\u00b7ne", "Geis\u00b7ter", "mir", "ent\u00b7ge\u00b7gen\u00b7fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df meine goldnen V\u00f6gel um mich singen?", "tokens": ["Da\u00df", "mei\u00b7ne", "gold\u00b7nen", "V\u00f6\u00b7gel", "um", "mich", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}