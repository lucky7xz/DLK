{"dta.poem.9207": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n Die ungl\u00fcckseligen K\u00fcsse.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ich ein liebes kindgen/", "tokens": ["Wann", "ich", "ein", "lie\u00b7bes", "kind\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mir gewogen ist/", "tokens": ["Das", "mir", "ge\u00b7wo\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf ihr corallen-m\u00fcndgen", "tokens": ["Auf", "ihr", "coral\u00b7len\u00b7m\u00fcnd\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zur kurtzweil nur gek\u00fcst/", "tokens": ["Zur", "kurt\u00b7zweil", "nur", "ge\u00b7k\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So hab ich stets in acht genommen/", "tokens": ["So", "hab", "ich", "stets", "in", "acht", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "CARD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df mir ein ungl\u00fcck drauff gekommen.", "tokens": ["Da\u00df", "mir", "ein", "un\u00b7gl\u00fcck", "drauff", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich armes mensche k\u00fc\u00dfte", "tokens": ["Ich", "ar\u00b7mes", "men\u00b7sche", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6nste Rosilis/", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Ro\u00b7si\u00b7lis", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wann es niemand w\u00fcste;", "tokens": ["Als", "wann", "es", "nie\u00b7mand", "w\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch dieses ist gewi\u00df/", "tokens": ["Doch", "die\u00b7ses", "ist", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich habe sie nach diesem k\u00fcssen", "tokens": ["Ich", "ha\u00b7be", "sie", "nach", "die\u00b7sem", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nunmehr drey jahr vermeiden m\u00fcssen.", "tokens": ["Nun\u00b7mehr", "drey", "jahr", "ver\u00b7mei\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Wann ich mich noch besinne/", "tokens": ["Wann", "ich", "mich", "noch", "be\u00b7sin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du schwartze Marilis/", "tokens": ["Du", "schwart\u00b7ze", "Ma\u00b7ri\u00b7lis", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich bey deinem kinne", "tokens": ["Als", "ich", "bey", "dei\u00b7nem", "kin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein kleines w\u00fcndgen bi\u00df/", "tokens": ["Ein", "klei\u00b7nes", "w\u00fcnd\u00b7gen", "bi\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So hat ich zwar den ku\u00df erworben/", "tokens": ["So", "hat", "ich", "zwar", "den", "ku\u00df", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch unsre freundschafft war verdorben.", "tokens": ["Doch", "uns\u00b7re", "freund\u00b7schafft", "war", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Du freundliche Cythere/", "tokens": ["Du", "freund\u00b7li\u00b7che", "Cy\u00b7the\u00b7re", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein honig-s\u00fcsser mund", "tokens": ["Dein", "ho\u00b7nig\u00b7s\u00fcs\u00b7ser", "mund"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ward mir zu grosser ehre/", "tokens": ["Ward", "mir", "zu", "gros\u00b7ser", "eh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor mich einmahl vergunt/", "tokens": ["Vor", "mich", "ein\u00b7mahl", "ver\u00b7gunt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Jedoch der ku\u00df hat mich gerochen/", "tokens": ["Je\u00b7doch", "der", "ku\u00df", "hat", "mich", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich habe dich nicht mehr gesprochen/", "tokens": ["Ich", "ha\u00b7be", "dich", "nicht", "mehr", "ge\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Es ist mir unvergessen/", "tokens": ["Es", "ist", "mir", "un\u00b7ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wolte nicht mit dir/", "tokens": ["Ich", "wol\u00b7te", "nicht", "mit", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein kind die nase messen/", "tokens": ["Mein", "kind", "die", "na\u00b7se", "mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch wie ergieng es mir/", "tokens": ["Doch", "wie", "er\u00b7gieng", "es", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie fieng der teuffel an zu zischen/", "tokens": ["Wie", "fi\u00b7eng", "der", "teuf\u00b7fel", "an", "zu", "zi\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Was schlug er nicht f\u00fcr noth darzwischen.", "tokens": ["Was", "schlug", "er", "nicht", "f\u00fcr", "noth", "darz\u00b7wi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Drum halte mirs zu gute/", "tokens": ["Drum", "hal\u00b7te", "mirs", "zu", "gu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "APPR", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich nicht k\u00fcssen kan/", "tokens": ["Da\u00df", "ich", "nicht", "k\u00fcs\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann mir ist flugs zu muthe", "tokens": ["Dann", "mir", "ist", "flugs", "zu", "mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00fcrd ich ausgethan/", "tokens": ["Als", "w\u00fcrd", "ich", "aus\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin zu andrer lust erkohren/", "tokens": ["Ich", "bin", "zu", "an\u00b7drer", "lust", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum k\u00fcssen bin ich nicht gebohren.", "tokens": ["Zum", "k\u00fcs\u00b7sen", "bin", "ich", "nicht", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}