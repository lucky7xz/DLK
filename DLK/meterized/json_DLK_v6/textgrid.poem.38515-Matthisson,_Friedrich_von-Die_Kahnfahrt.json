{"textgrid.poem.38515": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kahnfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eilend gleitet der Kahn \u00fcber des Abendsee's", "tokens": ["Ei\u00b7lend", "glei\u00b7tet", "der", "Kahn", "\u00fc\u00b7ber", "des", "A\u00b7bend\u00b7see's"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Sanfterr\u00f6thendes Blau, schwebet, im leichten Tanz,", "tokens": ["Sanf\u00b7ter\u00b7r\u00f6\u00b7then\u00b7des", "Blau", ",", "schwe\u00b7bet", ",", "im", "leich\u00b7ten", "Tanz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Saatgefilden vor\u00fcber,", "tokens": ["Saat\u00b7ge\u00b7fil\u00b7den", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und bebl\u00fctetem Haingeb\u00fcsch!", "tokens": ["Und", "be\u00b7bl\u00fc\u00b7te\u00b7tem", "Hain\u00b7ge\u00b7b\u00fcsch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Freude l\u00e4chelt der Flut blinkendes Angesicht!", "tokens": ["Freu\u00b7de", "l\u00e4\u00b7chelt", "der", "Flut", "blin\u00b7ken\u00b7des", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Freude fl\u00fcstert das Schilf, welches am Ufer wankt!", "tokens": ["Freu\u00b7de", "fl\u00fcs\u00b7tert", "das", "Schilf", ",", "wel\u00b7ches", "am", "U\u00b7fer", "wankt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Freude lispelt die Welle,", "tokens": ["Freu\u00b7de", "lis\u00b7pelt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wenn sie sch\u00e4umend den Nachen k\u00fc\u00dft!", "tokens": ["Wenn", "sie", "sch\u00e4u\u00b7mend", "den", "Na\u00b7chen", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Fl\u00fcgle rascher den Kahn, nervichter J\u00fcnglingsarm!", "tokens": ["Fl\u00fcg\u00b7le", "ra\u00b7scher", "den", "Kahn", ",", "ner\u00b7vich\u00b7ter", "J\u00fcng\u00b7lings\u00b7arm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da\u00df uns Feld und Geb\u00fcsch schneller vor\u00fcberflieh'!", "tokens": ["Da\u00df", "uns", "Feld", "und", "Ge\u00b7b\u00fcsch", "schnel\u00b7ler", "vor\u00b7\u00fcber\u00b7flieh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Jenes gr\u00fcnende Eiland", "tokens": ["Je\u00b7nes", "gr\u00fc\u00b7nen\u00b7de", "Ei\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Winkt zum fr\u00f6hlichen Traubenmahl!", "tokens": ["Winkt", "zum", "fr\u00f6h\u00b7li\u00b7chen", "Trau\u00b7ben\u00b7mahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Seht! wir fliegen heran! Nachtigallton entbebt", "tokens": ["Seht", "!", "wir", "flie\u00b7gen", "he\u00b7ran", "!", "Nach\u00b7ti\u00b7gall\u00b7ton", "ent\u00b7bebt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$.", "NN", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Allen Zweigen umher! Auf! den Pokal bekr\u00e4nzt!", "tokens": ["Al\u00b7len", "Zwei\u00b7gen", "um\u00b7her", "!", "Auf", "!", "den", "Po\u00b7kal", "be\u00b7kr\u00e4nzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$.", "APPR", "$.", "ART", "NN", "VVPP", "$."], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tiefer funkelt im Westen", "tokens": ["Tie\u00b7fer", "fun\u00b7kelt", "im", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schon der freundliche Abendstern!", "tokens": ["Schon", "der", "freund\u00b7li\u00b7che", "A\u00b7bends\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}