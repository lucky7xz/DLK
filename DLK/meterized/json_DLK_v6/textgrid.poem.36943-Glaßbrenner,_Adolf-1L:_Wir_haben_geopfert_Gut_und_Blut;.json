{"textgrid.poem.36943": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir haben geopfert Gut und Blut;", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben geopfert Gut und Blut;", "tokens": ["Wir", "ha\u00b7ben", "ge\u00b7o\u00b7pfert", "Gut", "und", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir haben erk\u00e4mpfet mit hei\u00dfem Muth", "tokens": ["Wir", "ha\u00b7ben", "er\u00b7k\u00e4mp\u00b7fet", "mit", "hei\u00b7\u00dfem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Unsere Freiheit.", "tokens": ["Un\u00b7se\u00b7re", "Frei\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wir haben befestigt der F\u00fcrsten Thron,", "tokens": ["Wir", "ha\u00b7ben", "be\u00b7fes\u00b7tigt", "der", "F\u00fcrs\u00b7ten", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und daf\u00fcr ward uns gerechter Lohn:", "tokens": ["Und", "da\u00b7f\u00fcr", "ward", "uns", "ge\u00b7rech\u00b7ter", "Lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADJD", "NN", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Unsere Freiheit.", "tokens": ["Un\u00b7se\u00b7re", "Frei\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch als die F\u00fcrsten sie n\u00e4her besahn,", "tokens": ["Doch", "als", "die", "F\u00fcrs\u00b7ten", "sie", "n\u00e4\u00b7her", "be\u00b7sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da war mit Fl\u00fcgeln sie angethan,", "tokens": ["Da", "war", "mit", "Fl\u00fc\u00b7geln", "sie", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unsere Freiheit.", "tokens": ["Un\u00b7se\u00b7re", "Frei\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da bildeten sie unter sich einen Bund,", "tokens": ["Da", "bil\u00b7de\u00b7ten", "sie", "un\u00b7ter", "sich", "ei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und hielten gefesselt in ihrem Rund", "tokens": ["Und", "hiel\u00b7ten", "ge\u00b7fes\u00b7selt", "in", "ih\u00b7rem", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Unsere Freiheit.", "tokens": ["Un\u00b7se\u00b7re", "Frei\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie haben in Gnaden nun vorgebeugt,", "tokens": ["Sie", "ha\u00b7ben", "in", "Gna\u00b7den", "nun", "vor\u00b7ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da\u00df nimmer wieder von uns entfleucht", "tokens": ["Da\u00df", "nim\u00b7mer", "wie\u00b7der", "von", "uns", "ent\u00b7fleucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unsere Freiheit.", "tokens": ["Un\u00b7se\u00b7re", "Frei\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie haben nun mit wohlweisem Bedacht", "tokens": ["Sie", "ha\u00b7ben", "nun", "mit", "wohl\u00b7wei\u00b7sem", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Unter Schlo\u00df und unter Riegel gebracht", "tokens": ["Un\u00b7ter", "Schlo\u00df", "und", "un\u00b7ter", "Rie\u00b7gel", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Unsere Freiheit.", "tokens": ["Un\u00b7se\u00b7re", "Frei\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wenn nun der Feind sich wieder erhebt,", "tokens": ["Und", "wenn", "nun", "der", "Feind", "sich", "wie\u00b7der", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann gilt es, da\u00df jeder Unterthan strebt,", "tokens": ["Dann", "gilt", "es", ",", "da\u00df", "je\u00b7der", "Un\u00b7ter\u00b7than", "strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Schreckenden Feuer zu d\u00e4mpfen,", "tokens": ["Der", "Schre\u00b7cken\u00b7den", "Feu\u00b7er", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann \u00f6ffnet man milde das Bundeshaus;", "tokens": ["Dann", "\u00f6ff\u00b7net", "man", "mil\u00b7de", "das", "Bun\u00b7des\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Dann nehmen wir uns die Freiheit heraus \u2013 \u2013", "tokens": ["Dann", "neh\u00b7men", "wir", "uns", "die", "Frei\u00b7heit", "he\u00b7raus", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "F\u00fcr unsere F\u00fcrsten zu k\u00e4mpfen.", "tokens": ["F\u00fcr", "un\u00b7se\u00b7re", "F\u00fcrs\u00b7ten", "zu", "k\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}