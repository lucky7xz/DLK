{"textgrid.poem.50840": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst stand ich mit dem ersten Fr\u00fchlicht auf", "tokens": ["J\u00fcngst", "stand", "ich", "mit", "dem", "ers\u00b7ten", "Fr\u00fch\u00b7licht", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nahm hinaus ins Freie meinen Lauf,", "tokens": ["Und", "nahm", "hin\u00b7aus", "ins", "Frei\u00b7e", "mei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo silbergrau die Morgend\u00e4mmrung lag,", "tokens": ["Wo", "sil\u00b7ber\u00b7grau", "die", "Mor\u00b7gen\u00b7d\u00e4mm\u00b7rung", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umflorend noch den rosenroten Tag.", "tokens": ["Um\u00b7flo\u00b7rend", "noch", "den", "ro\u00b7sen\u00b7ro\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mich einmal satt zu gehen auf den Feldern", "tokens": ["Mich", "ein\u00b7mal", "satt", "zu", "ge\u00b7hen", "auf", "den", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vom Morgen fr\u00fch bis in die sp\u00e4te Nacht,", "tokens": ["Vom", "Mor\u00b7gen", "fr\u00fch", "bis", "in", "die", "sp\u00e4\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein bleibend Lied zu holen in den W\u00e4ldern,", "tokens": ["Ein", "blei\u00b7bend", "Lied", "zu", "ho\u00b7len", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hatt ich zum festen Vorsatz mir gemacht!", "tokens": ["Hatt", "ich", "zum", "fes\u00b7ten", "Vor\u00b7satz", "mir", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Rein war der Morgen, bald zum Tag erhellt;", "tokens": ["Rein", "war", "der", "Mor\u00b7gen", ",", "bald", "zum", "Tag", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der volle Liebespuls schlug durch die Welt,", "tokens": ["Der", "vol\u00b7le", "Lie\u00b7be\u00b7spuls", "schlug", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die L\u00fcfte wehten und der Vogel sang,", "tokens": ["Die", "L\u00fcf\u00b7te", "weh\u00b7ten", "und", "der", "Vo\u00b7gel", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Eichen wuchsen und die Quelle sprang,", "tokens": ["Die", "Ei\u00b7chen", "wuch\u00b7sen", "und", "die", "Quel\u00b7le", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Blumen bl\u00fchten und die Fr\u00fcchte reiften,", "tokens": ["Die", "Blu\u00b7men", "bl\u00fch\u00b7ten", "und", "die", "Fr\u00fcch\u00b7te", "reif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein jeglich Gras tat seinen Odemzug,", "tokens": ["Ein", "jeg\u00b7lich", "Gras", "tat", "sei\u00b7nen", "O\u00b7dem\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Berge standen und die Wolken schweiften,", "tokens": ["Die", "Ber\u00b7ge", "stan\u00b7den", "und", "die", "Wol\u00b7ken", "schweif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und f\u00e4chelnd mich des Lebens Schwinge trug.", "tokens": ["Und", "f\u00e4\u00b7chelnd", "mich", "des", "Le\u00b7bens", "Schwin\u00b7ge", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich schlenderte den lieben Tag entlang,", "tokens": ["Ich", "schlen\u00b7der\u00b7te", "den", "lie\u00b7ben", "Tag", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Herzen schlummerte der Hochgesang;", "tokens": ["Im", "Her\u00b7zen", "schlum\u00b7mer\u00b7te", "der", "Hoch\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es brach sich Bahn der Wachtel leichter Schlag,", "tokens": ["Es", "brach", "sich", "Bahn", "der", "Wach\u00b7tel", "leich\u00b7ter", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jedoch ", "tokens": ["Je\u00b7doch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Es ward Mittag; ich lag an Silberfl\u00fcssen", "tokens": ["Es", "ward", "Mit\u00b7tag", ";", "ich", "lag", "an", "Sil\u00b7ber\u00b7fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sucht die Sonne in der klaren Flut:", "tokens": ["Und", "sucht", "die", "Son\u00b7ne", "in", "der", "kla\u00b7ren", "Flut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich durfte nicht von Angesicht sie gr\u00fc\u00dfen,", "tokens": ["Ich", "durf\u00b7te", "nicht", "von", "An\u00b7ge\u00b7sicht", "sie", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der ich allein in all dem Drang geruht!", "tokens": ["Der", "ich", "al\u00b7lein", "in", "all", "dem", "Drang", "ge\u00b7ruht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Sonne sank und lie\u00df die Welt der Ruh,", "tokens": ["Die", "Son\u00b7ne", "sank", "und", "lie\u00df", "die", "Welt", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Abendnebel gingen ab und zu.", "tokens": ["Die", "A\u00b7bend\u00b7ne\u00b7bel", "gin\u00b7gen", "ab", "und", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Ich lag auf Bergesh\u00f6hen, matt und m\u00fcd,", "tokens": ["Ich", "lag", "auf", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen", ",", "matt", "und", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tief in der Brust das ungesungne Lied:", "tokens": ["Tief", "in", "der", "Brust", "das", "un\u00b7ge\u00b7sung\u00b7ne", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Da nickten, spottend mein, die schlanken Tannen,", "tokens": ["Da", "nick\u00b7ten", ",", "spot\u00b7tend", "mein", ",", "die", "schlan\u00b7ken", "Tan\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "PPOSAT", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und h\u00f6hnisch sah der Erde Moos empor", "tokens": ["Und", "h\u00f6h\u00b7nisch", "sah", "der", "Er\u00b7de", "Moos", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit seinen W\u00fcrmern, die dar\u00fcber spannen,", "tokens": ["Mit", "sei\u00b7nen", "W\u00fcr\u00b7mern", ",", "die", "da\u00b7r\u00fc\u00b7ber", "span\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und lachend brach das Firmament hervor!", "tokens": ["Und", "la\u00b7chend", "brach", "das", "Fir\u00b7ma\u00b7ment", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von Osten wehte rein und scharf der Wind:", "tokens": ["Von", "Os\u00b7ten", "weh\u00b7te", "rein", "und", "scharf", "der", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwas suchst du hier, armselig Menschenkind!", "tokens": ["\u00bb", "was", "suchst", "du", "hier", ",", "arm\u00b7se\u00b7lig", "Men\u00b7schen\u00b7kind", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$,", "ADJD", "NN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du stumme Pfeife in dem Orgelchor,", "tokens": ["Du", "stum\u00b7me", "Pfei\u00b7fe", "in", "dem", "Or\u00b7gel\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du Schlemihl, der da Raum und Zeit verlor?", "tokens": ["Du", "Schle\u00b7mihl", ",", "der", "da", "Raum", "und", "Zeit", "ver\u00b7lor", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dir ward das Leichteste, das Lied, gegeben,", "tokens": ["Dir", "ward", "das", "Leich\u00b7tes\u00b7te", ",", "das", "Lied", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das, selbst sich bauend, aus der Kehle bricht:", "tokens": ["Das", ",", "selbst", "sich", "bau\u00b7end", ",", "aus", "der", "Keh\u00b7le", "bricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "PRF", "VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du aber legst dein unbeholfen Leben,", "tokens": ["Du", "a\u00b7ber", "legst", "dein", "un\u00b7be\u00b7hol\u00b7fen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie einen Stein, ihm auf den Weg zum Licht!\u00ab", "tokens": ["Wie", "ei\u00b7nen", "Stein", ",", "ihm", "auf", "den", "Weg", "zum", "Licht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So sprach der Wind? \u2013 O nein, so sprach der Schmerz,", "tokens": ["So", "sprach", "der", "Wind", "?", "\u2013", "O", "nein", ",", "so", "sprach", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NE", "PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der mir wie Ketten hing ums dunkle Herz.", "tokens": ["Der", "mir", "wie", "Ket\u00b7ten", "hing", "ums", "dunk\u00b7le", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein fremder K\u00f6rper, ohne Form und Schall,", "tokens": ["Ein", "frem\u00b7der", "K\u00f6r\u00b7per", ",", "oh\u00b7ne", "Form", "und", "Schall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So, deucht' mir, lag ich im lebend'gen All!", "tokens": ["So", ",", "deucht'", "mir", ",", "lag", "ich", "im", "le\u00b7ben\u00b7d'\u00b7gen", "All", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und Wind und Tannen, Berge, Moos und Sterne,", "tokens": ["Und", "Wind", "und", "Tan\u00b7nen", ",", "Ber\u00b7ge", ",", "Moos", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie schlangen l\u00e4chelnd ihren weiten Kranz;", "tokens": ["Sie", "schlan\u00b7gen", "l\u00e4\u00b7chelnd", "ih\u00b7ren", "wei\u00b7ten", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie an der Insel in der Meeresferne", "tokens": ["Wie", "an", "der", "In\u00b7sel", "in", "der", "Mee\u00b7res\u00b7fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Brach sich an mir der friedlich milde Glanz.", "tokens": ["Brach", "sich", "an", "mir", "der", "fried\u00b7lich", "mil\u00b7de", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}