{"dta.poem.4196": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der vern\u00fcnftige Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit innniglich-ger\u00fchrter Brust,", "tokens": ["Mit", "in\u00b7nnig\u00b7lich\u00b7ge\u00b7r\u00fchr\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und gleichsam \u00fcberschwemmt von Lust,", "tokens": ["Und", "gleich\u00b7sam", "\u00fc\u00b7bersc\u00b7hwemmt", "von", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Roch ich j\u00fcngst mehr als hundertmahl", "tokens": ["Roch", "ich", "j\u00fcngst", "mehr", "als", "hun\u00b7dert\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den kr\u00e4ftig-recht durchw\u00fcrzten Dust", "tokens": ["Den", "kr\u00e4f\u00b7tig\u00b7recht", "durch\u00b7w\u00fcrz\u00b7ten", "Dust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von der Violen Matronal.", "tokens": ["Von", "der", "Vi\u00b7o\u00b7len", "Mat\u00b7ro\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um recht ges\u00e4ttiget zu seyn,", "tokens": ["Um", "recht", "ge\u00b7s\u00e4t\u00b7ti\u00b7get", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zog ich die balsamirte Luft,", "tokens": ["Zog", "ich", "die", "bal\u00b7sa\u00b7mir\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit stets erneuertem Vergn\u00fcgen,", "tokens": ["Mit", "stets", "er\u00b7neu\u00b7er\u00b7tem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In kurzen unterbrochnen Z\u00fcgen,", "tokens": ["In", "kur\u00b7zen", "un\u00b7ter\u00b7broch\u00b7nen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bald hauchend und bald schnaufend, ein.", "tokens": ["Bald", "hau\u00b7chend", "und", "bald", "schnau\u00b7fend", ",", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um Lust und Dank nun auch zu f\u00fcgen;", "tokens": ["Um", "Lust", "und", "Dank", "nun", "auch", "zu", "f\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Beschlo\u00df ich, wenn ich roch und blies,", "tokens": ["Be\u00b7schlo\u00df", "ich", ",", "wenn", "ich", "roch", "und", "blies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Athem zog und von mir lie\u00df,", "tokens": ["Den", "A\u00b7them", "zog", "und", "von", "mir", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bey jedem Anziehn, jedem Hauchen", "tokens": ["Bey", "je\u00b7dem", "An\u00b7ziehn", ",", "je\u00b7dem", "Hau\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mich einer Sylbe zu gebrauchen", "tokens": ["Mich", "ei\u00b7ner", "Syl\u00b7be", "zu", "ge\u00b7brau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von diesem Liedchen, welches man", "tokens": ["Von", "die\u00b7sem", "Lied\u00b7chen", ",", "wel\u00b7ches", "man"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auch leicht, wie ich, verrichten kann.", "tokens": ["Auch", "leicht", ",", "wie", "ich", ",", "ver\u00b7rich\u00b7ten", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir - riech' - ich - die-se - sch\u00f6-ne - Bluh-me,", "tokens": ["Dir", "riech'", "ich", "die\u00b7se", "sch\u00f6\u00b7ne", "Bluh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVFIN", "$(", "PPER", "$(", "PDAT", "$(", "ADJA", "$(", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O - GOtt, - Der - sie - mir - schenkt, - zum - Ruh-me.", "tokens": ["O", "Gott", ",", "Der", "sie", "mir", "schenkt", ",", "zum", "Ruh\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$,", "$(", "ART", "$(", "PPER", "$(", "PPER", "$(", "VVFIN", "$,", "$(", "APPRART", "$(", "NN", "$."], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ich - riech' - und - freu' - mich - Dein - in - ihr;", "tokens": ["Ich", "riech'", "und", "freu'", "mich", "Dein", "in", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVFIN", "$(", "KON", "$(", "VVIMP", "$(", "PPER", "$(", "PPOSAT", "$(", "APPR", "$(", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Denn - Du - al-lein - for-mierst - und - gie-best,", "tokens": ["Denn", "Du", "al\u00b7lein", "for\u00b7mierst", "und", "gie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "$(", "NE", "$(", "FM", "$(", "KON", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zur - Pro-be, - wie - so - stark - Du - lie-best,", "tokens": ["Zur", "Pro\u00b7be", ",", "wie", "so", "stark", "Du", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "$(", "NN", "$,", "$(", "PWAV", "$(", "ADV", "$(", "ADJD", "$(", "PPER", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der - Bluh-men - Pracht, - Ge-ruch - und - Zier.", "tokens": ["Der", "Bluh\u00b7men", "Pracht", ",", "Ge\u00b7ruch", "und", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "NN", "$,", "$(", "NN", "$(", "KON", "$(", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die - Kraft - zu - rie-chen - schenkst - Du - mir.", "tokens": ["Die", "Kraft", "zu", "rie\u00b7chen", "schenkst", "Du", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "PTKZU", "$(", "ADJA", "$(", "VVFIN", "$(", "PPER", "$(", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}