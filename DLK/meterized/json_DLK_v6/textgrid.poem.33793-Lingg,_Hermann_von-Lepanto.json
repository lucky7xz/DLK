{"textgrid.poem.33793": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Lepanto", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bang schon vor dem Schlachtengotte", "tokens": ["Bang", "schon", "vor", "dem", "Schlach\u00b7ten\u00b7got\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lag das weite Meer und schwieg,", "tokens": ["Lag", "das", "wei\u00b7te", "Meer", "und", "schwieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die gro\u00dfe T\u00fcrkenflotte", "tokens": ["Und", "die", "gro\u00b7\u00dfe", "T\u00fcr\u00b7ken\u00b7flot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4umte sich schon Ruhm und Sieg.", "tokens": ["Tr\u00e4um\u00b7te", "sich", "schon", "Ruhm", "und", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von des Nil und Euphrat Wogen", "tokens": ["Von", "des", "Nil", "und", "Eu\u00b7ph\u00b7rat", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NE", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Waren Schiffe hergezogen", "tokens": ["Wa\u00b7ren", "Schif\u00b7fe", "her\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von Fez und Trebisond;", "tokens": ["Und", "von", "Fez", "und", "Tre\u00b7bi\u00b7sond", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weithin auf den blauen Wellen", "tokens": ["Weit\u00b7hin", "auf", "den", "blau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sah man ihre Flaggen schwellen,", "tokens": ["Sah", "man", "ih\u00b7re", "Flag\u00b7gen", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Blutrot, mit dem halben Mond.", "tokens": ["Blut\u00b7rot", ",", "mit", "dem", "hal\u00b7ben", "Mond", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Schwarz und finster wie der B\u00f6se,", "tokens": ["Schwarz", "und", "fins\u00b7ter", "wie", "der", "B\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Seele voll Verrat,", "tokens": ["Mit", "der", "See\u00b7le", "voll", "Ver\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stand am Mast der Calabrese", "tokens": ["Stand", "am", "Mast", "der", "Ca\u00b7lab\u00b7re\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Nyzalin, der Renegat;", "tokens": ["Ny\u00b7za\u00b7lin", ",", "der", "Re\u00b7ne\u00b7gat", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbheut im Blut der Nazarener", "tokens": ["\u00bb", "heut", "im", "Blut", "der", "Na\u00b7za\u00b7re\u00b7ner"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Trinkt euch satt, ihr Damaszener,\u00ab", "tokens": ["Trinkt", "euch", "satt", ",", "ihr", "Da\u00b7mas\u00b7ze\u00b7ner", ",", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$,", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Sprachen die Wesire schon;", "tokens": ["Spra\u00b7chen", "die", "We\u00b7si\u00b7re", "schon", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Doch im Himmel war's beschlossen,", "tokens": ["Doch", "im", "Him\u00b7mel", "wa\u00b7r's", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Da\u00df zerschmettert und zerschossen", "tokens": ["Da\u00df", "zer\u00b7schmet\u00b7tert", "und", "zer\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "St\u00fcrzen sollt' ihr Wellenthron.", "tokens": ["St\u00fcr\u00b7zen", "sollt'", "ihr", "Wel\u00b7len\u00b7thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als der Tag war angebrochen,", "tokens": ["Als", "der", "Tag", "war", "an\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der T\u00fcrk' in Donnerschall", "tokens": ["Hat", "der", "T\u00fcrk'", "in", "Don\u00b7ner\u00b7schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbkommt heran!\u00ab zu uns gesprochen", "tokens": ["\u00bb", "kommt", "he\u00b7ran", "!", "\u00ab", "zu", "uns", "ge\u00b7spro\u00b7chen"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "$(", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den Feuerschl\u00fcnden all.", "tokens": ["Aus", "den", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den", "all", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Antwort gab im Morgenstrahle", "tokens": ["Ant\u00b7wort", "gab", "im", "Mor\u00b7gen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoch vom Mast der Admirale", "tokens": ["Hoch", "vom", "Mast", "der", "Ad\u00b7mi\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unser Kreuzpanier sogleich,", "tokens": ["Un\u00b7ser", "Kreuz\u00b7pa\u00b7nier", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und nun sprach: \u00bbMit Gottes Wettern", "tokens": ["Und", "nun", "sprach", ":", "\u00bb", "Mit", "Got\u00b7tes", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$.", "$(", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "La\u00dft uns jetzt den Feind zerschmettern!\u00ab", "tokens": ["La\u00dft", "uns", "jetzt", "den", "Feind", "zer\u00b7schmet\u00b7tern", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Don Juan von \u00d6sterreich.", "tokens": ["Don", "Juan", "von", "\u00d6s\u00b7ter\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da begann das gro\u00dfe Feuern,", "tokens": ["Da", "be\u00b7gann", "das", "gro\u00b7\u00dfe", "Feu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die metallnen Schlangen spie'n,", "tokens": ["Die", "me\u00b7tall\u00b7nen", "Schlan\u00b7gen", "spie'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings von Masten, Tauwerk, Steuern", "tokens": ["Rings", "von", "Mas\u00b7ten", ",", "Tau\u00b7werk", ",", "Steu\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flog es in die Luft dahin.", "tokens": ["Flog", "es", "in", "die", "Luft", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jetzt geentert! ward befohlen,", "tokens": ["Jetzt", "ge\u00b7en\u00b7tert", "!", "ward", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drauf mit S\u00e4beln und Pistolen", "tokens": ["Drauf", "mit", "S\u00e4\u00b7beln", "und", "Pis\u00b7to\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Donnernd wirft sich Bord an Bord,", "tokens": ["Don\u00b7nernd", "wirft", "sich", "Bord", "an", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mann an Mann; durch Luck' und Decke,", "tokens": ["Mann", "an", "Mann", ";", "durch", "Luck'", "und", "De\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch Kaj\u00fcten und Verstecke", "tokens": ["Durch", "Ka\u00b7j\u00fc\u00b7ten", "und", "Ver\u00b7ste\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "\u00dcber Leichen ras't der Mord.", "tokens": ["\u00dc\u00b7ber", "Lei\u00b7chen", "ras't", "der", "Mord", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wilder wirft das Meer die Fluten,", "tokens": ["Wil\u00b7der", "wirft", "das", "Meer", "die", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zornig g\u00e4rt sein Drachenschlund,", "tokens": ["Zor\u00b7nig", "g\u00e4rt", "sein", "Dra\u00b7chen\u00b7schlund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Stern in Feuersgluten", "tokens": ["Wie", "ein", "Stern", "in", "Feu\u00b7ers\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinkt des Pascha Schiff zu Grund.", "tokens": ["Sinkt", "des", "Pa\u00b7scha", "Schiff", "zu", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Feuer regnet's auf die Mohren,", "tokens": ["Feu\u00b7er", "reg\u00b7net's", "auf", "die", "Moh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allem ist der Tod geschworen,", "tokens": ["Al\u00b7lem", "ist", "der", "Tod", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was noch aus den Wellen taucht;", "tokens": ["Was", "noch", "aus", "den", "Wel\u00b7len", "taucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Aber Sieg dem Christenvolke!", "tokens": ["A\u00b7ber", "Sieg", "dem", "Chris\u00b7ten\u00b7vol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jubelt's durch die Pulverwolke,", "tokens": ["Ju\u00b7belt's", "durch", "die", "Pul\u00b7ver\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Stille wird's, der Dampf verraucht.", "tokens": ["Stil\u00b7le", "wird's", ",", "der", "Dampf", "ver\u00b7raucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Spanier, Deutsche, Johanniter,", "tokens": ["Spa\u00b7nier", ",", "Deut\u00b7sche", ",", "Jo\u00b7han\u00b7ni\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer bewies den h\u00f6chsten Mut?", "tokens": ["Wer", "be\u00b7wies", "den", "h\u00f6chs\u00b7ten", "Mut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder K\u00e4mpfer war ein Ritter,", "tokens": ["Je\u00b7der", "K\u00e4mp\u00b7fer", "war", "ein", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Harnisch troff von Blut;", "tokens": ["Je\u00b7der", "Har\u00b7nisch", "troff", "von", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Don Colonna, Don Farnese,", "tokens": ["Don", "Co\u00b7lon\u00b7na", ",", "Don", "Far\u00b7ne\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gro\u00df im Heldenbuche lese", "tokens": ["Gro\u00df", "im", "Hel\u00b7den\u00b7bu\u00b7che", "le\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jede fernste Nachwelt euch;", "tokens": ["Je\u00b7de", "ferns\u00b7te", "Nach\u00b7welt", "euch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch der h\u00f6chste Stern der Ehren", "tokens": ["Doch", "der", "h\u00f6chs\u00b7te", "Stern", "der", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Gl\u00e4nzt euch, spanische Galeeren:", "tokens": ["Gl\u00e4nzt", "euch", ",", "spa\u00b7ni\u00b7sche", "Ga\u00b7lee\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": 