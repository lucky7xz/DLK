{"dta.poem.200": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er die Ehre hatte selbige unter den Nah-  \n men  Jbrahim  zu fahren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckter Jbrahim, der du in rauhen Zeiten", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Jbra\u00b7him", ",", "der", "du", "in", "rau\u00b7hen", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Der Anmuth holden Lentz durch Schnee und Ey\u00df kanst", "tokens": ["Der", "An\u00b7muth", "hol\u00b7den", "Lentz", "durch", "Schnee", "und", "Ey\u00df", "kanst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "leiten/", "tokens": ["lei\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Der du mit Purpur-Schmuck bemarmeln kanst den Schnee/", "tokens": ["Der", "du", "mit", "Pur\u00b7pur\u00b7Schmuck", "be\u00b7mar\u00b7meln", "kanst", "den", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "VMFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und Rosen wachsen stehst auf der beschneeten H\u00f6h", "tokens": ["Und", "Ro\u00b7sen", "wach\u00b7sen", "stehst", "auf", "der", "be\u00b7schnee\u00b7ten", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Alabaster Brust/ der du der Lippen-Nelcken/", "tokens": ["Der", "A\u00b7la\u00b7bas\u00b7ter", "Brust", "/", "der", "du", "der", "Lip\u00b7pen\u00b7Nel\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und Wangen-Liljen siehst in keinem Schnee verwelcken/", "tokens": ["Und", "Wan\u00b7gen\u00b7Lil\u00b7jen", "siehst", "in", "kei\u00b7nem", "Schnee", "ver\u00b7wel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der du den sanfften Strahl der Augen spielen stehst/", "tokens": ["Der", "du", "den", "sanff\u00b7ten", "Strahl", "der", "Au\u00b7gen", "spie\u00b7len", "stehst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und deren ", "tokens": ["Und", "de\u00b7ren"], "token_info": ["word", "word"], "pos": ["KON", "PDS"], "meter": "+--", "measure": "dactylic.init"}, "line.10": {"text": "Doch w\u00fcrdest du noch mehr der sch\u00f6nsten Sachen sehen/", "tokens": ["Doch", "w\u00fcr\u00b7dest", "du", "noch", "mehr", "der", "sch\u00f6ns\u00b7ten", "Sa\u00b7chen", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn du als ", "tokens": ["Wenn", "du", "als"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Allwo ein Balsam-Busch mit Zucker Rosen bl\u00fcht/", "tokens": ["All\u00b7wo", "ein", "Bal\u00b7sam\u00b7Busch", "mit", "Zu\u00b7cker", "Ro\u00b7sen", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu dem die Reitzung uns/ wie Agt das Spreu hinzieht.", "tokens": ["Zu", "dem", "die", "Reit\u00b7zung", "uns", "/", "wie", "Agt", "das", "Spreu", "hin\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "$(", "KOKOM", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}