{"dta.poem.23800": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Todes-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Das/ was der Erden weite Raum", "tokens": ["Das", "/", "was", "der", "Er\u00b7den", "wei\u00b7te", "Raum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "PWS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begreifft in seinen Schrancken/", "tokens": ["Be\u00b7greifft", "in", "sei\u00b7nen", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verfleucht als wie ein leichter Traum;", "tokens": ["Ver\u00b7fleucht", "als", "wie", "ein", "leich\u00b7ter", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich selbst/ dem die Gedancken", "tokens": ["Ich", "selbst", "/", "dem", "die", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$(", "ART", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Nichtigkeit itzt fallen ein/", "tokens": ["Der", "Nich\u00b7tig\u00b7keit", "itzt", "fal\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kan vielleicht der nechste seyn/", "tokens": ["Ich", "kan", "viel\u00b7leicht", "der", "nechs\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von abgek\u00fcrtztem Leben", "tokens": ["Von", "ab\u00b7ge\u00b7k\u00fcrtz\u00b7tem", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Beyspiel abzugeben.", "tokens": ["Ein", "Bey\u00b7spiel", "ab\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bin ich aus besserm Zeug gebaut", "tokens": ["Bin", "ich", "aus", "bes\u00b7serm", "Zeug", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als andre meiner Jahre/", "tokens": ["Als", "and\u00b7re", "mei\u00b7ner", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die man noch gestern frisch geschaut/", "tokens": ["Die", "man", "noch", "ge\u00b7stern", "frisch", "ge\u00b7schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und heut legt auff die Baare?", "tokens": ["Und", "heut", "legt", "auff", "die", "Baa\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zu was dient mir der Nahrungs-Safft/", "tokens": ["Zu", "was", "dient", "mir", "der", "Nah\u00b7rungs\u00b7\u00b7S\u00b7afft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Als da\u00df er neuen Zunder schafft/", "tokens": ["Als", "da\u00df", "er", "neu\u00b7en", "Zun\u00b7der", "schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der/ wenn es GOtt verh\u00e4nget/", "tokens": ["Der", "/", "wenn", "es", "Gott", "ver\u00b7h\u00e4n\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Leicht Gifft und Kranckheit f\u00e4nget.", "tokens": ["Leicht", "Gifft", "und", "Kran\u00b7ck\u00b7heit", "f\u00e4n\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Alsdann gibts keine ", "tokens": ["Als\u00b7dann", "gibts", "kei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den Schaden zu erg\u00e4ntzen.", "tokens": ["Den", "Scha\u00b7den", "zu", "er\u00b7g\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sehn der Haare Silber-Schnee/", "tokens": ["Wir", "sehn", "der", "Haa\u00b7re", "Sil\u00b7ber\u00b7Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auff wenig Scheiteln gl\u00e4ntzen.", "tokens": ["Auff", "we\u00b7nig", "Schei\u00b7teln", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Der Tod/ der ist es so gewohnt/", "tokens": ["Der", "Tod", "/", "der", "ist", "es", "so", "ge\u00b7wohnt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er der Jugend wenig schont/", "tokens": ["Da\u00df", "er", "der", "Ju\u00b7gend", "we\u00b7nig", "schont", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und die noch harte Trauben/", "tokens": ["Und", "die", "noch", "har\u00b7te", "Trau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Am liebsten pflegt zu rauben.", "tokens": ["Am", "liebs\u00b7ten", "pflegt", "zu", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was mehr ist/ manchem wird das Hertz", "tokens": ["Was", "mehr", "ist", "/", "man\u00b7chem", "wird", "das", "Hertz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "$(", "PIAT", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch seinen Griff ger\u00fchret/", "tokens": ["Durch", "sei\u00b7nen", "Griff", "ge\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Eh er noch Schwachheit oder Schmertz/", "tokens": ["Eh", "er", "noch", "Schwach\u00b7heit", "o\u00b7der", "Schmertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Als seine Boten sp\u00fchret.", "tokens": ["Als", "sei\u00b7ne", "Bo\u00b7ten", "sp\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es sind ja leyder! Schlag und Fluth/", "tokens": ["Es", "sind", "ja", "ley\u00b7der", "!", "Schlag", "und", "Fluth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$.", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gescho\u00df/ Wurff/ Stickflu\u00df/ Mord und Gluth/", "tokens": ["Ge\u00b7scho\u00df", "/", "Wurff", "/", "Stick\u00b7flu\u00df", "/", "Mord", "und", "Gluth", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und F\u00e4lle vieler Arten/", "tokens": ["Und", "F\u00e4l\u00b7le", "vie\u00b7ler", "Ar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die st\u00fcndlich auff uns warten.", "tokens": ["Die", "st\u00fcnd\u00b7lich", "auff", "uns", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dieweil nun alles dis/ mein GOtt/", "tokens": ["Die\u00b7weil", "nun", "al\u00b7les", "dis", "/", "mein", "Gott", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "PDS", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir vor den Augen schwebet/", "tokens": ["Mir", "vor", "den", "Au\u00b7gen", "schwe\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wie kommts/ da\u00df nicht in dieser Noth", "tokens": ["Wie", "kommts", "/", "da\u00df", "nicht", "in", "die\u00b7ser", "Noth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$(", "KOUS", "PTKNEG", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein tr\u00e4ger C\u00f6rper bebet/", "tokens": ["Mein", "tr\u00e4\u00b7ger", "C\u00f6r\u00b7per", "be\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und da\u00df die Seele ruhig ist/", "tokens": ["Und", "da\u00df", "die", "See\u00b7le", "ru\u00b7hig", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als h\u00e4tte sie noch lange frist/", "tokens": ["Als", "h\u00e4t\u00b7te", "sie", "noch", "lan\u00b7ge", "frist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wie in fremden Sachen/", "tokens": ["So", "wie", "in", "frem\u00b7den", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Uberschlag zu machen?", "tokens": ["Den", "U\u00b7ber\u00b7schlag", "zu", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O kindischer und toller Wahn/", "tokens": ["O", "kin\u00b7di\u00b7scher", "und", "tol\u00b7ler", "Wahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der bey mir eingerissen!", "tokens": ["Der", "bey", "mir", "ein\u00b7ge\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df gewi\u00df/ ich mu\u00df daran/", "tokens": ["Ich", "wei\u00df", "ge\u00b7wi\u00df", "/", "ich", "mu\u00df", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur wil ich es nicht wissen.", "tokens": ["Nur", "wil", "ich", "es", "nicht", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie manch ber\u00fchmtes Haupt geht ab!", "tokens":