{"dta.poem.20363": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Lipsii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich f\u00fchrt die kluge welt im hertzen/ mund und hand/", "tokens": ["Mich", "f\u00fchrt", "die", "klu\u00b7ge", "welt", "im", "hert\u00b7zen", "/", "mund", "und", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Flandern ruht der leib/ den ruhm beh\u00e4lt Braband.", "tokens": ["Zu", "Flan\u00b7dern", "ruht", "der", "leib", "/", "den", "ruhm", "be\u00b7h\u00e4lt", "Bra\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man lobte noch weit mehr die hoheit meiner sinnen/", "tokens": ["Man", "lob\u00b7te", "noch", "weit", "mehr", "die", "ho\u00b7heit", "mei\u00b7ner", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn ich im alter nur zwey weiber", "tokens": ["Wenn", "ich", "im", "al\u00b7ter", "nur", "zwey", "wei\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}