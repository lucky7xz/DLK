{"textgrid.poem.57670": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Alle Birken gr\u00fcnen ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Birken gr\u00fcnen in Moor und Heid,", "tokens": ["Al\u00b7le", "Bir\u00b7ken", "gr\u00fc\u00b7nen", "in", "Moor", "und", "Heid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jeder Brahmbusch leuchtet wie Gold,", "tokens": ["Je\u00b7der", "Brahm\u00b7busch", "leuch\u00b7tet", "wie", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Alle Heidlerchen dudeln vor Fr\u00f6hlichkeit,", "tokens": ["Al\u00b7le", "Heid\u00b7ler\u00b7chen", "du\u00b7deln", "vor", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--++-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Jeder Birkhahn kullert und tollt.", "tokens": ["Je\u00b7der", "Birk\u00b7hahn", "kul\u00b7lert", "und", "tollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Meine Augen, die gehen wohl hin und her", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", ",", "die", "ge\u00b7hen", "wohl", "hin", "und", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ADV", "PTKVZ", "KON", "ADV"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Auf dem schwarzen, wei\u00dfflockigen Moor,", "tokens": ["Auf", "dem", "schwar\u00b7zen", ",", "wei\u00df\u00b7flo\u00b7cki\u00b7gen", "Moor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf dem braunen, gr\u00fcnsch\u00e4umenden Heidemeer", "tokens": ["Auf", "dem", "brau\u00b7nen", ",", "gr\u00fcn\u00b7sch\u00e4u\u00b7men\u00b7den", "Hei\u00b7de\u00b7meer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "--+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und schweben zum Himmel empor.", "tokens": ["Und", "schwe\u00b7ben", "zum", "Him\u00b7mel", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Zum Blauhimmel hin, wo ein W\u00f6lkchen zieht", "tokens": ["Zum", "Blau\u00b7him\u00b7mel", "hin", ",", "wo", "ein", "W\u00f6lk\u00b7chen", "zieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie ein Wollgrasfl\u00f6ckchen so leicht,", "tokens": ["Wie", "ein", "Woll\u00b7gras\u00b7fl\u00f6ck\u00b7chen", "so", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und mein Herz, es singt sein leises Lied,", "tokens": ["Und", "mein", "Herz", ",", "es", "singt", "sein", "lei\u00b7ses", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das auf zum Himmel steigt.", "tokens": ["Das", "auf", "zum", "Him\u00b7mel", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein leises Lied, ein stilles Lied", "tokens": ["Ein", "lei\u00b7ses", "Lied", ",", "ein", "stil\u00b7les", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied, so fein und lind,", "tokens": ["Ein", "Lied", ",", "so", "fein", "und", "lind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ein W\u00f6lckchen, das \u00fcber die Bl\u00e4ue zieht,", "tokens": ["Wie", "ein", "W\u00f6l\u00b7ck\u00b7chen", ",", "das", "\u00fc\u00b7ber", "die", "Bl\u00e4u\u00b7e", "zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie ein Wollgrasfl\u00f6ckchen im Wind.", "tokens": ["Wie", "ein", "Woll\u00b7gras\u00b7fl\u00f6ck\u00b7chen", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}