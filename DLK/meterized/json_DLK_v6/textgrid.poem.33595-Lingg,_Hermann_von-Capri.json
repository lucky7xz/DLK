{"textgrid.poem.33595": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Capri", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Abend kamen die Winde frischer,", "tokens": ["Am", "A\u00b7bend", "ka\u00b7men", "die", "Win\u00b7de", "fri\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir fuhren das holde Capri vorbei.", "tokens": ["Wir", "fuh\u00b7ren", "das", "hol\u00b7de", "Cap\u00b7ri", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Barcarole sang ein Fischer", "tokens": ["Die", "Bar\u00b7ca\u00b7ro\u00b7le", "sang", "ein", "Fi\u00b7scher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hing sein tropfend Ruder bei.", "tokens": ["Und", "hing", "sein", "trop\u00b7fend", "Ru\u00b7der", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwei Vorgebirge, die Bucht umragend,", "tokens": ["Zwei", "Vor\u00b7ge\u00b7bir\u00b7ge", ",", "die", "Bucht", "um\u00b7ra\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erhoben, von Kaktus und Wein umlaubt,", "tokens": ["Er\u00b7ho\u00b7ben", ",", "von", "Kak\u00b7tus", "und", "Wein", "um\u00b7laubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Vorzeit Mauerkronen tragend,", "tokens": ["Der", "Vor\u00b7zeit", "Mau\u00b7er\u00b7kro\u00b7nen", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sonnverbranntes Felsenhaupt.", "tokens": ["Ihr", "sonn\u00b7ver\u00b7brann\u00b7tes", "Fel\u00b7sen\u00b7haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort dr\u00fcben die Villa des R\u00f6mertyrannen,", "tokens": ["Dort", "dr\u00fc\u00b7ben", "die", "Vil\u00b7la", "des", "R\u00f6\u00b7mer\u00b7ty\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein w\u00fcster, zertr\u00fcmmerter Steinkolo\u00df,", "tokens": ["Ein", "w\u00fcs\u00b7ter", ",", "zer\u00b7tr\u00fcm\u00b7mer\u00b7ter", "Stein\u00b7ko\u00b7lo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und hier fast wie aus deutschen Tannen", "tokens": ["Und", "hier", "fast", "wie", "aus", "deut\u00b7schen", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein hohenstaufisch Felsenschlo\u00df.", "tokens": ["Ein", "ho\u00b7hen\u00b7stau\u00b7fisch", "Fel\u00b7sen\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Schiffer wu\u00dft' uns viel zu erz\u00e4hlen", "tokens": ["Der", "Schif\u00b7fer", "wu\u00dft'", "uns", "viel", "zu", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom finstern C\u00e4sar Tiberius,", "tokens": ["Vom", "fins\u00b7tern", "C\u00e4\u00b7sar", "Ti\u00b7be\u00b7ri\u00b7us", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie er dort oben in prunkenden S\u00e4len", "tokens": ["Wie", "er", "dort", "o\u00b7ben", "in", "prun\u00b7ken\u00b7den", "S\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gehaust voll Angst und \u00dcberdru\u00df;", "tokens": ["Ge\u00b7haust", "voll", "Angst", "und", "\u00dc\u00b7berd\u00b7ru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie er um die hohlen Schl\u00e4fe", "tokens": ["Und", "wie", "er", "um", "die", "hoh\u00b7len", "Schl\u00e4\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Blitzgezuck am Meeresstrand,", "tokens": ["Beim", "Blitz\u00b7ge\u00b7zuck", "am", "Mee\u00b7res\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bef\u00fcrchtend, da\u00df der Gott ihn tr\u00e4fe,", "tokens": ["Be\u00b7f\u00fcrch\u00b7tend", ",", "da\u00df", "der", "Gott", "ihn", "tr\u00e4\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Lorbeer schlang mit feiger Hand.", "tokens": ["Den", "Lor\u00b7beer", "schlang", "mit", "fei\u00b7ger", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wei\u00dft du, fragt' ich, nichts zu sagen", "tokens": ["Und", "wei\u00dft", "du", ",", "fragt'", "ich", ",", "nichts", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von jenen andern Tr\u00fcmmern dort?", "tokens": ["Von", "je\u00b7nen", "an\u00b7dern", "Tr\u00fcm\u00b7mern", "dort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt auch von ihres Herrschers Tagen", "tokens": ["Lebt", "auch", "von", "ih\u00b7res", "Herr\u00b7schers", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ein Ged\u00e4chtnis der Menschen fort?", "tokens": ["Noch", "ein", "Ge\u00b7d\u00e4cht\u00b7nis", "der", "Men\u00b7schen", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Schiffer fuhr sich \u00fcber die Stirne", "tokens": ["Der", "Schif\u00b7fer", "fuhr", "sich", "\u00fc\u00b7ber", "die", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sprach: Das ist ein vergessener Traum.", "tokens": ["Und", "sprach", ":", "Das", "ist", "ein", "ver\u00b7ges\u00b7se\u00b7ner", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In meinem alten Matrosengehirne", "tokens": ["In", "mei\u00b7nem", "al\u00b7ten", "Mat\u00b7ro\u00b7sen\u00b7ge\u00b7hir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vergehn die M\u00e4rchen wie Meeresschaum.", "tokens": ["Ver\u00b7gehn", "die", "M\u00e4r\u00b7chen", "wie", "Mee\u00b7res\u00b7schaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Er sprach's, und eine Mandoline", "tokens": ["Er", "sprach's", ",", "und", "ei\u00b7ne", "Man\u00b7do\u00b7li\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erklang vom Strand \u2013 es mahnte mich,", "tokens": ["Er\u00b7klang", "vom", "Strand", "\u2013", "es", "mahn\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$(", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als k\u00e4m' aus jener Burgruine", "tokens": ["Als", "k\u00e4m'", "aus", "je\u00b7ner", "Bur\u00b7gru\u00b7i\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein klagend Echo: Friederich.", "tokens": ["Ein", "kla\u00b7gend", "E\u00b7cho", ":", "Frie\u00b7de\u00b7rich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und nicht mehr in den \u00f6den G\u00e4ngen", "tokens": ["Und", "nicht", "mehr", "in", "den", "\u00f6\u00b7den", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den finstern R\u00f6mer sah ich drohn;", "tokens": ["Den", "fins\u00b7tern", "R\u00f6\u00b7mer", "sah", "ich", "drohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah bei Fest und Minnes\u00e4ngen", "tokens": ["Ich", "sah", "bei", "Fest", "und", "Min\u00b7ne\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Constanza's blondgelockten Sohn.", "tokens": ["Con\u00b7stan\u00b7za's", "blond\u00b7ge\u00b7lock\u00b7ten", "Sohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich sah an des Altanes Borden", "tokens": ["Ich", "sah", "an", "des", "Al\u00b7ta\u00b7nes", "Bor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn sinnend stehn, aufs Schwert gelehnt,", "tokens": ["Ihn", "sin\u00b7nend", "stehn", ",", "aufs", "Schwert", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Geist bek\u00fcmmert um den Norden,", "tokens": ["Im", "Geist", "be\u00b7k\u00fcm\u00b7mert", "um", "den", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz dem S\u00fcden zugesehnt.", "tokens": ["Das", "Herz", "dem", "S\u00fc\u00b7den", "zu\u00b7ge\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und als schon Nacht den Strand umwebte,", "tokens": ["Und", "als", "schon", "Nacht", "den", "Strand", "um\u00b7web\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mond im dunkeln Meer erblich,", "tokens": ["Der", "Mond", "im", "dun\u00b7keln", "Meer", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meiner Seele Tiefen bebte", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "Tie\u00b7fen", "beb\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch lang das Echo: Friederich.", "tokens": ["Noch", "lang", "das", "E\u00b7cho", ":", "Frie\u00b7de\u00b7rich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}