{"textgrid.poem.52592": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3. (Faust)", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schlugest auf die holden Augenlieder,", "tokens": ["Du", "schlu\u00b7gest", "auf", "die", "hol\u00b7den", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da gr\u00fc\u00dfte michs wie junger Fr\u00fchlingsschein;", "tokens": ["Da", "gr\u00fc\u00df\u00b7te", "michs", "wie", "jun\u00b7ger", "Fr\u00fch\u00b7lings\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was in mir schlummerte, erwachte wieder,", "tokens": ["Was", "in", "mir", "schlum\u00b7mer\u00b7te", ",", "er\u00b7wach\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in mich selber zog der Fr\u00fchling ein.", "tokens": ["Und", "in", "mich", "sel\u00b7ber", "zog", "der", "Fr\u00fch\u00b7ling", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verworrener Gedanken wild Getriebe", "tokens": ["Ver\u00b7wor\u00b7re\u00b7ner", "Ge\u00b7dan\u00b7ken", "wild", "Ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Arbeitete mir l\u00e4ngst die Seele wund,", "tokens": ["Ar\u00b7bei\u00b7te\u00b7te", "mir", "l\u00e4ngst", "die", "See\u00b7le", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchlt es wohl, es fehlte mir die Liebe,", "tokens": ["Ich", "f\u00fchlt", "es", "wohl", ",", "es", "fehl\u00b7te", "mir", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deine Liebe machte mich gesund.", "tokens": ["Und", "dei\u00b7ne", "Lie\u00b7be", "mach\u00b7te", "mich", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn durch die Liebe leben wir im Leben,", "tokens": ["Denn", "durch", "die", "Lie\u00b7be", "le\u00b7ben", "wir", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ohne Liebe bist du ein Gespenst,", "tokens": ["Und", "oh\u00b7ne", "Lie\u00b7be", "bist", "du", "ein", "Ge\u00b7spenst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein ewig Fliehn, ein Niezufriedengeben,", "tokens": ["Ein", "e\u00b7wig", "Fliehn", ",", "ein", "Niez\u00b7u\u00b7frie\u00b7den\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ist dein Loos, eh du die Liebe kennst.", "tokens": ["Das", "ist", "dein", "Loos", ",", "eh", "du", "die", "Lie\u00b7be", "kennst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}