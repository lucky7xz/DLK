{"textgrid.poem.33214": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2257. Zum Jungfern-Fest", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das M\u00e4gdlein preiset seinen k\u00f6nig, 1730 \u2013 31.", "tokens": ["Das", "M\u00e4gd\u00b7lein", "prei\u00b7set", "sei\u00b7nen", "k\u00f6\u00b7nig", ",", "17\u00b730", "\u2013", "31."], "token_info": ["word", "word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJD", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wird das gesch\u00f6pf zu wenig, 1731 \u2013 32.", "tokens": ["Da", "wird", "das", "ge\u00b7sch\u00f6pf", "zu", "we\u00b7nig", ",", "17\u00b731", "\u2013", "32."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PIS", "$,", "CARD", "$(", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn f\u00e4ngt das Jungfern-Capellgen an, 1732 \u2013 33.", "tokens": ["Denn", "f\u00e4ngt", "das", "Jung\u00b7fern\u00b7Ca\u00b7pell\u00b7gen", "an", ",", "17\u00b732", "\u2013", "33."], "token_info": ["word", "word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Eh' in Gott gethan; 1733 \u2013 34.", "tokens": ["Die", "Eh'", "in", "Gott", "ge\u00b7than", ";", "17\u00b733", "\u2013", "34."], "token_info": ["word", "word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$.", "CARD", "$(", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man frey't und l\u00e4\u00dft es wieder seyn, 1734 \u2013 35.", "tokens": ["Man", "frey't", "und", "l\u00e4\u00dft", "es", "wie\u00b7der", "seyn", ",", "17\u00b734", "\u2013", "35."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "VAINF", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zieht w\u00e4yselein", "tokens": ["Und", "zieht", "w\u00e4y\u00b7sel\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Aus wechsel-b\u00e4lgen zu F\u00fcrsten-dirnen, 1735 \u2013 36.", "tokens": ["Aus", "wech\u00b7sel\u00b7b\u00e4l\u00b7gen", "zu", "F\u00fcrs\u00b7ten\u00b7dir\u00b7nen", ",", "17\u00b735", "\u2013", "36."], "token_info": ["word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["APPR", "ADJA", "APPR", "NN", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Man exuliret mit siegel-stirnen, 1736 \u2013 37.", "tokens": ["Man", "e\u00b7xu\u00b7li\u00b7ret", "mit", "sie\u00b7gel\u00b7stir\u00b7nen", ",", "17\u00b736", "\u2013", "37."], "token_info": ["word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "$,", "CARD", "$(", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Dann mit dem zeichen ", "tokens": ["Dann", "mit", "dem", "zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Stiffts Chor im Wetter-feld, 1738 \u2013 39.", "tokens": ["Stiffts", "Chor", "im", "Wet\u00b7ter\u00b7feld", ",", "17\u00b738", "\u2013", "39."], "token_info": ["word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["NN", "NN", "APPRART", "NN", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der neue zeugen und m\u00e4gdlein plan, 1739 \u2013 40.", "tokens": ["Der", "neu\u00b7e", "zeu\u00b7gen", "und", "m\u00e4gd\u00b7lein", "plan", ",", "17\u00b739", "\u2013", "40."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["ART", "ADJA", "VVFIN", "KON", "ADJD", "PTKVZ", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Salbt erd und Ocean, 40 \u2013 42.", "tokens": ["Salbt", "erd", "und", "O\u00b7cean", ",", "40", "\u2013", "42."], "token_info": ["word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["NE", "NN", "KON", "NN", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Holl-, Engeland, Canada, D\u00e4nnemark, 1742 \u2013 43.", "tokens": ["Holl", ",", "En\u00b7ge\u00b7land", ",", "Ca\u00b7na\u00b7da", ",", "D\u00e4n\u00b7ne\u00b7mark", ",", "1742", "\u2013", "43."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "number", "punct", "ordinal"], "pos": ["TRUNC", "$,", "NE", "$,", "NE", "$,", "NE", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Macht Huth und Haag zur arck, 1743 \u2013 44.", "tokens": ["Macht", "Huth", "und", "Haag", "zur", "arck", ",", "1743", "\u2013", "44."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["NN", "NN", "KON", "NE", "APPRART", "NN", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Amsterdam, Peil, Krausch, Saltz, 1744 \u2013 45.", "tokens": ["A\u00b7mster\u00b7dam", ",", "Peil", ",", "Krausch", ",", "Saltz", ",", "17\u00b744", "\u2013", "45."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "number", "punct", "ordinal"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "CARD", "$(", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ebersdorff, nun Gott walts! 1745 \u2013 46.", "tokens": ["E\u00b7bers\u00b7dorff", ",", "nun", "Gott", "walts", "!", "1745", "\u2013", "46."], "token_info": ["word", "punct", "word", "word", "word", "punct", "number", "punct", "ordinal"], "pos": ["NE", "$,", "ADV", "NN", "VVFIN", "$.", "CARD", "$(", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}