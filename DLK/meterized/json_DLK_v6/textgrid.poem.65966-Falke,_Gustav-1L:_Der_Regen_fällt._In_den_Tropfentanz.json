{"textgrid.poem.65966": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Regen f\u00e4llt. In den Tropfentanz", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regen f\u00e4llt. In den Tropfentanz", "tokens": ["Der", "Re\u00b7gen", "f\u00e4llt", ".", "In", "den", "Trop\u00b7fen\u00b7tanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Starr ich hinaus, versunken ganz", "tokens": ["Starr", "ich", "hin\u00b7aus", ",", "ver\u00b7sun\u00b7ken", "ganz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "PTKVZ", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In allerlei tr\u00fcbe Gedanken. Mir ist,", "tokens": ["In", "al\u00b7ler\u00b7lei", "tr\u00fc\u00b7be", "Ge\u00b7dan\u00b7ken", ".", "Mir", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als h\u00e4tt' es geregnet zu jeder Frist,", "tokens": ["Als", "h\u00e4tt'", "es", "ge\u00b7reg\u00b7net", "zu", "je\u00b7der", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und alles, so lange ich denken kann,", "tokens": ["Und", "al\u00b7les", ",", "so", "lan\u00b7ge", "ich", "den\u00b7ken", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Tr\u00fcb, grau und nass in einander rann,", "tokens": ["Tr\u00fcb", ",", "grau", "und", "nass", "in", "ein\u00b7an\u00b7der", "rann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als h\u00e4tte es nie eine Sonne gegeben,", "tokens": ["Als", "h\u00e4t\u00b7te", "es", "nie", "ei\u00b7ne", "Son\u00b7ne", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Als w\u00e4re nur immer das ganze Leben,", "tokens": ["Als", "w\u00e4\u00b7re", "nur", "im\u00b7mer", "das", "gan\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Jahre, die Tage, die Stunden all,", "tokens": ["Die", "Jah\u00b7re", ",", "die", "Ta\u00b7ge", ",", "die", "Stun\u00b7den", "all", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "PIAT", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Ein tr\u00fcber, hastiger Tropfenfall.", "tokens": ["Ein", "tr\u00fc\u00b7ber", ",", "has\u00b7ti\u00b7ger", "Tro\u00b7pfen\u00b7fall", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}