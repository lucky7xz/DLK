{"textgrid.poem.40680": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ist's doch um das Schweigen eigen!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist's doch um das Schweigen eigen!", "tokens": ["Wie", "ist's", "doch", "um", "das", "Schwei\u00b7gen", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weise schweigt, wie Thoren schweigen,", "tokens": ["Der", "Wei\u00b7se", "schweigt", ",", "wie", "Tho\u00b7ren", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei gleichem Schein ungleicher Sinn,", "tokens": ["Bei", "glei\u00b7chem", "Schein", "un\u00b7glei\u00b7cher", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier ein Verlust, dort ein Gewinn;", "tokens": ["Hier", "ein", "Ver\u00b7lust", ",", "dort", "ein", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein F\u00fcrstenmantel, der die Gr\u00f6\u00dfe", "tokens": ["Ein", "F\u00fcrs\u00b7ten\u00b7man\u00b7tel", ",", "der", "die", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Reichen klug vor Sp\u00e4hern deckt,", "tokens": ["Des", "Rei\u00b7chen", "klug", "vor", "Sp\u00e4\u00b7hern", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Bettlerhemd, drin sich die Bl\u00f6\u00dfe", "tokens": ["Ein", "Bett\u00b7ler\u00b7hemd", ",", "drin", "sich", "die", "Bl\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des armen Kr\u00fcppels scheu versteckt.", "tokens": ["Des", "ar\u00b7men", "Kr\u00fcp\u00b7pels", "scheu", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den weisen Schweiger kann es treffen,", "tokens": ["Den", "wei\u00b7sen", "Schwei\u00b7ger", "kann", "es", "tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df ihn die Welt als Thoren schilt;", "tokens": ["Da\u00df", "ihn", "die", "Welt", "als", "Tho\u00b7ren", "schilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Des Thoren Schweigen mag uns \u00e4ffen,", "tokens": ["Des", "Tho\u00b7ren", "Schwei\u00b7gen", "mag", "uns", "\u00e4f\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df er wohl gar als Weiser gilt.", "tokens": ["Da\u00df", "er", "wohl", "gar", "als", "Wei\u00b7ser", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zur Unzeit wird des Weisen Schweigen", "tokens": ["Zur", "Un\u00b7zeit", "wird", "des", "Wei\u00b7sen", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das Th\u00f6richt'ste, das er ersann,", "tokens": ["Das", "Th\u00f6\u00b7richt'\u00b7ste", ",", "das", "er", "er\u00b7sann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Doch allzeit bleibt des Thoren Schweigen", "tokens": ["Doch", "all\u00b7zeit", "bleibt", "des", "Tho\u00b7ren", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das Weiseste, das er begann.", "tokens": ["Das", "Wei\u00b7ses\u00b7te", ",", "das", "er", "be\u00b7gann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}