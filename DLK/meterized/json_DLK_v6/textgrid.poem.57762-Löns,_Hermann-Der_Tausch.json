{"textgrid.poem.57762": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Tausch", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mein Herz gefangen", "tokens": ["Du", "hast", "mein", "Herz", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit deiner wei\u00dfen Hand;", "tokens": ["Mit", "dei\u00b7ner", "wei\u00b7\u00dfen", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast mein Herz bestricket", "tokens": ["Du", "hast", "mein", "Herz", "be\u00b7stri\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem roten Band.", "tokens": ["Mit", "ei\u00b7nem", "ro\u00b7ten", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich komm zu dir gegangen,", "tokens": ["Ich", "komm", "zu", "dir", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Herz gib wieder her;", "tokens": ["Mein", "Herz", "gib", "wie\u00b7der", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn da, wo es geschlagen,", "tokens": ["Denn", "da", ",", "wo", "es", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Ist alles taub und leer.", "tokens": ["Ist", "al\u00b7les", "taub", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was willst du mit zwei Herzen,", "tokens": ["Was", "willst", "du", "mit", "zwei", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum gib zur\u00fcck es mir;", "tokens": ["Drum", "gib", "zu\u00b7r\u00fcck", "es", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVIMP", "PTKVZ", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und willst du es behalten,", "tokens": ["Und", "willst", "du", "es", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gib mir deins daf\u00fcr.", "tokens": ["So", "gib", "mir", "deins", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}