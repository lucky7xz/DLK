{"textgrid.poem.30348": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df so viel de\u00df g\u00f6ldnen Staubes hat die neue Welt gestreuet,", "tokens": ["Da\u00df", "so", "viel", "de\u00df", "g\u00f6ld\u00b7nen", "Stau\u00b7bes", "hat", "die", "neu\u00b7e", "Welt", "ge\u00b7streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dr\u00fcber ist noch nichts erschienen, da\u00df die alte Welt sich freuet;", "tokens": ["Dr\u00fc\u00b7ber", "ist", "noch", "nichts", "er\u00b7schie\u00b7nen", ",", "da\u00df", "die", "al\u00b7te", "Welt", "sich", "freu\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PIS", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dann das Gold der neuen Welt macht, da\u00df alte Welt sehr narrt;", "tokens": ["Dann", "das", "Gold", "der", "neu\u00b7en", "Welt", "macht", ",", "da\u00df", "al\u00b7te", "Welt", "sehr", "narrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jene macht wol gar, da\u00df die gantz in ihrem Blute starrt;", "tokens": ["Je\u00b7ne", "macht", "wol", "gar", ",", "da\u00df", "die", "gantz", "in", "ih\u00b7rem", "Blu\u00b7te", "starrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Dann auff prachten, dann auff kriegen pflegt man allen Schatz zu wagen;", "tokens": ["Dann", "auff", "prach\u00b7ten", ",", "dann", "auff", "krie\u00b7gen", "pflegt", "man", "al\u00b7len", "Schatz", "zu", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADV", "APPR", "VVINF", "VVFIN", "PIS", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-++-+++-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.6": {"text": "Arme Christen zu versorgen, wil die gantze Welt nichts tragen.", "tokens": ["Ar\u00b7me", "Chris\u00b7ten", "zu", "ver\u00b7sor\u00b7gen", ",", "wil", "die", "gant\u00b7ze", "Welt", "nichts", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,", "VMFIN", "ART", "ADJA", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}