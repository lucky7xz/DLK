{"textgrid.poem.28535": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Veit ist die kleine Welt; das meist in ihm ist Meer;", "tokens": ["Veit", "ist", "die", "klei\u00b7ne", "Welt", ";", "das", "meist", "in", "ihm", "ist", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$.", "PDS", "VVFIN", "APPR", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Wasser kummt es nicht, vom Weine kummt es her.", "tokens": ["Von", "Was\u00b7ser", "kummt", "es", "nicht", ",", "vom", "Wei\u00b7ne", "kummt", "es", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}