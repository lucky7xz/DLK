{"dta.poem.23736": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der May.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Es l\u00e4chelt aufs neu", "tokens": ["Es", "l\u00e4\u00b7chelt", "aufs", "neu"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der fr\u00f6hliche May", "tokens": ["Der", "fr\u00f6h\u00b7li\u00b7che", "May"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In buntem festlichen Kleide.", "tokens": ["In", "bun\u00b7tem", "fest\u00b7li\u00b7chen", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von H\u00f6hen und Thal", "tokens": ["Von", "H\u00f6\u00b7hen", "und", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "T\u00f6nt \u00fcberall", "tokens": ["T\u00f6nt", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die s\u00fc\u00dfe Stimme der Freude.", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Stim\u00b7me", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In Wiesen und Flur", "tokens": ["In", "Wie\u00b7sen", "und", "Flur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Giebt uns die Natur", "tokens": ["Giebt", "uns", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die sch\u00f6nsten Blumen zu pfl\u00fccken.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "zu", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Drum will ich zum Tanz", "tokens": ["Drum", "will", "ich", "zum", "Tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit einem Kranz", "tokens": ["Mit", "ei\u00b7nem", "Kranz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die blonden Haare mir schm\u00fccken.", "tokens": ["Die", "blon\u00b7den", "Haa\u00b7re", "mir", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch sollt ich nicht den,", "tokens": ["Doch", "sollt", "ich", "nicht", "den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der alles so sch\u00f6n", "tokens": ["Der", "al\u00b7les", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Erschuf, erst br\u00fcnstig erheben?", "tokens": ["Er\u00b7schuf", ",", "erst", "br\u00fcns\u00b7tig", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch Jubelgesang", "tokens": ["Durch", "Ju\u00b7bel\u00b7ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Preis ihn mein Dank,", "tokens": ["Preis", "ihn", "mein", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Doch mehr: mein k\u00fcnftiges Leben!", "tokens": ["Doch", "mehr", ":", "mein", "k\u00fcnf\u00b7ti\u00b7ges", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}