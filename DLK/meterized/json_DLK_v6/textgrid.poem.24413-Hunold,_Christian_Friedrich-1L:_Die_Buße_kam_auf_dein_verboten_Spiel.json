{"textgrid.poem.24413": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Bu\u00dfe kam auf dein verboten Spiel/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bu\u00dfe kam auf dein verboten Spiel/", "tokens": ["Die", "Bu\u00b7\u00dfe", "kam", "auf", "dein", "ver\u00b7bo\u00b7ten", "Spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So bald es nur die Geistlichen erfuhren.", "tokens": ["So", "bald", "es", "nur", "die", "Geist\u00b7li\u00b7chen", "er\u00b7fuh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und war es nun du S\u00fcnder noch zuviel?", "tokens": ["Und", "war", "es", "nun", "du", "S\u00fcn\u00b7der", "noch", "zu\u00b7viel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPER", "NN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Biloni\u00df gab dir Gott/ du gabst es einer Huren.", "tokens": ["Dein", "Bi\u00b7lo\u00b7ni\u00df", "gab", "dir", "Gott", "/", "du", "gabst", "es", "ei\u00b7ner", "Hu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}