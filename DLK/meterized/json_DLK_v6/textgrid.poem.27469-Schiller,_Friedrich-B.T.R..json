{"textgrid.poem.27469": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "B.T.R.", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kriechender Efeu, du rankest empor an Felsen und B\u00e4umen,", "tokens": ["Krie\u00b7chen\u00b7der", "E\u00b7feu", ",", "du", "ran\u00b7kest", "em\u00b7por", "an", "Fel\u00b7sen", "und", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "+---+--+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Faulen St\u00e4mmen; du rankst, kriechender Efeu, empor.", "tokens": ["Fau\u00b7len", "St\u00e4m\u00b7men", ";", "du", "rankst", ",", "krie\u00b7chen\u00b7der", "E\u00b7feu", ",", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "\u00dcberall bist du Poet, im Gespr\u00e4ch, in Gesch\u00e4ften, am Spieltisch,", "tokens": ["\u00dc\u00b7be\u00b7rall", "bist", "du", "Po\u00b7et", ",", "im", "Ge\u00b7spr\u00e4ch", ",", "in", "Ge\u00b7sch\u00e4f\u00b7ten", ",", "am", "Spiel\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "APPRART", "NN", "$,", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nur in der Poesie bist du nicht immer Poet.", "tokens": ["Nur", "in", "der", "Poe\u00b7sie", "bist", "du", "nicht", "im\u00b7mer", "Po\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Meine Freude verdarb er mir garstig, die versifizierte", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "ver\u00b7darb", "er", "mir", "gars\u00b7tig", ",", "die", "ver\u00b7si\u00b7fi\u00b7zier\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$,", "PRELS", "VVFIN"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Mein ich; die andre gottlob! wird mir durch den nicht verg\u00e4llt.", "tokens": ["Mein", "ich", ";", "die", "and\u00b7re", "gott\u00b7lob", "!", "wird", "mir", "durch", "den", "nicht", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "$.", "ART", "ADJA", "NN", "$.", "VAFIN", "PPER", "APPR", "ART", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Ecce rubet quidam, pallet, stupet, oscitat, odit.", "tokens": ["E\u00b7cce", "ru\u00b7bet", "qui\u00b7dam", ",", "pal\u00b7let", ",", "stu\u00b7pet", ",", "os\u00b7ci\u00b7tat", ",", "o\u00b7dit", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "VVFIN", "$,", "VVFIN", "$,", "FM.la", "$,", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hoc volo, nunc nobis carmina nostra placent.", "tokens": ["Hoc", "vo\u00b7lo", ",", "nunc", "no\u00b7bis", "car\u00b7mi\u00b7na", "nost\u00b7ra", "pla\u00b7cent", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+---+-+", "measure": "unknown.measure.hexa"}}}}}