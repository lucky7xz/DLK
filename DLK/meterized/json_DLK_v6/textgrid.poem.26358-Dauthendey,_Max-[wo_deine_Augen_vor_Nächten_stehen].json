{"textgrid.poem.26358": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[wo deine Augen vor N\u00e4chten stehen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo deine Augen vor N\u00e4chten stehen,", "tokens": ["Wo", "dei\u00b7ne", "Au\u00b7gen", "vor", "N\u00e4ch\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo deine Ohren durch Stillen gehen,", "tokens": ["Wo", "dei\u00b7ne", "Oh\u00b7ren", "durch", "Stil\u00b7len", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Brennen noch Welten und Willen im Dunkel:", "tokens": ["Bren\u00b7nen", "noch", "Wel\u00b7ten", "und", "Wil\u00b7len", "im", "Dun\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Hinter den wei\u00dfen Sonnen glei\u00dfen und funkeln", "tokens": ["Hin\u00b7ter", "den", "wei\u00b7\u00dfen", "Son\u00b7nen", "glei\u00b7\u00dfen", "und", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "KON", "VVINF"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Schwarze Sonnen n\u00e4chtiger Reiche.", "tokens": ["Schwar\u00b7ze", "Son\u00b7nen", "n\u00e4ch\u00b7ti\u00b7ger", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Den Lebensm\u00fcden,", "tokens": ["Den", "Le\u00b7bens\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den Tagsonnem\u00fcden", "tokens": ["Den", "Tag\u00b7son\u00b7ne\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Beschleichen gierig die dunkeln Sonnen.", "tokens": ["Be\u00b7schlei\u00b7chen", "gie\u00b7rig", "die", "dun\u00b7keln", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Lebenswunden,", "tokens": ["Den", "Le\u00b7bens\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Den Tagsonnewunden", "tokens": ["Den", "Tag\u00b7son\u00b7ne\u00b7wun\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Umschleichen die Sonnen der Totenreiche.", "tokens": ["Um\u00b7schlei\u00b7chen", "die", "Son\u00b7nen", "der", "To\u00b7ten\u00b7rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}