{"textgrid.poem.61662": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00e4\u00dft sich nicht vermeiden der Strau\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4\u00dft sich nicht vermeiden der Strau\u00df,", "tokens": ["L\u00e4\u00dft", "sich", "nicht", "ver\u00b7mei\u00b7den", "der", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "VVINF", "ART", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So fasse k\u00fchn das Schwert am Hefte.", "tokens": ["So", "fas\u00b7se", "k\u00fchn", "das", "Schwert", "am", "Hef\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Angriff wachsen dir die Kr\u00e4fte,", "tokens": ["Im", "An\u00b7griff", "wach\u00b7sen", "dir", "die", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem feigen Zaudrer gehn sie aus.", "tokens": ["Dem", "fei\u00b7gen", "Zaud\u00b7rer", "gehn", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}