{"dta.poem.10086": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Reise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man kann, wie herrlich und wie sch\u00f6n", "tokens": ["Man", "kann", ",", "wie", "herr\u00b7lich", "und", "wie", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "$,", "PWAV", "ADJD", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sch\u00f6pfers Werck, zu Seinem Preise,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Werck", ",", "zu", "Sei\u00b7nem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So gut in keiner Handlung sehn,", "tokens": ["So", "gut", "in", "kei\u00b7ner", "Hand\u00b7lung", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wann man etwan auf der Reise.", "tokens": ["Als", "wann", "man", "et\u00b7wan", "auf", "der", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PIS", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Wie gro\u00df, wie viel, wie mancherley", "tokens": ["Wie", "gro\u00df", ",", "wie", "viel", ",", "wie", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "PIS", "$,", "PWAV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Heer der Creaturen sey,", "tokens": ["Das", "Heer", "der", "Crea\u00b7tu\u00b7ren", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Giebt ieder Augenblick, im Wechsel, zu verstehn.", "tokens": ["Giebt", "ie\u00b7der", "Au\u00b7gen\u00b7blick", ",", "im", "Wech\u00b7sel", ",", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "APPRART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bald zeigen nett befurchte Felder,", "tokens": ["Bald", "zei\u00b7gen", "nett", "be\u00b7furch\u00b7te", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald Lust- und Schatten-reiche W\u00e4lder,", "tokens": ["Bald", "Lust", "und", "Schat\u00b7ten\u00b7rei\u00b7che", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier Gras und Kraut, dort Laub und Bl\u00fchte,", "tokens": ["Hier", "Gras", "und", "Kraut", ",", "dort", "Laub", "und", "Bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sch\u00f6pfers Weisheit, Macht und G\u00fcte.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "Macht", "und", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier zeigt ein Berg, und dort ein Thal", "tokens": ["Hier", "zeigt", "ein", "Berg", ",", "und", "dort", "ein", "Thal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesch\u00f6pf\u2019 und Vorw\u00fcrff ohne Zahl.", "tokens": ["Ge\u00b7sch\u00f6pf'", "und", "Vor\u00b7w\u00fcrff", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bald wird in bunt bebl\u00fchmten Wiesen", "tokens": ["Bald", "wird", "in", "bunt", "be\u00b7bl\u00fchm\u00b7ten", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der, so sie schuff, mit Recht gepriesen.", "tokens": ["Der", ",", "so", "sie", "schuff", ",", "mit", "Recht", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPER", "ADJD", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hier machet eine fette Weide", "tokens": ["Hier", "ma\u00b7chet", "ei\u00b7ne", "fet\u00b7te", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Voll Schaff\u2019 und K\u00fch\u2019 uns neue Freude.", "tokens": ["Voll", "Schaff'", "und", "K\u00fch'", "uns", "neu\u00b7e", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dort scheint ein Feld voll Korn und Weitzen", "tokens": ["Dort", "scheint", "ein", "Feld", "voll", "Korn", "und", "Weit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zugleich zur Lust und Andacht uns zu reitzen.", "tokens": ["Zu\u00b7gleich", "zur", "Lust", "und", "An\u00b7dacht", "uns", "zu", "reit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Hier wird bey einem gr\u00fcnen H\u00fcgel,", "tokens": ["Hier", "wird", "bey", "ei\u00b7nem", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der klare Bach ein Himmels-Spiegel.", "tokens": ["Der", "kla\u00b7re", "Bach", "ein", "Him\u00b7mels\u00b7Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die Erd\u2019, indem man f\u00e4hrt, scheint ruckwerts stets zu", "tokens": ["Die", "Erd'", ",", "in\u00b7dem", "man", "f\u00e4hrt", ",", "scheint", "ruck\u00b7werts", "stets", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Um gleichsam unserm Blick mit Hauffen", "tokens": ["Um", "gleich\u00b7sam", "un\u00b7serm", "Blick", "mit", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Von immer angenehmern Dingen", "tokens": ["Von", "im\u00b7mer", "an\u00b7ge\u00b7neh\u00b7mern", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Stets neuen Vorraht zuzubringen.", "tokens": ["Stets", "neu\u00b7en", "Vor\u00b7raht", "zu\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Hier hebet sich ein Thurm empor;", "tokens": ["Hier", "he\u00b7bet", "sich", "ein", "Thurm", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da sinckt der Blick in tieffe Th\u00e4ler; dort", "tokens": ["Da", "sinckt", "der", "Blick", "in", "tief\u00b7fe", "Th\u00e4\u00b7ler", ";", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Erstreckt er sich auf einer Ebne fort.", "tokens": ["Er\u00b7streckt", "er", "sich", "auf", "ei\u00b7ner", "Eb\u00b7ne", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Hier tritt ein Busch, und dort ein Berg hervor.", "tokens": ["Hier", "tritt", "ein", "Busch", ",", "und", "dort", "ein", "Berg", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Das Kutschen-Fenster stellet mir", "tokens": ["Das", "Kut\u00b7schen\u00b7Fens\u00b7ter", "stel\u00b7let", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Stets eine neue Schilderey", "tokens": ["Stets", "ei\u00b7ne", "neu\u00b7e", "Schil\u00b7de\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Von einer stets verneuten Landschaft f\u00fcr.", "tokens": ["Von", "ei\u00b7ner", "stets", "ver\u00b7neu\u00b7ten", "Land\u00b7schaft", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Es giebt des Fensters vordrer Theil", "tokens": ["Es", "giebt", "des", "Fens\u00b7ters", "vor\u00b7drer", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Mir eben so viel Gegenw\u00fcrff\u2019 in Eil\u2019,", "tokens": ["Mir", "e\u00b7ben", "so", "viel", "Ge\u00b7gen\u00b7w\u00fcrff'", "in", "Eil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Als mir, das hintre raubt.", "tokens": ["Als", "mir", ",", "das", "hin\u00b7tre", "raubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie herrlich gl\u00e4ntzt, wie lieblich glimmt", "tokens": ["Wie", "herr\u00b7lich", "gl\u00e4ntzt", ",", "wie", "lieb\u00b7lich", "glimmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Sonnen-Licht, wann auf der Fluth Crystallen", "tokens": ["Das", "Son\u00b7nen\u00b7Licht", ",", "wann", "auf", "der", "Fluth", "Crys\u00b7tal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Derselben Strahlen fallen!", "tokens": ["Der\u00b7sel\u00b7ben", "Strah\u00b7len", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Offt siehet man von weitem hohe Gipfel", "tokens": ["Offt", "sie\u00b7het", "man", "von", "wei\u00b7tem", "ho\u00b7he", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Bergen, gantz bedeckt durch dichter B\u00e4ume Wipfel;", "tokens": ["Von", "Ber\u00b7gen", ",", "gantz", "be\u00b7deckt", "durch", "dich\u00b7ter", "B\u00e4u\u00b7me", "Wip\u00b7fel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Worauf, da Zweig\u2019 und Laub sich angenehm verschr\u00e4ncken,", "tokens": ["Wo\u00b7rauf", ",", "da", "Zweig'", "und", "Laub", "sich", "an\u00b7ge\u00b7nehm", "ver\u00b7schr\u00e4n\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "NN", "KON", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und in die Tieffen sich der Sonnen Strahlen sencken;", "tokens": ["Und", "in", "die", "Tief\u00b7fen", "sich", "der", "Son\u00b7nen", "Strah\u00b7len", "sen\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die krausen hell-bestrahlten H\u00f6hn", "tokens": ["Die", "krau\u00b7sen", "hell\u00b7be\u00b7strahl\u00b7ten", "H\u00f6hn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Noch einst so angenehm und sch\u00f6n,", "tokens": ["Noch", "einst", "so", "an\u00b7ge\u00b7nehm", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch dunckel-gr\u00fcnen Wechsel, stehn.", "tokens": ["Durch", "dun\u00b7ckel\u00b7gr\u00fc\u00b7nen", "Wech\u00b7sel", ",", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie sanft versinckt der Blick (als wie das Licht", "tokens": ["Wie", "sanft", "ver\u00b7sinckt", "der", "Blick", "(", "als", "wie", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$(", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einen gr\u00fcnen Sammt mit Lust versincket)", "tokens": ["In", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Sammt", "mit", "Lust", "ver\u00b7sin\u00b7cket", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dicht belaubtes Buschwerck nicht!", "tokens": ["In", "dicht", "be\u00b7laub\u00b7tes", "Busc\u00b7hwerck", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lieblich l\u00e4sst es doch, wann das Getreide reifft,", "tokens": ["Wie", "lieb\u00b7lich", "l\u00e4sst", "es", "doch", ",", "wann", "das", "Ge\u00b7trei\u00b7de", "reifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und unser Blick so dann die Fl\u00e4chen \u00fcberl\u00e4ufft!", "tokens": ["Und", "un\u00b7ser", "Blick", "so", "dann", "die", "Fl\u00e4\u00b7chen", "\u00fc\u00b7berl\u00b7\u00e4ufft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier drengt von reiffen Korn ein schmahler gel