{"textgrid.poem.42751": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[es war einmal ein schlimmer Husten]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein schlimmer Husten,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "schlim\u00b7mer", "Hus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00f6rte gar nicht auf zu pusten.", "tokens": ["Der", "h\u00f6r\u00b7te", "gar", "nicht", "auf", "zu", "pus\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zwar kroch er hinter eine Hand,", "tokens": ["Zwar", "kroch", "er", "hin\u00b7ter", "ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was jedermann manierlich fand.", "tokens": ["Was", "je\u00b7der\u00b7mann", "ma\u00b7nier\u00b7lich", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und doch hat ihn der Doktor Lieben", "tokens": ["Und", "doch", "hat", "ihn", "der", "Dok\u00b7tor", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Liebens Malzbonbon vertrieben.", "tokens": ["Mit", "Lie\u00b7bens", "Malz\u00b7bon\u00b7bon", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bemerkt sei noch: F\u00fcr dies Gedicht", "tokens": ["Be\u00b7merkt", "sei", "noch", ":", "F\u00fcr", "dies", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "$.", "APPR", "PDS", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Bezahlte mich Herr Lieben nicht.", "tokens": ["Be\u00b7zahl\u00b7te", "mich", "Herr", "Lie\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}