{"dta.poem.7554": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Wunder ist die Welt, das nie wird ausgewundert,", "tokens": ["Ein", "Wun\u00b7der", "ist", "die", "Welt", ",", "das", "nie", "wird", "aus\u00b7ge\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das niederschl\u00e4gt den Geist und wieder ihn ermuntert.", "tokens": ["Das", "nie\u00b7der\u00b7schl\u00e4gt", "den", "Geist", "und", "wie\u00b7der", "ihn", "er\u00b7mun\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Daniederschl\u00e4gt den Geist vorm ew'gen Stoff ein Bangen,", "tokens": ["Da\u00b7nie\u00b7der\u00b7schl\u00e4gt", "den", "Geist", "vorm", "ew'\u00b7gen", "Stoff", "ein", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stets ermunterts ihn den Kampf neu anzufangen.", "tokens": ["Und", "stets", "er\u00b7mun\u00b7terts", "ihn", "den", "Kampf", "neu", "an\u00b7zu\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ob du benennen willst das Viele, Einzle, Kleine?", "tokens": ["Ob", "du", "be\u00b7nen\u00b7nen", "willst", "das", "Vie\u00b7le", ",", "Einz\u00b7le", ",", "Klei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob du erkennen willst das Gro\u00dfe, Ganze, Eine?", "tokens": ["Ob", "du", "er\u00b7ken\u00b7nen", "willst", "das", "Gro\u00b7\u00dfe", ",", "Gan\u00b7ze", ",", "Ei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "ADJA", "$,", "NN", "$,", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Unendlichkeit ist dort und hier Unendlichkeit,", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", "ist", "dort", "und", "hier", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit den beiden wagst du Endlicher den Streit!", "tokens": ["Und", "mit", "den", "bei\u00b7den", "wagst", "du", "End\u00b7li\u00b7cher", "den", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIAT", "VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Eh du am Boden ganz ein Gras hast durchbetrachtet,", "tokens": ["Eh", "du", "am", "Bo\u00b7den", "ganz", "ein", "Gras", "hast", "durch\u00b7be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gieng eine Welt voll Glanz vorbei dir unbeachtet.", "tokens": ["Gieng", "ei\u00b7ne", "Welt", "voll", "Glanz", "vor\u00b7bei", "dir", "un\u00b7be\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und eh du Zweig und Blatt gez\u00e4hlt am Sternenbaum,", "tokens": ["Und", "eh", "du", "Zweig", "und", "Blatt", "ge\u00b7z\u00e4hlt", "am", "Ster\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "KON", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bl\u00fcht ungenossen ab ein Erdenfr\u00fchlingstraum.", "tokens": ["Bl\u00fcht", "un\u00b7ge\u00b7nos\u00b7sen", "ab", "ein", "Er\u00b7den\u00b7fr\u00fch\u00b7lings\u00b7traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Getrost! zwar du nicht bist, doch Gott ist \u00fcberal;", "tokens": ["Ge\u00b7trost", "!", "zwar", "du", "nicht", "bist", ",", "doch", "Gott", "ist", "\u00fc\u00b7be\u00b7ral", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "PPER", "PTKNEG", "VAFIN", "$,", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du siehst das ganze Licht in jedem Farbenstral.", "tokens": ["Du", "siehst", "das", "gan\u00b7ze", "Licht", "in", "je\u00b7dem", "Far\u00b7ben\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und Alles ist dem Geist ein w\u00fcrd'ges Element,", "tokens": ["Und", "Al\u00b7les", "ist", "dem", "Geist", "ein", "w\u00fcrd'\u00b7ges", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sch\u00fcrt die Andachtsglut, in der die Sch\u00f6pfung brennt.", "tokens": ["Was", "sch\u00fcrt", "die", "An\u00b7dachts\u00b7glut", ",", "in", "der", "die", "Sch\u00f6p\u00b7fung", "brennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}