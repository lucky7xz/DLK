{"textgrid.poem.41509": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Im Moose", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst die Nacht dem sonnenm\u00fcden Land", "tokens": ["Als", "j\u00fcngst", "die", "Nacht", "dem", "son\u00b7nen\u00b7m\u00fc\u00b7den", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der D\u00e4mmrung leise Boten hat gesandt,", "tokens": ["Der", "D\u00e4mm\u00b7rung", "lei\u00b7se", "Bo\u00b7ten", "hat", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da lag ich einsam noch in Waldes Moose.", "tokens": ["Da", "lag", "ich", "ein\u00b7sam", "noch", "in", "Wal\u00b7des", "Moo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dunklen Zweige nickten so vertraut,", "tokens": ["Die", "dunk\u00b7len", "Zwei\u00b7ge", "nick\u00b7ten", "so", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An meiner Wange fl\u00fcsterte das Kraut,", "tokens": ["An", "mei\u00b7ner", "Wan\u00b7ge", "fl\u00fcs\u00b7ter\u00b7te", "das", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Unsichtbar duftete die Heiderose.", "tokens": ["Un\u00b7sicht\u00b7bar", "duf\u00b7te\u00b7te", "die", "Hei\u00b7de\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Und flimmern sah ich, durch der Linde Raum,", "tokens": ["Und", "flim\u00b7mern", "sah", "ich", ",", "durch", "der", "Lin\u00b7de", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "$,", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein mattes Licht, das im Gezweig der Baum", "tokens": ["Ein", "mat\u00b7tes", "Licht", ",", "das", "im", "Ge\u00b7zweig", "der", "Baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich einem m\u00e4cht'gen Gl\u00fchwurm schien zu tragen.", "tokens": ["Gleich", "ei\u00b7nem", "m\u00e4cht'\u00b7gen", "Gl\u00fch\u00b7wurm", "schien", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es sah so d\u00e4mmernd wie ein Traumgesicht,", "tokens": ["Es", "sah", "so", "d\u00e4m\u00b7mernd", "wie", "ein", "Traum\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wu\u00dfte ich, es war der Heimat Licht,", "tokens": ["Doch", "wu\u00df\u00b7te", "ich", ",", "es", "war", "der", "Hei\u00b7mat", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In meiner eignen Kammer angeschlagen.", "tokens": ["In", "mei\u00b7ner", "eig\u00b7nen", "Kam\u00b7mer", "an\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ringsum so still, da\u00df ich vernahm im Laub", "tokens": ["Ring\u00b7sum", "so", "still", ",", "da\u00df", "ich", "ver\u00b7nahm", "im", "Laub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Raupe Nagen, und wie gr\u00fcner Staub", "tokens": ["Der", "Rau\u00b7pe", "Na\u00b7gen", ",", "und", "wie", "gr\u00fc\u00b7ner", "Staub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "KON", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich leise wirbelnd Bl\u00e4tterfl\u00f6ckchen trafen.", "tokens": ["Mich", "lei\u00b7se", "wir\u00b7belnd", "Bl\u00e4t\u00b7ter\u00b7fl\u00f6ck\u00b7chen", "tra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich lag und dachte, ach so manchem nach,", "tokens": ["Ich", "lag", "und", "dach\u00b7te", ",", "ach", "so", "man\u00b7chem", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ITJ", "ADV", "PIAT", "APPR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich h\u00f6rte meines eignen Herzens Schlag,", "tokens": ["Ich", "h\u00f6r\u00b7te", "mei\u00b7nes", "eig\u00b7nen", "Her\u00b7zens", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Fast war es mir als sei ich schon entschlafen.", "tokens": ["Fast", "war", "es", "mir", "als", "sei", "ich", "schon", "ent\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "KOKOM", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gedanken tauchten aus Gedanken auf,", "tokens": ["Ge\u00b7dan\u00b7ken", "tauch\u00b7ten", "aus", "Ge\u00b7dan\u00b7ken", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Kinderspiel, der frischen Jahre Lauf,", "tokens": ["Das", "Kin\u00b7der\u00b7spiel", ",", "der", "fri\u00b7schen", "Jah\u00b7re", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gesichter, die mir lange fremd geworden;", "tokens": ["Ge\u00b7sich\u00b7ter", ",", "die", "mir", "lan\u00b7ge", "fremd", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verge\u00dfne T\u00f6ne summten um mein Ohr,", "tokens": ["Ver\u00b7ge\u00df\u00b7ne", "T\u00f6\u00b7ne", "summ\u00b7ten", "um", "mein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und endlich trat die Gegenwart hervor,", "tokens": ["Und", "end\u00b7lich", "trat", "die", "Ge\u00b7gen\u00b7wart", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da stand die Welle, wie an Ufers Borden.", "tokens": ["Da", "stand", "die", "Wel\u00b7le", ",", "wie", "an", "U\u00b7fers", "Bor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "APPR", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dann, gleich dem Bronnen, der verrinnt im Schlund,", "tokens": ["Dann", ",", "gleich", "dem", "Bron\u00b7nen", ",", "der", "ver\u00b7rinnt", "im", "Schlund", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dr\u00fcben wieder sprudelt aus dem Grund,", "tokens": ["Und", "dr\u00fc\u00b7ben", "wie\u00b7der", "spru\u00b7delt", "aus", "dem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So stand ich pl\u00f6tzlich in der Zukunft Lande;", "tokens": ["So", "stand", "ich", "pl\u00f6tz\u00b7lich", "in", "der", "Zu\u00b7kunft", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah mich selber, gar geb\u00fcckt und klein,", "tokens": ["Ich", "sah", "mich", "sel\u00b7ber", ",", "gar", "ge\u00b7b\u00fcckt", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geschw\u00e4chten Auges, am ererbten Schrein", "tokens": ["Ge\u00b7schw\u00e4ch\u00b7ten", "Au\u00b7ges", ",", "am", "er\u00b7erb\u00b7ten", "Schrein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sorgf\u00e4ltig ordnen staub'ge Liebespfande.", "tokens": ["Sorg\u00b7f\u00e4l\u00b7tig", "ord\u00b7nen", "staub'\u00b7ge", "Lie\u00b7be\u00b7spfan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Bilder meiner Lieben sah ich klar,", "tokens": ["Die", "Bil\u00b7der", "mei\u00b7ner", "Lie\u00b7ben", "sah", "ich", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einer Tracht, die jetzt veraltet war,", "tokens": ["In", "ei\u00b7ner", "Tracht", ",", "die", "jetzt", "ver\u00b7al\u00b7tet", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich sorgsam l\u00f6sen aus verblichnen H\u00fcllen,", "tokens": ["Mich", "sorg\u00b7sam", "l\u00f6\u00b7sen", "aus", "ver\u00b7blich\u00b7nen", "H\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00f6ckchen, vermorscht, zu Staub zerfallen schier,", "tokens": ["L\u00f6ck\u00b7chen", ",", "ver\u00b7morscht", ",", "zu", "Staub", "zer\u00b7fal\u00b7len", "schier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "APPR", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Sah \u00fcber die gefurchte Wange mir", "tokens": ["Sah", "\u00fc\u00b7ber", "die", "ge\u00b7furch\u00b7te", "Wan\u00b7ge", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Langsam herab die karge Tr\u00e4ne quillen.", "tokens": ["Lang\u00b7sam", "her\u00b7ab", "die", "kar\u00b7ge", "Tr\u00e4\u00b7ne", "quil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Und wieder an des Friedhofs Monument,", "tokens": ["Und", "wie\u00b7der", "an", "des", "Fried\u00b7hofs", "Mo\u00b7nu\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dran Namen standen die mein Lieben kennt,", "tokens": ["Dran", "Na\u00b7men", "stan\u00b7den", "die", "mein", "Lie\u00b7ben", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVFIN", "ART", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da lag ich betend, mit gebrochnen Knieen,", "tokens": ["Da", "lag", "ich", "be\u00b7tend", ",", "mit", "ge\u00b7broch\u00b7nen", "Kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und \u2013 horch, die Wachtel schlug! K\u00fchl strich der Hauch \u2013", "tokens": ["Und", "\u2013", "horch", ",", "die", "Wach\u00b7tel", "schlug", "!", "K\u00fchl", "strich", "der", "Hauch", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVIMP", "$,", "ART", "NN", "VVFIN", "$.", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und noch zuletzt sah ich, gleich einem Rauch,", "tokens": ["Und", "noch", "zu\u00b7letzt", "sah", "ich", ",", "gleich", "ei\u00b7nem", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich leise in der Erde Poren ziehen.", "tokens": ["Mich", "lei\u00b7se", "in", "der", "Er\u00b7de", "Po\u00b7ren", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich fuhr empor, und sch\u00fcttelte mich dann,", "tokens": ["Ich", "fuhr", "em\u00b7por", ",", "und", "sch\u00fct\u00b7tel\u00b7te", "mich", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie einer, der dem Scheintod erst entrann,", "tokens": ["Wie", "ei\u00b7ner", ",", "der", "dem", "Schein\u00b7tod", "erst", "ent\u00b7rann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und taumelte entlang die dunklen Hage,", "tokens": ["Und", "tau\u00b7mel\u00b7te", "ent\u00b7lang", "die", "dunk\u00b7len", "Ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch immer zweifelnd, ob der Stern am Rain", "tokens": ["Noch", "im\u00b7mer", "zwei\u00b7felnd", ",", "ob", "der", "Stern", "am", "Rain"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sei wirklich meiner Schlummerlampe Schein,", "tokens": ["Sei", "wirk\u00b7lich", "mei\u00b7ner", "Schlum\u00b7mer\u00b7lam\u00b7pe", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Oder das ew'ge Licht am Sarkophage.", "tokens": ["O\u00b7der", "das", "ew'\u00b7ge", "Licht", "am", "Sar\u00b7ko\u00b7pha\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}