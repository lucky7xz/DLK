{"textgrid.poem.44314": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Wollust", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwestern sind sie, doch sie meiden", "tokens": ["Schwes\u00b7tern", "sind", "sie", ",", "doch", "sie", "mei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ewig sich ohn Unterla\u00df;", "tokens": ["e\u00b7wig", "sich", "ohn", "Un\u00b7ter\u00b7la\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "w\u00e4hlst du eine von den beiden,", "tokens": ["w\u00e4hlst", "du", "ei\u00b7ne", "von", "den", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "APPR", "ART", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mu\u00dft du von der andern scheiden,", "tokens": ["mu\u00dft", "du", "von", "der", "an\u00b7dern", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "schw\u00f6rest du der andern Ha\u00df!", "tokens": ["schw\u00f6\u00b7rest", "du", "der", "an\u00b7dern", "Ha\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwestern sind sie, \u00e4hnlich scheinen", "tokens": ["Schwes\u00b7tern", "sind", "sie", ",", "\u00e4hn\u00b7lich", "schei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sie beim ersten Blicke, doch", "tokens": ["sie", "beim", "ers\u00b7ten", "Bli\u00b7cke", ",", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wer sie suchet zu vereinen,", "tokens": ["wer", "sie", "su\u00b7chet", "zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wird den Irrtum bald beweinen,", "tokens": ["wird", "den", "Irr\u00b7tum", "bald", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "denn er nimmt der Wollust Joch.", "tokens": ["denn", "er", "nimmt", "der", "Wol\u00b7lust", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unter mancherlei Gestalten", "tokens": ["Un\u00b7ter", "man\u00b7cher\u00b7lei", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schleichet sich die Listge ein,", "tokens": ["schlei\u00b7chet", "sich", "die", "List\u00b7ge", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "l\u00e4\u00dft du einmal sie obwalten,", "tokens": ["l\u00e4\u00dft", "du", "ein\u00b7mal", "sie", "ob\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wei\u00df sie schlau dich festzuhalten,", "tokens": ["wei\u00df", "sie", "schlau", "dich", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "mu\u00dft du stets ihr Sklave sein!", "tokens": ["mu\u00dft", "du", "stets", "ihr", "Skla\u00b7ve", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihren schimmernden Alt\u00e4ren", "tokens": ["Ih\u00b7ren", "schim\u00b7mern\u00b7den", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nahet sich die halbe Welt;", "tokens": ["na\u00b7het", "sich", "die", "hal\u00b7be", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ihre s\u00fc\u00dfe Macht verehren", "tokens": ["ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Macht", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die selbst unter Schmerzensz\u00e4hren,", "tokens": ["die", "selbst", "un\u00b7ter", "Schmer\u00b7zens\u00b7z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die ihr Stachel teuflisch qu\u00e4lt.", "tokens": ["die", "ihr", "Sta\u00b7chel", "teuf\u00b7lisch", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6n ist ihre Au\u00dfenseite,", "tokens": ["Sch\u00f6n", "ist", "ih\u00b7re", "Au\u00b7\u00dfen\u00b7sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "lieblich lacht ihr Rosenmund,", "tokens": ["lieb\u00b7lich", "lacht", "ihr", "Ro\u00b7sen\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aber lockend lacht sie heute", "tokens": ["a\u00b7ber", "lo\u00b7ckend", "lacht", "sie", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und st\u00f6\u00dft morgen ihre Beute", "tokens": ["und", "st\u00f6\u00dft", "mor\u00b7gen", "ih\u00b7re", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "wild in der Verzweiflung Schlund;", "tokens": ["wild", "in", "der", "Ver\u00b7zwei\u00b7flung", "Schlund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "in der hocherhabnen Linken", "tokens": ["in", "der", "ho\u00b7cher\u00b7hab\u00b7nen", "Lin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00e4lt sie schmeichelnd den Pokal,", "tokens": ["h\u00e4lt", "sie", "schmei\u00b7chelnd", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aber folgst du ihren Winken,", "tokens": ["a\u00b7ber", "folgst", "du", "ih\u00b7ren", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "willst den s\u00fc\u00dfen Nektar trinken,", "tokens": ["willst", "den", "s\u00fc\u00b7\u00dfen", "Nek\u00b7tar", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "mordet dich ihr scharfer Stahl.", "tokens": ["mor\u00b7det", "dich", "ihr", "schar\u00b7fer", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ruhig, mit bescheidnen Mienen", "tokens": ["Ru\u00b7hig", ",", "mit", "be\u00b7scheid\u00b7nen", "Mie\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "naht die zweite, sanft und mild;", "tokens": ["naht", "die", "zwei\u00b7te", ",", "sanft", "und", "mild", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und wohl denen, die ihr dienen", "tokens": ["und", "wohl", "de\u00b7nen", ",", "die", "ihr", "die\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PDS", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "rein und makellos; wohl ihnen,", "tokens": ["rein", "und", "ma\u00b7kel\u00b7los", ";", "wohl", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$.", "ADV", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "all ihr Sehnen wird erf\u00fcllt.", "tokens": ["all", "ihr", "Seh\u00b7nen", "wird", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Zwar lockt nicht mit frechen Blicken", "tokens": ["Zwar", "lockt", "nicht", "mit", "fre\u00b7chen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "sie dich an die keusche Brust,", "tokens": ["sie", "dich", "an", "die", "keu\u00b7sche", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "doch wen ihre Gaben schm\u00fccken,", "tokens": ["doch", "wen", "ih\u00b7re", "Ga\u00b7ben", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "f\u00fcllt das reine Herz Entz\u00fccken,", "tokens": ["f\u00fcllt", "das", "rei\u00b7ne", "Herz", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seligkeit und Himmelslust!", "tokens": ["Se\u00b7lig\u00b7keit", "und", "Him\u00b7mels\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wohl dem edlen Erdensohne,", "tokens": ["Wohl", "dem", "ed\u00b7len", "Er\u00b7den\u00b7soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der ihr ewge Treue schwor,", "tokens": ["der", "ihr", "ew\u00b7ge", "Treu\u00b7e", "schwor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "denn der reinen Liebe Krone", "tokens": ["denn", "der", "rei\u00b7nen", "Lie\u00b7be", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "reicht sie ihm zum sch\u00f6nen Lohne,", "tokens": ["reicht", "sie", "ihm", "zum", "sch\u00f6\u00b7nen", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "hebt zu G\u00f6ttern ihn empor!", "tokens": ["hebt", "zu", "G\u00f6t\u00b7tern", "ihn", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich auch habe sie gefunden,", "tokens": ["Ich", "auch", "ha\u00b7be", "sie", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mich auch schm\u00fcckt ihr Rosenband,", "tokens": ["mich", "auch", "schm\u00fcckt", "ihr", "Ro\u00b7sen\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "meine Stirn hat sie umwunden", "tokens": ["mei\u00b7ne", "Stirn", "hat", "sie", "um\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "in der seligsten der Stunden,", "tokens": ["in", "der", "se\u00b7ligs\u00b7ten", "der", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "als ich meine Molly fand!", "tokens": ["als", "ich", "mei\u00b7ne", "Mol\u00b7ly", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Mich reizt nicht das Gl\u00fcck der Toren,", "tokens": ["Mich", "reizt", "nicht", "das", "Gl\u00fcck", "der", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "nicht der Wollust Vollgenu\u00df,", "tokens": ["nicht", "der", "Wol\u00b7lust", "Voll\u00b7ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe, dich hab ich erkoren,", "tokens": ["Lie\u00b7be", ",", "dich", "hab", "ich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "als ich Molly Treu geschworen,", "tokens": ["als", "ich", "Mol\u00b7ly", "Treu", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "bei der Holden ersten Ku\u00df!", "tokens": ["bei", "der", "Hol\u00b7den", "ers\u00b7ten", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Weg da mit dem eiteln Ruhme,", "tokens": ["Weg", "da", "mit", "dem", "ei\u00b7teln", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "feiler Wollust Knecht zu sein!", "tokens": ["fei\u00b7ler", "Wol\u00b7lust", "Knecht", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in der Liebe Heiligtume", "tokens": ["in", "der", "Lie\u00b7be", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bl\u00fcht mir eine sch\u00f6nre Blume,", "tokens": ["bl\u00fcht", "mir", "ei\u00b7ne", "sch\u00f6n\u00b7re", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Molly, Molly, du bist mein!", "tokens": ["Mol\u00b7ly", ",", "Mol\u00b7ly", ",", "du", "bist", "mein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}