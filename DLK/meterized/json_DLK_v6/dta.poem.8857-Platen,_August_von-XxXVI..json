{"dta.poem.8857": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrcht' ich, da\u00df die braune Scheitel \u00fcber Nacht mir grau", "tokens": ["F\u00fcrcht'", "ich", ",", "da\u00df", "die", "brau\u00b7ne", "Schei\u00b7tel", "\u00fc\u00b7ber", "Nacht", "mir", "grau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "NN", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Geizest du mit Augenblicken, die mir mehr als dir ge-", "tokens": ["Gei\u00b7zest", "du", "mit", "Au\u00b7gen\u00b7bli\u00b7cken", ",", "die", "mir", "mehr", "als", "dir", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "PRELS", "PPER", "ADV", "KOUS", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Bist du, lieblicher Verschwender, pl\u00f6tzlich so genau ge-", "tokens": ["Bist", "du", ",", "lieb\u00b7li\u00b7cher", "Ver\u00b7schwen\u00b7der", ",", "pl\u00f6tz\u00b7lich", "so", "ge\u00b7nau", "ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADJD", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Haben deiner Treue Rosen sich als Dorn den Stolz er-", "tokens": ["Ha\u00b7ben", "dei\u00b7ner", "Treu\u00b7e", "Ro\u00b7sen", "sich", "als", "Dorn", "den", "Stolz", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "PRF", "KOUS", "NN", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Sind der Liebesg\u00f6ttin Tauben wie der Juno Pfau ge-", "tokens": ["Sind", "der", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tin", "Tau\u00b7ben", "wie", "der", "Ju\u00b7no", "Pfau", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "KOKOM", "ART", "NN", "NE", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Wenn dich Weiber mir gestohlen, werden sie so lang dich", "tokens": ["Wenn", "dich", "Wei\u00b7ber", "mir", "ge\u00b7stoh\u00b7len", ",", "wer\u00b7den", "sie", "so", "lang", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PPER", "VVPP", "$,", "VAFIN", "PPER", "ADV", "ADJD", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Bis der Tempel deiner Glieder ein zerst\u00f6rter Bau ge-", "tokens": ["Bis", "der", "Tem\u00b7pel", "dei\u00b7ner", "Glie\u00b7der", "ein", "zer\u00b7st\u00f6r\u00b7ter", "Bau", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Oder willst du blos mich locken, den du l\u00e4ngst im Netz", "tokens": ["O\u00b7der", "willst", "du", "blos", "mich", "lo\u00b7cken", ",", "den", "du", "l\u00e4ngst", "im", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "O so lohnt sich's nicht der M\u00fche, da\u00df du kalt und schlau", "tokens": ["O", "so", "lohnt", "sich's", "nicht", "der", "M\u00fc\u00b7he", ",", "da\u00df", "du", "kalt", "und", "schlau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PIS", "PTKNEG", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}