{"dta.poem.8377": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wardt gefangem zwar/ vndt habe viell erlitten/", "tokens": ["Ich", "wardt", "ge\u00b7fan\u00b7gem", "zwar", "/", "vndt", "ha\u00b7be", "viell", "er\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "ADV", "$(", "KON", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wilder Saracen/ nach dem ich dich bestritten:", "tokens": ["Du", "wil\u00b7der", "Sa\u00b7ra\u00b7cen", "/", "nach", "dem", "ich", "dich", "be\u00b7strit\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch was dann hast du jetzt von mir in deiner handt?", "tokens": ["Doch", "was", "dann", "hast", "du", "jetzt", "von", "mir", "in", "dei\u00b7ner", "handt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der himmel hatt den geist/ den leib das Vaterlandt.", "tokens": ["Der", "him\u00b7mel", "hatt", "den", "geist", "/", "den", "leib", "das", "Va\u00b7ter\u00b7landt", "."], "token_info": ["word", "word", "word", "word", "word",