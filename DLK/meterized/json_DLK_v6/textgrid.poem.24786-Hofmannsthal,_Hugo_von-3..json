{"textgrid.poem.24786": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebste sprach: \u00bbIch halt dich nicht,", "tokens": ["Die", "Liebs\u00b7te", "sprach", ":", "\u00bb", "Ich", "halt", "dich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast mir nichts geschworn.", "tokens": ["Du", "hast", "mir", "nichts", "ge\u00b7schworn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Menschen soll man halten nicht,", "tokens": ["Die", "Men\u00b7schen", "soll", "man", "hal\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind nicht zur Treu geborn.", "tokens": ["Sind", "nicht", "zur", "Treu", "ge\u00b7born", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zieh deine Stra\u00dfen hin, mein Freund,", "tokens": ["Zieh", "dei\u00b7ne", "Stra\u00b7\u00dfen", "hin", ",", "mein", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschau dir Land um Land,", "tokens": ["Be\u00b7schau", "dir", "Land", "um", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In vielen Betten ruh dich aus,", "tokens": ["In", "vie\u00b7len", "Bet\u00b7ten", "ruh", "dich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel Frauen nimm bei der Hand.", "tokens": ["Viel", "Frau\u00b7en", "nimm", "bei", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wo dir der Wein zu sauer ist,", "tokens": ["Wo", "dir", "der", "Wein", "zu", "sau\u00b7er", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da trink du Malvasier,", "tokens": ["Da", "trink", "du", "Mal\u00b7va\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn mein Mund dir s\u00fc\u00dfer ist,", "tokens": ["Und", "wenn", "mein", "Mund", "dir", "s\u00fc\u00b7\u00dfer", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So komm nur wieder zu mir!\u00ab", "tokens": ["So", "komm", "nur", "wie\u00b7der", "zu", "mir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}