{"textgrid.poem.37529": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Idiosynkrasie", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist grau. Die Wolken ziehn.", "tokens": ["Der", "Tag", "ist", "grau", ".", "Die", "Wol\u00b7ken", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es saust die alte M\u00fchle.", "tokens": ["Es", "saust", "die", "al\u00b7te", "M\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich schlendre durch das feuchte Gr\u00fcn", "tokens": ["Ich", "schlend\u00b7re", "durch", "das", "feuch\u00b7te", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denke an meine Gef\u00fchle.", "tokens": ["Und", "den\u00b7ke", "an", "mei\u00b7ne", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die Sache ist mir nicht genehm.", "tokens": ["Die", "Sa\u00b7che", "ist", "mir", "nicht", "ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich \u00e4rgre mich fast dar\u00fcber.", "tokens": ["Ich", "\u00e4r\u00b7gre", "mich", "fast", "da\u00b7r\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der M\u00fcller ist gut; trotz alledem", "tokens": ["Der", "M\u00fcl\u00b7ler", "ist", "gut", ";", "trotz", "al\u00b7le\u00b7dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$.", "APPR", "PIS"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ist mir die M\u00fcllerin lieber.", "tokens": ["Ist", "mir", "die", "M\u00fcl\u00b7le\u00b7rin", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}