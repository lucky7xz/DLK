{"dta.poem.8731": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Weil geb\u00e4ndiget vom Eise,", "tokens": ["Weil", "ge\u00b7b\u00e4n\u00b7di\u00b7get", "vom", "Ei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht die Jugend leichte Kreise,", "tokens": ["Zieht", "die", "Ju\u00b7gend", "leich\u00b7te", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelnd auf den Fl\u00fcgelschuhen.", "tokens": ["Wan\u00b7delnd", "auf", "den", "Fl\u00fc\u00b7gel\u00b7schu\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch ich wandle, Freund, alleine,", "tokens": ["Doch", "ich", "wand\u00b7le", ",", "Freund", ",", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "NN", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freund, allein und nicht zum Ziele:", "tokens": ["Freund", ",", "al\u00b7lein", "und", "nicht", "zum", "Zie\u00b7le", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KON", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Gestalten sind so viele,", "tokens": ["Der", "Ge\u00b7stal\u00b7ten", "sind", "so", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leider aber nicht die Deine.", "tokens": ["Lei\u00b7der", "a\u00b7ber", "nicht", "die", "Dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ART", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hefte den Kothurn der Wogen", "tokens": ["Hef\u00b7te", "den", "Ko\u00b7thurn", "der", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An die leichten Hermesf\u00fc\u00dfe,", "tokens": ["An", "die", "leich\u00b7ten", "Her\u00b7mes\u00b7f\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df begegnend bald dich gr\u00fc\u00dfe,", "tokens": ["Da\u00df", "be\u00b7ge\u00b7gnend", "bald", "dich", "gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "ADV", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Dem du dich so lang' entzogen!", "tokens": ["Dem", "du", "dich", "so", "lang'", "ent\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch ein Gl\u00fcck, dahin zu schwinden", "tokens": ["Welch", "ein", "Gl\u00fcck", ",", "da\u00b7hin", "zu", "schwin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PAV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Fl\u00e4che, klar und eben,", "tokens": ["Auf", "der", "Fl\u00e4\u00b7che", ",", "klar", "und", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Magisch sich vor\u00fcberschweben,", "tokens": ["Ma\u00b7gisch", "sich", "vor\u00b7\u00fc\u00b7bersc\u00b7hwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flieh'n sich und sich wiederfinden!", "tokens": ["Flieh'n", "sich", "und", "sich", "wie\u00b7der\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber ist es nicht vergebens?", "tokens": ["A\u00b7ber", "ist", "es", "nicht", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weilst du nicht, was kann es frommen?", "tokens": ["Weilst", "du", "nicht", ",", "was", "kann", "es", "from\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dies unst\u00e4te Gehn und Kommen", "tokens": ["Dies", "un\u00b7st\u00e4\u00b7te", "Gehn", "und", "Kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist das wahre Bild des Lebens.", "tokens": ["Ist", "das", "wah\u00b7re", "Bild", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}