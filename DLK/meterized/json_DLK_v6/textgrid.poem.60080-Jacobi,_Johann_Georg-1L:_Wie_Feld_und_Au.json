{"textgrid.poem.60080": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Feld und Au", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Feld und Au", "tokens": ["Wie", "Feld", "und", "Au"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So blinkend im Thau!", "tokens": ["So", "blin\u00b7kend", "im", "Thau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie Perlen-schwer", "tokens": ["Wie", "Per\u00b7len\u00b7schwer"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die Pflanzen umher!", "tokens": ["Die", "Pflan\u00b7zen", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie durch den Hain", "tokens": ["Wie", "durch", "den", "Hain"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die L\u00fcfte so rein!", "tokens": ["Die", "L\u00fcf\u00b7te", "so", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie laut im hellen Sonnenstrahl,", "tokens": ["Wie", "laut", "im", "hel\u00b7len", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die s\u00fc\u00dfen V\u00f6glein allzumahl!", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "V\u00f6\u00b7glein", "all\u00b7zu\u00b7mahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! aber da,", "tokens": ["Ach", "!", "a\u00b7ber", "da", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wo Liebchen ich sah,", "tokens": ["Wo", "Lieb\u00b7chen", "ich", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im K\u00e4mmerlein,", "tokens": ["Im", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So nieder und klein,", "tokens": ["So", "nie\u00b7der", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "So rings bedeckt,", "tokens": ["So", "rings", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Sonne versteckt \u2013", "tokens": ["Der", "Son\u00b7ne", "ver\u00b7steckt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wo blieb die Erde weit und breit,", "tokens": ["Wo", "blieb", "die", "Er\u00b7de", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit aller ihrer Herrlichkeit?", "tokens": ["Mit", "al\u00b7ler", "ih\u00b7rer", "Herr\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}