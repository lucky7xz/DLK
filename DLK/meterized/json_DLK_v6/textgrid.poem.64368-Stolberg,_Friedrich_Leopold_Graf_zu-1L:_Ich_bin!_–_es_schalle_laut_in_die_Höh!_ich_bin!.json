{"textgrid.poem.64368": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin! \u2013 es schalle laut in die H\u00f6h! ich bin!", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin! \u2013 es schalle laut in die H\u00f6h! ich bin!", "tokens": ["Ich", "bin", "!", "\u2013", "es", "schal\u00b7le", "laut", "in", "die", "H\u00f6h", "!", "ich", "bin", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "$(", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich bin! \u2013 es schalle laut in die Tief'! \u2013 O Sein,", "tokens": ["Ich", "bin", "!", "\u2013", "es", "schal\u00b7le", "laut", "in", "die", "Tie\u00b7f'", "!", "\u2013", "O", "Sein", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "$(", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$.", "$(", "NE", "PPOSAT", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du Born, aus welchem, Zwillingsquellen,", "tokens": ["Du", "Born", ",", "aus", "wel\u00b7chem", ",", "Zwil\u00b7lings\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NE", "$,", "APPR", "PWAT", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ewiges Leben und Wonne str\u00f6men;", "tokens": ["E\u00b7wi\u00b7ges", "Le\u00b7ben", "und", "Won\u00b7ne", "str\u00f6\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Zwar Staub, und Sturm, und himmelverbergender", "tokens": ["Zwar", "Staub", ",", "und", "Sturm", ",", "und", "him\u00b7mel\u00b7ver\u00b7ber\u00b7gen\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "KON", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gew\u00f6lke Schemen tr\u00fcben ihn oft, doch oft", "tokens": ["Ge\u00b7w\u00f6l\u00b7ke", "Sche\u00b7men", "tr\u00fc\u00b7ben", "ihn", "oft", ",", "doch", "oft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tr\u00fcbt unsern Blick der Feigheit Thr\u00e4ne,", "tokens": ["Tr\u00fcbt", "un\u00b7sern", "Blick", "der", "Feig\u00b7heit", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00fcbet im Borne das Bild des Himmels!", "tokens": ["Tr\u00fc\u00b7bet", "im", "Bor\u00b7ne", "das", "Bild", "des", "Him\u00b7mels", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Gescholten sei im Namen des Herrn, du Dunst", "tokens": ["Ge\u00b7schol\u00b7ten", "sei", "im", "Na\u00b7men", "des", "Herrn", ",", "du", "Dunst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$,", "PPER", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Abgrunds, Kleinmut! Hebe dich! \u2013 Nahet euch,", "tokens": ["Des", "Ab\u00b7grunds", ",", "Klein\u00b7mut", "!", "He\u00b7be", "dich", "!", "\u2013", "Na\u00b7het", "euch", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$.", "VVIMP", "PPER", "$.", "$(", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Himmels Kinder, Lieb' und Glaube,", "tokens": ["Des", "Him\u00b7mels", "Kin\u00b7der", ",", "Lieb'", "und", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stimmet die Seele des Erdensohnes.", "tokens": ["Stim\u00b7met", "die", "See\u00b7le", "des", "Er\u00b7den\u00b7soh\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Du Glaube, dessen Lampe wie Mondenschein", "tokens": ["Du", "Glau\u00b7be", ",", "des\u00b7sen", "Lam\u00b7pe", "wie", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELAT", "NN", "KOKOM", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Nacht erhellet! Liebe, Gespielin du", "tokens": ["Die", "Nacht", "er\u00b7hel\u00b7let", "!", "Lie\u00b7be", ",", "Ge\u00b7spie\u00b7lin", "du"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$,", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der hohen Weisheit, welche Sonnen", "tokens": ["Der", "ho\u00b7hen", "Weis\u00b7heit", ",", "wel\u00b7che", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "S\u00e4te, und Odem den Geistern einblies!", "tokens": ["S\u00e4\u00b7te", ",", "und", "O\u00b7dem", "den", "Geis\u00b7tern", "ein\u00b7blies", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "O kommt und bleibet! da\u00df sich mit Schwanensang", "tokens": ["O", "kommt", "und", "blei\u00b7bet", "!", "da\u00df", "sich", "mit", "Schwa\u00b7nen\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "PRF", "APPR", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Geist erhebe, wenn ihm die H\u00fcll' entsinkt!", "tokens": ["Mein", "Geist", "er\u00b7he\u00b7be", ",", "wenn", "ihm", "die", "H\u00fcll'", "ent\u00b7sinkt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn \u2013 o der Wonne, die des Menschen", "tokens": ["Wenn", "\u2013", "o", "der", "Won\u00b7ne", ",", "die", "des", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$(", "FM", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Harret! der immer verge\u00dfnen Wonne!", "tokens": ["Har\u00b7ret", "!", "der", "im\u00b7mer", "ver\u00b7ge\u00df\u00b7nen", "Won\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Uneingedenk der Zukunft frohlockt der Mensch", "tokens": ["Un\u00b7ein\u00b7ge\u00b7denk", "der", "Zu\u00b7kunft", "froh\u00b7lockt", "der", "Mensch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als Thor, und jammert wieder uneingedenk!", "tokens": ["Als", "Thor", ",", "und", "jam\u00b7mert", "wie\u00b7der", "un\u00b7ein\u00b7ge\u00b7denk", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Es staunen Engel! unsre Toten", "tokens": ["Es", "stau\u00b7nen", "En\u00b7gel", "!", "uns\u00b7re", "To\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schauen herab mit der Liebe Wehmut;", "tokens": ["Schau\u00b7en", "her\u00b7ab", "mit", "der", "Lie\u00b7be", "Weh\u00b7mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Verm\u00f6gen nicht den nimmergestorbenen", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "nicht", "den", "nim\u00b7mer\u00b7ge\u00b7stor\u00b7be\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das R\u00e4tsel Mensch zu l\u00f6sen! wiewohl auch sie", "tokens": ["Das", "R\u00e4t\u00b7sel", "Mensch", "zu", "l\u00f6\u00b7sen", "!", "wie\u00b7wohl", "auch", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$.", "KOUS", "ADV", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So neulich R\u00e4tsel, hofften, zagten,", "tokens": ["So", "neu\u00b7lich", "R\u00e4t\u00b7sel", ",", "hoff\u00b7ten", ",", "zag\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sterblich und ewig, und Staub und Geister!", "tokens": ["Sterb\u00b7lich", "und", "e\u00b7wig", ",", "und", "Staub", "und", "Geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}