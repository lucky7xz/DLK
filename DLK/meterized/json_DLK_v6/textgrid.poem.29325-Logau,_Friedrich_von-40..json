{"textgrid.poem.29325": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann Beamten, wie sie sollen,", "tokens": ["Wann", "Be\u00b7am\u00b7ten", ",", "wie", "sie", "sol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht Geschencke solten nemen,", "tokens": ["Nicht", "Ge\u00b7schen\u00b7cke", "sol\u00b7ten", "ne\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrde selten iemand wollen", "tokens": ["W\u00fcr\u00b7de", "sel\u00b7ten", "ie\u00b7mand", "wol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PIS", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zu Amt und Dienst bequemen.", "tokens": ["Sich", "zu", "Amt", "und", "Dienst", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}