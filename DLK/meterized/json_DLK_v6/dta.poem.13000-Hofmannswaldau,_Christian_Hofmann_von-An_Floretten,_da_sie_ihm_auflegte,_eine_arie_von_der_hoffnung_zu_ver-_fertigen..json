{"dta.poem.13000": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Floretten, da sie ihm auflegte,  \n eine arie von der hoffnung zu ver-  \n fertigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leanders hertze mu\u00df mit der verzweifflung ringen,", "tokens": ["Le\u00b7an\u00b7ders", "hert\u00b7ze", "mu\u00df", "mit", "der", "ver\u00b7zweif\u00b7flung", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihm Florettens kiel kein wort zur antwort giebt.", "tokens": ["Weil", "ihm", "Flo\u00b7ret\u00b7tens", "kiel", "kein", "wort", "zur", "ant\u00b7wort", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wie kan die Muse nun, die sich so tieff betr\u00fcbt,", "tokens": ["Wie", "kan", "die", "Mu\u00b7se", "nun", ",", "die", "sich", "so", "tieff", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein angenehmes lied von s\u00fcsser hoffnung singen?", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mes", "lied", "von", "s\u00fcs\u00b7ser", "hoff\u00b7nung", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": [