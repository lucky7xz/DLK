{"dta.poem.21543": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Maria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nach Monden stand ich wieder vor dem Hause,", "tokens": ["Nach", "Mon\u00b7den", "stand", "ich", "wie\u00b7der", "vor", "dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das einst dich, s\u00fc\u00dfe Huldgestalt, umfing.", "tokens": ["Das", "einst", "dich", ",", "s\u00fc\u00b7\u00dfe", "Huld\u00b7ge\u00b7stalt", ",", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie war doch Alles anders wie vordem,", "tokens": ["Wie", "war", "doch", "Al\u00b7les", "an\u00b7ders", "wie", "vor\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PIS", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tiefbange Stille herrschte rings umher,", "tokens": ["Tief\u00b7ban\u00b7ge", "Stil\u00b7le", "herrschte", "rings", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die B\u00e4ume standen schl\u00e4frig, m\u00fcde da,", "tokens": ["Die", "B\u00e4u\u00b7me", "stan\u00b7den", "schl\u00e4f\u00b7rig", ",", "m\u00fc\u00b7de", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Springquell, der uns einst das s\u00fc\u00dfe Lied,", "tokens": ["Der", "Spring\u00b7quell", ",", "der", "uns", "einst", "das", "s\u00fc\u00b7\u00dfe", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Zauberlied von Gl\u00fcck und Liebe sang,", "tokens": ["Das", "Zau\u00b7berl\u00b7ied", "von", "Gl\u00fcck", "und", "Lie\u00b7be", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er rauschte nicht; und stumm und traurig hockte", "tokens": ["Er", "rauschte", "nicht", ";", "und", "stumm", "und", "trau\u00b7rig", "hock\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "KON", "ADJD", "KON", "ADJD", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Gott Amor selbst, der Schelm, auf seinem Stein.", "tokens": ["Gott", "A\u00b7mor", "selbst", ",", "der", "Schelm", ",", "auf", "sei\u00b7nem", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Todt lag der Park, todt Haus und todt der Hof,", "tokens": ["Todt", "lag", "der", "Park", ",", "todt", "Haus", "und", "todt", "der", "Hof", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Denn ach die Seele, du, du warst entfloh\u2019n.", "tokens": ["Denn", "ach", "die", "See\u00b7le", ",", "du", ",", "du", "warst", "ent\u00b7floh'", "n."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PPER", "$,", "PPER", "VAFIN", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lang\u2019 lehnt ich an des Gartenthores Gitter", "tokens": ["Lang'", "lehnt", "ich", "an", "des", "Gar\u00b7ten\u00b7tho\u00b7res", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und starrte auf die St\u00e4tte meines todten,", "tokens": ["Und", "starr\u00b7te", "auf", "die", "St\u00e4t\u00b7te", "mei\u00b7nes", "tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlor\u2019nen Gl\u00fcckes; auf die Erde glitten", "tokens": ["Ver\u00b7lor'\u00b7nen", "Gl\u00fc\u00b7ckes", ";", "auf", "die", "Er\u00b7de", "glit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir Hut und Stab; der Nachtwind flatterte", "tokens": ["Mir", "Hut", "und", "Stab", ";", "der", "Nacht\u00b7wind", "flat\u00b7ter\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir durch das Haar und k\u00fchlte das vom Schmerze,", "tokens": ["Mir", "durch", "das", "Haar", "und", "k\u00fchl\u00b7te", "das", "vom", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von tiefem Weh durchzuckte, hei\u00dfe Hirn.", "tokens": ["Von", "tie\u00b7fem", "Weh", "durch\u00b7zuck\u00b7te", ",", "hei\u00b7\u00dfe", "Hirn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann wand\u2019 ich trauernd meinen Fu\u00df und ging,", "tokens": ["Dann", "wand'", "ich", "trau\u00b7ernd", "mei\u00b7nen", "Fu\u00df", "und", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In Thr\u00e4nen dacht\u2019 ich dein, entschlafene", "tokens": ["In", "Thr\u00e4\u00b7nen", "dacht'", "ich", "dein", ",", "ent\u00b7schla\u00b7fe\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "$,", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Maria . . . .", "tokens": ["Ma\u00b7ria", ".", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "punct"], "pos": ["NE", "$.", "$.", "$.", "$."], "meter": "--", "measure": "unknown.measure.zero"}}}}}