{"dta.poem.11291": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der jungferschafft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer liegt die jungferschafft. Dieweil sie nichts gewesen,", "tokens": ["Hjer", "liegt", "die", "jung\u00b7fer\u00b7schafft", ".", "Die\u00b7weil", "sie", "nichts", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "KOUS", "PPER", "PIS", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kan man auch allhier von ihr nichts weiter lesen.", "tokens": ["So", "kan", "man", "auch", "all\u00b7hier", "von", "ihr", "nichts", "wei\u00b7ter", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV", "APPR", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}