{"textgrid.poem.52414": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "64. Hei is woll klauk up sine B\u00e4uker,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Schaulrat Ix ut Ixenstein", "tokens": ["De", "Schaul\u00b7rat", "Ix", "ut", "I\u00b7xens\u00b7tein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00fcrd mal tau Kramersd\u00f6rp nahseihn,", "tokens": ["W\u00fcrd", "mal", "tau", "Kra\u00b7mers\u00b7d\u00f6rp", "nah\u00b7seihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wo wid in Schriwen, Lesen, Baukstafieren,", "tokens": ["Wo", "wid", "in", "Schri\u00b7wen", ",", "Le\u00b7sen", ",", "Bauk\u00b7sta\u00b7fie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In'n Katekism' de G\u00f6ren wiren.", "tokens": ["In'n", "Ka\u00b7te\u00b7kism'", "de", "G\u00f6\u00b7ren", "wi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hei f\u00fcnn dat Ganze man sihr swack,", "tokens": ["Hei", "f\u00fcnn", "dat", "Gan\u00b7ze", "man", "sihr", "swack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "V\u00f6r allen was't l\u00fctt J\u00f6ching Tack,", "tokens": ["V\u00f6r", "al\u00b7len", "was't", "l\u00fctt", "J\u00f6\u00b7ching", "Tack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "De w\u00fcrd sick hellischen blamieren,", "tokens": ["De", "w\u00fcrd", "sick", "hel\u00b7li\u00b7schen", "bla\u00b7mie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00fc\u00dft nicks un kratzt her\u00fcm in't Hor.", "tokens": ["W\u00fc\u00dft", "nicks", "un", "kratzt", "he\u00b7r\u00fcm", "in't", "Hor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Na, as tau En'n nu was de Krempel,", "tokens": ["Na", ",", "as", "tau", "En'n", "nu", "was", "de", "Krem\u00b7pel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Stellt em de Schaulrat as Exempel", "tokens": ["Stellt", "em", "de", "Schaul\u00b7rat", "as", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "---+--+--", "measure": "iambic.di.relaxed"}, "line.11": {"text": "\u00bbder schauderhaftsten Dummheit\u00ab dor.", "tokens": ["\u00bb", "der", "schau\u00b7der\u00b7hafts\u00b7ten", "Dumm\u00b7heit", "\u00ab", "dor."], "token_info": ["punct", "word", "word", "word", "punct", "abbreviation"], "pos": ["$(", "ART", "ADJA", "NN", "$(", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "De Schaulrat f\u00fchrt den Middag furt,", "tokens": ["De", "Schaul\u00b7rat", "f\u00fchrt", "den", "Mid\u00b7dag", "furt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Un as hei rut k\u00fcmmt ut den Urt,", "tokens": ["Un", "as", "hei", "rut", "k\u00fcmmt", "ut", "den", "Urt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Weit hei nich mit den Weg Bescheid.", "tokens": ["Weit", "hei", "nich", "mit", "den", "Weg", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hei r\u00f6ppt sick also einen Jungen,", "tokens": ["Hei", "r\u00f6ppt", "sick", "al\u00b7so", "ei\u00b7nen", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "De dor sin G\u00f6ssel h\u00e4uden deiht.", "tokens": ["De", "dor", "sin", "G\u00f6s\u00b7sel", "h\u00e4u\u00b7den", "deiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "De Jung' k\u00fcmmt munter ranne sprungen,", "tokens": ["De", "Jung'", "k\u00fcmmt", "mun\u00b7ter", "ran\u00b7ne", "sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Un de Herr Schaulrat fr\u00f6ggt: \u00bbMin S\u00e4hn,", "tokens": ["Un", "de", "Herr", "Schaul\u00b7rat", "fr\u00f6ggt", ":", "\u00bb", "Min", "S\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "NN", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Wo geiht de Weg nah Groten-Kl\u00e4hn?\u00ab", "tokens": ["Wo", "geiht", "de", "Weg", "nah", "Gro\u00b7ten\u00b7Kl\u00e4hn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "NE", "NN", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dunn lacht em J\u00f6ching dwaslings an:", "tokens": ["Dunn", "lacht", "em", "J\u00f6\u00b7ching", "dwas\u00b7lings", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbhei's s\u00fcs doch so en klauken Mann,", "tokens": ["\u00bb", "hei's", "s\u00fcs", "doch", "so", "en", "klau\u00b7ken", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "ADV", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Is Hei mit einmal d\u00e4mlich word'n?", "tokens": ["Is", "Hei", "mit", "ein\u00b7mal", "d\u00e4m\u00b7lich", "word'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Hei was doch noch so klauk vermorg'n?", "tokens": ["Hei", "was", "doch", "noch", "so", "klauk", "ver\u00b7mor\u00b7g'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Un nu weit Hei nich mal Bescheid,", "tokens": ["Un", "nu", "weit", "Hei", "nich", "mal", "Be\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "NN", "PTKNEG", "ADV", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Wo't nah dat negste D\u00f6rp hengeiht?\u00ab", "tokens": ["Wo't", "nah", "dat", "negs\u00b7te", "D\u00f6rp", "hen\u00b7geiht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}