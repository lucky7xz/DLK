{"dta.poem.10835": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n \u00fcber die  \n  aufrecht gestellte Figur der  \n Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Thiere welche blos der Erden an-\ngeh\u00f6rn,", "tokens": ["Die", "Thie\u00b7re", "wel\u00b7che", "blos", "der", "Er\u00b7den", "an", "ge\u00b7h\u00f6rn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRELS", "ADV", "ART", "NN", "TRUNC", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne die Vernunfft, nur sinn-", "tokens": ["Und", "oh\u00b7ne", "die", "Ver\u00b7nunfft", ",", "nur", "sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die k\u00f6nnen ihr Gesicht auch nie", "tokens": ["Die", "k\u00f6n\u00b7nen", "ihr", "Ge\u00b7sicht", "auch", "nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie m\u00fcssen blos allein geb\u00fckt die", "tokens": ["Sie", "m\u00fcs\u00b7sen", "blos", "al\u00b7lein", "ge\u00b7b\u00fckt", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Mensch das Hauptgesch\u00f6pf, der F\u00fcrste dieser", "tokens": ["Der", "Mensch", "das", "Haupt\u00b7ge\u00b7sch\u00f6pf", ",", "der", "F\u00fcrs\u00b7te", "die\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der theils vom Himmel stammt, theils aus dem", "tokens": ["Der", "theils", "vom", "Him\u00b7mel", "stammt", ",", "theils", "aus", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist von dem weisen ", "tokens": ["Ist", "von", "dem", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Da\u00df sein Gesichte kan zur H\u00f6h gelenket werden;", "tokens": ["Da\u00df", "sein", "Ge\u00b7sich\u00b7te", "kan", "zur", "H\u00f6h", "ge\u00b7len\u00b7ket", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und auch zur Erde hin. Wer nun ein Mensch wil", "tokens": ["Und", "auch", "zur", "Er\u00b7de", "hin", ".", "Wer", "nun", "ein", "Mensch", "wil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "PTKVZ", "$.", "PWS", "ADV", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der besser als ein Thier, und eine Seele heget,", "tokens": ["Der", "bes\u00b7ser", "als", "ein", "Thier", ",", "und", "ei\u00b7ne", "See\u00b7le", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der lenket sein Gesicht nicht nur zur Erd allein,", "tokens": ["Der", "len\u00b7ket", "sein", "Ge\u00b7sicht", "nicht", "nur", "zur", "Erd", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu dem was irdisch ist, und was der Erdball tr\u00e4-", "tokens": ["Zu", "dem", "was", "ir\u00b7disch", "ist", ",", "und", "was", "der", "Erd\u00b7ball", "tr\u00e4"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PRELS", "ADJD", "VAFIN", "$,", "KON", "PWS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er sieht vornemlich hin, woher sein Geist ent-", "tokens": ["Er", "sieht", "vor\u00b7nem\u00b7lich", "hin", ",", "wo\u00b7her", "sein", "Geist", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "TRUNC"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.14": {"text": "Und an dem Himmlischen find er nur sein Erg\u00f6z-", "tokens": ["Und", "an", "dem", "Himm\u00b7li\u00b7schen", "find", "er", "nur", "sein", "Er\u00b7g\u00f6z"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Er denkt die Kreatur, die theils vom Himmel stammt,", "tokens": ["Er", "denkt", "die", "Kre\u00b7a\u00b7tur", ",", "die", "theils", "vom", "Him\u00b7mel", "stammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die findet ihre Ruh nicht bei dem Erden-Sch\u00e4z-", "tokens": ["Die", "fin\u00b7det", "ih\u00b7re", "Ruh", "nicht", "bei", "dem", "Er\u00b7den\u00b7Sch\u00e4\u00b7z"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Mensch, der wie ein Wurm im Koth der Erde", "tokens": ["Ein", "Mensch", ",", "der", "wie", "ein", "Wurm", "im", "Koth", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und mit geb\u00fckten Haupt stets nach dem Staube", "tokens": ["Und", "mit", "ge\u00b7b\u00fck\u00b7ten", "Haupt", "stets", "nach", "dem", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und keinen andern Trieb, als nur zu Dingen f\u00fchlt,", "tokens": ["Und", "kei\u00b7nen", "an\u00b7dern", "Trieb", ",", "als", "nur", "zu", "Din\u00b7gen", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "KOUS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die auf der Erde sind, und sich damit verstrikket,", "tokens": ["Die", "auf", "der", "Er\u00b7de", "sind", ",", "und", "sich", "da\u00b7mit", "ver\u00b7strik\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$,", "KON", "PRF", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der ist ein menschlich Thier. Wie viele sind nun", "tokens": ["Der", "ist", "ein", "menschlich", "Thier", ".", "Wie", "vie\u00b7le", "sind", "nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$.", "PWAV", "PIS", "VAFIN", "ADV"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Die ihr erhabnes Haupt, blos nach dem Kothe len-", "tokens": ["Die", "ihr", "er\u00b7hab\u00b7nes", "Haupt", ",", "blos", "nach", "dem", "Ko\u00b7the", "len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Sie sind aufrecht gemacht mit ihren Angesicht:", "tokens": ["Sie", "sind", "auf\u00b7recht", "ge\u00b7macht", "mit", "ih\u00b7ren", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wollen als ein Thier sich doch zur Erde sen-", "tokens": ["Und", "wol\u00b7len", "als", "ein", "Thier", "sich", "doch", "zur", "Er\u00b7de", "sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "KOKOM", "ART", "NN", "PRF", "ADV", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.25": {"text": "Jhr Menschen! denkt daran, so offt ihr euch an-", "tokens": ["Ihr", "Men\u00b7schen", "!", "denkt", "da\u00b7ran", ",", "so", "offt", "ihr", "euch", "an"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PAV", "$,", "ADV", "ADV", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.26": {"text": "Eur Adel zeigt sich auch in dem erhabnen Haupte,", "tokens": ["Eur", "A\u00b7del", "zeigt", "sich", "auch", "in", "dem", "er\u00b7hab\u00b7nen", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Kopf ist aufgericht, zum Himmlischen erh\u00f6ht,", "tokens": ["Der", "Kopf", "ist", "auf\u00b7ge\u00b7richt", ",", "zum", "Himm\u00b7li\u00b7schen", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dies sieht ein jeder an: O! woll dem der auch", "tokens": ["Dies", "sieht", "ein", "je\u00b7der", "an", ":", "O", "!", "woll", "dem", "der", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "PIS", "PTKVZ", "$.", "NE", "$.", "ADV", "ART", "ART", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Es sey nun seine Pflicht, die Eitelkeit zu fliehn,", "tokens": ["Es", "sey", "nun", "sei\u00b7ne", "Pflicht", ",", "die", "Ei\u00b7tel\u00b7keit", "zu", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So ferne sie das Herz vom rechten Ziel verr\u00fck-", "tokens": ["So", "fer\u00b7ne", "sie", "das", "Herz", "vom", "rech\u00b7ten", "Ziel", "ver\u00b7r\u00fc\u00b7k"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein Mensch der m\u00fcsse sich nach solchem Gut be-", "tokens": ["Ein", "Mensch", "der", "m\u00fcs\u00b7se", "sich", "nach", "sol\u00b7chem", "Gut", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VMFIN", "PRF", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Wodurch die Seele wird, nicht blos der Leib be-", "tokens": ["Wo\u00b7durch", "die", "See\u00b7le", "wird", ",", "nicht", "blos", "der", "Leib", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "PTKNEG", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "O! woll dem dessen Aug stets nach den Ziele", "tokens": ["O", "!", "woll", "dem", "des\u00b7sen", "Aug", "stets", "nach", "den", "Zie\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und nach dem Himmel sieht: Wer darnach ist be-", "tokens": ["Und", "nach", "dem", "Him\u00b7mel", "sieht", ":", "Wer", "dar\u00b7nach", "ist", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$.", "PWS", "PAV", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Der w\u00fcnscht nach jener Welt, die vor dem Geist er-", "tokens": ["Der", "w\u00fcnscht", "nach", "je\u00b7ner", "Welt", ",", "die", "vor", "dem", "Geist", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Der sieht die Erd zwar an, und trit sie nur mit F\u00fcs-", "tokens": ["Der", "sieht", "die", "Erd", "zwar", "an", ",", "und", "trit", "sie", "nur", "mit", "F\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}