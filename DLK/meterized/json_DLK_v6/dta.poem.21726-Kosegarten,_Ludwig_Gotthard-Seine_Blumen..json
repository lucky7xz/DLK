{"dta.poem.21726": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Seine Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tausend der Blumen bl&#252;hn in meinem Garten.", "tokens": ["Tau\u00b7send", "der", "Blu\u00b7men", "bl", "&#252;", "hn", "in", "mei\u00b7nem", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "NE", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Schon durch des Jennerschnee krystallne Rinde", "tokens": ["Schon", "durch", "des", "Jen\u00b7ner\u00b7schnee", "krys\u00b7tall\u00b7ne", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dr\u00e4ngen sich fr\u00fchlingahnend des Galanthus", "tokens": ["Dr\u00e4n\u00b7gen", "sich", "fr\u00fch\u00b7lin\u00b7gah\u00b7nend", "des", "Ga\u00b7lan\u00b7thus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Silberne Gl\u00f6ckchen.", "tokens": ["Sil\u00b7ber\u00b7ne", "Gl\u00f6ck\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Tief im Gestr\u00e4uche schwillt die Anemone.", "tokens": ["Tief", "im", "Ge\u00b7str\u00e4u\u00b7che", "schwillt", "die", "A\u00b7ne\u00b7mo\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Rings auf den Beeten gl\u00e4nzt der g\u00fcldne", "tokens": ["Rings", "auf", "den", "Bee\u00b7ten", "gl\u00e4nzt", "der", "g\u00fcld\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Krokos.", "tokens": ["Kro\u00b7kos", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Heimlich err\u00f6thend str\u00f6mt das bl\u00f6de Veilchen", "tokens": ["Heim\u00b7lich", "er\u00b7r\u00f6\u00b7thend", "str\u00f6mt", "das", "bl\u00f6\u00b7de", "Veil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "K\u00f6stliche D\u00fcfte.", "tokens": ["K\u00f6st\u00b7li\u00b7che", "D\u00fcf\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Lockt dich der Schmelz der vielgef\u00e4rbten Primel?", "tokens": ["Lockt", "dich", "der", "Schmelz", "der", "viel\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Pri\u00b7mel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Freut dich der Silberstaub der Sammtaurikel?", "tokens": ["Freut", "dich", "der", "Sil\u00b7ber\u00b7staub", "der", "Samm\u00b7tau\u00b7ri\u00b7kel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Liebst du vielleicht der liebesiechen Echo", "tokens": ["Liebst", "du", "viel\u00b7leicht", "der", "lie\u00b7be\u00b7sie\u00b7chen", "E\u00b7cho"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Blendenden G\u00fcnstling?", "tokens": ["Blen\u00b7den\u00b7den", "G\u00fcnst\u00b7ling", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Tausend der Blumen funkeln in dem Kranze,", "tokens": ["Tau\u00b7send", "der", "Blu\u00b7men", "fun\u00b7keln", "in", "dem", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Welcher des Sommers gl\u00fch'nde Schl\u00e4ffen k\u00fchlet,", "tokens": ["Wel\u00b7cher", "des", "Som\u00b7mers", "gl\u00fch'n\u00b7de", "Schl\u00e4f\u00b7fen", "k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lilie du, und Nelk', und du, o Rose,", "tokens": ["Li\u00b7lie", "du", ",", "und", "Nel\u00b7k'", ",", "und", "du", ",", "o", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "NN", "$,", "KON", "PPER", "$,", "FM", "FM", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zypriens Brautschmuck.", "tokens": ["Zyp\u00b7riens", "Brautsc\u00b7hmuck", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Tausend der Blumen bl\u00fchn in meinem Garten,", "tokens": ["Tau\u00b7send", "der", "Blu\u00b7men", "bl\u00fchn", "in", "mei\u00b7nem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Oftmal pfl\u00fcckt' ich die duftigsten, die sch\u00f6n-", "tokens": ["Oft\u00b7mal", "pfl\u00fcckt'", "ich", "die", "duf\u00b7tigs\u00b7ten", ",", "die", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "$,", "ART", "TRUNC"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Barg sie zun\u00e4chst ans Herz mir, wahrte", "tokens": ["Barg", "sie", "zu\u00b7n\u00e4chst", "ans", "Herz", "mir", ",", "wahr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "PPER", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sorgsam", "tokens": ["sorg\u00b7sam"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Tief sie im Busen.", "tokens": ["Tief", "sie", "im", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Dir sie zu geben, wenn der Abend wehte,", "tokens": ["Dir", "sie", "zu", "ge\u00b7ben", ",", "wenn", "der", "A\u00b7bend", "weh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir sie zu reichen, wenn der Abschied schallte,", "tokens": ["Dir", "sie", "zu", "rei\u00b7chen", ",", "wenn", "der", "Ab\u00b7schied", "schall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dass sie ein leises Gedenke mein! dir", "tokens": ["Dass", "sie", "ein", "lei\u00b7ses", "Ge\u00b7den\u00b7ke", "mein", "!", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPOSAT", "$.", "PPER"], "meter": "+--+--+-++", "measure": "dactylic.di.plus"}, "line.4": {"text": "hauchten,", "tokens": ["hauch\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Schmachtet' und brannt' ich.", "tokens": ["Schmach\u00b7tet'", "und", "brannt'", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Aber mich hielt die Angst, die arme Gabe", "tokens": ["A\u00b7ber", "mich", "hielt", "die", "Angst", ",", "die", "ar\u00b7me", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ach, verschm\u00e4ht zu sehn von der Hochver-", "tokens": ["Ach", ",", "ver\u00b7schm\u00e4ht", "zu", "sehn", "von", "der", "Hoch\u00b7ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVPP", "PTKZU", "VVINF", "APPR", "ART", "TRUNC"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Traurig entwand ich dir mich, meine Blumen", "tokens": ["Trau\u00b7rig", "ent\u00b7wand", "ich", "dir", "mich", ",", "mei\u00b7ne", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PPER", "$,", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Welketen traurig.", "tokens": ["Wel\u00b7ke\u00b7ten", "trau\u00b7rig", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Klein und gering ist die Gabe treuer Liebe.", "tokens": ["Klein", "und", "ge\u00b7ring", "ist", "die", "Ga\u00b7be", "treu\u00b7er", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Aber verschm\u00e4ht zu sehn die arme Gabe,", "tokens": ["A\u00b7ber", "ver\u00b7schm\u00e4ht", "zu", "sehn", "die", "ar\u00b7me", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Knicket des Lebens Blume, st\u00f6sst den", "tokens": ["Kni\u00b7cket", "des", "Le\u00b7bens", "Blu\u00b7me", ",", "st\u00f6sst", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "VVFIN", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mordstahl", "tokens": ["Mord\u00b7stahl"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Tief in den Herzschlag.", "tokens": ["Tief", "in", "den", "Herz\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}