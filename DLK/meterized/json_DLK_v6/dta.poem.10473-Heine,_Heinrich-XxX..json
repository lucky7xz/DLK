{"dta.poem.10473": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Man glaubt, da\u00df ich mich gr\u00e4me", "tokens": ["Man", "glaubt", ",", "da\u00df", "ich", "mich", "gr\u00e4\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In bitter'm Liebesleid,", "tokens": ["In", "bit\u00b7ter'm", "Lie\u00b7be\u00b7sleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und endlich glaub' ich es selber,", "tokens": ["Und", "end\u00b7lich", "glaub'", "ich", "es", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So gut wie andre Leut'.", "tokens": ["So", "gut", "wie", "and\u00b7re", "Leut'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du Kleine mit gro\u00dfen Augen,", "tokens": ["Du", "Klei\u00b7ne", "mit", "gro\u00b7\u00dfen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich hab' es dir immer gesagt,", "tokens": ["Ich", "hab'", "es", "dir", "im\u00b7mer", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df ich dich uns\u00e4glich liebe,", "tokens": ["Da\u00df", "ich", "dich", "un\u00b7s\u00e4g\u00b7lich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df Liebe mein Herz zernagt.", "tokens": ["Da\u00df", "Lie\u00b7be", "mein", "Herz", "zer\u00b7nagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch nur in einsamer Kammer", "tokens": ["Doch", "nur", "in", "ein\u00b7sa\u00b7mer", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sprach ich auf solche Art,", "tokens": ["Sprach", "ich", "auf", "sol\u00b7che", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ach! ich hab' immer geschwiegen", "tokens": ["Und", "ach", "!", "ich", "hab'", "im\u00b7mer", "ge\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner Gegenwart.", "tokens": ["In", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da gab es b\u00f6se Engel,", "tokens": ["Da", "gab", "es", "b\u00f6\u00b7se", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hielten mir zu den Mund;", "tokens": ["Die", "hiel\u00b7ten", "mir", "zu", "den", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ach! durch b\u00f6se Engel", "tokens": ["Und", "ach", "!", "durch", "b\u00f6\u00b7se", "En\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bin ich so elend jetzund.", "tokens": ["Bin", "ich", "so", "e\u00b7lend", "je\u00b7tzund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}