{"dta.poem.7585": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Erden gehest du, und bist der Erde Geist;", "tokens": ["Auf", "Er\u00b7den", "ge\u00b7hest", "du", ",", "und", "bist", "der", "Er\u00b7de", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "KON", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Erd' erkennt dich nicht, die dich mit Bl\u00fcten preist.", "tokens": ["Die", "Erd'", "er\u00b7kennt", "dich", "nicht", ",", "die", "dich", "mit", "Bl\u00fc\u00b7ten", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf Sonnen stehest du, und bist der Sonne Geist;", "tokens": ["Auf", "Son\u00b7nen", "ste\u00b7hest", "du", ",", "und", "bist", "der", "Son\u00b7ne", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "KON", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonn' erkennt dich nicht, die dich mit Stralen preist.", "tokens": ["Die", "Sonn'", "er\u00b7kennt", "dich", "nicht", ",", "die", "dich", "mit", "Stra\u00b7len", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Winde wehest du, und bist der L\u00fcfte Geist;", "tokens": ["Im", "Win\u00b7de", "we\u00b7hest", "du", ",", "und", "bist", "der", "L\u00fcf\u00b7te", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "KON", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Luft erkennt dich nicht, die dich mit Athmen preist.", "tokens": ["Die", "Luft", "er\u00b7kennt", "dich", "nicht", ",", "die", "dich", "mit", "Ath\u00b7men", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Auf Wassern gehest du, und bist des Wassers Geist;", "tokens": ["Auf", "Was\u00b7sern", "ge\u00b7hest", "du", ",", "und", "bist", "des", "Was\u00b7sers", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "KON", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Wasser kennt dich nicht, das dich mit Rauschen preist.", "tokens": ["Das", "Was\u00b7ser", "kennt", "dich", "nicht", ",", "das", "dich", "mit", "Rau\u00b7schen", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Im Herzen stehest du, und bist der Liebe Geist;", "tokens": ["Im", "Her\u00b7zen", "ste\u00b7hest", "du", ",", "und", "bist", "der", "Lie\u00b7be", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "KON", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dich erkennt das Herz, das dich mit Liebe preist.", "tokens": ["Und", "dich", "er\u00b7kennt", "das", "Herz", ",", "das", "dich", "mit", "Lie\u00b7be", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}