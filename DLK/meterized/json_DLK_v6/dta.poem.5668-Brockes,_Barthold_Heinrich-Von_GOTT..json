{"dta.poem.5668": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ohn dieses H\u00f6chste Gut kan niemand sich erkennen.", "tokens": ["Ohn", "die\u00b7ses", "H\u00f6chs\u00b7te", "Gut", "kan", "nie\u00b7mand", "sich", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VMFIN", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich, der ich wei\u00df: ich sey; Bin ich, der mich gemacht?", "tokens": ["Ich", ",", "der", "ich", "wei\u00df", ":", "ich", "sey", ";", "Bin", "ich", ",", "der", "mich", "ge\u00b7macht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "$.", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan die Materie sich meinen Sch\u00f6pffer nennen?", "tokens": ["Kan", "die", "Ma\u00b7te\u00b7rie", "sich", "mei\u00b7nen", "Sch\u00f6pf\u00b7fer", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bin ich durch sie hervor gebracht,", "tokens": ["Bin", "ich", "durch", "sie", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sie sich selbst ja nimmer reget?", "tokens": ["Da", "sie", "sich", "selbst", "ja", "nim\u00b7mer", "re\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich find, ich sey ein Geist: Der Geist kennt, er erweget.", "tokens": ["Ich", "find", ",", "ich", "sey", "ein", "Geist", ":", "Der", "Geist", "kennt", ",", "er", "er\u00b7we\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die\u00df ist mein wahres Seyn. Wer giebt es mir?", "tokens": ["Die\u00df", "ist", "mein", "wah\u00b7res", "Seyn", ".", "Wer", "giebt", "es", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Kan es formiret seyn vom blinden Ohngefehr?", "tokens": ["Kan", "es", "for\u00b7mi\u00b7ret", "seyn", "vom", "blin\u00b7den", "Ohn\u00b7ge\u00b7fehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Kommt\u2019s nicht von ", "tokens": ["Kommt's", "nicht", "von"], "token_info": ["word", "word", "word"], "pos": ["NE", "PTKNEG", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Durch welchen groben Schlu\u00df und Jrrthum ziehen wir", "tokens": ["Durch", "wel\u00b7chen", "gro\u00b7ben", "Schlu\u00df", "und", "Jrr\u00b7thum", "zie\u00b7hen", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den Geist aus der Materie herf\u00fcr?", "tokens": ["Den", "Geist", "aus", "der", "Ma\u00b7te\u00b7rie", "her\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wer kan das Wesen doch den Geistern, welche dencken,", "tokens": ["Wer", "kan", "das", "We\u00b7sen", "doch", "den", "Geis\u00b7tern", ",", "wel\u00b7che", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sonst, als ein ", "tokens": ["Sonst", ",", "als", "ein"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART"], "meter": "+-+", "measure": "trochaic.di"}}}}}