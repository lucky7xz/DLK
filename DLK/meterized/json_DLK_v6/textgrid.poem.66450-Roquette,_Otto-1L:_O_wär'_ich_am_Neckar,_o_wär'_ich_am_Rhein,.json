{"textgrid.poem.66450": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: O w\u00e4r' ich am Neckar, o w\u00e4r' ich am Rhein,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O w\u00e4r' ich am Neckar, o w\u00e4r' ich am Rhein,", "tokens": ["O", "w\u00e4r'", "ich", "am", "Ne\u00b7ckar", ",", "o", "w\u00e4r'", "ich", "am", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPRART", "NN", "$,", "FM", "VAFIN", "PPER", "APPRART", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im bl\u00fchenden Rebenland, da m\u00f6cht' ich sein!", "tokens": ["Im", "bl\u00fc\u00b7hen\u00b7den", "Re\u00b7ben\u00b7land", ",", "da", "m\u00f6cht'", "ich", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo das Leben ein sprudelnder Becher der Lust,", "tokens": ["Wo", "das", "Le\u00b7ben", "ein", "spru\u00b7deln\u00b7der", "Be\u00b7cher", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Wo ich wandert' und wohnte an Freundesbrust,", "tokens": ["Wo", "ich", "wan\u00b7dert'", "und", "wohn\u00b7te", "an", "Freun\u00b7des\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Am Neckar, am Rhein,", "tokens": ["Am", "Ne\u00b7ckar", ",", "am", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Im bl\u00fchenden Rebenland, da m\u00f6cht' ich sein!", "tokens": ["Im", "bl\u00fc\u00b7hen\u00b7den", "Re\u00b7ben\u00b7land", ",", "da", "m\u00f6cht'", "ich", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr M\u00e4dchen, ihr St\u00e4dtchen am Ufer hinab,", "tokens": ["Ihr", "M\u00e4d\u00b7chen", ",", "ihr", "St\u00e4dt\u00b7chen", "am", "U\u00b7fer", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr des Herzens Lust, und der Augen Lab',", "tokens": ["Ihr", "des", "Her\u00b7zens", "Lust", ",", "und", "der", "Au\u00b7gen", "Lab'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ihr singenden, klingenden Wellen des Rheins,", "tokens": ["Ihr", "sin\u00b7gen\u00b7den", ",", "klin\u00b7gen\u00b7den", "Wel\u00b7len", "des", "Rheins", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr L\u00fcfte des Lebens, ihr D\u00fcfte des Weins,", "tokens": ["Ihr", "L\u00fcf\u00b7te", "des", "Le\u00b7bens", ",", "ihr", "D\u00fcf\u00b7te", "des", "Weins", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Durch die jubelnde Brust", "tokens": ["Durch", "die", "ju\u00b7beln\u00b7de", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Geht mir alle das Leben, und alle die Lust!", "tokens": ["Geht", "mir", "al\u00b7le", "das", "Le\u00b7ben", ",", "und", "al\u00b7le", "die", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ART", "NN", "$,", "KON", "PIS", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "La\u00dft mich wandern und singen wohl durch die Welt,", "tokens": ["La\u00dft", "mich", "wan\u00b7dern", "und", "sin\u00b7gen", "wohl", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "La\u00dft mich weilen und wohnen wo mir's gef\u00e4llt!", "tokens": ["La\u00dft", "mich", "wei\u00b7len", "und", "woh\u00b7nen", "wo", "mir's", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "KON", "VVINF", "PWAV", "NE", "VVPP", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dann zieh' ich zum Neckar, dann zieh' ich zum Rhein,", "tokens": ["Dann", "zieh'", "ich", "zum", "Ne\u00b7ckar", ",", "dann", "zieh'", "ich", "zum", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Aus den Th\u00e4lern zu Berg, von den Bergen thalein,", "tokens": ["Aus", "den", "Th\u00e4\u00b7lern", "zu", "Berg", ",", "von", "den", "Ber\u00b7gen", "tha\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Und ich jauchz' es hinaus:", "tokens": ["Und", "ich", "jauch\u00b7z'", "es", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wo mein Herz und mein Lied ist, da bin ich zu Haus!", "tokens": ["Wo", "mein", "Herz", "und", "mein", "Lied", "ist", ",", "da", "bin", "ich", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}