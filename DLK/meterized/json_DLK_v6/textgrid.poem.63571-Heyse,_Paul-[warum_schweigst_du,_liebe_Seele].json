{"textgrid.poem.63571": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[warum schweigst du, liebe Seele]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum schweigst du, liebe Seele?", "tokens": ["Wa\u00b7rum", "schweigst", "du", ",", "lie\u00b7be", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, verhehle", "tokens": ["Ach", ",", "ver\u00b7heh\u00b7le"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nichts dem Liebsten, nichts dem Freund!", "tokens": ["Nichts", "dem", "Liebs\u00b7ten", ",", "nichts", "dem", "Freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sich deine Wimpern senken,", "tokens": ["Wenn", "sich", "dei\u00b7ne", "Wim\u00b7pern", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df ich denken,", "tokens": ["Mu\u00df", "ich", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Da\u00df dein Aug' um mich geweint. \u2013", "tokens": ["Da\u00df", "dein", "Aug'", "um", "mich", "ge\u00b7weint", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbnicht gepre\u00dft von schwerem Kummer,", "tokens": ["\u00bb", "nicht", "ge\u00b7pre\u00dft", "von", "schwe\u00b7rem", "Kum\u00b7mer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur in stummer", "tokens": ["Nur", "in", "stum\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bangigkeit erbebt mein Herz.", "tokens": ["Ban\u00b7gig\u00b7keit", "er\u00b7bebt", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie vor nahenden Gewittern", "tokens": ["Wie", "vor", "na\u00b7hen\u00b7den", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df ich zittern:", "tokens": ["Mu\u00df", "ich", "zit\u00b7tern", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ach, die Freude lockt den Schmerz!", "tokens": ["Ach", ",", "die", "Freu\u00b7de", "lockt", "den", "Schmerz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fchlst du nicht, wie ich sie f\u00fchle,", "tokens": ["F\u00fchlst", "du", "nicht", ",", "wie", "ich", "sie", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Schw\u00fcle?", "tokens": ["Die\u00b7se", "Schw\u00fc\u00b7le", "?"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Fast wie Schuld beklemmt Besitz.", "tokens": ["Fast", "wie", "Schuld", "be\u00b7klemmt", "Be\u00b7sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da noch Herz an Herz wir pressen,", "tokens": ["Da", "noch", "Herz", "an", "Herz", "wir", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, indessen", "tokens": ["Ach", ",", "in\u00b7des\u00b7sen"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Lauert droben schon der Blitz.\u00ab \u2013", "tokens": ["Lau\u00b7ert", "dro\u00b7ben", "schon", "der", "Blitz", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df es lauern, la\u00df es blitzen!", "tokens": ["La\u00df", "es", "lau\u00b7ern", ",", "la\u00df", "es", "blit\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir besitzen,", "tokens": ["Wir", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was kein Schicksal f\u00fcrder raubt.", "tokens": ["Was", "kein", "Schick\u00b7sal", "f\u00fcr\u00b7der", "raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch sein \u00c4rgstes droht vergebens:", "tokens": ["Auch", "sein", "\u00c4rgs\u00b7tes", "droht", "ver\u00b7ge\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir erleben's", "tokens": ["Wir", "er\u00b7le\u00b7ben's"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Herz an Herzen, Haupt an Haupt.", "tokens": ["Herz", "an", "Her\u00b7zen", ",", "Haupt", "an", "Haupt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}