{"dta.poem.7476": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Russophobie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie gehet Ihr nur so verkehrte Bahnen!", "tokens": ["Wie", "ge\u00b7het", "Ihr", "nur", "so", "ver\u00b7kehr\u00b7te", "Bah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ihr h\u00e4ttet besser ewig sie gemieden,", "tokens": ["Ihr", "h\u00e4t\u00b7tet", "bes\u00b7ser", "e\u00b7wig", "sie", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Euch g\u00e4nzlich von der Politik geschieden,", "tokens": ["Euch", "g\u00e4nz\u00b7lich", "von", "der", "Po\u00b7li\u00b7tik", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Geisterseher, ihr Baschkiromanen!", "tokens": ["Ihr", "Geis\u00b7ter\u00b7se\u00b7her", ",", "ihr", "Baschki\u00b7ro\u00b7ma\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr m\u00f6chtet gern Europa's Zukunft ahnen?", "tokens": ["Ihr", "m\u00f6ch\u00b7tet", "gern", "Eu\u00b7ro\u00b7pa's", "Zu\u00b7kunft", "ah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sag' Euch, unsre Freiheit wird hienieden", "tokens": ["Ich", "sag'", "Euch", ",", "uns\u00b7re", "Frei\u00b7heit", "wird", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Czar an seinen Kaukasus je schmieden,", "tokens": ["Kein", "Czar", "an", "sei\u00b7nen", "Kau\u00b7ka\u00b7sus", "je", "schmie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Geisterseher, ihr Baschkiromanen!", "tokens": ["Ihr", "Geis\u00b7ter\u00b7se\u00b7her", ",", "ihr", "Baschki\u00b7ro\u00b7ma\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}