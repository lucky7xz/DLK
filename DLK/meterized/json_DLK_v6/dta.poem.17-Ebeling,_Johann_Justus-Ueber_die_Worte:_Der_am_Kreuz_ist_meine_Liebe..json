, "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich komme an dem Port,", "tokens": ["Wenn", "ich", "kom\u00b7me", "an", "dem", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da der Tod mich wird hinf\u00fchren:", "tokens": ["Da", "der", "Tod", "mich", "wird", "hin\u00b7f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch verstummt, alsdenn mein Mund,", "tokens": ["Doch", "ver\u00b7stummt", ",", "als\u00b7denn", "mein", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So macht doch das Herze kund,", "tokens": ["So", "macht", "doch", "das", "Her\u00b7ze", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So lang es sich noch wird r\u00fchren,", "tokens": ["So", "lang", "es", "sich", "noch", "wird", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Da\u00df mein fester Wahlspruch sey:", "tokens": ["Da\u00df", "mein", "fes\u00b7ter", "Wahl\u00b7spruch", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ich bin meinem JEsu treu,", "tokens": ["Ich", "bin", "mei\u00b7nem", "Je\u00b7su", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NE", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Der am Kreuz ist meine Liebe,", "tokens": ["Der", "am", "Kreuz", "ist", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nunmehr ein und allemahl,", "tokens": ["Nun\u00b7mehr", "ein", "und", "al\u00b7le\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mich auch die Welt begr\u00fcbe,", "tokens": ["Wenn", "mich", "auch", "die", "Welt", "be\u00b7gr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die gr\u00f6ste Noth und Qual.", "tokens": ["In", "die", "gr\u00f6s\u00b7te", "Noth", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich weis, wenn ich mu\u00df erblassen,", "tokens": ["Ich", "weis", ",", "wenn", "ich", "mu\u00df", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-