{"dta.poem.17": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ueber die Worte:  \n  Der am Kreuz ist meine Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der am Kreuz ist meine Liebe,", "tokens": ["Der", "am", "Kreuz", "ist", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn er hat mich treu geliebt,", "tokens": ["Denn", "er", "hat", "mich", "treu", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da er sich mit freien Triebe,", "tokens": ["Da", "er", "sich", "mit", "frei\u00b7en", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein B\u00fcrge willig giebt.", "tokens": ["Als", "ein", "B\u00fcr\u00b7ge", "wil\u00b7lig", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O! mein theurer Freund der Seelen,", "tokens": ["O", "!", "mein", "theu\u00b7rer", "Freund", "der", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du im Blut gef\u00e4rbtes Lamm", "tokens": ["Du", "im", "Blut", "ge\u00b7f\u00e4rb\u00b7tes", "Lamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich will ich zum Br\u00e4utigam,", "tokens": ["Dich", "will", "ich", "zum", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zum Eigenthum erw\u00e4hlen,", "tokens": ["Und", "zum", "Ei\u00b7gen\u00b7thum", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nunmehr soll ohn falschen Schein,", "tokens": ["Nun\u00b7mehr", "soll", "ohn", "fal\u00b7schen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Meines Herzens Walspruch seyn:", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "Wal\u00b7spruch", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Der am Kreuz ist meine Liebe,", "tokens": ["Der", "am", "Kreuz", "ist", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil er an der S\u00fcnder statt,", "tokens": ["Weil", "er", "an", "der", "S\u00fcn\u00b7der", "statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vergiften Schlangen-Hiebe,", "tokens": ["Die", "ver\u00b7gif\u00b7ten", "Schlan\u00b7gen\u00b7Hie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein Held erduldet hat.", "tokens": ["Als", "ein", "Held", "er\u00b7dul\u00b7det", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er hat ja sein Blut und Leben,", "tokens": ["Er", "hat", "ja", "sein", "Blut", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als ein kostbar L\u00f6segeld,", "tokens": ["Als", "ein", "kost\u00b7bar", "L\u00f6\u00b7se\u00b7geld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "F\u00fcr mich und der ganzen Welt,", "tokens": ["F\u00fcr", "mich", "und", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr die Feinde hingegeben:", "tokens": ["F\u00fcr", "die", "Fein\u00b7de", "hin\u00b7ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dadurch bin ich ihm verpflicht,", "tokens": ["Da\u00b7durch", "bin", "ich", "ihm", "ver\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df mein Herz aus Inbrunst spricht:", "tokens": ["Da\u00df", "mein", "Herz", "aus", "In\u00b7brunst", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Der am Kreuz ist meine Liebe,", "tokens": ["Der", "am", "Kreuz", "ist", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich zwar bisher veracht,", "tokens": ["Den", "ich", "zwar", "bis\u00b7her", "ver\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und noch leider oft betr\u00fcbe,", "tokens": ["Und", "noch", "lei\u00b7der", "oft", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch er hat mich rein gemacht,", "tokens": ["Doch", "er", "hat", "mich", "rein", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er hat seine Gnaden-Armen,", "tokens": ["Er", "hat", "sei\u00b7ne", "Gna\u00b7den\u00b7Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die am Kreuze ausgespannt,", "tokens": ["Die", "am", "Kreu\u00b7ze", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu mich S\u00fcnder doch gewand,", "tokens": ["Zu", "mich", "S\u00fcn\u00b7der", "doch", "ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "ADV", "VAPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und schlie\u00dft mich in sein Erbarmen:", "tokens": ["Und", "schlie\u00dft", "mich", "in", "sein", "Er\u00b7bar\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Darum bin ich herzlich froh,", "tokens": ["Da\u00b7rum", "bin", "ich", "herz\u00b7lich", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Darum hei\u00dft mein Wahlspruch so:", "tokens": ["Da\u00b7rum", "hei\u00dft", "mein", "Wahl\u00b7spruch", "so", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Der am Kreuz ist meine Liebe,", "tokens": ["Der", "am", "Kreuz", "ist", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob die Welt gleich schn\u00f6de fr\u00e4gt:", "tokens": ["Ob", "die", "Welt", "gleich", "schn\u00f6\u00b7de", "fr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum ich mich dem verschriebe,", "tokens": ["Wa\u00b7rum", "ich", "mich", "dem", "ver\u00b7schrie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "ADJA", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der ein schandbar Kreuze tr\u00e4gt.", "tokens": ["Der", "ein", "schand\u00b7bar", "Kreu\u00b7ze", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du machst Welt mit Satans-Rotten,", "tokens": ["Du", "machst", "Welt", "mit", "Sa\u00b7tans\u00b7Rot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Sein von Blut bestr\u00f6mmt Gesicht,", "tokens": ["Sein", "von", "Blut", "be\u00b7str\u00f6mmt", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sein geschwollnes Augenlicht,", "tokens": ["Sein", "ge\u00b7schwoll\u00b7nes", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als ein Fluch und Scheusal spotten,", "tokens": ["Als", "ein", "Fluch", "und", "Scheu\u00b7sal", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ich seh ihn im Glauben an,", "tokens": ["Ich", "seh", "ihn", "im", "Glau\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Als den allerliebsten Mann,", "tokens": ["Als", "den", "al\u00b7ler\u00b7liebs\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Der am Kreuz ist meine Liebe,", "tokens": ["Der", "am", "Kreuz", "ist", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er nimmt sich meiner an,", "tokens": ["Und", "er", "nimmt", "sich", "mei\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPOSAT", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df in dem Versuchungs-Siebe,", "tokens": ["Da\u00df", "in", "dem", "Ver\u00b7su\u00b7chungs\u00b7Sie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Satan mich nicht schaden kan,", "tokens": ["Sa\u00b7tan", "mich", "nicht", "scha\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sucht die H\u00f6lle mich zu sichten,", "tokens": ["Sucht", "die", "H\u00f6l\u00b7le", "mich", "zu", "sich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So st\u00e4rkt seine Wunderg\u00fct;", "tokens": ["So", "st\u00e4rkt", "sei\u00b7ne", "Wun\u00b7der\u00b7g\u00fct", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mein bewegtes Herz, Gem\u00fct", "tokens": ["Mein", "be\u00b7weg\u00b7tes", "Herz", ",", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df sie mich nicht kan zernichten.", "tokens": ["Da\u00df", "sie", "mich", "nicht", "kan", "zer\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Es bewahret mich sein Schuz,", "tokens": ["Es", "be\u00b7wah\u00b7ret", "mich", "sein", "Schuz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Darum sag ich ihr zum Truz:", "tokens": ["Da\u00b7rum", "sag", "ich", "ihr", "zum", "Truz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der am Kreuz ist meine Liebe,", "tokens": ["Der", "am", "Kreuz", "ist", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe ist nach meinem Sin,", "tokens": ["Lie\u00b7be", "ist", "nach", "mei\u00b7nem", "Sin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Tugend, die ich \u00fcbe,", "tokens": ["Ei\u00b7ne", "Tu\u00b7gend", ",", "die", "ich", "\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich hier auf Erden bin:", "tokens": ["Weil", "ich", "hier", "auf", "Er\u00b7den", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kom ich hin nach Salems Auen,", "tokens": ["Kom", "ich", "hin", "nach", "Sa\u00b7lems", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steige ich auf Tabors H\u00f6hn,", "tokens": ["Stei\u00b7ge", "ich", "auf", "Ta\u00b7bors", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werd ich ihn verkl\u00e4ret sehn,", "tokens": ["Werd", "ich", "ihn", "ver\u00b7kl\u00e4\u00b7ret", "sehn", ","], "token_info": ["word", "word", "w