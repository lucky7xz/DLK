{"textgrid.poem.38554": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Purpur malt die Tannenh\u00fcgel,", "tokens": ["Pur\u00b7pur", "malt", "die", "Tan\u00b7nen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Sonne Scheideblik,", "tokens": ["Nach", "der", "Son\u00b7ne", "Schei\u00b7de\u00b7blik", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich stralt des Baches Spiegel", "tokens": ["Lieb\u00b7lich", "stralt", "des", "Ba\u00b7ches", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hespers Fakkelglanz zur\u00fck;", "tokens": ["Hes\u00b7pers", "Fak\u00b7kel\u00b7glanz", "zu\u00b7r\u00fck", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie in Todtenhallen d\u00fcster", "tokens": ["Wie", "in", "Tod\u00b7ten\u00b7hal\u00b7len", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirds im Pappelweidenhain,", "tokens": ["Wirds", "im", "Pap\u00b7pel\u00b7wei\u00b7den\u00b7hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter leisem Blattgefl\u00fcster", "tokens": ["Un\u00b7ter", "lei\u00b7sem", "Blatt\u00b7ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummern alle V\u00f6gel ein!", "tokens": ["Schlum\u00b7mern", "al\u00b7le", "V\u00f6\u00b7gel", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur dein Abendlied, o Grille,", "tokens": ["Nur", "dein", "A\u00b7ben\u00b7dlied", ",", "o", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nt noch, aus bethautem Gr\u00fcn,", "tokens": ["T\u00f6nt", "noch", ",", "aus", "be\u00b7thau\u00b7tem", "Gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch der D\u00e4mrung Rosenh\u00fclle,", "tokens": ["Durch", "der", "D\u00e4m\u00b7rung", "Ro\u00b7sen\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe Trauermelodien;", "tokens": ["S\u00fc\u00b7\u00dfe", "Trau\u00b7er\u00b7me\u00b7lo\u00b7di\u00b7en", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Singt das bange Herz in Schlummer,", "tokens": ["Singt", "das", "ban\u00b7ge", "Herz", "in", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hemmt der Z\u00e4hren wilden Lauf,", "tokens": ["Hemmt", "der", "Z\u00e4h\u00b7ren", "wil\u00b7den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6s't der Liebe tiefsten Kummer", "tokens": ["L\u00f6s't", "der", "Lie\u00b7be", "tiefs\u00b7ten", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst in stille Wehmuth auf!", "tokens": ["Selbst", "in", "stil\u00b7le", "Weh\u00b7muth", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "T\u00f6nst du einst, im Abendhauche,", "tokens": ["T\u00f6nst", "du", "einst", ",", "im", "A\u00b7bend\u00b7hau\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grillchen, auf mein fr\u00fches Grab,", "tokens": ["Grill\u00b7chen", ",", "auf", "mein", "fr\u00fc\u00b7hes", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Freundschaft Rosenstrauche,", "tokens": ["Aus", "der", "Freund\u00b7schaft", "Ro\u00b7sen\u00b7strau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen Klaggesang herab:", "tokens": ["Dei\u00b7nen", "Klag\u00b7ge\u00b7sang", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wird noch stets mein Geist dir lauschen,", "tokens": ["Wird", "noch", "stets", "mein", "Geist", "dir", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horchend, wie er jezt dir lauscht,", "tokens": ["Hor\u00b7chend", ",", "wie", "er", "jezt", "dir", "lauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch des H\u00fcgels Blumen rauschen,", "tokens": ["Durch", "des", "H\u00fc\u00b7gels", "Blu\u00b7men", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dies Sommerl\u00fcftchen rauscht!", "tokens": ["Wie", "dies", "Som\u00b7mer\u00b7l\u00fcft\u00b7chen", "rauscht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}