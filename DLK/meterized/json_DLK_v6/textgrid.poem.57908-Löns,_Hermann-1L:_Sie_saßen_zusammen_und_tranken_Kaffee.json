{"textgrid.poem.57908": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie sa\u00dfen zusammen und tranken Kaffee", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sa\u00dfen zusammen und tranken Kaffee", "tokens": ["Sie", "sa\u00b7\u00dfen", "zu\u00b7sam\u00b7men", "und", "tran\u00b7ken", "Kaf\u00b7fee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und sprachen vom Achtuhrladenschlu\u00df:", "tokens": ["Und", "spra\u00b7chen", "vom", "A\u00b7chtuhr\u00b7la\u00b7den\u00b7schlu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbnu denken Se m\u00e4\u00f6l, was soll das werden,", "tokens": ["\u00bb", "nu", "den\u00b7ken", "Se", "m\u00e4\u00b7\u00f6l", ",", "was", "soll", "das", "wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "NE", "NE", "$,", "PWS", "VMFIN", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn man sp\u00e4t noch etwas h\u00e4\u00f6ben mu\u00df?!", "tokens": ["Wenn", "man", "sp\u00e4t", "noch", "et\u00b7was", "h\u00e4\u00f6\u00b7ben", "mu\u00df", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie oft ist nicht ger\u00e4\u00f6d das Petroleum alle,", "tokens": ["Wie", "oft", "ist", "nicht", "ge\u00b7r\u00e4\u00b7\u00f6d", "das", "Pet\u00b7ro\u00b7le\u00b7um", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+---+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oder V\u00e4\u00f6ters Zigarrenkiste ist leer,", "tokens": ["O\u00b7der", "V\u00e4\u00f6\u00b7ters", "Zi\u00b7gar\u00b7ren\u00b7kis\u00b7te", "ist", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Es kommt Besuch und man hat keinen Aufschnitt,", "tokens": ["Es", "kommt", "Be\u00b7such", "und", "man", "hat", "kei\u00b7nen", "Auf\u00b7schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "PIS", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht w\u00e4\u00f6hr, denn sitzt man d'r sch\u00f6ne her?!", "tokens": ["Nicht", "w\u00e4\u00b7\u00f6hr", ",", "denn", "sitzt", "man", "d'r", "sch\u00f6\u00b7ne", "her", "?!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KON", "VVFIN", "PIS", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wer kann denn auch immer an alles denken,", "tokens": ["Wer", "kann", "denn", "auch", "im\u00b7mer", "an", "al\u00b7les", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ADV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geht man zum Einholen in die Stadt?", "tokens": ["Geht", "man", "zum", "Ein\u00b7ho\u00b7len", "in", "die", "Stadt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Man trifft 'ne Bekannte und merkt erst zu Hause,", "tokens": ["Man", "trifft", "'ne", "Be\u00b7kann\u00b7te", "und", "merkt", "erst", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da\u00df man noch etwas vergessen hat.\u00ab", "tokens": ["Da\u00df", "man", "noch", "et\u00b7was", "ver\u00b7ges\u00b7sen", "hat", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sie sa\u00dfen und kl\u00f6nten bis viertel nach achte", "tokens": ["Sie", "sa\u00b7\u00dfen", "und", "kl\u00f6n\u00b7ten", "bis", "vier\u00b7tel", "nach", "ach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PIS", "APPR", "ADJA"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und kamen um dreiviertel neune nach Haus", "tokens": ["Und", "ka\u00b7men", "um", "drei\u00b7vier\u00b7tel", "neu\u00b7ne", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "CARD", "CARD", "APPR", "NN"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und hetzten die M\u00e4dchen zum Abendbrotkaufen", "tokens": ["Und", "hetz\u00b7ten", "die", "M\u00e4d\u00b7chen", "zum", "A\u00b7bend\u00b7brot\u00b7kau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sp\u00e4t abends um neun noch zum Kaufmann hinaus.", "tokens": ["Sp\u00e4t", "a\u00b7bends", "um", "neun", "noch", "zum", "Kauf\u00b7mann", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "CARD", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}