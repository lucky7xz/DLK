{"textgrid.poem.39960": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1. Wie auf einen Berg im Morgenlande zw\u00f6lf Sternseher gesetzt wurden", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umstr\u00f6mt von seiner Kr\u00e4uter D\u00fcften", "tokens": ["Um\u00b7str\u00f6mt", "von", "sei\u00b7ner", "Kr\u00e4u\u00b7ter", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcberwallt von edlem Holz,", "tokens": ["Und", "\u00fc\u00b7berw\u00b7allt", "von", "ed\u00b7lem", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der h\u00f6chste, steigt aus blauen L\u00fcften", "tokens": ["Der", "h\u00f6chs\u00b7te", ",", "steigt", "aus", "blau\u00b7en", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Berg, des Morgenlandes Stolz;", "tokens": ["Ein", "Berg", ",", "des", "Mor\u00b7gen\u00b7lan\u00b7des", "Stolz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steil ist der Pfad und lang die Reise,", "tokens": ["Steil", "ist", "der", "Pfad", "und", "lang", "die", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch oben herrlich Tag und Nacht;", "tokens": ["Doch", "o\u00b7ben", "herr\u00b7lich", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf seinem Gipfel stehn zw\u00f6lf Greise", "tokens": ["Auf", "sei\u00b7nem", "Gip\u00b7fel", "stehn", "zw\u00f6lf", "Grei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schauen in des Himmels Pracht.", "tokens": ["Und", "schau\u00b7en", "in", "des", "Him\u00b7mels", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie h\u00fcllen sich in die Gewande", "tokens": ["Sie", "h\u00fcl\u00b7len", "sich", "in", "die", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlummern \u00fcber jeden Tag,", "tokens": ["Und", "schlum\u00b7mern", "\u00fc\u00b7ber", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der unter ihnen auf die Lande", "tokens": ["Der", "un\u00b7ter", "ih\u00b7nen", "auf", "die", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umsonst sein Licht verbreiten mag.", "tokens": ["Um\u00b7sonst", "sein", "Licht", "ver\u00b7brei\u00b7ten", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie lassen sich vom Nachthauch wecken,", "tokens": ["Sie", "las\u00b7sen", "sich", "vom", "Nacht\u00b7hauch", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der durch der B\u00e4ume Wipfel f\u00e4hrt;", "tokens": ["Der", "durch", "der", "B\u00e4u\u00b7me", "Wip\u00b7fel", "f\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Sternen, die den Himmel decken,", "tokens": ["Den", "Ster\u00b7nen", ",", "die", "den", "Him\u00b7mel", "de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist dann ihr Auge zugekehrt.", "tokens": ["Ist", "dann", "ihr", "Au\u00b7ge", "zu\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit allen Wunderzeichen schimmert", "tokens": ["Mit", "al\u00b7len", "Wun\u00b7der\u00b7zei\u00b7chen", "schim\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Buch des Himmels aufgerollt;", "tokens": ["Das", "Buch", "des", "Him\u00b7mels", "auf\u00b7ge\u00b7rollt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was unten nur wie Silber flimmert,", "tokens": ["Was", "un\u00b7ten", "nur", "wie", "Sil\u00b7ber", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das leuchtet hier wie reines Gold.", "tokens": ["Das", "leuch\u00b7tet", "hier", "wie", "rei\u00b7nes", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ward in den Sternen je gelesen", "tokens": ["Ward", "in", "den", "Ster\u00b7nen", "je", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der irdischen Geschicke Pfand,", "tokens": ["Der", "ir\u00b7di\u00b7schen", "Ge\u00b7schi\u00b7cke", "Pfand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ist es dieser Berg gewesen,", "tokens": ["So", "ist", "es", "die\u00b7ser", "Berg", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf dem der Seher Gottes stand.", "tokens": ["Auf", "dem", "der", "Se\u00b7her", "Got\u00b7tes", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch diese stehen zu erkunden", "tokens": ["Auch", "die\u00b7se", "ste\u00b7hen", "zu", "er\u00b7kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem Gestirn des Himmels Rat,", "tokens": ["In", "dem", "Ge\u00b7stirn", "des", "Him\u00b7mels", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch haben sie noch nicht gefunden", "tokens": ["Doch", "ha\u00b7ben", "sie", "noch", "nicht", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Saatkorn in der reichen Saat:", "tokens": ["Ihr", "Saat\u00b7korn", "in", "der", "rei\u00b7chen", "Saat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Stern, der herrlich, \u00fcberschwenglich,", "tokens": ["Den", "Stern", ",", "der", "herr\u00b7lich", ",", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor allen andern stralenvoll,", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "stra\u00b7len\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Licht, ein Feuer unverg\u00e4nglich", "tokens": ["Ein", "Licht", ",", "ein", "Feu\u00b7er", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den blinden Heiden z\u00fcnden soll;", "tokens": ["Den", "blin\u00b7den", "Hei\u00b7den", "z\u00fcn\u00b7den", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Stern, den ", "tokens": ["Den", "Stern", ",", "den"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der einem K\u00f6nig stralen wird,", "tokens": ["Der", "ei\u00b7nem", "K\u00f6\u00b7nig", "stra\u00b7len", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der einst die ganze Welt ents\u00fcndigt", "tokens": ["Der", "einst", "die", "gan\u00b7ze", "Welt", "ent\u00b7s\u00fcn\u00b7digt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und herrschen soll, der V\u00f6lker Hirt.", "tokens": ["Und", "herr\u00b7schen", "soll", ",", "der", "V\u00f6l\u00b7ker", "Hirt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lautete der Spruch des Weisen", "tokens": ["So", "lau\u00b7te\u00b7te", "der", "Spruch", "des", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An das erstaunte Morgenland;", "tokens": ["An", "das", "er\u00b7staun\u00b7te", "Mor\u00b7gen\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das rief den himmelskund'gen Greisen", "tokens": ["Das", "rief", "den", "him\u00b7mels\u00b7kun\u00b7d'\u00b7gen", "Grei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu wachen auf des Berges Rand.", "tokens": ["Zu", "wa\u00b7chen", "auf", "des", "Ber\u00b7ges", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Hoffnung k\u00fcrzt des Weges Ferne,", "tokens": ["Die", "Hoff\u00b7nung", "k\u00fcrzt", "des", "We\u00b7ges", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ebnet rings den steilen Pfad,", "tokens": ["Sie", "eb\u00b7net", "rings", "den", "stei\u00b7len", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhellt die alten Augensterne,", "tokens": ["Er\u00b7hellt", "die", "al\u00b7ten", "Au\u00b7gens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Macht den gebeugten Nacken grad'.", "tokens": ["Macht", "den", "ge\u00b7beug\u00b7ten", "Na\u00b7cken", "grad'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und ist im Tod ihr Blick zerronnen,", "tokens": ["Und", "ist", "im", "Tod", "ihr", "Blick", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den langes Forschen aufw\u00e4rts zog,", "tokens": ["Den", "lan\u00b7ges", "For\u00b7schen", "auf\u00b7w\u00e4rts", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wecken ihn die tausend Sonnen,", "tokens": ["So", "we\u00b7cken", "ihn", "die", "tau\u00b7send", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu denen seine Sehnsucht flog.", "tokens": ["Zu", "de\u00b7nen", "sei\u00b7ne", "Sehn\u00b7sucht", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}