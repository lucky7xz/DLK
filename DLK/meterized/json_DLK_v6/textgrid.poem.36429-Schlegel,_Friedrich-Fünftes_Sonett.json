{"textgrid.poem.36429": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnftes Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So liegst du gut. Gleich wird sich's pr\u00e4chtig zeigen", "tokens": ["So", "liegst", "du", "gut", ".", "Gleich", "wird", "sich's", "pr\u00e4ch\u00b7tig", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VAFIN", "PIS", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie klug mein Rat: ich schiebe meinen Dicken", "tokens": ["Wie", "klug", "mein", "Rat", ":", "ich", "schie\u00b7be", "mei\u00b7nen", "Di\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dein bemoostes Tor \u2013 man nennt das Ficken.", "tokens": ["In", "dein", "be\u00b7moos\u00b7tes", "Tor", "\u2013", "man", "nennt", "das", "Fi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du fragst warum? \u2013 Davon la\u00df jetzt mich schweigen.", "tokens": ["Du", "fragst", "wa\u00b7rum", "?", "\u2013", "Da\u00b7von", "la\u00df", "jetzt", "mich", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAV", "$.", "$(", "PAV", "VVFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schon seh' ich Schmerz in deinen blanken Blicken,", "tokens": ["Schon", "seh'", "ich", "Schmerz", "in", "dei\u00b7nen", "blan\u00b7ken", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das geht vorbei: du mu\u00dft zur\u00fcck dich neigen,", "tokens": ["Das", "geht", "vor\u00b7bei", ":", "du", "mu\u00dft", "zu\u00b7r\u00fcck", "dich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleich wird dein Blut dir jubeln wie die Geigen", "tokens": ["Gleich", "wird", "dein", "Blut", "dir", "ju\u00b7beln", "wie", "die", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Engeln, welche ihre Br\u00fcnste schicken", "tokens": ["Von", "En\u00b7geln", ",", "wel\u00b7che", "ih\u00b7re", "Br\u00fcns\u00b7te", "schi\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In bebender Musik zum Ohr der Welt.", "tokens": ["In", "be\u00b7ben\u00b7der", "Mu\u00b7sik", "zum", "Ohr", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Famos! ... Du einst dich mir in bravem Schaukeln,", "tokens": ["Fa\u00b7mos", "!", "...", "Du", "einst", "dich", "mir", "in", "bra\u00b7vem", "Schau\u00b7keln", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "PPER", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schenkel schmiegen pressend, es umgaukeln", "tokens": ["Die", "Schen\u00b7kel", "schmie\u00b7gen", "pres\u00b7send", ",", "es", "um\u00b7gau\u00b7keln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mich D\u00fcfte, die mich locken in die Unterwelt.", "tokens": ["Mich", "D\u00fcf\u00b7te", ",", "die", "mich", "lo\u00b7cken", "in", "die", "Un\u00b7ter\u00b7welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Sto\u00df \u2013 ein Schrei! ... Die wei\u00dfen Glieder zittern", "tokens": ["Ein", "Sto\u00df", "\u2013", "ein", "Schrei", "!", "...", "Die", "wei\u00b7\u00dfen", "Glie\u00b7der", "zit\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "$.", "$(", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Kampf wie Apfelbl\u00fcten in Gewittern.", "tokens": ["Im", "Kampf", "wie", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten", "in", "Ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}