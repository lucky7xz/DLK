{"textgrid.poem.66685": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu unsres Seeheims sonnenhellen R\u00e4umen", "tokens": ["Zu", "uns\u00b7res", "See\u00b7heims", "son\u00b7nen\u00b7hel\u00b7len", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Blumen und mit Fr\u00fcchten kehrst du froh,", "tokens": ["Mit", "Blu\u00b7men", "und", "mit", "Fr\u00fcch\u00b7ten", "kehrst", "du", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Morgenpfad der Ehe sch\u00f6n zu s\u00e4umen,", "tokens": ["Den", "Mor\u00b7gen\u00b7pfad", "der", "E\u00b7he", "sch\u00f6n", "zu", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erscheinst du, Mutter, und gen\u00fcgst dir so.", "tokens": ["Er\u00b7scheinst", "du", ",", "Mut\u00b7ter", ",", "und", "ge\u00b7n\u00fcgst", "dir", "so", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du sahst mein Leben wirbelnd \u00fcbersch\u00e4umen", "tokens": ["Du", "sahst", "mein", "Le\u00b7ben", "wir\u00b7belnd", "\u00fc\u00b7bersc\u00b7h\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und littest, als mir Gl\u00fcck und Heil entfloh \u2013", "tokens": ["Und", "lit\u00b7test", ",", "als", "mir", "Gl\u00fcck", "und", "Heil", "ent\u00b7floh", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nun f\u00fchlst du mich vom Sturm der Jugendsorgen", "tokens": ["Nun", "f\u00fchlst", "du", "mich", "vom", "Sturm", "der", "Ju\u00b7gen\u00b7dsor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf sicherm Eiland liebevoll geborgen.", "tokens": ["Auf", "si\u00b7cherm", "Ei\u00b7land", "lie\u00b7be\u00b7voll", "ge\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Du guter Geist, der friedensstill geglitten", "tokens": ["Du", "gu\u00b7ter", "Geist", ",", "der", "frie\u00b7dens\u00b7still", "ge\u00b7glit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch unsre Sph\u00e4re wie ein sanfter Stern,", "tokens": ["Durch", "uns\u00b7re", "Sph\u00e4\u00b7re", "wie", "ein", "sanf\u00b7ter", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du Mutterherz, hast nur in unsrer Mitten", "tokens": ["Du", "Mut\u00b7ter\u00b7herz", ",", "hast", "nur", "in", "uns\u00b7rer", "Mit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu kurz geweilt \u2013 wie hielten wir dich gern!", "tokens": ["Zu", "kurz", "ge\u00b7weilt", "\u2013", "wie", "hiel\u00b7ten", "wir", "dich", "gern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$(", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht wahr, wenn wir dich doppelt innig bitten,", "tokens": ["Nicht", "wahr", ",", "wenn", "wir", "dich", "dop\u00b7pelt", "in\u00b7nig", "bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Steht uns der Tag der Wiederkehr nicht fern? ...", "tokens": ["Steht", "uns", "der", "Tag", "der", "Wie\u00b7der\u00b7kehr", "nicht", "fern", "?", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du hast es uns zum Abschied fest versprochen", "tokens": ["Du", "hast", "es", "uns", "zum", "Ab\u00b7schied", "fest", "ver\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und nicht nur f\u00fcr die karge Frist von Wochen.", "tokens": ["Und", "nicht", "nur", "f\u00fcr", "die", "kar\u00b7ge", "Frist", "von", "Wo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df, du willst die Liebe recht verteilen,", "tokens": ["Ich", "wei\u00df", ",", "du", "willst", "die", "Lie\u00b7be", "recht", "ver\u00b7tei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was dich ruft, ist sorgsam treuer Sinn \u2013", "tokens": ["Und", "was", "dich", "ruft", ",", "ist", "sorg\u00b7sam", "treu\u00b7er", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich s\u00e4he dich am liebsten dauernd weilen", "tokens": ["Ich", "s\u00e4\u00b7he", "dich", "am", "liebs\u00b7ten", "dau\u00b7ernd", "wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKA", "ADJD", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An dieser St\u00e4tte, wo ich heimisch bin.", "tokens": ["An", "die\u00b7ser", "St\u00e4t\u00b7te", ",", "wo", "ich", "hei\u00b7misch", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du hast die Hand, zu helfen und zu heilen,", "tokens": ["Du", "hast", "die", "Hand", ",", "zu", "hel\u00b7fen", "und", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wo du bist, winkt edelster Gewinn ...", "tokens": ["Und", "wo", "du", "bist", ",", "winkt", "e\u00b7dels\u00b7ter", "Ge\u00b7winn", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "$,", "VVFIN", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Du bist so unersch\u00f6pflich reich im Geben \u2013", "tokens": ["Du", "bist", "so", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "reich", "im", "Ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie d\u00fcrftig bettelarm sind wir daneben!", "tokens": ["Wie", "d\u00fcrf\u00b7tig", "bet\u00b7tel\u00b7arm", "sind", "wir", "da\u00b7ne\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "VAFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die hohen Lilien, die du freudig brachtest,", "tokens": ["Die", "ho\u00b7hen", "Li\u00b7li\u00b7en", ",", "die", "du", "freu\u00b7dig", "brach\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stehn unverwelklich tief im Herzensgrund:", "tokens": ["Stehn", "un\u00b7ver\u00b7wel\u00b7klich", "tief", "im", "Her\u00b7zens\u00b7grund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich seh dich stets, wie du f\u00fcr andre dachtest", "tokens": ["Ich", "seh", "dich", "stets", ",", "wie", "du", "f\u00fcr", "and\u00b7re", "dach\u00b7test"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit einer Kraft, die nur der Liebe kund.", "tokens": ["Mit", "ei\u00b7ner", "Kraft", ",", "die", "nur", "der", "Lie\u00b7be", "kund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weil schlicht du nach der wahren G\u00fcte trachtest,", "tokens": ["Weil", "schlicht", "du", "nach", "der", "wah\u00b7ren", "G\u00fc\u00b7te", "trach\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kam nie ein hartes Wort aus deinem Mund \u2013", "tokens": ["Kam", "nie", "ein", "har\u00b7tes", "Wort", "aus", "dei\u00b7nem", "Mund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was sch\u00f6n im Evangelium zu lesen,", "tokens": ["Was", "sch\u00f6n", "im", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das, Mutter, schenkt uns dein lebendig Wesen.", "tokens": ["Das", ",", "Mut\u00b7ter", ",", "schenkt", "uns", "dein", "le\u00b7ben\u00b7dig", "We\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und deine N\u00e4he, m\u00fctterliches Walten,", "tokens": ["Und", "dei\u00b7ne", "N\u00e4\u00b7he", ",", "m\u00fct\u00b7ter\u00b7li\u00b7ches", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weht mir den Hauch der alten Heimat zu \u2013", "tokens": ["Weht", "mir", "den", "Hauch", "der", "al\u00b7ten", "Hei\u00b7mat", "zu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was da noch flattert in des Herzens Falten,", "tokens": ["Was", "da", "noch", "flat\u00b7tert", "in", "des", "Her\u00b7zens", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du gibst ihm durch dein Mitverweilen Ruh.", "tokens": ["Du", "gibst", "ihm", "durch", "dein", "Mit\u00b7ver\u00b7wei\u00b7len", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Namen hegst du mir, die trauten, alten,", "tokens": ["Die", "Na\u00b7men", "hegst", "du", "mir", ",", "die", "trau\u00b7ten", ",", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Kindheit Scholle h\u00e4ngt an deinem Schuh,", "tokens": ["Der", "Kind\u00b7heit", "Schol\u00b7le", "h\u00e4ngt", "an", "dei\u00b7nem", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In deinen Armen f\u00fchl' ich fr\u00fchste Bande", "tokens": ["In", "dei\u00b7nen", "Ar\u00b7men", "f\u00fchl'", "ich", "fr\u00fchs\u00b7te", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gekn\u00fcpft zum heimischen Hannoverlande.", "tokens": ["Ge\u00b7kn\u00fcpft", "zum", "hei\u00b7mi\u00b7schen", "Han\u00b7no\u00b7ver\u00b7lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "'s ist wie ein liebebittend, leises Mahnen", "tokens": ["'s", "ist", "wie", "ein", "lie\u00b7be\u00b7bit\u00b7tend", ",", "lei\u00b7ses", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "An Vaterhaus und -garten, Ernst und Spiel,", "tokens": ["An", "Va\u00b7ter\u00b7haus", "und", ",", "Ernst", "und", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aufhissen lange zugerollter Fahnen", "tokens": ["Auf\u00b7his\u00b7sen", "lan\u00b7ge", "zu\u00b7ge\u00b7roll\u00b7ter", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An der Erinnrung silberknaufigem Stiel:", "tokens": ["An", "der", "E\u00b7rinn\u00b7rung", "sil\u00b7ber\u00b7knau\u00b7fi\u00b7gem", "Stiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Durch Korngefilde zieht auf hei\u00dfen Bahnen", "tokens": ["Durch", "Korn\u00b7ge\u00b7fil\u00b7de", "zieht", "auf", "hei\u00b7\u00dfen", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Knabe zu dem schattigen Waldesziel,", "tokens": ["Der", "Kna\u00b7be", "zu", "dem", "schat\u00b7ti\u00b7gen", "Wal\u00b7des\u00b7ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wie traumfern klingt mit sehnsuchtdunkler Klage", "tokens": ["Wie", "traum\u00b7fern", "klingt", "mit", "sehn\u00b7sucht\u00b7dunk\u00b7ler", "Kla\u00b7ge"], "token_info":