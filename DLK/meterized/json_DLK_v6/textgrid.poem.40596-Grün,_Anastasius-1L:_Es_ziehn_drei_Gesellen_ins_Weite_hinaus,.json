{"textgrid.poem.40596": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ziehn drei Gesellen ins Weite hinaus,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ziehn drei Gesellen ins Weite hinaus,", "tokens": ["Es", "ziehn", "drei", "Ge\u00b7sel\u00b7len", "ins", "Wei\u00b7te", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es litt sie nimmer im engen Haus;", "tokens": ["Es", "litt", "sie", "nim\u00b7mer", "im", "en\u00b7gen", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein jeder doch nahm was Liebes mit sich,", "tokens": ["Ein", "je\u00b7der", "doch", "nahm", "was", "Lie\u00b7bes", "mit", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "PIS", "NN", "APPR", "PRF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das hegt' er und pflegt' er gar inniglich.", "tokens": ["Das", "hegt'", "er", "und", "pflegt'", "er", "gar", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Der Erste ein wackerer Goldschmied war,", "tokens": ["Der", "Ers\u00b7te", "ein", "wa\u00b7cke\u00b7rer", "Gold\u00b7schmied", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der trug ein Ringlein aus Liebchens Haar,", "tokens": ["Der", "trug", "ein", "Rin\u00b7glein", "aus", "Lieb\u00b7chens", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das hatt' er gefa\u00dft in Gold und Stein", "tokens": ["Das", "hatt'", "er", "ge\u00b7fa\u00dft", "in", "Gold", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ihren Namen gegraben darein.", "tokens": ["Und", "ih\u00b7ren", "Na\u00b7men", "ge\u00b7gra\u00b7ben", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Zweite ein herrlicher Maler war,", "tokens": ["Der", "Zwei\u00b7te", "ein", "herr\u00b7li\u00b7cher", "Ma\u00b7ler", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der trug ein Bildni\u00df gar wunderbar,", "tokens": ["Der", "trug", "ein", "Bild\u00b7ni\u00df", "gar", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es war des Liebchens l\u00e4chelndes Bild,", "tokens": ["Es", "war", "des", "Lieb\u00b7chens", "l\u00e4\u00b7cheln\u00b7des", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das trug er auf seinem Herzen als Schild.", "tokens": ["Das", "trug", "er", "auf", "sei\u00b7nem", "Her\u00b7zen", "als", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "KOUS", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Dichterj\u00fcngling der Dritte war", "tokens": ["Ein", "Dich\u00b7ter\u00b7j\u00fcng\u00b7ling", "der", "Drit\u00b7te", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit bl\u00fchendem Antlitz und g\u00fcldnem Haar,", "tokens": ["Mit", "bl\u00fc\u00b7hen\u00b7dem", "Ant\u00b7litz", "und", "g\u00fcld\u00b7nem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Trug Bild und Namen im Herzen sein,", "tokens": ["Trug", "Bild", "und", "Na\u00b7men", "im", "Her\u00b7zen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Manch sch\u00f6nes Lied noch obendrein.", "tokens": ["Manch", "sch\u00f6\u00b7nes", "Lied", "noch", "o\u00b7ben\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie sie einst sehn in den Strom hinab,", "tokens": ["Und", "wie", "sie", "einst", "sehn", "in", "den", "Strom", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Sinkt's Ringlein des Ersten ins Wellengrab;", "tokens": ["Sinkt's", "Rin\u00b7glein", "des", "Ers\u00b7ten", "ins", "Wel\u00b7len\u00b7grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und wie sie einst stehen auf hohem Thurm,", "tokens": ["Und", "wie", "sie", "einst", "ste\u00b7hen", "auf", "ho\u00b7hem", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da raubt das Bildni\u00df des Zweiten der Sturm.", "tokens": ["Da", "raubt", "das", "Bild\u00b7ni\u00df", "des", "Zwei\u00b7ten", "der", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Beiden ringen die H\u00e4nde sich wund,", "tokens": ["Die", "Bei\u00b7den", "rin\u00b7gen", "die", "H\u00e4n\u00b7de", "sich", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch jubelnd t\u00f6nt des Dichters Mund;", "tokens": ["Doch", "ju\u00b7belnd", "t\u00f6nt", "des", "Dich\u00b7ters", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tr\u00e4gt Namen und Bild ja im Herzen sein,", "tokens": ["Tr\u00e4gt", "Na\u00b7men", "und", "Bild", "ja", "im", "Her\u00b7zen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "APPRART", "NN", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Manch sch\u00f6nes Lied noch obendrein.", "tokens": ["Manch", "sch\u00f6\u00b7nes", "Lied", "noch", "o\u00b7ben\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}