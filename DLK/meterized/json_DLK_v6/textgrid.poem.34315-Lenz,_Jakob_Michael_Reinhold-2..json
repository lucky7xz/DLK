{"textgrid.poem.34315": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was fang ich an? was r\u00e4thst du Liebe mir?", "tokens": ["Was", "fang", "ich", "an", "?", "was", "r\u00e4thst", "du", "Lie\u00b7be", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu sterben w\u00e4r es Zeit. Was zaudr' ich hier?", "tokens": ["Zu", "ster\u00b7ben", "w\u00e4r", "es", "Zeit", ".", "Was", "zaudr'", "ich", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Madonna todt, mein Herz hinweg genommen,", "tokens": ["Ma\u00b7don\u00b7na", "todt", ",", "mein", "Herz", "hin\u00b7weg", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPOSAT", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mu\u00df ich M\u00f6rder seyn, zu ihr zu kommen?", "tokens": ["Und", "mu\u00df", "ich", "M\u00f6r\u00b7der", "seyn", ",", "zu", "ihr", "zu", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VAINF", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja, ja, ich mu\u00df, weil ich sie nie", "tokens": ["Ja", ",", "ja", ",", "ich", "mu\u00df", ",", "weil", "ich", "sie", "nie"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mehr hoffen kann zu sehn; ach! ohne sie", "tokens": ["Mehr", "hof\u00b7fen", "kann", "zu", "sehn", ";", "ach", "!", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIS", "VVINF", "VMFIN", "PTKZU", "VVINF", "$.", "XY", "$.", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was ist das Leben? T\u00f6dtendlangsam Sehnen", "tokens": ["Was", "ist", "das", "Le\u00b7ben", "?", "T\u00f6d\u00b7tend\u00b7lang\u00b7sam", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach der Erl\u00f6sung, was die Freude? \u2013 Tr\u00e4nen.", "tokens": ["Nach", "der", "Er\u00b7l\u00f6\u00b7sung", ",", "was", "die", "Freu\u00b7de", "?", "\u2013", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "$.", "$(", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Du wei\u00dft es Liebe, kennest das Gewicht", "tokens": ["Du", "wei\u00dft", "es", "Lie\u00b7be", ",", "ken\u00b7nest", "das", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der grauenvollen Schmerzen alle.", "tokens": ["Der", "grau\u00b7en\u00b7vol\u00b7len", "Schmer\u00b7zen", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gescheitert unser Schiff, dahin das Licht,", "tokens": ["Ge\u00b7schei\u00b7tert", "un\u00b7ser", "Schiff", ",", "da\u00b7hin", "das", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$,", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das uns geleitet. Diesem Trauerfalle", "tokens": ["Das", "uns", "ge\u00b7lei\u00b7tet", ".", "Die\u00b7sem", "Trau\u00b7er\u00b7fal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PPER", "VVPP", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vergleicht sich nichts. O Erde! wie entstellt,", "tokens": ["Ver\u00b7gleicht", "sich", "nichts", ".", "O", "Er\u00b7de", "!", "wie", "ent\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "$.", "NE", "NN", "$.", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach wie verwayset, undanckbare Welt!", "tokens": ["Ach", "wie", "ver\u00b7way\u00b7set", ",", "un\u00b7dan\u00b7ck\u00b7ba\u00b7re", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Dein Reitz ist hin, elende Welt voll Thoren.", "tokens": ["Dein", "Reitz", "ist", "hin", ",", "e\u00b7len\u00b7de", "Welt", "voll", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Ach, w\u00fc\u00dftest du, was du an ihr verloren,", "tokens": ["Ach", ",", "w\u00fc\u00df\u00b7test", "du", ",", "was", "du", "an", "ihr", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Du traurtest ewig. Nur durch sie noch sch\u00f6n:", "tokens": ["Du", "traur\u00b7test", "e\u00b7wig", ".", "Nur", "durch", "sie", "noch", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ADV", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und sahsts nicht ein, du warsts nicht werth zu sehn,", "tokens": ["Und", "sahsts", "nicht", "ein", ",", "du", "warsts", "nicht", "werth", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht werth, da\u00df ihre F\u00fcsse dich ber\u00fchrten,", "tokens": ["Nicht", "werth", ",", "da\u00df", "ih\u00b7re", "F\u00fcs\u00b7se", "dich", "be\u00b7r\u00fchr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die heiligen F\u00fcsse, die gen Himmel f\u00fchrten.", "tokens": ["Die", "hei\u00b7li\u00b7gen", "F\u00fcs\u00b7se", ",", "die", "gen", "Him\u00b7mel", "f\u00fchr\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Der Himmel neidisch auf dein Gl\u00fcck", "tokens": ["Der", "Him\u00b7mel", "nei\u00b7disch", "auf", "dein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nahm sein geliehnes Pfand zur\u00fcck.", "tokens": ["Nahm", "sein", "ge\u00b7lieh\u00b7nes", "Pfand", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und ich Verla\u00dfner! der ich ohne", "tokens": ["Und", "ich", "Ver\u00b7la\u00df\u00b7ner", "!", "der", "ich", "oh\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "$.", "PRELS", "PPER", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sie weder Welt noch mich ertragen kann,", "tokens": ["Sie", "we\u00b7der", "Welt", "noch", "mich", "er\u00b7tra\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "NN", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ich sitze hier und weine. Ruffe", "tokens": ["Ich", "sit\u00b7ze", "hier", "und", "wei\u00b7ne", ".", "Ruf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Vergeblich sie zur\u00fcck. Trost, da\u00df ich weinen kann!", "tokens": ["Ver\u00b7geb\u00b7lich", "sie", "zu\u00b7r\u00fcck", ".", "Trost", ",", "da\u00df", "ich", "wei\u00b7nen", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "$.", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Weh mir! ihr Antlitz Erde! ihre Mienen,", "tokens": ["Weh", "mir", "!", "ihr", "Ant\u00b7litz", "Er\u00b7de", "!", "ih\u00b7re", "Mie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPOSAT", "NN", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Auf denen Hofnungen des Himmels schienen", "tokens": ["Auf", "de\u00b7nen", "Hof\u00b7nun\u00b7gen", "des", "Him\u00b7mels", "schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die uns allein ihn glauben machten! Nein,", "tokens": ["Die", "uns", "al\u00b7lein", "ihn", "glau\u00b7ben", "mach\u00b7ten", "!", "Nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPER", "VVINF", "VVFIN", "$.", "PTKANT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die g\u00f6ttliche Gestalt kann nicht verweset seyn.", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7stalt", "kann", "nicht", "ver\u00b7we\u00b7set", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den Schleyer hat sie abgelegt, der ihre Bl\u00fcthe", "tokens": ["Den", "Schle\u00b7yer", "hat", "sie", "ab\u00b7ge\u00b7legt", ",", "der", "ih\u00b7re", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hier eingeschattet; ganz voll Lieb' und G\u00fcte", "tokens": ["Hier", "ein\u00b7ge\u00b7schat\u00b7tet", ";", "ganz", "voll", "Lieb'", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$.", "ADV", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Schwebt sie im Paradiese \u2013 oder hier", "tokens": ["Schwebt", "sie", "im", "Pa\u00b7ra\u00b7die\u00b7se", "\u2013", "o\u00b7der", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$(", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "O g\u00f6ttlich s\u00fcsser Schaur! \u2013 unsichtbar neben mir \u2013 u.s.f.", "tokens": ["O", "g\u00f6tt\u00b7lich", "s\u00fcs\u00b7ser", "Schaur", "!", "\u2013", "un\u00b7sicht\u00b7bar", "ne\u00b7ben", "mir", "\u2013", "u.", "s.", "f."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "ADJD", "ADJA", "NN", "$.", "$(", "ADJD", "APPR", "PPER", "$(", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}