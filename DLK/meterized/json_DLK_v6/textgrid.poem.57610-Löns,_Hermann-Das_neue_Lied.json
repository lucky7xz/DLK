{"textgrid.poem.57610": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Lied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt hab' ich satt den schlappen Singsang", "tokens": ["Jetzt", "hab'", "ich", "satt", "den", "schlap\u00b7pen", "Sings\u00b7ang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von Liebe, Triebe, Weh und Ach,", "tokens": ["Von", "Lie\u00b7be", ",", "Trie\u00b7be", ",", "Weh", "und", "Ach", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den veilchenblauen Goldschnittsklingklang,", "tokens": ["Den", "veil\u00b7chen\u00b7blau\u00b7en", "Gold\u00b7schnitts\u00b7kling\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich durst' nach einem Donnerschlag;", "tokens": ["Ich", "dur\u00b7st'", "nach", "ei\u00b7nem", "Don\u00b7ner\u00b7schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Verfaulten Leichen gilt ihr Singen,", "tokens": ["Ver\u00b7faul\u00b7ten", "Lei\u00b7chen", "gilt", "ihr", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Aasgestank die Poesie \u2013", "tokens": ["Voll", "Aas\u00b7ge\u00b7stank", "die", "Poe\u00b7sie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum la\u00dft ein neues Lied erklingen", "tokens": ["Drum", "la\u00dft", "ein", "neu\u00b7es", "Lied", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach einer neuen Melodie!", "tokens": ["Nach", "ei\u00b7ner", "neu\u00b7en", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schafft ab die ungesunde Mode,", "tokens": ["Schafft", "ab", "die", "un\u00b7ge\u00b7sun\u00b7de", "Mo\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Leib zu betten in die Gruft,", "tokens": ["Den", "Leib", "zu", "bet\u00b7ten", "in", "die", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verbrennt, was zinsbar ward dem Tode,", "tokens": ["Ver\u00b7brennt", ",", "was", "zins\u00b7bar", "ward", "dem", "To\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und streut die Asche in die Luft;", "tokens": ["Und", "streut", "die", "A\u00b7sche", "in", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Asche soll den Acker d\u00fcngen,", "tokens": ["Die", "A\u00b7sche", "soll", "den", "A\u00b7cker", "d\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Friedhof, mach' Platz der Industrie \u2013", "tokens": ["Fried\u00b7hof", ",", "mach'", "Platz", "der", "In\u00b7dust\u00b7rie", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und la\u00dft das neue Lied uns singen", "tokens": ["Und", "la\u00dft", "das", "neu\u00b7e", "Lied", "uns", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach einer neuen Melodie.", "tokens": ["Nach", "ei\u00b7ner", "neu\u00b7en", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Blumen, die auf Leichen bl\u00fchen,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "auf", "Lei\u00b7chen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind ohne D\u00fcfte und verjaucht,", "tokens": ["Sind", "oh\u00b7ne", "D\u00fcf\u00b7te", "und", "ver\u00b7jaucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Herzen, die f\u00fcr Totes gl\u00fchen,", "tokens": ["Die", "Her\u00b7zen", ",", "die", "f\u00fcr", "To\u00b7tes", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind f\u00fcr das Leben l\u00e4ngst verbraucht,", "tokens": ["Sind", "f\u00fcr", "das", "Le\u00b7ben", "l\u00e4ngst", "ver\u00b7braucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur ein gesunder Geist kann singen", "tokens": ["Nur", "ein", "ge\u00b7sun\u00b7der", "Geist", "kann", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die zeitgerechte Poesie \u2013", "tokens": ["Die", "zeit\u00b7ge\u00b7rech\u00b7te", "Poe\u00b7sie", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum la\u00dft ein neues Lied erklingen", "tokens": ["Drum", "la\u00dft", "ein", "neu\u00b7es", "Lied", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach einer neuen Melodie!", "tokens": ["Nach", "ei\u00b7ner", "neu\u00b7en", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00dft die Vergangenheit vergangen", "tokens": ["La\u00dft", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00dft begraben sein, was tot,", "tokens": ["Und", "la\u00dft", "be\u00b7gra\u00b7ben", "sein", ",", "was", "tot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "VVPP", "VAINF", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fa\u00dft ein mutiges Verlangen", "tokens": ["Und", "fa\u00dft", "ein", "mu\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Sonnenlicht und Morgenrot;", "tokens": ["Nach", "Son\u00b7nen\u00b7licht", "und", "Mor\u00b7gen\u00b7rot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sind noch jung, uns mu\u00df gelingen", "tokens": ["Wir", "sind", "noch", "jung", ",", "uns", "mu\u00df", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die l\u00e4ngst ertr\u00e4umte Poesie \u2013", "tokens": ["Die", "l\u00e4ngst", "er\u00b7tr\u00e4um\u00b7te", "Poe\u00b7sie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein neues Lied soll jetzt erklingen", "tokens": ["Ein", "neu\u00b7es", "Lied", "soll", "jetzt", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach einer neuen Melodie!", "tokens": ["Nach", "ei\u00b7ner", "neu\u00b7en", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}