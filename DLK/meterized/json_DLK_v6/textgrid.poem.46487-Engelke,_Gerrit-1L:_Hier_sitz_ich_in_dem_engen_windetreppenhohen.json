{"textgrid.poem.46487": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier sitz ich in dem engen windetreppenhohen", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sitz ich in dem engen windetreppenhohen", "tokens": ["Hier", "sitz", "ich", "in", "dem", "en\u00b7gen", "win\u00b7de\u00b7trep\u00b7pen\u00b7ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steinstadt-Zimmer.", "tokens": ["Stein\u00b7stadt\u00b7Zim\u00b7mer", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ich m\u00f6chte raus aus diesem rohen", "tokens": ["Ich", "m\u00f6ch\u00b7te", "raus", "aus", "die\u00b7sem", "ro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stra\u00dfenleben, diesem Gr\u00fcnzeugmarkt-Gewimmer \u2013", "tokens": ["Stra\u00b7\u00dfen\u00b7le\u00b7ben", ",", "die\u00b7sem", "Gr\u00fcn\u00b7zeug\u00b7mark\u00b7tGe\u00b7wim\u00b7mer", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Fort von diesen Tanten, diesem Schw\u00e4her \u2013", "tokens": ["Fort", "von", "die\u00b7sen", "Tan\u00b7ten", ",", "die\u00b7sem", "Schw\u00e4\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,", "PDAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ich, der l\u00e4rm- und werkdurchfurchte Europ\u00e4er,", "tokens": ["Ich", ",", "der", "l\u00e4r\u00b7m", "und", "werk\u00b7durch\u00b7furch\u00b7te", "Eu\u00b7ro\u00b7p\u00e4\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O' Tehura \u2013", "tokens": ["O'", "Te\u00b7hu\u00b7ra", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "O' Tehura, weit zu deinem S\u00fcdseeriffe,", "tokens": ["O'", "Te\u00b7hu\u00b7ra", ",", "weit", "zu", "dei\u00b7nem", "S\u00fcd\u00b7see\u00b7rif\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo noch Krater in die Wolken br\u00fcllen,", "tokens": ["Wo", "noch", "Kra\u00b7ter", "in", "die", "Wol\u00b7ken", "br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo die Menschen nur in Sonnedunst sich h\u00fcllen,", "tokens": ["Wo", "die", "Men\u00b7schen", "nur", "in", "Son\u00b7ne\u00b7dunst", "sich", "h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Gleiten, gleiten meine weichen Sehnsucht-Schiffe \u2013", "tokens": ["Glei\u00b7ten", ",", "glei\u00b7ten", "mei\u00b7ne", "wei\u00b7chen", "Sehn\u00b7sucht\u00b7Schif\u00b7fe", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "O' Tehura \u2013", "tokens": ["O'", "Te\u00b7hu\u00b7ra", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wenn ich bei dir s\u00e4\u00dfe, sagte: sing!", "tokens": ["Wenn", "ich", "bei", "dir", "s\u00e4\u00b7\u00dfe", ",", "sag\u00b7te", ":", "sing", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und du zupftest die Zweisaitenlaute:", "tokens": ["Und", "du", "zupf\u00b7test", "die", "Zwei\u00b7sai\u00b7ten\u00b7lau\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kling-zum, kling-zum, ping \u2013 \u2013", "tokens": ["Kling\u00b7zum", ",", "kling\u00b7zum", ",", "ping", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "VVFIN", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn mich vor den b\u00f6sen Urwelt-G\u00f6ttern graute,", "tokens": ["Wenn", "mich", "vor", "den", "b\u00f6\u00b7sen", "Ur\u00b7welt\u00b7G\u00f6t\u00b7tern", "grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Vor den flammendgro\u00dfen Keulentaten,", "tokens": ["Vor", "den", "flam\u00b7mend\u00b7gro\u00b7\u00dfen", "Keu\u00b7len\u00b7ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Vor den Sternfall-Feuersaaten,", "tokens": ["Vor", "den", "Stern\u00b7fall\u00b7Feu\u00b7er\u00b7saa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die du monoton mir singst \u2013", "tokens": ["Die", "du", "mo\u00b7no\u00b7ton", "mir", "singst", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kling-zum, kling-zum, pings \u2013", "tokens": ["Kling\u00b7zum", ",", "kling\u00b7zum", ",", "pings", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ADV", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "O' Tehura \u2013", "tokens": ["O'", "Te\u00b7hu\u00b7ra", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Zuckendsummend rollen Wogen zu mir auf \u2013", "tokens": ["Zu\u00b7ckend\u00b7sum\u00b7mend", "rol\u00b7len", "Wo\u00b7gen", "zu", "mir", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "PPER", "APPR", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sind das nicht die bl\u00fctenblauen Glitzerwellen?", "tokens": ["Sind", "das", "nicht", "die", "bl\u00fc\u00b7ten\u00b7blau\u00b7en", "Glit\u00b7zer\u00b7wel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Zuckendsummend brandet L\u00e4rm-Gerauf,", "tokens": ["Zu\u00b7ckend\u00b7sum\u00b7mend", "bran\u00b7det", "L\u00e4r\u00b7mGe\u00b7rauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schwere, tr\u00fcbe T\u00f6nemasse", "tokens": ["Schwe\u00b7re", ",", "tr\u00fc\u00b7be", "T\u00f6\u00b7ne\u00b7mas\u00b7se"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der dunklen Gasse. \u2013 \u2013", "tokens": ["Aus", "der", "dunk\u00b7len", "Gas\u00b7se", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "O' Tehura \u2013", "tokens": ["O'", "Te\u00b7hu\u00b7ra", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Es brodelt schon der Menschen Arbeits-Sorgen:", "tokens": ["Es", "bro\u00b7delt", "schon", "der", "Men\u00b7schen", "Ar\u00b7beits\u00b7Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es dr\u00f6hnt und sch\u00fcttert durch die Stra\u00dfentiefen,", "tokens": ["Es", "dr\u00f6hnt", "und", "sch\u00fct\u00b7tert", "durch", "die", "Stra\u00b7\u00dfen\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gierig warten meine Europ\u00e4ersorgen,", "tokens": ["Gie\u00b7rig", "war\u00b7ten", "mei\u00b7ne", "Eu\u00b7ro\u00b7p\u00e4\u00b7er\u00b7sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Die nur n\u00e4chtlich kurz verschliefen \u2013 \u2013", "tokens": ["Die", "nur", "n\u00e4cht\u00b7lich", "kurz", "ver\u00b7schlie\u00b7fen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVPP", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O' Tehura \u2013", "tokens": ["O'", "Te\u00b7hu\u00b7ra", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Manchmal wieder wenn die Stille singt,", "tokens": ["Manch\u00b7mal", "wie\u00b7der", "wenn", "die", "Stil\u00b7le", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn der Abend von den Kirchent\u00fcrmen klingt,", "tokens": ["Wenn", "der", "A\u00b7bend", "von", "den", "Kir\u00b7chen\u00b7t\u00fcr\u00b7men", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Denk ich irgendwo nach Pete, Honga-Sura \u2013", "tokens": ["Denk", "ich", "ir\u00b7gend\u00b7wo", "nach", "Pe\u00b7te", ",", "Hon\u00b7ga\u00b7Su\u00b7ra", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NE", "$,", "NE", "$("], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Denke ich an dich Korallenketten-Kind,", "tokens": ["Den\u00b7ke", "ich", "an", "dich", "Ko\u00b7ral\u00b7len\u00b7ket\u00b7ten\u00b7Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Bronzebraunes S\u00fcdsee-Kind,", "tokens": ["Bron\u00b7ze\u00b7brau\u00b7nes", "S\u00fcd\u00b7see\u00b7Kind", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tehura.", "tokens": ["Te\u00b7hu\u00b7ra", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}