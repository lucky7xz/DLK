{"textgrid.poem.47061": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du mir, Glocke, zutr\u00e4gst deine Kl\u00e4nge,", "tokens": ["Die", "du", "mir", ",", "Glo\u00b7cke", ",", "zu\u00b7tr\u00e4gst", "dei\u00b7ne", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Warum denn hast du in des Sommers Schimmer", "tokens": ["Wa\u00b7rum", "denn", "hast", "du", "in", "des", "Som\u00b7mers", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei mir dich hier vernehmen lassen nimmer", "tokens": ["Bei", "mir", "dich", "hier", "ver\u00b7neh\u00b7men", "las\u00b7sen", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "ADV", "VVINF", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und thust im Winter jetzt so weite G\u00e4nge?", "tokens": ["Und", "thust", "im", "Win\u00b7ter", "jetzt", "so", "wei\u00b7te", "G\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbim Sommer war vom Turme, wo ich h\u00e4nge,", "tokens": ["\u00bb", "im", "Som\u00b7mer", "war", "vom", "Tur\u00b7me", ",", "wo", "ich", "h\u00e4n\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis hieher, wo du wohnst im stillen Zimmer,", "tokens": ["Bis", "hie\u00b7her", ",", "wo", "du", "wohnst", "im", "stil\u00b7len", "Zim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "$,", "PWAV", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Gass' und Stra\u00dfen solch ein Leben immer,", "tokens": ["Aus", "Gass'", "und", "Stra\u00b7\u00dfen", "solch", "ein", "Le\u00b7ben", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich nicht kommen konnte durchs Gedr\u00e4nge.", "tokens": ["Da\u00df", "ich", "nicht", "kom\u00b7men", "konn\u00b7te", "durchs", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Blumen und Gr\u00e4ser waren lauter Ohren,", "tokens": ["Blu\u00b7men", "und", "Gr\u00e4\u00b7ser", "wa\u00b7ren", "lau\u00b7ter", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "An Strauch und B\u00e4umen lauschten alle Sprossen,", "tokens": ["An", "Strauch", "und", "B\u00e4u\u00b7men", "lauschten", "al\u00b7le", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und alle Felsen horchten auch, die schroffen.", "tokens": ["Und", "al\u00b7le", "Fel\u00b7sen", "horch\u00b7ten", "auch", ",", "die", "schrof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da ging mein Reden unterwegs verloren;", "tokens": ["Da", "ging", "mein", "Re\u00b7den", "un\u00b7ter\u00b7wegs", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt sind die Ohren drau\u00dfen all' geschlossen,", "tokens": ["Jetzt", "sind", "die", "Oh\u00b7ren", "drau\u00b7\u00dfen", "all'", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur deins hier steht der Lieb' auch ewig offen.\u00ab", "tokens": ["Nur", "deins", "hier", "steht", "der", "Lieb'", "auch", "e\u00b7wig", "of\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}