{"textgrid.poem.48629": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "42. Auf M. Johan Albrecht von Mandelsloh, F\u00fcrstl. Holstein. Gesandten Stallmeistern, seinen Geburtstag, in der Moskaw 1636", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgen gehn wir an die Reise,", "tokens": ["Mor\u00b7gen", "gehn", "wir", "an", "die", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gestern war kein Sonnenschein.", "tokens": ["ge\u00b7stern", "war", "kein", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Heute la\u00dft uns unser sein,", "tokens": ["Heu\u00b7te", "la\u00dft", "uns", "un\u00b7ser", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "edler ", "tokens": ["ed\u00b7ler"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "dir zu Preise, weil dein Licht", "tokens": ["dir", "zu", "Prei\u00b7se", ",", "weil", "dein", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "dir und uns viel Lust verspricht!", "tokens": ["dir", "und", "uns", "viel", "Lust", "ver\u00b7spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schau, wir kommen ungeladen!", "tokens": ["Schau", ",", "wir", "kom\u00b7men", "un\u00b7ge\u00b7la\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasse keinen Mangel sein!", "tokens": ["Las\u00b7se", "kei\u00b7nen", "Man\u00b7gel", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb uns Wein und Zucker drein,", "tokens": ["Gieb", "uns", "Wein", "und", "Zu\u00b7cker", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "aber tu dir keinen Schaden!", "tokens": ["a\u00b7ber", "tu", "dir", "kei\u00b7nen", "Scha\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es ist nicht so arg gemeint,", "tokens": ["Es", "ist", "nicht", "so", "arg", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "als es anfangs mit uns scheint.", "tokens": ["als", "es", "an\u00b7fangs", "mit", "uns", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bringt ein Freund ein freundlichs Herze,", "tokens": ["Bringt", "ein", "Freund", "ein", "freund\u00b7lichs", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "o so nimt er wol vor gut,", "tokens": ["o", "so", "nimt", "er", "wol", "vor", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "s\u00e4ttigt Magen und den Mut", "tokens": ["s\u00e4t\u00b7tigt", "Ma\u00b7gen", "und", "den", "Mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mit der Kost, doch mehr mit Scherze.", "tokens": ["mit", "der", "Kost", ",", "doch", "mehr", "mit", "Scher\u00b7ze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sein Vergn\u00fcgen ist allein", "tokens": ["Sein", "Ver\u00b7gn\u00fc\u00b7gen", "ist", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "k\u00f6nnen recht vergn\u00fcget sein.", "tokens": ["k\u00f6n\u00b7nen", "recht", "ver\u00b7gn\u00fc\u00b7get", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kanstu uns hier nicht ergetzen", "tokens": ["Kans\u00b7tu", "uns", "hier", "nicht", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mit des Frauenzimmers Gunst", "tokens": ["mit", "des", "Frau\u00b7en\u00b7zim\u00b7mers", "Gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "oder Trachten nach der Kunst", "tokens": ["o\u00b7der", "Trach\u00b7ten", "nach", "der", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "auf die Tafel lassen setzen,", "tokens": ["auf", "die", "Ta\u00b7fel", "las\u00b7sen", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "nichts versehn! Es ist genung,", "tokens": ["nichts", "ver\u00b7sehn", "!", "Es", "ist", "ge\u00b7nung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$.", "PPER", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "schaff' uns einen guten Trunk!", "tokens": ["schaff'", "uns", "ei\u00b7nen", "gu\u00b7ten", "Trunk", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wilst du uns denn mehr bewirten,", "tokens": ["Wilst", "du", "uns", "denn", "mehr", "be\u00b7wir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die Musik ist unverwehrt.", "tokens": ["die", "Mu\u00b7sik", "ist", "un\u00b7ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ists, die den Himmel mehrt.", "tokens": ["Die", "ists", ",", "die", "den", "Him\u00b7mel", "mehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie macht G\u00f6tter auch aus Hirten.", "tokens": ["Sie", "macht", "G\u00f6t\u00b7ter", "auch", "aus", "Hir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Kein Gelag kan treflich sein,", "tokens": ["Kein", "Ge\u00b7lag", "kan", "tref\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wo nicht Saiten sind und Wein.", "tokens": ["wo", "nicht", "Sai\u00b7ten", "sind", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NN", "VAFIN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn so lachet sichs von Herzen,", "tokens": ["Denn", "so", "la\u00b7chet", "sichs", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn der hellen Trompten Klang,", "tokens": ["wenn", "der", "hel\u00b7len", "Tromp\u00b7ten", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "scharfe Saiten und Gesang", "tokens": ["schar\u00b7fe", "Sai\u00b7ten", "und", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ernstlich durch einander scherzen,", "tokens": ["ernst\u00b7lich", "durch", "ein\u00b7an\u00b7der", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und inzwischen, weils so geht,", "tokens": ["und", "in\u00b7zwi\u00b7schen", ",", "weils", "so", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "keine Schale m\u00fc\u00dfig steht.", "tokens": ["kei\u00b7ne", "Scha\u00b7le", "m\u00fc\u00b7\u00dfig", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Zweie sind es, so auf Erden", "tokens": ["Zwei\u00b7e", "sind", "es", ",", "so", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schon dem Himmel \u00e4hnlich sein:", "tokens": ["schon", "dem", "Him\u00b7mel", "\u00e4hn\u00b7lich", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die Musik und edler Wein.", "tokens": ["die", "Mu\u00b7sik", "und", "ed\u00b7ler", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch sie kan man himmlisch werden.", "tokens": ["Durch", "sie", "kan", "man", "himm\u00b7lisch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so lebt der G\u00f6tter Schaar", "tokens": ["Und", "so", "lebt", "der", "G\u00f6t\u00b7ter", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "durch das liebe lange Jahr.", "tokens": ["durch", "das", "lie\u00b7be", "lan\u00b7ge", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie, sie sinds, des Alters Meister,", "tokens": ["Sie", ",", "sie", "sinds", ",", "des", "Al\u00b7ters", "Meis\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VAFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tod der Angst, der Sorgen Ha\u00df.", "tokens": ["Tod", "der", "Angst", ",", "der", "Sor\u00b7gen", "Ha\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Edle Saiten, edles Glas,", "tokens": ["Ed\u00b7le", "Sai\u00b7ten", ",", "ed\u00b7les", "Glas", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ihr erquicket unsre Geister!", "tokens": ["ihr", "er\u00b7quic\u00b7ket", "uns\u00b7re", "Geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmermehr kan elend sein,", "tokens": ["Nim\u00b7mer\u00b7mehr", "kan", "e\u00b7lend", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wer frisch singt und frisch schenkt ein.", "tokens": ["wer", "frisch", "singt", "und", "frisch", "schenkt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber mich beginnt zu d\u00fcrsten.", "tokens": ["A\u00b7ber", "mich", "be\u00b7ginnt", "zu", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Junger, schenke Span'schen ein!", "tokens": ["Jun\u00b7ger", ",", "schen\u00b7ke", "Span'\u00b7schen", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Edler, das soll deine sein!", "tokens": ["Ed\u00b7ler", ",", "das", "soll", "dei\u00b7ne", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VMFIN", "PPOSAT", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf Gesundheit unsres F\u00fcrsten!", "tokens": ["Auf", "Ge\u00b7sund\u00b7heit", "uns\u00b7res", "F\u00fcrs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Di\u00df soll heute fort so gehn,", "tokens": ["Di\u00df", "soll", "heu\u00b7te", "fort", "so", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PTKVZ", "ADV", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "bis wir mehr nicht k\u00f6nnen stehn.", "tokens": ["bis", "wir", "mehr", "nicht", "k\u00f6n\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}