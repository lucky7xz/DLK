{"textgrid.poem.37363": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6ne Sommer ging von hinnen,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Som\u00b7mer", "ging", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herbst, der reiche, zog ins Land.", "tokens": ["Der", "Herbst", ",", "der", "rei\u00b7che", ",", "zog", "ins", "Land", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun weben all die guten Spinnen", "tokens": ["Nun", "we\u00b7ben", "all", "die", "gu\u00b7ten", "Spin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So manches feine Festgewand.", "tokens": ["So", "man\u00b7ches", "fei\u00b7ne", "Fest\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie weben zu des Tages Feier", "tokens": ["Sie", "we\u00b7ben", "zu", "des", "Ta\u00b7ges", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit kunstge\u00fcbtem Hinterbein", "tokens": ["Mit", "kunst\u00b7ge\u00b7\u00fcb\u00b7tem", "Hin\u00b7ter\u00b7bein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz allerliebste Elfenschleier", "tokens": ["Ganz", "al\u00b7ler\u00b7liebs\u00b7te", "El\u00b7fen\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Schmuck f\u00fcr Wiese, Flur und Hain.", "tokens": ["Als", "Schmuck", "f\u00fcr", "Wie\u00b7se", ",", "Flur", "und", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, tausend Silberf\u00e4den geben", "tokens": ["Ja", ",", "tau\u00b7send", "Sil\u00b7ber\u00b7f\u00e4\u00b7den", "ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Winde sie zum leichten Spiel,", "tokens": ["Dem", "Win\u00b7de", "sie", "zum", "leich\u00b7ten", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ziehen sanft dahin und schweben", "tokens": ["Die", "zie\u00b7hen", "sanft", "da\u00b7hin", "und", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "PAV", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ans unbewu\u00dft bestimmte Ziel.", "tokens": ["Ans", "un\u00b7be\u00b7wu\u00dft", "be\u00b7stimm\u00b7te", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie ziehen in das Wunderl\u00e4ndchen,", "tokens": ["Sie", "zie\u00b7hen", "in", "das", "Wun\u00b7der\u00b7l\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Liebe scheu im Anbeginn,", "tokens": ["Wo", "Lie\u00b7be", "scheu", "im", "An\u00b7be\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leis verkn\u00fcpft ein zartes B\u00e4ndchen", "tokens": ["Und", "leis", "ver\u00b7kn\u00fcpft", "ein", "zar\u00b7tes", "B\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sch\u00e4fer mit der Sch\u00e4ferin.", "tokens": ["Den", "Sch\u00e4\u00b7fer", "mit", "der", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}