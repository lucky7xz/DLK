{"textgrid.poem.54014": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Zeitungsdeutsch und Briefstil", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geheim! Tagebuch-Nr. 69/218.", "tokens": ["Ge\u00b7heim", "!", "Ta\u00b7ge\u00b7buch", "Nr.", "69", "/", "218."], "token_info": ["word", "punct", "word", "abbreviation", "number", "punct", "ordinal"], "pos": ["NN", "$.", "TRUNC", "NN", "CARD", "$(", "ADJA"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "1. Meine Neigung zu Dir ist unver\u00e4ndert.", "tokens": ["Mei\u00b7ne", "Nei\u00b7gung", "zu", "Dir", "ist", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "2. Du stehst heute abend, ", "tokens": ["Du", "stehst", "heu\u00b7te", "a\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "4. Abendessen im Gambrinus, 8.10 Uhr.", "tokens": ["A\u00b7ben\u00b7des\u00b7sen", "im", "Gam\u00b7bri\u00b7nus", ",", "8.10", "Uhr", "."], "token_info": ["word", "word", "word", "punct", "number", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "CARD", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "5. Es wird nachher in meiner Wohnung voraussichtlich zu Z\u00e4rtlichkeiten kommen.", "tokens": ["Es", "wird", "nach\u00b7her", "in", "mei\u00b7ner", "Woh\u00b7nung", "vor\u00b7aus\u00b7sicht\u00b7lich", "zu", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bban einer Seite Prosa wie an einer Bilds\u00e4ule arbeiten . . . \u00ab schrieb Nietzsche. So siehst du aus.", "tokens": ["\u00bb", "an", "ei\u00b7ner", "Sei\u00b7te", "Pro\u00b7sa", "wie", "an", "ei\u00b7ner", "Bild\u00b7s\u00e4u\u00b7le", "ar\u00b7bei\u00b7ten", ".", ".", ".", "\u00ab", "schrieb", "Nietz\u00b7sche", ".", "So", "siehst", "du", "aus", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "NN", "KOKOM", "APPR", "ART", "NN", "VVINF", "$.", "$.", "$.", "$(", "VVFIN", "PIS", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+---+--+--+-+", "measure": "iambic.octa.plus.relaxed"}}}}}