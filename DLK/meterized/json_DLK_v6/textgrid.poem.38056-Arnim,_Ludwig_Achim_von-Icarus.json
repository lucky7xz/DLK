{"textgrid.poem.38056": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Icarus", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umt, ich fl\u00f6g gar bange", "tokens": ["Mir", "tr\u00e4umt", ",", "ich", "fl\u00f6g", "gar", "ban\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in die Welt hinaus,", "tokens": ["Wohl", "in", "die", "Welt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Stra\u00dfburg durch alle Gassen", "tokens": ["Zu", "Stra\u00df\u00b7burg", "durch", "al\u00b7le", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis vor Feinsliebchens Haus.", "tokens": ["Bis", "vor", "Feins\u00b7lieb\u00b7chens", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Feinsliebchen ist betr\u00fcbt,", "tokens": ["Feins\u00b7lieb\u00b7chen", "ist", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als ich so flieg und rennt:", "tokens": ["Als", "ich", "so", "flieg", "und", "rennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer dich so fliegen lehrt,", "tokens": ["Wer", "dich", "so", "flie\u00b7gen", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das ist der b\u00f6se Feind.", "tokens": ["Das", "ist", "der", "b\u00f6\u00b7se", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Feinsliebchen, was hilft hier l\u00fcgen,", "tokens": ["Feins\u00b7lieb\u00b7chen", ",", "was", "hilft", "hier", "l\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da du doch alles weist,", "tokens": ["Da", "du", "doch", "al\u00b7les", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer mich so fliegen lehrt,", "tokens": ["Wer", "mich", "so", "flie\u00b7gen", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das ist der b\u00f6se Geist.", "tokens": ["Das", "ist", "der", "b\u00f6\u00b7se", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Feinsliebchen weint und schreiet,", "tokens": ["Feins\u00b7lieb\u00b7chen", "weint", "und", "schrei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich vom Schrey erwacht,", "tokens": ["Da\u00df", "ich", "vom", "Schrey", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sa\u00df ich ach! in Augsburg", "tokens": ["Da", "sa\u00df", "ich", "ach", "!", "in", "Augs\u00b7burg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gefangen auf der Wacht.", "tokens": ["Ge\u00b7fan\u00b7gen", "auf", "der", "Wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und morgen mu\u00df ich hangen,", "tokens": ["Und", "mor\u00b7gen", "mu\u00df", "ich", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Feinslieb mich nicht mehr ruft,", "tokens": ["Feins\u00b7lieb", "mich", "nicht", "mehr", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl morgen als ein Vogel", "tokens": ["Wohl", "mor\u00b7gen", "als", "ein", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwank ich in freyer Luft.", "tokens": ["Schwank", "ich", "in", "frey\u00b7er", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}