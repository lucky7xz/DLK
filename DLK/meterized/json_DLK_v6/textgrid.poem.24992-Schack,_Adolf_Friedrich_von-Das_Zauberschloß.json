{"textgrid.poem.24992": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Zauberschlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein Schlo\u00df, das hoch auf Klippen ragt;", "tokens": ["Ich", "wei\u00df", "ein", "Schlo\u00df", ",", "das", "hoch", "auf", "Klip\u00b7pen", "ragt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Adlern ist sein Zinnendach umflogen", "tokens": ["Von", "Ad\u00b7lern", "ist", "sein", "Zin\u00b7nen\u00b7dach", "um\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wirft den Morgenglanz, lang, eh es tagt,", "tokens": ["Und", "wirft", "den", "Mor\u00b7gen\u00b7glanz", ",", "lang", ",", "eh", "es", "tagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schon weithin auf die blauen Meereswogen;", "tokens": ["Schon", "weit\u00b7hin", "auf", "die", "blau\u00b7en", "Mee\u00b7res\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Traum hab' ich, o meines Herzens Braut,", "tokens": ["Im", "Traum", "hab'", "ich", ",", "o", "mei\u00b7nes", "Her\u00b7zens", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "$,", "FM", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Uns beiden diesen Wonnesitz erbaut.", "tokens": ["Uns", "bei\u00b7den", "die\u00b7sen", "Won\u00b7ne\u00b7sitz", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort in den G\u00e4rten schweifen wir umher", "tokens": ["Dort", "in", "den", "G\u00e4r\u00b7ten", "schwei\u00b7fen", "wir", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sehen von den h\u00e4ngenden Terrassen", "tokens": ["Und", "se\u00b7hen", "von", "den", "h\u00e4n\u00b7gen\u00b7den", "Ter\u00b7ras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu F\u00fc\u00dfen uns den Himmel und das Meer", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "uns", "den", "Him\u00b7mel", "und", "das", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Liebesschauern bald, gleich uns, erblassen,", "tokens": ["In", "Lie\u00b7bes\u00b7schau\u00b7ern", "bald", ",", "gleich", "uns", ",", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "ADV", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bald so wie wir, wenn Mund am Munde ruht,", "tokens": ["Bald", "so", "wie", "wir", ",", "wenn", "Mund", "am", "Mun\u00b7de", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "PPER", "$,", "KOUS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hoch aufgl\u00fchn in des Abends Purpurglut.", "tokens": ["Hoch", "auf\u00b7gl\u00fchn", "in", "des", "A\u00b7bends", "Pur\u00b7pur\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und o! die sonn'gen Halden an der Kluft,", "tokens": ["Und", "o", "!", "die", "sonn'\u00b7gen", "Hal\u00b7den", "an", "der", "Kluft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "$.", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Grotten, die zu sel'gem Schlummer laden,", "tokens": ["Die", "Grot\u00b7ten", ",", "die", "zu", "sel'\u00b7gem", "Schlum\u00b7mer", "la\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indessen meerhauchfeuchter Myrtenduft", "tokens": ["In\u00b7des\u00b7sen", "meer\u00b7hauch\u00b7feuch\u00b7ter", "Myr\u00b7ten\u00b7duft"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Emporwallt von den hallenden Gestaden,", "tokens": ["Em\u00b7por\u00b7wallt", "von", "den", "hal\u00b7len\u00b7den", "Ge\u00b7sta\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und durch die Brandung, die am Felsen dr\u00f6hnt,", "tokens": ["Und", "durch", "die", "Bran\u00b7dung", ",", "die", "am", "Fel\u00b7sen", "dr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Wonnestammeln unsrer Herzen t\u00f6nt.", "tokens": ["Das", "Won\u00b7nes\u00b7tam\u00b7meln", "uns\u00b7rer", "Her\u00b7zen", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}