{"textgrid.poem.41477": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diesem Wald, in diesen Gr\u00fcnden", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem Wald, in diesen Gr\u00fcnden", "tokens": ["In", "die\u00b7sem", "Wald", ",", "in", "die\u00b7sen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herrscht nichts, als Freiheit, Lust und Ruh.", "tokens": ["Herrscht", "nichts", ",", "als", "Frei\u00b7heit", ",", "Lust", "und", "Ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier sagen wir der Liebe zu,", "tokens": ["Hier", "sa\u00b7gen", "wir", "der", "Lie\u00b7be", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im dicksten Schatten uns zu finden:", "tokens": ["Im", "dicks\u00b7ten", "Schat\u00b7ten", "uns", "zu", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da find' ich dich, mich findest du.", "tokens": ["Da", "find'", "ich", "dich", ",", "mich", "fin\u00b7dest", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier paaren sich Natur und Liebe,", "tokens": ["Hier", "paa\u00b7ren", "sich", "Na\u00b7tur", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Jugend und die Fr\u00f6hlichkeit,", "tokens": ["Die", "Ju\u00b7gend", "und", "die", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lust und die Gelegenheit:", "tokens": ["Die", "Lust", "und", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht Gelegenheit ja Diebe;", "tokens": ["Und", "macht", "Ge\u00b7le\u00b7gen\u00b7heit", "ja", "Die\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wird der Raub der Lust geweiht.", "tokens": ["So", "wird", "der", "Raub", "der", "Lust", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die V\u00f6gel lieben hier und singen.", "tokens": ["Die", "V\u00f6\u00b7gel", "lie\u00b7ben", "hier", "und", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es liebt der in den L\u00fcften schwebt;", "tokens": ["Es", "liebt", "der", "in", "den", "L\u00fcf\u00b7ten", "schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es liebt was kaum der Fittich hebt", "tokens": ["Es", "liebt", "was", "kaum", "der", "Fit\u00b7tich", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und suchet aus dem Nest zu dringen:", "tokens": ["Und", "su\u00b7chet", "aus", "dem", "Nest", "zu", "drin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil alles nach der Freiheit strebt.", "tokens": ["Weil", "al\u00b7les", "nach", "der", "Frei\u00b7heit", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Nachtigall in diesen Str\u00e4uchen", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "in", "die\u00b7sen", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleicht durch die s\u00fc\u00dfe Stimme dir;", "tokens": ["Gleicht", "durch", "die", "s\u00fc\u00b7\u00dfe", "Stim\u00b7me", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ihrer Scherzlust gleicht sie mir:", "tokens": ["In", "ih\u00b7rer", "Scherz\u00b7lust", "gleicht", "sie", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sucht, uns beiden mehr zu gleichen,", "tokens": ["Und", "sucht", ",", "uns", "bei\u00b7den", "mehr", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "PIAT", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die sichern Schatten, so wie wir.", "tokens": ["Die", "si\u00b7chern", "Schat\u00b7ten", ",", "so", "wie", "wir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "KOKOM", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Die Lerche steiget in die H\u00f6he.", "tokens": ["Die", "Ler\u00b7che", "stei\u00b7get", "in", "die", "H\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr buhlerischer Lustgesang", "tokens": ["Ihr", "buh\u00b7le\u00b7ri\u00b7scher", "Lust\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verehrt und lobet lebenslang", "tokens": ["Ver\u00b7ehrt", "und", "lo\u00b7bet", "le\u00b7bens\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die freie Liebe, nicht die Ehe;", "tokens": ["Die", "frei\u00b7e", "Lie\u00b7be", ",", "nicht", "die", "E\u00b7he", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die stete Wahl, und keinen Zwang.", "tokens": ["Die", "ste\u00b7te", "Wahl", ",", "und", "kei\u00b7nen", "Zwang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie scherzt und h\u00fcpfet durch die Felder", "tokens": ["Wie", "scherzt", "und", "h\u00fcp\u00b7fet", "durch", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die oft gepaarte Wachtelbrut!", "tokens": ["Die", "oft", "ge\u00b7paar\u00b7te", "Wach\u00b7tel\u00b7brut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die frohen Schl\u00e4ge, die sie thut,", "tokens": ["Die", "fro\u00b7hen", "Schl\u00e4\u00b7ge", ",", "die", "sie", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erschallen in die nahen W\u00e4lder", "tokens": ["Er\u00b7schal\u00b7len", "in", "die", "na\u00b7hen", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und t\u00f6nen nur von Lust und Muth.", "tokens": ["Und", "t\u00f6\u00b7nen", "nur", "von", "Lust", "und", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie buhlen dort die Turteltauben:", "tokens": ["Wie", "buh\u00b7len", "dort", "die", "Tur\u00b7tel\u00b7tau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer kann ihr Girren nicht verstehn?", "tokens": ["Wer", "kann", "ihr", "Gir\u00b7ren", "nicht", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe macht es doppelt sch\u00f6n,", "tokens": ["Die", "Lie\u00b7be", "macht", "es", "dop\u00b7pelt", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will und soll uns auch erlauben,", "tokens": ["Und", "will", "und", "soll", "uns", "auch", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Schn\u00e4beln ihnen abzusehn.", "tokens": ["Das", "Schn\u00e4\u00b7beln", "ih\u00b7nen", "ab\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Sperling theilt sein kurzes Leben", "tokens": ["Der", "Sper\u00b7ling", "theilt", "sein", "kur\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Zwitschern und in Lieben ein.", "tokens": ["In", "Zwit\u00b7schern", "und", "in", "Lie\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man wei\u00df, er liebet ungemein:", "tokens": ["Man", "wei\u00df", ",", "er", "lie\u00b7bet", "un\u00b7ge\u00b7mein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will man sein Singen nicht erheben,", "tokens": ["Will", "man", "sein", "Sin\u00b7gen", "nicht", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wird er wol zu tr\u00f6sten sein.", "tokens": ["So", "wird", "er", "wol", "zu", "tr\u00f6s\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Noch eh' wir uns von hier entfernen,", "tokens": ["Noch", "eh'", "wir", "uns", "von", "hier", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm jetzt nebst mir doch den Entschlu\u00df,", "tokens": ["Nimm", "jetzt", "nebst", "mir", "doch", "den", "Ent\u00b7schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Bei jedem Scherz, bei jedem Ku\u00df", "tokens": ["Bei", "je\u00b7dem", "Scherz", ",", "bei", "je\u00b7dem", "Ku\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den V\u00f6geln etwas abzulernen,", "tokens": ["Den", "V\u00f6\u00b7geln", "et\u00b7was", "ab\u00b7zu\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das dir und mir gefallen mu\u00df.", "tokens": ["Das", "dir", "und", "mir", "ge\u00b7fal\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}