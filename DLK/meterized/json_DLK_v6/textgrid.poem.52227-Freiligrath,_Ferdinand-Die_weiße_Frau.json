: "iambic.tetra"}, "line.2": {"text": "La\u00df ab \u2013 es fleht, es hebt die H\u00e4nde!", "tokens": ["La\u00df", "ab", "\u2013", "es", "fleht", ",", "es", "hebt", "die", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df ab \u2013 da\u00df neuer Kindermord", "tokens": ["La\u00df", "ab", "\u2013", "da\u00df", "neu\u00b7er", "Kin\u00b7der\u00b7mord"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$(", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Hauses alten Ruhm nicht sch\u00e4nde!", "tokens": ["Des", "Hau\u00b7ses", "al\u00b7ten", "Ruhm", "nicht", "sch\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O glaub': entsetzlich ist ein Fluch!", "tokens": ["O", "glaub'", ":", "ent\u00b7setz\u00b7lich", "ist", "ein", "Fluch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Er lastet auf der Brust wie Berge!", "tokens": ["Er", "las\u00b7tet", "auf", "der", "Brust", "wie", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er sengt wie Wetterstrahl! \u2013 Genug!", "tokens": ["Er", "sengt", "wie", "Wet\u00b7ter\u00b7strahl", "!", "\u2013", "Ge\u00b7nug", "!"], "token_info": ["word", "word",