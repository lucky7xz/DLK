{"textgrid.poem.52227": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die wei\u00dfe Frau", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, es l\u00e4\u00dft die wei\u00dfe Frau", "tokens": ["Man", "sagt", ",", "es", "l\u00e4\u00dft", "die", "wei\u00b7\u00dfe", "Frau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich hier und dorten wieder sehen;", "tokens": ["Sich", "hier", "und", "dor\u00b7ten", "wie\u00b7der", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch mehr als einen F\u00fcrstenbau", "tokens": ["Durch", "mehr", "als", "ei\u00b7nen", "F\u00fcrs\u00b7ten\u00b7bau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit fahlem Antlitz soll sie gehen.", "tokens": ["Mit", "fah\u00b7lem", "Ant\u00b7litz", "soll", "sie", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In wei\u00dfer Robe, wei\u00df verbr\u00e4mt,", "tokens": ["In", "wei\u00b7\u00dfer", "Ro\u00b7be", ",", "wei\u00df", "ver\u00b7br\u00e4mt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tritt sie aus W\u00e4nden und aus Bildern;", "tokens": ["Tritt", "sie", "aus", "W\u00e4n\u00b7den", "und", "aus", "Bil\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dastehn die Wachen wie gel\u00e4hmt,", "tokens": ["Das\u00b7tehn", "die", "Wa\u00b7chen", "wie", "ge\u00b7l\u00e4hmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die in den Korridoren schildern.", "tokens": ["Die", "in", "den", "Kor\u00b7ri\u00b7do\u00b7ren", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wem gilt ihr abermalig Nahn", "tokens": ["Wem", "gilt", "ihr", "a\u00b7ber\u00b7ma\u00b7lig", "Nahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rings in den Reichen und Provinzen?", "tokens": ["Rings", "in", "den", "Rei\u00b7chen", "und", "Pro\u00b7vin\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sagt sie, wie sonst, ein Sterben an?", "tokens": ["Sagt", "sie", ",", "wie", "sonst", ",", "ein", "Ster\u00b7ben", "an", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tod eines F\u00fcrsten oder Prinzen?", "tokens": ["Tod", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "o\u00b7der", "Prin\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Es k\u00f6nnte sein \u2013 ich wei\u00df es nicht!", "tokens": ["Es", "k\u00f6nn\u00b7te", "sein", "\u2013", "ich", "wei\u00df", "es", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Rede geht: ein tiefrer Jammer", "tokens": ["Die", "Re\u00b7de", "geht", ":", "ein", "tief\u00b7rer", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Treibt sie hervor ans Tageslicht", "tokens": ["Treibt", "sie", "her\u00b7vor", "ans", "Ta\u00b7ges\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihrer dunst'gen Totenkammer!", "tokens": ["Aus", "ih\u00b7rer", "dunst'\u00b7gen", "To\u00b7ten\u00b7kam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie schwebt durch Schlafgemach und Saal,", "tokens": ["Sie", "schwebt", "durch", "Schlaf\u00b7ge\u00b7mach", "und", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie beugt sich \u00fcber goldne Wiegen,", "tokens": ["Sie", "beugt", "sich", "\u00fc\u00b7ber", "gold\u00b7ne", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sieht den Herrn und sein Gemahl", "tokens": ["Sie", "sieht", "den", "Herrn", "und", "sein", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seidnen Pf\u00fchlen schlummernd liegen.", "tokens": ["Auf", "seid\u00b7nen", "Pf\u00fch\u00b7len", "schlum\u00b7mernd", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie haucht ihn an: \u00bbWas schlummerst du?", "tokens": ["Sie", "haucht", "ihn", "an", ":", "\u00bb", "Was", "schlum\u00b7merst", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O, da\u00df du s\u00e4hest meinen Kummer!", "tokens": ["O", ",", "da\u00df", "du", "s\u00e4\u00b7hest", "mei\u00b7nen", "Kum\u00b7mer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Ohren taub, die Augen zu \u2013", "tokens": ["Die", "Oh\u00b7ren", "taub", ",", "die", "Au\u00b7gen", "zu", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach, ewig find' ich dich im Schlummer!", "tokens": ["Ach", ",", "e\u00b7wig", "find'", "ich", "dich", "im", "Schlum\u00b7mer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf, mein Geschlecht! \u2013 H\u00f6r', wie weithin", "tokens": ["Auf", ",", "mein", "Ge\u00b7schlecht", "!", "\u2013", "H\u00f6r'", ",", "wie", "weit\u00b7hin"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$.", "$(", "NE", "$,", "PWAV", "ADV"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Ein Schrei gellt, den du selbst beschworen!", "tokens": ["Ein", "Schrei", "gellt", ",", "den", "du", "selbst", "be\u00b7schwo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch meiner S\u00e4rge doppelt Zinn", "tokens": ["Durch", "mei\u00b7ner", "S\u00e4r\u00b7ge", "dop\u00b7pelt", "Zinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt' ich ihn spitz mein Herz durchbohren!", "tokens": ["F\u00fchlt'", "ich", "ihn", "spitz", "mein", "Herz", "durch\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist der Schrei, den um sein Recht", "tokens": ["Es", "ist", "der", "Schrei", ",", "den", "um", "sein", "Recht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Volk erhebt \u2013 annoch in Treuen!", "tokens": ["Das", "Volk", "er\u00b7hebt", "\u2013", "an\u00b7noch", "in", "Treu\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du schl\u00e4fst sehr fest, o mein Geschlecht,", "tokens": ["Du", "schl\u00e4fst", "sehr", "fest", ",", "o", "mein", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu \u00fcberh\u00f6ren solch ein Schreien!", "tokens": ["Zu", "\u00fc\u00b7ber\u00b7h\u00f6\u00b7ren", "solch", "ein", "Schrei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Toten weckt es in der Gruft \u2013", "tokens": ["Die", "To\u00b7ten", "weckt", "es", "in", "der", "Gruft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Gott, und die Lebend'gen schlafen!", "tokens": ["Herr", "Gott", ",", "und", "die", "Le\u00b7ben\u00b7d'\u00b7gen", "schla\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Absch\u00fcttl' ich Staub und Moderduft:", "tokens": ["Ab\u00b7sch\u00fcttl'", "ich", "Staub", "und", "Mo\u00b7der\u00b7duft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ich m\u00f6chte wecken, warnen, strafen!", "tokens": ["Ich", "m\u00f6ch\u00b7te", "we\u00b7cken", ",", "war\u00b7nen", ",", "stra\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab' nicht Rast, ich hab' nicht Ruh' \u2013", "tokens": ["Ich", "hab'", "nicht", "Rast", ",", "ich", "hab'", "nicht", "Ruh'", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eil', o mein Stamm, dich zu erheben!", "tokens": ["Eil'", ",", "o", "mein", "Stamm", ",", "dich", "zu", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Mund des Todes ruft dir zu:", "tokens": ["Der", "Mund", "des", "To\u00b7des", "ruft", "dir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erfasse frisch und k\u00fchn das Leben!", "tokens": ["Er\u00b7fas\u00b7se", "frisch", "und", "k\u00fchn", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du t\u00e4test besser, in der Tat,", "tokens": ["Du", "t\u00e4\u00b7test", "bes\u00b7ser", ",", "in", "der", "Tat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frei das Panier ihm zu entfalten,", "tokens": ["Frei", "das", "Pa\u00b7nier", "ihm", "zu", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Als am verwitterten Brokat", "tokens": ["Als", "am", "ver\u00b7wit\u00b7ter\u00b7ten", "Bro\u00b7kat"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von meiner Bahre dich zu halten!", "tokens": ["Von", "mei\u00b7ner", "Bah\u00b7re", "dich", "zu", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, la\u00df ihn fahren, eh' dich's reut!", "tokens": ["O", ",", "la\u00df", "ihn", "fah\u00b7ren", ",", "eh'", "dich's", "reut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Blick' aus nach St\u00fctzen, j\u00fcngern, festern!", "tokens": ["Blick'", "aus", "nach", "St\u00fct\u00b7zen", ",", "j\u00fcn\u00b7gern", ",", "fes\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "$,", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mehr w\u00e4rmt ein Bauernwams von heut,", "tokens": ["Mehr", "w\u00e4rmt", "ein", "Bau\u00b7ern\u00b7wams", "von", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als Hermelin und Samt von gestern!", "tokens": ["Als", "Her\u00b7me\u00b7lin", "und", "Samt", "von", "ge\u00b7stern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "O, schrecklich war, was ich beging", "tokens": ["O", ",", "schreck\u00b7lich", "war", ",", "was", "ich", "be\u00b7ging"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "VAFIN", "$,", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meinem Schlo\u00df zu Orlam\u00fcnde!", "tokens": ["Auf", "mei\u00b7nem", "Schlo\u00df", "zu", "Or\u00b7la\u00b7m\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich als Schatten geh' und ging,", "tokens": ["Da\u00df", "ich", "als", "Schat\u00b7ten", "geh'", "und", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist ja nur f\u00fcr jene S\u00fcnde!", "tokens": ["Es", "ist", "ja", "nur", "f\u00fcr", "je\u00b7ne", "S\u00fcn\u00b7de", "!"], "to