{"textgrid.poem.29377": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werden auch wol meine Reimen alle f\u00fcr die Jungfern t\u00fcgen?", "tokens": ["Wer\u00b7den", "auch", "wol", "mei\u00b7ne", "Rei\u00b7men", "al\u00b7le", "f\u00fcr", "die", "Jung\u00b7fern", "t\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die, als Jungfern, mehr verstehen, die wird ihr Gewissen r\u00fcgen,", "tokens": ["Die", ",", "als", "Jung\u00b7fern", ",", "mehr", "ver\u00b7ste\u00b7hen", ",", "die", "wird", "ihr", "Ge\u00b7wis\u00b7sen", "r\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "NN", "$,", "ADV", "VVINF", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Da\u00df sie schweigen vom Verstande; die die Unschuld alber machet,", "tokens": ["Da\u00df", "sie", "schwei\u00b7gen", "vom", "Ver\u00b7stan\u00b7de", ";", "die", "die", "Un\u00b7schuld", "al\u00b7ber", "ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$.", "ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Denen haben meine Reimen schwerlich arges urgesachet.", "tokens": ["De\u00b7nen", "ha\u00b7ben", "mei\u00b7ne", "Rei\u00b7men", "schwer\u00b7lich", "ar\u00b7ges", "ur\u00b7ge\u00b7sa\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}