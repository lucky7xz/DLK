{"dta.poem.11519": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.71", "no:0.28"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Tage Regen fort und fort,", "tokens": ["Drei", "Ta\u00b7ge", "Re\u00b7gen", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Sonnenschein zur Stunde,", "tokens": ["Kein", "Son\u00b7nen\u00b7schein", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drei Tage lang kein gutes Wort", "tokens": ["Drei", "Ta\u00b7ge", "lang", "kein", "gu\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meiner Liebsten Munde!", "tokens": ["Aus", "mei\u00b7ner", "Liebs\u00b7ten", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie truzt mit mir und ich mit ihr,", "tokens": ["Sie", "truzt", "mit", "mir", "und", "ich", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hat sie's haben wollen;", "tokens": ["So", "hat", "sie's", "ha\u00b7ben", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir aber nagt's am Herzen hier,", "tokens": ["Mir", "a\u00b7ber", "nagt's", "am", "Her\u00b7zen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schmollen und das Grollen.", "tokens": ["Das", "Schmol\u00b7len", "und", "das", "Grol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Willkommen denn, des J\u00e4gers Lust,", "tokens": ["Will\u00b7kom\u00b7men", "denn", ",", "des", "J\u00e4\u00b7gers", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewittersturm und Regen!", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7sturm", "und", "Re\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fest zugekn\u00f6pft die hei\u00dfe Brust,", "tokens": ["Fest", "zu\u00b7ge\u00b7kn\u00f6pft", "die", "hei\u00b7\u00dfe", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und jauchzend euch entgegen!", "tokens": ["Und", "jauch\u00b7zend", "euch", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun sizt sie wohl daheim und lacht,", "tokens": ["Nun", "sizt", "sie", "wohl", "da\u00b7heim", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und scherzt mit den Geschwistern;", "tokens": ["Und", "scherzt", "mit", "den", "Ge\u00b7schwis\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6re in des Waldes Nacht", "tokens": ["Ich", "h\u00f6\u00b7re", "in", "des", "Wal\u00b7des", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alten Bl\u00e4tter fl\u00fcstern.", "tokens": ["Die", "al\u00b7ten", "Bl\u00e4t\u00b7ter", "fl\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun sizt sie wohl und weinet laut", "tokens": ["Nun", "sizt", "sie", "wohl", "und", "wei\u00b7net", "laut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im K\u00e4mmerlein, in Sorgen;", "tokens": ["Im", "K\u00e4m\u00b7mer\u00b7lein", ",", "in", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist es wie dem Wilde traut,", "tokens": ["Mir", "ist", "es", "wie", "dem", "Wil\u00b7de", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Finsterni\u00df geborgen.", "tokens": ["In", "Fins\u00b7ter\u00b7ni\u00df", "ge\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Kein Hirsch und Rehlein \u00fcberall!", "tokens": ["Kein", "Hirsch", "und", "Reh\u00b7lein", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schu\u00df zum Zeitvertreibe!", "tokens": ["Ein", "Schu\u00df", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesunder Knall und Widerhall", "tokens": ["Ge\u00b7sun\u00b7der", "Knall", "und", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erfrischt das Mark im Leibe. \u2014", "tokens": ["Er\u00b7frischt", "das", "Mark", "im", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch wie der Donner nun verhallt", "tokens": ["Doch", "wie", "der", "Don\u00b7ner", "nun", "ver\u00b7hallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Th\u00e4lern in die Runde,", "tokens": ["In", "Th\u00e4\u00b7lern", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein pl\u00f6tzlich Weh mich \u00fcberwallt,", "tokens": ["Ein", "pl\u00f6tz\u00b7lich", "Weh", "mich", "\u00fc\u00b7berw\u00b7allt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir sinkt das Herz zu Grunde.", "tokens": ["Mir", "sinkt", "das", "Herz", "zu", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie truzt mit mir und ich mit ihr,", "tokens": ["Sie", "truzt", "mit", "mir", "und", "ich", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hat sie's haben wollen,", "tokens": ["So", "hat", "sie's", "ha\u00b7ben", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir aber fri\u00dft's am Herzen hier,", "tokens": ["Mir", "a\u00b7ber", "fri\u00dft's", "am", "Her\u00b7zen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schmollen und das Grollen.", "tokens": ["Das", "Schmol\u00b7len", "und", "das", "Grol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und auf! und nach der Liebsten Haus!", "tokens": ["Und", "auf", "!", "und", "nach", "der", "Liebs\u00b7ten", "Haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sie gefa\u00dft um's Mieder!", "tokens": ["Und", "sie", "ge\u00b7fa\u00dft", "um's", "Mie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201edr\u00fcck' mir die nassen Locken aus,", "tokens": ["\u201e", "dr\u00fcck", "mir", "die", "nas\u00b7sen", "Lo\u00b7cken", "aus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und k\u00fcss' und hab' mich wieder!\u201c", "tokens": ["Und", "k\u00fcss'", "und", "hab'", "mich", "wie\u00b7der", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "KON", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}