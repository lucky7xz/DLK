{"textgrid.poem.40844": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Golf hinaus, fort von Pompeji's K\u00fcsten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Golf hinaus, fort von Pompeji's K\u00fcsten", "tokens": ["Den", "Golf", "hin\u00b7aus", ",", "fort", "von", "Pom\u00b7pe\u00b7ji's", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APZR", "$,", "PTKVZ", "APPR", "NE", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wogt eines Schiffes majest\u00e4t'scher Bau;", "tokens": ["Wogt", "ei\u00b7nes", "Schif\u00b7fes", "ma\u00b7je\u00b7st\u00e4t'\u00b7scher", "Bau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Segel, die vom Abendwind gek\u00fc\u00dften,", "tokens": ["Die", "Se\u00b7gel", ",", "die", "vom", "Ab\u00b7end\u00b7wind", "ge\u00b7k\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bl\u00e4hn lustig sich, es knarrt in Mast und Tau!", "tokens": ["Bl\u00e4hn", "lus\u00b7tig", "sich", ",", "es", "knarrt", "in", "Mast", "und", "Tau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und, horch! Kanonendonner lauthin knallen \u2013", "tokens": ["Und", ",", "horch", "!", "Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", "laut\u00b7hin", "knal\u00b7len", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVIMP", "$.", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Abschiedsgru\u00df, o Cincinnatus, klingt,", "tokens": ["Dein", "Ab\u00b7schieds\u00b7gru\u00df", ",", "o", "Cin\u00b7cin\u00b7na\u00b7tus", ",", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "FM", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df, aufgeschreckt, die Schaar der Nachtigallen", "tokens": ["Da\u00df", ",", "auf\u00b7ge\u00b7schreckt", ",", "die", "Schaar", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "VVPP", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Maro's Grab sich, \u00e4ngstlich flatternd, schwingt!", "tokens": ["Von", "Ma\u00b7ro's", "Grab", "sich", ",", "\u00e4ngst\u00b7lich", "flat\u00b7ternd", ",", "schwingt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "NN", "PRF", "$,", "ADJD", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie rauh, o Mensch, ist selbst dein Gru\u00df der Liebe:", "tokens": ["Wie", "rauh", ",", "o", "Mensch", ",", "ist", "selbst", "dein", "Gru\u00df", "der", "Lie\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "FM", "NN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pre\u00dft deine Hand des Freundes Hand in sich,", "tokens": ["Pre\u00dft", "dei\u00b7ne", "Hand", "des", "Freun\u00b7des", "Hand", "in", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Scheint's fast, als ob es dich zu sprechen triebe:", "tokens": ["Scheint's", "fast", ",", "als", "ob", "es", "dich", "zu", "spre\u00b7chen", "trie\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOKOM", "KOUS", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Freund, f\u00fchle meine Kraft, und wahre dich!", "tokens": ["Freund", ",", "f\u00fch\u00b7le", "mei\u00b7ne", "Kraft", ",", "und", "wah\u00b7re", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Sohn Amerika's, gekreuzt die H\u00e4nde,", "tokens": ["Der", "Sohn", "A\u00b7me\u00b7ri\u00b7ka's", ",", "ge\u00b7kreuzt", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lehnt still am Hauptmast an des Schiffes Bord,", "tokens": ["Lehnt", "still", "am", "Haupt\u00b7mast", "an", "des", "Schif\u00b7fes", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Aug' durchschweift im Flug des Golfs Gel\u00e4nde,", "tokens": ["Sein", "Aug'", "durch\u00b7schweift", "im", "Flug", "des", "Golfs", "Ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Winkt hier ein Lebewohl, nickt Gr\u00fc\u00dfe dort:", "tokens": ["Winkt", "hier", "ein", "Le\u00b7be\u00b7wohl", ",", "nickt", "Gr\u00fc\u00b7\u00dfe", "dort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbleb' wohl, Europa! Da\u00df dein Aug' sich helle,", "tokens": ["\u00bb", "leb'", "wohl", ",", "Eu\u00b7ro\u00b7pa", "!", "Da\u00df", "dein", "Aug'", "sich", "hel\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "NE", "$.", "KOUS", "PPOSAT", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Niobe, versch\u00f6nt vom Riesenschmerz!", "tokens": ["Du", "Ni\u00b7o\u00b7be", ",", "ver\u00b7sch\u00f6nt", "vom", "Rie\u00b7sen\u00b7schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich ihrer ist auch deiner Leiden Quelle", "tokens": ["Gleich", "ih\u00b7rer", "ist", "auch", "dei\u00b7ner", "Lei\u00b7den", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "VAFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Reichthum, den du liebend dr\u00fcckst ans Herz!", "tokens": ["Dein", "Reicht\u00b7hum", ",", "den", "du", "lie\u00b7bend", "dr\u00fcckst", "ans", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Gegr\u00fc\u00dft, Amerika, du j\u00fcngre Schwester!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", ",", "A\u00b7me\u00b7ri\u00b7ka", ",", "du", "j\u00fcng\u00b7re", "Schwes\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O nimm des Schmerzes Kinder mild von ihr,", "tokens": ["O", "nimm", "des", "Schmer\u00b7zes", "Kin\u00b7der", "mild", "von", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Leg' an dein Herz sie, da\u00df der Schmerzen gr\u00f6\u00dfter", "tokens": ["Leg'", "an", "dein", "Herz", "sie", ",", "da\u00df", "der", "Schmer\u00b7zen", "gr\u00f6\u00df\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "$,", "KOUS", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seiner F\u00fclle Heilung trink' aus dir!", "tokens": ["In", "sei\u00b7ner", "F\u00fcl\u00b7le", "Hei\u00b7lung", "trink'", "aus", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Schlingt Hand in Hand, la\u00dft Haupt am Haupte lehnen,", "tokens": ["Schlingt", "Hand", "in", "Hand", ",", "la\u00dft", "Haupt", "am", "Haup\u00b7te", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,", "VVIMP", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Schwestern, euch zu F\u00fc\u00dfen Meeresglanz!", "tokens": ["Ihr", "Schwes\u00b7tern", ",", "euch", "zu", "F\u00fc\u00b7\u00dfen", "Mee\u00b7res\u00b7glanz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es stehn die Kronen, die Europa kr\u00f6nen,", "tokens": ["Es", "stehn", "die", "Kro\u00b7nen", ",", "die", "Eu\u00b7ro\u00b7pa", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gut an Amerika's laubgr\u00fcnem Kranz!", "tokens": ["Gut", "an", "A\u00b7me\u00b7ri\u00b7ka's", "laub\u00b7gr\u00fc\u00b7nem", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Wie bunt und herrlich rauscht dein Wald, o Leben!", "tokens": ["Wie", "bunt", "und", "herr\u00b7lich", "rauscht", "dein", "Wald", ",", "o", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sieh, doch ist's nur Eine Lebenskraft,", "tokens": ["Und", "sieh", ",", "doch", "ist's", "nur", "Ei\u00b7ne", "Le\u00b7bens\u00b7kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die graue Moose hei\u00dft am Boden kleben", "tokens": ["Die", "grau\u00b7e", "Moo\u00b7se", "hei\u00dft", "am", "Bo\u00b7den", "kle\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Palm' und Ceder in die Wolken rafft,", "tokens": ["Und", "Palm'", "und", "Ce\u00b7der", "in", "die", "Wol\u00b7ken", "rafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die bl\u00fch'nden Lotos wiegt im Wellenschaume,", "tokens": ["Die", "bl\u00fch'n\u00b7den", "Lo\u00b7tos", "wiegt", "im", "Wel\u00b7len\u00b7schau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Rosen Purpurkleider taucht in Duft,", "tokens": ["Der", "Ro\u00b7sen", "Pur\u00b7pur\u00b7klei\u00b7der", "taucht", "in", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Reben lehrt den Flug von Baum zu Baume,", "tokens": ["Die", "Re\u00b7ben", "lehrt", "den", "Flug", "von", "Baum", "zu", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Kaktus keilt in starre Felsenkluft!", "tokens": ["Den", "Kak\u00b7tus", "keilt", "in", "star\u00b7re", "Fel\u00b7sen\u00b7kluft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie reich, o Menschengeist, dein Garten gl\u00fchte,", "tokens": ["Wie", "reich", ",", "o", "Men\u00b7schen\u00b7geist", ",", "dein", "Gar\u00b7ten", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Eine Kraft ist's, die zum Keim dich dr\u00e4ngt,", "tokens": ["Nur", "Ei\u00b7ne", "Kraft", "ist's", ",", "die", "zum", "Keim", "dich", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Krone, Lyra, Hirtenstab als Bl\u00fcthe,", "tokens": ["Und", "Kro\u00b7ne", ",", "Ly\u00b7ra", ",", "Hir\u00b7ten\u00b7stab", "als", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NE", "$,", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 Ach, auch das Schwert! \u2013 an deinen Baum geh\u00e4ngt!", "tokens": ["\u2013", "Ach", ",", "auch", "das", "Schwert", "!", "\u2013", "an", "dei\u00b7nen", "Baum", "ge\u00b7h\u00e4ngt", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "ART", "NN", "$.", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und diese Bl\u00fcthen sind zum Kranze worden,", "tokens": ["Und", "die\u00b7se", "Bl\u00fc\u00b7then", "sind", "zum", "Kran\u00b7ze", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der bunt sich um der Zeiten Harfe schlingt,", "tokens": ["Der", "bunt", "sich", "um", "der", "Zei\u00b7ten", "Har\u00b7fe", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die bebend in den ewigen Accorden", "tokens": ["Die", "be\u00b7bend", "in", "den", "e\u00b7wi\u00b7gen", "Ac\u00b7cor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Menschheit Schmerz, der Menschheit Jubel klingt!", "tokens": ["Der", "Menschheit", "Schmerz", ",", "der", "Menschheit", "Ju\u00b7bel", "klingt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Der alte Baum sieht, ewig gr\u00fcnend, nieder", "tokens": ["Der", "al\u00b7te", "Baum", "sieht", ",", "e\u00b7wig", "gr\u00fc\u00b7nend", ",", "nie\u