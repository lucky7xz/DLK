{"textgrid.poem.34406": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[voll erbl\u00fcht die sch\u00fcchterne Rosenknospe]", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll erbl\u00fcht die sch\u00fcchterne Rosenknospe", "tokens": ["Voll", "er\u00b7bl\u00fcht", "die", "sch\u00fcch\u00b7ter\u00b7ne", "Ro\u00b7sen\u00b7knos\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "in dem flurdurchw\u00e4rmenden Strahl der Sonne,", "tokens": ["in", "dem", "flur\u00b7durch\u00b7w\u00e4r\u00b7men\u00b7den", "Strahl", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und bewundernd schaut ihr der Mensch ins offne,", "tokens": ["und", "be\u00b7wun\u00b7dernd", "schaut", "ihr", "der", "Mensch", "ins", "off\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "gl\u00fchende Antlitz.", "tokens": ["gl\u00fc\u00b7hen\u00b7de", "Ant\u00b7litz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch der Nacht erst will sie den letzten Reichthum,", "tokens": ["Doch", "der", "Nacht", "erst", "will", "sie", "den", "letz\u00b7ten", "Reicht\u00b7hum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ihrer D\u00fcfte s\u00fcsseren Strom sie spenden \u2013", "tokens": ["ih\u00b7rer", "D\u00fcf\u00b7te", "s\u00fcs\u00b7se\u00b7ren", "Strom", "sie", "spen\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "O versteh mein Schweigen! \u2013 Im Schwarm der Menge", "tokens": ["O", "ver\u00b7steh", "mein", "Schwei\u00b7gen", "!", "\u2013", "Im", "Schwarm", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$.", "$(", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "lass mich verstummen!", "tokens": ["lass", "mich", "ver\u00b7stum\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}