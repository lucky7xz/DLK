{"textgrid.poem.58534": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "572. Bei Verlobten kommt man zu sp\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh, eitle Welt,", "tokens": ["Geh", ",", "eit\u00b7le", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Lock, lob und schelt,", "tokens": ["Lock", ",", "lob", "und", "schelt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du kommst zu sp\u00e4t, ich tu nicht einen Blick", "tokens": ["Du", "kommst", "zu", "sp\u00e4t", ",", "ich", "tu", "nicht", "ei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf deine Phantasie zur\u00fcck,", "tokens": ["Auf", "dei\u00b7ne", "Phan\u00b7ta\u00b7sie", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe schon mein bl\u00fchend junges Leben", "tokens": ["Ich", "ha\u00b7be", "schon", "mein", "bl\u00fc\u00b7hend", "jun\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem sch\u00f6nsten Freunde \u00fcbergeben!", "tokens": ["Dem", "sch\u00f6ns\u00b7ten", "Freun\u00b7de", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er hat mein Herz erkauft, gesuchet und gefunden", "tokens": ["Er", "hat", "mein", "Herz", "er\u00b7kauft", ",", "ge\u00b7su\u00b7chet", "und", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und an sein Herz und Joch gebunden,", "tokens": ["Und", "an", "sein", "Herz", "und", "Joch", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ihn darf ich nennen", "tokens": ["Ihn", "darf", "ich", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und frei bekennen,", "tokens": ["Und", "frei", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Er hei\u00dfet Jesus Christ,", "tokens": ["Er", "hei\u00b7\u00dfet", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Der ewiglich mein Schatz, mein Heil, mein Alles ist.", "tokens": ["Der", "e\u00b7wig\u00b7lich", "mein", "Schatz", ",", "mein", "Heil", ",", "mein", "Al\u00b7les", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}