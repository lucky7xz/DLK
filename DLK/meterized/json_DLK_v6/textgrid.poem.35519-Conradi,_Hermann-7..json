{"textgrid.poem.35519": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht um mich leis und geheimnisschwer", "tokens": ["Es", "rauscht", "um", "mich", "leis", "und", "ge\u00b7heim\u00b7nis\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJD", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Mitternacht phantastisch T\u00f6nemeer ...", "tokens": ["Der", "Mit\u00b7ter\u00b7nacht", "phan\u00b7tas\u00b7tisch", "T\u00f6\u00b7ne\u00b7meer", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie Nebelschatten, wie ein Geisterreigen,", "tokens": ["Wie", "Ne\u00b7bel\u00b7schat\u00b7ten", ",", "wie", "ein", "Geis\u00b7ter\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entsteigt es flutend dem geschw\u00e4tz'gen Schweigen ...", "tokens": ["Ent\u00b7steigt", "es", "flu\u00b7tend", "dem", "ge\u00b7schw\u00e4tz'\u00b7gen", "Schwei\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Wehr, die ich durch Markt und Gassen trug \u2013", "tokens": ["Die", "Wehr", ",", "die", "ich", "durch", "Markt", "und", "Gas\u00b7sen", "trug", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur eine Zahl im gro\u00dfen Kriegerzug,", "tokens": ["Nur", "ei\u00b7ne", "Zahl", "im", "gro\u00b7\u00dfen", "Krie\u00b7ger\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der lebenstrotzig ringt um karge Spenden", "tokens": ["Der", "le\u00b7bens\u00b7trot\u00b7zig", "ringt", "um", "kar\u00b7ge", "Spen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit derben F\u00e4usten, hagern Bettlerlenden;", "tokens": ["Mit", "der\u00b7ben", "F\u00e4us\u00b7ten", ",", "ha\u00b7gern", "Bett\u00b7ler\u00b7len\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gesichtern bleich, hohlwangig, schmerzzerwittert,", "tokens": ["Ge\u00b7sich\u00b7tern", "bleich", ",", "hohl\u00b7wan\u00b7gig", ",", "schmerz\u00b7zer\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von der Verg\u00e4ngnis Pestgestank umzittert;", "tokens": ["Von", "der", "Ver\u00b7g\u00e4ng\u00b7nis", "Pest\u00b7ge\u00b7stank", "um\u00b7zit\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Gepackt von der Verzweiflung Geierklauen,", "tokens": ["Ge\u00b7packt", "von", "der", "Ver\u00b7zwei\u00b7flung", "Gei\u00b7er\u00b7klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gepeitscht von d\u00fcrrer Armut Gei\u00dfeltauen \u2013", "tokens": ["Ge\u00b7peitscht", "von", "d\u00fcr\u00b7rer", "Ar\u00b7mut", "Gei\u00b7\u00dfel\u00b7tau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die ich in diesem Sturme trug, die Wehr:", "tokens": ["Die", "ich", "in", "die\u00b7sem", "Stur\u00b7me", "trug", ",", "die", "Wehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach solch ein Kerl! Ein Kampflegion\u00e4r!", "tokens": ["Ach", "solch", "ein", "Kerl", "!", "Ein", "Kampf\u00b7le\u00b7gi\u00b7o\u00b7n\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Auch solch ein Held! Ein armer Proletar!", "tokens": ["Auch", "solch", "ein", "Held", "!", "Ein", "ar\u00b7mer", "Pro\u00b7le\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "D\u00fcsteren Auges ... mit wirr strupp'gem Haar,", "tokens": ["D\u00fcs\u00b7te\u00b7ren", "Au\u00b7ges", "...", "mit", "wirr", "strupp'\u00b7gem", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Zerdachter Stirne, schw\u00e4renschwerem Leib,", "tokens": ["Zer\u00b7dach\u00b7ter", "Stir\u00b7ne", ",", "schw\u00e4\u00b7ren\u00b7schwe\u00b7rem", "Leib", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geh\u00fcllt in Fetzen, zunderd\u00fcnn Gest\u00e4ub \u2013", "tokens": ["Ge\u00b7h\u00fcllt", "in", "Fet\u00b7zen", ",", "zun\u00b7der\u00b7d\u00fcnn", "Ge\u00b7st\u00e4ub", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADV", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Auch solch ein R\u00fcttler, Zweifler, Schrankenbrecher,", "tokens": ["Auch", "solch", "ein", "R\u00fctt\u00b7ler", ",", "Zweif\u00b7ler", ",", "Schran\u00b7ken\u00b7bre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Meuterer, Rebell und \u00bb", "tokens": ["Ein", "Meu\u00b7te\u00b7rer", ",", "Re\u00b7bell", "und", "\u00bb"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "$("], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Und nochmals einer, der, was sakrosankt", "tokens": ["Und", "noch\u00b7mals", "ei\u00b7ner", ",", "der", ",", "was", "sak\u00b7ro\u00b7sankt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PIS", "$,", "PRELS", "$,", "PWS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In tiefstem Marke ", "tokens": ["In", "tiefs\u00b7tem", "Mar\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Was reich verb\u00fcrgt, best\u00e4tigt und verbrieft", "tokens": ["Was", "reich", "ver\u00b7b\u00fcrgt", ",", "be\u00b7st\u00e4\u00b7tigt", "und", "ver\u00b7brieft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VVPP", "$,", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Zerbricht \u2013 von einem ", "tokens": ["Zer\u00b7bricht", "\u2013", "von", "ei\u00b7nem"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nicht zum Verrat mit rotem Gold erkauft \u2013", "tokens": ["Nicht", "zum", "Ver\u00b7rat", "mit", "ro\u00b7tem", "Gold", "er\u00b7kauft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Solch ein Gesell in hartem Tagesringen,", "tokens": ["Solch", "ein", "Ge\u00b7sell", "in", "har\u00b7tem", "Ta\u00b7ges\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Kampf mit Wahnpropheten, Finsterlingen:", "tokens": ["Im", "Kampf", "mit", "Wahn\u00b7pro\u00b7phe\u00b7ten", ",", "Fins\u00b7ter\u00b7lin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Leg' ich die Wehr von mir um Mitternacht,", "tokens": ["Leg'", "ich", "die", "Wehr", "von", "mir", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn es um mich wie Geisterruf erwacht ...", "tokens": ["Wenn", "es", "um", "mich", "wie", "Geis\u00b7ter\u00b7ruf", "er\u00b7wacht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Wenn es zu mir in die Mansarde tritt:", "tokens": ["Wenn", "es", "zu", "mir", "in", "die", "Man\u00b7sar\u00b7de", "tritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das hohe Weib mit traumhaft leisem Schritt ...", "tokens": ["Das", "ho\u00b7he", "Weib", "mit", "traum\u00b7haft", "lei\u00b7sem", "Schritt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Schwer flie\u00dft sein Haar in goldnen Wellen nieder,", "tokens": ["Schwer", "flie\u00dft", "sein", "Haar", "in", "gold\u00b7nen", "Wel\u00b7len", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf seinen Lippen liegen ew'ge Lieder ...", "tokens": ["Auf", "sei\u00b7nen", "Lip\u00b7pen", "lie\u00b7gen", "ew'\u00b7ge", "Lie\u00b7der", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Von seiner Stirne flammen Gottgedanken \u2013", "tokens": ["Von", "sei\u00b7ner", "Stir\u00b7ne", "flam\u00b7men", "Gott\u00b7ge\u00b7dan\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welt\u00fcberwindend, sprengend alle Schranken ...", "tokens": ["Welt\u00b7\u00fc\u00b7berw\u00b7in\u00b7dend", ",", "spren\u00b7gend", "al\u00b7le", "Schran\u00b7ken", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "PIAT", "NN", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.19": {"line.1": {"text": "Und mit der hoch erhobnen Rechten weist", "tokens": ["Und", "mit", "der", "hoch", "er\u00b7hob\u00b7nen", "Rech\u00b7ten", "weist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es in die Zukunft \u2013 und es jauchzt mein Geist! ...", "tokens": ["Es", "in", "die", "Zu\u00b7kunft", "\u2013", "und", "es", "jauchzt", "mein", "Geist", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$(", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.20": {"line.1": {"text": "Denn folgend diesem Zeugnis seiner Gnade", "tokens": ["Denn", "fol\u00b7gend", "die\u00b7sem", "Zeug\u00b7nis", "sei\u00b7ner", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PDAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Enth\u00fcllt mein Auge neue Zukunftspfade,", "tokens": ["Ent\u00b7h\u00fcllt", "mein", "Au\u00b7ge", "neu\u00b7e", "Zu\u00b7kunfts\u00b7pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "Die ich gesucht tag\u00fcber, doch nicht fand,", "tokens": ["Die", "ich", "ge\u00b7sucht", "ta\u00b7g\u00fc\u00b7ber", ",", "doch", "nicht", "fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "NN", "$,", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dieweil die Lippe sog nur d\u00fcnnen Sand ...", "tokens": ["Die\u00b7weil", "die", "Lip\u00b7pe", "sog", "nur", "d\u00fcn\u00b7nen", "Sand", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.22": {"line.1": {"text": "Von der Erkenntnis hellem Glanz ums\u00e4umt,", "tokens": ["Von", "der", "Er\u00b7kennt\u00b7nis", "hel\u00b7lem", "Glanz", "um\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Liegt's deutlich vor mir, was ich nur getr\u00e4umt ...", "tokens": ["Liegt's", "deut\u00b7lich", "vor", "mir", ",", "was", "ich", "nur", "ge\u00b7tr\u00e4umt", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PPER", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.23": {"line.1": {"text": "Die Nebelfetzen in die Tiefen sanken,", "tokens": ["Die", "Ne\u00b7bel\u00b7fet\u00b7zen", "in", "die", "Tie\u00b7fen", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerweht von meines Geistes Lichtgedanken! ...", "tokens": ["Zer\u00b7weht", "von", "mei\u00b7nes", "Geis\u00b7tes", "Licht\u00b7ge\u00b7dan\u00b7ken", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.24": {"line.1": {"text": "Sei mir gesegnet, dreimal heilig Weib \u2013", "tokens": ["Sei", "mir", "ge\u00b7seg\u00b7net", ",", "drei\u00b7mal", "hei\u00b7lig", "Weib", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geh\u00f6rt dem Tage auch mein Sklavenleib \u2013", "tokens": ["Ge\u00b7h\u00f6rt", "dem", "Ta\u00b7ge", "auch", "mein", "Skla\u00b7ven\u00b7leib", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.25": {"line.1": {"text": "Mein Leib, von enger Waffenwehr umschn\u00fcrt:", "tokens": ["Mein", "Leib", ",", "von", "en\u00b7ger", "Waf\u00b7fen\u00b7wehr", "um\u00b7schn\u00fcrt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure"