{"textgrid.poem.36226": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine Freundin in der Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft seh' ich vor mir deine blauen Augen", "tokens": ["Oft", "seh'", "ich", "vor", "mir", "dei\u00b7ne", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und t\u00e4usche mich, vergessend da\u00df du ferne.", "tokens": ["Und", "t\u00e4u\u00b7sche", "mich", ",", "ver\u00b7ges\u00b7send", "da\u00df", "du", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVPP", "KOUS", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6chte Huld aus deinen Blicken saugen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "Huld", "aus", "dei\u00b7nen", "Bli\u00b7cken", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versinke tr\u00e4umend in die dunkeln Sterne,", "tokens": ["Ver\u00b7sin\u00b7ke", "tr\u00e4u\u00b7mend", "in", "die", "dun\u00b7keln", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und acht' es nicht, da\u00df andre wenig taugen,", "tokens": ["Und", "acht'", "es", "nicht", ",", "da\u00df", "and\u00b7re", "we\u00b7nig", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Froh, wenn ich dein Gem\u00fct vernehmen lerne;", "tokens": ["Froh", ",", "wenn", "ich", "dein", "Ge\u00b7m\u00fct", "ver\u00b7neh\u00b7men", "ler\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Seh' ich dann um den Mund dein L\u00e4cheln schweben", "tokens": ["Seh'", "ich", "dann", "um", "den", "Mund", "dein", "L\u00e4\u00b7cheln", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So w\u00fcnsch' ich heiter neben dir zu leben.", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", "hei\u00b7ter", "ne\u00b7ben", "dir", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}