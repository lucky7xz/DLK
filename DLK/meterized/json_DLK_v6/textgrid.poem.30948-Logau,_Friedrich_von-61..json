{"textgrid.poem.30948": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Reime r\u00fcchen nicht", "tokens": ["Mei\u00b7ne", "Rei\u00b7me", "r\u00fc\u00b7chen", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch nach \u00d6le, noch nach Wein;", "tokens": ["Noch", "nach", "\u00d6\u00b7le", ",", "noch", "nach", "Wein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Beydes kan gar schwerlich seyn,", "tokens": ["Bey\u00b7des", "kan", "gar", "schwer\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenes wegen Amtes Pflicht,", "tokens": ["Je\u00b7nes", "we\u00b7gen", "Am\u00b7tes", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses wegen schlimmer Gicht.", "tokens": ["Die\u00b7ses", "we\u00b7gen", "schlim\u00b7mer", "Gicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}