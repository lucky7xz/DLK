{"dta.poem.13542": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "An Burgen, die vom Felsen einsam grollen,", "tokens": ["An", "Bur\u00b7gen", ",", "die", "vom", "Fel\u00b7sen", "ein\u00b7sam", "grol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "APPRART", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Waldesdunkel zwischen Rebenh\u00fcgeln", "tokens": ["Aus", "Wal\u00b7des\u00b7dun\u00b7kel", "zwi\u00b7schen", "Re\u00b7ben\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor\u00fcbergleitend in die duft'ge Ferne,", "tokens": ["Vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7tend", "in", "die", "duft'\u00b7ge", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entwandelt er zum Meer, dem wundervollen,", "tokens": ["Ent\u00b7wan\u00b7delt", "er", "zum", "Meer", ",", "dem", "wun\u00b7der\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo tr\u00e4umend sich die seel'gen Inseln spiegeln", "tokens": ["Wo", "tr\u00e4u\u00b7mend", "sich", "die", "seel'\u00b7gen", "In\u00b7seln", "spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "PRF", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und auf den Fluthen ruh'n die ew'gen Sterne.", "tokens": ["Und", "auf", "den", "Flut\u00b7hen", "ruh'n", "die", "ew'\u00b7gen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}