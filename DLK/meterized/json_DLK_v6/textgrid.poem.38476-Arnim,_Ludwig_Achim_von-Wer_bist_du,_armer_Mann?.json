{"textgrid.poem.38476": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wer bist du, armer Mann?", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel ist mein Hut,", "tokens": ["Der", "Him\u00b7mel", "ist", "mein", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde ist mein Schuh,", "tokens": ["Die", "Er\u00b7de", "ist", "mein", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das heil'ge Kreuz ist mein Schwerd,", "tokens": ["Das", "heil'\u00b7ge", "Kreuz", "ist", "mein", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wer mich sieht, hat mich lieb und werth.", "tokens": ["Wer", "mich", "sieht", ",", "hat", "mich", "lieb", "und", "werth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}