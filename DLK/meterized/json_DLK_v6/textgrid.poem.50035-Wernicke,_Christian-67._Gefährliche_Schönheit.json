{"textgrid.poem.50035": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "67. Gef\u00e4hrliche Sch\u00f6nheit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind gleich was sonderlich nachfolgende Gedancken,", "tokens": ["Sind", "gleich", "was", "son\u00b7der\u00b7lich", "nach\u00b7fol\u00b7gen\u00b7de", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch setzen sie die Sach' in ein erkenntlich Licht:", "tokens": ["Doch", "set\u00b7zen", "sie", "die", "Sach'", "in", "ein", "er\u00b7kennt\u00b7lich", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gleich sind der ", "tokens": ["Gleich", "sind", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Die dort ein ", "tokens": ["Die", "dort", "ein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Durch diese das, durch jene diss,", "tokens": ["Durch", "die\u00b7se", "das", ",", "durch", "je\u00b7ne", "diss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PDS", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was beyde ", "tokens": ["Was", "bey\u00b7de"], "token_info": ["word", "word"], "pos": ["PWS", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}}}}}