{"textgrid.poem.55549": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Freuden", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da flattert um die Quelle", "tokens": ["Da", "flat\u00b7tert", "um", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wechselnde Libelle,", "tokens": ["Die", "wech\u00b7seln\u00b7de", "Li\u00b7bel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Wasserpapillon,", "tokens": ["Der", "Was\u00b7ser\u00b7pa\u00b7pil\u00b7lon", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bald dunkel und bald helle,", "tokens": ["Bald", "dun\u00b7kel", "und", "bald", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie ein Cham\u00e4leon;", "tokens": ["Wie", "ein", "Cha\u00b7m\u00e4\u00b7le\u00b7on", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bald rot und blau, bald blau und gr\u00fcn.", "tokens": ["Bald", "rot", "und", "blau", ",", "bald", "blau", "und", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O da\u00df ich in der N\u00e4he", "tokens": ["O", "da\u00df", "ich", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Doch seine Farben s\u00e4he!", "tokens": ["Doch", "sei\u00b7ne", "Far\u00b7ben", "s\u00e4\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da fliegt der Kleine vor mir hin", "tokens": ["Da", "fliegt", "der", "Klei\u00b7ne", "vor", "mir", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und setzt sich auf die stillen Weiden.", "tokens": ["Und", "setzt", "sich", "auf", "die", "stil\u00b7len", "Wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da hab ich ihn!", "tokens": ["Da", "hab", "ich", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und nun betracht ich ihn genau,", "tokens": ["Und", "nun", "be\u00b7tracht", "ich", "ihn", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seh ein traurig dunkles Blau.", "tokens": ["Und", "seh", "ein", "trau\u00b7rig", "dunk\u00b7les", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So geht es dir, Zergliedrer deiner Freuden!", "tokens": ["So", "geht", "es", "dir", ",", "Zer\u00b7glied\u00b7rer", "dei\u00b7ner", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}