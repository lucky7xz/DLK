{"dta.poem.6843": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin die Handlungen von allen Seiten", "tokens": ["Wo\u00b7hin", "die", "Hand\u00b7lun\u00b7gen", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich f\u00fcgen und damit verbinden,", "tokens": ["Sich", "f\u00fc\u00b7gen", "und", "da\u00b7mit", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und welches alles kan zu einem leiten.", "tokens": ["Und", "wel\u00b7ches", "al\u00b7les", "kan", "zu", "ei\u00b7nem", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "VMFIN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}