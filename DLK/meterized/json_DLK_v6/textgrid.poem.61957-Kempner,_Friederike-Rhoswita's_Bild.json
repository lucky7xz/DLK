{"textgrid.poem.61957": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Rhoswita's Bild", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In stiller Klosterzelle sa\u00df", "tokens": ["In", "stil\u00b7ler", "Klos\u00b7ter\u00b7zel\u00b7le", "sa\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ernstes Frauenbild,", "tokens": ["Ein", "erns\u00b7tes", "Frau\u00b7en\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eifrig schrieb und dacht und las", "tokens": ["Und", "eif\u00b7rig", "schrieb", "und", "dacht", "und", "las"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rhoswita, sanft und mild!", "tokens": ["Rhos\u00b7wi\u00b7ta", ",", "sanft", "und", "mild", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein h\u00e4renes Gewand", "tokens": ["Ein", "h\u00e4\u00b7re\u00b7nes", "Ge\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bis an den Hals sie tr\u00e4gt,", "tokens": ["Bis", "an", "den", "Hals", "sie", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein gro\u00dfes Tuch von Pergament", "tokens": ["Ein", "gro\u00b7\u00dfes", "Tuch", "von", "Per\u00b7ga\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist vor ihr aufgelegt.", "tokens": ["Ist", "vor", "ihr", "auf\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}