{"textgrid.poem.64596": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "220. Als mich der Abb\u00e9 wegen dieses Sinngedichts zum Abendessen bat und mir viele H\u00f6flichkeit erzeigte", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Sinngedicht fand der Abb\u00e9 sehr fein,", "tokens": ["Mein", "Sinn\u00b7ge\u00b7dicht", "fand", "der", "Ab\u00b7b\u00e9", "sehr", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lud daf\u00fcr zum Abendmahl mich ein.", "tokens": ["Und", "lud", "da\u00b7f\u00fcr", "zum", "A\u00b7bend\u00b7mahl", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O w\u00fc\u00dfte **", "tokens": ["O", "w\u00fc\u00df\u00b7te", "*", "*"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "XY", "XY"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der sollte mir zwey Jahr zu essen geben.", "tokens": ["Der", "soll\u00b7te", "mir", "zwey", "Jahr", "zu", "es\u00b7sen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "CARD", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}