{"textgrid.poem.56834": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gebt mir ein Ro\u00df, und la\u00dft mich reiten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt mir ein Ro\u00df, und la\u00dft mich reiten", "tokens": ["Gebt", "mir", "ein", "Ro\u00df", ",", "und", "la\u00dft", "mich", "rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "KON", "VVIMP", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "aus diesem Meer von Staub und Stein,", "tokens": ["aus", "die\u00b7sem", "Meer", "von", "Staub", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in W\u00e4ldernacht, in Steppenweiten", "tokens": ["in", "W\u00e4l\u00b7der\u00b7nacht", ",", "in", "Step\u00b7pen\u00b7wei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "la\u00dft einsam mich und selig sein!", "tokens": ["la\u00dft", "ein\u00b7sam", "mich", "und", "se\u00b7lig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hurrah! hussah! Der Rappe fliegt ...", "tokens": ["Hur\u00b7rah", "!", "hus\u00b7sah", "!", "Der", "Rap\u00b7pe", "fliegt", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "$.", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die schwarzen Mauern fliehn zur\u00fcck ...", "tokens": ["Die", "schwar\u00b7zen", "Mau\u00b7ern", "fliehn", "zu\u00b7r\u00fcck", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor mir in stiller Ferne liegt", "tokens": ["Vor", "mir", "in", "stil\u00b7ler", "Fer\u00b7ne", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der Freiheit unaussprechlich Gl\u00fcck ...", "tokens": ["der", "Frei\u00b7heit", "un\u00b7aus\u00b7sprech\u00b7lich", "Gl\u00fcck", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor\u00fcber tausend glatten St\u00e4dten,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "tau\u00b7send", "glat\u00b7ten", "St\u00e4d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bis mich ein freies Land empf\u00e4ngt,", "tokens": ["bis", "mich", "ein", "frei\u00b7es", "Land", "emp\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wo nicht Kultur mit Sklavenketten", "tokens": ["wo", "nicht", "Kul\u00b7tur", "mit", "Skla\u00b7ven\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die k\u00fchne Mannesfaust beh\u00e4ngt!", "tokens": ["die", "k\u00fch\u00b7ne", "Man\u00b7nes\u00b7faust", "be\u00b7h\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hurrah! hussah! Zigeunerkind!", "tokens": ["Hur\u00b7rah", "!", "hus\u00b7sah", "!", "Zi\u00b7geu\u00b7ner\u00b7kind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herauf zu mir! mein Arm h\u00e4lt fest!", "tokens": ["Her\u00b7auf", "zu", "mir", "!", "mein", "Arm", "h\u00e4lt", "fest", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hin, wo die Berge pfadlos sind!", "tokens": ["Hin", ",", "wo", "die", "Ber\u00b7ge", "pfad\u00b7los", "sind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ein Horst sei unser Hochzeitsnest! ...", "tokens": ["Ein", "Horst", "sei", "un\u00b7ser", "Hoch\u00b7zeits\u00b7nest", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Und sp\u00fcrt uns die verruchte Sippe", "tokens": ["Und", "sp\u00fcrt", "uns", "die", "ver\u00b7ruch\u00b7te", "Sip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "im hohen Felsenbrautbett auf \u2013 \u2013", "tokens": ["im", "ho\u00b7hen", "Fel\u00b7sen\u00b7braut\u00b7bett", "auf", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "todwilde Jagd zur n\u00e4chsten Klippe!", "tokens": ["tod\u00b7wil\u00b7de", "Jagd", "zur", "n\u00e4chs\u00b7ten", "Klip\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die letzte Kugel aus dem Lauf!", "tokens": ["Die", "letz\u00b7te", "Ku\u00b7gel", "aus", "dem", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hurrah! hussah! Die Tiefe droht ...", "tokens": ["Hur\u00b7rah", "!", "hus\u00b7sah", "!", "Die", "Tie\u00b7fe", "droht", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "$.", "ART", "NN", "VVFIN", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Umschling mich, Weib! H\u00f6rst du sie schrein? ...", "tokens": ["Um\u00b7schling", "mich", ",", "Weib", "!", "H\u00f6rst", "du", "sie", "schrein", "?", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Viel lieber hier im Abgrund tot", "tokens": ["Viel", "lie\u00b7ber", "hier", "im", "Ab\u00b7grund", "tot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "als dort im Staub lebendig sein! ...", "tokens": ["als", "dort", "im", "Staub", "le\u00b7ben\u00b7dig", "sein", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}