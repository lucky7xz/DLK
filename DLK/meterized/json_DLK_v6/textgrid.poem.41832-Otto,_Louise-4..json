{"textgrid.poem.41832": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch k\u00fchnres Tr\u00e4umen m\u00e4chtig mich erfa\u00dfte,", "tokens": ["Doch", "k\u00fchn\u00b7res", "Tr\u00e4u\u00b7men", "m\u00e4ch\u00b7tig", "mich", "er\u00b7fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Als Opfer sterben oder k\u00e4mpfend leben,", "tokens": ["Als", "Op\u00b7fer", "ster\u00b7ben", "o\u00b7der", "k\u00e4mp\u00b7fend", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich von Not und Druck sie mit entlaste.", "tokens": ["Da\u00df", "ich", "von", "Not", "und", "Druck", "sie", "mit", "ent\u00b7las\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O wie ich z\u00fcrnend ihre Feinde ha\u00dfte!", "tokens": ["O", "wie", "ich", "z\u00fcr\u00b7nend", "ih\u00b7re", "Fein\u00b7de", "ha\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ich empor mich rang mit k\u00fchnem Streben,", "tokens": ["Wie", "ich", "em\u00b7por", "mich", "rang", "mit", "k\u00fch\u00b7nem", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie ich der Freiheit Banner wollte heben,", "tokens": ["Wie", "ich", "der", "Frei\u00b7heit", "Ban\u00b7ner", "woll\u00b7te", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob es auch f\u00fcr die schwache Hand nicht pa\u00dfte!", "tokens": ["Ob", "es", "auch", "f\u00fcr", "die", "schwa\u00b7che", "Hand", "nicht", "pa\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und mehr als das; es war ein stetig Trachten", "tokens": ["Und", "mehr", "als", "das", ";", "es", "war", "ein", "ste\u00b7tig", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "PDS", "$.", "PPER", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur vorw\u00e4rts durch die Nacht zum Licht zu wallen.", "tokens": ["Nur", "vor\u00b7w\u00e4rts", "durch", "die", "Nacht", "zum", "Licht", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gefahr und Furcht, wie lernt ich sie verachten,", "tokens": ["Ge\u00b7fahr", "und", "Furcht", ",", "wie", "lernt", "ich", "sie", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie lie\u00df ich meine Losung laut erschallen:", "tokens": ["Wie", "lie\u00df", "ich", "mei\u00b7ne", "Lo\u00b7sung", "laut", "er\u00b7schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Traum wird Wahrheit! wachet auf! \u2013 ", "tokens": ["Der", "Traum", "wird", "Wahr\u00b7heit", "!", "wa\u00b7chet", "auf", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$.", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}