s": ["Ein", "Mann", "aus", "der", "Fins\u00b7ter\u00b7nis", "rag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schaut' still in die Stube herein.", "tokens": ["Schaut'", "still", "in", "die", "Stu\u00b7be", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.20": {"line.1": {"text": "Die Haare wild umgehangen,", "tokens": ["Die", "Haa\u00b7re", "wild", "um\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von blutigen Tropfen na\u00df.", "tokens": ["Von", "blu\u00b7ti\u00b7gen", "Trop\u00b7fen", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zwei blutige Streifen sich schlangen,", "tokens": ["Zwei", "blu\u00b7ti\u00b7ge", "Strei\u00b7fen", "sich", "schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie Kr\u00e4nzlein, ums Antlitz bla\u00df.", "tokens": ["Wie", "Kr\u00e4nz\u00b7lein", ",", "ums", "Ant\u00b7litz", "bla\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "Er gr\u00fc\u00dft' sie so f\u00fcrchterlich heiter,", "tokens": ["Er", "gr\u00fc\u00dft'", "sie", "so", "f\u00fcrch\u00b7ter\u0