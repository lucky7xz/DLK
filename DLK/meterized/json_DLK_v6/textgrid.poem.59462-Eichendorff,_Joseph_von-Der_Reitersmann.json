morgens da lag so schaurig", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", "da", "lag", "so", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Verfallen im Walde das Haus,", "tokens": ["Ver\u00b7fal\u00b7len", "im", "Wal\u00b7de", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein Waldv\u00f6glein sang so traurig,", "tokens": ["Ein", "Wald\u00b7v\u00f6\u00b7glein", "sang", "so", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Flog fort \u00fcber den See hinaus.", "tokens": ["Flog", "fort", "\u00fc\u00b7ber", "den", "See", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}