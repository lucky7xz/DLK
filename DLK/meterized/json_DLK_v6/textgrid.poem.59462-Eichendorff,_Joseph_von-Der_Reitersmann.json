{"textgrid.poem.59462": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch \u00fcber den stillen H\u00f6hen", "tokens": ["Hoch", "\u00fc\u00b7ber", "den", "stil\u00b7len", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Stand in dem Wald ein Haus,", "tokens": ["Stand", "in", "dem", "Wald", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort war's so einsam zu sehen", "tokens": ["Dort", "wa\u00b7r's", "so", "ein\u00b7sam", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weit \u00fcbern Wald hinaus.", "tokens": ["Weit", "\u00fc\u00b7bern", "Wald", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drin sa\u00df ein M\u00e4dchen am Rocken", "tokens": ["Drin", "sa\u00df", "ein", "M\u00e4d\u00b7chen", "am", "Ro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den ganzen Abend lang,", "tokens": ["Den", "gan\u00b7zen", "A\u00b7bend", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der wurden die Augen nicht trocken,", "tokens": ["Der", "wur\u00b7den", "die", "Au\u00b7gen", "nicht", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie spann und sann und sang:", "tokens": ["Sie", "spann", "und", "sann", "und", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Liebster, der war ein Reiter,", "tokens": ["\u00bb", "mein", "Liebs\u00b7ter", ",", "der", "war", "ein", "Rei\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem schwur ich Treu bis in Tod,", "tokens": ["Dem", "schwur", "ich", "Treu", "bis", "in", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der zog \u00fcber Land und weiter,", "tokens": ["Der", "zog", "\u00fc\u00b7ber", "Land", "und", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu Krieges Lust und Not.", "tokens": ["Zu", "Krie\u00b7ges", "Lust", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als ein Jahr war vergangen,", "tokens": ["Und", "als", "ein", "Jahr", "war", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und wieder bl\u00fchte das Land,", "tokens": ["Und", "wie\u00b7der", "bl\u00fch\u00b7te", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da stand ich voller Verlangen", "tokens": ["Da", "stand", "ich", "vol\u00b7ler", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hoch an des Waldes Rand.", "tokens": ["Hoch", "an", "des", "Wal\u00b7des", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Und zwischen den Bergesbogen,", "tokens": ["Und", "zwi\u00b7schen", "den", "Ber\u00b7ges\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohl \u00fcber den gr\u00fcnen Plan,", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "gr\u00fc\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kam mancher Reiter gezogen,", "tokens": ["Kam", "man\u00b7cher", "Rei\u00b7ter", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Der meine kam nicht mit an.", "tokens": ["Der", "mei\u00b7ne", "kam", "nicht", "mit", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "PTKNEG", "APPR", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und zwischen den Bergesbogen,", "tokens": ["Und", "zwi\u00b7schen", "den", "Ber\u00b7ges\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohl \u00fcber den gr\u00fcnen Plan,", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "gr\u00fc\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein J\u00e4gersmann kam geflogen,", "tokens": ["Ein", "J\u00e4\u00b7gers\u00b7mann", "kam", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der sah mich so mutig an.", "tokens": ["Der", "sah", "mich", "so", "mu\u00b7tig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "So lieblich die Sonne schiene,", "tokens": ["So", "lieb\u00b7lich", "die", "Son\u00b7ne", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Waldhorn scholl weit und breit,", "tokens": ["Das", "Wald\u00b7horn", "scholl", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da f\u00fchrt' er mich in das Gr\u00fcne,", "tokens": ["Da", "f\u00fchrt'", "er", "mich", "in", "das", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das war eine sch\u00f6ne Zeit! \u2013", "tokens": ["Das", "war", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Zeit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Der hat so lieblich gelogen", "tokens": ["Der", "hat", "so", "lieb\u00b7lich", "ge\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich aus der Treue heraus,", "tokens": ["Mich", "aus", "der", "Treu\u00b7e", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Der Falsche hat mich betrogen,", "tokens": ["Der", "Fal\u00b7sche", "hat", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zog weit in die Welt hinaus.\u00ab", "tokens": ["Zog", "weit", "in", "die", "Welt", "hin\u00b7aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie konnte nicht weitersingen,", "tokens": ["Sie", "konn\u00b7te", "nicht", "wei\u00b7ter\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor bitterem Schmerz und Leid,", "tokens": ["Vor", "bit\u00b7te\u00b7rem", "Schmerz", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Augen ihr \u00fcbergingen", "tokens": ["Die", "Au\u00b7gen", "ihr", "\u00fc\u00b7ber\u00b7gin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In ihrer Einsamkeit.", "tokens": ["In", "ih\u00b7rer", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Muhme, die sa\u00df beim Feuer", "tokens": ["Die", "Muh\u00b7me", ",", "die", "sa\u00df", "beim", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und w\u00e4rmte sich am Kamin,", "tokens": ["Und", "w\u00e4rm\u00b7te", "sich", "am", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es flackert' und spr\u00fcht' das Feuer,", "tokens": ["Es", "fla\u00b7ckert'", "und", "spr\u00fcht'", "das", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hell \u00fcber die Stube es schien.", "tokens": ["Hell", "\u00fc\u00b7ber", "die", "Stu\u00b7be", "es", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Sie sprach: \u00bbEin Kr\u00e4nzlein in Haaren,", "tokens": ["Sie", "sprach", ":", "\u00bb", "Ein", "Kr\u00e4nz\u00b7lein", "in", "Haa\u00b7ren", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das st\u00fcnde dir heut gar sch\u00f6n,", "tokens": ["Das", "st\u00fcn\u00b7de", "dir", "heut", "gar", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Willst drau\u00dfen auf dem See nicht fahren?", "tokens": ["Willst", "drau\u00b7\u00dfen", "auf", "dem", "See", "nicht", "fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hohe Blumen am Ufer dort stehn.\u00ab", "tokens": ["Ho\u00b7he", "Blu\u00b7men", "am", "U\u00b7fer", "dort", "stehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "ADV", "VVINF", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbich kann nicht holen die Blumen,", "tokens": ["\u00bb", "ich", "kann", "nicht", "ho\u00b7len", "die", "Blu\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Im Hemdlein wei\u00df am Teich", "tokens": ["Im", "Hemd\u00b7lein", "wei\u00df", "am", "Teich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein M\u00e4dchen h\u00fctet die Blumen,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "h\u00fc\u00b7tet", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die sieht so totenbleich.\u00ab", "tokens": ["Die", "sieht", "so", "to\u00b7ten\u00b7bleich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbund hoch auf des Sees Weite,", "tokens": ["\u00bb", "und", "hoch", "auf", "des", "Sees", "Wei\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Wenn alles finster und still,", "tokens": ["Wenn", "al\u00b7les", "fins\u00b7ter", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da rudern zwei stille Leute, \u2013", "tokens": ["Da", "ru\u00b7dern", "zwei", "stil\u00b7le", "Leu\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der eine dich haben will.\u00ab", "tokens": ["Der", "ei\u00b7ne", "dich", "ha\u00b7ben", "will", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "PPER", "VAINF", "VMFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "\u00bbsie schauen wie alte Bekannte,", "tokens": ["\u00bb", "sie", "schau\u00b7en", "wie", "al\u00b7te", "Be\u00b7kann\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVINF", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Still, ewig stille sie sind.", "tokens": ["Still", ",", "e\u00b7wig", "stil\u00b7le", "sie", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch einmal der eine sich wandte,", "tokens": ["Doch", "ein\u00b7mal", "der", "ei\u00b7ne", "sich", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ART", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da fa\u00dft' mich ein eiskalter Wind. \u2013", "tokens": ["Da", "fa\u00dft'", "mich", "ein", "eis\u00b7kal\u00b7ter", "Wind", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Mir ist zu wehe zum Weinen \u2013", 