{"textgrid.poem.46004": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Weihnacht, sternenhelle,", "tokens": ["Zur", "Weih\u00b7nacht", ",", "ster\u00b7nen\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df einsam auf der Schwelle", "tokens": ["Sa\u00df", "ein\u00b7sam", "auf", "der", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Kind so reich als fromm,", "tokens": ["Ein", "Kind", "so", "reich", "als", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und hie\u00df im reinen Herzen", "tokens": ["Und", "hie\u00df", "im", "rei\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Jesukind willkomm.", "tokens": ["Das", "Je\u00b7su\u00b7kind", "will\u00b7komm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sehnt sich nicht nach Gaben,", "tokens": ["Es", "sehnt", "sich", "nicht", "nach", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es wollte mehr nicht haben,", "tokens": ["Es", "woll\u00b7te", "mehr", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als was Maria hat,", "tokens": ["Als", "was", "Ma\u00b7ria", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "NE", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Den lieben Gottesknaben,", "tokens": ["Den", "lie\u00b7ben", "Got\u00b7tes\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der macht es freudensatt.", "tokens": ["Der", "macht", "es", "freu\u00b7den\u00b7satt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als mit Freud und Schmerzen", "tokens": ["Und", "als", "mit", "Freud", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er in des Kindes Herzen", "tokens": ["Er", "in", "des", "Kin\u00b7des", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als einer Krippe spielt,", "tokens": ["Als", "ei\u00b7ner", "Krip\u00b7pe", "spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da sang das Kind dem Kinde,", "tokens": ["Da", "sang", "das", "Kind", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das es am Busen hielt:", "tokens": ["Das", "es", "am", "Bu\u00b7sen", "hielt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbach h\u00e4tt' ich Engelzungen,", "tokens": ["\u00bb", "ach", "h\u00e4tt'", "ich", "En\u00b7gel\u00b7zun\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich h\u00e4tt' dir l\u00e4ngst gesungen", "tokens": ["Ich", "h\u00e4tt'", "dir", "l\u00e4ngst", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das s\u00fc\u00dfe liebe Lied,", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "lie\u00b7be", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das mir so still und selig", "tokens": ["Das", "mir", "so", "still", "und", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im jungen Herzen gl\u00fcht.", "tokens": ["Im", "jun\u00b7gen", "Her\u00b7zen", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df ja keine Weisen,", "tokens": ["Ich", "wei\u00df", "ja", "kei\u00b7ne", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich Heiland so zu preisen,", "tokens": ["Dich", "Hei\u00b7land", "so", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich Jesu fromm und mild,", "tokens": ["Dich", "Je\u00b7su", "fromm", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie meine ganze Seele", "tokens": ["Wie", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dir jauchzt und singt und spielt.", "tokens": ["Dir", "jauchzt", "und", "singt", "und", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich mu\u00df mein Haupt dir neigen", "tokens": ["Ich", "mu\u00df", "mein", "Haupt", "dir", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann weinen nur und schweigen", "tokens": ["Kann", "wei\u00b7nen", "nur", "und", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "ADV", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Seligkeit und Schmerz,", "tokens": ["In", "Se\u00b7lig\u00b7keit", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach Kind, du wei\u00dft mein Lieben,", "tokens": ["Ach", "Kind", ",", "du", "wei\u00dft", "mein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du siehst mir ja ins Herz!\u00ab", "tokens": ["Du", "siehst", "mir", "ja", "ins", "Herz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Als Jesus dies geh\u00f6ret,", "tokens": ["Als", "Je\u00b7sus", "dies", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spricht er: \u00bbdir sei bescheret", "tokens": ["Spricht", "er", ":", "\u00bb", "dir", "sei", "be\u00b7sche\u00b7ret"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Kindlein, Schmerzenreich,", "tokens": ["Ein", "Kin\u00b7dlein", ",", "Schmer\u00b7zen\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das sollst du betend wiegen,", "tokens": ["Das", "sollst", "du", "be\u00b7tend", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Rein meiner Mutter gleich.", "tokens": ["Rein", "mei\u00b7ner", "Mut\u00b7ter", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und auch sei dir bescheret", "tokens": ["Und", "auch", "sei", "dir", "be\u00b7sche\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Herz zu mir bekehret,", "tokens": ["Ein", "Herz", "zu", "mir", "be\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Herz an Schmerzen reich,", "tokens": ["Ein", "Herz", "an", "Schmer\u00b7zen", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Darinnen sollst du wiegen", "tokens": ["Da\u00b7rin\u00b7nen", "sollst", "du", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Kind mit mir zugleich.", "tokens": ["Das", "Kind", "mit", "mir", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und auch sei dir bescheret", "tokens": ["Und", "auch", "sei", "dir", "be\u00b7sche\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Weise, die mich ehret,", "tokens": ["Die", "Wei\u00b7se", ",", "die", "mich", "eh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit freud'gem Flug und Fall,", "tokens": ["Mit", "freu\u00b7d'\u00b7gem", "Flug", "und", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Lied, das mir lobsinget", "tokens": ["Das", "Lied", ",", "das", "mir", "lob\u00b7sin\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trotz Lerch', trotz Nachtigall.", "tokens": ["Trotz", "Ler\u00b7ch'", ",", "trotz", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sieh, was ich dir gegeben,", "tokens": ["Sieh", ",", "was", "ich", "dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum sollst das Haupt du heben,", "tokens": ["Drum", "sollst", "das", "Haupt", "du", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00f6ffnen deinen Mund,", "tokens": ["Und", "\u00f6ff\u00b7nen", "dei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und freud'ge Lieder singen,", "tokens": ["Und", "freu\u00b7d'\u00b7ge", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Steh auf und sei gesund!", "tokens": ["Steh", "auf", "und", "sei", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es wird ein Fr\u00fchling kommen,", "tokens": ["Es", "wird", "ein", "Fr\u00fch\u00b7ling", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der bringt, was ward genommen,", "tokens": ["Der", "bringt", ",", "was", "ward", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Blumen und den Kranz,", "tokens": ["Die", "Blu\u00b7men", "und", "den", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sei freudig, sei geschm\u00fccket,", "tokens": ["Sei", "freu\u00b7dig", ",", "sei", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Unschuld ist ein Glanz!", "tokens": ["Die", "Un\u00b7schuld", "ist", "ein", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es wird ein Sommer kommen,", "tokens": ["Es", "wird", "ein", "Som\u00b7mer", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll Segen f\u00fcr die Frommen,", "tokens": ["Voll", "Se\u00b7gen", "f\u00fcr", "die", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll lichtem \u00c4hrengold,", "tokens": ["Voll", "lich\u00b7tem", "\u00c4h\u00b7ren\u00b7gold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da lese fromm geb\u00fccket", "tokens": ["Da", "le\u00b7se", "fromm", "ge\u00b7b\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein G\u00e4rblein dir zum Sold.", "tokens": ["Ein", "G\u00e4rb\u00b7lein", "dir", "zum", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Es wird ein Herbst sich neigen,", "tokens": ["Es", "wird", "ein", "Herbst", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Brich Wein und Frucht von Zweigen", "tokens": ["Brich", "Wein", "und", "Frucht", "von", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' dir's zugedacht,", "tokens": ["Ich", "hab'", "dir's", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Drum sei in Freud und Wonne", "tokens": ["Drum", "sei", "in", "Freud", "und", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Ernde eingebracht.", "tokens": ["Die", "Ern\u00b7de", "ein\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und k\u00f6mmt der ernste Winter,", "tokens": ["Und", "k\u00f6mmt", "der", "erns\u00b7te", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann sei wie andre Kinder", "tokens": ["Dann", "sei", "wie", "and\u00b7re", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An meiner Wiege froh.\u00ab", "tokens": ["An", "mei\u00b7ner", "Wie\u00b7ge", "froh", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da sprach das Kind ergeben:", "tokens": ["Da", "sprach", "das", "Kind", "er\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbja Kind, das will ich so!", "tokens": ["\u00bb", "ja", "Kind", ",", "das", "will", "ich", "so", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "PDS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "All, was du mir bescheret,", "tokens": ["All", ",", "was", "du", "mir", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich von dir begehret", "tokens": ["Hab'", "ich", "von", "dir", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Mit Liedes Flug und Fall,", "tokens": ["Mit", "Lie\u00b7des", "Flug", "und", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Drum will ich dir lobsingen", "tokens": ["Drum", "will", "ich", "dir", "lob\u00b7sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trotz Lerch', trotz Nachtigall!\u00ab", "tokens": ["Trotz", "Ler\u00b7ch'", ",", "trotz", "Nach\u00b7ti\u00b7gall", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}