{"textgrid.poem.58763": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "11. Allgegenw\u00e4rtig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ganz \u00fcberall, ganz hier;", "tokens": ["Du", "bist", "ganz", "\u00fc\u00b7be\u00b7rall", ",", "ganz", "hier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bete an und mich verlier,", "tokens": ["Ich", "be\u00b7te", "an", "und", "mich", "ver\u00b7lier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kann so hoch, so tief nicht denken.", "tokens": ["Ich", "kann", "so", "hoch", ",", "so", "tief", "nicht", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ist, hat in dir sein Bestehn,", "tokens": ["Was", "ist", ",", "hat", "in", "dir", "sein", "Be\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wird dein Nahsein nicht gesehn,", "tokens": ["Doch", "wird", "dein", "Na\u00b7hsein", "nicht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Du mu\u00dft dich offenbar'n und schenken.", "tokens": ["Du", "mu\u00dft", "dich", "of\u00b7fen\u00b7ba\u00b7r'n", "und", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKVZ", "KON", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mein Gott, mein Heil, ach, wann geschicht's?", "tokens": ["Mein", "Gott", ",", "mein", "Heil", ",", "ach", ",", "wann", "ge\u00b7schicht's", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ITJ", "$,", "PWAV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bist du bei mir, dann f\u00fcrcht' ich nichts.", "tokens": ["Bist", "du", "bei", "mir", ",", "dann", "f\u00fcrcht'", "ich", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}