{"dta.poem.738": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxIV. An Johannem Fridericum  von Sack  \n  An. \u217d\u217c\u2184 \u217c\u0254\u2184  cxxxvi.  d.  xxiv.  Junij.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nt' ich ein ander band' als trawte trewe finden ", "tokens": ["K\u00f6nt'", "ich", "ein", "an\u00b7der", "band'", "als", "traw\u00b7te", "tre\u00b7we", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJD", "ADJD", "KOKOM", "VVFIN", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als fester Liebe krafft/ vnd vnverf\u00e4lschte gunst", "tokens": ["Als", "fes\u00b7ter", "Lie\u00b7be", "krafft", "/", "vnd", "vn\u00b7ver\u00b7f\u00e4lschte", "gunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "NN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vnd Freundschaft die sich nicht geschminckt mit falschem", "tokens": ["Vnd", "Freund\u00b7schaft", "die", "sich", "nicht", "ge\u00b7schminckt", "mit", "fal\u00b7schem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "PRF", "PTKNEG", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dunst", "tokens": ["dunst"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "So wolt ich ewer Hertz/ Herr Sack/ damit vmbwinden.", "tokens": ["So", "wolt", "ich", "e\u00b7wer", "Hertz", "/", "Herr", "Sack", "/", "da\u00b7mit", "vmbwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$(", "NN", "NN", "$(", "PAV", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Weil ich mein w\u00fcndschen denn auff nichts kan fester gr\u00fcn- ", "tokens": ["Weil", "ich", "mein", "w\u00fcnd\u00b7schen", "denn", "auff", "nichts", "kan", "fes\u00b7ter", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "ADV", "APPR", "PIS", "VMFIN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den;", "tokens": ["den", ";"], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Weil lieben vber stand/ vnd reiche pracht/ vnd kunst/ ", "tokens": ["Weil", "lie\u00b7ben", "vber", "stand", "/", "vnd", "rei\u00b7che", "pracht", "/", "vnd", "kunst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "VVFIN", "$(", "KON", "ADJA", "NN", "$(", "KON", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Weil st\u00e4rcker als der Tod die flamme trewer brunst/", "tokens": ["Weil", "st\u00e4r\u00b7cker", "als", "der", "Tod", "die", "flam\u00b7me", "tre\u00b7wer", "brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KOKOM", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wil ich euch vnd mich darmit auff ewig binden.", "tokens": ["So", "wil", "ich", "euch", "vnd", "mich", "dar\u00b7mit", "auff", "e\u00b7wig", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "KON", "PPER", "PAV", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo fern in kurtzem wird/ (gleich wie wir wol verstehn/)", "tokens": ["Wo", "fern", "in", "kurt\u00b7zem", "wird", "/", "(", "gleich", "wie", "wir", "wol", "ver\u00b7stehn", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "VAFIN", "$(", "$(", "ADV", "KOKOM", "PPER", "ADV", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der vnverhoffte schlus des Himmels f\u00fcr sich gehn/", "tokens": ["Der", "vn\u00b7ver\u00b7hoff\u00b7te", "schlus", "des", "Him\u00b7mels", "f\u00fcr", "sich", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mich von Euch/ dahin ich nie gedacht/ wil f\u00fchren:", "tokens": ["Der", "mich", "von", "Euch", "/", "da\u00b7hin", "ich", "nie", "ge\u00b7dacht", "/", "wil", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "$(", "PAV", "PPER", "ADV", "VVPP", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So bleib ich dennoch hier/ ich las euch nichts denn mich", "tokens": ["So", "bleib", "ich", "den\u00b7noch", "hier", "/", "ich", "las", "euch", "nichts", "denn", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$(", "PPER", "VVFIN", "PPER", "PIS", "KON", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ander siht mich nicht/ jhr k\u00f6nn\u2019t mein ander ich", "tokens": ["Ein", "an\u00b7der", "siht", "mich", "nicht", "/", "jhr", "k\u00f6nn't", "mein", "an\u00b7der", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "PPER", "PTKNEG", "$(", "PPER", "VMFIN", "PPOSAT", "ADJD", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sey auch wo ich sey mich nimmer mehr verlihren.", "tokens": ["Ich", "sey", "auch", "wo", "ich", "sey", "mich", "nim\u00b7mer", "mehr", "ver\u00b7lih\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWAV", "PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}