{"textgrid.poem.63466": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "De krank S\u0153hn", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, Moder! Hier is gor keen Tog;", "tokens": ["Hier", ",", "Mo\u00b7der", "!", "Hier", "is", "gor", "ke\u00b7en", "Tog", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "ADV", "FM", "FM", "VVFIN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "hier achter schint de S\u00fcnn", "tokens": ["hier", "ach\u00b7ter", "schint", "de", "S\u00fcnn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "VVFIN", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "grad as in'n Mai so warming noch,", "tokens": ["grad", "as", "in'n", "Mai", "so", "war\u00b7ming", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dat's noch nich lat \u2013 du weest dat doch,", "tokens": ["dat's", "noch", "nich", "lat", "\u2013", "du", "weest", "dat", "doch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "VVFIN", "$(", "PPER", "VVFIN", "PAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wu girn ick buten b\u00fcn.", "tokens": ["wu", "girn", "ick", "bu\u00b7ten", "b\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Grad hier bi \u00fcns' oll Immenschur,", "tokens": ["Grad", "hier", "bi", "\u00fcns'", "oll", "Im\u00b7men\u00b7schur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wenn'ck dor so sitt un denk", "tokens": ["wenn'ck", "dor", "so", "sitt", "un", "denk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "VVFIN", "FM", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "un't s\u00fcmmst so dichting v\u00f6r min Uhr,", "tokens": ["un't", "s\u00fcmmst", "so", "dich\u00b7ting", "v\u00f6r", "min", "Uhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dat stimmt so recht to min Natur \u2013", "tokens": ["dat", "stimmt", "so", "recht", "to", "min", "Na\u00b7tur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADJD", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "dor stell man hen de B\u00e4nk!", "tokens": ["dor", "stell", "man", "hen", "de", "B\u00e4nk", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "De Kirch v\u00f6rbi un Kirchhoff kann'ck", "tokens": ["De", "Kirch", "v\u00f6r\u00b7bi", "un", "Kirch\u00b7hoff", "kann'ck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "mi dor so wit \u00fcmsehn,", "tokens": ["mi", "dor", "so", "wit", "\u00fcm\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "de grote Wisch un Schapdrift lank,", "tokens": ["de", "gro\u00b7te", "Wisch", "un", "Schap\u00b7drift", "lank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "FM", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "de heel deep Grund noch af un mank", "tokens": ["de", "heel", "deep", "Grund", "noch", "af", "un", "mank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "de Hellbarg beid de Seen.", "tokens": ["de", "Hell\u00b7barg", "beid", "de", "Seen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.da", "FM.da", "FM.da", "FM.da", "FM.da", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn ward mi gor to sch\u00f6n to Mot,", "tokens": ["Denn", "ward", "mi", "gor", "to", "sch\u00f6n", "to", "Mot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "as wir keen Hoor mi kr\u00fcmmt \u2013", "tokens": ["as", "wir", "ke\u00b7en", "Hoor", "mi", "kr\u00fcmmt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "NE", "NE", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ick denk denn gor nich an den Dod,", "tokens": ["ick", "denk", "denn", "gor", "nich", "an", "den", "Dod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NE", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ick meen denn, all ward wedder got,", "tokens": ["ick", "meen", "denn", ",", "all", "ward", "wed\u00b7der", "got", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PIAT", "VAFIN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "noch ihr de Winter k\u00fcmmt.", "tokens": ["noch", "ihr", "de", "Win\u00b7ter", "k\u00fcmmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Un wenn de S\u00fcnn denn deeper stiggt", "tokens": ["Un", "wenn", "de", "S\u00fcnn", "denn", "dee\u00b7per", "stiggt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "NE", "NE", "KON", "PDS", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "un geiht to R\u00fcst dor still,", "tokens": ["un", "geiht", "to", "R\u00fcst", "dor", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "denn ward mi as sonn Vagel licht,", "tokens": ["denn", "ward", "mi", "as", "sonn", "Va\u00b7gel", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "FM", "FM", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "de grad sin irst Swunkfeddern kriggt", "tokens": ["de", "grad", "sin", "irst", "Swunk\u00b7fed\u00b7dern", "kriggt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "un se vers\u00f6ken will.", "tokens": ["un", "se", "ver\u00b7s\u00f6\u00b7ken", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}