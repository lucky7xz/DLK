{"dta.poem.9469": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach sch\u00f6ne Braut/ mein ander Ich/", "tokens": ["Ach", "sch\u00f6\u00b7ne", "Braut", "/", "mein", "an\u00b7der", "Ich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "PPOSAT", "ADJD", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O mein Rubien/ Jhr geb ich mich", "tokens": ["O", "mein", "Ru\u00b7bi\u00b7en", "/", "Ihr", "geb", "ich", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "anjtzt auch gantz zu eigen:", "tokens": ["anjtzt", "auch", "gantz", "zu", "ei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Es zwinget mich ihr Rosen-mund/", "tokens": ["Es", "zwin\u00b7get", "mich", "ihr", "Ro\u00b7sen\u00b7mund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das lieblich-sehn das macht mich wund", "tokens": ["Das", "lieb\u00b7lich\u00b7sehn", "das", "macht", "mich", "wund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PDS", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kann es nicht verschweigen.", "tokens": ["Ich", "kann", "es", "nicht", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es soll kein groll", "tokens": ["Es", "soll", "kein", "groll"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Noch was feindlich und nicht freundlich", "tokens": ["Noch", "was", "feind\u00b7lich", "und", "nicht", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "ADJD", "KON", "PTKNEG", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "uns betr\u00fcben;", "tokens": ["uns", "be\u00b7tr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Ich will Sie von hertzen lieben.", "tokens": ["Ich", "will", "Sie", "von", "hert\u00b7zen", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Vom Himmel k\u00f6m\u0303t das sch\u00f6ne Bild/", "tokens": ["Vom", "Him\u00b7mel", "k\u00f6m\u0303t", "das", "sch\u00f6\u00b7ne", "Bild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Trost/ die Lust/ das Freuden-schild/", "tokens": ["Der", "Trost", "/", "die", "Lust", "/", "das", "Freu\u00b7den\u00b7schild", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun kann ich mich erquicken;", "tokens": ["Nun", "kann", "ich", "mich", "er\u00b7qui\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn mich der Krieg ja traurig macht", "tokens": ["Wenn", "mich", "der", "Krieg", "ja", "trau\u00b7rig", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kann Sie doch zu Tag und Nacht", "tokens": ["So", "kann", "Sie", "doch", "zu", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Trost mich recht anblicken/", "tokens": ["Mit", "Trost", "mich", "recht", "an\u00b7bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach mein Sch\u00e4tzlein/", "tokens": ["Ach", "mein", "Sch\u00e4tz\u00b7lein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "La\u00dft erwarmen in den Armen", "tokens": ["La\u00dft", "er\u00b7war\u00b7men", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Meine Glieder", "tokens": ["Mei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Legt den Nahmen bey mier nieder.", "tokens": ["Legt", "den", "Nah\u00b7men", "bey", "mier", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}