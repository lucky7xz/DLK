{"textgrid.poem.55094": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Totentanz", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der T\u00fcrmer, der schaut zumitten der Nacht", "tokens": ["Der", "T\u00fcr\u00b7mer", ",", "der", "schaut", "zu\u00b7mit\u00b7ten", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hinab auf die Gr\u00e4ber in Lage;", "tokens": ["Hin\u00b7ab", "auf", "die", "Gr\u00e4\u00b7ber", "in", "La\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Mond, der hat alles ins Helle gebracht;", "tokens": ["Der", "Mond", ",", "der", "hat", "al\u00b7les", "ins", "Hel\u00b7le", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PIS", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Kirchhof, er liegt wie am Tage.", "tokens": ["Der", "Kirch\u00b7hof", ",", "er", "liegt", "wie", "am", "Ta\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da regt sich ein Grab und ein anderes dann:", "tokens": ["Da", "regt", "sich", "ein", "Grab", "und", "ein", "an\u00b7de\u00b7res", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "KON", "ART", "PIS", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie kommen hervor, ein Weib da, ein Mann,", "tokens": ["Sie", "kom\u00b7men", "her\u00b7vor", ",", "ein", "Weib", "da", ",", "ein", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In wei\u00dfen und schleppenden Hemden.", "tokens": ["In", "wei\u00b7\u00dfen", "und", "schlep\u00b7pen\u00b7den", "Hem\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Das reckt nun, es will sich ergetzen sogleich,", "tokens": ["Das", "reckt", "nun", ",", "es", "will", "sich", "er\u00b7get\u00b7zen", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PRF", "VVFIN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Kn\u00f6chel zur Runde, zum Kranze,", "tokens": ["Die", "Kn\u00f6\u00b7chel", "zur", "Run\u00b7de", ",", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "So arm und so jung und so alt und so reich;", "tokens": ["So", "arm", "und", "so", "jung", "und", "so", "alt", "und", "so", "reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "KON", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch hindern die Schleppen am Tanze.", "tokens": ["Doch", "hin\u00b7dern", "die", "Schlep\u00b7pen", "am", "Tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und weil hier die Scham nun nicht weiter gebeut,", "tokens": ["Und", "weil", "hier", "die", "Scham", "nun", "nicht", "wei\u00b7ter", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie sch\u00fctteln sich alle, da liegen zerstreut", "tokens": ["Sie", "sch\u00fct\u00b7teln", "sich", "al\u00b7le", ",", "da", "lie\u00b7gen", "zer\u00b7streut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "$,", "ADV", "VVFIN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Hemdelein \u00fcber den H\u00fcgeln.", "tokens": ["Die", "Hem\u00b7del\u00b7ein", "\u00fc\u00b7ber", "den", "H\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Nun hebt sich der Schenkel, nun wackelt das Bein,", "tokens": ["Nun", "hebt", "sich", "der", "Schen\u00b7kel", ",", "nun", "wa\u00b7ckelt", "das", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Geb\u00e4rden da gibt es vertrackte;", "tokens": ["Ge\u00b7b\u00e4r\u00b7den", "da", "gibt", "es", "ver\u00b7track\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dann klippert's und klappert's mitunter hinein,", "tokens": ["Dann", "klip\u00b7pert's", "und", "klap\u00b7pert's", "mi\u00b7tun\u00b7ter", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als schl\u00fcg man die H\u00f6lzlein zum Takte.", "tokens": ["Als", "schl\u00fcg", "man", "die", "H\u00f6lz\u00b7lein", "zum", "Tak\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Das kommt nun dem T\u00fcrmer so l\u00e4cherlich vor;", "tokens": ["Das", "kommt", "nun", "dem", "T\u00fcr\u00b7mer", "so", "l\u00e4\u00b7cher\u00b7lich", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da raunt ihm der Schalk, der Versucher, ins Ohr:", "tokens": ["Da", "raunt", "ihm", "der", "Schalk", ",", "der", "Ver\u00b7su\u00b7cher", ",", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "\u00bbgeh! hole dir einen der Laken.\u00ab", "tokens": ["\u00bb", "geh", "!", "ho\u00b7le", "dir", "ei\u00b7nen", "der", "La\u00b7ken", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "PPER", "ART", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Getan wie gedacht! und er fl\u00fcchtet sich schnell", "tokens": ["Ge\u00b7tan", "wie", "ge\u00b7dacht", "!", "und", "er", "fl\u00fcch\u00b7tet", "sich", "schnell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "VVPP", "$.", "KON", "PPER", "VVFIN", "PRF", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nun hinter geheiligte T\u00fcren.", "tokens": ["Nun", "hin\u00b7ter", "ge\u00b7hei\u00b7lig\u00b7te", "T\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Mond und noch immer er scheinet so hell", "tokens": ["Der", "Mond", "und", "noch", "im\u00b7mer", "er", "schei\u00b7net", "so", "hell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADV", "ADV", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zum Tanz, den sie schauderlich f\u00fchren.", "tokens": ["Zum", "Tanz", ",", "den", "sie", "schau\u00b7der\u00b7lich", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Doch endlich verlieret sich dieser und der,", "tokens": ["Doch", "end\u00b7lich", "ver\u00b7lie\u00b7ret", "sich", "die\u00b7ser", "und", "der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PDAT", "KON", "ART", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Schleicht eins nach dem andern gekleidet einher,", "tokens": ["Schleicht", "eins", "nach", "dem", "an\u00b7dern", "ge\u00b7klei\u00b7det", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "VVPP", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und husch! ist es unter dem Rasen.", "tokens": ["Und", "husch", "!", "ist", "es", "un\u00b7ter", "dem", "Ra\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Nur einer, der trippelt und stolpert zuletzt", "tokens": ["Nur", "ei\u00b7ner", ",", "der", "trip\u00b7pelt", "und", "stol\u00b7pert", "zu\u00b7letzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und tappet und grapst an den Gr\u00fcften;", "tokens": ["Und", "tap\u00b7pet", "und", "grapst", "an", "den", "Gr\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch hat kein Geselle so schwer ihn verletzt;", "tokens": ["Doch", "hat", "kein", "Ge\u00b7sel\u00b7le", "so", "schwer", "ihn", "ver\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er wittert das Tuch in den L\u00fcften.", "tokens": ["Er", "wit\u00b7tert", "das", "Tuch", "in", "den", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er r\u00fcttelt die Turmt\u00fcr, sie schl\u00e4gt ihn zur\u00fcck,", "tokens": ["Er", "r\u00fct\u00b7telt", "die", "Turm\u00b7t\u00fcr", ",", "sie", "schl\u00e4gt", "ihn", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Geziert und gesegnet, dem T\u00fcrmer zum Gl\u00fcck,", "tokens": ["Ge\u00b7ziert", "und", "ge\u00b7seg\u00b7net", ",", "dem", "T\u00fcr\u00b7mer", "zum", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Sie blinkt von metallenen Kreuzen.", "tokens": ["Sie", "blinkt", "von", "me\u00b7tal\u00b7le\u00b7nen", "Kreu\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Das Hemd mu\u00df er haben, da rastet er nicht,", "tokens": ["Das", "Hemd", "mu\u00df", "er", "ha\u00b7ben", ",", "da", "ras\u00b7tet", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da gilt auch kein langes Besinnen,", "tokens": ["Da", "gilt", "auch", "kein", "lan\u00b7ges", "Be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den gotischen Zierat ergreift nun der Wicht", "tokens": ["Den", "go\u00b7ti\u00b7schen", "Zie\u00b7rat", "er\u00b7greift", "nun", "der", "Wicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und klettert von Zinne zu Zinnen.", "tokens": ["Und", "klet\u00b7tert", "von", "Zin\u00b7ne", "zu", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Nun ist's um den armen, den T\u00fcrmer getan!", "tokens": ["Nun", "ist's", "um", "den", "ar\u00b7men", ",", "den", "T\u00fcr\u00b7mer", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es ruckt sich von Schn\u00f6rkel zu Schn\u00f6rkel hinan,", "tokens": ["Es", "ruckt", "sich", "von", "Schn\u00f6r\u00b7kel", "zu", "Schn\u00f6r\u00b7kel", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Langbeinigen Spinnen vergleichbar.", "tokens": ["Lang\u00b7bei\u00b7ni\u00b7gen", "Spin\u00b7nen", "ver\u00b7gleich\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Der T\u00fcrmer erbleichet, der T\u00fcrmer erbebt,", "tokens": ["Der", "T\u00fcr\u00b7mer", "er\u00b7blei\u00b7chet", ",", "der", "T\u00fcr\u00b7mer", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gern g\u00e4b er ihn wieder, den Laken.", "tokens": ["Gern", "g\u00e4b", "er", "ihn", "wie\u00b7der", ",", "den", "La\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da h\u00e4kelt \u2013 jetzt hat er am l\u00e4ngsten gelebt \u2013", "tokens": ["Da", "h\u00e4\u00b7kelt", "\u2013", "jetzt", "hat", "er", "am", "l\u00e4ngs\u00b7ten", "ge\u00b7lebt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "APPRART", "ADJA", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Zipfel ein eiserner Zacken.", "tokens": ["Den", "Zip\u00b7fel", "ein", "ei\u00b7ser\u00b7ner", "Za\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Schon tr\u00fcbet der Mond sich verschwindenden Scheins,", "tokens": ["Schon", "tr\u00fc\u00b7bet", "der", "Mond", "sich", "ver\u00b7schwin\u00b7den\u00b7den", "Scheins", ","], "token_info": ["word", "word", "