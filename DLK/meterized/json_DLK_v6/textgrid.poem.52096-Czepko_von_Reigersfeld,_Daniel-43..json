{"textgrid.poem.52096": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil auff den j\u00fcngsten Tag sol alles untergehn,", "tokens": ["Weil", "auff", "den", "j\u00fcng\u00b7sten", "Tag", "sol", "al\u00b7les", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00e4ngt Sylvius schon an das Seine zu verkochen:", "tokens": ["F\u00e4ngt", "Syl\u00b7vius", "schon", "an", "das", "Sei\u00b7ne", "zu", "ver\u00b7ko\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Meint untern Armen so das Himmel Reich zu suchen,", "tokens": ["Meint", "un\u00b7tern", "Ar\u00b7men", "so", "das", "Him\u00b7mel", "Reich", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du must Geist- und nicht Geldarm seyn, wilt du bestehn.", "tokens": ["Du", "must", "Geist", "und", "nicht", "Gel\u00b7darm", "seyn", ",", "wilt", "du", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "TRUNC", "KON", "PTKNEG", "NN", "VAINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}