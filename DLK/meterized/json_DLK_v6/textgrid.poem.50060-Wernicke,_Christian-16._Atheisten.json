{"textgrid.poem.50060": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "16. Atheisten", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass ein verfluchter Mensch vor seinen ", "tokens": ["Dass", "ein", "ver\u00b7fluch\u00b7ter", "Mensch", "vor", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eh' er ", "tokens": ["Und", "eh'", "er"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ja ", "tokens": ["Ja"], "token_info": ["word"], "pos": ["PTKANT"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ist wider die Vernunfft. Wer keinen Gott nicht glaubet,", "tokens": ["Ist", "wi\u00b7der", "die", "Ver\u00b7nunfft", ".", "Wer", "kei\u00b7nen", "Gott", "nicht", "glau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$.", "PWS", "PIAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der glaubt kein ander Leben nicht,", "tokens": ["Der", "glaubt", "kein", "an\u00b7der", "Le\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJD", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kan mit freudigem Gesicht,", "tokens": ["Und", "kan", "mit", "freu\u00b7di\u00b7gem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um dieses Leben zu erhalten", "tokens": ["Um", "die\u00b7ses", "Le\u00b7ben", "zu", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PDAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die unerschrockne H\u00e4nde falten", "tokens": ["Die", "un\u00b7er\u00b7schrock\u00b7ne", "H\u00e4n\u00b7de", "fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vor jedem Baal und Astaroth.", "tokens": ["Vor", "je\u00b7dem", "Baal", "und", "As\u00b7ta\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Ursach kan, und kommt drum nirgends anders her,", "tokens": ["Die", "Ur\u00b7sach", "kan", ",", "und", "kommt", "drum", "nir\u00b7gends", "an\u00b7ders", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "KON", "VVFIN", "PAV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als dass in seinem strengen Raht", "tokens": ["Als", "dass", "in", "sei\u00b7nem", "stren\u00b7gen", "Raht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gott ihm ", "tokens": ["Gott", "ihm"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "So gar, dass keiner mehr als der,", "tokens": ["So", "gar", ",", "dass", "kei\u00b7ner", "mehr", "als", "der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIS", "PIS", "KOKOM", "ART", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}