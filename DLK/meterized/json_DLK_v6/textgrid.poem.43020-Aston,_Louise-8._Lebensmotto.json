{"textgrid.poem.43020": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "8. Lebensmotto", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fromme Seelen, fromme Herzen,", "tokens": ["From\u00b7me", "See\u00b7len", ",", "from\u00b7me", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelssehnend, lebenssatt;", "tokens": ["Him\u00b7mels\u00b7seh\u00b7nend", ",", "le\u00b7bens\u00b7satt", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine finst're Sch\u00e4delstatt!", "tokens": ["Ei\u00b7ne", "finst'\u00b7re", "Sch\u00e4\u00b7del\u00b7statt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag in schreckenden Gesichten", "tokens": ["Mag", "in", "schre\u00b7cken\u00b7den", "Ge\u00b7sich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bang vor ", "tokens": ["Bang", "vor"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Nie soll mich der Schmerz vernichten,", "tokens": ["Nie", "soll", "mich", "der", "Schmerz", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nie zerknirscht und reuig seh'n!", "tokens": ["Nie", "zer\u00b7knirscht", "und", "reu\u00b7ig", "seh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich auf weiten Fluten tr\u00e4gt:", "tokens": ["Mich", "auf", "wei\u00b7ten", "Flu\u00b7ten", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Tiefen freudig lauschend", "tokens": ["Dei\u00b7nen", "Tie\u00b7fen", "freu\u00b7dig", "lau\u00b7schend"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steh' ich sinnend, stummbewegt.", "tokens": ["Steh'", "ich", "sin\u00b7nend", ",", "stumm\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzt Gewittersturm, der wilde,", "tokens": ["St\u00fcrzt", "Ge\u00b7wit\u00b7ter\u00b7sturm", ",", "der", "wil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jauchzend sich in's Meer hinein,", "tokens": ["Jauch\u00b7zend", "sich", "in's", "Meer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schau' ich in dem Flammenbilde", "tokens": ["Schau'", "ich", "in", "dem", "Flam\u00b7men\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meines Lebens Wiederschein.", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Wie\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von dem blinden Wahn verkannt,", "tokens": ["Von", "dem", "blin\u00b7den", "Wahn", "ver\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft gemartert, oft geknechtet,", "tokens": ["Oft", "ge\u00b7mar\u00b7tert", ",", "oft", "ge\u00b7knech\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Recht und Vaterland;", "tokens": ["Oh\u00b7ne", "Recht", "und", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fester Bund von stolzen Seelen", "tokens": ["Fes\u00b7ter", "Bund", "von", "stol\u00b7zen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den des Lebens Glut gebar,", "tokens": ["Den", "des", "Le\u00b7bens", "Glut", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freier Herzen freies W\u00e4hlen,", "tokens": ["Frei\u00b7er", "Her\u00b7zen", "frei\u00b7es", "W\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor der Sch\u00f6pfung Hochaltar!", "tokens": ["Vor", "der", "Sch\u00f6p\u00b7fung", "Hoc\u00b7hal\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so lang' die Pulse beben,", "tokens": ["Und", "so", "lang'", "die", "Pul\u00b7se", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zum letzten Athemzug,", "tokens": ["Bis", "zum", "letz\u00b7ten", "A\u00b7them\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weih' der Liebe ich dies Leben,", "tokens": ["Weih'", "der", "Lie\u00b7be", "ich", "dies", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrem Segen, ihrem Fluch!", "tokens": ["Ih\u00b7rem", "Se\u00b7gen", ",", "ih\u00b7rem", "Fluch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6ne Welt, du bl\u00fchend Eden,", "tokens": ["Sch\u00f6\u00b7ne", "Welt", ",", "du", "bl\u00fc\u00b7hend", "E\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner Freuden reicher Schatz", "tokens": ["Dei\u00b7ner", "Freu\u00b7den", "rei\u00b7cher", "Schatz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Giebt f\u00fcr alle Schicksals Fehden", "tokens": ["Giebt", "f\u00fcr", "al\u00b7le", "Schick\u00b7sals", "Feh\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vollen, k\u00f6stlichen Ersatz!", "tokens": ["Vol\u00b7len", ",", "k\u00f6st\u00b7li\u00b7chen", "Er\u00b7satz", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}