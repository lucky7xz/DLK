{"textgrid.poem.65730": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anmut, unwirtliche! so mu\u00dft du nur", "tokens": ["An\u00b7mut", ",", "un\u00b7wirt\u00b7li\u00b7che", "!", "so", "mu\u00dft", "du", "nur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "$.", "ADV", "VMFIN", "PPER", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf dich dein reizendes Verm\u00e4chtnis wenden?", "tokens": ["Auf", "dich", "dein", "rei\u00b7zen\u00b7des", "Ver\u00b7m\u00e4cht\u00b7nis", "wen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch schenket nichts, es leihet nur Natur,", "tokens": ["Doch", "schen\u00b7ket", "nichts", ",", "es", "lei\u00b7het", "nur", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und leiht, freigebig selbst, nur freien H\u00e4nden.", "tokens": ["Und", "leiht", ",", "frei\u00b7ge\u00b7big", "selbst", ",", "nur", "frei\u00b7en", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "ADV", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Warum mi\u00dfbrauchst du sch\u00f6ner Karger dann", "tokens": ["Wa\u00b7rum", "mi\u00df\u00b7brauchst", "du", "sch\u00f6\u00b7ner", "Kar\u00b7ger", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dies reiche Gut, zum Geben dir gegeben?", "tokens": ["Dies", "rei\u00b7che", "Gut", ",", "zum", "Ge\u00b7ben", "dir", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was brauchst du unbelohnter Wuchersmann", "tokens": ["Was", "brauchst", "du", "un\u00b7be\u00b7lohn\u00b7ter", "Wu\u00b7chers\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Summen h\u00f6chste Summ', und kannst nicht leben?", "tokens": ["Der", "Sum\u00b7men", "h\u00f6chs\u00b7te", "Summ'", ",", "und", "kannst", "nicht", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KON", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn handeltreibend nur mit dir allein,", "tokens": ["Denn", "han\u00b7del\u00b7trei\u00b7bend", "nur", "mit", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Beraubst du seiner selbst dein holdes Ich:", "tokens": ["Be\u00b7raubst", "du", "sei\u00b7ner", "selbst", "dein", "hol\u00b7des", "Ich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADV", "PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie kann dann deine Rechnung richtig sein,", "tokens": ["Wie", "kann", "dann", "dei\u00b7ne", "Rech\u00b7nung", "rich\u00b7tig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wenn einst Natur gebietet \u00fcber dich?", "tokens": ["Wenn", "einst", "Na\u00b7tur", "ge\u00b7bie\u00b7tet", "\u00fc\u00b7ber", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sch\u00f6nheit, die du nicht brauchst, lischt mit dir aus;", "tokens": ["Sch\u00f6n\u00b7heit", ",", "die", "du", "nicht", "brauchst", ",", "lischt", "mit", "dir", "aus", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Gebraucht, bestellt sie hinter dir dein Haus.", "tokens": ["Ge\u00b7braucht", ",", "be\u00b7stellt", "sie", "hin\u00b7ter", "dir", "dein", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}