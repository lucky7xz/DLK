{"dta.poem.7267": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Vogelh\u00fctte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Schall \u2014 und wieder \u2014 wieder \u2014 was ist das?", "tokens": ["Ein", "Schall", "und", "wie\u00b7der", "wie\u00b7der", "was", "ist", "das", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ADV", "$(", "ADV", "$(", "PWS", "VAFIN", "PDS", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Bei Gott, das Schlo\u00df! Da schl\u00e4gt es Acht im Thurme \u2014", "tokens": ["Bei", "Gott", ",", "das", "Schlo\u00df", "!", "Da", "schl\u00e4gt", "es", "Acht", "im", "Thur\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "CARD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weh mein Gedicht! o weh mir armem Wurme,", "tokens": ["Weh", "mein", "Ge\u00b7dicht", "!", "o", "weh", "mir", "ar\u00b7mem", "Wur\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "FM", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun f\u00e4llt mir alles ein, was ich verga\u00df!", "tokens": ["Nun", "f\u00e4llt", "mir", "al\u00b7les", "ein", ",", "was", "ich", "ver\u00b7ga\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein Hut, mein Hammer, hurtig fortgetrabt \u2014", "tokens": ["Mein", "Hut", ",", "mein", "Ham\u00b7mer", ",", "hur\u00b7tig", "fort\u00b7ge\u00b7trabt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vielleicht, vielleicht ist man discret gewesen,", "tokens": ["Viel\u00b7leicht", ",", "viel\u00b7leicht", "ist", "man", "di\u00b7scret", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PIS", "VVFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und harrte meiner, der sein Federlesen", "tokens": ["Und", "harr\u00b7te", "mei\u00b7ner", ",", "der", "sein", "Fe\u00b7der\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Inde\u00df mit Kraut und W\u00fcrmern hat gehabt. \u2014", "tokens": ["In\u00b7de\u00df", "mit", "Kraut", "und", "W\u00fcr\u00b7mern", "hat", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VAFIN", "VAPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nun kommt der Steeg und nun des Teiches Ried,", "tokens": ["Nun", "kommt", "der", "Steeg", "und", "nun", "des", "Tei\u00b7ches", "Ried", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nun steigen der Alleen schlanke Streifen;", "tokens": ["Nun", "stei\u00b7gen", "der", "Al\u00b7leen", "schlan\u00b7ke", "Strei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ich wei\u00df es nicht, ich kann es nicht begreifen,", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "ich", "kann", "es", "nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wie ich so g\u00e4nzlich mich vom Leben schied \u2014", "tokens": ["Wie", "ich", "so", "g\u00e4nz\u00b7lich", "mich", "vom", "Le\u00b7ben", "schied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch freilich \u2014 damals war ich Eremit!", "tokens": ["Doch", "frei\u00b7lich", "da\u00b7mals", "war", "ich", "E\u00b7re\u00b7mit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}