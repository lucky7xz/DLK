{"dta.poem.23323": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vaterl\u00e4ndischer Rosenkranz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lieber todt als ein Verr\u00e4ther!", "tokens": ["Lie\u00b7ber", "todt", "als", "ein", "Ver\u00b7r\u00e4\u00b7ther", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber todt als schlechtgesinnt!", "tokens": ["Lie\u00b7ber", "todt", "als", "schlecht\u00b7ge\u00b7sinnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehet fest, ihr Volksvertreter,", "tokens": ["Ste\u00b7het", "fest", ",", "ihr", "Volks\u00b7ver\u00b7tre\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil der Tag des Kampfs beginnt!", "tokens": ["Weil", "der", "Tag", "des", "Kampfs", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaar der Guten, auf! erwache!", "tokens": ["Schaar", "der", "Gu\u00b7ten", ",", "auf", "!", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PTKVZ", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stelle dich in unsre Reih'n!", "tokens": ["Stel\u00b7le", "dich", "in", "uns\u00b7re", "Reih'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn des Volkes gute Sache", "tokens": ["Denn", "des", "Vol\u00b7kes", "gu\u00b7te", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df der Guten Sache sein.", "tokens": ["Mu\u00df", "der", "Gu\u00b7ten", "Sa\u00b7che", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stehet fest, ihr Volksvertreter,", "tokens": ["Ste\u00b7het", "fest", ",", "ihr", "Volks\u00b7ver\u00b7tre\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Tag des Lohns beginnt!", "tokens": ["Da\u00df", "der", "Tag", "des", "Lohns", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieber todt als ein Verr\u00e4ther!", "tokens": ["Lie\u00b7ber", "todt", "als", "ein", "Ver\u00b7r\u00e4\u00b7ther", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber todt als schlechtgesinnt!", "tokens": ["Lie\u00b7ber", "todt", "als", "schlecht\u00b7ge\u00b7sinnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}