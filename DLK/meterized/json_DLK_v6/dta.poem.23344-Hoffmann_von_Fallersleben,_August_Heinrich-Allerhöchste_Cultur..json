{"dta.poem.23344": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allerh\u00f6chste Cultur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar immer tragen wir noch Kleider", "tokens": ["Zwar", "im\u00b7mer", "tra\u00b7gen", "wir", "noch", "Klei\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gehn in Stiefeln und in Schuh'n,", "tokens": ["Und", "gehn", "in", "Stie\u00b7feln", "und", "in", "Schuh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo aber sind anjetzt die Schneider?", "tokens": ["Wo", "a\u00b7ber", "sind", "an\u00b7jetzt", "die", "Schnei\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ADV", "ART", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wo aber sind die Schuster nun?", "tokens": ["Wo", "a\u00b7ber", "sind", "die", "Schus\u00b7ter", "nun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Schuster einst von Gottes Gnaden", "tokens": ["Ein", "Schus\u00b7ter", "einst", "von", "Got\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hei\u00dft jetzt ein Stiefelfabrikant,", "tokens": ["Hei\u00dft", "jetzt", "ein", "Stie\u00b7fel\u00b7fab\u00b7ri\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eines Schneiders H\u00f6ll' und Laden", "tokens": ["Und", "ei\u00b7nes", "Schnei\u00b7ders", "H\u00f6ll'", "und", "La\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird jetzt ein Magazin genannt.", "tokens": ["Wird", "jetzt", "ein", "Ma\u00b7ga\u00b7zin", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So werden wir denn noch erleben", "tokens": ["So", "wer\u00b7den", "wir", "denn", "noch", "er\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kleideranpassungs-Bureau", "tokens": ["Ein", "Klei\u00b7deran\u00b7pas\u00b7sungs\u00b7Bu\u00b7reau"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ganz gewi\u00df auch noch daneben", "tokens": ["Und", "ganz", "ge\u00b7wi\u00df", "auch", "noch", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Fu\u00dfbedeckungsst\u00fcck-Depot.", "tokens": ["Ein", "Fu\u00df\u00b7be\u00b7de\u00b7ckungs\u00b7st\u00fc\u00b7ck\u00b7De\u00b7pot", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}}}}}