{"dta.poem.19170": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Kraut das Allmannsharnisch wird genannt;", "tokens": ["Es", "ist", "ein", "Kraut", "das", "All\u00b7manns\u00b7har\u00b7nisch", "wird", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer's an sich tr\u00e4gt, der siegt, wo er wird angerannt.", "tokens": ["Wer's", "an", "sich", "tr\u00e4gt", ",", "der", "siegt", ",", "wo", "er", "wird", "an\u00b7ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PRF", "VVFIN", "$,", "PRELS", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Aberglaube sucht das Kraut auf Feld und Wiese,", "tokens": ["Der", "A\u00b7berg\u00b7lau\u00b7be", "sucht", "das", "Kraut", "auf", "Feld", "und", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kommts dem Menschen nur herab vom Paradiese.", "tokens": ["Doch", "kommts", "dem", "Men\u00b7schen", "nur", "her\u00b7ab", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Gottbewu\u00dftseyn ists, das droben ist zu Haus,", "tokens": ["Das", "Gott\u00b7be\u00b7wu\u00df\u00b7tseyn", "ists", ",", "das", "dro\u00b7ben", "ist", "zu", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADV", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist der Straus mit dem du siegst in jedem Straus.", "tokens": ["Das", "ist", "der", "Straus", "mit", "dem", "du", "siegst", "in", "je\u00b7dem", "Straus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}