{"textgrid.poem.57489": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine vornehme Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Gl\u00fcck ist offenbar,", "tokens": ["Dein", "Gl\u00fcck", "ist", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vergn\u00fcgtes Hochzeitpaar!", "tokens": ["Ver\u00b7gn\u00fcg\u00b7tes", "Hoch\u00b7zeit\u00b7paar", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gl\u00fcck zu den sch\u00f6nen Flammen!", "tokens": ["Gl\u00fcck", "zu", "den", "sch\u00f6\u00b7nen", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wer kann die Glut verdammen?", "tokens": ["Wer", "kann", "die", "Glut", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Funken eurer Liebe", "tokens": ["Die", "Fun\u00b7ken", "eu\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Entstehn ganz sonnenklar", "tokens": ["Ent\u00b7stehn", "ganz", "son\u00b7nen\u00b7klar"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Selbst von des Himmels Triebe.", "tokens": ["Selbst", "von", "des", "Him\u00b7mels", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein Gl\u00fcck etc.", "tokens": ["Dein", "Gl\u00fcck", "et\u00b7c."], "token_info": ["word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "ADV"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "So stimmet einer Freundinn Pflicht,", "tokens": ["So", "stim\u00b7met", "ei\u00b7ner", "Freun\u00b7dinn", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey deiner Hochzeitfackeln Licht,", "tokens": ["Bey", "dei\u00b7ner", "Hoch\u00b7zeit\u00b7fac\u00b7keln", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geehrtes Paar! den frohen Gl\u00fcckwunsch an;", "tokens": ["Ge\u00b7ehr\u00b7tes", "Paar", "!", "den", "fro\u00b7hen", "Gl\u00fcck\u00b7wunsch", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil sie die Lust nicht bergen kann.", "tokens": ["Weil", "sie", "die", "Lust", "nicht", "ber\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du kennst die Wirkung wahrer Treue,", "tokens": ["Du", "kennst", "die", "Wir\u00b7kung", "wah\u00b7rer", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den unverf\u00e4lschten Sinn,", "tokens": ["Den", "un\u00b7ver\u00b7f\u00e4lschten", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Womit ich dir ergeben bin;", "tokens": ["Wo\u00b7mit", "ich", "dir", "er\u00b7ge\u00b7ben", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wenn ich mich bey deinem Wohl erfreue:", "tokens": ["Und", "wenn", "ich", "mich", "bey", "dei\u00b7nem", "Wohl", "er\u00b7freu\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So sollen itzo Reim und Seyten", "tokens": ["So", "sol\u00b7len", "it\u00b7zo", "Reim", "und", "Sey\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Ausdruck meiner Lust begleiten.", "tokens": ["Den", "Aus\u00b7druck", "mei\u00b7ner", "Lust", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kommt, ihr Nymphen! sinnt auf T\u00e4nze,", "tokens": ["Kommt", ",", "ihr", "Nym\u00b7phen", "!", "sinnt", "auf", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flora, komm und winde Kr\u00e4nze,", "tokens": ["Flo\u00b7ra", ",", "komm", "und", "win\u00b7de", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bunte Kr\u00e4nze f\u00fcr die Braut.", "tokens": ["Bun\u00b7te", "Kr\u00e4n\u00b7ze", "f\u00fcr", "die", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fccke Haupt und Brust nicht minder", "tokens": ["Schm\u00fc\u00b7cke", "Haupt", "und", "Brust", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch den Glanz der Fr\u00fchlingskinder,", "tokens": ["Durch", "den", "Glanz", "der", "Fr\u00fch\u00b7lings\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als man Tellus prangen schaut.", "tokens": ["Als", "man", "Tel\u00b7lus", "pran\u00b7gen", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NE", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch was bedarfst du fremden Schmuck?", "tokens": ["Doch", "was", "be\u00b7darfst", "du", "frem\u00b7den", "Schmuck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die blumengleiche Jugend,", "tokens": ["Die", "blu\u00b7men\u00b7glei\u00b7che", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sch\u00f6nheit und die Tugend", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "und", "die", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind dir, belobte Braut! genug.", "tokens": ["Sind", "dir", ",", "be\u00b7lob\u00b7te", "Braut", "!", "ge\u00b7nug", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Liebster sieht nicht auf geborgten Schimmer;", "tokens": ["Dein", "Liebs\u00b7ter", "sieht", "nicht", "auf", "ge\u00b7borg\u00b7ten", "Schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein wohlgezognes Frauenzimmer", "tokens": ["Ein", "wohl\u00b7ge\u00b7zog\u00b7nes", "Frau\u00b7en\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vergn\u00fcgt ihn mehr in dir,", "tokens": ["Ver\u00b7gn\u00fcgt", "ihn", "mehr", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als alle weitgesuchte Zier:", "tokens": ["Als", "al\u00b7le", "weit\u00b7ge\u00b7such\u00b7te", "Zier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wer ihn lobt, da\u00df er so wohl gew\u00e4hlet,", "tokens": ["Und", "wer", "ihn", "lobt", ",", "da\u00df", "er", "so", "wohl", "ge\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der lobet allemal", "tokens": ["Der", "lo\u00b7bet", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Auch deinerseits die Wahl;", "tokens": ["Auch", "dei\u00b7ner\u00b7seits", "die", "Wahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Indem du dich an ihn verm\u00e4hlet.", "tokens": ["In\u00b7dem", "du", "dich", "an", "ihn", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "So liebet und lebet recht gl\u00fccklich, recht lange,", "tokens": ["So", "lie\u00b7bet", "und", "le\u00b7bet", "recht", "gl\u00fcck\u00b7lich", ",", "recht", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADJD", "ADJD", "$,", "ADV", "ADV", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verbundene Zwey!", "tokens": ["Ver\u00b7bun\u00b7de\u00b7ne", "Zwey", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "CARD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Seyd ewig von Kummer und Ungemach frey.", "tokens": ["Seyd", "e\u00b7wig", "von", "Kum\u00b7mer", "und", "Un\u00b7ge\u00b7mach", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und macht euch hinf\u00fchro was Trauriges bange:", "tokens": ["Und", "macht", "euch", "hin\u00b7f\u00fch\u00b7ro", "was", "Trau\u00b7ri\u00b7ges", "ban\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PWS", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "So denket der Hochzeit, und lachet dabey.", "tokens": ["So", "den\u00b7ket", "der", "Hoch\u00b7zeit", ",", "und", "la\u00b7chet", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PAV", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So liebet und lebet recht gl\u00fccklich, recht lange!", "tokens": ["So", "lie\u00b7bet", "und", "le\u00b7bet", "recht", "gl\u00fcck\u00b7lich", ",", "recht", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADJD", "ADJD", "$,", "ADV", "ADV", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Verbundene Zwey!", "tokens": ["Ver\u00b7bun\u00b7de\u00b7ne", "Zwey", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "CARD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}