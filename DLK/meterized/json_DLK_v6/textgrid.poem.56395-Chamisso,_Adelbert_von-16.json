{"textgrid.poem.56395": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "16", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbklug la\u00df er erklingen", "tokens": ["\u00bb", "klug", "la\u00df", "er", "er\u00b7klin\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geklirr der Schl\u00fcssel;", "tokens": ["Ge\u00b7klirr", "der", "Schl\u00fcs\u00b7sel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ein weiblich Gewand", "tokens": ["Ein", "weib\u00b7lich", "Ge\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Umwalle sein Knie;", "tokens": ["Um\u00b7wal\u00b7le", "sein", "Knie", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "La\u00df blinken die Brust ihm", "tokens": ["La\u00df", "blin\u00b7ken", "die", "Brust", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von breiten Juwelen,", "tokens": ["Von", "brei\u00b7ten", "Ju\u00b7we\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Hochget\u00fcrmt und geh\u00fcllt", "tokens": ["Hoch\u00b7ge\u00b7t\u00fcrmt", "und", "ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVPP"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Das Haar ihm auch sein.\u00ab", "tokens": ["Das", "Haar", "ihm", "auch", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VAINF", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}