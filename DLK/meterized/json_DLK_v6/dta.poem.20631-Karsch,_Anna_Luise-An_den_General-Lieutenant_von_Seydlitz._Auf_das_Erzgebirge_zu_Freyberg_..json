{"dta.poem.20631": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den General-Lieutenant von Seydlitz.  \n  Auf das Erzgebirge zu Freyberg .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da liegen sie, versteckt in Felsenl\u00f6chern,                 ", "tokens": ["Da", "lie\u00b7gen", "sie", ",", "ver\u00b7steckt", "in", "Fel\u00b7sen\u00b7l\u00f6\u00b7chern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Feindeshaufen, welche Du", "tokens": ["Die", "Fein\u00b7des\u00b7hau\u00b7fen", ",", "wel\u00b7che", "Du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heruntertriebst, als fl\u00f6g\u2019 aus zehentausend K\u00f6chern", "tokens": ["Her\u00b7un\u00b7ter\u00b7triebst", ",", "als", "fl\u00f6g'", "aus", "ze\u00b7hen\u00b7tau\u00b7send", "K\u00f6\u00b7chern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der Tod auf ihre K\u00f6pfe zu.", "tokens": ["Der", "Tod", "auf", "ih\u00b7re", "K\u00f6p\u00b7fe", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das blasse Schrecken und der Hunger sitzen", "tokens": ["Das", "blas\u00b7se", "Schre\u00b7cken", "und", "der", "Hun\u00b7ger", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf ihrer Stirne. Jeder flucht", "tokens": ["Auf", "ih\u00b7rer", "Stir\u00b7ne", ".", "Je\u00b7der", "flucht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Kriegesgotte nach, der in der Schlacht mit Blitzen", "tokens": ["Dem", "Krie\u00b7ges\u00b7got\u00b7te", "nach", ",", "der", "in", "der", "Schlacht", "mit", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des gro\u00dfen Auges sie gesucht.", "tokens": ["Des", "gro\u00b7\u00dfen", "Au\u00b7ges", "sie", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du aber l\u00e4chelst von der Wange wieder", "tokens": ["Du", "a\u00b7ber", "l\u00e4\u00b7chelst", "von", "der", "Wan\u00b7ge", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Pirrhus Miene; dieser Blick,", "tokens": ["Des", "Pirr\u00b7hus", "Mie\u00b7ne", ";", "die\u00b7ser", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schrecklich vor Dir her den Reuter st\u00fcrzte nieder,", "tokens": ["Der", "schreck\u00b7lich", "vor", "Dir", "her", "den", "Reu\u00b7ter", "st\u00fcrz\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "ADV", "ART", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kam in Dein Auge nicht zur\u00fcck.", "tokens": ["Kam", "in", "Dein", "Au\u00b7ge", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jezt winde von den Schl\u00e4fen an den Degen", "tokens": ["Jezt", "win\u00b7de", "von", "den", "Schl\u00e4\u00b7fen", "an", "den", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des wohlverdienten Lorbeers Pracht,", "tokens": ["Des", "wohl\u00b7ver\u00b7dien\u00b7ten", "Lor\u00b7beers", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eil\u2019 im Myrthenkranz Asterien entgegen,", "tokens": ["Und", "eil'", "im", "Myr\u00b7then\u00b7kranz", "As\u00b7te\u00b7ri\u00b7en", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Liebesg\u00f6ttern Dir gebracht,", "tokens": ["Von", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern", "Dir", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und la\u00df ihr f\u00fchlen, welch ein Ueberwinder", "tokens": ["Und", "la\u00df", "ihr", "f\u00fch\u00b7len", ",", "welch", "ein", "Ue\u00b7ber\u00b7win\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Haupt an ihren Busen neigt,", "tokens": ["Sein", "Haupt", "an", "ih\u00b7ren", "Bu\u00b7sen", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gieb dem Vaterlande m\u00e4nnlich sch\u00f6ne Kinder,", "tokens": ["Und", "gieb", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "m\u00e4nn\u00b7lich", "sch\u00f6\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als h\u00e4tte Mavors sie gezeugt", "tokens": ["Als", "h\u00e4t\u00b7te", "Ma\u00b7vors", "sie", "ge\u00b7zeugt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "NE", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit Jupiters dem Schaum entstiegnem Kinde,", "tokens": ["Mit", "Ju\u00b7pi\u00b7ters", "dem", "Schaum", "ent\u00b7stieg\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das auf der Muschel fuhr daher,", "tokens": ["Das", "auf", "der", "Mu\u00b7schel", "fuhr", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und Huldigungen nahm vom Wallfisch, von dem Winde,", "tokens": ["Und", "Hul\u00b7di\u00b7gun\u00b7gen", "nahm", "vom", "Wall\u00b7fisch", ",", "von", "dem", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihr zu Fu\u00df gefallnem Meer.", "tokens": ["Und", "ihr", "zu", "Fu\u00df", "ge\u00b7fall\u00b7nem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}