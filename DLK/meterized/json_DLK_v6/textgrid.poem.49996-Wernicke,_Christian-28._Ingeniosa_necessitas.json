{"textgrid.poem.49996": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "28. Ingeniosa necessitas", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahr ists! Die Noht lehrt manchem Witz,", "tokens": ["Wahr", "ists", "!", "Die", "Noht", "lehrt", "man\u00b7chem", "Witz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$.", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist seine ", "tokens": ["Ist", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Noht dringt ihm auf die M\u00e4ssigkeit,", "tokens": ["Noht", "dringt", "ihm", "auf", "die", "M\u00e4s\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die ", "tokens": ["Und", "die"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die eine Tugend folgt der andern auf den Fuss,", "tokens": ["Die", "ei\u00b7ne", "Tu\u00b7gend", "folgt", "der", "an\u00b7dern", "auf", "den", "Fuss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein was ist der letzte Schluss?", "tokens": ["Al\u00b7lein", "was", "ist", "der", "letz\u00b7te", "Schluss", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}