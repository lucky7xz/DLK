{"textgrid.poem.61549": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie schmachtet' ich noch j\u00fcngst", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schmachtet' ich noch j\u00fcngst", "tokens": ["Wie", "schmach\u00b7tet'", "ich", "noch", "j\u00fcngst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Um seinetwillen!", "tokens": ["Um", "sei\u00b7net\u00b7wil\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und dennoch wein' ich nun", "tokens": ["Und", "den\u00b7noch", "wein'", "ich", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr mich im stillen.", "tokens": ["F\u00fcr", "mich", "im", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ach, als er heute mich", "tokens": ["Ach", ",", "als", "er", "heu\u00b7te", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So hei\u00df umfangen,", "tokens": ["So", "hei\u00df", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Kam in die Seele mir", "tokens": ["Kam", "in", "die", "See\u00b7le", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein endlos Bangen.", "tokens": ["Ein", "end\u00b7los", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Schluchzend an seinem Hals", "tokens": ["Schluch\u00b7zend", "an", "sei\u00b7nem", "Hals"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Konnt' ich nicht sprechen;", "tokens": ["Konnt'", "ich", "nicht", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mir war's, als wollte was", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "woll\u00b7te", "was"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PWS"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "In mir zerbrechen.", "tokens": ["In", "mir", "zer\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Das h\u00f6chste Gl\u00fcck, so nah,", "tokens": ["Das", "h\u00f6chs\u00b7te", "Gl\u00fcck", ",", "so", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Macht, da\u00df ich bebe \u2013", "tokens": ["Macht", ",", "da\u00df", "ich", "be\u00b7be", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "O Liebster, w\u00fc\u00dftest du,", "tokens": ["O", "Liebs\u00b7ter", ",", "w\u00fc\u00df\u00b7test", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}