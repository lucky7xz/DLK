{"textgrid.poem.38787": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[ihr kindisch spielenden unschuldgen Reime]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kindisch spielenden unschuldgen Reime,", "tokens": ["Ihr", "kin\u00b7disch", "spie\u00b7len\u00b7den", "un\u00b7schuld\u00b7gen", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was zwingt ihr mich mit lockendem Geschw\u00e4tze,", "tokens": ["Was", "zwingt", "ihr", "mich", "mit", "lo\u00b7cken\u00b7dem", "Ge\u00b7schw\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich vertrauend liebend in euch setze,", "tokens": ["Da\u00df", "ich", "ver\u00b7trau\u00b7end", "lie\u00b7bend", "in", "euch", "set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Liebesleid und Lust die zarten Keime?", "tokens": ["Von", "Lie\u00b7be\u00b7sleid", "und", "Lust", "die", "zar\u00b7ten", "Kei\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "La\u00dft ihr aus ihnen wachsen gr\u00fcne B\u00e4ume,", "tokens": ["La\u00dft", "ihr", "aus", "ih\u00b7nen", "wach\u00b7sen", "gr\u00fc\u00b7ne", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sanft Ger\u00e4usch im Alter mich erg\u00f6tze,", "tokens": ["Da\u00df", "sanft", "Ge\u00b7r\u00e4usch", "im", "Al\u00b7ter", "mich", "er\u00b7g\u00f6t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich s\u00fc\u00df erinnernd an des Herzens Sch\u00e4tze,", "tokens": ["Mich", "s\u00fc\u00df", "e\u00b7rin\u00b7nernd", "an", "des", "Her\u00b7zens", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Zweigen spielend meine Jugendtr\u00e4ume?", "tokens": ["In", "Zwei\u00b7gen", "spie\u00b7lend", "mei\u00b7ne", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Seid ihr so fromm und gut, will ich versprechen,", "tokens": ["Seid", "ihr", "so", "fromm", "und", "gut", ",", "will", "ich", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mag Musenkunst und Jugend von mir ziehen,", "tokens": ["Mag", "Mu\u00b7sen\u00b7kunst", "und", "Ju\u00b7gend", "von", "mir", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Blatt aus diesem G\u00f6tterhain zu brechen,", "tokens": ["Kein", "Blatt", "aus", "die\u00b7sem", "G\u00f6t\u00b7ter\u00b7hain", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja jeder Sturm soll diesen Garten fliehen,", "tokens": ["Ja", "je\u00b7der", "Sturm", "soll", "die\u00b7sen", "Gar\u00b7ten", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer lieblos naht, den soll die Rose stechen,", "tokens": ["Wer", "lieb\u00b7los", "naht", ",", "den", "soll", "die", "Ro\u00b7se", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "ART", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir und der Liebsten Duft und Farbe bl\u00fchen.", "tokens": ["Mir", "und", "der", "Liebs\u00b7ten", "Duft", "und", "Far\u00b7be", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}