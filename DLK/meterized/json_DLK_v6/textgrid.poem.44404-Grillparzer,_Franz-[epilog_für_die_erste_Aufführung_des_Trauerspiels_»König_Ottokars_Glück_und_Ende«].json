{"textgrid.poem.44404": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[epilog f\u00fcr die erste Auff\u00fchrung des Trauerspiels \u00bbK\u00f6nig Ottokars Gl\u00fcck und Ende\u00ab]", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sonst im Reich der ", "tokens": ["Wenn", "sonst", "im", "Reich", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sich Bild und Gleichnis sucht, so haben wir gewagt,", "tokens": ["Sich", "Bild", "und", "Gleich\u00b7nis", "sucht", ",", "so", "ha\u00b7ben", "wir", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Nicht blo\u00df zum allgemeinen Sinn des Sch\u00f6nen,", "tokens": ["Nicht", "blo\u00df", "zum", "all\u00b7ge\u00b7mei\u00b7nen", "Sinn", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum ", "tokens": ["Zum"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Ein schweres Wagnis! ob es uns gelang?", "tokens": ["Ein", "schwe\u00b7res", "Wag\u00b7nis", "!", "ob", "es", "uns", "ge\u00b7lang", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir alle, teils geboren unter Euch,", "tokens": ["Wir", "al\u00b7le", ",", "teils", "ge\u00b7bo\u00b7ren", "un\u00b7ter", "Euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "ADV", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Teils eingeb\u00fcrgert hier durch Eure Huld,", "tokens": ["Teils", "ein\u00b7ge\u00b7b\u00fcr\u00b7gert", "hier", "durch", "Eu\u00b7re", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wir f\u00fchlten uns als ", "tokens": ["Wir", "f\u00fchl\u00b7ten", "uns", "als"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Als \u00d6sterreicher sprachen wir. Der Landsmann", "tokens": ["Als", "\u00d6s\u00b7ter\u00b7rei\u00b7cher", "spra\u00b7chen", "wir", ".", "Der", "Lands\u00b7mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nimmt leicht nicht \u00fcbel, was der Landsmann sprach!", "tokens": ["Nimmt", "leicht", "nicht", "\u00fc\u00b7bel", ",", "was", "der", "Lands\u00b7mann", "sprach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKNEG", "ADJD", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und steht von dem, was heut wir dargebracht,", "tokens": ["Und", "steht", "von", "dem", ",", "was", "heut", "wir", "dar\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Euch auch das ", "tokens": ["Euch", "auch", "das"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.14": {"text": "Das segenreiche, selige Ereignis,", "tokens": ["Das", "se\u00b7gen\u00b7rei\u00b7che", ",", "se\u00b7li\u00b7ge", "Er\u00b7eig\u00b7nis", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Das heute noch begl\u00fcckt so uns als Euch,", "tokens": ["Das", "heu\u00b7te", "noch", "be\u00b7gl\u00fcckt", "so", "uns", "als", "Euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "ADV", "PPER", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Gr\u00fcndung jenes Stamms \u2013 allein genug!", "tokens": ["Die", "Gr\u00fcn\u00b7dung", "je\u00b7nes", "Stamms", "\u2013", "al\u00b7lein", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$(", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mich mahnet Ottokar von Hornek, jener Wackre,", "tokens": ["Mich", "mah\u00b7net", "Ot\u00b7to\u00b7kar", "von", "Hor\u00b7nek", ",", "je\u00b7ner", "Wack\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "NE", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der kurz erst hier vor seinem Kaiser stand,", "tokens": ["Der", "kurz", "erst", "hier", "vor", "sei\u00b7nem", "Kai\u00b7ser", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Er mahnt zur rechten Zeit mich abzubrechen:", "tokens": ["Er", "mahnt", "zur", "rech\u00b7ten", "Zeit", "mich", "ab\u00b7zu\u00b7bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Der \u00d6sterreicher denkt, wo andre sprechen!", "tokens": ["Der", "\u00d6s\u00b7ter\u00b7rei\u00b7cher", "denkt", ",", "wo", "and\u00b7re", "spre\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}