{"dta.poem.12678": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Dorinden,  \n Als sie auf dem eise gefallen.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dorinde! du bist noch begl\u00fcckt zu falle kommen,", "tokens": ["Do\u00b7rin\u00b7de", "!", "du", "bist", "noch", "be\u00b7gl\u00fcckt", "zu", "fal\u00b7le", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "ADV", "VVPP", "PTKZU", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil deine jungferschafft nicht schaden hat genommen;", "tokens": ["Weil", "dei\u00b7ne", "jung\u00b7fer\u00b7schafft", "nicht", "scha\u00b7den", "hat", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun nimm dich ja in acht, auf da\u00df die wohlluft-bahn", "tokens": ["Nun", "nimm", "dich", "ja", "in", "acht", ",", "auf", "da\u00df", "die", "wohl\u00b7luft\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "CARD", "$,", "APPR", "KOUS", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dich, durch ihr feurig eis nicht anders f\u00e4llen kan.", "tokens": ["Dich", ",", "durch", "ihr", "feu\u00b7rig", "eis", "nicht", "an\u00b7ders", "f\u00e4l\u00b7len", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPOSAT", "ADJD", "ADJD", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}