{"textgrid.poem.37882": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schildwache Nachtlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich kann und mag nicht fr\u00f6hlich seyn,", "tokens": ["\u00bb", "ich", "kann", "und", "mag", "nicht", "fr\u00f6h\u00b7lich", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alle Leute schlafen,", "tokens": ["Wenn", "al\u00b7le", "Leu\u00b7te", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00df ich wachen,", "tokens": ["So", "mu\u00df", "ich", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mu\u00df traurig seyn.\u00ab", "tokens": ["Mu\u00df", "trau\u00b7rig", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u00bbach Knabe du sollst nicht traurig seyn,", "tokens": ["\u00bb", "ach", "Kna\u00b7be", "du", "sollst", "nicht", "trau\u00b7rig", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will deiner warten", "tokens": ["Will", "dei\u00b7ner", "war\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Im Rosengarten,", "tokens": ["Im", "Ro\u00b7sen\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Im gr\u00fcnen Klee.\u00ab", "tokens": ["Im", "gr\u00fc\u00b7nen", "Klee", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "\u00bbzum gr\u00fcnen Klee, da komm ich nicht,", "tokens": ["\u00bb", "zum", "gr\u00fc\u00b7nen", "Klee", ",", "da", "komm", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Waffengarten", "tokens": ["Zum", "Waf\u00b7fen\u00b7gar\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Voll Helleparten", "tokens": ["Voll", "Hel\u00b7le\u00b7par\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bin ich gestellt.\u00ab", "tokens": ["Bin", "ich", "ge\u00b7stellt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "---+", "measure": "unknown.measure.single"}}, "stanza.4": {"line.1": {"text": "\u00bbstehst du im Feld, so helf dir Gott,", "tokens": ["\u00bb", "stehst", "du", "im", "Feld", ",", "so", "helf", "dir", "Gott", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Gottes Segen", "tokens": ["An", "Got\u00b7tes", "Se\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ist alles gelegen,", "tokens": ["Ist", "al\u00b7les", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wers glauben thut.\u00ab", "tokens": ["Wers", "glau\u00b7ben", "thut", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u00bbwers glauben thut, ist weit davon,", "tokens": ["\u00bb", "wers", "glau\u00b7ben", "thut", ",", "ist", "weit", "da\u00b7von", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVINF", "VVFIN", "$,", "VAFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist ein K\u00f6nig,", "tokens": ["Er", "ist", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Er ist ein Kaiser,", "tokens": ["Er", "ist", "ein", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Er f\u00fchrt den Krieg.\u00ab", "tokens": ["Er", "f\u00fchrt", "den", "Krieg", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Halt! Wer da? Rund! Wer sang zur Stund?", "tokens": ["Halt", "!", "Wer", "da", "?", "Rund", "!", "Wer", "sang", "zur", "Stund", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PWS", "ADV", "$.", "ADJD", "$.", "PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlohrne Feldwacht", "tokens": ["Ver\u00b7lohr\u00b7ne", "Feld\u00b7wacht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sang es um Mitternacht:", "tokens": ["Sang", "es", "um", "Mit\u00b7ter\u00b7nacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bleib mir vom Leib!", "tokens": ["Bleib", "mir", "vom", "Leib", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}