{"textgrid.poem.67041": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An meine Freundinnen", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen! die ihr mein Herz, die ihr mein Schicksal kennt,", "tokens": ["M\u00e4d\u00b7chen", "!", "die", "ihr", "mein", "Herz", ",", "die", "ihr", "mein", "Schick\u00b7sal", "kennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PPER", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und das Auge, das oft Tr\u00e4nen im Tale weint", "tokens": ["Und", "das", "Au\u00b7ge", ",", "das", "oft", "Tr\u00e4\u00b7nen", "im", "Ta\u00b7le", "weint"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "NN", "APPRART", "NN", "VVFIN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In den Stunden des Elends \u2013", "tokens": ["In", "den", "Stun\u00b7den", "des", "E\u00b7lends", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dies mein traurendes Auge seht!", "tokens": ["Dies", "mein", "trau\u00b7ren\u00b7des", "Au\u00b7ge", "seht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "In der Stille der Nacht denket an euch mein Lied,", "tokens": ["In", "der", "Stil\u00b7le", "der", "Nacht", "den\u00b7ket", "an", "euch", "mein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo mein ewiger Gram jeglichen Stundenschlag,", "tokens": ["Wo", "mein", "e\u00b7wi\u00b7ger", "Gram", "jeg\u00b7li\u00b7chen", "Stun\u00b7den\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Welcher n\u00e4her mich bringt dem", "tokens": ["Wel\u00b7cher", "n\u00e4\u00b7her", "mich", "bringt", "dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJD", "PPER", "VVFIN", "ART"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Trauten Grabe, mit Dank begr\u00fc\u00dft.", "tokens": ["Trau\u00b7ten", "Gra\u00b7be", ",", "mit", "Dank", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Aber da\u00df ich mein Herz redlich und treu, und rein", "tokens": ["A\u00b7ber", "da\u00df", "ich", "mein", "Herz", "red\u00b7lich", "und", "treu", ",", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,", "KON", "ADJD"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Im Gewirre der Welt, unter den L\u00e4sterern", "tokens": ["Im", "Ge\u00b7wir\u00b7re", "der", "Welt", ",", "un\u00b7ter", "den", "L\u00e4s\u00b7te\u00b7rern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Treu und rein es behielt, ist", "tokens": ["Treu", "und", "rein", "es", "be\u00b7hielt", ",", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "ADJD", "PPER", "VVFIN", "$,", "VAFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Himmelswonne dem Leidenden.", "tokens": ["Him\u00b7mels\u00b7won\u00b7ne", "dem", "Lei\u00b7den\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "M\u00e4dchen! bleibet auch ihr redlich und rein und treu!", "tokens": ["M\u00e4d\u00b7chen", "!", "blei\u00b7bet", "auch", "ihr", "red\u00b7lich", "und", "rein", "und", "treu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gute Seelen! Vielleicht wartet auf euch ein Los,", "tokens": ["Gu\u00b7te", "See\u00b7len", "!", "Viel\u00b7leicht", "war\u00b7tet", "auf", "euch", "ein", "Los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Das dem meinigen gleicht. Dann", "tokens": ["Das", "dem", "mei\u00b7ni\u00b7gen", "gleicht", ".", "Dann"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "ART", "PPOSS", "VVFIN", "$.", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00e4rkt im Leiden auch euch mein Trost.", "tokens": ["St\u00e4rkt", "im", "Lei\u00b7den", "auch", "euch", "mein", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}