{"dta.poem.11012": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ungewi\u00dfheit  \n auch in Zahlen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie neulich Mathematidor, mit der Gewi\u00dfheit seiner\nZahlen,", "tokens": ["Wie", "neu\u00b7lich", "Ma\u00b7the\u00b7ma\u00b7ti\u00b7dor", ",", "mit", "der", "Ge\u00b7wi\u00df\u00b7heit", "sei\u00b7ner", "Zah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "$,", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Durchaus nicht m\u00fcde werden wollte, sich breit zu machen, und", "tokens": ["Durc\u00b7haus", "nicht", "m\u00fc\u00b7de", "wer\u00b7den", "woll\u00b7te", ",", "sich", "breit", "zu", "ma\u00b7chen", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKNEG", "ADJD", "VAINF", "VMFIN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,", "KON"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Fragt Antidor, ob er wohl w\u00fc\u00dfte, da\u00df \u00f6fters neun und sechs", "tokens": ["Fragt", "An\u00b7ti\u00b7dor", ",", "ob", "er", "wohl", "w\u00fc\u00df\u00b7te", ",", "da\u00df", "\u00f6f\u00b7ters", "neun", "und", "sechs"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "ADV", "CARD", "KON", "CARD"], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "(statt er vermuthlich, neun und sechs sind funfzehn, sagen", "tokens": ["(", "statt", "er", "ver\u00b7muth\u00b7lich", ",", "neun", "und", "sechs", "sind", "funf\u00b7zehn", ",", "sa\u00b7gen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "APPR", "PPER", "ADV", "$,", "CARD", "KON", "CARD", "VAFIN", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie erster nun dar\u00fcber stutzt, und meynte, jener m\u00fc\u00dfte", "tokens": ["Wie", "ers\u00b7ter", "nun", "da\u00b7r\u00fc\u00b7ber", "stutzt", ",", "und", "meyn\u00b7te", ",", "je\u00b7ner", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "PAV", "VVFIN", "$,", "KON", "VVFIN", "$,", "PDS", "VMFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Sprach Antidor: Er d\u00fcrfte nur die Nacht-und Tages-Stun-", "tokens": ["Sprach", "An\u00b7ti\u00b7dor", ":", "Er", "d\u00fcrf\u00b7te", "nur", "die", "Nacht\u00b7\u00b7und", "Ta\u00b7ges\u00b7Stun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PPER", "VMFIN", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}