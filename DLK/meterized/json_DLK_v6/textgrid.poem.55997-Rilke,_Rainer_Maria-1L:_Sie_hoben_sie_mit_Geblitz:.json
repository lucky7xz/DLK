{"textgrid.poem.55997": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie hoben sie mit Geblitz:", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hoben sie mit Geblitz:", "tokens": ["Sie", "ho\u00b7ben", "sie", "mit", "Ge\u00b7blitz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "der Sturm aus den H\u00f6rnern schwellte", "tokens": ["der", "Sturm", "aus", "den", "H\u00f6r\u00b7nern", "schwell\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "seidene, breitgewellte", "tokens": ["sei\u00b7de\u00b7ne", ",", "breit\u00b7ge\u00b7well\u00b7te"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Fahnen. Der herrlich Erhellte", "tokens": ["Fah\u00b7nen", ".", "Der", "herr\u00b7lich", "Er\u00b7hell\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJD", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "nahm im hochoffenen Zelte,", "tokens": ["nahm", "im", "hoch\u00b7of\u00b7fe\u00b7nen", "Zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "das jauchzendes Volk umstellte,", "tokens": ["das", "jauch\u00b7zen\u00b7des", "Volk", "um\u00b7stell\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "zehn Frauen in Besitz,", "tokens": ["zehn", "Frau\u00b7en", "in", "Be\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "die (gewohnt an des alternden F\u00fcrsten", "tokens": ["die", "(", "ge\u00b7wohnt", "an", "des", "al\u00b7tern\u00b7den", "F\u00fcrs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "sparsame Nacht und Tat)", "tokens": ["spar\u00b7sa\u00b7me", "Nacht", "und", "Tat", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "unter seinem D\u00fcrsten", "tokens": ["un\u00b7ter", "sei\u00b7nem", "D\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "wogten wie Sommersaat.", "tokens": ["wog\u00b7ten", "wie", "Som\u00b7mer\u00b7saat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Dann trat er heraus zum Rate.", "tokens": ["Dann", "trat", "er", "he\u00b7raus", "zum", "Ra\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "wie vermindert um nichts,", "tokens": ["wie", "ver\u00b7min\u00b7dert", "um", "nichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "PIS", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "und jeder, der ihm nahte,", "tokens": ["und", "je\u00b7der", ",", "der", "ihm", "nah\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "erblindete seines Lichts.", "tokens": ["er\u00b7blin\u00b7de\u00b7te", "sei\u00b7nes", "Lichts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "So zog er auch den Heeren", "tokens": ["So", "zog", "er", "auch", "den", "Hee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "voran wie ein Stern dem Jahr;", "tokens": ["vo\u00b7ran", "wie", "ein", "Stern", "dem", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00fcber allen Speeren", "tokens": ["\u00fc\u00b7ber", "al\u00b7len", "Spee\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "wehte sein warmes Haar,", "tokens": ["weh\u00b7te", "sein", "war\u00b7mes", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "das der Helm nicht fa\u00dfte,", "tokens": ["das", "der", "Helm", "nicht", "fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "und das er manchmal ha\u00dfte,", "tokens": ["und", "das", "er", "manch\u00b7mal", "ha\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "weil es schwerer war", "tokens": ["weil", "es", "schwe\u00b7rer", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "als seine reichsten Kleider.", "tokens": ["als", "sei\u00b7ne", "reichs\u00b7ten", "Klei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig hatte geboten,", "tokens": ["Der", "K\u00f6\u00b7nig", "hat\u00b7te", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "da\u00df man den Sch\u00f6nen schone.", "tokens": ["da\u00df", "man", "den", "Sch\u00f6\u00b7nen", "scho\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch man sah ihn ohne", "tokens": ["Doch", "man", "sah", "ihn", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Helm an den bedrohten", "tokens": ["Helm", "an", "den", "be\u00b7droh\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Orten die \u00e4rgsten Knoten", "tokens": ["Or\u00b7ten", "die", "\u00e4rgs\u00b7ten", "Kno\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "zu roten St\u00fccken von Toten", "tokens": ["zu", "ro\u00b7ten", "St\u00fc\u00b7cken", "von", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "auseinanderhaun.", "tokens": ["aus\u00b7ein\u00b7an\u00b7der\u00b7haun", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Dann wu\u00dfte lange keiner", "tokens": ["Dann", "wu\u00df\u00b7te", "lan\u00b7ge", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "von ihm, bis pl\u00f6tzlich einer", "tokens": ["von", "ihm", ",", "bis", "pl\u00f6tz\u00b7lich", "ei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KOUS", "ADJD", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "schrie: Er h\u00e4ngt dort hinten", "tokens": ["schrie", ":", "Er", "h\u00e4ngt", "dort", "hin\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "an den Terebinthen", "tokens": ["an", "den", "Te\u00b7re\u00b7bin\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "mit hochgezogenen Braun.", "tokens": ["mit", "hoch\u00b7ge\u00b7zo\u00b7ge\u00b7nen", "Braun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Das war genug des Winks.", "tokens": ["Das", "war", "ge\u00b7nug", "des", "Winks", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Joab, wie ein J\u00e4ger,", "tokens": ["Joab", ",", "wie", "ein", "J\u00e4\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "ersp\u00e4hte das Haar \u2013: ein schr\u00e4ger", "tokens": ["er\u00b7sp\u00e4h\u00b7te", "das", "Haar", "\u2013", ":", "ein", "schr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$(", "$.", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "gedrehter Ast: da hings.", "tokens": ["ge\u00b7dreh\u00b7ter", "Ast", ":", "da", "hings", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er durchrannte den schlanken Kl\u00e4ger,", "tokens": ["Er", "durch\u00b7rann\u00b7te", "den", "schlan\u00b7ken", "Kl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "und seine Waffentr\u00e4ger", "tokens": ["und", "sei\u00b7ne", "Waf\u00b7fen\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "durchbohrten ihn rechts und links.", "tokens": ["durch\u00b7bohr\u00b7ten", "ihn", "rechts", "und", "links", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}