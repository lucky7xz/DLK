{"dta.poem.12432": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Maria .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ey wie so einsam, wie so geschwind?               ", "tokens": ["Ey", "wie", "so", "ein\u00b7sam", ",", "wie", "so", "ge\u00b7schwind", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "$,", "PWAV", "ADV", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Jungfrau Maria nicht so eile;", "tokens": ["Jung\u00b7frau", "Ma\u00b7ria", "nicht", "so", "ei\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringfertig, wacker, als wie der Wind,", "tokens": ["Ring\u00b7fer\u00b7tig", ",", "wa\u00b7cker", ",", "als", "wie", "der", "Wind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ach, warum l\u00e4st dir nicht der Weile?", "tokens": ["Ach", ",", "wa\u00b7rum", "l\u00e4st", "dir", "nicht", "der", "Wei\u00b7le", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hoch sind die Berg, sehr rauh ist der Weg,", "tokens": ["Hoch", "sind", "die", "Berg", ",", "sehr", "rauh", "ist", "der", "Weg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Dazu auch manche lange Meile,", "tokens": ["Da\u00b7zu", "auch", "man\u00b7che", "lan\u00b7ge", "Mei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zart sind die F\u00fc\u00df, gibt oft schmale Steg,", "tokens": ["Zart", "sind", "die", "F\u00fc\u00df", ",", "gibt", "oft", "schma\u00b7le", "Steg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Jungfrau Maria nicht so eile.", "tokens": ["Jung\u00b7frau", "Ma\u00b7ria", "nicht", "so", "ei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201ewarum so einsam und so geschwind,", "tokens": ["\u201e", "wa\u00b7rum", "so", "ein\u00b7sam", "und", "so", "ge\u00b7schwind", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u201ewill ich dir herzlich gern anzeigen,", "tokens": ["\u201e", "will", "ich", "dir", "herz\u00b7lich", "gern", "an\u00b7zei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eweil du mich fragst mein liebes Kind,", "tokens": ["\u201e", "weil", "du", "mich", "fragst", "mein", "lie\u00b7bes", "Kind", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ewill ich die Ursach nicht verschweigen,", "tokens": ["\u201e", "will", "ich", "die", "Ur\u00b7sach", "nicht", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ejungfrauen wills geb\u00fchren gar nicht", "tokens": ["\u201e", "jung\u00b7frau\u00b7en", "wills", "ge\u00b7b\u00fch\u00b7ren", "gar", "nicht"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "\u201eviel untern Leuten umzuziehen,", "tokens": ["\u201e", "viel", "un\u00b7tern", "Leu\u00b7ten", "um\u00b7zu\u00b7zie\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eeben darum viel B\u00f6ses geschicht,", "tokens": ["\u201e", "e\u00b7ben", "da\u00b7rum", "viel", "B\u00f6\u00b7ses", "ge\u00b7schicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PAV", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "\u201eweil sie die Leut bey Zeit nicht fliehen.", "tokens": ["\u201e", "weil", "sie", "die", "Leut", "bey", "Zeit", "nicht", "flie\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201edurch das Geb\u00fcrg \u00fcber Berg und Thal,", "tokens": ["\u201e", "durch", "das", "Ge\u00b7b\u00fcrg", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u201ethut sich mein Geist in Gott erschwingen,", "tokens": ["\u201e", "thut", "sich", "mein", "Geist", "in", "Gott", "er\u00b7schwin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eals wie ein himmlische Nachtigal", "tokens": ["\u201e", "als", "wie", "ein", "himm\u00b7li\u00b7sche", "Nach\u00b7ti\u00b7gal"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201eich das Magnifikat thu singen,", "tokens": ["\u201e", "ich", "das", "Mag\u00b7ni\u00b7fi\u00b7kat", "thu", "sin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "\u201ewer gern allein ist, und betet gern,", "tokens": ["\u201e", "wer", "gern", "al\u00b7lein", "ist", ",", "und", "be\u00b7tet", "gern", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "ADV", "VAFIN", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u201eder thut sein Zeit gar sch\u00f6n zubringen.\u201c", "tokens": ["\u201e", "der", "thut", "sein", "Zeit", "gar", "sch\u00f6n", "zu\u00b7brin\u00b7gen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mensch, unser Frauen die Kunst ablern!", "tokens": ["Mensch", ",", "un\u00b7ser", "Frau\u00b7en", "die", "Kunst", "ab\u00b7lern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Gott geb, da\u00df dir es m\u00f6g gelingen.", "tokens": ["Gott", "geb", ",", "da\u00df", "dir", "es", "m\u00f6g", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}