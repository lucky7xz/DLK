{"textgrid.poem.41828": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Immer wieder zu erbl\u00fchen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer wieder zu erbl\u00fchen,", "tokens": ["Im\u00b7mer", "wie\u00b7der", "zu", "er\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwingt man jetzt die sch\u00f6nsten Rosen", "tokens": ["Zwingt", "man", "jetzt", "die", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Farbenpr\u00e4chtig zu ergl\u00fchen,", "tokens": ["Far\u00b7ben\u00b7pr\u00e4ch\u00b7tig", "zu", "er\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4hlt sich gern die riesengro\u00dfen.", "tokens": ["W\u00e4hlt", "sich", "gern", "die", "rie\u00b7sen\u00b7gro\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Centifolien! veraltet", "tokens": ["Cen\u00b7ti\u00b7fo\u00b7li\u00b7en", "!", "ver\u00b7al\u00b7tet"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$.", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nennt man Euch, im neuen Garten", "tokens": ["Nennt", "man", "Euch", ",", "im", "neu\u00b7en", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo Ihr siegend einst gewaltet", "tokens": ["Wo", "Ihr", "sie\u00b7gend", "einst", "ge\u00b7wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag man Eurer kaum noch warten.", "tokens": ["Mag", "man", "Eu\u00b7rer", "kaum", "noch", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn Ihr la\u00dft's Euch nicht gefallen,", "tokens": ["Denn", "Ihr", "la\u00dft's", "Euch", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchend Euch zu wiederholen,", "tokens": ["Bl\u00fc\u00b7hend", "Euch", "zu", "wie\u00b7der\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Sommermonden allen,", "tokens": ["In", "den", "Som\u00b7mer\u00b7mon\u00b7den", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie's des G\u00e4rtners Kunst befohlen,", "tokens": ["Wie's", "des", "G\u00e4rt\u00b7ners", "Kunst", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seid Euch gleich und keusch geblieben", "tokens": ["Seid", "Euch", "gleich", "und", "keusch", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "ADV", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hundertbl\u00e4ttrig in der Runde,", "tokens": ["Hun\u00b7dert\u00b7bl\u00e4tt\u00b7rig", "in", "der", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ernst verschlossen falschen Trieben,", "tokens": ["Ernst", "ver\u00b7schlos\u00b7sen", "fal\u00b7schen", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebt Ihr echter Liebe Kunde!", "tokens": ["Gebt", "Ihr", "ech\u00b7ter", "Lie\u00b7be", "Kun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebe, die zum Wiederholen,", "tokens": ["Lie\u00b7be", ",", "die", "zum", "Wie\u00b7der\u00b7ho\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie und nirgend sich verstanden \u2013", "tokens": ["Nie", "und", "nir\u00b7gend", "sich", "ver\u00b7stan\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "PRF", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heutzutag doch wirds befohlen:", "tokens": ["Heut\u00b7zu\u00b7tag", "doch", "wirds", "be\u00b7foh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Modisch sind die Remontanten.", "tokens": ["Mo\u00b7disch", "sind", "die", "Re\u00b7mon\u00b7tan\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wen'ge nur die jetzt noch hegen", "tokens": ["Wen'\u00b7ge", "nur", "die", "jetzt", "noch", "he\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene alten Rosenb\u00e4ume,", "tokens": ["Je\u00b7ne", "al\u00b7ten", "Ro\u00b7sen\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denen treuer, Liebessegen,", "tokens": ["De\u00b7nen", "treu\u00b7er", ",", "Lie\u00b7bes\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht geh\u00f6rt in's Reich der Tr\u00e4ume.", "tokens": ["Nicht", "ge\u00b7h\u00f6rt", "in's", "Reich", "der", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}