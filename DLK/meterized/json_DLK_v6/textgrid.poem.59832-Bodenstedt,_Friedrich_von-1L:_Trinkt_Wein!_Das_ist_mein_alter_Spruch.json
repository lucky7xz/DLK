{"textgrid.poem.59832": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trinkt Wein! Das ist mein alter Spruch", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trinkt Wein! Das ist mein alter Spruch", "tokens": ["Trinkt", "Wein", "!", "Das", "ist", "mein", "al\u00b7ter", "Spruch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wird auch stets mein neuer sein,", "tokens": ["Und", "wird", "auch", "stets", "mein", "neu\u00b7er", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kauft euch der Flasche Weisheitsbuch,", "tokens": ["Kauft", "euch", "der", "Fla\u00b7sche", "Weis\u00b7heits\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sollt' es noch so teuer sein!", "tokens": ["Und", "sollt'", "es", "noch", "so", "teu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als Gott der Herr die Welt erschuf,", "tokens": ["Als", "Gott", "der", "Herr", "die", "Welt", "er\u00b7schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach er: Der Mensch sei K\u00f6nig hier,", "tokens": ["Sprach", "er", ":", "Der", "Mensch", "sei", "K\u00f6\u00b7nig", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ART", "NN", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es soll des Menschen Haupt voll Witz,", "tokens": ["Es", "soll", "des", "Men\u00b7schen", "Haupt", "voll", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es soll sein Trank voll Feuer sein!", "tokens": ["Es", "soll", "sein", "Trank", "voll", "Feu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dies ist der Grund, da\u00df Adam bald", "tokens": ["Dies", "ist", "der", "Grund", ",", "da\u00df", "A\u00b7dam", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Paradies vertrieben ward;", "tokens": ["Vom", "Pa\u00b7ra\u00b7dies", "ver\u00b7trie\u00b7ben", "ward", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er floh den Wein, drum konnt' es ihm", "tokens": ["Er", "floh", "den", "Wein", ",", "drum", "konnt'", "es", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PAV", "VMFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Eden nicht geheuer sein!", "tokens": ["Im", "E\u00b7den", "nicht", "ge\u00b7heu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die ganze Menschheit ward vertilgt,", "tokens": ["Die", "gan\u00b7ze", "Menschheit", "ward", "ver\u00b7tilgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nur Noah blieb mit seinem Haus,", "tokens": ["Nur", "Noah", "blieb", "mit", "sei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Herr sprach: Weil du Wein gebaut,", "tokens": ["Der", "Herr", "sprach", ":", "Weil", "du", "Wein", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sollst du mein Knecht, mein treuer sein.", "tokens": ["Sollst", "du", "mein", "Knecht", ",", "mein", "treu\u00b7er", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Wassertrinker seien jetzt", "tokens": ["Die", "Was\u00b7ser\u00b7trin\u00b7ker", "sei\u00b7en", "jetzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ers\u00e4uft im Wasser allzumal,", "tokens": ["Er\u00b7s\u00e4uft", "im", "Was\u00b7ser", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur du, mein Knecht, sollst aufbewahrt", "tokens": ["Nur", "du", ",", "mein", "Knecht", ",", "sollst", "auf\u00b7be\u00b7wahrt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In h\u00f6lzernem Gem\u00e4uer sein!", "tokens": ["In", "h\u00f6l\u00b7zer\u00b7nem", "Ge\u00b7m\u00e4u\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mirza-Schaffy! Dir ward die Wahl", "tokens": ["Mir\u00b7za\u00b7Schaffy", "!", "Dir", "ward", "die", "Wahl"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In diesem Falle nicht zur Qual;", "tokens": ["In", "die\u00b7sem", "Fal\u00b7le", "nicht", "zur", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast den Wein erk\u00fcrt, willst nie", "tokens": ["Du", "hast", "den", "Wein", "er\u00b7k\u00fcrt", ",", "willst", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wasserungeheuer sein!", "tokens": ["Ein", "Was\u00b7se\u00b7run\u00b7ge\u00b7heu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}