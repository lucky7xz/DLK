{"dta.poem.3196": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leicht ist's, mit der Natur im Einklang dich empfinden,", "tokens": ["Leicht", "ist's", ",", "mit", "der", "Na\u00b7tur", "im", "Ein\u00b7klang", "dich", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie im wonn'gen Schoo\u00df dich wiegt mit weichen Winden.", "tokens": ["Wenn", "sie", "im", "wonn'\u00b7gen", "Schoo\u00df", "dich", "wiegt", "mit", "wei\u00b7chen", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch anders, wenn sie an dich haucht mit eis'gem Sturm,", "tokens": ["Doch", "an\u00b7ders", ",", "wenn", "sie", "an", "dich", "haucht", "mit", "eis'\u00b7gem", "Sturm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schauernd du vor ihr dich kr\u00fcmmest wie ein Wurm.", "tokens": ["Und", "schau\u00b7ernd", "du", "vor", "ihr", "dich", "kr\u00fcm\u00b7mest", "wie", "ein", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "PRF", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann f\u00fchlest du, da\u00df sie das Leben nicht allein,", "tokens": ["Dann", "f\u00fch\u00b7lest", "du", ",", "da\u00df", "sie", "das", "Le\u00b7ben", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tod auch ist, und ihr gleichg\u00fcltig Herz und Stein.", "tokens": ["Der", "Tod", "auch", "ist", ",", "und", "ihr", "gleich\u00b7g\u00fcl\u00b7tig", "Herz", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "KON", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann danke Gott, der dich nicht gab in ihre Macht,", "tokens": ["Dann", "dan\u00b7ke", "Gott", ",", "der", "dich", "nicht", "gab", "in", "ih\u00b7re", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nimm dich k\u00fcnftig auch vor ihrer Huld in Acht.", "tokens": ["Und", "nimm", "dich", "k\u00fcnf\u00b7tig", "auch", "vor", "ih\u00b7rer", "Huld", "in", "Acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}