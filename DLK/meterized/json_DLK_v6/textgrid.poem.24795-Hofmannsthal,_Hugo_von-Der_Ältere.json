{"textgrid.poem.24795": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Der \u00c4ltere", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun meine ich, ist mir ein Ma\u00df geschenkt,", "tokens": ["Nun", "mei\u00b7ne", "ich", ",", "ist", "mir", "ein", "Ma\u00df", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein unver\u00e4nderlich und sichres Ma\u00df", "tokens": ["Ein", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "und", "sich\u00b7res", "Ma\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das mich f\u00fcr immer und untr\u00fcglich abh\u00e4lt,", "tokens": ["Das", "mich", "f\u00fcr", "im\u00b7mer", "und", "un\u00b7tr\u00fcg\u00b7lich", "ab\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADV", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein leeres Ding f\u00fcr voll zu nehmen, mich", "tokens": ["Ein", "lee\u00b7res", "Ding", "f\u00fcr", "voll", "zu", "neh\u00b7men", ",", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "PTKZU", "VVINF", "$,", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr Schales zu vergeuden, fremdem F\u00fchlen", "tokens": ["F\u00fcr", "Scha\u00b7les", "zu", "ver\u00b7geu\u00b7den", ",", "frem\u00b7dem", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und angelerntem Denken irgend Platz", "tokens": ["Und", "an\u00b7ge\u00b7lern\u00b7tem", "Den\u00b7ken", "ir\u00b7gend", "Platz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In einer meiner Adern zu gestatten.", "tokens": ["In", "ei\u00b7ner", "mei\u00b7ner", "A\u00b7dern", "zu", "ge\u00b7stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nun kann zwar Krankheit, Elend oder Tod", "tokens": ["Nun", "kann", "zwar", "Krank\u00b7heit", ",", "E\u00b7lend", "o\u00b7der", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mich noch bedrohen, aber L\u00fcge kaum.", "tokens": ["Mich", "noch", "be\u00b7dro\u00b7hen", ",", "a\u00b7ber", "L\u00fc\u00b7ge", "kaum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dazu ist dies mein neues Amt zu voll", "tokens": ["Da\u00b7zu", "ist", "dies", "mein", "neu\u00b7es", "Amt", "zu", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PDS", "PPOSAT", "ADJA", "NN", "PTKA", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Einfacher Hoheit. Und daran gemessen", "tokens": ["Ein\u00b7fa\u00b7cher", "Ho\u00b7heit", ".", "Und", "da\u00b7ran", "ge\u00b7mes\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "KON", "PAV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Vergeht erlogne Wichtigkeit zu Nichts.", "tokens": ["Ver\u00b7geht", "er\u00b7log\u00b7ne", "Wich\u00b7tig\u00b7keit", "zu", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ins Schlo\u00df gefallen sind die letzten T\u00fcren,", "tokens": ["Ins", "Schlo\u00df", "ge\u00b7fal\u00b7len", "sind", "die", "letz\u00b7ten", "T\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Durch die ich hatte einen schlimmen Weg", "tokens": ["Durch", "die", "ich", "hat\u00b7te", "ei\u00b7nen", "schlim\u00b7men", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Antreten k\u00f6nnen. Durch und durch verst\u00f6rt,", "tokens": ["An\u00b7tre\u00b7ten", "k\u00f6n\u00b7nen", ".", "Durch", "und", "durch", "ver\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$.", "APPR", "KON", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Im Kern beschmutzt und v\u00f6llig irre an G\u00fcte", "tokens": ["Im", "Kern", "be\u00b7schmutzt", "und", "v\u00f6l\u00b7lig", "ir\u00b7re", "an", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "KON", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Werd ich nun nicht mehr. Denn mich hat ein Glanz", "tokens": ["Werd", "ich", "nun", "nicht", "mehr", ".", "Denn", "mich", "hat", "ein", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$.", "KON", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Vom wahren Sinn des Lebens angegl\u00fcht.", "tokens": ["Vom", "wah\u00b7ren", "Sinn", "des", "Le\u00b7bens", "an\u00b7ge\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}