{"textgrid.poem.63164": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komme aus der W\u00fcste,", "tokens": ["Ich", "kom\u00b7me", "aus", "der", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo ich bei der L\u00f6win lag.", "tokens": ["Wo", "ich", "bei", "der", "L\u00f6\u00b7win", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich habe den Schakalen die Knochen aus dem blutigen Gefr\u00e4ss gerissen,", "tokens": ["Ich", "ha\u00b7be", "den", "Scha\u00b7ka\u00b7len", "die", "Kno\u00b7chen", "aus", "dem", "blu\u00b7ti\u00b7gen", "Ge\u00b7fr\u00e4ss", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+---+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Ich bin mit dem Strauss um die Wette gelaufen", "tokens": ["Ich", "bin", "mit", "dem", "Strauss", "um", "die", "Wet\u00b7te", "ge\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und habe dem Fuchs das Junge aus der H\u00f6hle gestohlen.", "tokens": ["Und", "ha\u00b7be", "dem", "Fuchs", "das", "Jun\u00b7ge", "aus", "der", "H\u00f6h\u00b7le", "ge\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NE", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Hei! Hei!", "tokens": ["Hei", "!", "Hei", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Mein Blut saust hinter der Betonstirn", "tokens": ["Mein", "Blut", "saust", "hin\u00b7ter", "der", "Be\u00b7tons\u00b7tirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie der Orinoko.", "tokens": ["Wie", "der", "O\u00b7ri\u00b7no\u00b7ko", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Auf meinen ausgebreiteten H\u00e4nden", "tokens": ["Auf", "mei\u00b7nen", "aus\u00b7ge\u00b7brei\u00b7te\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Trag ich zwei Sterne.", "tokens": ["Trag", "ich", "zwei", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "CARD", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ich stemme Sterne,", "tokens": ["Ich", "stem\u00b7me", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Denn ich bin Mitglied des Athletenklubs S\u00fcdost.", "tokens": ["Denn", "ich", "bin", "Mit\u00b7glied", "des", "Ath\u00b7le\u00b7ten\u00b7klubs", "S\u00fcd\u00b7ost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "ART", "NE", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.13": {"text": "Ich saufe die Milch vom Euter der Kuh", "tokens": ["Ich", "sau\u00b7fe", "die", "Milch", "vom", "Eu\u00b7ter", "der", "Kuh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und von den Zitzen einer Frau,", "tokens": ["Und", "von", "den", "Zit\u00b7zen", "ei\u00b7ner", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Zwillinge warf.", "tokens": ["Die", "Zwil\u00b7lin\u00b7ge", "warf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.16": {"text": "Lasst mich taufreden", "tokens": ["Lasst", "mich", "tauf\u00b7re\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.17": {"text": "Mit euren Faseleien", "tokens": ["Mit", "eu\u00b7ren", "Fa\u00b7se\u00b7lei\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Von der Weisheit der Tapergreise.", "tokens": ["Von", "der", "Weis\u00b7heit", "der", "Ta\u00b7per\u00b7grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Mein Feind.", "tokens": ["Mein", "Feind", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Auf deinem Grabe will ich deine Witwe umarmen.", "tokens": ["Auf", "dei\u00b7nem", "Gra\u00b7be", "will", "ich", "dei\u00b7ne", "Wit\u00b7we", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die von den Verwandten gestifteten Nelkent\u00f6pfe", "tokens": ["Die", "von", "den", "Ver\u00b7wand\u00b7ten", "ge\u00b7stif\u00b7te\u00b7ten", "Nel\u00b7ken\u00b7t\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Sollen zerscherben.", "tokens": ["Sol\u00b7len", "zer\u00b7scher\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und das Holzkreuz,", "tokens": ["Und", "das", "Holz\u00b7kreuz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Auf dem dein ver\u00e4chtlicher Name steht,", "tokens": ["Auf", "dem", "dein", "ver\u00b7\u00e4cht\u00b7li\u00b7cher", "Na\u00b7me", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sei zerbrochen", "tokens": ["Sei", "zer\u00b7bro\u00b7chen"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wie die Bundeslade", "tokens": ["Wie", "die", "Bun\u00b7des\u00b7la\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Und die Tontafelbibliothek des Assurbanipal.", "tokens": ["Und", "die", "Ton\u00b7ta\u00b7fel\u00b7bib\u00b7lio\u00b7thek", "des", "As\u00b7sur\u00b7ba\u00b7ni\u00b7pal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}