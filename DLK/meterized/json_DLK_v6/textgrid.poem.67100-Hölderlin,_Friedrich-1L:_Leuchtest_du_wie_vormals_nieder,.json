{"textgrid.poem.67100": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leuchtest du wie vormals nieder,", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leuchtest du wie vormals nieder,", "tokens": ["Leuch\u00b7test", "du", "wie", "vor\u00b7mals", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldner Tag! und sprossen mir", "tokens": ["Gold\u00b7ner", "Tag", "!", "und", "spros\u00b7sen", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Gesanges Blumen wieder", "tokens": ["Des", "Ge\u00b7san\u00b7ges", "Blu\u00b7men", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebenatmend auf zu dir?", "tokens": ["Le\u00b7be\u00b7nat\u00b7mend", "auf", "zu", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie so anders ists geworden!", "tokens": ["Wie", "so", "an\u00b7ders", "ists", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manches, was ich trauernd mied,", "tokens": ["Man\u00b7ches", ",", "was", "ich", "trau\u00b7ernd", "mied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stimmt in freundlichen Akkorden", "tokens": ["Stimmt", "in", "freund\u00b7li\u00b7chen", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nun in meiner Freude Lied,", "tokens": ["Nun", "in", "mei\u00b7ner", "Freu\u00b7de", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und mit jedem Stundenschlage", "tokens": ["Und", "mit", "je\u00b7dem", "Stun\u00b7den\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Werd ich wunderbar gemahnt", "tokens": ["Werd", "ich", "wun\u00b7der\u00b7bar", "ge\u00b7mahnt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "An der Kindheit stille Tage,", "tokens": ["An", "der", "Kind\u00b7heit", "stil\u00b7le", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Seit ich Sie, die Eine, fand.", "tokens": ["Seit", "ich", "Sie", ",", "die", "Ei\u00b7ne", ",", "fand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "PRELS", "ART", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Diotima! edles Leben!", "tokens": ["Dio\u00b7ti\u00b7ma", "!", "ed\u00b7les", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Schwester, heilig mir verwandt!", "tokens": ["Schwes\u00b7ter", ",", "hei\u00b7lig", "mir", "ver\u00b7wandt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh ich dir die Hand gegeben,", "tokens": ["Eh", "ich", "dir", "die", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab ich ferne dich gekannt.", "tokens": ["Hab", "ich", "fer\u00b7ne", "dich", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Damals schon, da ich in Tr\u00e4umen,", "tokens": ["Da\u00b7mals", "schon", ",", "da", "ich", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir entlockt vom heitern Tag,", "tokens": ["Mir", "ent\u00b7lockt", "vom", "hei\u00b7tern", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter meines Gartens B\u00e4umen,", "tokens": ["Un\u00b7ter", "mei\u00b7nes", "Gar\u00b7tens", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein zufriedner Knabe, lag,", "tokens": ["Ein", "zu\u00b7fried\u00b7ner", "Kna\u00b7be", ",", "lag", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da in leiser Lust und Sch\u00f6ne", "tokens": ["Da", "in", "lei\u00b7ser", "Lust", "und", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meiner Seele Mai begann,", "tokens": ["Mei\u00b7ner", "See\u00b7le", "Mai", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "S\u00e4uselte, wie Zephirst\u00f6ne,", "tokens": ["S\u00e4u\u00b7sel\u00b7te", ",", "wie", "Ze\u00b7phir\u00b7st\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "G\u00f6ttliche! dein Geist mich an.", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "!", "dein", "Geist", "mich", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! und da, wie eine Sage,", "tokens": ["Ach", "!", "und", "da", ",", "wie", "ei\u00b7ne", "Sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder frohe Gott mir schwand,", "tokens": ["Je\u00b7der", "fro\u00b7he", "Gott", "mir", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ich vor des Himmels Tage", "tokens": ["Da", "ich", "vor", "des", "Him\u00b7mels", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Darbend, wie ein Blinder, stand,", "tokens": ["Dar\u00b7bend", ",", "wie", "ein", "Blin\u00b7der", ",", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da die Last der Zeit mich beugte,", "tokens": ["Da", "die", "Last", "der", "Zeit", "mich", "beug\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und mein Leben, kalt und bleich,", "tokens": ["Und", "mein", "Le\u00b7ben", ",", "kalt", "und", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sehnend schon hinab sich neigte", "tokens": ["Seh\u00b7nend", "schon", "hin\u00b7ab", "sich", "neig\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Toten stummes Reich:", "tokens": ["In", "der", "To\u00b7ten", "stum\u00b7mes", "Reich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "W\u00fcnscht ich \u00f6fters noch, dem blinden", "tokens": ["W\u00fcnscht", "ich", "\u00f6f\u00b7ters", "noch", ",", "dem", "blin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wanderer, dies Eine mir,", "tokens": ["Wan\u00b7de\u00b7rer", ",", "dies", "Ei\u00b7ne", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "ART", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Meines Herzens Bild zu finden", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "Bild", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bei den Schatten oder hier.", "tokens": ["Bei", "den", "Schat\u00b7ten", "o\u00b7der", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun! ich habe dich gefunden!", "tokens": ["Nun", "!", "ich", "ha\u00b7be", "dich", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner, als ich ahndend sah,", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "ich", "ahn\u00b7dend", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffend in den Feierstunden,", "tokens": ["Hof\u00b7fend", "in", "den", "Fei\u00b7er\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holde Muse! bist du da;", "tokens": ["Hol\u00b7de", "Mu\u00b7se", "!", "bist", "du", "da", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den Himmlischen dort oben,", "tokens": ["Von", "den", "Himm\u00b7li\u00b7schen", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo hinauf die Freude flieht,", "tokens": ["Wo", "hin\u00b7auf", "die", "Freu\u00b7de", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo, des Alterns \u00fcberhoben,", "tokens": ["Wo", ",", "des", "Al\u00b7terns", "\u00fc\u00b7berh\u00b7o\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Immerheitre Sch\u00f6ne bl\u00fcht,", "tokens": ["Im\u00b7mer\u00b7hei\u00b7tre", "Sch\u00f6\u00b7ne", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Scheinst du mir herabgestiegen,", "tokens": ["Scheinst", "du", "mir", "her\u00b7ab\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "G\u00f6tterbotin! weiltest du", "tokens": ["G\u00f6t\u00b7ter\u00b7bo\u00b7tin", "!", "weil\u00b7test", "du"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nun in g\u00fctigem Gen\u00fcgen", "tokens": ["Nun", "in", "g\u00fc\u00b7ti\u00b7gem", "Ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bei dem S\u00e4nger immerzu.", "tokens": ["Bei", "dem", "S\u00e4n\u00b7ger", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sommerglut und Fr\u00fchlingsmilde,", "tokens": ["Som\u00b7mer\u00b7glut", "und", "Fr\u00fch\u00b7lings\u00b7mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streit und Frieden wechselt hier", "tokens": ["Streit", "und", "Frie\u00b7den", "wech\u00b7selt", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem stillen G\u00f6tterbilde", "tokens": ["Vor", "dem", "stil\u00b7len", "G\u00f6t\u00b7ter\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderbar im Busen mir;", "tokens": ["Wun\u00b7der\u00b7bar", "im", "Bu\u00b7sen", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Z\u00fcrnend unter Huldigungen", "tokens": ["Z\u00fcr\u00b7nend", "un\u00b7ter", "Hul\u00b7di\u00b7gun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hab ich oft, besch\u00e4mt, besiegt,", "tokens": ["Hab", "ich", "oft", ",", "be\u00b7sch\u00e4mt", ",", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie zu fassen, schon gerungen,", "tokens": ["Sie", "zu", "fas\u00b7sen", ",", "schon", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die mein K\u00fchnstes \u00fcberfliegt;", "tokens": ["Die", "mein", "K\u00fchns\u00b7tes", "\u00fc\u00b7berf\u00b7liegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Unzufrieden im Gewinne,", "tokens": ["Un\u00b7zu\u00b7frie\u00b7den", "im", "Ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hab ich stolz darob geweint,", "tokens": ["Hab", "ich", "stolz", "da\u00b7rob", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PAV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df zu herrlich meinem Sinne", "tokens": ["Da\u00df", "zu", "herr\u00b7lich", "mei\u00b7nem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKA", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und zu m\u00e4chtig sie erscheint.", "tokens": ["Und", "zu", "m\u00e4ch\u00b7tig", "sie", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! an deine stille Sch\u00f6ne,", "tokens": ["Ach", "!", "an", "dei\u00b7ne", "stil\u00b7le", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selig holdes Angesicht!", "tokens": ["Se\u00b7lig", "hol\u00b7des", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herz! an deine Himmelst\u00f6ne", "tokens": ["Herz", "!", "an", "dei\u00b7ne", "Him\u00b7mel\u00b7st\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist gewohnt das meine nicht;", "tokens": ["Ist", "ge\u00b7wohnt", "das", "mei\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber deine Melodien", "tokens": ["A\u00b7ber", "dei\u00b7ne", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heitern m\u00e4hlig mir den Sinn,", "tokens": ["Hei\u00b7tern", "m\u00e4h\u00b7lig", "mir", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die tr\u00fcben Tr\u00e4ume fliehen,", "tokens": ["Da\u00df", "die", "tr\u00fc\u00b7ben", "Tr\u00e4u\u00b7me", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ich selbst ein andrer bin;", "tokens": ["Und", "ich", "selbst", "ein", "an\u00b7drer", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Bin ich dazu denn erkoren?", "tokens": ["Bin", "ich", "da\u00b7zu", "denn", "er\u00b7ko\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ich zu deiner hohen Ruh,", "tokens": ["Ich", "zu", "dei\u00b7ner", "ho\u00b7hen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "So zu Licht und Lust geboren,", "tokens": ["So", "zu", "Licht", "und", "Lust", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "G\u00f6ttlichgl\u00fcckliche! wie du? \u2013", "tokens": ["G\u00f6tt\u00b7lich\u00b7gl\u00fcck\u00b7li\u00b7che", "!", "wie", "du", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie dein Vater und der meine,", "tokens": ["Wie", "dein", "Va\u00b7ter", "und", "der", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in heitrer Majest\u00e4t", "tokens": ["Der", "in", "hei\u00b7trer", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber seinem Eichenhaine", "tokens": ["\u00dc\u00b7ber", "sei\u00b7nem", "Ei\u00b7chen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort in lichter H\u00f6he geht,", "tokens": ["Dort", "in", "lich\u00b7ter", "H\u00f6\u00b7he", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie er in die Meereswogen,", "tokens": ["Wie", "er", "in", "die", "Mee\u00b7res\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die k\u00fchle Tiefe blaut,", "tokens": ["Wo", "die", "k\u00fch\u00b7le", "Tie\u00b7fe", "blaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Steigend von des Himmels Bogen,", "tokens": ["Stei\u00b7gend", "von", "des", "Him\u00b7mels", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Klar und still herunterschaut:", "tokens": ["Klar", "und", "still", "her\u00b7un\u00b7ter\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "So will ich aus G\u00f6tterh\u00f6hen,", "tokens": ["So", "will", "ich", "aus", "G\u00f6t\u00b7ter\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.10": {"text": "Neu geweiht in sch\u00f6nrem Gl\u00fcck,", "tokens": ["Neu", "ge\u00b7weiht", "in", "sch\u00f6n\u00b7rem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Froh zu singen und zu sehen,", "tokens": ["Froh", "zu", "sin\u00b7gen", "und", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nun zu Sterblichen zur\u00fcck.", "tokens": ["Nun", "zu", "Sterb\u00b7li\u00b7chen", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}