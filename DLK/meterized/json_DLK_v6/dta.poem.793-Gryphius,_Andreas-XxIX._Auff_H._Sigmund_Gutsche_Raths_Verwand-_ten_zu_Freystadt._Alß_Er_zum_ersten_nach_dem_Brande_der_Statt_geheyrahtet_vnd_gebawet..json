{"dta.poem.793": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.  \n Auff H. Sigmund Gutsche/ Raths Verwand-  \n ten zu Freystadt. Al\u00df Er zum ersten nach dem  \n Brande der Statt geheyrahtet vnd gebawet.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Stellt ewre Schmertzen eyn; ob schon das grimme toben ", "tokens": ["Stellt", "ew\u00b7re", "Schmert\u00b7zen", "eyn", ";", "ob", "schon", "das", "grim\u00b7me", "to\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VAINF", "$.", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der flammen ewre Statt zu Aschen hat gemacht", "tokens": ["Der", "flam\u00b7men", "ew\u00b7re", "Statt", "zu", "A\u00b7schen", "hat", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob Kirch vnd Herdt gleich hin; ob schon in einer Nacht", "tokens": ["Ob", "Kirch", "vnd", "Herdt", "gleich", "hin", ";", "ob", "schon", "in", "ei\u00b7ner", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "PTKVZ", "$.", "KOUS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was so viel zeit gebaw\u2019t in rauch vnd glutt verstoben.", "tokens": ["Was", "so", "viel", "zeit", "ge\u00b7ba\u00b7w't", "in", "rauch", "vnd", "glutt", "ver\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "VVFIN", "APPR", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Ob gleich der Mauren pracht/ der T\u00fcrme last von oben", "tokens": ["Ob", "gleich", "der", "Mau\u00b7ren", "pracht", "/", "der", "T\u00fcr\u00b7me", "last", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun auf die Erde st\u00fcrtzt/ ist Gutsche doch bedacht", "tokens": ["Nun", "auf", "die", "Er\u00b7de", "st\u00fcrtzt", "/", "ist", "Gut\u00b7sche", "doch", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$(", "VAFIN", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie/ was die glutt auf-fra\u00df/ Euch werde widerbracht.", "tokens": ["Wie", "/", "was", "die", "glutt", "auf\u00b7fra\u00df", "/", "Euch", "wer\u00b7de", "wi\u00b7der\u00b7bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "PWS", "ART", "ADJD", "NN", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weyut nicht mehr \u00fcber die/ die wir im Grab auch loben", "tokens": ["We\u00b7yut", "nicht", "mehr", "\u00fc\u00b7ber", "die", "/", "die", "wir", "im", "Grab", "auch", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "ART", "$(", "PRELS", "PPER", "APPRART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Gr\u00fc\u00dft den/ der so mit Rath al\u00df that sein Hau\u00df aufbawt/", "tokens": ["Gr\u00fc\u00dft", "den", "/", "der", "so", "mit", "Rath", "al\u00df", "that", "sein", "Hau\u00df", "auf\u00b7bawt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$(", "ART", "ADV", "APPR", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Vnd mit dem Eh-gemahl/ die gestern jhm vertrawt/", "tokens": ["Vnd", "mit", "dem", "Eh\u00b7ge\u00b7mahl", "/", "die", "ge\u00b7stern", "jhm", "ver\u00b7trawt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "ART", "ADJA", "PPER", "VVFIN", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Jtzt rathschl\u00e4gt wie er m\u00f6g auch junge B\u00fcrger geben.", "tokens": ["Jtzt", "rath\u00b7schl\u00e4gt", "wie", "er", "m\u00f6g", "auch", "jun\u00b7ge", "B\u00fcr\u00b7ger", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "PPER", "VMFIN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja freylich wird Ers thun. Mein ", "tokens": ["Ja", "frey\u00b7lich", "wird", "Ers", "thun", ".", "Mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "ADV", "VAFIN", "PIS", "VVINF", "$.", "PPOSAT"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das/ ehr der Mond neunmal besucht den Wassermann.", "tokens": ["Das", "/", "ehr", "der", "Mond", "neun\u00b7mal", "be\u00b7sucht", "den", "Was\u00b7ser\u00b7mann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was mehr noch werd\u2019 als itzt auff seiner Gutsche leben.", "tokens": ["Was", "mehr", "noch", "werd'", "als", "itzt", "auff", "sei\u00b7ner", "Gut\u00b7sche", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VAFIN", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}