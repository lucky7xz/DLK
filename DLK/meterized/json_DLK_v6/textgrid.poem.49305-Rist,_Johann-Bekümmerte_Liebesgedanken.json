{"textgrid.poem.49305": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Bek\u00fcmmerte Liebesgedanken", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Daphnis gieng f\u00fcr wenig Tagen", "tokens": ["Daph\u00b7nis", "gieng", "f\u00fcr", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber die begr\u00fcnte Heid',", "tokens": ["Ue\u00b7ber", "die", "be\u00b7gr\u00fcn\u00b7te", "Heid'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heimlich fieng er an zu klagen", "tokens": ["Heim\u00b7lich", "fi\u00b7eng", "er", "an", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bei sich selbst sein schweres Leid,", "tokens": ["Bei", "sich", "selbst", "sein", "schwe\u00b7res", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sang aus hochbetr\u00fcbten Herzen", "tokens": ["Sang", "aus", "hoch\u00b7be\u00b7tr\u00fcb\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den bittern Liebesschmerzen:", "tokens": ["Von", "den", "bit\u00b7tern", "Lie\u00b7bes\u00b7schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, da\u00df ich dich nicht mehr seh',", "tokens": ["Ach", ",", "da\u00df", "ich", "dich", "nicht", "mehr", "seh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Allersch\u00f6nste Galathe!", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ga\u00b7la\u00b7the", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Ist mir recht, das sind die Spitzen,", "tokens": ["Ist", "mir", "recht", ",", "das", "sind", "die", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die ich an den B\u00e4umen schau',", "tokens": ["Die", "ich", "an", "den", "B\u00e4u\u00b7men", "schau'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Hinter welchen pflegt zu sitzen", "tokens": ["Hin\u00b7ter", "wel\u00b7chen", "pflegt", "zu", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Galathee bei der Au',", "tokens": ["Ga\u00b7la\u00b7thee", "bei", "der", "Au'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Als sie zwinget meine Sinnen,", "tokens": ["Als", "sie", "zwin\u00b7get", "mei\u00b7ne", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "O du Preis der Sch\u00e4ferinnen;", "tokens": ["O", "du", "Preis", "der", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Weh mir, da\u00df ich nicht mehr seh'", "tokens": ["Weh", "mir", ",", "da\u00df", "ich", "nicht", "mehr", "seh'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Allersch\u00f6nste Galathe.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ga\u00b7la\u00b7the", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "K\u00f6nt' ich in den L\u00fcften fliegen", "tokens": ["K\u00f6nt'", "ich", "in", "den", "L\u00fcf\u00b7ten", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wie ein schnelles V\u00f6gelein,", "tokens": ["Wie", "ein", "schnel\u00b7les", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Ach, wie wolt' ich dich betriegen!", "tokens": ["Ach", ",", "wie", "wolt'", "ich", "dich", "be\u00b7trie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Bald, bald wolt' ich bei dir sein", "tokens": ["Bald", ",", "bald", "wolt'", "ich", "bei", "dir", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "APPR", "PPER", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Und dir tausend Schm\u00e4tzlein geben,", "tokens": ["Und", "dir", "tau\u00b7send", "Schm\u00e4tz\u00b7lein", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Das w\u00e4r mein erw\u00fcnschtes Leben;", "tokens": ["Das", "w\u00e4r", "mein", "er\u00b7w\u00fcnschtes", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Nun ist mir von Herzen weh,", "tokens": ["Nun", "ist", "mir", "von", "Her\u00b7zen", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Allersch\u00f6nste Galathe.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ga\u00b7la\u00b7the", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.25": {"text": "M\u00f6cht' ich bei der Sonnen stehen,", "tokens": ["M\u00f6cht'", "ich", "bei", "der", "Son\u00b7nen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Bei dem g\u00fcldnen Himmelslicht,", "tokens": ["Bei", "dem", "g\u00fcld\u00b7nen", "Him\u00b7mels\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "O wie flei\u00dfig wolt' ich sehen", "tokens": ["O", "wie", "flei\u00b7\u00dfig", "wolt'", "ich", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Auf dein freundlichs Angesicht;", "tokens": ["Auf", "dein", "freund\u00b7lichs", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Tausend Stralen wolt' ich schie\u00dfen,", "tokens": ["Tau\u00b7send", "Stra\u00b7len", "wolt'", "ich", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Deiner Aeuglein zu genie\u00dfen.", "tokens": ["Dei\u00b7ner", "A\u00b7e\u00b7u\u00b7glein", "zu", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Nun ist mir von Herzen weh,", "tokens": ["Nun", "ist", "mir", "von", "Her\u00b7zen", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Allersch\u00f6nste Galathe.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ga\u00b7la\u00b7the", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.33": {"text": "Kan ich denn nicht zu dir kommen,", "tokens": ["Kan", "ich", "denn", "nicht", "zu", "dir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Der ich dir so nah jetzt bin,", "tokens": ["Der", "ich", "dir", "so", "nah", "jetzt", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADJD", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Ist mir schon der Weg benommen,", "tokens": ["Ist", "mir", "schon", "der", "Weg", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Ei, so nim die Seufzer hin,", "tokens": ["Ei", ",", "so", "nim", "die", "Seuf\u00b7zer", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Die ich dir von Herzen sende,", "tokens": ["Die", "ich", "dir", "von", "Her\u00b7zen", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Bis das Gl\u00fcck sich wiedrum wende", "tokens": ["Bis", "das", "Gl\u00fcck", "sich", "wie\u00b7drum", "wen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Und ich dich mit Freuden seh',", "tokens": ["Und", "ich", "dich", "mit", "Freu\u00b7den", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Allersch\u00f6nste Galathe.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ga\u00b7la\u00b7the", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.41": {"text": "Drum, ihr Winde solt ihr bringen", "tokens": ["Drum", ",", "ihr", "Win\u00b7de", "solt", "ihr", "brin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Meine Klag und Seufzer zu;", "tokens": ["Mei\u00b7ne", "Klag", "und", "Seuf\u00b7zer", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Selber kan ich nicht mehr singen,", "tokens": ["Sel\u00b7ber", "kan", "ich", "nicht", "mehr", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Denn mein Herz ist sonder Ruh';", "tokens": ["Denn", "mein", "Herz", "ist", "son\u00b7der", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Ach, ich Armer hab' ersehen", "tokens": ["Ach", ",", "ich", "Ar\u00b7mer", "hab'", "er\u00b7se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Ihr Gezelt von ferne stehen;", "tokens": ["Ihr", "Ge\u00b7zelt", "von", "fer\u00b7ne", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Nun ist mir von Herzen weh,", "tokens": ["Nun", "ist", "mir", "von", "Her\u00b7zen", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "Allersch\u00f6nste Galathe.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ga\u00b7la\u00b7the", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.49": {"text": "O, ihr V\u00f6glein, die ihr wendet", "tokens": ["O", ",", "ihr", "V\u00f6\u00b7glein", ",", "die", "ihr", "wen\u00b7det"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Euren Flug an ihren Ort,", "tokens": ["Eu\u00b7ren", "Flug", "an", "ih\u00b7ren", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Sagt, ich hab' euch hergesendet,", "tokens": ["Sagt", ",", "ich", "hab'", "euch", "her\u00b7ge\u00b7sen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Da\u00df ihr mit euch nehmet fort", "tokens": ["Da\u00df", "ihr", "mit", "euch", "neh\u00b7met", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Die getreuen Liebesthr\u00e4nen,", "tokens": ["Die", "ge\u00b7treu\u00b7en", "Lie\u00b7best\u00b7hr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Die sich st\u00fcndlich nach dir sehnen,", "tokens": ["Die", "sich", "st\u00fcnd\u00b7lich", "nach", "dir", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Bis ich dich in Freuden seh',", "tokens": ["Bis", "ich", "dich", "in", "Freu\u00b7den", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Allersch\u00f6nste Galathe.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ga\u00b7la\u00b7the", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.57": {"text": "Galathee, du mein Leben,", "tokens": ["Ga\u00b7la\u00b7thee", ",", "du", "mein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.58": {"text": "Nimm die Wind' und V\u00f6glein auf,", "tokens": ["Nimm", "die", "Wind'", "und", "V\u00f6\u00b7glein", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Die sich dir zu Dienst ergeben", "tokens": ["Die", "sich", "dir", "zu", "Dienst", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "PPER", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Mit so schnellem Flug und Lauf!", "tokens": ["Mit", "so", "schnel\u00b7lem", "Flug", "und", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Und weil ich dich nicht kan schauen,", "tokens": ["Und", "weil", "ich", "dich", "nicht", "kan", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.62": {"text": "Wollest du den Boten trauen,", "tokens": ["Wol\u00b7lest", "du", "den", "Bo\u00b7ten", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Bis ich selbst dich wiederseh',", "tokens": ["Bis", "ich", "selbst", "dich", "wie\u00b7der\u00b7seh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Allersch\u00f6nste Galathe.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ga\u00b7la\u00b7the", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}