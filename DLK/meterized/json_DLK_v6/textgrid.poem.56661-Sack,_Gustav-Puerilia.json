{"textgrid.poem.56661": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Puerilia", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da war einmal ein Maientag", "tokens": ["Da", "war", "ein\u00b7mal", "ein", "Mai\u00b7en\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit Primelduft und Lerchenliederleiter,", "tokens": ["mit", "Pri\u00b7mel\u00b7duft", "und", "Ler\u00b7chen\u00b7lie\u00b7der\u00b7lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit blauem Himmel und so weiter \u2013", "tokens": ["mit", "blau\u00b7em", "Him\u00b7mel", "und", "so", "wei\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "doch meine liebeswunde Seele lag", "tokens": ["doch", "mei\u00b7ne", "lie\u00b7bes\u00b7wun\u00b7de", "See\u00b7le", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "an einem Strom und unter gelben Weiden", "tokens": ["an", "ei\u00b7nem", "Strom", "und", "un\u00b7ter", "gel\u00b7ben", "Wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "trank schluchzend sie ihr erstes Leiden.", "tokens": ["trank", "schluch\u00b7zend", "sie", "ihr", "ers\u00b7tes", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und diese Stunde mu\u00df ich wiederfinden", "tokens": ["Und", "die\u00b7se", "Stun\u00b7de", "mu\u00df", "ich", "wie\u00b7der\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit ihrer jungen, jungendummen Trauer", "tokens": ["mit", "ih\u00b7rer", "jun\u00b7gen", ",", "jun\u00b7gen\u00b7dum\u00b7men", "Trau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und ihrem stolzen \u00dcberwinden.", "tokens": ["und", "ih\u00b7rem", "stol\u00b7zen", "\u00dc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}