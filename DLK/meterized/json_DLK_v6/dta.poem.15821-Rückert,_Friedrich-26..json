{"dta.poem.15821": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du f\u00fchlst dich heim bei dir stiefm\u00fctterlich bedacht,", "tokens": ["Du", "f\u00fchlst", "dich", "heim", "bei", "dir", "stief\u00b7m\u00fct\u00b7ter\u00b7lich", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu wenig Fr\u00fchlingstag, und zuviel Winternacht.", "tokens": ["Zu", "we\u00b7nig", "Fr\u00fch\u00b7lings\u00b7tag", ",", "und", "zu\u00b7viel", "Win\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Menschheit Uebel schien' ertr\u00e4glich dir, wenn nur", "tokens": ["Der", "Menschheit", "Ue\u00b7bel", "schien'", "er\u00b7tr\u00e4g\u00b7lich", "dir", ",", "wenn", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "PPER", "$,", "KOUS", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mit ihm nicht tr\u00e4t' in Bund das Uebel der Natur.", "tokens": ["Mit", "ihm", "nicht", "tr\u00e4t'", "in", "Bund", "das", "Ue\u00b7bel", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "VVFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bei dir allein nicht ist der Jammer zu erproben,", "tokens": ["Bei", "dir", "al\u00b7lein", "nicht", "ist", "der", "Jam\u00b7mer", "zu", "er\u00b7pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKNEG", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ganze Erde, Freund, ist nebenaus geschoben.", "tokens": ["Die", "gan\u00b7ze", "Er\u00b7de", ",", "Freund", ",", "ist", "ne\u00b7be\u00b7naus", "ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es lohnt der M\u00fche nicht, von einem Pol zum andern", "tokens": ["Es", "lohnt", "der", "M\u00fc\u00b7he", "nicht", ",", "von", "ei\u00b7nem", "Pol", "zum", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "APPR", "ART", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach einem Umtausch nur von M\u00fchsal auszuwandern.", "tokens": ["Nach", "ei\u00b7nem", "Um\u00b7tausch", "nur", "von", "M\u00fch\u00b7sal", "aus\u00b7zu\u00b7wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir bleiben in Geduld, bis unsre Reise geht", "tokens": ["Wir", "blei\u00b7ben", "in", "Ge\u00b7duld", ",", "bis", "uns\u00b7re", "Rei\u00b7se", "geht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach einem Sterne, der in besserm Gleise dreht.", "tokens": ["Nach", "ei\u00b7nem", "Ster\u00b7ne", ",", "der", "in", "bes\u00b7serm", "Glei\u00b7se", "dreht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}