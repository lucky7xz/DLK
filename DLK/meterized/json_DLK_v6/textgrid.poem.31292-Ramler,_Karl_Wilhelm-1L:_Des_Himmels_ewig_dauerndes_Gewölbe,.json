{"textgrid.poem.31292": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Himmels ewig dauerndes Gew\u00f6lbe,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Himmels ewig dauerndes Gew\u00f6lbe,", "tokens": ["Des", "Him\u00b7mels", "e\u00b7wig", "dau\u00b7ern\u00b7des", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das \u00fcber allen Sternen h\u00e4ngt,", "tokens": ["Das", "\u00fc\u00b7ber", "al\u00b7len", "Ster\u00b7nen", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Erdball unter ihm, gegr\u00fcndet auf sich selber,", "tokens": ["Der", "Erd\u00b7ball", "un\u00b7ter", "ihm", ",", "ge\u00b7gr\u00fcn\u00b7det", "auf", "sich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "VVPP", "APPR", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verk\u00fcndigt seinen Herrn.", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "sei\u00b7nen", "Herrn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihn lobt der Tag, ihn singt mit tausend Zungen", "tokens": ["Ihn", "lobt", "der", "Tag", ",", "ihn", "singt", "mit", "tau\u00b7send", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht, und alle Welt vernimmt", "tokens": ["Die", "Nacht", ",", "und", "al\u00b7le", "Welt", "ver\u00b7nimmt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Lobgesang der Nacht, und alle V\u00f6lker h\u00f6ren", "tokens": ["Den", "Lob\u00b7ge\u00b7sang", "der", "Nacht", ",", "und", "al\u00b7le", "V\u00f6l\u00b7ker", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Tages K\u00f6nigin.", "tokens": ["Des", "Ta\u00b7ges", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie steigt auf ihren Purpurthron im Osten,", "tokens": ["Sie", "steigt", "auf", "ih\u00b7ren", "Pur\u00b7pur\u00b7thron", "im", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geht triumphirend ihre Bahn,", "tokens": ["Geht", "tri\u00b7um\u00b7phi\u00b7rend", "ih\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcberschaut ihr Reich, bis sie der Abendhimmel", "tokens": ["Und", "\u00fc\u00b7bersc\u00b7haut", "ihr", "Reich", ",", "bis", "sie", "der", "A\u00b7bend\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In seine Thore nimmt.", "tokens": ["In", "sei\u00b7ne", "Tho\u00b7re", "nimmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr Anblick, wenn sie durch den Aether wallet,", "tokens": ["Ihr", "An\u00b7blick", ",", "wenn", "sie", "durch", "den", "A\u00b7e\u00b7ther", "wal\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zieht W\u00e4lder aus der Erde Schoo\u00df,", "tokens": ["Zieht", "W\u00e4l\u00b7der", "aus", "der", "Er\u00b7de", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus der Fluth den Thau, der aus den Wolken tr\u00e4ufelt,", "tokens": ["Und", "aus", "der", "Fluth", "den", "Thau", ",", "der", "aus", "den", "Wol\u00b7ken", "tr\u00e4u\u00b7felt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und aus den Bergen str\u00f6mt.", "tokens": ["Und", "aus", "den", "Ber\u00b7gen", "str\u00f6mt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie wickelt das erw\u00e4rmte Rund der Erde", "tokens": ["Sie", "wi\u00b7ckelt", "das", "er\u00b7w\u00e4rm\u00b7te", "Rund", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einen gr\u00fcnen Teppich ein,", "tokens": ["In", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Tep\u00b7pich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bestreut mit Blumen ihn, hell leuchtend, wie die Farben", "tokens": ["Be\u00b7streut", "mit", "Blu\u00b7men", "ihn", ",", "hell", "leuch\u00b7tend", ",", "wie", "die", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PPER", "$,", "ADJD", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des G\u00fcrtels, den sie webt.", "tokens": ["Des", "G\u00fcr\u00b7tels", ",", "den", "sie", "webt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Aus ihrem Feuermeer f\u00fcllt seine Lampe", "tokens": ["Aus", "ih\u00b7rem", "Feu\u00b7er\u00b7meer", "f\u00fcllt", "sei\u00b7ne", "Lam\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mond mit Licht; der Morgenstern", "tokens": ["Der", "Mond", "mit", "Licht", ";", "der", "Mor\u00b7gens\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und seiner Br\u00fcder Chor, bekr\u00e4nzt von ihr mit Strahlen,", "tokens": ["Und", "sei\u00b7ner", "Br\u00fc\u00b7der", "Chor", ",", "be\u00b7kr\u00e4nzt", "von", "ihr", "mit", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tanzt freudig um sie her.", "tokens": ["Tanzt", "freu\u00b7dig", "um", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Laut ruft sie durch die grenzenlose Tiefe,", "tokens": ["Laut", "ruft", "sie", "durch", "die", "gren\u00b7zen\u00b7lo\u00b7se", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alle Sterne rufen laut:", "tokens": ["Und", "al\u00b7le", "Ster\u00b7ne", "ru\u00b7fen", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allm\u00e4chtig ist die Hand, die uns zusammenfa\u00dfte,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "ist", "die", "Hand", ",", "die", "uns", "zu\u00b7sam\u00b7men\u00b7fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in den Weltraum warf.", "tokens": ["Und", "in", "den", "Welt\u00b7raum", "warf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}