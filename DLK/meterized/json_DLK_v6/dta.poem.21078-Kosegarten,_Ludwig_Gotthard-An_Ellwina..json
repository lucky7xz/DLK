{"dta.poem.21078": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Ellwina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Theures M&#228;dchen, wenn ein andrer Himmel,               ", "tokens": ["Theu\u00b7res", "M", "&#228;", "dchen", ",", "wenn", "ein", "an\u00b7drer", "Him\u00b7mel", ","], "token_info": ["word", "word", "XML_entity", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch kein sch\u00f6n'rer, einstens um dich wallt;", "tokens": ["Doch", "kein", "sch\u00f6n'\u00b7rer", ",", "eins\u00b7tens", "um", "dich", "wallt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "$,", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wenn der Stadt zerstreuendes Get\u00fcmmel", "tokens": ["Wenn", "der", "Stadt", "zer\u00b7streu\u00b7en\u00b7des", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lauter itzt, itzt dumpfer um dich schallt;", "tokens": ["Lau\u00b7ter", "itzt", ",", "itzt", "dum\u00b7pfer", "um", "dich", "schallt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn die bunten Gecken um dich gaukeln,", "tokens": ["Wenn", "die", "bun\u00b7ten", "Ge\u00b7cken", "um", "dich", "gau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kr\u00e4useln gleich, sich um dich drehn und schaukeln,", "tokens": ["Kr\u00e4u\u00b7seln", "gleich", ",", "sich", "um", "dich", "drehn", "und", "schau\u00b7keln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRF", "APPR", "PRF", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ekeln Weihrauch deiner Sch\u00f6nheit streun,", "tokens": ["E\u00b7keln", "Weih\u00b7rauch", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Dann, Geliebte, denke mein!", "tokens": ["Dann", ",", "Ge\u00b7lieb\u00b7te", ",", "den\u00b7ke", "mein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn du, satt des seelelosen L\u00e4rmens,", "tokens": ["Wenn", "du", ",", "satt", "des", "see\u00b7le\u00b7lo\u00b7sen", "L\u00e4r\u00b7mens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Abends in dein einsam Zimmer eilst;", "tokens": ["A\u00b7bends", "in", "dein", "ein\u00b7sam", "Zim\u00b7mer", "eilst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In der Wonne dann des s\u00fcssen Schw\u00e4rmens", "tokens": ["In", "der", "Won\u00b7ne", "dann", "des", "s\u00fcs\u00b7sen", "Schw\u00e4r\u00b7mens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Noch ein stilles St\u00fcndchen staunend weilst;", "tokens": ["Noch", "ein", "stil\u00b7les", "St\u00fcnd\u00b7chen", "stau\u00b7nend", "weilst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dann dem Genius der Ruhe winkest,", "tokens": ["Dann", "dem", "Ge\u00b7nius", "der", "Ru\u00b7he", "win\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann dem Schlummer in die Arme sinkest,", "tokens": ["Dann", "dem", "Schlum\u00b7mer", "in", "die", "Ar\u00b7me", "sin\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Der dich wiegt in holde Tr\u00e4umerein,", "tokens": ["Der", "dich", "wiegt", "in", "hol\u00b7de", "Tr\u00e4u\u00b7me\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Edle Seele, denke mein!", "tokens": ["Ed\u00b7le", "See\u00b7le", ",", "den\u00b7ke", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn, dieweil die m\u00fcde Sch\u00f6pfung feyert", "tokens": ["Wenn", ",", "die\u00b7weil", "die", "m\u00fc\u00b7de", "Sch\u00f6p\u00b7fung", "fe\u00b7yert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und die D\u00e4mmerung die Welt verh\u00fcllt,", "tokens": ["Und", "die", "D\u00e4m\u00b7me\u00b7rung", "die", "Welt", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sanfte Schwermuth deinen Geist umschleyert,", "tokens": ["Sanf\u00b7te", "Schwer\u00b7muth", "dei\u00b7nen", "Geist", "um\u00b7schley\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und von Ahndungen dein Busen schwillt,", "tokens": ["Und", "von", "Ahn\u00b7dun\u00b7gen", "dein", "Bu\u00b7sen", "schwillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Zarte Sorgen dann dein Herz beklemmen,", "tokens": ["Zar\u00b7te", "Sor\u00b7gen", "dann", "dein", "Herz", "be\u00b7klem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Thr\u00e4nen deine Wimper \u00fcberschwemmen,", "tokens": ["Thr\u00e4\u00b7nen", "dei\u00b7ne", "Wim\u00b7per", "\u00fc\u00b7bersc\u00b7hwem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "S\u00fcsse Thr\u00e4nen, die die Neugier scheun \u2014", "tokens": ["S\u00fcs\u00b7se", "Thr\u00e4\u00b7nen", ",", "die", "die", "Neu\u00b7gier", "scheun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Edle, so gedenke mein!", "tokens": ["Ed\u00b7le", ",", "so", "ge\u00b7den\u00b7ke", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich gedenk' an dich in meiner Wildniss,", "tokens": ["Ich", "ge\u00b7denk'", "an", "dich", "in", "mei\u00b7ner", "Wild\u00b7niss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Einsamkeit vertrautem Arm.", "tokens": ["In", "der", "Ein\u00b7sam\u00b7keit", "ver\u00b7trau\u00b7tem", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch das tiefe Dunkel gl\u00e4nzt dein Bildniss,", "tokens": ["Durch", "das", "tie\u00b7fe", "Dun\u00b7kel", "gl\u00e4nzt", "dein", "Bild\u00b7niss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "T\u00e4uscht mit holdem L\u00e4cheln meinen Harm.", "tokens": ["T\u00e4uscht", "mit", "hol\u00b7dem", "L\u00e4\u00b7cheln", "mei\u00b7nen", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn das Sp\u00e4troth mein Gemach durchschimmert,", "tokens": ["Wenn", "das", "Sp\u00e4t\u00b7roth", "mein", "Ge\u00b7mach", "durch\u00b7schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hesperus in meine Fenster flimmert,", "tokens": ["Hes\u00b7pe\u00b7rus", "in", "mei\u00b7ne", "Fens\u00b7ter", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Fr\u00fch mich weckt Aurorens rother Schein \u2014", "tokens": ["Fr\u00fch", "mich", "weckt", "Au\u00b7ro\u00b7rens", "ro\u00b7ther", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "NE", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Immer, Edle, denk' ich dein!", "tokens": ["Im\u00b7mer", ",", "Ed\u00b7le", ",", "denk'", "ich", "dein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich lese, funkelt mir aus jeder", "tokens": ["Wenn", "ich", "le\u00b7se", ",", "fun\u00b7kelt", "mir", "aus", "je\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zeile deines Namens theurer Zug.", "tokens": ["Zei\u00b7le", "dei\u00b7nes", "Na\u00b7mens", "theu\u00b7rer", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich schreibe, zeichnet meine Feder", "tokens": ["Wenn", "ich", "schrei\u00b7be", ",", "zeich\u00b7net", "mei\u00b7ne", "Fe\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unwillk\u00fchrlich den geliebten Zug.", "tokens": ["Un\u00b7will\u00b7k\u00fchr\u00b7lich", "den", "ge\u00b7lieb\u00b7ten", "Zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn ich lieg' und tr\u00e4ume, horch! so schwimmen", "tokens": ["Wenn", "ich", "lieg'", "und", "tr\u00e4u\u00b7me", ",", "horch", "!", "so", "schwim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVIMP", "$.", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Um mich ferne leise s\u00fcsse Stimmen.", "tokens": ["Um", "mich", "fer\u00b7ne", "lei\u00b7se", "s\u00fcs\u00b7se", "Stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, die Stimmen nennen dich allein.", "tokens": ["Ach", ",", "die", "Stim\u00b7men", "nen\u00b7nen", "dich", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Immer, Edle, denk' ich dein.", "tokens": ["Im\u00b7mer", ",", "Ed\u00b7le", ",", "denk'", "ich", "dein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn ich einst das helle Land erfliege,", "tokens": ["Wenn", "ich", "einst", "das", "hel\u00b7le", "Land", "er\u00b7flie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Draus die Wahrheit und die Freyheit stammt,", "tokens": ["Draus", "die", "Wahr\u00b7heit", "und", "die", "Frey\u00b7heit", "stammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Selig mich in jenen R\u00e4umen wiege,", "tokens": ["Se\u00b7lig", "mich", "in", "je\u00b7nen", "R\u00e4u\u00b7men", "wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo Orion und die Lyra flammt,", "tokens": ["Wo", "O\u00b7rion", "und", "die", "Ly\u00b7ra", "flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00d6fter schweb' ich aus der hohen Ferne", "tokens": ["\u00d6f\u00b7ter", "schweb'", "ich", "aus", "der", "ho\u00b7hen", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dann herab zum blassen Erdensterne,", "tokens": ["Dann", "her\u00b7ab", "zum", "blas\u00b7sen", "Er\u00b7dens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wiege dich in s\u00fcsses Staunen ein \u2014", "tokens": ["Wie\u00b7ge", "dich", "in", "s\u00fcs\u00b7ses", "Stau\u00b7nen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "ART", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ewig, Edle, denk' ich dein.", "tokens": ["E\u00b7wig", ",", "Ed\u00b7le", ",", "denk'", "ich", "dein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}