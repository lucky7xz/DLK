{"dta.poem.21342": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel erhalte dich, wackres Volk,", "tokens": ["Der", "Him\u00b7mel", "er\u00b7hal\u00b7te", "dich", ",", "wack\u00b7res", "Volk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er segne deine Saaten,", "tokens": ["Er", "seg\u00b7ne", "dei\u00b7ne", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bewahre dich vor Krieg und Ruhm,", "tokens": ["Be\u00b7wah\u00b7re", "dich", "vor", "Krieg", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Helden und Heldenthaten.", "tokens": ["Vor", "Hel\u00b7den", "und", "Hel\u00b7den\u00b7tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er schenke deinen S\u00f6hnen stets", "tokens": ["Er", "schen\u00b7ke", "dei\u00b7nen", "S\u00f6h\u00b7nen", "stets"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sehr gelindes Examen,", "tokens": ["Ein", "sehr", "ge\u00b7lin\u00b7des", "E\u00b7xa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und deine T\u00f6chter bringe er h\u00fcbsch", "tokens": ["Und", "dei\u00b7ne", "T\u00f6ch\u00b7ter", "brin\u00b7ge", "er", "h\u00fcbsch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Unter die Haube \u2013 Amen!", "tokens": ["Un\u00b7ter", "die", "Hau\u00b7be", "\u2013", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}