{"textgrid.poem.65561": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "34. Vers\u00e4um's nicht", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du willst ein Frommer werden,", "tokens": ["Wenn", "du", "willst", "ein", "From\u00b7mer", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil es Mode wird auf Erden,", "tokens": ["Weil", "es", "Mo\u00b7de", "wird", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fang' es heute lieber an,", "tokens": ["Fang'", "es", "heu\u00b7te", "lie\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen kann es Jedermann.", "tokens": ["Mor\u00b7gen", "kann", "es", "Je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}