{"textgrid.poem.34864": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlage die Trommel und f\u00fcrchte dich nicht,", "tokens": ["Schla\u00b7ge", "die", "Trom\u00b7mel", "und", "f\u00fcrch\u00b7te", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und k\u00fcsse die Marketenderin!", "tokens": ["Und", "k\u00fcs\u00b7se", "die", "Mar\u00b7ke\u00b7ten\u00b7de\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das ist die ganze Wissenschaft,", "tokens": ["Das", "ist", "die", "gan\u00b7ze", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der B\u00fccher tiefster Sinn.", "tokens": ["Das", "ist", "der", "B\u00fc\u00b7cher", "tiefs\u00b7ter", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Trommle die Leute aus dem Schlaf,", "tokens": ["Tromm\u00b7le", "die", "Leu\u00b7te", "aus", "dem", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Trommle Reveille mit Jugendkraft,", "tokens": ["Tromm\u00b7le", "Re\u00b7veil\u00b7le", "mit", "Ju\u00b7gend\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Marschiere trommelnd immer voran,", "tokens": ["Mar\u00b7schie\u00b7re", "trom\u00b7melnd", "im\u00b7mer", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ADV", "PTKVZ", "$,"], "meter": "+--+-++-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Das ist die ganze Wissenschaft.", "tokens": ["Das", "ist", "die", "gan\u00b7ze", "Wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ist die Hegelsche Philosophie,", "tokens": ["Das", "ist", "die", "He\u00b7gel\u00b7sche", "Phi\u00b7lo\u00b7so\u00b7phie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ist der B\u00fccher tiefster Sinn!", "tokens": ["Das", "ist", "der", "B\u00fc\u00b7cher", "tiefs\u00b7ter", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab sie begriffen, weil ich gescheit,", "tokens": ["Ich", "hab", "sie", "be\u00b7grif\u00b7fen", ",", "weil", "ich", "ge\u00b7scheit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und weil ich ein guter Tambour bin.", "tokens": ["Und", "weil", "ich", "ein", "gu\u00b7ter", "Tam\u00b7bour", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}