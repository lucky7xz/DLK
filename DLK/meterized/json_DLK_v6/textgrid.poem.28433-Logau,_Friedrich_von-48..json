{"textgrid.poem.28433": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die mit uns halten Zorn, die zu uns Racha sagen,", "tokens": ["Die", "mit", "uns", "hal\u00b7ten", "Zorn", ",", "die", "zu", "uns", "Ra\u00b7cha", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wie die Narren uns vexiren, schlagen, jagen,", "tokens": ["Die", "wie", "die", "Nar\u00b7ren", "uns", "ve\u00b7xi\u00b7ren", ",", "schla\u00b7gen", ",", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die lasse so man seyn und habe nur Geduld;", "tokens": ["Die", "las\u00b7se", "so", "man", "seyn", "und", "ha\u00b7be", "nur", "Ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIS", "VAINF", "KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es trifft sie schwer genug zu zahlen Gottes Schuld.", "tokens": ["Es", "trifft", "sie", "schwer", "ge\u00b7nug", "zu", "zah\u00b7len", "Got\u00b7tes", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}