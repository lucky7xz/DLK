{"dta.poem.8863": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und wandte mich vom Tand der Welt in Eile weg!", "tokens": ["Und", "wand\u00b7te", "mich", "vom", "Tand", "der", "Welt", "in", "Ei\u00b7le", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von frost'ger N\u00fcchternheit, von gr\u00fcbelnder Vernunft,", "tokens": ["Von", "frost'\u00b7ger", "N\u00fcch\u00b7tern\u00b7heit", ",", "von", "gr\u00fc\u00b7beln\u00b7der", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie sehn' ich mich davon, aus langer Weile, weg!", "tokens": ["Wie", "sehn'", "ich", "mich", "da\u00b7von", ",", "aus", "lan\u00b7ger", "Wei\u00b7le", ",", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PAV", "$,", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sagt ihr mir Schlimmes nach, so sagt' ich's im Voraus,", "tokens": ["Sagt", "ihr", "mir", "Schlim\u00b7mes", "nach", ",", "so", "sagt'", "ich's", "im", "Vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Und nahm euch diesen Ruhm zum besten Theile weg:", "tokens": ["Und", "nahm", "euch", "die\u00b7sen", "Ruhm", "zum", "bes\u00b7ten", "Thei\u00b7le", "weg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich z\u00f6ge gern den Weg, den eure Tugend bahnt,", "tokens": ["Ich", "z\u00f6\u00b7ge", "gern", "den", "Weg", ",", "den", "eu\u00b7re", "Tu\u00b7gend", "bahnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch blieb ich stets davon um eine Meile weg;", "tokens": ["Doch", "blieb", "ich", "stets", "da\u00b7von", "um", "ei\u00b7ne", "Mei\u00b7le", "weg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn wer zur Scheibe sich, zum Ziel die Sonne w\u00e4hlt,", "tokens": ["Denn", "wer", "zur", "Schei\u00b7be", "sich", ",", "zum", "Ziel", "die", "Son\u00b7ne", "w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "PRF", "$,", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der sendet stets umsonst die leichten Pfeile weg!", "tokens": ["Der", "sen\u00b7det", "stets", "um\u00b7sonst", "die", "leich\u00b7ten", "Pfei\u00b7le", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Nun aber, Dichter, schweig und la\u00df der Welt den Lauf,", "tokens": ["Nun", "a\u00b7ber", ",", "Dich\u00b7ter", ",", "schweig", "und", "la\u00df", "der", "Welt", "den", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "ADJD", "KON", "VVIMP", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und was ihr nicht behagt, vertilge, feile weg!", "tokens": ["Und", "was", "ihr", "nicht", "be\u00b7hagt", ",", "ver\u00b7til\u00b7ge", ",", "fei\u00b7le", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}