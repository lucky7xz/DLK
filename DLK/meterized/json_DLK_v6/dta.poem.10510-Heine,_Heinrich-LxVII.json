{"dta.poem.10510": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxVII", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' Euch im besten Juli verlassen,", "tokens": ["Ich", "hab'", "Euch", "im", "bes\u00b7ten", "Ju\u00b7li", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und find' Euch wieder im Januar;", "tokens": ["Und", "find'", "Euch", "wie\u00b7der", "im", "Ja\u00b7nu\u00b7ar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr sa\u00dfet damals so recht in der Hitze.", "tokens": ["Ihr", "sa\u00b7\u00dfet", "da\u00b7mals", "so", "recht", "in", "der", "Hit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt seyd ihr gek\u00fchlt und kalt sogar.", "tokens": ["Jetzt", "seyd", "ihr", "ge\u00b7k\u00fchlt", "und", "kalt", "so\u00b7gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "KON", "ADJD", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bald scheid' ich nochmals und komm' ich einst wieder,", "tokens": ["Bald", "scheid'", "ich", "noch\u00b7mals", "und", "komm'", "ich", "einst", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann seyd ihr weder warm noch kalt,", "tokens": ["Dann", "seyd", "ihr", "we\u00b7der", "warm", "noch", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber Eure Gr\u00e4ber schreit' ich,", "tokens": ["Und", "\u00fc\u00b7ber", "Eu\u00b7re", "Gr\u00e4\u00b7ber", "schreit'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das eigne Herz ist arm und alt.", "tokens": ["Und", "das", "eig\u00b7ne", "Herz", "ist", "arm", "und", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}