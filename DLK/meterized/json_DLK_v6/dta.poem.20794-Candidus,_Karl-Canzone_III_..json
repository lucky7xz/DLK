{"dta.poem.20794": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   III .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie uns ihr eignes Licht die Sichtbarkeiten", "tokens": ["Wie", "uns", "ihr", "eig\u00b7nes", "Licht", "die", "Sicht\u00b7bar\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf wunderbarer Kunst Jodsilber malet,", "tokens": ["Auf", "wun\u00b7der\u00b7ba\u00b7rer", "Kunst", "Jod\u00b7sil\u00b7ber", "ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So malt dein Bild auf schlichter Seelen Grunde", "tokens": ["So", "malt", "dein", "Bild", "auf", "schlich\u00b7ter", "See\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Klarheit, welche dir, o Herr, entstralet", "tokens": ["Die", "Klar\u00b7heit", ",", "wel\u00b7che", "dir", ",", "o", "Herr", ",", "ent\u00b7stra\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und lieblich f\u00e4llt in Schmerzes Dunkelheiten,", "tokens": ["Und", "lieb\u00b7lich", "f\u00e4llt", "in", "Schmer\u00b7zes", "Dun\u00b7kel\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn du den Schieber hebst zur rechten Stunde.", "tokens": ["Wenn", "du", "den", "Schie\u00b7ber", "hebst", "zur", "rech\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O stets mit Herz und Munde", "tokens": ["O", "stets", "mit", "Herz", "und", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Will ich dem heilig hehren K\u00fcnstler danken,", "tokens": ["Will", "ich", "dem", "hei\u00b7lig", "heh\u00b7ren", "K\u00fcnst\u00b7ler", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der mir durch seiner Weltanschauung Normen", "tokens": ["Der", "mir", "durch", "sei\u00b7ner", "Welt\u00b7an\u00b7schau\u00b7ung", "Nor\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und Grundempfindungsformen", "tokens": ["Und", "Grun\u00b7demp\u00b7fin\u00b7dungs\u00b7for\u00b7men"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Hat umgebildet g\u00e4nzlich die Gedanken!", "tokens": ["Hat", "um\u00b7ge\u00b7bil\u00b7det", "g\u00e4nz\u00b7lich", "die", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die christliche Bestimmtheit der Gem\u00fcter", "tokens": ["Die", "christ\u00b7li\u00b7che", "Be\u00b7stimm\u00b7theit", "der", "Ge\u00b7m\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ist Kraft als ", "tokens": ["Ist", "Kraft", "als"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Dein Geist ist deine Weise Gott zu haben,", "tokens": ["Dein", "Geist", "ist", "dei\u00b7ne", "Wei\u00b7se", "Gott", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist deine Demut und dein Selbstbewu\u00dftsein,", "tokens": ["Ist", "dei\u00b7ne", "De\u00b7mut", "und", "dein", "Selbst\u00b7be\u00b7wu\u00df\u00b7tsein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Denn weil du ", "tokens": ["Denn", "weil", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Doch gleicherma\u00dfen mu\u00dft' in deiner Brust sein", "tokens": ["Doch", "glei\u00b7cher\u00b7ma\u00b7\u00dfen", "mu\u00dft'", "in", "dei\u00b7ner", "Brust", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Selbstgef\u00fchl unnennbar, hehr, erhaben,", "tokens": ["Ein", "Selbst\u00b7ge\u00b7f\u00fchl", "un\u00b7nenn\u00b7bar", ",", "hehr", ",", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn du mich nun, Entbl\u00f6\u00dfer", "tokens": ["Wenn", "du", "mich", "nun", ",", "Ent\u00b7bl\u00f6\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von allen Mosisdecken, mir bew\u00e4hrest", "tokens": ["Von", "al\u00b7len", "Mo\u00b7sis\u00b7de\u00b7cken", ",", "mir", "be\u00b7w\u00e4h\u00b7rest"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als tief von deiner Herrlichkeit durchdrungen,", "tokens": ["Als", "tief", "von", "dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und wie durch Spiegelungen", "tokens": ["Und", "wie", "durch", "Spie\u00b7ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mich in ein dir verwandtes Bild verkl\u00e4rest,", "tokens": ["Mich", "in", "ein", "dir", "ver\u00b7wand\u00b7tes", "Bild", "ver\u00b7kl\u00e4\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ja mich zur Brudergleichheit willst erw\u00e4hlen,", "tokens": ["Ja", "mich", "zur", "Bru\u00b7der\u00b7gleich\u00b7heit", "willst", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mu\u00df Demut nicht und Stolz ", "tokens": ["Mu\u00df", "De\u00b7mut", "nicht", "und", "Stolz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PTKNEG", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du wolltest niemals ", "tokens": ["Du", "woll\u00b7test", "nie\u00b7mals"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u201enur Gott ist gut,\u201c so sprachst du gro\u00df bescheiden,", "tokens": ["\u201e", "nur", "Gott", "ist", "gut", ",", "\u201c", "so", "sprachst", "du", "gro\u00df", "be\u00b7schei\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "VAFIN", "ADJD", "$,", "$(", "ADV", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch warst du Eins mit Gott, weil nur als nichtig", "tokens": ["Doch", "warst", "du", "Eins", "mit", "Gott", ",", "weil", "nur", "als", "nich\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "APPR", "NN", "$,", "KOUS", "ADV", "KOUS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du dich von Gotte konntest unterscheiden,", "tokens": ["Du", "dich", "von", "Got\u00b7te", "konn\u00b7test", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ob du als ein Mensch zwar an Geberden", "tokens": ["Und", "ob", "du", "als", "ein", "Mensch", "zwar", "an", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "KOUS", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mannichfacher Schwachheit warst ersichtig,", "tokens": ["Und", "man\u00b7nich\u00b7fa\u00b7cher", "Schwach\u00b7heit", "warst", "er\u00b7sich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Warst du doch nimmer pflichtig", "tokens": ["Warst", "du", "doch", "nim\u00b7mer", "pflich\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch unterworfen dem Gesetz der S\u00fcnden,", "tokens": ["Noch", "un\u00b7ter\u00b7wor\u00b7fen", "dem", "Ge\u00b7setz", "der", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn \u00fcber die pers\u00f6nlich enge Schranke", "tokens": ["Denn", "\u00fc\u00b7ber", "die", "per\u00b7s\u00f6n\u00b7lich", "en\u00b7ge", "Schran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Hinaus war dein Gedanke,", "tokens": ["Hin\u00b7aus", "war", "dein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Gott mochtest du als wahres Selbst verk\u00fcnden.", "tokens": ["Gott", "moch\u00b7test", "du", "als", "wah\u00b7res", "Selbst", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zu gleicher Reinheit dr\u00e4ngst du nun die Geister.", "tokens": ["Zu", "glei\u00b7cher", "Rein\u00b7heit", "dr\u00e4ngst", "du", "nun", "die", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So wardst du dienend unser Aller Meister.", "tokens": ["So", "wardst", "du", "die\u00b7nend", "un\u00b7ser", "Al\u00b7ler", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und Quelle deiner Demut war dein Lieben,", "tokens": ["Und", "Quel\u00b7le", "dei\u00b7ner", "De\u00b7mut", "war", "dein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So auch dein Lieben Quelle deiner Hoheit.", "tokens": ["So", "auch", "dein", "Lie\u00b7ben", "Quel\u00b7le", "dei\u00b7ner", "Ho\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind dies denn nicht der Liebe beide Pole?", "tokens": ["Sind", "dies", "denn", "nicht", "der", "Lie\u00b7be", "bei\u00b7de", "Po\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PTKNEG", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O wie beklagenswert ist jene Rohheit,", "tokens": ["O", "wie", "be\u00b7kla\u00b7gens\u00b7wert", "ist", "je\u00b7ne", "Roh\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVPP", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die an des Stolzes und der Demut Trieben", "tokens": ["Die", "an", "des", "Stol\u00b7zes", "und", "der", "De\u00b7mut", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur Streit hat, leere Strebungen, gleich hohle!", "tokens": ["Nur", "Streit", "hat", ",", "lee\u00b7re", "Stre\u00b7bun\u00b7gen", ",", "gleich", "hoh\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$,", "ADJA", "NN", "$,", "ADV", "ADJA", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "O wenn zu Eurem Wole", "tokens": ["O", "wenn", "zu", "Eu\u00b7rem", "Wo\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr liebtet und begriffet! Elend scheinen", "tokens": ["Ihr", "lieb\u00b7tet", "und", "be\u00b7grif\u00b7fet", "!", "E\u00b7lend", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "M\u00fc\u00dft ihr mir vollends wo ihr vor m\u00f6gt wenden", "tokens": ["M\u00fc\u00dft", "ihr", "mir", "vol\u00b7lends", "wo", "ihr", "vor", "m\u00f6gt", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PWAV", "PPER", "APPR", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Angeblich schuft ihr Gott und seinen Reinen,", "tokens": ["An\u00b7ge\u00b7blich", "schuft", "ihr", "Gott", "und", "sei\u00b7nen", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und k\u00f6nnt das eigne Werk nicht menschlich lieben?", "tokens": ["Und", "k\u00f6nnt", "das", "eig\u00b7ne", "Werk", "nicht", "menschlich", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "O wo sind Stolz und Demut euch ", "tokens": ["O", "wo", "sind", "Stolz", "und", "De\u00b7mut", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VAFIN", "NN", "KON", "NN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Du bist in mich und ich in dich gestaltet", "tokens": ["Du", "b