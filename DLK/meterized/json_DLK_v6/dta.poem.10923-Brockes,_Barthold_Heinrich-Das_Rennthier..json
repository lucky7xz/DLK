{"dta.poem.10923": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Rennthier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein erfrorner Blick erstarrt, da ich wahres Eis, auch\nSchnee,", "tokens": ["Mein", "er\u00b7fror\u00b7ner", "Blick", "er\u00b7starrt", ",", "da", "ich", "wah\u00b7res", "Eis", ",", "auch", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "KOUS", "PPER", "ADJA", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und von Eis - Gebirg - und Schollen, alles hier erf\u00fcllet, seh.", "tokens": ["Und", "von", "Eis", "Ge\u00b7birg", "und", "Schol\u00b7len", ",", "al\u00b7les", "hier", "er\u00b7f\u00fcl\u00b7let", ",", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "$(", "NN", "$(", "KON", "NN", "$,", "PIS", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "--+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mich verblendet diese Weisse, die die schwarze Luft noch st\u00e4rkt,", "tokens": ["Mich", "ver\u00b7blen\u00b7det", "die\u00b7se", "Weis\u00b7se", ",", "die", "die", "schwar\u00b7ze", "Luft", "noch", "st\u00e4rkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df mein Auge kaum die Stralen der entfernten Sonne", "tokens": ["Da\u00df", "mein", "Au\u00b7ge", "kaum", "die", "Stra\u00b7len", "der", "ent\u00b7fern\u00b7ten", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Welche, mit gef\u00e4rbten Streifen, die beeiste Luft verg\u00fcldet.", "tokens": ["Wel\u00b7che", ",", "mit", "ge\u00b7f\u00e4rb\u00b7ten", "Strei\u00b7fen", ",", "die", "be\u00b7eis\u00b7te", "Luft", "ver\u00b7g\u00fcl\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wird denn hier, durch Schw\u00e4rz und Schatten, Schnee und", "tokens": ["Wird", "denn", "hier", ",", "durch", "Schw\u00e4rz", "und", "Schat\u00b7ten", ",", "Schnee", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,", "NN", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Doch was f\u00e4hrt daher? ein Rennthier. M\u00f6chte doch auch", "tokens": ["Doch", "was", "f\u00e4hrt", "da\u00b7her", "?", "ein", "Renn\u00b7thier", ".", "M\u00f6ch\u00b7te", "doch", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PAV", "$.", "ART", "NN", "$.", "VMFIN", "ADV", "ADV"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df ich es betrachten k\u00f6nnte! Gut. Es starrt, es stehet still,", "tokens": ["Da\u00df", "ich", "es", "be\u00b7trach\u00b7ten", "k\u00f6nn\u00b7te", "!", "Gut", ".", "Es", "starrt", ",", "es", "ste\u00b7het", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$.", "ADJD", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.9": {"text": "Und, als ob es sich von uns recht beschauen lassen will,", "tokens": ["Und", ",", "als", "ob", "es", "sich", "von", "uns", "recht", "be\u00b7schau\u00b7en", "las\u00b7sen", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOKOM", "KOUS", "PPER", "PRF", "APPR", "PPER", "ADJD", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Scheinet es, zum fernern Lauf, alle Kr\u00e4fte zu verlieren.", "tokens": ["Schei\u00b7net", "es", ",", "zum", "fer\u00b7nern", "Lauf", ",", "al\u00b7le", "Kr\u00e4f\u00b7te", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Welch ein stark und rasches Thier! welch ein pr\u00e4chtiges", "tokens": ["Welch", "ein", "stark", "und", "ra\u00b7sches", "Thier", "!", "welch", "ein", "pr\u00e4ch\u00b7ti\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJD", "KON", "ADJA", "NN", "$.", "PWAT", "ART", "ADJA"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Das sich vorn und hinten streckt! wie ein Pferd ist es gem\u00e4hnet,", "tokens": ["Das", "sich", "vorn", "und", "hin\u00b7ten", "streckt", "!", "wie", "ein", "Pferd", "ist", "es", "ge\u00b7m\u00e4h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "KON", "ADV", "VVFIN", "$.", "PWAV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Einem Kalbe gleicht sein Haupt. Einige sind wild und frey;", "tokens": ["Ei\u00b7nem", "Kal\u00b7be", "gleicht", "sein", "Haupt", ".", "Ei\u00b7ni\u00b7ge", "sind", "wild", "und", "frey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PIS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Andere sind, uns zum Dienst, zahm, und sonderbar gew\u00f6hnet.", "tokens": ["An\u00b7de\u00b7re", "sind", ",", "uns", "zum", "Dienst", ",", "zahm", ",", "und", "son\u00b7der\u00b7bar", "ge\u00b7w\u00f6h\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PPER", "APPRART", "NN", "$,", "VVFIN", "$,", "KON", "ADJD", "VVPP", "$."], "meter": "-+-++-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.15": {"text": "Dieses Thier nun zu erhalten, sind die Kosten gar nicht gro\u00df,", "tokens": ["Die\u00b7ses", "Thier", "nun", "zu", "er\u00b7hal\u00b7ten", ",", "sind", "die", "Kos\u00b7ten", "gar", "nicht", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Denn es kratzt, zu seiner Nahrung, ein verworfnes weisses Moo\u00df,", "tokens": ["Denn", "es", "kratzt", ",", "zu", "sei\u00b7ner", "Nah\u00b7rung", ",", "ein", "ver\u00b7worf\u00b7nes", "weis\u00b7ses", "Moo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Das in \u00f6den Feldern w\u00e4chst, selber unterm Schnee, herf\u00fcr,", "tokens": ["Das", "in", "\u00f6\u00b7den", "Fel\u00b7dern", "w\u00e4chst", ",", "sel\u00b7ber", "un\u00b7term", "Schnee", ",", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "$,", "ADV", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und dennoch sind Fleisch und Haut, Knochen, Sehnen, Milch", "tokens": ["Und", "den\u00b7noch", "sind", "Fleisch", "und", "Haut", ",", "Kno\u00b7chen", ",", "Seh\u00b7nen", ",", "Milch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Allesamt dem Menschen n\u00fctzlich. Wird denn, auch in diesem", "tokens": ["Al\u00b7le\u00b7samt", "dem", "Men\u00b7schen", "n\u00fctz\u00b7lich", ".", "Wird", "denn", ",", "auch", "in", "die\u00b7sem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "$.", "VAFIN", "ADV", "$,", "ADV", "APPR", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "Seines Sch\u00f6pfers Weisheit, Allmacht, samt der Huld, nicht", "tokens": ["Sei\u00b7nes", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "All\u00b7macht", ",", "samt", "der", "Huld", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "NN", "$,", "NN", "$,", "APPR", "ART", "NN", "$,", "PTKNEG"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}