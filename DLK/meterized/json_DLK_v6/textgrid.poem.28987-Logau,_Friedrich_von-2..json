{"textgrid.poem.28987": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man meint, ein abgestohlner Ku\u00df sey minder angeneme.", "tokens": ["Man", "meint", ",", "ein", "ab\u00b7ge\u00b7stohl\u00b7ner", "Ku\u00df", "sey", "min\u00b7der", "an\u00b7ge\u00b7ne\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Der Ku\u00df wird s\u00fcsser, wann man schaut, wie sie so sch\u00f6n sich sch\u00e4me,", "tokens": ["Der", "Ku\u00df", "wird", "s\u00fcs\u00b7ser", ",", "wann", "man", "schaut", ",", "wie", "sie", "so", "sch\u00f6n", "sich", "sch\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "PIS", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und was man leichtlich haben kan, ist selten gar bequeme.", "tokens": ["Und", "was", "man", "leicht\u00b7lich", "ha\u00b7ben", "kan", ",", "ist", "sel\u00b7ten", "gar", "be\u00b7que\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADJD", "VAINF", "VMFIN", "$,", "VAFIN", "ADJD", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}