{"textgrid.poem.63456": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Dat Leed vun dat Pack", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dor achter dat Holt wahnt", "tokens": ["Dor", "ach\u00b7ter", "dat", "Holt", "wahnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "'ne lustig Ort L\u00fcr;", "tokens": ["'ne", "lus\u00b7tig", "Ort", "L\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "lat 's Abens denn bring se", "tokens": ["lat", "'s", "A\u00b7bens", "denn", "bring", "se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "VVFIN", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "ehr Middag to F\u00fcr.", "tokens": ["ehr", "Mid\u00b7dag", "to", "F\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ehr Mandag, so segg se,", "tokens": ["Ehr", "Man\u00b7dag", ",", "so", "segg", "se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "FM", "FM", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "geiht Dingstags irst an;", "tokens": ["geiht", "Dings\u00b7tags", "irst", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "PTKVZ", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "un de Fru dreggt de B\u00fccks,", "tokens": ["un", "de", "Fru", "dreggt", "de", "B\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "un de Pi dreggt de Mann.", "tokens": ["un", "de", "Pi", "dreggt", "de", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Dat Wif kann nich haken,", "tokens": ["Dat", "Wif", "kann", "nich", "ha\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "de Kirl mag nich neig'n;", "tokens": ["de", "Kirl", "mag", "nich", "neig'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "un bi se geiht de S\u00fcnn up", "tokens": ["un", "bi", "se", "geiht", "de", "S\u00fcnn", "up"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "nich ihr as Klock teihn.", "tokens": ["nich", "ihr", "as", "Klock", "teihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Se n\u00e4hm' nicks nich v\u00f6r sick,", "tokens": ["Se", "n\u00e4hm'", "nicks", "nich", "v\u00f6r", "sick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "denn so sleiht se nicks f\u00e4hl \u2013", "tokens": ["denn", "so", "sleiht", "se", "nicks", "f\u00e4hl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "FM", "FM", "FM", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "un ehr P\u00f6tt s\u00fcnd ahn Henk", "tokens": ["un", "ehr", "P\u00f6tt", "s\u00fcnd", "ahn", "Henk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "un ehr Emmers ahn Seel.", "tokens": ["un", "ehr", "Em\u00b7mers", "ahn", "Seel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Un f\u00e4ngt't an to frier'n,", "tokens": ["Un", "f\u00e4ngt't", "an", "to", "frier'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "FM", "FM", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "denn is ehr Holt all;", "tokens": ["denn", "is", "ehr", "Holt", "all", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "NN", "NE", "PIAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "un as de Koh dot wir,", "tokens": ["un", "as", "de", "Koh", "dot", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "dunn so flickten s' den Stall.", "tokens": ["dunn", "so", "flick\u00b7ten", "s'", "den", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00f6r't Hungern dor heww se", "tokens": ["F\u00f6r't", "Hun\u00b7gern", "dor", "heww", "se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "man oft 'ne dr\u00f6g K\u00f6st,", "tokens": ["man", "oft", "'ne", "dr\u00f6g", "K\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "doch \u00fcmmer 'ne Buddel", "tokens": ["doch", "\u00fcm\u00b7mer", "'ne", "Bud\u00b7del"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "vull Snaps f\u00f6r'n D\u00f6st.", "tokens": ["vull", "Snaps", "f\u00f6r'n", "D\u00f6st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Vun St\u00e4wels dor holl se", "tokens": ["Vun", "St\u00e4\u00b7wels", "dor", "holl", "se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "tohop man een Poor", "tokens": ["to\u00b7hop", "man", "e\u00b7en", "Poor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "un lat' viermal d\u00f6pen", "tokens": ["un", "lat'", "vier\u00b7mal", "d\u00f6\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["FM", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "allmal in dree Johr.", "tokens": ["all\u00b7mal", "in", "dree", "Johr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Un wenn dat viert r\u00fcnd is,", "tokens": ["Un", "wenn", "dat", "viert", "r\u00fcnd", "is", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "denn so warrn dat ball s\u00f6\u00df,", "tokens": ["denn", "so", "warrn", "dat", "ball", "s\u00f6\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "un'n Kamm heww se gor nich \u2013", "tokens": ["un'n", "Kamm", "heww", "se", "gor", "nich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wen k\u00e4mmt Hasen un V\u00f6\u00df?", "tokens": ["wen", "k\u00e4mmt", "Ha\u00b7sen", "un", "V\u00f6\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "FM", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}