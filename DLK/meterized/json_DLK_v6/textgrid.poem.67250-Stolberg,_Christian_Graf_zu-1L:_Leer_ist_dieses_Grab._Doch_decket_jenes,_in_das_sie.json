{"textgrid.poem.67250": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leer ist dieses Grab. Doch decket jenes, in das sie", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leer ist dieses Grab. Doch decket jenes, in das sie", "tokens": ["Leer", "ist", "die\u00b7ses", "Grab", ".", "Doch", "de\u00b7cket", "je\u00b7nes", ",", "in", "das", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PDAT", "NN", "$.", "KON", "VVFIN", "PDS", "$,", "APPR", "PRELS", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Weinend ihn legten, nicht auch Asche nur selber und Staub?", "tokens": ["Wei\u00b7nend", "ihn", "leg\u00b7ten", ",", "nicht", "auch", "A\u00b7sche", "nur", "sel\u00b7ber", "und", "Staub", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "NN", "ADV", "ADV", "KON", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Er ist nicht ein Gruftbewohner. Da, wo nicht Tod, nicht", "tokens": ["Er", "ist", "nicht", "ein", "Gruft\u00b7be\u00b7woh\u00b7ner", ".", "Da", ",", "wo", "nicht", "Tod", ",", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "$.", "ADV", "$,", "PWAV", "PTKNEG", "NN", "$,", "PTKNEG"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Schmerz und die Thr\u00e4ne nicht ist, lebt der Unsterbliche nun.", "tokens": ["Schmerz", "und", "die", "Thr\u00e4\u00b7ne", "nicht", "ist", ",", "lebt", "der", "U\u00b7nsterb\u00b7li\u00b7che", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "PTKNEG", "VAFIN", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Unsre flossen f\u00fcr Dich; sie netzten die duftenden Stauden,", "tokens": ["Uns\u00b7re", "flos\u00b7sen", "f\u00fcr", "Dich", ";", "sie", "netz\u00b7ten", "die", "duf\u00b7ten\u00b7den", "Stau\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Netzten die Blumen des Maals deiner Erinnrung geweiht.", "tokens": ["Netz\u00b7ten", "die", "Blu\u00b7men", "des", "Maals", "dei\u00b7ner", "E\u00b7rinn\u00b7rung", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Auch uns liebtest du. Blick' herab, und bereite du dort, wie", "tokens": ["Auch", "uns", "lieb\u00b7test", "du", ".", "Blick'", "her\u00b7ab", ",", "und", "be\u00b7rei\u00b7te", "du", "dort", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$.", "NN", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "$,", "PWAV"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Deine Mutter f\u00fcr dich, Lauben des Himmels f\u00fcr uns!", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "f\u00fcr", "dich", ",", "Lau\u00b7ben", "des", "Him\u00b7mels", "f\u00fcr", "uns", "!"], "token_info": ["word", "word", "word", "word", "punct", "word"