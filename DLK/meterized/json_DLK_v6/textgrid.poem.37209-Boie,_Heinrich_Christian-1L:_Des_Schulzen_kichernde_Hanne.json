{"textgrid.poem.37209": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Schulzen kichernde Hanne", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Schulzen kichernde Hanne", "tokens": ["Des", "Schul\u00b7zen", "ki\u00b7chern\u00b7de", "Han\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Neckt nur und spottet der Kanne,", "tokens": ["Neckt", "nur", "und", "spot\u00b7tet", "der", "Kan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die gegen\u00fcber mir steht.", "tokens": ["Die", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "mir", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wie knapp auch immer sie geht,", "tokens": ["Wie", "knapp", "auch", "im\u00b7mer", "sie", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ich la\u00dfe, mag sie sich br\u00fcsten,", "tokens": ["Ich", "la\u00b7\u00dfe", ",", "mag", "sie", "sich", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mich ihrer traun! nicht gel\u00fcsten.", "tokens": ["Mich", "ih\u00b7rer", "traun", "!", "nicht", "ge\u00b7l\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "VVINF", "$.", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mir lob ich B\u00e4rbel die runde!", "tokens": ["Mir", "lob", "ich", "B\u00e4r\u00b7bel", "die", "run\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ART", "ADJA", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Komm ich mit lallendem Munde,", "tokens": ["Komm", "ich", "mit", "lal\u00b7len\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So reicht sie schlau mir das Glas:", "tokens": ["So", "reicht", "sie", "schlau", "mir", "das", "Glas", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "\u00bbauf mein Vergn\u00fcgen noch das!\u00ab", "tokens": ["\u00bb", "auf", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "noch", "das", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ADV", "ART", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie n\u00e4hm ich, wenn sie das Bette", "tokens": ["Sie", "n\u00e4hm", "ich", ",", "wenn", "sie", "das", "Bet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auch unterm Leibe nicht h\u00e4tte!", "tokens": ["Auch", "un\u00b7term", "Lei\u00b7be", "nicht", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}