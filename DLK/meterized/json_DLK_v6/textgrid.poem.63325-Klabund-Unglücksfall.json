{"textgrid.poem.63325": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ungl\u00fccksfall", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stehen vor dem Hebekran", "tokens": ["Es", "ste\u00b7hen", "vor", "dem", "He\u00b7be\u00b7kran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kleines Kind, ein Hund, ein Mann.", "tokens": ["Ein", "klei\u00b7nes", "Kind", ",", "ein", "Hund", ",", "ein", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Eisenkette rollt und rinnt,", "tokens": ["Die", "Ei\u00b7sen\u00b7ket\u00b7te", "rollt", "und", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es staunen Mann und Hund und Kind.", "tokens": ["Es", "stau\u00b7nen", "Mann", "und", "Hund", "und", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da saust sie nieder auf den Grund,", "tokens": ["Da", "saust", "sie", "nie\u00b7der", "auf", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zerschmettert Mann und Kind und Hund.", "tokens": ["Zer\u00b7schmet\u00b7tert", "Mann", "und", "Kind", "und", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gem\u00e4\u00dfigt naht die Polizei,", "tokens": ["Ge\u00b7m\u00e4\u00b7\u00dfigt", "naht", "die", "Po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Chemiker ist auch dabei,", "tokens": ["Ein", "Che\u00b7mi\u00b7ker", "ist", "auch", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bis er den Totbestand befund:", "tokens": ["Bis", "er", "den", "Tot\u00b7be\u00b7stand", "be\u00b7fund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein kleines Kind, ein Mann, ein Hund.", "tokens": ["Ein", "klei\u00b7nes", "Kind", ",", "ein", "Mann", ",", "ein", "Hund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}