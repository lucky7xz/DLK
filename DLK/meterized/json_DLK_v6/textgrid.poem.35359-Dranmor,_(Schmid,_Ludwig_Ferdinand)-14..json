{"textgrid.poem.35359": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wi\u00dft ihr, was eines Dichters Brust durchw\u00fchlt?", "tokens": ["Wi\u00dft", "ihr", ",", "was", "ei\u00b7nes", "Dich\u00b7ters", "Brust", "durch\u00b7w\u00fchlt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihn qu\u00e4lt das Todesr\u00f6cheln eines Lammes;", "tokens": ["Ihn", "qu\u00e4lt", "das", "To\u00b7des\u00b7r\u00f6\u00b7cheln", "ei\u00b7nes", "Lam\u00b7mes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verwandt ist alles ihm, was lebt und f\u00fchlt,", "tokens": ["Ver\u00b7wandt", "ist", "al\u00b7les", "ihm", ",", "was", "lebt", "und", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "PPER", "$,", "PWS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und was Gedanken hat, ist seines Stammes.", "tokens": ["Und", "was", "Ge\u00b7dan\u00b7ken", "hat", ",", "ist", "sei\u00b7nes", "Stam\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VAFIN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er sucht in Freundesherzen seine Wohnung,", "tokens": ["Er", "sucht", "in", "Freun\u00b7des\u00b7her\u00b7zen", "sei\u00b7ne", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Beharrlich, trotz der Seltenheit des Fundes,", "tokens": ["Be\u00b7harr\u00b7lich", ",", "trotz", "der", "Sel\u00b7ten\u00b7heit", "des", "Fun\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und mehr als karg gespendete Belohnung", "tokens": ["Und", "mehr", "als", "karg", "ge\u00b7spen\u00b7de\u00b7te", "Be\u00b7loh\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "R\u00fchrt ihn das treue Auge seines Hundes.", "tokens": ["R\u00fchrt", "ihn", "das", "treu\u00b7e", "Au\u00b7ge", "sei\u00b7nes", "Hun\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er kann nicht ernten, was er ausges\u00e4t;", "tokens": ["Er", "kann", "nicht", "ern\u00b7ten", ",", "was", "er", "aus\u00b7ge\u00b7s\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Gegenwart bel\u00e4chelt dessen Meinung,", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "be\u00b7l\u00e4\u00b7chelt", "des\u00b7sen", "Mei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der in so mancher schillernden Erscheinung", "tokens": ["Der", "in", "so", "man\u00b7cher", "schil\u00b7lern\u00b7den", "Er\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nichts achtet als des Todes Majest\u00e4t;", "tokens": ["Nichts", "ach\u00b7tet", "als", "des", "To\u00b7des", "Ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Heil w\u00fcnscht er jedem, der mit voller Hand", "tokens": ["Heil", "w\u00fcnscht", "er", "je\u00b7dem", ",", "der", "mit", "vol\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PIS", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sich zu den Armen und Verlass'nen wendet,", "tokens": ["Sich", "zu", "den", "Ar\u00b7men", "und", "Ver\u00b7lass'\u00b7nen", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der seinen Trost aus k\u00fchlen Bronnen spendet,", "tokens": ["Der", "sei\u00b7nen", "Trost", "aus", "k\u00fch\u00b7len", "Bron\u00b7nen", "spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Heil dem Propheten in der Sonne Brand!", "tokens": ["Heil", "dem", "Pro\u00b7phe\u00b7ten", "in", "der", "Son\u00b7ne", "Brand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Nicht jeder Dichter ist ein Quellenfinder;", "tokens": ["Nicht", "je\u00b7der", "Dich\u00b7ter", "ist", "ein", "Quel\u00b7len\u00b7fin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Doch allen schwebt das Wort des Meisters vor:", "tokens": ["Doch", "al\u00b7len", "schwebt", "das", "Wort", "des", "Meis\u00b7ters", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbunsterbliche heben verlorene Kinder", "tokens": ["\u00bb", "uns\u00b7terb\u00b7li\u00b7che", "he\u00b7ben", "ver\u00b7lo\u00b7re\u00b7ne", "Kin\u00b7der"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "VVFIN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.20": {"text": "Mit feurigen Armen zum Himmel empor.\u00ab", "tokens": ["Mit", "feu\u00b7ri\u00b7gen", "Ar\u00b7men", "zum", "Him\u00b7mel", "em\u00b7por", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}