{"textgrid.poem.65186": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "Der Versucher", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas du von dieses Berges Zinnen", "tokens": ["\u00bb", "was", "du", "von", "die\u00b7ses", "Ber\u00b7ges", "Zin\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erschaust im weitgedehnten Kreis,", "tokens": ["Er\u00b7schaust", "im", "weit\u00b7ge\u00b7dehn\u00b7ten", "Kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch meine Gunst kannst du's erringen,", "tokens": ["Durch", "mei\u00b7ne", "Gunst", "kannst", "du's", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, wahrlich, um geringen Preis.", "tokens": ["Und", ",", "wahr\u00b7lich", ",", "um", "ge\u00b7rin\u00b7gen", "Preis", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "$,", "KOUI", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich trage dich zu Ruhm und Ehre", "tokens": ["Ich", "tra\u00b7ge", "dich", "zu", "Ruhm", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Empor mit meines Fittichs Schwung!", "tokens": ["Em\u00b7por", "mit", "mei\u00b7nes", "Fit\u00b7tichs", "Schwung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du fragst, was ich daf\u00fcr begehre?", "tokens": ["Du", "fragst", ",", "was", "ich", "da\u00b7f\u00fcr", "be\u00b7geh\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts als nur deine Huldigung.", "tokens": ["Nichts", "als", "nur", "dei\u00b7ne", "Hul\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADV", "PPOSAT", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Jedwedes Ziel magst du erstreben,", "tokens": ["Jed\u00b7we\u00b7des", "Ziel", "magst", "du", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du vor mir die Kniee beugst,", "tokens": ["Wenn", "du", "vor", "mir", "die", "Kni\u00b7ee", "beugst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit der Ehrfurcht scheuem Beben", "tokens": ["Und", "mit", "der", "Ehr\u00b7furcht", "scheu\u00b7em", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr meine Oberhoheit zeugst.", "tokens": ["F\u00fcr", "mei\u00b7ne", "O\u00b7berh\u00b7o\u00b7heit", "zeugst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein sei das Ma\u00df der Herrlichkeiten,", "tokens": ["Dein", "sei", "das", "Ma\u00df", "der", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So lang du mir zu Willen bist!\u00ab", "tokens": ["So", "lang", "du", "mir", "zu", "Wil\u00b7len", "bist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "APPR", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Satan sprach's in alten Zeiten,", "tokens": ["Der", "Sa\u00b7tan", "sprach's", "in", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und heute sagt's der Journalist.", "tokens": ["Und", "heu\u00b7te", "sagt's", "der", "Jour\u00b7na\u00b7list", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}