{"textgrid.poem.67117": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Buonaparte", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige Gef\u00e4\u00dfe sind die Dichter,", "tokens": ["Hei\u00b7li\u00b7ge", "Ge\u00b7f\u00e4\u00b7\u00dfe", "sind", "die", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Worin des Lebens Wein, der Geist", "tokens": ["Wo\u00b7rin", "des", "Le\u00b7bens", "Wein", ",", "der", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Helden, sich aufbewahrt,", "tokens": ["Der", "Hel\u00b7den", ",", "sich", "auf\u00b7be\u00b7wahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber der Geist dieses J\u00fcnglings,", "tokens": ["A\u00b7ber", "der", "Geist", "die\u00b7ses", "J\u00fcng\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der schnelle, m\u00fc\u00dft er es nicht zersprengen,", "tokens": ["Der", "schnel\u00b7le", ",", "m\u00fc\u00dft", "er", "es", "nicht", "zer\u00b7spren\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo es ihn fassen wollte, das Gef\u00e4\u00df?", "tokens": ["Wo", "es", "ihn", "fas\u00b7sen", "woll\u00b7te", ",", "das", "Ge\u00b7f\u00e4\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Dichter la\u00df ihn unber\u00fchrt wie den Geist der Natur,", "tokens": ["Der", "Dich\u00b7ter", "la\u00df", "ihn", "un\u00b7be\u00b7r\u00fchrt", "wie", "den", "Geist", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "An solchem Stoffe wird zum Knaben der Meister.", "tokens": ["An", "sol\u00b7chem", "Stof\u00b7fe", "wird", "zum", "Kna\u00b7ben", "der", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Er kann im Gedichte nicht leben und bleiben,", "tokens": ["Er", "kann", "im", "Ge\u00b7dich\u00b7te", "nicht", "le\u00b7ben", "und", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er lebt und bleibt in der Welt.", "tokens": ["Er", "lebt", "und", "bleibt", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}