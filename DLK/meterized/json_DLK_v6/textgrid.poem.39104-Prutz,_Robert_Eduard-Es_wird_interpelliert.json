{"textgrid.poem.39104": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Es wird interpelliert", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, ihr Bienen, auf die Drohnen!", "tokens": ["Auf", ",", "ihr", "Bie\u00b7nen", ",", "auf", "die", "Droh\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Aufschub, ohne Schonen,", "tokens": ["Oh\u00b7ne", "Auf\u00b7schub", ",", "oh\u00b7ne", "Scho\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "auf, ihr Interpellationen!", "tokens": ["auf", ",", "ihr", "In\u00b7ter\u00b7pel\u00b7la\u00b7ti\u00b7o\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dem Dolche, mit der Klinge,", "tokens": ["Mit", "dem", "Dol\u00b7che", ",", "mit", "der", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "mit der Rede goldnem Schwert,", "tokens": ["mit", "der", "Re\u00b7de", "gold\u00b7nem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "auf, o Stolz der Nationen!", "tokens": ["auf", ",", "o", "Stolz", "der", "Na\u00b7ti\u00b7o\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Endlich mu\u00df das Land es h\u00f6ren,", "tokens": ["End\u00b7lich", "mu\u00df", "das", "Land", "es", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "endlich mu\u00df das Volk es wissen;", "tokens": ["end\u00b7lich", "mu\u00df", "das", "Volk", "es", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "alle Bande sind zerrissen,", "tokens": ["al\u00b7le", "Ban\u00b7de", "sind", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "wenn es dieses noch erf\u00e4hrt:", "tokens": ["wenn", "es", "die\u00b7ses", "noch", "er\u00b7f\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u00dcber gro\u00dfe und geringe,", "tokens": ["\u00dc\u00b7ber", "gro\u00b7\u00dfe", "und", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "\u00fcber wahr' und falsche Dinge,", "tokens": ["\u00fc\u00b7ber", "wahr'", "und", "fal\u00b7sche", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "da\u00df die Herzen sich emp\u00f6ren,", "tokens": ["da\u00df", "die", "Her\u00b7zen", "sich", "em\u00b7p\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "ohne Aufschub, ohne Schonen,", "tokens": ["oh\u00b7ne", "Auf\u00b7schub", ",", "oh\u00b7ne", "Scho\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "auf, ihr Interpellationen! \u2013", "tokens": ["auf", ",", "ihr", "In\u00b7ter\u00b7pel\u00b7la\u00b7ti\u00b7o\u00b7nen", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Nun \u2013 und man interpellierte,", "tokens": ["Nun", "\u2013", "und", "man", "in\u00b7ter\u00b7pel\u00b7lier\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "r\u00e4sonierte, debattierte,", "tokens": ["r\u00e4\u00b7so\u00b7nier\u00b7te", ",", "de\u00b7bat\u00b7tier\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.18": {"text": "da\u00df es selbst die Steine r\u00fchrte,", "tokens": ["da\u00df", "es", "selbst", "die", "Stei\u00b7ne", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "und bewunderndes Gefl\u00fcster", "tokens": ["und", "be\u00b7wun\u00b7dern\u00b7des", "Ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "fernste Rechte selbst durchdrang \u2013", "tokens": ["ferns\u00b7te", "Rech\u00b7te", "selbst", "durch\u00b7drang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Und was weiter drauf passierte?", "tokens": ["Und", "was", "wei\u00b7ter", "drauf", "pas\u00b7sier\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ward dem Lande davon wohler?", "tokens": ["Ward", "dem", "Lan\u00b7de", "da\u00b7von", "woh\u00b7ler", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PAV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Werden jetzt die Kurse steigen?", "tokens": ["Wer\u00b7den", "jetzt", "die", "Kur\u00b7se", "stei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Ei, ich d\u00e4chte! Dumpfes Schweigen", "tokens": ["Ei", ",", "ich", "d\u00e4ch\u00b7te", "!", "Dum\u00b7pfes", "Schwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "blieb auf der Ministerbank ...", "tokens": ["blieb", "auf", "der", "Mi\u00b7nis\u00b7ter\u00b7bank", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Hol der Kuckuck und sein K\u00fcster", "tokens": ["Hol", "der", "Ku\u00b7ckuck", "und", "sein", "K\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "diese schweigsamen Minister!", "tokens": ["die\u00b7se", "schweig\u00b7sa\u00b7men", "Mi\u00b7nis\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.28": {"text": "Doch zuerst vor allen hol er,", "tokens": ["Doch", "zu\u00b7erst", "vor", "al\u00b7len", "hol", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "ohne Aufschub, ohne Schonen,", "tokens": ["oh\u00b7ne", "Auf\u00b7schub", ",", "oh\u00b7ne", "Scho\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "hol die Interpellationen!", "tokens": ["hol", "die", "In\u00b7ter\u00b7pel\u00b7la\u00b7ti\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}