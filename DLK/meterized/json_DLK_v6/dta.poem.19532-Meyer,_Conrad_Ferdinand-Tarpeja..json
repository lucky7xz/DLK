{"dta.poem.19532": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Tarpeja.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Am Brunnen \u00fcberfluthet im D\u00e4mmerlicht", "tokens": ["Am", "Brun\u00b7nen", "\u00fc\u00b7berf\u00b7lut\u00b7het", "im", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der volle Krug und die M\u00e4gde merken's nicht,", "tokens": ["Der", "vol\u00b7le", "Krug", "und", "die", "M\u00e4g\u00b7de", "mer\u00b7ken's", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "NE", "PTKNEG", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn Nina plaudert: \u201eFreundinnen, wisst ihr wohl,", "tokens": ["Denn", "Ni\u00b7na", "plau\u00b7dert", ":", "\u201e", "Freun\u00b7din\u00b7nen", ",", "wisst", "ihr", "wohl", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "$(", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df Eine sitzt im Gestein am Capitol?", "tokens": ["Da\u00df", "Ei\u00b7ne", "sitzt", "im", "Ge\u00b7stein", "am", "Ca\u00b7pi\u00b7tol", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Schatz, der Beppo, hat sie unl\u00e4ngst gesehn", "tokens": ["Mein", "Schatz", ",", "der", "Bep\u00b7po", ",", "hat", "sie", "un\u00b7l\u00e4ngst", "ge\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Vor ihrem runden Silberspiegel stehn,", "tokens": ["Vor", "ih\u00b7rem", "run\u00b7den", "Sil\u00b7ber\u00b7spie\u00b7gel", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die sich zu Haupt das g\u00fcldene Kr\u00f6nlein hub \u2014", "tokens": ["Die", "sich", "zu", "Haupt", "das", "g\u00fcl\u00b7de\u00b7ne", "Kr\u00f6n\u00b7lein", "hub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mein Schatz, der Beppo, da er nach M\u00fcnzen grub.", "tokens": ["Mein", "Schatz", ",", "der", "Bep\u00b7po", ",", "da", "er", "nach", "M\u00fcn\u00b7zen", "grub", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Er schl\u00fcpfte durch einen schmalen Felsengang,", "tokens": ["Er", "schl\u00fcpf\u00b7te", "durch", "ei\u00b7nen", "schma\u00b7len", "Fel\u00b7sen\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er tappte sich einen finstern Pfad entlang \u2014", "tokens": ["Er", "tapp\u00b7te", "sich", "ei\u00b7nen", "fins\u00b7tern", "Pfad", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "APPO", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie glomm in H\u00f6llenlicht! Er rief: \u201eWie sch\u00f6n!\u201c", "tokens": ["Sie", "glomm", "in", "H\u00f6l\u00b7len\u00b7licht", "!", "Er", "rief", ":", "\u201e", "Wie", "sch\u00f6n", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "$.", "$(", "PWAV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Treppe brach mit donnerndem Get\u00f6n.", "tokens": ["Die", "Trep\u00b7pe", "brach", "mit", "don\u00b7nern\u00b7dem", "Ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie war des r\u00f6mischen Castellanes Kind", "tokens": ["Sie", "war", "des", "r\u00f6\u00b7mi\u00b7schen", "Cas\u00b7tel\u00b7la\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sie verrieth die Burg und das Burggesind!", "tokens": ["Und", "sie", "ver\u00b7rieth", "die", "Burg", "und", "das", "Burg\u00b7ge\u00b7sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Fingerdeut bedang sich die schlaue Maid", "tokens": ["Mit", "Fin\u00b7ger\u00b7deut", "be\u00b7dang", "sich", "die", "schlau\u00b7e", "Maid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Feindes Helmgekr\u00f6n und Schildgeschmeid!", "tokens": ["Des", "Fein\u00b7des", "Helm\u00b7ge\u00b7kr\u00f6n", "und", "Schild\u00b7ge\u00b7schmeid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Kr\u00f6nlein all und die Stein' und die goldnen Ring'", "tokens": ["Die", "Kr\u00f6n\u00b7lein", "all", "und", "die", "Stein'", "und", "die", "gold\u00b7nen", "Ring'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "KON", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Be\u00e4ugelt' sie, die in Feindes Lager ging!", "tokens": ["Be\u00b7\u00e4u\u00b7gelt'", "sie", ",", "die", "in", "Fein\u00b7des", "La\u00b7ger", "ging", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie \u00f6ffnet' ihm ein Thor mit s\u00fcnd'gem Mut", "tokens": ["Sie", "\u00f6ff\u00b7net'", "ihm", "ein", "Thor", "mit", "s\u00fcn\u00b7d'\u00b7gem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und sah des Vaters Haupt, es schwamm in Blut.", "tokens": ["Und", "sah", "des", "Va\u00b7ters", "Haupt", ",", "es", "schwamm", "in", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch da am Feinde sie die L\u00f6hnung sucht',", "tokens": ["Doch", "da", "am", "Fein\u00b7de", "sie", "die", "L\u00f6h\u00b7nung", "sucht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ward sie mit Hohn erdr\u00fcckt und mit Schildeswucht,", "tokens": ["Ward", "sie", "mit", "Hohn", "er\u00b7dr\u00fcckt", "und", "mit", "Schil\u00b7des\u00b7wucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "KON", "APPR", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sie st\u00fcrzte, von ihrem eigenen Hort entseelt,", "tokens": ["Sie", "st\u00fcrz\u00b7te", ",", "von", "ih\u00b7rem", "ei\u00b7ge\u00b7nen", "Hort", "ent\u00b7seelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Erstickt vom Lohne den sie selbst gew\u00e4hlt.", "tokens": ["Er\u00b7stickt", "vom", "Loh\u00b7ne", "den", "sie", "selbst", "ge\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Dann grub die Zeit sie tief und tiefer ein,", "tokens": ["Dann", "grub", "die", "Zeit", "sie", "tief", "und", "tie\u00b7fer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sank hinunter, hinab ins Felsgestein,", "tokens": ["Sie", "sank", "hin\u00b7un\u00b7ter", ",", "hin\u00b7ab", "ins", "Fels\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hinab, hinunter viel hundert Klafter tief", "tokens": ["Hin\u00b7ab", ",", "hin\u00b7un\u00b7ter", "viel", "hun\u00b7dert", "Klaf\u00b7ter", "tief"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "CARD", "NN", "ADJD"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit ihrem glei\u00dfenden Hort, darin sie schlief.", "tokens": ["Mit", "ih\u00b7rem", "glei\u00b7\u00dfen\u00b7den", "Hort", ",", "da\u00b7rin", "sie", "schlief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Da sitzt die arme Seele nun in Pein", "tokens": ["Da", "sitzt", "die", "ar\u00b7me", "See\u00b7le", "nun", "in", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und putzt, die eitle, sich mutterseelallein \u2014", "tokens": ["Und", "putzt", ",", "die", "eit\u00b7le", ",", "sich", "mut\u00b7ter\u00b7see\u00b7lal\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "$,", "PRF", "ADJD", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tarpeja, gieb heraus der Kettlein drei!", "tokens": ["Tar\u00b7pe\u00b7ja", ",", "gieb", "he\u00b7raus", "der", "Ket\u00b7tlein", "drei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ADV", "ART", "NN", "CARD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wir tragen's den Knaben zu Lust in L\u00fcften frei!", "tokens": ["Wir", "tra\u00b7gen's", "den", "Kna\u00b7ben", "zu", "Lust", "in", "L\u00fcf\u00b7ten", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Tarpeja, gleite durch den Felsenspalt", "tokens": ["Tar\u00b7pe\u00b7ja", ",", "glei\u00b7te", "durch", "den", "Fel\u00b7sen\u00b7spalt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Drei Kettlein und drei goldene Ringlein bald!", "tokens": ["Drei", "Ket\u00b7tlein", "und", "drei", "gol\u00b7de\u00b7ne", "Rin\u00b7glein", "bald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "CARD", "ADJA", "NN", "ADV", "$."], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tarpeja lieb! Wir sind zufrieden, giebst", "tokens": ["Tar\u00b7pe\u00b7ja", "lieb", "!", "Wir", "sind", "zu\u00b7frie\u00b7den", ",", "giebst"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADJD", "$.", "PPER", "VAFIN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du nur, was du ver\u00e4chtlich bei Seite schiebst.", "tokens": ["Du", "nur", ",", "was", "du", "ver\u00b7\u00e4cht\u00b7lich", "bei", "Sei\u00b7te", "schiebst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Beppo sagt: weil du begingst Verrat,", "tokens": ["Der", "Bep\u00b7po", "sagt", ":", "weil", "du", "be\u00b7gingst", "Ver\u00b7rat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist du verdammt f\u00fcr deine Missethat!", "tokens": ["Bist", "du", "ver\u00b7dammt", "f\u00fcr", "dei\u00b7ne", "Mis\u00b7se\u00b7that", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beh\u00fct' mich Gott! In Ewigkeit verdammt!", "tokens": ["Be\u00b7h\u00fct'", "mich", "Gott", "!", "In", "E\u00b7wig\u00b7keit", "ver\u00b7dammt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil dir nach rothem Gold das Herz geflammt.", "tokens": ["Weil", "dir", "nach", "ro\u00b7them", "Gold", "das", "Herz", "ge\u00b7flammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Man h\u00f6rt es oft \u2014 so sagt er \u2014 wie du lachst", "tokens": ["Man", "h\u00f6rt", "es", "oft", "so", "sagt", "er", "wie", "du", "lachst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$(", "ADV", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wann du dich sch\u00f6n vor deinem Spiegel machst!", "tokens": ["Wann", "du", "dich", "sch\u00f6n", "vor", "dei\u00b7nem", "Spie\u00b7gel", "machst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man h\u00f6rt es oft \u2014 so sagt er \u2014 wie du weinst,", "tokens": ["Man", "h\u00f6rt", "es", "oft", "so", "sagt", "er", "wie", "du", "weinst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$(", "ADV", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weil nicht du kommst in den sch\u00f6nen Himmel einst!", "tokens": ["Weil", "nicht", "du", "kommst", "in", "den", "sch\u00f6\u00b7nen", "Him\u00b7mel", "einst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Tarpeja lieb, entsage der b\u00f6sen Lust!", "tokens": ["Tar\u00b7pe\u00b7ja", "lieb", ",", "ent\u00b7sa\u00b7ge", "der", "b\u00f6\u00b7sen", "Lus