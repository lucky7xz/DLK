{"textgrid.poem.37699": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Durstige", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Gott, wie th\u00e4t mir gut", "tokens": ["Ach", "Gott", ",", "wie", "th\u00e4t", "mir", "gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Ku\u00df auf meinem Mund,", "tokens": ["Ein", "Ku\u00df", "auf", "mei\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lippe w\u00e4r' nicht wund", "tokens": ["Die", "Lip\u00b7pe", "w\u00e4r'", "nicht", "wund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Durst und hei\u00dfer Gluth;", "tokens": ["Von", "Durst", "und", "hei\u00b7\u00dfer", "Gluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich w\u00e4re dann gesund", "tokens": ["Ich", "w\u00e4\u00b7re", "dann", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ruhig lief mein Blut,", "tokens": ["Und", "ru\u00b7hig", "lief", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ach Gott, wie th\u00e4t mir gut", "tokens": ["Ach", "Gott", ",", "wie", "th\u00e4t", "mir", "gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein Ku\u00df auf ihrem Mund.", "tokens": ["Ein", "Ku\u00df", "auf", "ih\u00b7rem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Liebe w\u00e4r' dann aus,", "tokens": ["Die", "Lie\u00b7be", "w\u00e4r'", "dann", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich w\u00fcrde flei\u00dfig sein,", "tokens": ["Ich", "w\u00fcr\u00b7de", "flei\u00b7\u00dfig", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fiel mir manches ein,", "tokens": ["Es", "fiel", "mir", "man\u00b7ches", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich bliebe dann zu Haus;", "tokens": ["Ich", "blie\u00b7be", "dann", "zu", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich flieg' dem Leuchtwurm nach,", "tokens": ["Ich", "flieg'", "dem", "Leucht\u00b7wurm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ihn l\u00f6scht kein Dunkel aus,", "tokens": ["Ihn", "l\u00f6scht", "kein", "Dun\u00b7kel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er fliegt zu Liebchens Haus,", "tokens": ["Er", "fliegt", "zu", "Lieb\u00b7chens", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wenn niemand drinnen wach.", "tokens": ["Wenn", "nie\u00b7mand", "drin\u00b7nen", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach Gott, so mu\u00df ich hin", "tokens": ["Ach", "Gott", ",", "so", "mu\u00df", "ich", "hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "ADV", "VMFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In jeder n\u00e4cht'gen Stund',", "tokens": ["In", "je\u00b7der", "n\u00e4cht'\u00b7gen", "Stund'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird schon allen kund,", "tokens": ["Es", "wird", "schon", "al\u00b7len", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich verliebet bin;", "tokens": ["Da\u00df", "ich", "ver\u00b7lie\u00b7bet", "bin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ob ich geliebet bin,", "tokens": ["Ob", "ich", "ge\u00b7lie\u00b7bet", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach Gott, thu es mir kund,", "tokens": ["Ach", "Gott", ",", "thu", "es", "mir", "kund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Durch ihren sch\u00f6nen Mund,", "tokens": ["Durch", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zu ihr mu\u00df ich jetzt hin.", "tokens": ["Zu", "ihr", "mu\u00df", "ich", "jetzt", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach Gott, heut schlie\u00df' mich ein", "tokens": ["Ach", "Gott", ",", "heut", "schlie\u00df'", "mich", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In ihre Lippen dicht,", "tokens": ["In", "ih\u00b7re", "Lip\u00b7pen", "dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im n\u00e4chtlichen Gesicht,", "tokens": ["Im", "n\u00e4cht\u00b7li\u00b7chen", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sind sie wie Wellenschein,", "tokens": ["Sind", "sie", "wie", "Wel\u00b7len\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach brennen hei\u00df und licht,", "tokens": ["Ach", "bren\u00b7nen", "hei\u00df", "und", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVINF", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Erl\u00f6sche mich darein,", "tokens": ["Er\u00b7l\u00f6\u00b7sche", "mich", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es kann nicht anders sein", "tokens": ["Es", "kann", "nicht", "an\u00b7ders", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und ich versag's mir nicht.", "tokens": ["Und", "ich", "ver\u00b7sag's", "mir", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}