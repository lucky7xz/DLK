{"textgrid.poem.56914": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Weib, ein Hund, ein Segelboot,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Weib, ein Hund, ein Segelboot,", "tokens": ["Ein", "Weib", ",", "ein", "Hund", ",", "ein", "Se\u00b7gel\u00b7boot", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mein Freund, sein Weib und sonst nichts mehr;", "tokens": ["mein", "Freund", ",", "sein", "Weib", "und", "sonst", "nichts", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein freies Schaffen, ein edler Tod,", "tokens": ["ein", "frei\u00b7es", "Schaf\u00b7fen", ",", "ein", "ed\u00b7ler", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "das w\u00e4re so mein Begehr.", "tokens": ["das", "w\u00e4\u00b7re", "so", "mein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Verga\u00df ich nichts? Wer fehlt noch, wer?", "tokens": ["Ver\u00b7ga\u00df", "ich", "nichts", "?", "Wer", "fehlt", "noch", ",", "wer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$.", "PWS", "VVFIN", "ADV", "$,", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Triumph wider den Tod!", "tokens": ["Mein", "Tri\u00b7umph", "wi\u00b7der", "den", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Sohn, dem mein Wollen im Blute loht", "tokens": ["Ein", "Sohn", ",", "dem", "mein", "Wol\u00b7len", "im", "Blu\u00b7te", "loht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und Kraft noch tausendmal mehr.", "tokens": ["und", "Kraft", "noch", "tau\u00b7send\u00b7mal", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}