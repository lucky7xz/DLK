{"dta.poem.10060": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Schnee-Ball-Bluhme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist mir? seh ich recht? ich seh,", "tokens": ["Wie", "ist", "mir", "?", "seh", "ich", "recht", "?", "ich", "seh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm schw\u00fclen Strahl der Sonnen, Schnee,", "tokens": ["Jm", "schw\u00fc\u00b7len", "Strahl", "der", "Son\u00b7nen", ",", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zwar recht als in runden Ballen,", "tokens": ["Und", "zwar", "recht", "als", "in", "run\u00b7den", "Bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf gr\u00fcn belaubten Zweigen wallen.", "tokens": ["Auf", "gr\u00fcn", "be\u00b7laub\u00b7ten", "Zwei\u00b7gen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kann Schnee sich mit der Gluth, und Laub mit Schnee", "tokens": ["Kann", "Schnee", "sich", "mit", "der", "Gluth", ",", "und", "Laub", "mit", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PRF", "APPR", "ART", "NN", "$,", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So deucht mich, hatt\u2019 ich recht zu fragen,", "tokens": ["So", "deucht", "mich", ",", "hatt'", "ich", "recht", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich dein blendend wei\u00df, geliebte Bluhme,", "tokens": ["Als", "ich", "dein", "blen\u00b7dend", "wei\u00df", ",", "ge\u00b7lieb\u00b7te", "Bluh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die man mit Recht, vom Schnee-Ball, Schnee-Ball heisst,", "tokens": ["Die", "man", "mit", "Recht", ",", "vom", "Schnee\u00b7Ball", ",", "Schnee\u00b7Ball", "heisst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "$,", "APPRART", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem inniglich ger\u00fchrten Geist,", "tokens": ["Mit", "ei\u00b7nem", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fchr\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In meiner Lust, zu deines Sch\u00f6pfers Ruhme,", "tokens": ["In", "mei\u00b7ner", "Lust", ",", "zu", "dei\u00b7nes", "Sch\u00f6p\u00b7fers", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit aufmercksamen Aug\u2019 erblickte.", "tokens": ["Mit", "auf\u00b7merck\u00b7sa\u00b7men", "Aug'", "er\u00b7blick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du sch\u00f6ner Bluhmen-Baum, du zeigst, in deiner Zier,", "tokens": ["Du", "sch\u00f6\u00b7ner", "Bluh\u00b7men\u00b7Baum", ",", "du", "zeigst", ",", "in", "dei\u00b7ner", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Weisheit, Macht und Huld desjenigen, so dir", "tokens": ["Die", "Weis\u00b7heit", ",", "Macht", "und", "Huld", "des\u00b7je\u00b7ni\u00b7gen", ",", "so", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "PAV", "$,", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Wesen mitgetheilt, der dich so lieblich schm\u00fcckte:", "tokens": ["Dein", "We\u00b7sen", "mit\u00b7ge\u00b7theilt", ",", "der", "dich", "so", "lieb\u00b7lich", "schm\u00fcck\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die\u00df ergetzet mich. Nicht nur die Seele sp\u00fcret,", "tokens": ["Und", "die\u00df", "er\u00b7get\u00b7zet", "mich", ".", "Nicht", "nur", "die", "See\u00b7le", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$.", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df sie ein Andachts-Strahl, ein Trieb zur Unschuld r\u00fchret,", "tokens": ["Da\u00df", "sie", "ein", "An\u00b7dachts\u00b7Strahl", ",", "ein", "Trieb", "zur", "Un\u00b7schuld", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Sch\u00f6pfer, so wie mir dein unbefleckter Schein", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", ",", "so", "wie", "mir", "dein", "un\u00b7be\u00b7fleck\u00b7ter", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "KOKOM", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gef\u00e4llt, in reinem Glantz gef\u00e4llig auch zu seyn.", "tokens": ["Ge\u00b7f\u00e4llt", ",", "in", "rei\u00b7nem", "Glantz", "ge\u00b7f\u00e4l\u00b7lig", "auch", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "ADJD", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach! la\u00df, o ewigs Gut, mein br\u00fcnstiges Verlangen", "tokens": ["Ach", "!", "la\u00df", ",", "o", "e\u00b7wigs", "Gut", ",", "mein", "br\u00fcns\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VVIMP", "$,", "FM", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Dir erh\u00f6ret seyn: weil wir so gar das Wollen,", "tokens": ["Von", "Dir", "er\u00b7h\u00f6\u00b7ret", "seyn", ":", "weil", "wir", "so", "gar", "das", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$.", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dasjenige zu thun, was wir verrichten sollen,", "tokens": ["Das\u00b7je\u00b7ni\u00b7ge", "zu", "thun", ",", "was", "wir", "ver\u00b7rich\u00b7ten", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht haben, wo wir es von Dir nicht auch empfangen!", "tokens": ["Nicht", "ha\u00b7ben", ",", "wo", "wir", "es", "von", "Dir", "nicht", "auch", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAINF", "$,", "PWAV", "PPER", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nachdem ich nun hierauf verschiedene gepfl\u00fccket,", "tokens": ["Nach\u00b7dem", "ich", "nun", "hier\u00b7auf", "ver\u00b7schie\u00b7de\u00b7ne", "ge\u00b7pfl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um ihre Bildung recht zu sehu,", "tokens": ["Um", "ih\u00b7re", "Bil\u00b7dung", "recht", "zu", "se\u00b7hu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und wie sie von der Hand der bildenden Natur", "tokens": ["Und", "wie", "sie", "von", "der", "Hand", "der", "bil\u00b7den\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So Circul-rund, als wie ein Ball, gedr\u00fccket;", "tokens": ["So", "Cir\u00b7cul\u00b7rund", ",", "als", "wie", "ein", "Ball", ",", "ge\u00b7dr\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erblickt\u2019 ich, voller Lust, nicht nur,", "tokens": ["Er\u00b7blickt'", "ich", ",", "vol\u00b7ler", "Lust", ",", "nicht", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df von dem Stengel sich f\u00fcnf Haupt-Zweig\u2019 artig strecken.", "tokens": ["Da\u00df", "von", "dem", "Sten\u00b7gel", "sich", "f\u00fcnf", "Haup\u00b7tZweig'", "ar\u00b7tig", "stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "CARD", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein ieder theilte sich in eben so viel Ecken,", "tokens": ["Ein", "ie\u00b7der", "theil\u00b7te", "sich", "in", "e\u00b7ben", "so", "viel", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und iede breitete von neuen", "tokens": ["Und", "ie\u00b7de", "brei\u00b7te\u00b7te", "von", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sich in f\u00fcnf Stengelchen, auf deren iedem denn", "tokens": ["Sich", "in", "f\u00fcnf", "Sten\u00b7gel\u00b7chen", ",", "auf", "de\u00b7ren", "ie\u00b7dem", "denn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "CARD", "NN", "$,", "APPR", "PRELAT", "PIS", "ADV"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ein Bl\u00fcmchen mit f\u00fcnf Bl\u00e4tterchen,", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "mit", "f\u00fcnf", "Bl\u00e4t\u00b7ter\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.11": {"text": "Die so an Form als Farbe sch\u00f6n,", "tokens": ["Die", "so", "an", "Form", "als", "Far\u00b7be", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KOUS", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Unmittelbar, und ohne Kelch, zu sehn,", "tokens": ["Un\u00b7mit\u00b7tel\u00b7bar", ",", "und", "oh\u00b7ne", "Kelch", ",", "zu", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "APPR", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und die, da alle gleiche weit", "tokens": ["Und", "die", ",", "da", "al\u00b7le", "glei\u00b7che", "weit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "$,", "KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Von ihrem Mittel-Punct entfernet stehen,", "tokens": ["Von", "ih\u00b7rem", "Mit\u00b7tel\u00b7Punct", "ent\u00b7fer\u00b7net", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ich in so angenehm-vollkommner R\u00fcnde", "tokens": ["Ich", "in", "so", "an\u00b7ge\u00b7nehm\u00b7voll\u00b7komm\u00b7ner", "R\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mit sonderbarer Anmuth finde.", "tokens": ["Mit", "son\u00b7der\u00b7ba\u00b7rer", "An\u00b7muth", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Von ungemeiner Lieblichkeit", "tokens": ["Von", "un\u00b7ge\u00b7mei\u00b7ner", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist dieser Bluhmen Wei\u00df. Es bricht", "tokens": ["Ist", "die\u00b7ser", "Bluh\u00b7men", "Wei\u00df", ".", "Es", "bricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indem zweyhundert achtzig Bluhmen", "tokens": ["In\u00b7dem", "zwey\u00b7hun\u00b7dert", "acht\u00b7zig", "Bluh\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf vierzehn hundert Bl\u00e4tter zeigen,", "tokens": ["Auf", "vier\u00b7zehn", "hun\u00b7dert", "Bl\u00e4t\u00b7ter", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die sich auf manche Weise beugen)", "tokens": ["Die", "sich", "auf", "man\u00b7che", "Wei\u00b7se", "beu\u00b7gen", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Auf manche Weise sich das Licht:", "tokens": ["Auf", "man\u00b7che", "Wei\u00b7se", "sich", "das", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wodurch wir, da bey so viel H\u00f6hen", "tokens": ["Wo\u00b7durch", "wir", ",", "da", "bey", "so", "viel", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KOUS", "APPR", "ADV", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Die ein Schnee-weisser Glantz bestrahlt,", "tokens": ["Die", "ein", "Schnee\u00b7weis\u00b7ser", "Glantz", "be\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Auch eben so viel Tieffen sehen,", "tokens": ["Auch", "e\u00b