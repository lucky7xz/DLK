{"textgrid.poem.66173": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir, ", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, ", "tokens": ["Dir", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Des Erdengottes Friederich entflo\u00df,", "tokens": ["Des", "Er\u00b7den\u00b7got\u00b7tes", "Frie\u00b7de\u00b7rich", "ent\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Gott ins Herz mit seines Vaters Muthe", "tokens": ["Dem", "Gott", "ins", "Herz", "mit", "sei\u00b7nes", "Va\u00b7ters", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mutter Z\u00e4rtlichkeit aus goldner Schale go\u00df;", "tokens": ["Der", "Mut\u00b7ter", "Z\u00e4rt\u00b7lich\u00b7keit", "aus", "gold\u00b7ner", "Scha\u00b7le", "go\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dir, Nachbild deines Ahnherrn Alexanders,", "tokens": ["Dir", ",", "Nach\u00b7bild", "dei\u00b7nes", "Ahn\u00b7herrn", "A\u00b7lex\u00b7an\u00b7ders", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hoch im Kreis der Teckerf\u00fcrsten steht,", "tokens": ["Der", "hoch", "im", "Kreis", "der", "Te\u00b7cker\u00b7f\u00fcrs\u00b7ten", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um die, wie um die Helden des Skamanders,", "tokens": ["Um", "die", ",", "wie", "um", "die", "Hel\u00b7den", "des", "Ska\u00b7man\u00b7ders", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Des ew'gen Nachruhms Fl\u00fcgel weht;", "tokens": ["Des", "ew'\u00b7gen", "Nach\u00b7ruhms", "Fl\u00fc\u00b7gel", "weht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir donnert \u2013 wie aus feurigem Metalle", "tokens": ["Dir", "don\u00b7nert", "\u2013", "wie", "aus", "feu\u00b7ri\u00b7gem", "Me\u00b7tal\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Alexanderberges Genius", "tokens": ["Des", "A\u00b7lex\u00b7an\u00b7der\u00b7ber\u00b7ges", "Ge\u00b7nius"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Herab vom wolkenblauen Walle", "tokens": ["Her\u00b7ab", "vom", "wol\u00b7ken\u00b7blau\u00b7en", "Wal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Heldenohr den kriegerischen Gru\u00df.", "tokens": ["Ins", "Hel\u00b7de\u00b7nohr", "den", "krie\u00b7ge\u00b7ri\u00b7schen", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er sah's, wie Joseph dir, dich seinem Dienst zu weihen,", "tokens": ["Er", "sah's", ",", "wie", "Jo\u00b7se\u00b7ph", "dir", ",", "dich", "sei\u00b7nem", "Dienst", "zu", "wei\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "PPER", "$,", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Wodanschwert um deine H\u00fcften schnallt,", "tokens": ["Ein", "Wo\u00b7dan\u00b7schwert", "um", "dei\u00b7ne", "H\u00fcf\u00b7ten", "schnallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie dein Federbusch in dichtgedr\u00e4ngten Reihen", "tokens": ["Und", "wie", "dein", "Fe\u00b7der\u00b7busch", "in", "dicht\u00b7ge\u00b7dr\u00e4ng\u00b7ten", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Helden Oestreichs furchtbardrohend wallt.", "tokens": ["Der", "Hel\u00b7den", "O\u00b7e\u00b7streichs", "furcht\u00b7bar\u00b7dro\u00b7hend", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Er sah es, wie aus goldges\u00e4umter Wolke", "tokens": ["Er", "sah", "es", ",", "wie", "aus", "gold\u00b7ge\u00b7s\u00e4um\u00b7ter", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Sitze der Unsterblichkeit,", "tokens": ["Vom", "Sit\u00b7ze", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dein Ahnherr niedersah und dich vor Deutschlands Volke", "tokens": ["Dein", "Ahn\u00b7herr", "nie\u00b7der\u00b7sah", "und", "dich", "vor", "Deutschlands", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "PRF", "APPR", "NE", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zum Helden eingeweiht.", "tokens": ["Zum", "Hel\u00b7den", "ein\u00b7ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbauch ich,\u00ab sprach Alexander, \u00bbhab' getragen", "tokens": ["\u00bb", "auch", "ich", ",", "\u00ab", "sprach", "A\u00b7lex\u00b7an\u00b7der", ",", "\u00bb", "hab'", "ge\u00b7tra\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ADV", "PPER", "$,", "$(", "VVFIN", "NE", "$,", "$(", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies Schwert, das dir nun Joseph giebt,", "tokens": ["Dies", "Schwert", ",", "das", "dir", "nun", "Jo\u00b7se\u00b7ph", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hab' mit den Feinden Oestreichs mich geschlagen", "tokens": ["Hab'", "mit", "den", "Fein\u00b7den", "O\u00b7e\u00b7streichs", "mich", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NE", "PPER", "VVPP"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und Josephs V\u00e4ter treu geliebt;", "tokens": ["Und", "Jo\u00b7se\u00b7phs", "V\u00e4\u00b7ter", "treu", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich ging als edler deutscher Ritter", "tokens": ["Ich", "ging", "als", "ed\u00b7ler", "deut\u00b7scher", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oft hoch im Donnerfeld der Schlacht,", "tokens": ["Oft", "hoch", "im", "Don\u00b7ner\u00b7feld", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df von zertretnen Sch\u00e4deln mir die Splitter", "tokens": ["Da\u00df", "von", "zer\u00b7tret\u00b7nen", "Sch\u00e4\u00b7deln", "mir", "die", "Split\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sohlen wund gemacht.", "tokens": ["Die", "Soh\u00b7len", "wund", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So streit' auch du, befeu'rt von meinem Bilde,", "tokens": ["So", "streit'", "auch", "du", ",", "be\u00b7feu'rt", "von", "mei\u00b7nem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O du mein Enkel Ferdinand!", "tokens": ["O", "du", "mein", "En\u00b7kel", "Fer\u00b7di\u00b7nand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott decke dich mit seinem Flammenschilde!\u00ab", "tokens": ["Gott", "de\u00b7cke", "dich", "mit", "sei\u00b7nem", "Flam\u00b7men\u00b7schil\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "So sprach dein Ahnherr und verschwand.", "tokens": ["So", "sprach", "dein", "Ahn\u00b7herr", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Dein Ahnherr! Ach der F\u00fcrsten Beste,", "tokens": ["Dein", "Ahn\u00b7herr", "!", "Ach", "der", "F\u00fcrs\u00b7ten", "Bes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unsres Berges Schutzgeist war.", "tokens": ["Der", "uns\u00b7res", "Ber\u00b7ges", "Schutz\u00b7geist", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch steht f\u00fcr ihn im Tempel meiner Veste", "tokens": ["Noch", "steht", "f\u00fcr", "ihn", "im", "Tem\u00b7pel", "mei\u00b7ner", "Ves\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein ewig rauchender Altar.", "tokens": ["Ein", "e\u00b7wig", "rau\u00b7chen\u00b7der", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mit einem Felsengurt umth\u00fcrmte", "tokens": ["Mit", "ei\u00b7nem", "Fel\u00b7sen\u00b7gurt", "umt\u00b7h\u00fcrm\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er unsern Berg und wandelte auf ihm;", "tokens": ["Er", "un\u00b7sern", "Berg", "und", "wan\u00b7del\u00b7te", "auf", "ihm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn Wettersturm auf Aspergs Rippen st\u00fcrmte,", "tokens": ["Wenn", "Wet\u00b7ter\u00b7sturm", "auf", "As\u00b7pergs", "Rip\u00b7pen", "st\u00fcrm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So trotzt' er k\u00fchn des Wetters Ungest\u00fcm.", "tokens": ["So", "trotzt'", "er", "k\u00fchn", "des", "Wet\u00b7ters", "Un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Sein Geist ist nun dem Himmel zugeflogen,", "tokens": ["Sein", "Geist", "ist", "nun", "dem", "Him\u00b7mel", "zu\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihn reizt nicht Erdengr\u00f6\u00dfe mehr;", "tokens": ["Ihn", "reizt", "nicht", "Er\u00b7den\u00b7gr\u00f6\u00b7\u00dfe", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hat er Enkel nachgezogen,", "tokens": ["Doch", "hat", "er", "En\u00b7kel", "nach\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So deutsch und gro\u00df wie er.", "tokens": ["So", "deutsch", "und", "gro\u00df", "wie", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Du Ferdinand, dem schon aus weichen Haaren", "tokens": ["Du", "Fer\u00b7di\u00b7nand", ",", "dem", "schon", "aus", "wei\u00b7chen", "Haa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lorbeer keimt, der du dein Stammhaus zierst;", "tokens": ["Der", "Lor\u00b7beer", "keimt", ",", "der", "du", "dein", "Stamm\u00b7haus", "zierst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie gl\u00fccklich sind die Kriegesschaaren,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "sind", "die", "Krie\u00b7ges\u00b7schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die du dereinst ins Feld der Ehre f\u00fchrst;", "tokens": ["Die", "du", "de\u00b7reinst", "ins", "Feld", "der", "Eh\u00b7re", "f\u00fchrst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Nimm deinen gro\u00dfen Ahnherrn zum Exempel,", "tokens": ["Nimm", "dei\u00b7nen", "gro\u00b7\u00dfen", "Ahn\u00b7herrn", "zum", "Ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sei Held! sei Menschenfreund! sei Christ!", "tokens": ["Sei", "Held", "!", "sei", "Men\u00b7schen\u00b7freund", "!", "sei", "Christ", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "VAFIN", "NN", "$.", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis du einst sp\u00e4t im Heldentempel", "tokens": ["Bis", "du", "einst", "sp\u00e4t", "im", "Hel\u00b7den\u00b7tem\u00b7pel"], "token_info": ["word", "