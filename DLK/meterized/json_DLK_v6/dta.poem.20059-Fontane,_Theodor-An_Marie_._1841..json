{"dta.poem.20059": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Marie .  \n 1841.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Maria, zur Madonne,", "tokens": ["Zur", "Ma\u00b7ria", ",", "zur", "Ma\u00b7don\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "APPRART", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Bet\u2019 ich gl\u00e4ubig sp\u00e4t und fr\u00fch,", "tokens": ["Bet'", "ich", "gl\u00e4u\u00b7big", "sp\u00e4t", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Sein ist Andachtswonne", "tokens": ["All", "mein", "Sein", "ist", "An\u00b7dachts\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor der himmlischen Marie.", "tokens": ["Vor", "der", "himm\u00b7li\u00b7schen", "Ma\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und das Himmelskind Maria\u2019s,", "tokens": ["Und", "das", "Him\u00b7mel\u00b7skind", "Ma\u00b7ria's", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Erl\u00f6ser Jesus Christ,", "tokens": ["Der", "Er\u00b7l\u00f6\u00b7ser", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, die Liebe \u2014 der Messias", "tokens": ["Ja", ",", "die", "Lie\u00b7be", "der", "Mes\u00b7si\u00b7as"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich mir erschienen ist.", "tokens": ["End\u00b7lich", "mir", "er\u00b7schie\u00b7nen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df Maria doch verbliebe", "tokens": ["Da\u00df", "Ma\u00b7ria", "doch", "ver\u00b7blie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihrem Kinde treugesinnt!", "tokens": ["Ih\u00b7rem", "Kin\u00b7de", "treu\u00b7ge\u00b7sinnt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6dtet sie in mir die Liebe", "tokens": ["T\u00f6d\u00b7tet", "sie", "in", "mir", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreuzigt sie das eigne Kind.", "tokens": ["Kreu\u00b7zigt", "sie", "das", "eig\u00b7ne", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}