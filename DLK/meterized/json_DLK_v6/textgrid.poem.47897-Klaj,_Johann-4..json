{"textgrid.poem.47897": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fried im Himmel!", "tokens": ["Fried", "im", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Rufft der Engel Heerget\u00fcmmel:", "tokens": ["Rufft", "der", "En\u00b7gel", "Heer\u00b7ge\u00b7t\u00fcm\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Krieg auff Erden!", "tokens": ["Krieg", "auff", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ach! was will es immer werden?", "tokens": ["Ach", "!", "was", "will", "es", "im\u00b7mer", "wer\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich von dem Siegen sagen/", "tokens": ["Soll", "ich", "von", "dem", "Sie\u00b7gen", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder zagen?", "tokens": ["O\u00b7der", "za\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "H\u00f6rt/ die Engel singen wieder!", "tokens": ["H\u00f6rt", "/", "die", "En\u00b7gel", "sin\u00b7gen", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber leider! unsre Lieder", "tokens": ["A\u00b7ber", "lei\u00b7der", "!", "uns\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sind in Eile", "tokens": ["Sind", "in", "Ei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "wie die windgeschwinde Pfeile/", "tokens": ["wie", "die", "wind\u00b7ge\u00b7schwin\u00b7de", "Pfei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Von dem Widerhall zergellet", "tokens": ["Von", "dem", "Wi\u00b7der\u00b7hall", "zer\u00b7gel\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "in den Hal\u00df zu ruck geprellet.", "tokens": ["in", "den", "Hal\u00df", "zu", "ruck", "ge\u00b7prel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "O ihr Himmel regnet Feuer", "tokens": ["O", "ihr", "Him\u00b7mel", "reg\u00b7net", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "auff das Teufflisch Vngeheuer!", "tokens": ["auff", "das", "Teuf\u00b7flisch", "Vn\u00b7ge\u00b7heu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Seht die Schwefelblaue Nasen", "tokens": ["Seht", "die", "Schwe\u00b7felb\u00b7lau\u00b7e", "Na\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "rasen/ blasen:", "tokens": ["ra\u00b7sen", "/", "bla\u00b7sen", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "da\u00df die kriegerische Flammen", "tokens": ["da\u00df", "die", "krie\u00b7ge\u00b7ri\u00b7sche", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "brennen Liechterloh zusammen.", "tokens": ["bren\u00b7nen", "Liech\u00b7ter\u00b7loh", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Ich bin gantz au\u00df Furcht erstarrt: mu\u00df die Reden n\u00f6then", "tokens": ["Ich", "bin", "gantz", "au\u00df", "Furcht", "er\u00b7starrt", ":", "mu\u00df", "die", "Re\u00b7den", "n\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$.", "VMFIN", "ART", "NN", "ADJA"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "wann ich den Ertz-Engel Streit/ und den Kunst-Poeten/", "tokens": ["wann", "ich", "den", "Ertz\u00b7En\u00b7gel", "Streit", "/", "und", "den", "Kunst\u00b7Po\u00b7e\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "$(", "KON", "ART", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "den das teutsche Nordgau r\u00fchmet/ recht beloben will/", "tokens": ["den", "das", "teut\u00b7sche", "Nord\u00b7gau", "r\u00fch\u00b7met", "/", "recht", "be\u00b7lo\u00b7ben", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$(", "ADJD", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "O ihr Ei\u00dfgefrorne Musen schweigt nur di\u00dfmal still.", "tokens": ["O", "ihr", "Ei\u00df\u00b7ge\u00b7fror\u00b7ne", "Mu\u00b7sen", "schweigt", "nur", "di\u00df\u00b7mal", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Klio h\u00e4lt den Feuergeist/ den Apollo angez\u00fcndet/", "tokens": ["Klio", "h\u00e4lt", "den", "Feu\u00b7er\u00b7geist", "/", "den", "A\u00b7pol\u00b7lo", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$(", "ART", "NE", "VVPP", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "da\u00df Herr Klaj unser Freund/ Engelreine Reimen bindet.", "tokens": ["da\u00df", "Herr", "Klaj", "un\u00b7ser", "Freund", "/", "En\u00b7gel\u00b7rei\u00b7ne", "Rei\u00b7men", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "PPOSAT", "NN", "$(", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Der kan bunte Verse schreib\u0113 von dem Feuer-Drachen-Kampf/", "tokens": ["Der", "kan", "bun\u00b7te", "Ver\u00b7se", "schreib\u0113", "von", "dem", "Feu\u00b7er\u00b7Dra\u00b7chen\u00b7Kampf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der/ der l\u00e4st die Stralen blitz\u0113 durch den d\u00fcstern P\u00f6bel-dampf/", "tokens": ["Der", "/", "der", "l\u00e4st", "die", "Stra\u00b7len", "blitz\u0113", "durch", "den", "d\u00fcs\u00b7tern", "P\u00f6\u00b7bel\u00b7dampf", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "die Crystalgestreimte Pegnitz mu\u00df mit Wellen-zungen lallen/", "tokens": ["die", "Crys\u00b7tal\u00b7ge\u00b7streim\u00b7te", "Peg\u00b7nitz", "mu\u00df", "mit", "Wel\u00b7len\u00b7zun\u00b7gen", "lal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "wen sie h\u00f6rt die Klinggedichte an den gl\u00e4sern Strand abhallen.", "tokens": ["wen", "sie", "h\u00f6rt", "die", "Kling\u00b7ge\u00b7dich\u00b7te", "an", "den", "gl\u00e4\u00b7sern", "Strand", "ab\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Alle Musen heissen ihn nur ihren Sohn/", "tokens": ["Al\u00b7le", "Mu\u00b7sen", "heis\u00b7sen", "ihn", "nur", "ih\u00b7ren", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Alle Nymfen reichen ihm den S\u00e4nger Lohn.", "tokens": ["Al\u00b7le", "Nym\u00b7fen", "rei\u00b7chen", "ihm", "den", "S\u00e4n\u00b7ger", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}