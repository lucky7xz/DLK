{"textgrid.poem.43960": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bistu denn noch Leonore,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bistu denn noch Leonore,", "tokens": ["Bis\u00b7tu", "denn", "noch", "Le\u00b7o\u00b7no\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "NE", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Der so manch verliebter Schwur", "tokens": ["Der", "so", "manch", "ver\u00b7lieb\u00b7ter", "Schwur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "(sinne nach, bey welchem Thore!)", "tokens": ["(", "sin\u00b7ne", "nach", ",", "bey", "wel\u00b7chem", "Tho\u00b7re", "!", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "APPR", "PWAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Ku\u00df und Schmerz entfuhr,", "tokens": ["Un\u00b7ter", "Ku\u00df", "und", "Schmerz", "ent\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, so nimm die stummen Lieder", "tokens": ["Ach", ",", "so", "nimm", "die", "stum\u00b7men", "Lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eben noch mit dieser Hand,", "tokens": ["E\u00b7ben", "noch", "mit", "die\u00b7ser", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die mir ehmahls Herz und Glieder", "tokens": ["Die", "mir", "eh\u00b7mahls", "Herz", "und", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit der st\u00e4rcksten Reizung band.", "tokens": ["Mit", "der", "st\u00e4rcks\u00b7ten", "Rei\u00b7zung", "band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch dein sehnliches Entbehren", "tokens": ["Durch", "dein", "sehn\u00b7li\u00b7ches", "Ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd ich vor den Jahren grau,", "tokens": ["Werd", "ich", "vor", "den", "Jah\u00b7ren", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Zuflu\u00df meiner Z\u00e4hren", "tokens": ["Und", "der", "Zu\u00b7flu\u00df", "mei\u00b7ner", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Mehrt schon lange Reif und Thau;", "tokens": ["Mehrt", "schon", "lan\u00b7ge", "Reif", "und", "Thau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Schwachheit, mein Verbleichen", "tokens": ["Mei\u00b7ne", "Schwach\u00b7heit", ",", "mein", "Ver\u00b7blei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Brust, so st\u00fcndlich lechst,", "tokens": ["Und", "die", "Brust", ",", "so", "st\u00fcnd\u00b7lich", "lechst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird des Kummers Siegeszeichen,", "tokens": ["Wird", "des", "Kum\u00b7mers", "Sie\u00b7ges\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der aus unsrer Trennung w\u00e4chst.", "tokens": ["Der", "aus", "uns\u00b7rer", "Tren\u00b7nung", "w\u00e4chst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lust und Muth und Geist zum Dichten,", "tokens": ["Lust", "und", "Muth", "und", "Geist", "zum", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feuer, Jugend, Ruhm und Flei\u00df", "tokens": ["Feu\u00b7er", ",", "Ju\u00b7gend", ",", "Ruhm", "und", "Flei\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchen mit Gewalt zu fl\u00fcchten", "tokens": ["Su\u00b7chen", "mit", "Ge\u00b7walt", "zu", "fl\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verlieren ihren Preis,", "tokens": ["Und", "ver\u00b7lie\u00b7ren", "ih\u00b7ren", "Preis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil der Zunder deiner K\u00fc\u00dfe", "tokens": ["Weil", "der", "Zun\u00b7der", "dei\u00b7ner", "K\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinen Trieb nicht mehr erweckt", "tokens": ["Mei\u00b7nen", "Trieb", "nicht", "mehr", "er\u00b7weckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die F\u00fchrung harter Schl\u00fc\u00dfe", "tokens": ["Und", "die", "F\u00fch\u00b7rung", "har\u00b7ter", "Schl\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein betr\u00fcbtes Ziel gesteckt.", "tokens": ["Ein", "be\u00b7tr\u00fcb\u00b7tes", "Ziel", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle Bilder meiner Sinnen", "tokens": ["Al\u00b7le", "Bil\u00b7der", "mei\u00b7ner", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind mir Eckel und Verdru\u00df,", "tokens": ["Sind", "mir", "E\u00b7ckel", "und", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sie nichts als Gram gewinnen,", "tokens": ["Da", "sie", "nichts", "als", "Gram", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich dich noch suchen mu\u00df;", "tokens": ["Weil", "ich", "dich", "noch", "su\u00b7chen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts erg\u00f6zt mich mehr auf Erden", "tokens": ["Nichts", "er\u00b7g\u00f6zt", "mich", "mehr", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als das Weinen in der Nacht,", "tokens": ["Als", "das", "Wei\u00b7nen", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn es unter viel Beschwerden", "tokens": ["Wenn", "es", "un\u00b7ter", "viel", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dein Ged\u00e4chtn\u00fc\u00df munter macht.", "tokens": ["Dein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "mun\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jedes Blat von deinen H\u00e4nden", "tokens": ["Je\u00b7des", "Blat", "von", "dei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein Blat voll Klag und Weh,", "tokens": ["Ist", "ein", "Blat", "voll", "Klag", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich kan es niemahls wenden,", "tokens": ["Und", "ich", "kan", "es", "nie\u00b7mahls", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df kein Stich ans Herze geh;", "tokens": ["Da\u00df", "kein", "Stich", "ans", "Her\u00b7ze", "geh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Versichrung leerer Zeilen", "tokens": ["Die", "Ver\u00b7sich\u00b7rung", "lee\u00b7rer", "Zei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Giebt den Leibern wenig Kraft,", "tokens": ["Giebt", "den", "Lei\u00b7bern", "we\u00b7nig", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welche Luft und Ort zertheilen.", "tokens": ["Wel\u00b7che", "Luft", "und", "Ort", "zer\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "O bedr\u00e4ngte Leidenschaft!", "tokens": ["O", "be\u00b7dr\u00e4ng\u00b7te", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}