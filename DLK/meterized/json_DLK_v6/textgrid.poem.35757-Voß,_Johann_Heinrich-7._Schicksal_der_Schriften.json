{"textgrid.poem.35757": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7. Schicksal der Schriften", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer auf gemeiner Bahn gemeine Werke treibet:", "tokens": ["Wer", "auf", "ge\u00b7mei\u00b7ner", "Bahn", "ge\u00b7mei\u00b7ne", "Wer\u00b7ke", "trei\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leicht macht er's allen recht;", "tokens": ["Leicht", "macht", "er's", "al\u00b7len", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gemacht in kurzem hat er's schlecht.", "tokens": ["Ge\u00b7macht", "in", "kur\u00b7zem", "hat", "er's", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "VAFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer neue Bahnen w\u00e4hlt, k\u00fchn denkt und edel schreibet:", "tokens": ["Wer", "neu\u00b7e", "Bah\u00b7nen", "w\u00e4hlt", ",", "k\u00fchn", "denkt", "und", "e\u00b7del", "schrei\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Leicht macht er's allen schlecht;", "tokens": ["Leicht", "macht", "er's", "al\u00b7len", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gemacht in kurzem hat er's recht.", "tokens": ["Ge\u00b7macht", "in", "kur\u00b7zem", "hat", "er's", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}