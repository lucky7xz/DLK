{"dta.poem.11038": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n unertr\u00e4gliche Empfindlichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir strafen, statt der andern, uns; wir zeigen, da\u00df sie\ngro\u00df, wir klein,", "tokens": ["Wir", "stra\u00b7fen", ",", "statt", "der", "an\u00b7dern", ",", "uns", ";", "wir", "zei\u00b7gen", ",", "da\u00df", "sie", "gro\u00df", ",", "wir", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ART", "ADJA", "$,", "PPER", "$.", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADJD", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Sie klug, wir aber stolz und gr\u00e4mlich; wenn wir zu sehr em-", "tokens": ["Sie", "klug", ",", "wir", "a\u00b7ber", "stolz", "und", "gr\u00e4m\u00b7lich", ";", "wenn", "wir", "zu", "sehr", "em"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "$,", "PPER", "ADV", "ADJD", "KON", "ADJD", "$.", "KOUS", "PPER", "PTKA", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So da\u00df, von der Empfindlichkeit, ich die\u00df mit h\u00f6chstem Rechte", "tokens": ["So", "da\u00df", ",", "von", "der", "Emp\u00b7find\u00b7lich\u00b7keit", ",", "ich", "die\u00df", "mit", "h\u00f6chs\u00b7tem", "Rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "$,", "APPR", "ART", "NN", "$,", "PPER", "PDS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Sie sey ein Selbstverrath, von einer in uns vorhandnen stol-", "tokens": ["Sie", "sey", "ein", "Selbst\u00b7ver\u00b7rath", ",", "von", "ei\u00b7ner", "in", "uns", "vor\u00b7hand\u00b7nen", "stol"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "APPR", "PPER", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}