{"textgrid.poem.53404": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Welcher doch den Tod f\u00fcr allen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Welcher doch den Tod f\u00fcr allen", "tokens": ["O", "Wel\u00b7cher", "doch", "den", "Tod", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADV", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm liesse nimmermehr entfallen:", "tokens": ["Ihm", "lies\u00b7se", "nim\u00b7mer\u00b7mehr", "ent\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist uns gar zu sehr gemein.", "tokens": ["Er", "ist", "uns", "gar", "zu", "sehr", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir m\u00fcssen uns mit ihm schon stallen", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "mit", "ihm", "schon", "stal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie alt und jung wir m\u00f6gen seyn.", "tokens": ["Wie", "alt", "und", "jung", "wir", "m\u00f6\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du solst dir keine Rechnung machen", "tokens": ["Du", "solst", "dir", "kei\u00b7ne", "Rech\u00b7nung", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von diesen oder jenen Sachen,", "tokens": ["Von", "die\u00b7sen", "o\u00b7der", "je\u00b7nen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du weisst nicht was die Satzung spricht,", "tokens": ["Du", "weisst", "nicht", "was", "die", "Sat\u00b7zung", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn von des Todes weitem Rachen,", "tokens": ["Ohn", "von", "des", "To\u00b7des", "wei\u00b7tem", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nur dieser Rechnung fehlst du nicht.", "tokens": ["Nur", "die\u00b7ser", "Rech\u00b7nung", "fehlst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Such, J\u00fcngling, in den zarten Jahren", "tokens": ["Such", ",", "J\u00fcng\u00b7ling", ",", "in", "den", "zar\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der edlen Tugend nach zu fahren.", "tokens": ["Der", "ed\u00b7len", "Tu\u00b7gend", "nach", "zu", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verla\u00df die Eltern und dein Haus,", "tokens": ["Ver\u00b7la\u00df", "die", "El\u00b7tern", "und", "dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schm\u00fcck dein Gem\u00fcte mit den Wahren", "tokens": ["Schm\u00fcck", "dein", "Ge\u00b7m\u00fc\u00b7te", "mit", "den", "Wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der K\u00fcnst' und Sprachen l\u00f6blich aus.", "tokens": ["Der", "K\u00fcnst'", "und", "Spra\u00b7chen", "l\u00f6b\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vnd k\u00f6mmst du dan nach Hause wieder", "tokens": ["Vnd", "k\u00f6mmst", "du", "dan", "nach", "Hau\u00b7se", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der deinen Trost, so leg dich nieder", "tokens": ["Der", "dei\u00b7nen", "Trost", ",", "so", "leg", "dich", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erw\u00fcrget durch des Todes Macht,", "tokens": ["Er\u00b7w\u00fcr\u00b7get", "durch", "des", "To\u00b7des", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Betr\u00fcb die Mutter, Schwestern, Br\u00fcder", "tokens": ["Be\u00b7tr\u00fcb", "die", "Mut\u00b7ter", ",", "Schwes\u00b7tern", ",", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd gieb uns allen gute Nacht.", "tokens": ["Vnd", "gieb", "uns", "al\u00b7len", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was schwebet der stets auff den Wellen", "tokens": ["Was", "schwe\u00b7bet", "der", "stets", "auff", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem schn\u00f6den Reichthum nach zu stellen?", "tokens": ["Dem", "schn\u00f6\u00b7den", "Reicht\u00b7hum", "nach", "zu", "stel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der folgt dem L\u00e4ger fort und fort", "tokens": ["Der", "folgt", "dem", "L\u00e4\u00b7ger", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd suchet seinen Feind zu f\u00e4llen,", "tokens": ["Vnd", "su\u00b7chet", "sei\u00b7nen", "Feind", "zu", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn selbst f\u00e4llt bald der Satzung Mord.", "tokens": ["Ihn", "selbst", "f\u00e4llt", "bald", "der", "Sat\u00b7zung", "Mord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Wol dem der stets gedenckt zusterben", "tokens": ["Wol", "dem", "der", "stets", "ge\u00b7denckt", "zus\u00b7ter\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ART", "ADV", "VVPP", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sucht Gottes Gnade zu erwerben,", "tokens": ["Sucht", "Got\u00b7tes", "Gna\u00b7de", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und zeucht von S\u00fcnd und Bo\u00dfheit weit,", "tokens": ["Und", "zeucht", "von", "S\u00fcnd", "und", "Bo\u00df\u00b7heit", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird durch keinen Tod verderben,", "tokens": ["Der", "wird", "durch", "kei\u00b7nen", "Tod", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er stirbt und lebt in Ewigheit!", "tokens": ["Er", "stirbt", "und", "lebt", "in", "E\u00b7wig\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr Jesu, f\u00fchr durch deine G\u00fcte", "tokens": ["Herr", "Je\u00b7su", ",", "f\u00fchr", "durch", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tod uns allzeit zu Gem\u00fcte,", "tokens": ["Den", "Tod", "uns", "all\u00b7zeit", "zu", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Damit wir klug seyn immerdar,", "tokens": ["Da\u00b7mit", "wir", "klug", "seyn", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer also stirbt, auch in der Bl\u00fcte", "tokens": ["Wer", "al\u00b7so", "stirbt", ",", "auch", "in", "der", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der f\u00e4hrt dahin im grawen Haar.", "tokens": ["Der", "f\u00e4hrt", "da\u00b7hin", "im", "gra\u00b7wen", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}