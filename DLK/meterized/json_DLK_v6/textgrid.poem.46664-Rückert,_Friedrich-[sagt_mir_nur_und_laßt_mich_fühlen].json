{"textgrid.poem.46664": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sagt mir nur und la\u00dft mich f\u00fchlen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagt mir nur und la\u00dft mich f\u00fchlen,", "tokens": ["Sagt", "mir", "nur", "und", "la\u00dft", "mich", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich noch was andres bin", "tokens": ["Da\u00df", "ich", "noch", "was", "and\u00b7res", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PWS", "PIS", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein Vater, dem durchw\u00fchlen", "tokens": ["Als", "ein", "Va\u00b7ter", ",", "dem", "durch\u00b7w\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Schmerzen jeden Sinn!", "tokens": ["Die\u00b7se", "Schmer\u00b7zen", "je\u00b7den", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sagt mir nur und la\u00dft mich f\u00fchlen,", "tokens": ["Sagt", "mir", "nur", "und", "la\u00dft", "mich", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich sonst noch etwas bin", "tokens": ["Da\u00df", "ich", "sonst", "noch", "et\u00b7was", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIS", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein Vater! denn es sp\u00fclen", "tokens": ["Als", "ein", "Va\u00b7ter", "!", "denn", "es", "sp\u00fc\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$.", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich die Vaterschmerzen hin.", "tokens": ["Mich", "die", "Va\u00b7ter\u00b7schmer\u00b7zen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sagt es mir und la\u00dft mich's f\u00fchlen,", "tokens": ["Sagt", "es", "mir", "und", "la\u00dft", "mich's", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KON", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich au\u00dfer'm Vater bin,", "tokens": ["Was", "ich", "au\u00b7\u00dfer'm", "Va\u00b7ter", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei's ein Dichter! auch den k\u00fchlen", "tokens": ["Sei's", "ein", "Dich\u00b7ter", "!", "auch", "den", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weltruhm nehm ich f\u00fcr Gewinn.", "tokens": ["Welt\u00b7ruhm", "nehm", "ich", "f\u00fcr", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn der Vater ist vernichtet,", "tokens": ["Denn", "der", "Va\u00b7ter", "ist", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Engel sind entschwebt;", "tokens": ["Sei\u00b7ne", "En\u00b7gel", "sind", "ent\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' ich auch umsonst gedichtet,", "tokens": ["Hab'", "ich", "auch", "um\u00b7sonst", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wozu hab' ich dann gelebt?", "tokens": ["Wo\u00b7zu", "hab'", "ich", "dann", "ge\u00b7lebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}