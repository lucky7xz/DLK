{"textgrid.poem.65886": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Keats", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geheimer Mondschein unter Mittagsstrahlen,", "tokens": ["Ge\u00b7hei\u00b7mer", "Mond\u00b7schein", "un\u00b7ter", "Mit\u00b7tags\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit sachter Muschel, fahl wie D\u00e4mmerung,", "tokens": ["Mit", "sach\u00b7ter", "Mu\u00b7schel", ",", "fahl", "wie", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Erstaunt dich Aphrodites naher Schwung", "tokens": ["Er\u00b7staunt", "dich", "A\u00b7phro\u00b7di\u00b7tes", "na\u00b7her", "Schwung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf eigner Rhythmen Fl\u00fcgeltum bei Qualen.", "tokens": ["Auf", "eig\u00b7ner", "Rhyth\u00b7men", "Fl\u00fc\u00b7gel\u00b7tum", "bei", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es dunkle nie ein Blut in Kupferschalen,", "tokens": ["Es", "dunk\u00b7le", "nie", "ein", "Blut", "in", "Kup\u00b7fer\u00b7scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das du, Verwundeten zur Linderung \u2013", "tokens": ["Das", "du", ",", "Ver\u00b7wun\u00b7de\u00b7ten", "zur", "Lin\u00b7de\u00b7rung", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als G\u00fcte Narben schlo\u00df \u2013 durch Hilfesprung", "tokens": ["Als", "G\u00fc\u00b7te", "Nar\u00b7ben", "schlo\u00df", "\u2013", "durch", "Hil\u00b7fe\u00b7sprung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "NN", "VVFIN", "$(", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Obhut nahmst, auf lichtblauen Sandalen!", "tokens": ["In", "Ob\u00b7hut", "nahmst", ",", "auf", "licht\u00b7blau\u00b7en", "San\u00b7da\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Der Atem Hellas' \u00fcber Sonnenbuchten", "tokens": ["Der", "A\u00b7tem", "Hel\u00b7las'", "\u00fc\u00b7ber", "Son\u00b7nen\u00b7buch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewog deiner Gebietung klarem Strom,", "tokens": ["Be\u00b7wog", "dei\u00b7ner", "Ge\u00b7bie\u00b7tung", "kla\u00b7rem", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Um Sch\u00f6nheit schluchzend, Berge zu durchschluchten.", "tokens": ["Um", "Sch\u00f6n\u00b7heit", "schluch\u00b7zend", ",", "Ber\u00b7ge", "zu", "durch\u00b7schluch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADJD", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geklungne Wonne aus geliebtem Rom,", "tokens": ["Ge\u00b7klung\u00b7ne", "Won\u00b7ne", "aus", "ge\u00b7lieb\u00b7tem", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das deine H\u00fclle ewig sch\u00fctzend h\u00fclle,", "tokens": ["Das", "dei\u00b7ne", "H\u00fcl\u00b7le", "e\u00b7wig", "sch\u00fct\u00b7zend", "h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gabst du der H\u00fcgelburg mit Dankesf\u00fclle.", "tokens": ["Gabst", "du", "der", "H\u00fc\u00b7gel\u00b7burg", "mit", "Dan\u00b7kes\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}