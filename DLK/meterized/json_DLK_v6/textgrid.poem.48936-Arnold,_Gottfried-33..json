{"textgrid.poem.48936": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie grauet doch dem Fleisch vor seinem Tod/", "tokens": ["Wie", "grau\u00b7et", "doch", "dem", "Fleisch", "vor", "sei\u00b7nem", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es fleucht das Creutz/ ersch\u00fcttert vor dem Leiden/", "tokens": ["Es", "fleucht", "das", "Creutz", "/", "er\u00b7sch\u00fct\u00b7tert", "vor", "dem", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum sind dem Geist des Glaubens Fl\u00fcgel noth/", "tokens": ["Drum", "sind", "dem", "Geist", "des", "Glau\u00b7bens", "Fl\u00fc\u00b7gel", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ART", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der mich auffhebt und bringt in deine Seiten/", "tokens": ["Der", "mich", "auff\u00b7hebt", "und", "bringt", "in", "dei\u00b7ne", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da steig ich frisch die Jacobs-Leiter an/", "tokens": ["Da", "steig", "ich", "frisch", "die", "Ja\u00b7cobs\u00b7Lei\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach geh mir doch von deinem Creutz entgegen/", "tokens": ["Ach", "geh", "mir", "doch", "von", "dei\u00b7nem", "Creutz", "ent\u00b7ge\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach la\u00df mich fein in deine Wunden legen/", "tokens": ["Ach", "la\u00df", "mich", "fein", "in", "dei\u00b7ne", "Wun\u00b7den", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und bind mich fest/ da\u00df ich nicht weichen kan!", "tokens": ["Und", "bind", "mich", "fest", "/", "da\u00df", "ich", "nicht", "wei\u00b7chen", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKVZ", "$(", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "La\u00df die Natur entkr\u00e4fftet sincken hin/", "tokens": ["La\u00df", "die", "Na\u00b7tur", "ent\u00b7kr\u00e4ff\u00b7tet", "sin\u00b7cken", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPO", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und Lust und Nutz und Ehre seyn erstorben/", "tokens": ["Und", "Lust", "und", "Nutz", "und", "Eh\u00b7re", "seyn", "ers\u00b7tor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Bi\u00df ich der Welt ein rechtes Scheusal bin/", "tokens": ["Bi\u00df", "ich", "der", "Welt", "ein", "rech\u00b7tes", "Scheu\u00b7sal", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und an dem Creutz in Adam gantz verdorben.", "tokens": ["Und", "an", "dem", "Creutz", "in", "A\u00b7dam", "gantz", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Erheb daf\u00fcr den neuen Sinn empor/", "tokens": ["Er\u00b7heb", "da\u00b7f\u00fcr", "den", "neu\u00b7en", "Sinn", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und mach ihn leicht zu dir hinauff zu fliegen/", "tokens": ["Und", "mach", "ihn", "leicht", "zu", "dir", "hin\u00b7auff", "zu", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Leg immer ihm die leichten Lasten vor/", "tokens": ["Leg", "im\u00b7mer", "ihm", "die", "leich\u00b7ten", "Las\u00b7ten", "vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Darunter er sich schmiegen soll und biegen.", "tokens": ["Da\u00b7run\u00b7ter", "er", "sich", "schmie\u00b7gen", "soll", "und", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVINF", "VMFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "O la\u00df mich stets zu dir gestrecket seyn/", "tokens": ["O", "la\u00df", "mich", "stets", "zu", "dir", "ge\u00b7stre\u00b7cket", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und doch noch mehr ins Tieffe gehen ein:", "tokens": ["Und", "doch", "noch", "mehr", "ins", "Tief\u00b7fe", "ge\u00b7hen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Hoch an dem Creutz/ tieff in mein Nichts gesencket/", "tokens": ["Hoch", "an", "dem", "Creutz", "/", "tieff", "in", "mein", "Nichts", "ge\u00b7sen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$(", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Bi\u00df du mir gantz und ich dir werd geschencket.", "tokens": ["Bi\u00df", "du", "mir", "gantz", "und", "ich", "dir", "werd", "ge\u00b7schen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "KON", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}