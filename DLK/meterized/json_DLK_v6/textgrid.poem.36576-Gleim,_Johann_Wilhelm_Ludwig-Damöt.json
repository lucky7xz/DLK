{"textgrid.poem.36576": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Dam\u00f6t", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der liebensw\u00fcrdige Dam\u00f6t", "tokens": ["Der", "lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7ge", "Da\u00b7m\u00f6t"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fand neulich mich allein,", "tokens": ["Fand", "neu\u00b7lich", "mich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und klagte, bis des Abends sp\u00e4t,", "tokens": ["Und", "klag\u00b7te", ",", "bis", "des", "A\u00b7bends", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir seine Liebespein!", "tokens": ["Mir", "sei\u00b7ne", "Lie\u00b7be\u00b7spein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein h\u00f6chster Wunsch, sprach er, bist du!", "tokens": ["Mein", "h\u00f6chs\u00b7ter", "Wunsch", ",", "sprach", "er", ",", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es lie\u00df ihm gar zu sch\u00f6n,", "tokens": ["Es", "lie\u00df", "ihm", "gar", "zu", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kam mein Mamachen nicht dazu;", "tokens": ["Kam", "mein", "Ma\u00b7ma\u00b7chen", "nicht", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So war's um mich geschehn.", "tokens": ["So", "wa\u00b7r's", "um", "mich", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Ich wollt' entfliehen, er stand still,", "tokens": ["Ich", "wollt'", "ent\u00b7flie\u00b7hen", ",", "er", "stand", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachseufzen h\u00f6rt' ich ihn!", "tokens": ["Nach\u00b7seuf\u00b7zen", "h\u00f6rt'", "ich", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich blieb: wen Amor halten will,", "tokens": ["Ich", "blieb", ":", "wen", "A\u00b7mor", "hal\u00b7ten", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "NE", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann der denn wohl entfliehn?", "tokens": ["Kann", "der", "denn", "wohl", "ent\u00b7fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein h\u00f6chster Wunsch, sprach er, bist du;", "tokens": ["Mein", "h\u00f6chs\u00b7ter", "Wunsch", ",", "sprach", "er", ",", "bist", "du", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es lie\u00df ihm gar zu sch\u00f6n!", "tokens": ["Es", "lie\u00df", "ihm", "gar", "zu", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kam mein Mamachen nicht dazu,", "tokens": ["Kam", "mein", "Ma\u00b7ma\u00b7chen", "nicht", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So war's um mich geschehn.", "tokens": ["So", "wa\u00b7r's", "um", "mich", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}