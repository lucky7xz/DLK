{"textgrid.poem.57455": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Neuer Orpheus deiner Zeiten!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neuer Orpheus deiner Zeiten!", "tokens": ["Neu\u00b7er", "Or\u00b7pheus", "dei\u00b7ner", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen wundervolle Seyten,", "tokens": ["Des\u00b7sen", "wun\u00b7der\u00b7vol\u00b7le", "Sey\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohn ein sonderlich Bem\u00fchn,", "tokens": ["Ohn", "ein", "son\u00b7der\u00b7lich", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00e4um und Felsen nach sich ziehn;", "tokens": ["B\u00e4um", "und", "Fel\u00b7sen", "nach", "sich", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Edler! = = wenn deine Liebe", "tokens": ["Ed\u00b7ler", "!", "=", "=", "wenn", "dei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["NE", "$.", "XY", "XY", "KOUS", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Nicht die alte Freundschaft st\u00f6rt:", "tokens": ["Nicht", "die", "al\u00b7te", "Freund\u00b7schaft", "st\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So nimm hin, was dir geh\u00f6rt,", "tokens": ["So", "nimm", "hin", ",", "was", "dir", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PTKVZ", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Diese Frucht der treusten Triebe;", "tokens": ["Die\u00b7se", "Frucht", "der", "treus\u00b7ten", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann gleich meiner Musen Lallen", "tokens": ["Kann", "gleich", "mei\u00b7ner", "Mu\u00b7sen", "Lal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dir nicht, wie du mir, gefallen.", "tokens": ["Dir", "nicht", ",", "wie", "du", "mir", ",", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wahrlich, o du Freund der Neune!", "tokens": ["Wahr\u00b7lich", ",", "o", "du", "Freund", "der", "Neu\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4re deine Kunst die Meine,", "tokens": ["W\u00e4\u00b7re", "dei\u00b7ne", "Kunst", "die", "Mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4ng ich, wie dein Bogen spielt,", "tokens": ["S\u00e4ng", "ich", ",", "wie", "dein", "Bo\u00b7gen", "spielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den man in der Seele f\u00fchlt;", "tokens": ["Den", "man", "in", "der", "See\u00b7le", "f\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt ich so die Herzen regen,", "tokens": ["K\u00f6nnt", "ich", "so", "die", "Her\u00b7zen", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So bezaubern, wie du thust:", "tokens": ["So", "be\u00b7zau\u00b7bern", ",", "wie", "du", "thust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrd ich dir bey deiner Lust", "tokens": ["W\u00fcrd", "ich", "dir", "bey", "dei\u00b7ner", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Adern, Mark und Bein bewegen;", "tokens": ["A\u00b7dern", ",", "Mark", "und", "Bein", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und von lauter sch\u00f6nen Dingen", "tokens": ["Und", "von", "lau\u00b7ter", "sch\u00f6\u00b7nen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deiner Auserw\u00e4hlten singen.", "tokens": ["Dei\u00b7ner", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber sprich, wer kann dir gleichen,", "tokens": ["A\u00b7ber", "sprich", ",", "wer", "kann", "dir", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So geschickt die Seyten streichen,", "tokens": ["So", "ge\u00b7schickt", "die", "Sey\u00b7ten", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So genau die Noten sehn,", "tokens": ["So", "ge\u00b7nau", "die", "No\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So gewi\u00df den Wirbel drehn?", "tokens": ["So", "ge\u00b7wi\u00df", "den", "Wir\u00b7bel", "drehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine s\u00fc\u00dfen Harmonien", "tokens": ["Dei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nehmen Ohr und Herzen ein.", "tokens": ["Neh\u00b7men", "Ohr", "und", "Her\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und was klingt so ungemein,", "tokens": ["Und", "was", "klingt", "so", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als die sanften Melodien?", "tokens": ["Als", "die", "sanf\u00b7ten", "Me\u00b7lo\u00b7dien", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Welche tr\u00f6sten und entz\u00fccken,", "tokens": ["Wel\u00b7che", "tr\u00f6s\u00b7ten", "und", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schrecken, dr\u00e4uen und erquicken.", "tokens": ["Schre\u00b7cken", ",", "dr\u00e4u\u00b7en", "und", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sage selbst, verliebte Sch\u00f6ne!", "tokens": ["Sa\u00b7ge", "selbst", ",", "ver\u00b7lieb\u00b7te", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie gef\u00e4llt dir sein Get\u00f6ne?", "tokens": ["Wie", "ge\u00b7f\u00e4llt", "dir", "sein", "Ge\u00b7t\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, du denkst, ein blo\u00dfer Klang", "tokens": ["Doch", ",", "du", "denkst", ",", "ein", "blo\u00b7\u00dfer", "Klang"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nicht das, was mich bezwang.", "tokens": ["Ist", "nicht", "das", ",", "was", "mich", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PDS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freylich hat er andre Gaben,", "tokens": ["Frey\u00b7lich", "hat", "er", "and\u00b7re", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Witz, Verstand und H\u00f6flichkeit,", "tokens": ["Witz", ",", "Ver\u00b7stand", "und", "H\u00f6f\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine Brust, die sich dir weiht,", "tokens": ["Ei\u00b7ne", "Brust", ",", "die", "sich", "dir", "weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und was sonst die Freyer haben:", "tokens": ["Und", "was", "sonst", "die", "Frey\u00b7er", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die\u00df bewog dich, wie wir denken,", "tokens": ["Die\u00df", "be\u00b7wog", "dich", ",", "wie", "wir", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihm dein treues Herz zu schenken.", "tokens": ["Ihm", "dein", "treu\u00b7es", "Herz", "zu", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O wie wohl hei\u00dft das getroffen!", "tokens": ["O", "wie", "wohl", "hei\u00dft", "das", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VVFIN", "PDS", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Itzo kannst du alles hoffen,", "tokens": ["It\u00b7zo", "kannst", "du", "al\u00b7les", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der Hochzeitfackeln Pracht", "tokens": ["Was", "der", "Hoch\u00b7zeit\u00b7fac\u00b7keln", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Angenehm und heiter macht.", "tokens": ["An\u00b7ge\u00b7nehm", "und", "hei\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn wie seine Violine,", "tokens": ["Denn", "wie", "sei\u00b7ne", "Vi\u00b7o\u00b7li\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch die z\u00e4rtsten Striche f\u00fchlt;", "tokens": ["Auch", "die", "z\u00e4rts\u00b7ten", "Stri\u00b7che", "f\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So empfindt auch, der sie spielt,", "tokens": ["So", "emp\u00b7findt", "auch", ",", "der", "sie", "spielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seiner Sch\u00f6nen z\u00e4rtste Mine:", "tokens": ["Sei\u00b7ner", "Sch\u00f6\u00b7nen", "z\u00e4rts\u00b7te", "Mi\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weil ein Blick, der von ihr stammet,", "tokens": ["Weil", "ein", "Blick", ",", "der", "von", "ihr", "stam\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gleich sein ganzes Blut entflammet.", "tokens": ["Gleich", "sein", "gan\u00b7zes", "Blut", "ent\u00b7flam\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Musik geweihte Seelen", "tokens": ["Der", "Mu\u00b7sik", "ge\u00b7weih\u00b7te", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind sehr ekel im Erw\u00e4hlen:", "tokens": ["Sind", "sehr", "e\u00b7kel", "im", "Er\u00b7w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn nicht jedes Haberrohr", "tokens": ["Denn", "nicht", "je\u00b7des", "Ha\u00b7berr\u00b7ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4llt gleich angenehm ins Ohr.", "tokens": ["F\u00e4llt", "gleich", "an\u00b7ge\u00b7nehm", "ins", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber was sie lieb gewinnen,", "tokens": ["A\u00b7ber", "was", "sie", "lieb", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lassen sie durchaus nicht mehr,", "tokens": ["Las\u00b7sen", "sie", "durc\u00b7haus", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die\u00df z\u00e4rtliche Geh\u00f6r", "tokens": ["Und", "die\u00df", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Leitet auch die andern Sinnen;", "tokens": ["Lei\u00b7tet", "auch", "die", "an\u00b7dern", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Drum verspricht dir = = Liebe", "tokens": ["Drum", "ver\u00b7spricht", "dir", "=", "=", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["PAV", "VVFIN", "PPER", "XY", "XY", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Unausl\u00f6schlich hei\u00dfe Triebe.", "tokens": ["Un\u00b7aus\u00b7l\u00f6schlich", "hei\u00b7\u00dfe", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Sollte dich der Tod ihm rauben,", "tokens": ["Soll\u00b7te", "dich", "der", "Tod", "ihm", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O so kannst du sicher glauben:", "tokens": ["O", "so", "kannst", "du", "si\u00b7cher", "glau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlten kaum ein herber Weh!", "tokens": ["F\u00fchl\u00b7ten", "kaum", "ein", "her\u00b7ber", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieng nun der mit schnellen Schritten", "tokens": ["Gieng", "nun", "der", "mit", "schnel\u00b7len", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seufzend nach der Unterwelt,", "tokens": ["Seuf\u00b7zend", "nach", "der", "Un\u00b7ter\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das, was ihm der Tod gef\u00e4llt,", "tokens": ["Das", ",", "was", "ihm", "der", "Tod", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durch die Laute zu erbitten:", "tokens": ["Durch", "die", "Lau\u00b7te", "zu", "er\u00b7bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ey so w\u00fcrden = = Seyten", "tokens": ["Ey", "so", "w\u00fcr\u00b7den", "=", "=", "Sey\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["NN", "ADV", "VAFIN", "XY", "XY", "XY"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Dich gewi\u00df zur Gruft begleiten.", "tokens": ["Dich", "ge\u00b7wi\u00df", "zur", "Gruft", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wer weis, was noch gesch\u00e4he,", "tokens": ["Und", "wer", "weis", ",", "was", "noch", "ge\u00b7sch\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKVZ", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihn ", "tokens": ["Wenn", "ihn"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ob nicht deine Wiederkehr", "tokens": ["Ob", "nicht", "dei\u00b7ne", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Kunst Belohnung w\u00e4r!", "tokens": ["Sei\u00b7ner", "Kunst", "Be\u00b7loh\u00b7nung", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch kein trauriges Besorgen", "tokens": ["Doch", "kein", "trau\u00b7ri\u00b7ges", "Be\u00b7sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schickt sich hier zur Hochzeitlust,", "tokens": ["Schickt", "sich", "hier", "zur", "Hoch\u00b7zeit\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Drum vergn\u00fcget eure Brust,", "tokens": ["Drum", "ver\u00b7gn\u00fc\u00b7get", "eu\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Werthes Paar! bis an den Morgen.", "tokens": ["Wert\u00b7hes", "Paar", "!", "bis", "an", "den", "Mor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber gebt auch bald die Proben,", "tokens": ["A\u00b7ber", "gebt", "auch", "bald", "die", "Pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df ihr sie nicht aufgeschoben.", "tokens": ["Da\u00df", "ihr", "sie", "nicht", "auf\u00b7ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Soll ich euch noch W\u00fcnsche machen?", "tokens": ["Soll", "ich", "euch", "noch", "W\u00fcn\u00b7sche", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ja! man m\u00f6chte mich verlachen,", "tokens": ["Ja", "!", "man", "m\u00f6ch\u00b7te", "mich", "ver\u00b7la\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df ich schon so viel gereimt,", "tokens": ["Da\u00df", "ich", "schon", "so", "viel", "ge\u00b7reimt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und das Beste doch vers\u00e4umt.", "tokens": ["Und", "das", "Bes\u00b7te", "doch", "ver\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Nun, das g\u00fctige Geschicke", "tokens": ["Nun", ",", "das", "g\u00fc\u00b7ti\u00b7ge", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sey der Harmonie geneigt,", "tokens": ["Sey", "der", "Har\u00b7mo\u00b7nie", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Die sich an euch Beyden zeigt;", "tokens": ["Die", "sich", "an", "euch", "Bey\u00b7den", "zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "So bestr\u00f6mt euch alles Gl\u00fccke.", "tokens": ["So", "be\u00b7str\u00f6mt", "euch", "al\u00b7les", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Denn wo Lieb und Treu sich zeigen,", "tokens": ["Denn", "wo", "Lieb", "und", "Treu", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "H\u00e4ngt der Ehstand voller Geigen.", "tokens": ["H\u00e4ngt", "der", "Eh\u00b7stand", "vol\u00b7ler", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}