{"textgrid.poem.39889": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den Wassern Babylons", "tokens": ["Auf", "den", "Was\u00b7sern", "Ba\u00b7by\u00b7lons"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt man keine Ruder schlagen;", "tokens": ["H\u00f6rt", "man", "kei\u00b7ne", "Ru\u00b7der", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch das Ro\u00df des G\u00f6ttersohns", "tokens": ["Auch", "das", "Ro\u00df", "des", "G\u00f6t\u00b7ter\u00b7sohns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiehert nicht am schnellen Wagen.", "tokens": ["Wie\u00b7hert", "nicht", "am", "schnel\u00b7len", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach, vom Thron ist er gestiegen,", "tokens": ["Ach", ",", "vom", "Thron", "ist", "er", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An der Wand lehnt Schwert und Speer,", "tokens": ["An", "der", "Wand", "lehnt", "Schwert", "und", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf dem Lager mu\u00df er liegen,", "tokens": ["Auf", "dem", "La\u00b7ger", "mu\u00df", "er", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sterbend gr\u00fc\u00dft der Held sein Heer.", "tokens": ["Ster\u00b7bend", "gr\u00fc\u00dft", "der", "Held", "sein", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur ein K\u00f6nigsgrabmal schwimmt", "tokens": ["Nur", "ein", "K\u00f6\u00b7nigs\u00b7grab\u00b7mal", "schwimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem Auge, das schon dunkelt.", "tokens": ["Vor", "dem", "Au\u00b7ge", ",", "das", "schon", "dun\u00b7kelt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Fieberstirne nimmt", "tokens": ["Von", "der", "Fie\u00b7bers\u00b7tir\u00b7ne", "nimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er das Band, das gl\u00fchend funkelt;", "tokens": ["Er", "das", "Band", ",", "das", "gl\u00fc\u00b7hend", "fun\u00b7kelt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er's ohne Freundest\u00fccke", "tokens": ["Da\u00df", "er's", "oh\u00b7ne", "Freun\u00b7de\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lassen darf auf Freundeshaupt,", "tokens": ["Las\u00b7sen", "darf", "auf", "Freun\u00b7des\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Daf\u00fcr danket er dem Gl\u00fccke,", "tokens": ["Da\u00b7f\u00fcr", "dan\u00b7ket", "er", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das dem Traum sein Gift geraubt.", "tokens": ["Das", "dem", "Traum", "sein", "Gift", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbtretet um mich alle her,", "tokens": ["\u00bb", "tre\u00b7tet", "um", "mich", "al\u00b7le", "her", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Waffenbr\u00fcder, n\u00e4her, n\u00e4her!", "tokens": ["Waf\u00b7fen\u00b7br\u00fc\u00b7der", ",", "n\u00e4\u00b7her", ",", "n\u00e4\u00b7her", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm auch du, gedankenschwer,", "tokens": ["Komm", "auch", "du", ",", "ge\u00b7dan\u00b7ken\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahnungsvoller, ernster Seher!", "tokens": ["Ah\u00b7nungs\u00b7vol\u00b7ler", ",", "erns\u00b7ter", "Se\u00b7her", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm die Binde, hilf enth\u00fcllen,", "tokens": ["Nimm", "die", "Bin\u00b7de", ",", "hilf", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVIMP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die Nacht als R\u00e4tsel sprach,", "tokens": ["Was", "die", "Nacht", "als", "R\u00e4t\u00b7sel", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hilf den G\u00f6tterschlu\u00df erf\u00fcllen,", "tokens": ["Hilf", "den", "G\u00f6t\u00b7ter\u00b7schlu\u00df", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der mir schon das Auge brach!", "tokens": ["Der", "mir", "schon", "das", "Au\u00b7ge", "brach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Welche Stirne soll die\u00df Band,", "tokens": ["Wel\u00b7che", "Stir\u00b7ne", "soll", "die\u00df", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seher! von den vielen schm\u00fccken?", "tokens": ["Se\u00b7her", "!", "von", "den", "vie\u00b7len", "schm\u00fc\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ART", "PIAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer statt meiner abgesandt", "tokens": ["Wer", "statt", "mei\u00b7ner", "ab\u00b7ge\u00b7sandt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll den Erdenkreis begl\u00fccken?", "tokens": ["Soll", "den", "Er\u00b7den\u00b7kreis", "be\u00b7gl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprich den w\u00fcrdigsten der Namen,", "tokens": ["Sprich", "den", "w\u00fcr\u00b7digs\u00b7ten", "der", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Da\u00df die V\u00f6lker sich erfreun!", "tokens": ["Da\u00df", "die", "V\u00f6l\u00b7ker", "sich", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kannst du nur den b\u00f6sen Samen,", "tokens": ["Kannst", "du", "nur", "den", "b\u00f6\u00b7sen", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht die Saat des Gl\u00fcckes streun?", "tokens": ["Nicht", "die", "Saat", "des", "Gl\u00fc\u00b7ckes", "streun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch sie bl\u00fchet schon, die Saat!", "tokens": ["Doch", "sie", "bl\u00fc\u00b7het", "schon", ",", "die", "Saat", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O der herrlichen Enth\u00fcllung!", "tokens": ["O", "der", "herr\u00b7li\u00b7chen", "Ent\u00b7h\u00fcl\u00b7lung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alexanders Gl\u00fcck, es naht", "tokens": ["A\u00b7lex\u00b7an\u00b7ders", "Gl\u00fcck", ",", "es", "naht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der reichlichsten Erf\u00fcllung!", "tokens": ["Mit", "der", "reich\u00b7lichs\u00b7ten", "Er\u00b7f\u00fcl\u00b7lung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seh' ich lichte K\u00f6nigskronen", "tokens": ["Seh'", "ich", "lich\u00b7te", "K\u00f6\u00b7nigs\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rings auf allen H\u00e4uptern nicht?", "tokens": ["Rings", "auf", "al\u00b7len", "H\u00e4up\u00b7tern", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flammet nicht von zehen Thronen", "tokens": ["Flam\u00b7met", "nicht", "von", "ze\u00b7hen", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meines Ruhmes Stralenlicht?", "tokens": ["Mei\u00b7nes", "Ruh\u00b7mes", "Stra\u00b7len\u00b7licht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Welch ein friedlich Diadem", "tokens": ["Welch", "ein", "fried\u00b7lich", "Di\u00b7a\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJD", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Schm\u00fcckt die Stirn dir, leichter Schwimmer,", "tokens": ["Schm\u00fcckt", "die", "Stirn", "dir", ",", "leich\u00b7ter", "Schwim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sendet mild und angenehm", "tokens": ["Sen\u00b7det", "mild", "und", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jungen St\u00e4dten seinen Schimmer.", "tokens": ["Jun\u00b7gen", "St\u00e4d\u00b7ten", "sei\u00b7nen", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber du dort, k\u00fchner Retter,", "tokens": ["A\u00b7ber", "du", "dort", ",", "k\u00fch\u00b7ner", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4gst es unter Blut und Schwei\u00df,", "tokens": ["Tr\u00e4gst", "es", "un\u00b7ter", "Blut", "und", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von stetem Kriegeswetter", "tokens": ["Und", "von", "ste\u00b7tem", "Krie\u00b7ges\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dampfet dir die Stirne hei\u00df!", "tokens": ["Damp\u00b7fet", "dir", "die", "Stir\u00b7ne", "hei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jener leichter, dieser schwer,", "tokens": ["Je\u00b7ner", "leich\u00b7ter", ",", "die\u00b7ser", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "$,", "PDAT", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tragt es, wie's der Gott geschlungen!", "tokens": ["Tragt", "es", ",", "wie's", "der", "Gott", "ge\u00b7schlun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich dr\u00fcckt keine Krone mehr,", "tokens": ["Mich", "dr\u00fcckt", "kei\u00b7ne", "Kro\u00b7ne", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mich f\u00fchrt Lethe's Flut, entschwungen", "tokens": ["Mich", "f\u00fchrt", "Lethe's", "Flut", ",", "ent\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NE", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ueber jene schwarzen Wellen,", "tokens": ["Ue\u00b7ber", "je\u00b7ne", "schwar\u00b7zen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem K\u00f6nigsgrab vorbei,", "tokens": ["An", "dem", "K\u00f6\u00b7nigs\u00b7grab", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich auf den sel'gen, hellen", "tokens": ["Da\u00df", "ich", "auf", "den", "sel'\u00b7gen", ",", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Inseln frei mit Freien sei!\u00ab", "tokens": ["In\u00b7seln", "frei", "mit", "Frei\u00b7en", "sei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und der Tod wischt aus dem Blick", "tokens": ["Und", "der", "Tod", "wischt", "aus", "dem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Thronen ihm und K\u00f6nigsbinden.", "tokens": ["Thro\u00b7nen", "ihm", "und", "K\u00f6\u00b7nigs\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"],