{"textgrid.poem.66667": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Wildbach bin ich abends nachgegangen,", "tokens": ["Dem", "Wild\u00b7bach", "bin", "ich", "a\u00b7bends", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der vom Gewitterregen lustig schwoll.", "tokens": ["Der", "vom", "Ge\u00b7wit\u00b7ter\u00b7re\u00b7gen", "lus\u00b7tig", "schwoll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus dem verst\u00e4rkten Flutgebrause klangen", "tokens": ["Aus", "dem", "ver\u00b7st\u00e4rk\u00b7ten", "Flut\u00b7ge\u00b7brau\u00b7se", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Grundt\u00f6ne, wie des Donners fern Gegroll.", "tokens": ["Grund\u00b7t\u00f6\u00b7ne", ",", "wie", "des", "Don\u00b7ners", "fern", "Ge\u00b7groll", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Als so die Wasser sch\u00e4umend niedersprangen,", "tokens": ["Als", "so", "die", "Was\u00b7ser", "sch\u00e4u\u00b7mend", "nie\u00b7der\u00b7spran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "War's, wie wenn frisch mir durch die Seele quoll,", "tokens": ["Wa\u00b7r's", ",", "wie", "wenn", "frisch", "mir", "durch", "die", "See\u00b7le", "quoll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOKOM", "KOUS", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Was ich in lustgeschwellten Torenstunden,", "tokens": ["Was", "ich", "in", "lust\u00b7ge\u00b7schwell\u00b7ten", "To\u00b7ren\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch Wald und Talschlucht schweifend, je empfunden.", "tokens": ["Durch", "Wald", "und", "Tal\u00b7schlucht", "schwei\u00b7fend", ",", "je", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mich durchrauschten dunkle Grundgef\u00fchle", "tokens": ["Und", "mich", "durc\u00b7hrauschten", "dunk\u00b7le", "Grund\u00b7ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit langentbehrter wonniger Gewalt ...", "tokens": ["Mit", "lan\u00b7gent\u00b7behr\u00b7ter", "won\u00b7ni\u00b7ger", "Ge\u00b7walt", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da aus des Berggew\u00e4ssers Quellenk\u00fchle", "tokens": ["Da", "aus", "des", "Berg\u00b7ge\u00b7w\u00e4s\u00b7sers", "Quel\u00b7len\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Urpl\u00f6tzlich zitternd hebt sich die Gestalt,", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "zit\u00b7ternd", "hebt", "sich", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die wieder mein ist ... winkt vom feuchten Pf\u00fchle,", "tokens": ["Die", "wie\u00b7der", "mein", "ist", "...", "winkt", "vom", "feuch\u00b7ten", "Pf\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "VAFIN", "$(", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit einem Hauch gebietet sie mir Halt ...", "tokens": ["Mit", "ei\u00b7nem", "Hauch", "ge\u00b7bie\u00b7tet", "sie", "mir", "Halt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf schmalem Br\u00fccklein bleib ich droben stehen,", "tokens": ["Auf", "schma\u00b7lem", "Br\u00fcck\u00b7lein", "bleib", "ich", "dro\u00b7ben", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ins altvertraute Nixenaug zu sehen.", "tokens": ["Ins", "alt\u00b7ver\u00b7trau\u00b7te", "Ni\u00b7xen\u00b7aug", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Blick so tief, ein Gru\u00df so unergr\u00fcndlich \u2013", "tokens": ["Ein", "Blick", "so", "tief", ",", "ein", "Gru\u00df", "so", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin ein Kind in gr\u00fcner Wellen Scho\u00df:", "tokens": ["Ich", "bin", "ein", "Kind", "in", "gr\u00fc\u00b7ner", "Wel\u00b7len", "Scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df nicht, was im Staube fortkriecht s\u00fcndlich,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "im", "Stau\u00b7be", "fort\u00b7kriecht", "s\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "APPRART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df nicht, was ein elend Menschenlos.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ein", "e\u00b7lend", "Men\u00b7schen\u00b7los", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sp\u00fcre nur, was rein und ewig kindlich,", "tokens": ["Ich", "sp\u00fc\u00b7re", "nur", ",", "was", "rein", "und", "e\u00b7wig", "kind\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und w\u00e4hne mich der F\u00f6hre gleich und gro\u00df,", "tokens": ["Und", "w\u00e4h\u00b7ne", "mich", "der", "F\u00f6h\u00b7re", "gleich", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die von dem r\u00f6tlich tropfenden Gesteine", "tokens": ["Die", "von", "dem", "r\u00f6t\u00b7lich", "trop\u00b7fen\u00b7den", "Ge\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Hangs aufragt im letzten Purpurscheine ...", "tokens": ["Des", "Hangs", "auf\u00b7ragt", "im", "letz\u00b7ten", "Pur\u00b7pur\u00b7schei\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}