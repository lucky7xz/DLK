{"textgrid.poem.36566": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied an die Kaiserin-K\u00f6nigin nach Wiedereroberung der Stadt Breslau", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun beschlie\u00dfe deinen Krieg,", "tokens": ["Nun", "be\u00b7schlie\u00b7\u00dfe", "dei\u00b7nen", "Krieg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaiser-K\u00f6nigin!", "tokens": ["Kai\u00b7ser\u00b7K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gieb Dir selbst den sch\u00f6nsten Sieg!", "tokens": ["Gieb", "Dir", "selbst", "den", "sch\u00f6ns\u00b7ten", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werde Siegerin!", "tokens": ["Wer\u00b7de", "Sie\u00b7ge\u00b7rin", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00dcberwinde Dich, und gieb", "tokens": ["\u00dc\u00b7berw\u00b7in\u00b7de", "Dich", ",", "und", "gieb"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVIMP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschlichkeit Geh\u00f6r!", "tokens": ["Menschlich\u00b7keit", "Ge\u00b7h\u00f6r", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Habe deine V\u00f6lker lieb!", "tokens": ["Ha\u00b7be", "dei\u00b7ne", "V\u00f6l\u00b7ker", "lieb", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Opfere nicht mehr!", "tokens": ["Op\u00b7fe\u00b7re", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Unsern ", "tokens": ["Un\u00b7sern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Der auch Weiser ist,", "tokens": ["Der", "auch", "Wei\u00b7ser", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der ein Wunder ist der Welt,", "tokens": ["Der", "ein", "Wun\u00b7der", "ist", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Du selber bist;", "tokens": ["Wie", "Du", "sel\u00b7ber", "bist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Der gerechte Waffen tr\u00e4gt", "tokens": ["Der", "ge\u00b7rech\u00b7te", "Waf\u00b7fen", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ins Gefecht mit Dir,", "tokens": ["Ins", "Ge\u00b7fecht", "mit", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mit uns kommt, und sieht und schl\u00e4gt,", "tokens": ["Mit", "uns", "kommt", ",", "und", "sieht", "und", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tapferer als wir;", "tokens": ["Tap\u00b7fe\u00b7rer", "als", "wir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Heldin, den bezwingst du nicht;", "tokens": ["Hel\u00b7din", ",", "den", "be\u00b7zwingst", "du", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott kann Wunder thun!", "tokens": ["Gott", "kann", "Wun\u00b7der", "thun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schenk ihm Freundesangesicht,", "tokens": ["Schenk", "ihm", "Freun\u00b7de\u00b7san\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Biete Frieden nun!", "tokens": ["Bie\u00b7te", "Frie\u00b7den", "nun", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Williger war nie ein Feind,", "tokens": ["Wil\u00b7li\u00b7ger", "war", "nie", "ein", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Feinden zu verzeihn;", "tokens": ["Fein\u00b7den", "zu", "ver\u00b7zeihn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schneller nie ein Menschenfreund,", "tokens": ["Schnel\u00b7ler", "nie", "ein", "Men\u00b7schen\u00b7freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausges\u00f6hnt zu sein;", "tokens": ["Aus\u00b7ge\u00b7s\u00f6hnt", "zu", "sein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Nie ein gr\u00f6\u00dfrer Feind der Schlacht,", "tokens": ["Nie", "ein", "gr\u00f6\u00df\u00b7rer", "Feind", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Heldenthat,", "tokens": ["Und", "der", "Hel\u00b7den\u00b7that", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Als der Held, der deine Macht", "tokens": ["Als", "der", "Held", ",", "der", "dei\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberwunden hat!", "tokens": ["\u00dc\u00b7berw\u00b7un\u00b7den", "hat", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}