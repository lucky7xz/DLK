{"dta.poem.11756": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du machest manches mit, weil man dir's vorgemacht,", "tokens": ["Du", "ma\u00b7chest", "man\u00b7ches", "mit", ",", "weil", "man", "dir's", "vor\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "KOUS", "PIS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bringst es weiter so, wie es ist hergebracht.", "tokens": ["Und", "bringst", "es", "wei\u00b7ter", "so", ",", "wie", "es", "ist", "her\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit Messern schneidest du des Brotes weiche Rinde,", "tokens": ["Mit", "Mes\u00b7sern", "schnei\u00b7dest", "du", "des", "Bro\u00b7tes", "wei\u00b7che", "Rin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bei\u00dfest mit dem Zahn die Nu\u00df, die ungelinde.", "tokens": ["Und", "bei\u00b7\u00dfest", "mit", "dem", "Zahn", "die", "Nu\u00df", ",", "die", "un\u00b7ge\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So ists einmal dein Brauch, doch brauchtest du viel besser,", "tokens": ["So", "ists", "ein\u00b7mal", "dein", "Brauch", ",", "doch", "brauch\u00b7test", "du", "viel", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich d\u00fcnkt, den Zahn f\u00fcrs Brot und f\u00fcr die Nu\u00df das Messer.", "tokens": ["Mich", "d\u00fcnkt", ",", "den", "Zahn", "f\u00fcrs", "Brot", "und", "f\u00fcr", "die", "Nu\u00df", "das", "Mes\u00b7ser", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}