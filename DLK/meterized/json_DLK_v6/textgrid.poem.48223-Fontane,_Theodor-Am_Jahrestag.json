{"textgrid.poem.48223": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Am Jahrestag", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut ist's ein Jahr, da\u00df man hinaus dich trug,", "tokens": ["Heut", "ist's", "ein", "Jahr", ",", "da\u00df", "man", "hin\u00b7aus", "dich", "trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "APZR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hin durch die Gasse ging der lange Zug,", "tokens": ["Hin", "durch", "die", "Gas\u00b7se", "ging", "der", "lan\u00b7ge", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne schien, es schwiegen Hast und L\u00e4rmen,", "tokens": ["Die", "Son\u00b7ne", "schien", ",", "es", "schwie\u00b7gen", "Hast", "und", "L\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Tauben stiegen auf in ganzen Schw\u00e4rmen.", "tokens": ["Die", "Tau\u00b7ben", "stie\u00b7gen", "auf", "in", "gan\u00b7zen", "Schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und rings der Felder herbstlich buntes Kleid,", "tokens": ["Und", "rings", "der", "Fel\u00b7der", "herbst\u00b7lich", "bun\u00b7tes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es nahm dem Trauerzuge fast sein Leid,", "tokens": ["Es", "nahm", "dem", "Trau\u00b7er\u00b7zu\u00b7ge", "fast", "sein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Fl\u00fcstern klang mit ein in den Choral,", "tokens": ["Ein", "Fl\u00fcs\u00b7tern", "klang", "mit", "ein", "in", "den", "Cho\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nun aber schwieg's \u2013 wir hielten am Portal.", "tokens": ["Nun", "a\u00b7ber", "schwieg's", "\u2013", "wir", "hiel\u00b7ten", "am", "Por\u00b7tal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$(", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Zug bog ein, da war das frische Grab,", "tokens": ["Der", "Zug", "bog", "ein", ",", "da", "war", "das", "fri\u00b7sche", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir n\u00e4chsten beide sahen still hinab,", "tokens": ["Wir", "n\u00e4chs\u00b7ten", "bei\u00b7de", "sa\u00b7hen", "still", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Geistliche, des Tages letztes Licht", "tokens": ["Der", "Geist\u00b7li\u00b7che", ",", "des", "Ta\u00b7ges", "letz\u00b7tes", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umleuchtete sein freundlich ernst Gesicht,", "tokens": ["Um\u00b7leuch\u00b7te\u00b7te", "sein", "freund\u00b7lich", "ernst", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und als er nun die Abschiedsworte sprach,", "tokens": ["Und", "als", "er", "nun", "die", "Ab\u00b7schieds\u00b7wor\u00b7te", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da sank der Sarg, und Blumen fielen nach,", "tokens": ["Da", "sank", "der", "Sarg", ",", "und", "Blu\u00b7men", "fie\u00b7len", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sp\u00e4trosen, rot und wei\u00dfe, wei\u00dfe Malven,", "tokens": ["Sp\u00e4t\u00b7ro\u00b7sen", ",", "rot", "und", "wei\u00b7\u00dfe", ",", "wei\u00b7\u00dfe", "Mal\u00b7ven", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mit den Blumen fielen die drei Salven.", "tokens": ["Und", "mit", "den", "Blu\u00b7men", "fie\u00b7len", "die", "drei", "Sal\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das klang so frisch in unser Ohr und Herz,", "tokens": ["Das", "klang", "so", "frisch", "in", "un\u00b7ser", "Ohr", "und", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hin schwand das Leid uns, aller Gram und Schmerz.", "tokens": ["Hin", "schwand", "das", "Leid", "uns", ",", "al\u00b7ler", "Gram", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$,", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben, war dir' s wenig, war dir' s viel?", "tokens": ["Das", "Le\u00b7ben", ",", "war", "dir'", "s", "we\u00b7nig", ",", "war", "dir'", "s", "viel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "VVFIN", "PIS", "$,", "VAFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich wei\u00df das eine nur, du bist am Ziel,", "tokens": ["Ich", "wei\u00df", "das", "ei\u00b7ne", "nur", ",", "du", "bist", "am", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ART", "ADV", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Blumen durftest du gebettet werden,", "tokens": ["In", "Blu\u00b7men", "durf\u00b7test", "du", "ge\u00b7bet\u00b7tet", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du hast die Ruh' nun, Erde wird zu Erden,", "tokens": ["Du", "hast", "die", "Ruh'", "nun", ",", "Er\u00b7de", "wird", "zu", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und kommt die Stund' uns, dir uns anzureihn,", "tokens": ["Und", "kommt", "die", "Stund'", "uns", ",", "dir", "uns", "an\u00b7zu\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "PPER", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So la\u00df die Stunde, Gott, wie diese sein.", "tokens": ["So", "la\u00df", "die", "Stun\u00b7de", ",", "Gott", ",", "wie", "die\u00b7se", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "$,", "NN", "$,", "PWAV", "PDS", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}