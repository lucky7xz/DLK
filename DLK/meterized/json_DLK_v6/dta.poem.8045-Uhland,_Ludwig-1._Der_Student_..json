{"dta.poem.8045": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.  Der Student .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich einst bei Salamanka", "tokens": ["Als", "ich", "einst", "bei", "Sa\u00b7la\u00b7man\u00b7ka"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fr\u00fch in einem Garten sa\u00df", "tokens": ["Fr\u00fch", "in", "ei\u00b7nem", "Gar\u00b7ten", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bei\u2019m Schlag der Nachtigallen", "tokens": ["Und", "bei'm", "Schlag", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Emsig im Homerus las:", "tokens": ["Em\u00b7sig", "im", "Ho\u00b7me\u00b7rus", "las", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NE", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Wie in gl\u00e4nzenden Gewanden", "tokens": ["Wie", "in", "gl\u00e4n\u00b7zen\u00b7den", "Ge\u00b7wan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Helena zur Zinne trat", "tokens": ["He\u00b7le\u00b7na", "zur", "Zin\u00b7ne", "trat"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so herrlich sich erzeigte", "tokens": ["Und", "so", "herr\u00b7lich", "sich", "er\u00b7zeig\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem trojanischen Senat,", "tokens": ["Dem", "tro\u00b7ja\u00b7ni\u00b7schen", "Se\u00b7nat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df vernehmlich Der und Jener", "tokens": ["Da\u00df", "ver\u00b7nehm\u00b7lich", "Der", "und", "Je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "KON", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Brummt\u2019 in seinen grauen Bart:", "tokens": ["Brummt'", "in", "sei\u00b7nen", "grau\u00b7en", "Bart", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u201esolch ein Weib ward nie gesehen,", "tokens": ["\u201e", "solch", "ein", "Weib", "ward", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Traun, sie ist von G\u00f6tterart!\u201c", "tokens": ["Traun", ",", "sie", "ist", "von", "G\u00f6t\u00b7ter\u00b7art", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPR", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Als ich so mich ganz vertiefet,", "tokens": ["Als", "ich", "so", "mich", "ganz", "ver\u00b7tie\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Wu\u00dft\u2019 ich nicht, wie mir geschah:", "tokens": ["Wu\u00dft'", "ich", "nicht", ",", "wie", "mir", "ge\u00b7schah", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "In die Bl\u00e4tter fuhr ein Wehen,", "tokens": ["In", "die", "Bl\u00e4t\u00b7ter", "fuhr", "ein", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da\u00df ich staunend um mich sah.", "tokens": ["Da\u00df", "ich", "stau\u00b7nend", "um", "mich", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Auf benachbartem Balkone,", "tokens": ["Auf", "be\u00b7nach\u00b7bar\u00b7tem", "Bal\u00b7ko\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.18": {"text": "Welch ein Wunder schaut\u2019 ich da!", "tokens": ["Welch", "ein", "Wun\u00b7der", "schaut'", "ich", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Dort in gl\u00e4nzenden Gewanden", "tokens": ["Dort", "in", "gl\u00e4n\u00b7zen\u00b7den", "Ge\u00b7wan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Stand ein Weib wie Helena,", "tokens": ["Stand", "ein", "Weib", "wie", "He\u00b7le\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KOKOM", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Und ein Graubart ihr zur Seite,", "tokens": ["Und", "ein", "Grau\u00b7bart", "ihr", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Der so seltsam freundlich that,", "tokens": ["Der", "so", "selt\u00b7sam", "freund\u00b7lich", "that", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Da\u00df ich schw\u00f6ren mocht\u2019, er w\u00e4re", "tokens": ["Da\u00df", "ich", "schw\u00f6\u00b7ren", "mocht'", ",", "er", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Von der Troer hohem Rath.", "tokens": ["Von", "der", "Troer", "ho\u00b7hem", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.25": {"text": "Doch ich selbst ward ein Ach\u00e4er,", "tokens": ["Doch", "ich", "selbst", "ward", "ein", "A\u00b7ch\u00e4\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.26": {"text": "Der ich nun seit jenem Tag", "tokens": ["Der", "ich", "nun", "seit", "je\u00b7nem", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Vor dem festen Gartenhause,", "tokens": ["Vor", "dem", "fes\u00b7ten", "Gar\u00b7ten\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Einer neuen Troja, lag.", "tokens": ["Ei\u00b7ner", "neu\u00b7en", "Tro\u00b7ja", ",", "lag", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Um es unverbl\u00fcmt zu sagen:", "tokens": ["Um", "es", "un\u00b7ver\u00b7bl\u00fcmt", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Manche Sommerwoch\u2019 entlang", "tokens": ["Man\u00b7che", "Som\u00b7mer\u00b7wo\u00b7ch'", "ent\u00b7lang"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "APPO"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.31": {"text": "Kam ich dorthin jeden Abend", "tokens": ["Kam", "ich", "dor\u00b7thin", "je\u00b7den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Mit der Laut\u2019 und mit Gesang,", "tokens": ["Mit", "der", "Laut'", "und", "mit", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Klagt\u2019 in manigfachen Weisen", "tokens": ["Klagt'", "in", "ma\u00b7nig\u00b7fa\u00b7chen", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Meiner Liebe Qual und Drang,", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "Qual", "und", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Bis zuletzt vom hohen Gitter", "tokens": ["Bis", "zu\u00b7letzt", "vom", "ho\u00b7hen", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "S\u00fc\u00dfe Antwort niederklang.", "tokens": ["S\u00fc\u00b7\u00dfe", "Ant\u00b7wort", "nie\u00b7der\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Solches Spiel mit Wort und T\u00f6nen", "tokens": ["Sol\u00b7ches", "Spiel", "mit", "Wort", "und", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Trieben wir ein halbes Jahr,", "tokens": ["Trie\u00b7ben", "wir", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Und auch dies war nur verg\u00f6nnet", "tokens": ["Und", "auch", "dies", "war", "nur", "ver\u00b7g\u00f6n\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDS", "VAFIN", "ADV", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.40": {"text": "Weil halbtaub der Vormund war.", "tokens": ["Weil", "halb\u00b7taub", "der", "Vor\u00b7mund", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Hub er gleich sich oft vom Lager,", "tokens": ["Hub", "er", "gleich", "sich", "oft", "vom", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Schlaflos, eifers\u00fcchtig bang,", "tokens": ["Schlaf\u00b7los", ",", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "bang", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Blieben doch ihm unsre Stimmen", "tokens": ["Blie\u00b7ben", "doch", "ihm", "uns\u00b7re", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Ungeh\u00f6rt, wie Sph\u00e4renklang.", "tokens": ["Un\u00b7ge\u00b7h\u00f6rt", ",", "wie", "Sph\u00e4\u00b7ren\u00b7klang", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Aber einst, die Nacht war schaurig,", "tokens": ["A\u00b7ber", "einst", ",", "die", "Nacht", "war", "schau\u00b7rig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Sternlos, finster wie das Grab,", "tokens": ["Stern\u00b7los", ",", "fins\u00b7ter", "wie", "das", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Klang auf das gewohnte Zeichen", "tokens": ["Klang", "auf", "das", "ge\u00b7wohn\u00b7te", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "Keine Antwort mir herab.", "tokens": ["Kei\u00b7ne", "Ant\u00b7wort", "mir", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Nur ein alt zahnloses Fr\u00e4ulein", "tokens": ["Nur", "ein", "alt", "zahn\u00b7lo\u00b7ses", "Fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.50": {"text": "Ward von meiner Stimme wach,", "tokens": ["Ward", "von", "mei\u00b7ner", "Stim\u00b7me", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Nur das alte Fr\u00e4ulein Echo", "tokens": ["Nur", "das", "al\u00b7te", "Fr\u00e4u\u00b7lein", "E\u00b7cho"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "St\u00f6hnte meine Klagen nach.", "tokens": ["St\u00f6hn\u00b7te", "mei\u00b7ne", "Kla\u00b7gen", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Meine Sch\u00f6ne war verschwunden,", "tokens": ["Mei\u00b7ne", "Sch\u00f6\u00b7ne", "war", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Leer die Zimmer, leer der Saal,", "tokens": ["Leer", "die", "Zim\u00b7mer", ",", "leer", "der", "Saal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Leer der blumenreiche Garten,", "tokens": ["Leer", "der", "blu\u00b7men\u00b7rei\u00b7che", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Rings ver\u00f6det Berg und Thal.", "tokens": ["Rings", "ver\u00b7\u00f6\u00b7det", "Berg", "und", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Ach! und nie hatt\u2019 ich erfahren", "tokens": ["Ach", "!", "und", "nie", "hatt'", "ich", "er\u00b7fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Ihre Heimath, ihren Stand,", "tokens": ["Ih\u00b7re", "Hei\u00b7math", ",", "ih\u00b7ren", "Stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Weil sie, Beides zu verschweigen,", "tokens": ["Weil", "sie", ",", "Bei\u00b7des", "zu", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Angelobt mit Mund und Hand.", "tokens"