AT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Unter jedem Gitterfenster", "tokens": ["Un\u00b7ter", "je\u00b7dem", "Git\u00b7ter\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Frag\u2019 ich leis mit T\u00f6nen an,", "tokens": ["Frag'", "ich", "leis", "mit", "T\u00f6\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.69": {"text": "Sing\u2019 in Stadt und Feld das Liedchen,", "tokens": ["Sing'", "in", "Stadt", "und", "Feld", "das", "Lied\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Das im Salamanker Thal", "tokens": ["Das", "im", "Sa\u00b7la\u00b7man\u00b7ker", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.71": {"text": "Jeden Abend ich gesungen", "tokens": ["Je\u00b7den", "A\u00b7bend", "ich", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Meiner Liebsten zum Signal;", "tokens": ["Mei\u00b7ner", "Liebs\u00b7ten", "zum", "Sig\u00b7nal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.73": {"text": "Doch die Antwort, die ersehnte,", "tokens": ["Doch", "die", "Ant\u00b7wort", ",", "die", "er\u00b7sehn\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "T\u00f6net nimmermehr und ach!", "tokens": ["T\u00f6\u00b7net", "nim\u00b7mer\u00b7mehr", "und", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "XY", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.75": {"text": "Nur das alte Fr\u00e4ulein Echo", "tokens": ["Nur", "das", "al\u00b7te", "Fr\u00e4u\u00b7lein", "E\u00b7cho"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Reist zur Qual mir ewig nach.", "tokens": ["Reist", "zur", "Qual", "mir", "e\u00b7wig", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}