{"textgrid.poem.43844": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Morgen kommt der Weihnachtsmann,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgen kommt der Weihnachtsmann,", "tokens": ["Mor\u00b7gen", "kommt", "der", "Weih\u00b7nachts\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt mit seinen Gaben.", "tokens": ["Kommt", "mit", "sei\u00b7nen", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Trommel, Pfeifen und Gewehr,", "tokens": ["Trom\u00b7mel", ",", "Pfei\u00b7fen", "und", "Ge\u00b7wehr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fahn' und S\u00e4bel, und noch mehr,", "tokens": ["Fahn'", "und", "S\u00e4\u00b7bel", ",", "und", "noch", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "NN", "$,", "KON", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, ein ganzes Kriegesheer", "tokens": ["Ja", ",", "ein", "gan\u00b7zes", "Krie\u00b7ges\u00b7heer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6cht' ich gerne haben!", "tokens": ["M\u00f6cht'", "ich", "ger\u00b7ne", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bring' uns, lieber Weihnachtsmann,", "tokens": ["Bring'", "uns", ",", "lie\u00b7ber", "Weih\u00b7nachts\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bring' auch morgen, bringe", "tokens": ["Bring'", "auch", "mor\u00b7gen", ",", "brin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "ADV", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Musketier und Grenadier,", "tokens": ["Mus\u00b7ke\u00b7tier", "und", "Gre\u00b7na\u00b7di\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zottelb\u00e4r und Pantherthier,", "tokens": ["Zot\u00b7tel\u00b7b\u00e4r", "und", "Pan\u00b7ther\u00b7thier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ro\u00df und Esel, Schaf und Stier,", "tokens": ["Ro\u00df", "und", "E\u00b7sel", ",", "Schaf", "und", "Stier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lauter sch\u00f6ne Dinge!", "tokens": ["Lau\u00b7ter", "sch\u00f6\u00b7ne", "Din\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Doch du wei\u00dft ja unsern Wunsch,", "tokens": ["Doch", "du", "wei\u00dft", "ja", "un\u00b7sern", "Wunsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennst ja unsre Herzen.", "tokens": ["Kennst", "ja", "uns\u00b7re", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kinder, Vater und Mama,", "tokens": ["Kin\u00b7der", ",", "Va\u00b7ter", "und", "Ma\u00b7ma", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch sogar der Gro\u00dfpapa,", "tokens": ["Auch", "so\u00b7gar", "der", "Gro\u00df\u00b7pa\u00b7pa", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle, Alle sind wir da,", "tokens": ["Al\u00b7le", ",", "Al\u00b7le", "sind", "wir", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Warten dein mit Schmerzen.", "tokens": ["War\u00b7ten", "dein", "mit", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}