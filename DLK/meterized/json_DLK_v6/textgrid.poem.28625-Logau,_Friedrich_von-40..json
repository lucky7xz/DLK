{"textgrid.poem.28625": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Acker wol durchpfl\u00fcgen, einen Acker wohl bet\u00fcngen,", "tokens": ["Ei\u00b7nen", "A\u00b7cker", "wol", "durch\u00b7pfl\u00fc\u00b7gen", ",", "ei\u00b7nen", "A\u00b7cker", "wohl", "be\u00b7t\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Macht, da\u00df Unkraut mu\u00df verwelcken und das Land mu\u00df Fr\u00fcchte bringen.", "tokens": ["Macht", ",", "da\u00df", "Un\u00b7kraut", "mu\u00df", "ver\u00b7wel\u00b7cken", "und", "das", "Land", "mu\u00df", "Fr\u00fcch\u00b7te", "brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VMFIN", "VVINF", "KON", "ART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Lasterhafftes Wesen straffen, tugendhafftes Thun belohnen,", "tokens": ["Las\u00b7ter\u00b7haff\u00b7tes", "We\u00b7sen", "straf\u00b7fen", ",", "tu\u00b7gend\u00b7haff\u00b7tes", "Thun", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Macht, da\u00df Unheil ausser Landes, inner Landes Heil mu\u00df wohnen.", "tokens": ["Macht", ",", "da\u00df", "Un\u00b7heil", "aus\u00b7ser", "Lan\u00b7des", ",", "in\u00b7ner", "Lan\u00b7des", "Heil", "mu\u00df", "woh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "ADJA", "NN", "$,", "ADJA", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}