{"textgrid.poem.67141": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gesang des Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O heilig Herz der V\u00f6lker, o Vaterland!", "tokens": ["O", "hei\u00b7lig", "Herz", "der", "V\u00f6l\u00b7ker", ",", "o", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allduldend, gleich der schweigenden Mutter Erd,", "tokens": ["A\u00b7lldul\u00b7dend", ",", "gleich", "der", "schwei\u00b7gen\u00b7den", "Mut\u00b7ter", "Erd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und allverkannt, wenn schon aus deiner", "tokens": ["Und", "all\u00b7ver\u00b7kannt", ",", "wenn", "schon", "aus", "dei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tiefe die Fremden ihr Bestes haben!", "tokens": ["Tie\u00b7fe", "die", "Frem\u00b7den", "ihr", "Bes\u00b7tes", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sie ernten den Gedanken, den Geist von dir,", "tokens": ["Sie", "ern\u00b7ten", "den", "Ge\u00b7dan\u00b7ken", ",", "den", "Geist", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie pfl\u00fccken gern die Traube, doch h\u00f6hnen sie", "tokens": ["Sie", "pfl\u00fc\u00b7cken", "gern", "die", "Trau\u00b7be", ",", "doch", "h\u00f6h\u00b7nen", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dich, ungestalte Rebe! da\u00df du", "tokens": ["Dich", ",", "un\u00b7ge\u00b7stal\u00b7te", "Re\u00b7be", "!", "da\u00df", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$.", "KOUS", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schwankend den Boden und wild umirrest.", "tokens": ["Schwan\u00b7kend", "den", "Bo\u00b7den", "und", "wild", "um\u00b7ir\u00b7rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Du Land des hohen ernsteren Genius!", "tokens": ["Du", "Land", "des", "ho\u00b7hen", "erns\u00b7te\u00b7ren", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Land der Liebe! bin ich der deine schon,", "tokens": ["Du", "Land", "der", "Lie\u00b7be", "!", "bin", "ich", "der", "dei\u00b7ne", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$.", "VAFIN", "PPER", "ART", "PPOSAT", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oft z\u00fcrnt ich weinend, da\u00df du immer", "tokens": ["Oft", "z\u00fcrnt", "ich", "wei\u00b7nend", ",", "da\u00df", "du", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Bl\u00f6de die eigene Seele leugnest.", "tokens": ["Bl\u00f6\u00b7de", "die", "ei\u00b7ge\u00b7ne", "See\u00b7le", "leug\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Doch magst du manches Sch\u00f6ne nicht bergen mir;", "tokens": ["Doch", "magst", "du", "man\u00b7ches", "Sch\u00f6\u00b7ne", "nicht", "ber\u00b7gen", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "PTKNEG", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Oft stand ich \u00fcberschauend das holde Gr\u00fcn,", "tokens": ["Oft", "stand", "ich", "\u00fc\u00b7bersc\u00b7hau\u00b7end", "das", "hol\u00b7de", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den weiten Garten hoch in deinen", "tokens": ["Den", "wei\u00b7ten", "Gar\u00b7ten", "hoch", "in", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00fcften auf hellem Gebirg und sah dich.", "tokens": ["L\u00fcf\u00b7ten", "auf", "hel\u00b7lem", "Ge\u00b7birg", "und", "sah", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "An deinen Str\u00f6men ging ich und dachte dich,", "tokens": ["An", "dei\u00b7nen", "Str\u00f6\u00b7men", "ging", "ich", "und", "dach\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Indes die T\u00f6ne sch\u00fcchtern die Nachtigall", "tokens": ["In\u00b7des", "die", "T\u00f6\u00b7ne", "sch\u00fcch\u00b7tern", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf schwanker Weide sang, und still auf", "tokens": ["Auf", "schwan\u00b7ker", "Wei\u00b7de", "sang", ",", "und", "still", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "KON", "ADJD", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "D\u00e4mmerndem Grunde die Welle weilte.", "tokens": ["D\u00e4m\u00b7mern\u00b7dem", "Grun\u00b7de", "die", "Wel\u00b7le", "weil\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Und an den Ufern sah ich die St\u00e4dte bl\u00fchn,", "tokens": ["Und", "an", "den", "U\u00b7fern", "sah", "ich", "die", "St\u00e4d\u00b7te", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Edlen, wo der Flei\u00df in der Werkstatt schweigt,", "tokens": ["Die", "Ed\u00b7len", ",", "wo", "der", "Flei\u00df", "in", "der", "Werk\u00b7statt", "schweigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Wissenschaft, wo deine Sonne", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", ",", "wo", "dei\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Milde dem K\u00fcnstler zum Ernste leuchtet.", "tokens": ["Mil\u00b7de", "dem", "K\u00fcnst\u00b7ler", "zum", "Erns\u00b7te", "leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Kennst du Minervas Kinder? sie w\u00e4hlten sich", "tokens": ["Kennst", "du", "Mi\u00b7ner\u00b7vas", "Kin\u00b7der", "?", "sie", "w\u00e4hl\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "NN", "$.", "PPER", "VVFIN", "PRF"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Den \u00d6lbaum fr\u00fch zum Lieblinge; kennst du sie?", "tokens": ["Den", "\u00d6l\u00b7baum", "fr\u00fch", "zum", "Lieb\u00b7lin\u00b7ge", ";", "kennst", "du", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Noch lebt, noch waltet der Athener", "tokens": ["Noch", "lebt", ",", "noch", "wal\u00b7tet", "der", "A\u00b7the\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seele, die sinnende, still bei Menschen,", "tokens": ["See\u00b7le", ",", "die", "sin\u00b7nen\u00b7de", ",", "still", "bei", "Men\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Wenn Platons frommer Garten auch schon nicht mehr", "tokens": ["Wenn", "Pla\u00b7tons", "from\u00b7mer", "Gar\u00b7ten", "auch", "schon", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN", "ADV", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am alten Strome gr\u00fcnt und der d\u00fcrftge Mann", "tokens": ["Am", "al\u00b7ten", "Stro\u00b7me", "gr\u00fcnt", "und", "der", "d\u00fcrft\u00b7ge", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Heldenasche pfl\u00fcgt, und scheu der", "tokens": ["Die", "Hel\u00b7de\u00b7na\u00b7sche", "pfl\u00fcgt", ",", "und", "scheu", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vogel der Nacht auf der S\u00e4ule trauert.", "tokens": ["Vo\u00b7gel", "der", "Nacht", "auf", "der", "S\u00e4u\u00b7le", "trau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "O heilger Wald! o Attika! traf Er doch", "tokens": ["O", "heil\u00b7ger", "Wald", "!", "o", "At\u00b7ti\u00b7ka", "!", "traf", "Er", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "FM", "$.", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit seinem furchtbarn Strahle dich auch, so bald,", "tokens": ["Mit", "sei\u00b7nem", "furcht\u00b7barn", "Strah\u00b7le", "dich", "auch", ",", "so", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und eilten sie, die dich belebt, die", "tokens": ["Und", "eil\u00b7ten", "sie", ",", "die", "dich", "be\u00b7lebt", ",", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flammen entbunden zum Aether \u00fcber?", "tokens": ["Flam\u00b7men", "ent\u00b7bun\u00b7den", "zum", "A\u00b7e\u00b7ther", "\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "APPR", "$."], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}}, "stanza.10": {"line.1": {"text": "Doch, wie der Fr\u00fchling, wandelt der Genius", "tokens": ["Doch", ",", "wie", "der", "Fr\u00fch\u00b7ling", ",", "wan\u00b7delt", "der", "Ge\u00b7nius"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Land zu Land. Und wir? ist denn ", "tokens": ["Von", "Land", "zu", "Land", ".", "Und", "wir", "?", "ist", "denn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$.", "KON", "PPER", "$.", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von unsern J\u00fcnglingen, der nicht ein", "tokens": ["Von", "un\u00b7sern", "J\u00fcng\u00b7lin\u00b7gen", ",", "der", "nicht", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PTKNEG", "ART"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ahnden, ein R\u00e4tsel der Brust, verschwiege?", "tokens": ["Ahn\u00b7den", ",", "ein", "R\u00e4t\u00b7sel", "der", "Brust", ",", "ver\u00b7schwie\u00b7ge", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Den deutschen Frauen danket! sie haben uns", "tokens": ["Den", "deut\u00b7schen", "Frau\u00b7en", "dan\u00b7ket", "!", "sie", "ha\u00b7ben", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der G\u00f6tterbilder freundlichen Geist bewahrt,", "tokens": ["Der", "G\u00f6t\u00b7ter\u00b7bil\u00b7der", "freund\u00b7li\u00b7chen", "Geist", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und t\u00e4glich s\u00fchnt der holde klare", "tokens": ["Und", "t\u00e4g\u00b7lich", "s\u00fchnt", "der", "hol\u00b7de", "kla\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Friede das b\u00f6se Gewirre wieder.", "tokens": ["Frie\u00b7de", "das", "b\u00f6\u00b7se", "Ge\u00b7wir\u00b7re", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Wo sind jetzt Dichter, denen der Gott es gab,", "tokens": ["Wo", "sind", "jetzt", "Dich\u00b7ter", ",", "de\u00b7nen", "der", "Gott", "es", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie unsern Alten, freudig und fromm zu sein,", "tokens": ["Wie", "un\u00b7sern", "Al\u00b7ten", ",", "freu\u00b7dig", "und", "fromm", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo Weise, wie die unsre sind? die", "tokens": ["Wo", "Wei\u00b7se", ",", "wie", "die", "uns\u00b7re", "sind", "?", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "$,", "PWAV", "ART", "NN", "VAFIN", "$.", "ART"], "meter": "-+-+-+