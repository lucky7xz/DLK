{"textgrid.poem.35721": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "45. Rundgesang beim Bischof", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Wirt, die Gl\u00e4ser voll geschenket,", "tokens": ["Herr", "Wirt", ",", "die", "Gl\u00e4\u00b7ser", "voll", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als tugendsamer Wirt!", "tokens": ["Als", "tu\u00b7gend\u00b7sa\u00b7mer", "Wirt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es weidet nicht allein, es tr\u00e4nket", "tokens": ["Es", "wei\u00b7det", "nicht", "al\u00b7lein", ",", "es", "tr\u00e4n\u00b7ket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Herd' ein guter Hirt,", "tokens": ["Die", "Herd'", "ein", "gu\u00b7ter", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bald stockt die Red' im d\u00fcrren Halse,", "tokens": ["Bald", "stockt", "die", "Red'", "im", "d\u00fcr\u00b7ren", "Hal\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Braten, Fisch und Heringssalse,", "tokens": ["Von", "Bra\u00b7ten", ",", "Fisch", "und", "He\u00b7rings\u00b7sal\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo nicht gefeuchtet wird.", "tokens": ["Wo", "nicht", "ge\u00b7feuch\u00b7tet", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}