{"textgrid.poem.39122": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr schwatzt mir viel von Lebenszwecken,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr schwatzt mir viel von Lebenszwecken,", "tokens": ["Ihr", "schwatzt", "mir", "viel", "von", "Le\u00b7bens\u00b7zwe\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lebensziel und Ruhetag,", "tokens": ["Von", "Le\u00b7bens\u00b7ziel", "und", "Ru\u00b7he\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und qu\u00e4lt Euch m\u00fchsam auszuhecken,", "tokens": ["Und", "qu\u00e4lt", "Euch", "m\u00fch\u00b7sam", "aus\u00b7zu\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was wohl aus mir noch werden mag.", "tokens": ["Was", "wohl", "aus", "mir", "noch", "wer\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWS", "ADV", "APPR", "PPER", "ADV", "VAFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des stillen Gl\u00fcckes Seligkeiten", "tokens": ["Des", "stil\u00b7len", "Gl\u00fc\u00b7ckes", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erz\u00e4hlt Ihr alle gro\u00df und breit,", "tokens": ["Er\u00b7z\u00e4hlt", "Ihr", "al\u00b7le", "gro\u00df", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr sucht mich in den Pfad zu leiten", "tokens": ["Ihr", "sucht", "mich", "in", "den", "Pfad", "zu", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Philisterhafter H\u00e4uslichkeit.", "tokens": ["Phi\u00b7lis\u00b7ter\u00b7haf\u00b7ter", "H\u00e4us\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da soll ich von den Lenzgewittern", "tokens": ["Da", "soll", "ich", "von", "den", "Lenz\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der frischen Jugend endlich ruhn,", "tokens": ["Der", "fri\u00b7schen", "Ju\u00b7gend", "end\u00b7lich", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Weib mir nehmen, Kinder f\u00fcttern", "tokens": ["Ein", "Weib", "mir", "neh\u00b7men", ",", "Kin\u00b7der", "f\u00fct\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und still und fromm und h\u00e4uslich tun.", "tokens": ["Und", "still", "und", "fromm", "und", "h\u00e4us\u00b7lich", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da soll ich Flachs und Wolle schlichten", "tokens": ["Da", "soll", "ich", "Flachs", "und", "Wol\u00b7le", "schlich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Garben z\u00e4hlen nach dem Schock,", "tokens": ["Und", "Gar\u00b7ben", "z\u00e4h\u00b7len", "nach", "dem", "Schock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll G\u00e4nse m\u00e4sten, Hengste z\u00fcchten", "tokens": ["Soll", "G\u00e4n\u00b7se", "m\u00e4s\u00b7ten", ",", "Hengs\u00b7te", "z\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "NN", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ochs und Schaf und Ziegenbock.", "tokens": ["Und", "Ochs", "und", "Schaf", "und", "Zie\u00b7gen\u00b7bock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Indes die Sinne wild und waglich", "tokens": ["In\u00b7des", "die", "Sin\u00b7ne", "wild", "und", "wag\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Ungew\u00f6hnlichen mich ziehn,", "tokens": ["Zum", "Un\u00b7ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "mich", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll ich im Schlafrock tr\u00e4g', behaglich", "tokens": ["Soll", "ich", "im", "Schla\u00b7frock", "tr\u00e4g'", ",", "be\u00b7hag\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Kaffee sehn mein Pfeifchen gl\u00fchn.", "tokens": ["Beim", "Kaf\u00b7fee", "sehn", "mein", "Pfei\u00b7fchen", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich aber sag' Euch: eher fesseln", "tokens": ["Ich", "a\u00b7ber", "sag'", "Euch", ":", "e\u00b7her", "fes\u00b7seln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00f6nnt Ihr im Sturz den Wasserfall,", "tokens": ["K\u00f6nnt", "Ihr", "im", "Sturz", "den", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' Ihr's verm\u00f6gt, mich einzukesseln", "tokens": ["Eh'", "Ih\u00b7r's", "ver\u00b7m\u00f6gt", ",", "mich", "ein\u00b7zu\u00b7kes\u00b7seln"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In Euren engen G\u00e4nsestall.", "tokens": ["In", "Eu\u00b7ren", "en\u00b7gen", "G\u00e4n\u00b7se\u00b7stall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich aber sag' Euch: eher wandeln", "tokens": ["Ich", "a\u00b7ber", "sag'", "Euch", ":", "e\u00b7her", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00f6nnt Ihr zur Gans den Falken um,", "tokens": ["K\u00f6nnt", "Ihr", "zur", "Gans", "den", "Fal\u00b7ken", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' Ihr's verm\u00f6gt, mir einzuhandeln", "tokens": ["Eh'", "Ih\u00b7r's", "ver\u00b7m\u00f6gt", ",", "mir", "ein\u00b7zu\u00b7han\u00b7deln"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PPER", "VVINF"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Eu'r h\u00e4usliches Elysium.", "tokens": ["Eu'r", "h\u00e4us\u00b7li\u00b7ches", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich mag einmal darauf nicht eingehn,", "tokens": ["Ich", "mag", "ein\u00b7mal", "da\u00b7rauf", "nicht", "ein\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Euren schalen Alltagsspa\u00df,", "tokens": ["Auf", "Eu\u00b7ren", "scha\u00b7len", "All\u00b7tags\u00b7spa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will kecklich durch die Welt allein gehn", "tokens": ["Will", "keck\u00b7lich", "durch", "die", "Welt", "al\u00b7lein", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "APPR", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit meiner Lieb' und meinem Ha\u00df.", "tokens": ["Mit", "mei\u00b7ner", "Lieb'", "und", "mei\u00b7nem", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So h\u00f6rt denn auf mir vorzuleiern,", "tokens": ["So", "h\u00f6rt", "denn", "auf", "mir", "vor\u00b7zu\u00b7lei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df einst der freud'ge Drang vergeht,", "tokens": ["Da\u00df", "einst", "der", "freu\u00b7d'\u00b7ge", "Drang", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Drang nach Tat und Abenteuern,", "tokens": ["Der", "Drang", "nach", "Tat", "und", "A\u00b7bent\u00b7eu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wild durch meine Pulse weht.", "tokens": ["Der", "wild", "durch", "mei\u00b7ne", "Pul\u00b7se", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und m\u00fc\u00dft' er endlich doch erschlaffen,", "tokens": ["Und", "m\u00fc\u00dft'", "er", "end\u00b7lich", "doch", "er\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sprecht davon mir heut noch nicht,", "tokens": ["So", "sprecht", "da\u00b7von", "mir", "heut", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PPER", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indes im trotzigen Erraffen", "tokens": ["In\u00b7des", "im", "trot\u00b7zi\u00b7gen", "Er\u00b7raf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder Herzschlag anders spricht.", "tokens": ["Ein", "je\u00b7der", "Herz\u00b7schlag", "an\u00b7ders", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Indes ums Haupt sich, Kraft verk\u00fcndend,", "tokens": ["In\u00b7des", "ums", "Haupt", "sich", ",", "Kraft", "ver\u00b7k\u00fcn\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PRF", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die braune Jugendlocke schmiegt,", "tokens": ["Die", "brau\u00b7ne", "Ju\u00b7gen\u00b7dlo\u00b7cke", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indes das Aug' noch hell und z\u00fcndend,", "tokens": ["In\u00b7des", "das", "Aug'", "noch", "hell", "und", "z\u00fcn\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Geist noch frisch und unbesiegt.", "tokens": ["Der", "Geist", "noch", "frisch", "und", "un\u00b7be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Indes die Faust noch stahleskr\u00e4ftig", "tokens": ["In\u00b7des", "die", "Faust", "noch", "stah\u00b7les\u00b7kr\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich pre\u00dft an Feder oder Schwert;", "tokens": ["Sich", "pre\u00dft", "an", "Fe\u00b7der", "o\u00b7der", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indes das Blut noch wild gesch\u00e4ftig", "tokens": ["In\u00b7des", "das", "Blut", "noch", "wild", "ge\u00b7sch\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Herzen nach der Zunge f\u00e4hrt.", "tokens": ["Vom", "Her\u00b7zen", "nach", "der", "Zun\u00b7ge", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Sprecht mir davon, wenn matt und schw\u00e4chlich", "tokens": ["Sprecht", "mir", "da\u00b7von", ",", "wenn", "matt", "und", "schw\u00e4ch\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "$,", "KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Herz und Hand und Zunge ward:", "tokens": ["Mir", "Herz", "und", "Hand", "und", "Zun\u00b7ge", "ward", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann will ich leben fein gem\u00e4chlich", "tokens": ["Dann", "will", "ich", "le\u00b7ben", "fein", "ge\u00b7m\u00e4ch\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Eurer saubern Lebensart.", "tokens": ["Nach", "Eu\u00b7rer", "sau\u00b7bern", "Le\u00b7ben\u00b7sart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "La\u00dft vorderhand mich ungeschoren,", "tokens": ["La\u00dft", "vor\u00b7der\u00b7hand", "mich", "un\u00b7ge\u00b7scho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Philister bleibt vom Kopf zum Fu\u00df,", "tokens": ["Phi\u00b7lis\u00b7ter", "bleibt", "vom", "Kopf", "zum", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weil ich nicht dazu geboren,", "tokens": ["Und", "weil", "ich", "nicht", "da\u00b7zu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So will ich's sein erst, wenn ich mu\u00df.", "tokens": ["So", "will", "ich's", "sein", "erst", ",", "wenn", "ich", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPOSAT", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}