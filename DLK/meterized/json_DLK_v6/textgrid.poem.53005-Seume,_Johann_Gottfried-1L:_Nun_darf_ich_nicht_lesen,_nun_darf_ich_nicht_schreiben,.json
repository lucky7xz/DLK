{"textgrid.poem.53005": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun darf ich nicht lesen, nun darf ich nicht schreiben,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun darf ich nicht lesen, nun darf ich nicht schreiben,", "tokens": ["Nun", "darf", "ich", "nicht", "le\u00b7sen", ",", "nun", "darf", "ich", "nicht", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und mu\u00df mir mit Grillen die Tage vertreiben:", "tokens": ["Und", "mu\u00df", "mir", "mit", "Gril\u00b7len", "die", "Ta\u00b7ge", "ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da sitz' ich denn hier, ich erb\u00e4rmlicher Tropf,", "tokens": ["Da", "sitz'", "ich", "denn", "hier", ",", "ich", "er\u00b7b\u00e4rm\u00b7li\u00b7cher", "Tropf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Mit brausendem \u00fcbel zerr\u00fctteten Kopf.", "tokens": ["Mit", "brau\u00b7sen\u00b7dem", "\u00fc\u00b7bel", "zer\u00b7r\u00fct\u00b7te\u00b7ten", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Ich hab' in der neuen Welt und in der alten", "tokens": ["Ich", "hab'", "in", "der", "neu\u00b7en", "Welt", "und", "in", "der", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ART", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu Wasser und Lande manch St\u00fcrmchen gehalten,", "tokens": ["Zu", "Was\u00b7ser", "und", "Lan\u00b7de", "manch", "St\u00fcrm\u00b7chen", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und manche Kartatsche flog gl\u00fccklich vorbey;", "tokens": ["Und", "man\u00b7che", "Kar\u00b7tat\u00b7sche", "flog", "gl\u00fcck\u00b7lich", "vor\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nun brach ich fast selbst mir den Schedel entzwey.", "tokens": ["Nun", "brach", "ich", "fast", "selbst", "mir", "den", "Sche\u00b7del", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Herr Eckold, der Meister, schnitt r\u00fcstig und blickte,", "tokens": ["Herr", "E\u00b7ckold", ",", "der", "Meis\u00b7ter", ",", "schnitt", "r\u00fcs\u00b7tig", "und", "blick\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was unter und \u00fcber dem Schlafe mich dr\u00fcckte,", "tokens": ["Was", "un\u00b7ter", "und", "\u00fc\u00b7ber", "dem", "Schla\u00b7fe", "mich", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und sondete kl\u00fcglich bis nah an das Ohr,", "tokens": ["Und", "son\u00b7de\u00b7te", "kl\u00fcg\u00b7lich", "bis", "nah", "an", "das", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und drehte das Knochenfragmentchen hervor.", "tokens": ["Und", "dreh\u00b7te", "das", "Kno\u00b7chen\u00b7frag\u00b7ment\u00b7chen", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Das dr\u00f6hnte, das w\u00fchlte, das brannte von innen,", "tokens": ["Das", "dr\u00f6hn\u00b7te", ",", "das", "w\u00fchl\u00b7te", ",", "das", "brann\u00b7te", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als wollte das Hirn in dem Kasten zerrinnen,", "tokens": ["Als", "woll\u00b7te", "das", "Hirn", "in", "dem", "Kas\u00b7ten", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als br\u00e4che der Kn\u00f6chler von oben herein:", "tokens": ["Als", "br\u00e4\u00b7che", "der", "Kn\u00f6ch\u00b7ler", "von", "o\u00b7ben", "her\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ART", "NN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So trennt sich mit Wuth nur ein Z\u00f6llchen Gebein.", "tokens": ["So", "trennt", "sich", "mit", "Wuth", "nur", "ein", "Z\u00f6ll\u00b7chen", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Hier lungr' ich indessen, mit Blindheit geschlagen,", "tokens": ["Hier", "lun\u00b7gr'", "ich", "in\u00b7des\u00b7sen", ",", "mit", "Blind\u00b7heit", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bey schuftigem Schedel und herrlichem Magen,", "tokens": ["Bey", "schuf\u00b7ti\u00b7gem", "Sche\u00b7del", "und", "herr\u00b7li\u00b7chem", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Den Kopf in der Binde, und tr\u00e4ume mit Ruh", "tokens": ["Den", "Kopf", "in", "der", "Bin\u00b7de", ",", "und", "tr\u00e4u\u00b7me", "mit", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von Hirngicht und Knochenfra\u00df etwas dazu.", "tokens": ["Von", "Hirn\u00b7gicht", "und", "Kno\u00b7chen\u00b7fra\u00df", "et\u00b7was", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}}, "stanza.6": {"line.1": {"text": "Der Schmerz ist ein \u00dcbel von Upsal bis Goa,", "tokens": ["Der", "Schmerz", "ist", "ein", "\u00dc\u00b7bel", "von", "Up\u00b7sal", "bis", "Goa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Trotz aller Behauptung der Herrn aus der Stoa:", "tokens": ["Trotz", "al\u00b7ler", "Be\u00b7haup\u00b7tung", "der", "Herrn", "aus", "der", "Stoa", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch darum hat man mit der Weisheit gedingt,", "tokens": ["Doch", "da\u00b7rum", "hat", "man", "mit", "der", "Weis\u00b7heit", "ge\u00b7dingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Damit sie den Schmerz und das \u00dcbel bezwingt.", "tokens": ["Da\u00b7mit", "sie", "den", "Schmerz", "und", "das", "\u00dc\u00b7bel", "be\u00b7zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Der Mann nimmt die Schickungen, wie sie ihm kamen;", "tokens": ["Der", "Mann", "nimmt", "die", "Schi\u00b7ckun\u00b7gen", ",", "wie", "sie", "ihm", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wer dann nicht Kraft hat, verdient nicht den Nahmen,", "tokens": ["Und", "wer", "dann", "nicht", "Kraft", "hat", ",", "ver\u00b7dient", "nicht", "den", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PTKNEG", "NN", "VAFIN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was w\u00e4re denn unsere Philosophie?", "tokens": ["Was", "w\u00e4\u00b7re", "denn", "un\u00b7se\u00b7re", "Phi\u00b7lo\u00b7so\u00b7phie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hilft sie nicht, wenns Noth ist, so braucht man sie nie.", "tokens": ["Hilft", "sie", "nicht", ",", "wenns", "Noth", "ist", ",", "so", "braucht", "man", "sie", "nie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PIS", "PPER", "ADV", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich h\u00e4tte ja sch\u00e4ndlich die Jahre versplittert,", "tokens": ["Ich", "h\u00e4t\u00b7te", "ja", "sch\u00e4nd\u00b7lich", "die", "Jah\u00b7re", "ver\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "W\u00e4r' ich jetzt ein Knabe, der weinerlich zittert.", "tokens": ["W\u00e4r'", "ich", "jetzt", "ein", "Kna\u00b7be", ",", "der", "wei\u00b7ner\u00b7lich", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wem Tod und Gefahren noch f\u00fcrchterlich sind,", "tokens": ["Wem", "Tod", "und", "Ge\u00b7fah\u00b7ren", "noch", "f\u00fcrch\u00b7ter\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der bleibt f\u00fcr die Wahrheit wohl ewig ein Kind.", "tokens": ["Der", "bleibt", "f\u00fcr", "die", "Wahr\u00b7heit", "wohl", "e\u00b7wig", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Schon wird es, Dank sey es der Zang' und dem Messer,", "tokens": ["Schon", "wird", "es", ",", "Dank", "sey", "es", "der", "Zang'", "und", "dem", "Mes\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schon wird es ums Auge mir leichter und besser.", "tokens": ["Schon", "wird", "es", "ums", "Au\u00b7ge", "mir", "leich\u00b7ter", "und", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Unfug hat Luft und die Splitterchen drehn", "tokens": ["Der", "Un\u00b7fug", "hat", "Luft", "und", "die", "Split\u00b7ter\u00b7chen", "drehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sich sanft, um ganz sanft ihre Wege zu gehn.", "tokens": ["Sich", "sanft", ",", "um", "ganz", "sanft", "ih\u00b7re", "We\u00b7ge", "zu", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "KOUI", "ADV", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Es kommen die Freunde mit traulichem Wesen,", "tokens": ["Es", "kom\u00b7men", "die", "Freun\u00b7de", "mit", "trau\u00b7li\u00b7chem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den Zustand bey jedem Verbande zu lesen.", "tokens": ["Den", "Zu\u00b7stand", "bey", "je\u00b7dem", "Ver\u00b7ban\u00b7de", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Das thut denn doch g\u00fctlich; so nimmt man den Schnitt,", "tokens": ["Das", "thut", "denn", "doch", "g\u00fct\u00b7lich", ";", "so", "nimmt", "man", "den", "Schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADJD", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Schmerz, die Verknorplung, die Narbe noch mit.", "tokens": ["Den", "Schmerz", ",", "die", "Ver\u00b7knor\u00b7plung", ",", "die", "Nar\u00b7be", "noch", "mit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}