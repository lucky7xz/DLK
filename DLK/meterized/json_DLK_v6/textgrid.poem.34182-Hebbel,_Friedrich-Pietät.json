{"textgrid.poem.34182": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Piet\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Etwas Mitleid den K\u00fcnstlern und Dichtern, welche das H\u00f6chste", "tokens": ["Et\u00b7was", "Mit\u00b7leid", "den", "K\u00fcnst\u00b7lern", "und", "Dich\u00b7tern", ",", "wel\u00b7che", "das", "H\u00f6chs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "KON", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nicht erreichen, es sagt's ihnen kein Joseph voraus,", "tokens": ["Nicht", "er\u00b7rei\u00b7chen", ",", "es", "sagt's", "ih\u00b7nen", "kein", "Jo\u00b7se\u00b7ph", "vo\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PPER", "VVFIN", "PPER", "PIAT", "NE", "PTKVZ", "$,"], "meter": "+-+--++--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Und sie m\u00fcssen das Leben erst opfern, um zu erfahren,", "tokens": ["Und", "sie", "m\u00fcs\u00b7sen", "das", "Le\u00b7ben", "erst", "op\u00b7fern", ",", "um", "zu", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da\u00df es vergebens geschieht, darum verschont sie mit Spott.", "tokens": ["Da\u00df", "es", "ver\u00b7ge\u00b7bens", "ge\u00b7schieht", ",", "da\u00b7rum", "ver\u00b7schont", "sie", "mit", "Spott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}