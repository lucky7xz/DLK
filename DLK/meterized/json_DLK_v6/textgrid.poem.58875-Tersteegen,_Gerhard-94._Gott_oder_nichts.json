{"textgrid.poem.58875": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "94. Gott oder nichts", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du in D\u00fcrre bist inwendig,", "tokens": ["Wenn", "du", "in", "D\u00fcr\u00b7re", "bist", "in\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bleibe doch bei Gott best\u00e4ndig;", "tokens": ["So", "blei\u00b7be", "doch", "bei", "Gott", "be\u00b7st\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Such keinen Trost in andern Dingen,", "tokens": ["Such", "kei\u00b7nen", "Trost", "in", "an\u00b7dern", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die D\u00fcrre wird viel Fr\u00fcchte bringen!", "tokens": ["Die", "D\u00fcr\u00b7re", "wird", "viel", "Fr\u00fcch\u00b7te", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}