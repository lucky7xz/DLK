{"textgrid.poem.39150": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[ich habe nie das Knie gebogen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nie das Knie gebogen,", "tokens": ["Ich", "ha\u00b7be", "nie", "das", "Knie", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den starren Nacken nie gebeugt,", "tokens": ["Den", "star\u00b7ren", "Na\u00b7cken", "nie", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Stolze ward ich aufgezogen,", "tokens": ["Mit", "Stol\u00b7ze", "ward", "ich", "auf\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Freiheit ward ich aufges\u00e4ugt.", "tokens": ["Mit", "Frei\u00b7heit", "ward", "ich", "auf\u00b7ge\u00b7s\u00e4ugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch allem Stolz im Sein und Handeln", "tokens": ["Doch", "al\u00b7lem", "Stolz", "im", "Sein", "und", "Han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "NN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entsagt' ich, und der Freiheit mit,", "tokens": ["Ent\u00b7sagt'", "ich", ",", "und", "der", "Frei\u00b7heit", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich mich in den Staub verwandeln,", "tokens": ["K\u00f6nnt'", "ich", "mich", "in", "den", "Staub", "ver\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Deines Schuhes Sohle tritt.", "tokens": ["Den", "Dei\u00b7nes", "Schu\u00b7hes", "Soh\u00b7le", "tritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}