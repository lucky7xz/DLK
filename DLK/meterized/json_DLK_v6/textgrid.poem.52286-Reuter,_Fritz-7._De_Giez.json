{"textgrid.poem.52286": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "7. De Giez", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbjung'\u00ab, s\u00e4d de Oll, \u00bbdat kann nich ewig wohren,", "tokens": ["\u00bb", "jung'", "\u00ab", ",", "s\u00e4d", "de", "Oll", ",", "\u00bb", "dat", "kann", "nich", "e\u00b7wig", "woh\u00b7ren", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$(", "$,", "FM.la", "FM.la", "FM.la", "$,", "$(", "PAV", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du b\u00fcst nu ok all in de Johren,", "tokens": ["Du", "b\u00fcst", "nu", "ok", "all", "in", "de", "Joh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "APPR", "NE", "NE", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In unsre Schaul kannst ok nich wat mihr lihren,", "tokens": ["In", "uns\u00b7re", "Schaul", "kannst", "ok", "nich", "wat", "mihr", "lih\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "NE", "PTKNEG", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "De Preister sall di kunfirmieren,", "tokens": ["De", "Preis\u00b7ter", "sall", "di", "kun\u00b7fir\u00b7mie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Un wenn du di denn schickst in allen G\u00e4uden,", "tokens": ["Un", "wenn", "du", "di", "denn", "schickst", "in", "al\u00b7len", "G\u00e4u\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "NE", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn sallst du nahsten Ossen h\u00e4uden.\u00ab", "tokens": ["Denn", "sallst", "du", "nahs\u00b7ten", "Os\u00b7sen", "h\u00e4u\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Na, wenn de Oll wat s\u00e4d, denn h\u00fclp kein Reden;", "tokens": ["Na", ",", "wenn", "de", "Oll", "wat", "s\u00e4d", ",", "denn", "h\u00fclp", "kein", "Re\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "FM", "FM", "FM", "FM", "$,", "KON", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ick m\u00fc\u00dft nu also hen tau'n Beden.", "tokens": ["Ick", "m\u00fc\u00dft", "nu", "al\u00b7so", "hen", "tau'n", "Be\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Uns' Paster was en klauken Mann;", "tokens": ["Un\u00b7s'", "Pas\u00b7ter", "was", "en", "klau\u00b7ken", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "FM", "FM", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Hei sach't mi glik an mine Ogen an,", "tokens": ["Hei", "sach't", "mi", "glik", "an", "mi\u00b7ne", "O\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Dat ick man 'n beten d\u00fcsig wir", "tokens": ["Dat", "ick", "man", "'n", "be\u00b7ten", "d\u00fc\u00b7sig", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "ART", "ADJA", "ADJD", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Un dat ick \u00f6fter up de Pier", "tokens": ["Un", "dat", "ick", "\u00f6f\u00b7ter", "up", "de", "Pier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "As up de B\u00e4nken in de Schaul hadd reden.", "tokens": ["As", "up", "de", "B\u00e4n\u00b7ken", "in", "de", "Schaul", "hadd", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "APPR", "NE", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Indessen gung de Sak vel beter, as ick dacht:", "tokens": ["In\u00b7des\u00b7sen", "gung", "de", "Sak", "vel", "be\u00b7ter", ",", "as", "ick", "dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ick namm mi hellischen in acht", "tokens": ["Ick", "namm", "mi", "hel\u00b7li\u00b7schen", "in", "acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "VVFIN", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Un lihrt den Katekismus un de Bibel", "tokens": ["Un", "lihrt", "den", "Ka\u00b7te\u00b7kis\u00b7mus", "un", "de", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "FM", "FM", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Un lihrt de grote Waterfrag'.", "tokens": ["Un", "lihrt", "de", "gro\u00b7te", "Wa\u00b7ter\u00b7frag'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.18": {"text": "De Preister s\u00e4d: \u00bbMein Sohn, nicht \u00fcbel!", "tokens": ["De", "Preis\u00b7ter", "s\u00e4d", ":", "\u00bb", "Mein", "Sohn", ",", "nicht", "\u00fc\u00b7bel", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$.", "$(", "PPOSAT", "NN", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wenn du so bei bleibst, kommst du nach.\u00ab", "tokens": ["Wenn", "du", "so", "bei", "bleibst", ",", "kommst", "du", "nach", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Binah was ick all Primus, as sei't nenn'n,", "tokens": ["Bi\u00b7nah", "was", "ick", "all", "Pri\u00b7mus", ",", "as", "sei't", "nenn'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "PIAT", "NN", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Un \u00e4wer mi, dor stunnen man noch drei.", "tokens": ["Un", "\u00e4\u00b7wer", "mi", ",", "dor", "stun\u00b7nen", "man", "noch", "drei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "ADV", "VVFIN", "PIS", "ADV", "CARD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.22": {"text": "Doch jedes Ding, dat hett en En'n;", "tokens": ["Doch", "je\u00b7des", "Ding", ",", "dat", "hett", "en", "En'n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "'ne Wust hett sogar twei.", "tokens": ["'ne", "Wust", "hett", "so\u00b7gar", "twei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "De Preister kamm mal nah de Stuw' herinner", "tokens": ["De", "Preis\u00b7ter", "kamm", "mal", "nah", "de", "Stu\u00b7w'", "he\u00b7rin\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "ADJD", "NE", "NE", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Un s\u00e4d tau uns: \u00bbMin leiwen Kinner,", "tokens": ["Un", "s\u00e4d", "tau", "uns", ":", "\u00bb", "Min", "lei\u00b7wen", "Kin\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "Was ist das f\u00fcr ein Mensch, der immer", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "Mensch", ",", "der", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Sich weidet an des Goldes Schimmer", "tokens": ["Sich", "wei\u00b7det", "an", "des", "Gol\u00b7des", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und an den eitlen Mammonsgaben,", "tokens": ["Und", "an", "den", "eit\u00b7len", "Mam\u00b7mons\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der gierig ist, noch mehr zu haben,", "tokens": ["Der", "gie\u00b7rig", "ist", ",", "noch", "mehr", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "ADV", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Der immerfort sein Hab und Gut vermehrt", "tokens": ["Der", "im\u00b7mer\u00b7fort", "sein", "Hab", "und", "Gut", "ver\u00b7mehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und Sch\u00e4tze sammelt, die der Rost verzehrt?", "tokens": ["Und", "Sch\u00e4t\u00b7ze", "sam\u00b7melt", ",", "die", "der", "Rost", "ver\u00b7zehrt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Wie nennt man einen solchen Mann?\u00ab", "tokens": ["Wie", "nennt", "man", "ei\u00b7nen", "sol\u00b7chen", "Mann", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ART", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Hir stunn hei up un wendt sick an", "tokens": ["Hir", "stunn", "hei", "up", "un", "wendt", "sick", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.34": {"text": "Stin Durtig Hannemanns ut Golchen.", "tokens": ["Stin", "Dur\u00b7tig", "Han\u00b7ne\u00b7manns", "ut", "Gol\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "\u00bbmein liebes Kind, wie nennt man einen solchen?", "tokens": ["\u00bb", "mein", "lie\u00b7bes", "Kind", ",", "wie", "nennt", "man", "ei\u00b7nen", "sol\u00b7chen", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PIS", "ART", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "'n Gei ... 'n Gei ...?\u00ab Doch Durtig, de blew stumm.", "tokens": ["'n", "Gei", "...", "'n", "Gei", "...", "?", "\u00ab", "Doch", "Dur\u00b7tig", ",", "de", "blew", "stumm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "$.", "$(", "KON", "ADJD", "$,", "NE", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Herr Je! wat is de Dirn doch dumm!", "tokens": ["Herr", "Je", "!", "wat", "is", "de", "Dirn", "doch", "dumm", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "FM", "FM", "FM", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "So dacht ick, un ick plinkt ganz schlau", "tokens": ["So", "dacht", "ick", ",", "un", "ick", "plinkt", "ganz", "schlau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Den Preister mit de Ogen tau.", "tokens": ["Den", "Preis\u00b7ter", "mit", "de", "O\u00b7gen", "tau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Hei w\u00fcrd't gewohr un redt mi an:", "tokens": ["Hei", "w\u00fcrd't", "ge\u00b7wohr", "un", "redt", "mi", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.41": {"text": "\u00bbna, du, mein Sohn, was ist das f\u00fcr ein Mann,", "tokens": ["\u00bb", "na", ",", "du", ",", "mein", "Sohn", ",", "was", "ist", "das", "f\u00fcr", "ein", "Mann", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "$,", "PPOSAT", "NN", "$,", "PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Der fr\u00f6hnet so verruchtem Laster?", "tokens": ["Der", "fr\u00f6h\u00b7net", "so", "ver\u00b7ruch\u00b7tem", "Las\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "'n Gei ... 'n Gei ...?\u00ab \u2013 \u00bb'n Geistlichen, Herr Paster.\u00ab", "tokens": ["'n", "Gei", "...", "'n", "Gei", "...", "?", "\u00ab", "\u2013", "\u00bb", "'n", "Geist\u00b7li\u00b7chen", ",", "Herr", "Pas\u00b7ter", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "$.", "$(", "$(", "$(", "ART", "NN", "$,", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}