{"dta.poem.23312": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vetter Michel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verspottet nur den Vetter Michel!", "tokens": ["Ver\u00b7spot\u00b7tet", "nur", "den", "Vet\u00b7ter", "Mi\u00b7chel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er pfl\u00fcgt und s\u00e4t:", "tokens": ["Er", "pfl\u00fcgt", "und", "s\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Einst sprie\u00dft die Saat, die keine Sichel", "tokens": ["Einst", "sprie\u00dft", "die", "Saat", ",", "die", "kei\u00b7ne", "Si\u00b7chel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der l\u00f6blichen Censur ihm m\u00e4ht.", "tokens": ["Der", "l\u00f6b\u00b7li\u00b7chen", "Cen\u00b7sur", "ihm", "m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie leben noch die etwas wollen", "tokens": ["Sie", "le\u00b7ben", "noch", "die", "et\u00b7was", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIS", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Herz und Hand,", "tokens": ["Mit", "Herz", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Gut und Blut noch freudig zollen", "tokens": ["Die", "Gut", "und", "Blut", "noch", "freu\u00b7dig", "zol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Gott und f\u00fcr das Vaterland.", "tokens": ["F\u00fcr", "Gott", "und", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}