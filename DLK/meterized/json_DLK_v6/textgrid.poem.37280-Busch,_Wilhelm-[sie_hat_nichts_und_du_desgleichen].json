{"textgrid.poem.37280": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[sie hat nichts und du desgleichen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hat nichts und du desgleichen;", "tokens": ["Sie", "hat", "nichts", "und", "du", "des\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KON", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dennoch wollt ihr, wie ich sehe,", "tokens": ["Den\u00b7noch", "wollt", "ihr", ",", "wie", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Bund der heil'gen Ehe", "tokens": ["Zu", "dem", "Bund", "der", "heil'\u00b7gen", "E\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch bereits die H\u00e4nde reichen.", "tokens": ["Euch", "be\u00b7reits", "die", "H\u00e4n\u00b7de", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kinder, seid ihr denn bei Sinnen?", "tokens": ["Kin\u00b7der", ",", "seid", "ihr", "denn", "bei", "Sin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberlegt euch das Kapitel!", "tokens": ["\u00dc\u00b7ber\u00b7legt", "euch", "das", "Ka\u00b7pi\u00b7tel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne die geh\u00f6r'gen Mittel", "tokens": ["Oh\u00b7ne", "die", "ge\u00b7h\u00f6r'\u00b7gen", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll man keinen Krieg beginnen.", "tokens": ["Soll", "man", "kei\u00b7nen", "Krieg", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}