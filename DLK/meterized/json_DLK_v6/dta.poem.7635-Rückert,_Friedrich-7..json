{"dta.poem.7635": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sohn, du sollst dich nur auf Stra\u00dfen und auf Gassen,", "tokens": ["Mein", "Sohn", ",", "du", "sollst", "dich", "nur", "auf", "Stra\u00b7\u00dfen", "und", "auf", "Gas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehn mit ehrbaren, mit geehrten Leuten lassen.", "tokens": ["Sehn", "mit", "ehr\u00b7ba\u00b7ren", ",", "mit", "ge\u00b7ehr\u00b7ten", "Leu\u00b7ten", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "VVINF", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die halbe Ehr' ist dein, wenn man sich neigt vor ihnen;", "tokens": ["Die", "hal\u00b7be", "Ehr'", "ist", "dein", ",", "wenn", "man", "sich", "neigt", "vor", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "$,", "KOUS", "PIS", "PRF", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Ende lernest du die ganze selbst verdienen.", "tokens": ["Am", "En\u00b7de", "ler\u00b7nest", "du", "die", "gan\u00b7ze", "selbst", "ver\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "pun