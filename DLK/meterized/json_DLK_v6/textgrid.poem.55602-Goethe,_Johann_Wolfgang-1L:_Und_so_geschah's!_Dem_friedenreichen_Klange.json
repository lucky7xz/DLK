{"textgrid.poem.55602": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und so geschah's! Dem friedenreichen Klange", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so geschah's! Dem friedenreichen Klange", "tokens": ["Und", "so", "ge\u00b7scha\u00b7h's", "!", "Dem", "frie\u00b7den\u00b7rei\u00b7chen", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Bewegte sich das Land, und segenbar", "tokens": ["Be\u00b7weg\u00b7te", "sich", "das", "Land", ",", "und", "se\u00b7gen\u00b7bar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein frisches Gl\u00fcck erschien: im Hochgesange", "tokens": ["Ein", "fri\u00b7sches", "Gl\u00fcck", "er\u00b7schien", ":", "im", "Hoch\u00b7ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begr\u00fc\u00dften wir das junge F\u00fcrstenpaar;", "tokens": ["Be\u00b7gr\u00fc\u00df\u00b7ten", "wir", "das", "jun\u00b7ge", "F\u00fcrs\u00b7ten\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Vollgew\u00fchl, in lebensregem Drange", "tokens": ["Im", "Voll\u00b7ge\u00b7w\u00fchl", ",", "in", "le\u00b7bens\u00b7re\u00b7gem", "Dran\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vermischte sich die t\u00e4t'ge V\u00f6lkerschar,", "tokens": ["Ver\u00b7mischte", "sich", "die", "t\u00e4t'\u00b7ge", "V\u00f6l\u00b7ker\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Lind festlich ward an die geschm\u00fcckten Stufen", "tokens": ["Lind", "fest\u00b7lich", "ward", "an", "die", "ge\u00b7schm\u00fcck\u00b7ten", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Da h\u00f6r ich schreckhaft mittern\u00e4cht'ges L\u00e4uten,", "tokens": ["Da", "h\u00f6r", "ich", "schreck\u00b7haft", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7ges", "L\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das dumpf und schwer die Trauert\u00f6ne schwellt.", "tokens": ["Das", "dumpf", "und", "schwer", "die", "Trau\u00b7er\u00b7t\u00f6\u00b7ne", "schwellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist's m\u00f6glich? Soll es unsern Freund bedeuten,", "tokens": ["Ist's", "m\u00f6g\u00b7lich", "?", "Soll", "es", "un\u00b7sern", "Freund", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An den sich jeder Wunsch geklammert h\u00e4lt?", "tokens": ["An", "den", "sich", "je\u00b7der", "Wunsch", "ge\u00b7klam\u00b7mert", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRF", "PIAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Lebensw\u00fcrd'gen soll der Tod erbeuten?", "tokens": ["Den", "Le\u00b7bens\u00b7w\u00fcrd'\u00b7gen", "soll", "der", "Tod", "er\u00b7beu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ach! wie verwirrt solch ein Verlust die Welt!", "tokens": ["Ach", "!", "wie", "ver\u00b7wirrt", "solch", "ein", "Ver\u00b7lust", "die", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADJD", "PIAT", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ach! was zerst\u00f6rt ein solcher Ri\u00df den Seinen!", "tokens": ["Ach", "!", "was", "zer\u00b7st\u00f6rt", "ein", "sol\u00b7cher", "Ri\u00df", "den", "Sei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "ART", "PIAT", "NN", "ART", "PPOSS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nun weint die Welt, und sollten wir nicht weinen?", "tokens": ["Nun", "weint", "die", "Welt", ",", "und", "soll\u00b7ten", "wir", "nicht", "wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn er war unser! Wie bequem gesellig", "tokens": ["Denn", "er", "war", "un\u00b7ser", "!", "Wie", "be\u00b7quem", "ge\u00b7sel\u00b7lig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "$.", "PWAV", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den hohen Mann der gute Tag gezeigt,", "tokens": ["Den", "ho\u00b7hen", "Mann", "der", "gu\u00b7te", "Tag", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie bald sein Ernst, anschlie\u00dfend, wohlgef\u00e4llig,", "tokens": ["Wie", "bald", "sein", "Ernst", ",", "an\u00b7schlie\u00b7\u00dfend", ",", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Wechselrede heiter sich geneigt,", "tokens": ["Zur", "Wech\u00b7sel\u00b7re\u00b7de", "hei\u00b7ter", "sich", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald raschgewandt, geistreich und sicherstellig", "tokens": ["Bald", "raschge\u00b7wandt", ",", "geist\u00b7reich", "und", "si\u00b7cher\u00b7stel\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Lebensplane tiefen Sinn erzeugt", "tokens": ["Der", "Le\u00b7ben\u00b7spla\u00b7ne", "tie\u00b7fen", "Sinn", "er\u00b7zeugt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und fruchtbar sich in Rat und Tat ergossen;", "tokens": ["Und", "frucht\u00b7bar", "sich", "in", "Rat", "und", "Tat", "er\u00b7gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das haben wir erfahren und genossen.", "tokens": ["Das", "ha\u00b7ben", "wir", "er\u00b7fah\u00b7ren", "und", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn er war unser! Mag das stolze Wort", "tokens": ["Denn", "er", "war", "un\u00b7ser", "!", "Mag", "das", "stol\u00b7ze", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "$.", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den lauten Schmerz gewaltig \u00fcbert\u00f6nen!", "tokens": ["Den", "lau\u00b7ten", "Schmerz", "ge\u00b7wal\u00b7tig", "\u00fc\u00b7ber\u00b7t\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er mochte sich bei uns im sichern Port", "tokens": ["Er", "moch\u00b7te", "sich", "bei", "uns", "im", "si\u00b7chern", "Port"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach wildem Sturm zum Dauernden gew\u00f6hnen.", "tokens": ["Nach", "wil\u00b7dem", "Sturm", "zum", "Dau\u00b7ern\u00b7den", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Indessen schritt sein Geist gewaltig fort", "tokens": ["In\u00b7des\u00b7sen", "schritt", "sein", "Geist", "ge\u00b7wal\u00b7tig", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ins Ewige des Wahren, Guten, Sch\u00f6nen,", "tokens": ["Ins", "E\u00b7wi\u00b7ge", "des", "Wah\u00b7ren", ",", "Gu\u00b7ten", ",", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und hinter ihm, in wesenlosem Scheine,", "tokens": ["Und", "hin\u00b7ter", "ihm", ",", "in", "we\u00b7sen\u00b7lo\u00b7sem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Lag, was uns alle b\u00e4ndigt, das Gemeine.", "tokens": ["Lag", ",", "was", "uns", "al\u00b7le", "b\u00e4n\u00b7digt", ",", "das", "Ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun schm\u00fcckt' er sich die sch\u00f6ne Gartenzinne,", "tokens": ["Nun", "schm\u00fcckt'", "er", "sich", "die", "sch\u00f6\u00b7ne", "Gar\u00b7ten\u00b7zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von wannen er der Sterne Wort vernahm,", "tokens": ["Von", "wan\u00b7nen", "er", "der", "Ster\u00b7ne", "Wort", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das dem gleich ew'gen, gleich lebend'gen Sinne", "tokens": ["Das", "dem", "gleich", "ew'\u00b7gen", ",", "gleich", "le\u00b7ben\u00b7d'\u00b7gen", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PRELS", "ADV", "VVINF", "$,", "ADV", "ADJA", "NN"], "meter": "+--+----+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Geheimnisvoll und klar entgegenkam.", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "und", "klar", "ent\u00b7ge\u00b7gen\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort, sich und uns zu k\u00f6stlichem Gewinne,", "tokens": ["Dort", ",", "sich", "und", "uns", "zu", "k\u00f6st\u00b7li\u00b7chem", "Ge\u00b7win\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRF", "KON", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verwechselt' er die Zeiten wundersam,", "tokens": ["Ver\u00b7wech\u00b7selt'", "er", "die", "Zei\u00b7ten", "wun\u00b7der\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Begegnet' so, im W\u00fcrdigsten besch\u00e4ftigt,", "tokens": ["Be\u00b7geg\u00b7net'", "so", ",", "im", "W\u00fcr\u00b7digs\u00b7ten", "be\u00b7sch\u00e4f\u00b7tigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der D\u00e4mmerung, der Nacht, die uns entkr\u00e4ftigt.", "tokens": ["Der", "D\u00e4m\u00b7me\u00b7rung", ",", "der", "Nacht", ",", "die", "uns", "ent\u00b7kr\u00e4f\u00b7tigt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ihm schwollen der Geschichte Flut' auf Fluten,", "tokens": ["Ihm", "schwol\u00b7len", "der", "Ge\u00b7schich\u00b7te", "Flut'", "auf", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versp\u00fclend, was getadelt, was gelobt,", "tokens": ["Ver\u00b7sp\u00fc\u00b7lend", ",", "was", "ge\u00b7ta\u00b7delt", ",", "was", "ge\u00b7lobt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "VVPP", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Erdbeherrscher wilde Heeresgluten,", "tokens": ["Der", "Erd\u00b7be\u00b7herr\u00b7scher", "wil\u00b7de", "Hee\u00b7res\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die in der Welt sich grimmig ausgetobt,", "tokens": ["Die", "in", "der", "Welt", "sich", "grim\u00b7mig", "aus\u00b7ge\u00b7tobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im niedrig Schrecklichsten, im h\u00f6chsten Guten", "tokens": ["Im", "nied\u00b7rig", "Schreck\u00b7lichs\u00b7ten", ",", "im", "h\u00f6chs\u00b7ten", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJD", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Nach ihrem Wesen deutlich durchgeprobt. \u2013", "tokens": ["Nach", "ih\u00b7rem", "We\u00b7sen", "deut\u00b7lich", "durch\u00b7ge\u00b7probt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nun sank der Mond, und zu erneuter Wonne", "tokens": ["Nun", "sank", "der", "Mond", ",", "und", "zu", "er\u00b7neu\u00b7ter", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vom klaren Berg her\u00fcber stieg die Sonne.", "tokens": ["Vom", "kla\u00b7ren", "Berg", "her\u00b7\u00fc\u00b7ber", "stieg", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nun gl\