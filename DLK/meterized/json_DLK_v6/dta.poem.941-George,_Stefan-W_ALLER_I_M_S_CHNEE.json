{"dta.poem.941": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Mir ist als ob ein blick im dunkel glimme &#183;               ", "tokens": ["Mir", "ist", "als", "ob", "ein", "blick", "im", "dun\u00b7kel", "glim\u00b7me", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["PPER", "VAFIN", "KOKOM", "KOUS", "ART", "NN", "APPRART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So bebend w&#228;hltest du mich zum begleite               ", "tokens": ["So", "be\u00b7bend", "w", "&#228;", "hl\u00b7test", "du", "mich", "zum", "be\u00b7glei\u00b7te"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NE", "$(", "VVFIN", "PPER", "PRF", "APPRART", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dass ich die schwere wandrung benedeite               ", "tokens": ["Dass", "ich", "die", "schwe\u00b7re", "wand\u00b7rung", "be\u00b7ne\u00b7dei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So r&#252;hrte mich dein schritt und deine stimme             ", "tokens": ["So", "r", "&#252;", "hr\u00b7te", "mich", "dein", "schritt", "und", "dei\u00b7ne", "stim\u00b7me"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$(", "VVFIN", "PPER", "PPOSAT", "VVFIN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Du priesest mir die pracht der stillen erde               ", "tokens": ["Du", "prie\u00b7sest", "mir", "die", "pracht", "der", "stil\u00b7len", "er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ihrem silberlaub und k&#252;hlen strahle               ", "tokens": ["In", "ih\u00b7rem", "sil\u00b7ber\u00b7laub", "und", "k", "&#252;", "hlen", "strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NE", "$(", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die frei der lauten freude und beschwerde &#183;               ", "tokens": ["Die", "frei", "der", "lau\u00b7ten", "freu\u00b7de", "und", "be\u00b7schwer\u00b7de", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir nannten sie die einsam keusche fahle             ", "tokens": ["Wir", "nann\u00b7ten", "sie", "die", "ein\u00b7sam", "keu\u00b7sche", "fah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wir bekannten ihren rauhen m&#228;chten               ", "tokens": ["Und", "wir", "be\u00b7kann\u00b7ten", "ih\u00b7ren", "rau\u00b7hen", "m", "&#228;", "ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "NE"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dass in den reinen l&#252;ften t\u00f6ne hallten               ", "tokens": ["Dass", "in", "den", "rei\u00b7nen", "l", "&#252;", "ften", "t\u00f6\u00b7ne", "hall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$(", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dass sich die himmel f&#252;llten mit gestalten               ", "tokens": ["Dass", "sich", "die", "him\u00b7mel", "f", "&#252;", "ll\u00b7ten", "mit", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NE", "$(", "VVFIN", "APPR", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "So herrlich wie in keinen maien-n&#228;chten.             ", "tokens": ["So", "herr\u00b7lich", "wie", "in", "kei\u00b7nen", "mai\u00b7en\u00b7\u00b7n", "&#228;", "ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "PIAT", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}