{"dta.poem.510": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie liebet ihn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Bellise spricht zu mir/", "tokens": ["Bel\u00b7li\u00b7se", "spricht", "zu", "mir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Da\u00df ich ihr angenehm/", "tokens": ["Da\u00df", "ich", "ihr", "an\u00b7ge\u00b7nehm", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schweret hoch und theur", "tokens": ["Sie", "schwe\u00b7ret", "hoch", "und", "theur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihr mein Liebes-Feur/", "tokens": ["Da\u00df", "ihr", "mein", "Lie\u00b7bes\u00b7Feur", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gar nicht zu wiedern sey.", "tokens": ["Gar", "nicht", "zu", "wie\u00b7dern", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcg\u2019 ich mich hin zu ihr/", "tokens": ["F\u00fcg'", "ich", "mich", "hin", "zu", "ihr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "So stellet sie mir frey/", "tokens": ["So", "stel\u00b7let", "sie", "mir", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df ich den Mund bek\u00fcsse/", "tokens": ["Da\u00df", "ich", "den", "Mund", "be\u00b7k\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sie tr\u00e4gt da auf ", "tokens": ["Sie", "tr\u00e4gt", "da", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "In pr\u00e5chtigen ", "tokens": ["In", "pr\u00e5ch\u00b7ti\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Sie tr\u00e4nckt mit ", "tokens": ["Sie", "tr\u00e4nckt", "mit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Und zeuget Honig Fl\u00fcsse/", "tokens": ["Und", "zeu\u00b7get", "Ho\u00b7nig", "Fl\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Sie lehrt mich/ da\u00df die Kost/", "tokens": ["Sie", "lehrt", "mich", "/", "da\u00df", "die", "Kost", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Jm Lieben recht bequehm.", "tokens": ["Jm", "Lie\u00b7ben", "recht", "be\u00b7quehm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}