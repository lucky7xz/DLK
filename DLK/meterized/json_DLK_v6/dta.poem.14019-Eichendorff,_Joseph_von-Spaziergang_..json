{"dta.poem.14019": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Spaziergang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00df dich, Klatschrose und G\u00e4nseblum', Butterblum',", "tokens": ["Gr\u00fc\u00df", "dich", ",", "Klatsc\u00b7hro\u00b7se", "und", "G\u00e4n\u00b7se\u00b7blum'", ",", "But\u00b7ter\u00b7blum'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "KON", "NN", "$,", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "l\u00e4ndliches V\u00f6lkchen,", "tokens": ["l\u00e4nd\u00b7li\u00b7ches", "V\u00f6lk\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schmucklos und ohne Geruch, unschuldig \u2014 weiter", "tokens": ["Schmuck\u00b7los", "und", "oh\u00b7ne", "Ge\u00b7ruch", ",", "un\u00b7schul\u00b7dig", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "KON", "APPR", "NN", "$,", "ADJD", "$(", "ADV"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "sonst nichts? \u2014", "tokens": ["sonst", "nichts", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADV", "PIS", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}}}}