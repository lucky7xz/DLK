{"textgrid.poem.55803": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Deiner Lieder dunkelwarme Laute", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Deiner Lieder dunkelwarme Laute", "tokens": ["Wenn", "Dei\u00b7ner", "Lie\u00b7der", "dun\u00b7kel\u00b7war\u00b7me", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Glockent\u00f6ne weich ans Herz mir drangen,", "tokens": ["Wie", "Glo\u00b7cken\u00b7t\u00f6\u00b7ne", "weich", "ans", "Herz", "mir", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis meiner Seele starre H\u00fcllen sprangen", "tokens": ["Bis", "mei\u00b7ner", "See\u00b7le", "star\u00b7re", "H\u00fcl\u00b7len", "spran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Thr\u00e4n' auf Thr\u00e4ne trotzig niederthaute,", "tokens": ["Und", "Thr\u00e4n'", "auf", "Thr\u00e4\u00b7ne", "trot\u00b7zig", "nie\u00b7dert\u00b7hau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie ich dann in wonnig-s\u00fc\u00dfem Bangen,", "tokens": ["Und", "wie", "ich", "dann", "in", "won\u00b7nig\u00b7s\u00fc\u00b7\u00dfem", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In heiliger Scheu zu athmen kaum mich traute,", "tokens": ["In", "hei\u00b7li\u00b7ger", "Scheu", "zu", "ath\u00b7men", "kaum", "mich", "trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nach Deinen Lippen sehnsuchtsvoll nur schaute", "tokens": ["Nach", "Dei\u00b7nen", "Lip\u00b7pen", "sehn\u00b7suchts\u00b7voll", "nur", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In uners\u00e4ttlich seligem Verlangen \u2013 \u2013", "tokens": ["In", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "se\u00b7li\u00b7gem", "Ver\u00b7lan\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O, wer verg\u00e4\u00dfe jemals dieser, Tage,", "tokens": ["O", ",", "wer", "ver\u00b7g\u00e4\u00b7\u00dfe", "je\u00b7mals", "die\u00b7ser", ",", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "ADV", "PDAT", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich Natur und Kunst so sch\u00f6n verbunden,", "tokens": ["Wo", "sich", "Na\u00b7tur", "und", "Kunst", "so", "sch\u00f6n", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo leis' und leiser klang die tiefe Klage,", "tokens": ["Wo", "leis'", "und", "lei\u00b7ser", "klang", "die", "tie\u00b7fe", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und milder schmerzten ewig-off'ne Wunden,", "tokens": ["Und", "mil\u00b7der", "schmerz\u00b7ten", "e\u00b7wig\u00b7off'\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich gest\u00e4hlt mein Herz, das lebenszage,", "tokens": ["Wo", "sich", "ge\u00b7st\u00e4hlt", "mein", "Herz", ",", "das", "le\u00b7bens\u00b7za\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVPP", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr neuer K\u00e4mpfe schicksalsschwere Stunden.", "tokens": ["F\u00fcr", "neu\u00b7er", "K\u00e4mp\u00b7fe", "schick\u00b7sals\u00b7schwe\u00b7re", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}