{"textgrid.poem.45640": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Verst\u00e4rkter Reichsrat", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Reichsrats Ausspruch tut uns n\u00e4chstens kund,", "tokens": ["Des", "Reichs\u00b7rats", "Aus\u00b7spruch", "tut", "uns", "n\u00e4chs\u00b7tens", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob \u00d6streich Bundesstaat, ob Staatenbund,", "tokens": ["Ob", "\u00d6s\u00b7treich", "Bun\u00b7des\u00b7staat", ",", "ob", "Staa\u00b7ten\u00b7bund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und den man etwa Kaiser nennt,", "tokens": ["Und", "den", "man", "et\u00b7wa", "Kai\u00b7ser", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist dann noch h\u00f6chstens Pr\u00e4sident.", "tokens": ["Ist", "dann", "noch", "h\u00f6chs\u00b7tens", "Pr\u00e4\u00b7si\u00b7dent", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}