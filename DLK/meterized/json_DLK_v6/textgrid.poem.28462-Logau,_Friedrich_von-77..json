{"textgrid.poem.28462": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es \u00e4ndern sich die Leut, es \u00e4ndert sich die Zeit;", "tokens": ["Es", "\u00e4n\u00b7dern", "sich", "die", "Leut", ",", "es", "\u00e4n\u00b7dert", "sich", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum trauren dienet di\u00df, zur Freude jenes Kleid.", "tokens": ["Zum", "trau\u00b7ren", "die\u00b7net", "di\u00df", ",", "zur", "Freu\u00b7de", "je\u00b7nes", "Kleid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VVFIN", "PDS", "$,", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man \u00e4ndre gleich den Pfeil, wann nur verbleibt das Ziel;", "tokens": ["Man", "\u00e4nd\u00b7re", "gleich", "den", "Pfeil", ",", "wann", "nur", "ver\u00b7bleibt", "das", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann dieses wird erreicht, der Pfeil sey, wie er wil.", "tokens": ["Wann", "die\u00b7ses", "wird", "er\u00b7reicht", ",", "der", "Pfeil", "sey", ",", "wie", "er", "wil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}