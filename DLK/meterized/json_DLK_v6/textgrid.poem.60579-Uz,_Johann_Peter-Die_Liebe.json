{"textgrid.poem.60579": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da auf rauschendem Gefieder", "tokens": ["Da", "auf", "rau\u00b7schen\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zephyr uns den Fr\u00fchling bringt:", "tokens": ["Ze\u00b7phyr", "uns", "den", "Fr\u00fch\u00b7ling", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So erwacht die Freude wieder;", "tokens": ["So", "er\u00b7wacht", "die", "Freu\u00b7de", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles lacht und scherzt und singt.", "tokens": ["Al\u00b7les", "lacht", "und", "scherzt", "und", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tanzt, o tanzet, junge Sch\u00f6nen!", "tokens": ["Tanzt", ",", "o", "tan\u00b7zet", ",", "jun\u00b7ge", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner sanften Leyer nach,", "tokens": ["Mei\u00b7ner", "sanf\u00b7ten", "Le\u00b7yer", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welche nie mit leichtern T\u00f6nen", "tokens": ["Wel\u00b7che", "nie", "mit", "leich\u00b7tern", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter meinen H\u00e4nden sprach.", "tokens": ["Un\u00b7ter", "mei\u00b7nen", "H\u00e4n\u00b7den", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles f\u00fchlet nun die Triebe,", "tokens": ["Al\u00b7les", "f\u00fch\u00b7let", "nun", "die", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein Herze stets verschwur:", "tokens": ["Die", "kein", "Her\u00b7ze", "stets", "ver\u00b7schwur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles ladet euch zur Liebe,", "tokens": ["Al\u00b7les", "la\u00b7det", "euch", "zur", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jugend, Fr\u00fchling und Natur.", "tokens": ["Ju\u00b7gend", ",", "Fr\u00fch\u00b7ling", "und", "Na\u00b7tur", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie bekannt wird euerm Ohre", "tokens": ["Wie", "be\u00b7kannt", "wird", "eu\u00b7erm", "Oh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun die Stimme schlauer Lust!", "tokens": ["Nun", "die", "Stim\u00b7me", "schlau\u00b7er", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wie str\u00e4ubt im regen Flohre", "tokens": ["Und", "wie", "str\u00e4ubt", "im", "re\u00b7gen", "Floh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Sich die halbumflohrte Brust!", "tokens": ["Sich", "die", "hal\u00b7bum\u00b7flohr\u00b7te", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sollt ihr eine Wollust meiden,", "tokens": ["Sollt", "ihr", "ei\u00b7ne", "Wol\u00b7lust", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Weisen selbst beth\u00f6rt,", "tokens": ["Die", "den", "Wei\u00b7sen", "selbst", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Bildern trunkner Freuden", "tokens": ["Und", "mit", "Bil\u00b7dern", "trunk\u00b7ner", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch der Frommen Andacht st\u00f6rt?", "tokens": ["Auch", "der", "From\u00b7men", "An\u00b7dacht", "st\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "D\u00fcrft ihr die Natur verdammen?", "tokens": ["D\u00fcrft", "ihr", "die", "Na\u00b7tur", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr aufr\u00fchrisch widerstehn?", "tokens": ["Ihr", "auf\u00b7r\u00fch\u00b7risch", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns mit Liebe zu entflammen,", "tokens": ["Uns", "mit", "Lie\u00b7be", "zu", "ent\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6nen! wurdet ihr so sch\u00f6n.", "tokens": ["Sch\u00f6\u00b7nen", "!", "wur\u00b7det", "ihr", "so", "sch\u00f6n", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Liebet, weil ihr lieben sollet!", "tokens": ["Lie\u00b7bet", ",", "weil", "ihr", "lie\u00b7ben", "sol\u00b7let", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliehet Platons Unterricht!", "tokens": ["Flie\u00b7het", "Pla\u00b7tons", "Un\u00b7ter\u00b7richt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ihr niemals k\u00fcssen wollet,", "tokens": ["Wenn", "ihr", "nie\u00b7mals", "k\u00fcs\u00b7sen", "wol\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O so liebet lieber nicht.", "tokens": ["O", "so", "lie\u00b7bet", "lie\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weg mit Liebe, die nur denket,", "tokens": ["Weg", "mit", "Lie\u00b7be", ",", "die", "nur", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und, voll Schul-Gelehrsamkeit,", "tokens": ["Und", ",", "voll", "Schul\u00b7Ge\u00b7lehr\u00b7sam\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stets im kalten Ernst versenket,", "tokens": ["Stets", "im", "kal\u00b7ten", "Ernst", "ver\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch Begierden sich verbeut!", "tokens": ["Auch", "Be\u00b7gier\u00b7den", "sich", "ver\u00b7beut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Als in jenen dunkeln Jahren", "tokens": ["Als", "in", "je\u00b7nen", "dun\u00b7keln", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Amor ganz platonisch hie\u00df,", "tokens": ["A\u00b7mor", "ganz", "pla\u00b7to\u00b7nisch", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihm von best\u00e4ubten Haaren", "tokens": ["Und", "ihm", "von", "be\u00b7st\u00e4ub\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Rose d\u00fcftend blies:", "tokens": ["Kei\u00b7ne", "Ro\u00b7se", "d\u00fcf\u00b7tend", "blies", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flog er fern vom stillen Scherze,", "tokens": ["Flog", "er", "fern", "vom", "stil\u00b7len", "Scher\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis zum Sirius hinauf,", "tokens": ["Bis", "zum", "Si\u00b7rius", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PTKVZ", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Und besorgte seine Kerze", "tokens": ["Und", "be\u00b7sorg\u00b7te", "sei\u00b7ne", "Ker\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlechter, als der Sterne Lauf.", "tokens": ["Schlech\u00b7ter", ",", "als", "der", "Ster\u00b7ne", "Lauf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihn vom Himmel abzubringen,", "tokens": ["Ihn", "vom", "Him\u00b7mel", "ab\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ihn Erd und Menschheit rief;", "tokens": ["Da", "ihn", "Erd", "und", "Menschheit", "rief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "K\u00fcrztet ihr die stolzen Schwingen,", "tokens": ["K\u00fcrz\u00b7tet", "ihr", "die", "stol\u00b7zen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holde Nymphen! da er schlief.", "tokens": ["Hol\u00b7de", "Nym\u00b7phen", "!", "da", "er", "schlief", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da der Himmel ihm entgangen,", "tokens": ["Da", "der", "Him\u00b7mel", "ihm", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Flattert nun der Gott der Lust", "tokens": ["Flat\u00b7tert", "nun", "der", "Gott", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Um die rosenvollen Wangen", "tokens": ["Um", "die", "ro\u00b7sen\u00b7vol\u00b7len", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und um jede Liljen-Brust.", "tokens": ["Und", "um", "je\u00b7de", "Lil\u00b7jen\u00b7Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber wie an Fr\u00fchlings-Morgen", "tokens": ["A\u00b7ber", "wie", "an", "Fr\u00fch\u00b7lings\u00b7Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer jungen Rose Pracht,", "tokens": ["Ei\u00b7ner", "jun\u00b7gen", "Ro\u00b7se", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrdig Zephyrs liebster Sorgen,", "tokens": ["W\u00fcr\u00b7dig", "Ze\u00b7phyrs", "liebs\u00b7ter", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrdig aller W\u00fcnsche, lacht;", "tokens": ["W\u00fcr\u00b7dig", "al\u00b7ler", "W\u00fcn\u00b7sche", ",", "lacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die bis Titans niedrer Wagen", "tokens": ["Die", "bis", "Ti\u00b7tans", "nie\u00b7drer", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich im Abend-Meer verliert,", "tokens": ["Sich", "im", "A\u00b7ben\u00b7dMeer", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welket und in k\u00fcnftgen Tagen", "tokens": ["Wel\u00b7ket", "und", "in", "k\u00fcnft\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Keine Blicke mehr verf\u00fchrt:", "tokens": ["Kei\u00b7ne", "Bli\u00b7cke", "mehr", "ver\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "So verbl\u00fchn mit kurzem Prangen", "tokens": ["So", "ver\u00b7bl\u00fchn", "mit", "kur\u00b7zem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Bluhmen unsrer Lust,", "tokens": ["Auch", "die", "Bluh\u00b7men", "uns\u00b7rer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Rosen frischer Wangen,", "tokens": ["Die\u00b7se", "Ro\u00b7sen", "fri\u00b7scher", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Liljen einer Brust.", "tokens": ["Die\u00b7se", "Lil\u00b7jen", "ei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra