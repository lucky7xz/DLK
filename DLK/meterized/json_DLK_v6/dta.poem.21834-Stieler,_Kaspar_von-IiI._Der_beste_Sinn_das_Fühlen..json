{"dta.poem.21834": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Der beste Sinn/ das F\u00fchlen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist es/ edles F\u00fchlen/", "tokens": ["Du", "bist", "es", "/", "ed\u00b7les", "F\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "du sch\u00f6nster Sinn allein/", "tokens": ["du", "sch\u00f6ns\u00b7ter", "Sinn", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dehm aller Tichter Kielen", "tokens": ["dehm", "al\u00b7ler", "Tich\u00b7ter", "Kie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "zu Dienste sollen sein/", "tokens": ["zu", "Diens\u00b7te", "sol\u00b7len", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und ihm ein Lobmahl sezzen", "tokens": ["und", "ihm", "ein", "Lob\u00b7mahl", "sez\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "das nicht Gewalt noch Zeit/", "tokens": ["das", "nicht", "Ge\u00b7walt", "noch", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "NN", "ADV", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "noch Unfall kan verlezzen/", "tokens": ["noch", "Un\u00b7fall", "kan", "ver\u00b7lez\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "bi\u00df nach der Ewigkeit.", "tokens": ["bi\u00df", "nach", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Kommt her/ ihr Wei\u00dfheit-Gr\u00fcnder/", "tokens": ["Kommt", "her", "/", "ihr", "Wei\u00df\u00b7heit\u00b7Gr\u00fcn\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ihr Priester der Natur/", "tokens": ["ihr", "Pries\u00b7ter", "der", "Na\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "kommt alle F\u00f6bus-Kinder/", "tokens": ["kommt", "al\u00b7le", "F\u00f6\u00b7bus\u00b7Kin\u00b7der", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wofern ihr nur der Spur", "tokens": ["wo\u00b7fern", "ihr", "nur", "der", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "der Wahrheit/ nachzugehen", "tokens": ["der", "Wahr\u00b7heit", "/", "nach\u00b7zu\u00b7ge\u00b7hen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$(", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ein wenig seid gesinnt:", "tokens": ["ein", "we\u00b7nig", "seid", "ge\u00b7sinnt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "da\u00df f\u00fchlen \u00fcberwindt.", "tokens": ["da\u00df", "f\u00fch\u00b7len", "\u00fc\u00b7berw\u00b7indt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gesicht/ die G\u00f6tter-Gabe/", "tokens": ["Ge\u00b7sicht", "/", "die", "G\u00f6t\u00b7ter\u00b7Ga\u00b7be", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "bringt manchen zu dem Grabe/", "tokens": ["bringt", "man\u00b7chen", "zu", "dem", "Gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "der sich zusehr vergist", "tokens": ["der", "sich", "zu\u00b7sehr", "ver\u00b7gist"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "in einer Sch\u00f6nen blikken/", "tokens": ["in", "ei\u00b7ner", "Sch\u00f6\u00b7nen", "blik\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "was ich nicht sehen kan/", "tokens": ["was", "ich", "nicht", "se\u00b7hen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "das kan mich nicht bestrikken", "tokens": ["das", "kan", "mich", "nicht", "be\u00b7strik\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "noch str\u00e4fflich reizen an.", "tokens": ["noch", "str\u00e4ff\u00b7lich", "rei\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das h\u00f6ren bringt offt Schrekken", "tokens": ["Das", "h\u00f6\u00b7ren", "bringt", "offt", "Schrek\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVINF", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und schafft Uneinigkeit.", "tokens": ["und", "schafft", "Un\u00b7ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was Mus", "tokens": ["Was", "Mus"], "token_info": ["word", "word"], "pos": ["PWS", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "w\u00e4hrt eine kurze Zeit.", "tokens": ["w\u00e4hrt", "ei\u00b7ne", "kur\u00b7ze", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach! manche wird beth\u00f6ret/", "tokens": ["Ach", "!", "man\u00b7che", "wird", "be\u00b7th\u00f6\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIS", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wenn sie der Rede Tohn", "tokens": ["wenn", "sie", "der", "Re\u00b7de", "Tohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "der Junggesellen h\u00f6net/", "tokens": ["der", "Jung\u00b7ge\u00b7sel\u00b7len", "h\u00f6\u00b7net", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "und k\u00f6mmt in Spott und Hohn.", "tokens": ["und", "k\u00f6mmt", "in", "Spott", "und", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Geruch ist kaum zunennen/", "tokens": ["Ge\u00b7ruch", "ist", "kaum", "zu\u00b7nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Rosen-wind nicht kennen/", "tokens": ["Die", "Ro\u00b7sen\u00b7wind", "nicht", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "veriaget kein Gestank", "tokens": ["ve\u00b7ria\u00b7get", "kein", "Ge\u00b7stank"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Mensche kan wol leben/", "tokens": ["Ein", "Men\u00b7sche", "kan", "wol", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und h\u00e4tt\u2019 ihm nimmermehr", "tokens": ["und", "h\u00e4tt'", "ihm", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "das Riechen Lust gegeben.", "tokens": ["das", "Rie\u00b7chen", "Lust", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bleibt Schmekken denn die Ehr.", "tokens": ["Bleibt", "Schmek\u00b7ken", "denn", "die", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dem Wollust-vollem Schmekken/", "tokens": ["Dem", "Wol\u00b7lust\u00b7vol\u00b7lem", "Schmek\u00b7ken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dem Lufft/ Fluht/ Erde dient/", "tokens": ["dem", "Lufft", "/", "Fluht", "/", "Er\u00b7de", "dient", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dem Vogel\u2019 junge hekken/", "tokens": ["dem", "Vo\u00b7gel'", "jun\u00b7ge", "hek\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dem Wald und Wiese gr\u00fcnt/", "tokens": ["dem", "Wald", "und", "Wie\u00b7se", "gr\u00fcnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "umb den der Fischer leget", "tokens": ["umb", "den", "der", "Fi\u00b7scher", "le\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "die falschen Reusen ein/", "tokens": ["die", "fal\u00b7schen", "Reu\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "ists nicht/ der Beutel feget/", "tokens": ["ists", "nicht", "/", "der", "Beu\u00b7tel", "fe\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "und hei\u00dft uns kranke sein.", "tokens": ["und", "hei\u00dft", "uns", "kran\u00b7ke", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In F\u00fchlen nur alleine", "tokens": ["In", "F\u00fch\u00b7len", "nur", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "besteht der Sinnen Grund/", "tokens": ["be\u00b7steht", "der", "Sin\u00b7nen", "Grund", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ohn diesen Leben keine.", "tokens": ["ohn", "die\u00b7sen", "Le\u00b7ben", "kei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aug/ Ohren/ Nase/ Mund/", "tokens": ["Aug", "/", "Oh\u00b7ren", "/", "Na\u00b7se", "/", "Mund", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "ergreiffen keine Sachen", "tokens": ["er\u00b7greif\u00b7fen", "kei\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die ihnen Gegend stehn.", "tokens": ["die", "ih\u00b7nen