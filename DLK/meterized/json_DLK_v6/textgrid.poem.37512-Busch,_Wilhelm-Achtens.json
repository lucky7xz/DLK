{"textgrid.poem.37512": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Achtens", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Christ versp\u00fcret gro\u00dfen Drang,", "tokens": ["Ein", "Christ", "ver\u00b7sp\u00fc\u00b7ret", "gro\u00b7\u00dfen", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das heil'ge Grab zu sehn;", "tokens": ["Das", "heil'\u00b7ge", "Grab", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum will Antonius schon lang", "tokens": ["Drum", "will", "An\u00b7to\u00b7ni\u00b7us", "schon", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "NE", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin wallfahrten gehn.", "tokens": ["Da\u00b7hin", "wall\u00b7fahr\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schickt sich, da\u00df ein frommer Mann", "tokens": ["Es", "schickt", "sich", ",", "da\u00df", "ein", "from\u00b7mer", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sache \u00fcberlegt;", "tokens": ["Die", "Sa\u00b7che", "\u00fc\u00b7ber\u00b7legt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er schafft sich einen Esel an,", "tokens": ["Er", "schafft", "sich", "ei\u00b7nen", "E\u00b7sel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ihm den Ranzen tr\u00e4gt.", "tokens": ["Der", "ihm", "den", "Ran\u00b7zen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So zogen sie hinaus zum Tor", "tokens": ["So", "zo\u00b7gen", "sie", "hin\u00b7aus", "zum", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APZR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcrder allgemach;", "tokens": ["Und", "f\u00fcr\u00b7der", "all\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Heilige, der ging hervor,", "tokens": ["Der", "Hei\u00b7li\u00b7ge", ",", "der", "ging", "her\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Esel hinten nach.", "tokens": ["Der", "E\u00b7sel", "hin\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da kam aus seinem Hinterhalt", "tokens": ["Da", "kam", "aus", "sei\u00b7nem", "Hin\u00b7ter\u00b7halt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein B\u00e4r in schnellem Lauf;", "tokens": ["Ein", "B\u00e4r", "in", "schnel\u00b7lem", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er greift den Esel alsobald", "tokens": ["Er", "greift", "den", "E\u00b7sel", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zehrt ihn m\u00e4hlich auf.", "tokens": ["Und", "zehrt", "ihn", "m\u00e4h\u00b7lich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Antonius, als ein guter Christ,", "tokens": ["An\u00b7to\u00b7ni\u00b7us", ",", "als", "ein", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Schaut's an mit Seelenruh':", "tokens": ["Schaut's", "an", "mit", "See\u00b7len\u00b7ruh'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbhe, Alter! Wenn du fertig bist, \u2013", "tokens": ["\u00bb", "he", ",", "Al\u00b7ter", "!", "Wenn", "du", "fer\u00b7tig", "bist", ",", "\u2013"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$,", "NN", "$.", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohlan! \u2013 so trage du!\u00ab", "tokens": ["Wo\u00b7hlan", "!", "\u2013", "so", "tra\u00b7ge", "du", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der heilige Antonius macht", "tokens": ["Der", "hei\u00b7li\u00b7ge", "An\u00b7to\u00b7ni\u00b7us", "macht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich bald das Ding bequem;", "tokens": ["Sich", "bald", "das", "Ding", "be\u00b7quem", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er setzt sich auf und reitet sacht", "tokens": ["Er", "setzt", "sich", "auf", "und", "rei\u00b7tet", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis nach Jerusalem.", "tokens": ["Bis", "nach", "Je\u00b7ru\u00b7sa\u00b7lem", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wo Salomonis Tempel stand,", "tokens": ["Wo", "Sa\u00b7lo\u00b7mo\u00b7nis", "Tem\u00b7pel", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt mancher dicke Stein;", "tokens": ["Liegt", "man\u00b7cher", "di\u00b7cke", "Stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den allerdicksten, den er fand,", "tokens": ["Den", "al\u00b7ler\u00b7dicks\u00b7ten", ",", "den", "er", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Packt St. Antonius ein.", "tokens": ["Packt", "St.", "An\u00b7to\u00b7ni\u00b7us", "ein", "."], "token_info": ["word", "abbreviation", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sprach: \u00bbDen Stein, den nehm' ich mit!\u00ab", "tokens": ["Er", "sprach", ":", "\u00bb", "Den", "Stein", ",", "den", "nehm'", "ich", "mit", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "$,", "ART", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der B\u00e4r, der macht: Brumm brumm!", "tokens": ["Der", "B\u00e4r", ",", "der", "macht", ":", "Brumm", "brumm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$.", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das hilft ihm aber alles nit,", "tokens": ["Das", "hilft", "ihm", "a\u00b7ber", "al\u00b7les", "nit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir k\u00fcmmern uns nicht drum.", "tokens": ["Wir", "k\u00fcm\u00b7mern", "uns", "nicht", "drum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der B\u00e4r, obschon ganz krumm und matt,", "tokens": ["Der", "B\u00e4r", ",", "ob\u00b7schon", "ganz", "krumm", "und", "matt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Setzt sich in kurzen Trab", "tokens": ["Setzt", "sich", "in", "kur\u00b7zen", "Trab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Bis hin nach Padua der Stadt;", "tokens": ["Bis", "hin", "nach", "Pa\u00b7dua", "der", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NE", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da stieg Antonius ab.", "tokens": ["Da", "stieg", "An\u00b7to\u00b7ni\u00b7us", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Und milde sprach der heil'ge Mann:", "tokens": ["Und", "mil\u00b7de", "sprach", "der", "heil'\u00b7ge", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmein Freund, du kannst nun gehn!", "tokens": ["\u00bb", "mein", "Freund", ",", "du", "kannst", "nun", "gehn", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie es einem gehen kann,", "tokens": ["Und", "wie", "es", "ei\u00b7nem", "ge\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das hast du nun gesehn!\u00ab", "tokens": ["Das", "hast", "du", "nun", "ge\u00b7sehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der B\u00e4r, als er zum Walde schlich,", "tokens": ["Der", "B\u00e4r", ",", "als", "er", "zum", "Wal\u00b7de", "schlich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der brummte vor sich her:", "tokens": ["Der", "brumm\u00b7te", "vor", "sich", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein lebelang bek\u00fcmmr' ich mich", "tokens": ["\u00bb", "mein", "le\u00b7be\u00b7lang", "be\u00b7k\u00fcmm\u00b7r'", "ich", "mich"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJD", "VVFIN", "PPER", "PRF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Um keinen Esel mehr!\u00ab", "tokens": ["Um", "kei\u00b7nen", "E\u00b7sel", "mehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PIAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}