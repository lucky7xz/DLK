{"textgrid.poem.56078": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lampe auf den verlassenen Papieren,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lampe auf den verlassenen Papieren,", "tokens": ["Lam\u00b7pe", "auf", "den", "ver\u00b7las\u00b7se\u00b7nen", "Pa\u00b7pie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und ringsum Nacht bis weit hinein ins Holz", "tokens": ["und", "ring\u00b7sum", "Nacht", "bis", "weit", "hin\u00b7ein", "ins", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "ADV", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Schr\u00e4nke. Und er konnte sich verlieren", "tokens": ["der", "Schr\u00e4n\u00b7ke", ".", "Und", "er", "konn\u00b7te", "sich", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "an sein Geschlecht, das nun mit ihm zerschmolz;", "tokens": ["an", "sein", "Ge\u00b7schlecht", ",", "das", "nun", "mit", "ihm", "zer\u00b7schmolz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ihm schien, je mehr er las, er h\u00e4tte ihren,", "tokens": ["ihm", "schien", ",", "je", "mehr", "er", "las", ",", "er", "h\u00e4t\u00b7te", "ih\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sie aber hatten alle seinen Stolz.", "tokens": ["sie", "a\u00b7ber", "hat\u00b7ten", "al\u00b7le", "sei\u00b7nen", "Stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hochm\u00fctig steiften sich die leeren St\u00fchle", "tokens": ["Hoch\u00b7m\u00fc\u00b7tig", "steif\u00b7ten", "sich", "die", "lee\u00b7ren", "St\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Wand entlang, und lauter Selbstgef\u00fchle", "tokens": ["die", "Wand", "ent\u00b7lang", ",", "und", "lau\u00b7ter", "Selbst\u00b7ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPO", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "machten sich schl\u00e4fernd in den M\u00f6beln breit;", "tokens": ["mach\u00b7ten", "sich", "schl\u00e4\u00b7fernd", "in", "den", "M\u00f6\u00b7beln", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "von oben go\u00df sich Nacht auf die Pend\u00fcle,", "tokens": ["von", "o\u00b7ben", "go\u00df", "sich", "Nacht", "auf", "die", "Pen\u00b7d\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PRF", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und zitternd rann aus ihrer goldnen M\u00fchle,", "tokens": ["und", "zit\u00b7ternd", "rann", "aus", "ih\u00b7rer", "gold\u00b7nen", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ganz fein gemahlen, seine Zeit.", "tokens": ["ganz", "fein", "ge\u00b7mah\u00b7len", ",", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er nahm sie nicht. Um fiebernd unter jenen,", "tokens": ["Er", "nahm", "sie", "nicht", ".", "Um", "fie\u00b7bernd", "un\u00b7ter", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "KOUI", "ADJD", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "als z\u00f6ge er die Laken ihrer Leiber,", "tokens": ["als", "z\u00f6\u00b7ge", "er", "die", "La\u00b7ken", "ih\u00b7rer", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "andere Zeiten wegzuzerrn.", "tokens": ["an\u00b7de\u00b7re", "Zei\u00b7ten", "weg\u00b7zu\u00b7zerrn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bis er ins Fl\u00fcstern kam; (was war ihm fern?)", "tokens": ["Bis", "er", "ins", "Fl\u00fcs\u00b7tern", "kam", ";(", "was", "war", "ihm", "fern", "?", ")"], "token_info": ["word", "word", "word", "word", "word", "emoticon", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$(", "PWS", "VAFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er lobte einen dieser Briefeschreiber,", "tokens": ["Er", "lob\u00b7te", "ei\u00b7nen", "die\u00b7ser", "Brie\u00b7fe\u00b7schrei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "als sei der Brief an ihn: Wie du mich kennst;", "tokens": ["als", "sei", "der", "Brief", "an", "ihn", ":", "Wie", "du", "mich", "kennst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "ART", "NN", "APPR", "PPER", "$.", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und klopfte lustig auf die Seitenlehnen.", "tokens": ["und", "klopf\u00b7te", "lus\u00b7tig", "auf", "die", "Sei\u00b7ten\u00b7leh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Spiegel aber, innen unbegrenzter,", "tokens": ["Der", "Spie\u00b7gel", "a\u00b7ber", ",", "in\u00b7nen", "un\u00b7be\u00b7grenz\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "lie\u00df leise einen Vorhang aus, ein Fenster \u2013 :", "tokens": ["lie\u00df", "lei\u00b7se", "ei\u00b7nen", "Vor\u00b7hang", "aus", ",", "ein", "Fens\u00b7ter", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "denn dorten stand, fast fertig, das Gespenst.", "tokens": ["denn", "dor\u00b7ten", "stand", ",", "fast", "fer\u00b7tig", ",", "das", "Ge\u00b7spenst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADV", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}