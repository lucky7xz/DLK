{"dta.poem.8634": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Leichte Tr\u00fcbung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Woher dies pl\u00f6tzliche Verstummen?", "tokens": ["Wo\u00b7her", "dies", "pl\u00f6tz\u00b7li\u00b7che", "Ver\u00b7stum\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und diese Wolken, kummerschwer,", "tokens": ["Und", "die\u00b7se", "Wol\u00b7ken", ",", "kum\u00b7mer\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mir dein Angesicht vermummen,", "tokens": ["Die", "mir", "dein", "An\u00b7ge\u00b7sicht", "ver\u00b7mum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das erst so froh gestrahlt, woher?", "tokens": ["Das", "erst", "so", "froh", "ge\u00b7strahlt", ",", "wo\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VVPP", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201esiehst du den blauen Berg dort ragen,", "tokens": ["\u201e", "siehst", "du", "den", "blau\u00b7en", "Berg", "dort", "ra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Felsen in die L\u00fcfte hebt,", "tokens": ["Der", "Fel\u00b7sen", "in", "die", "L\u00fcf\u00b7te", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An welchen selbst die Gemsen zagen,", "tokens": ["An", "wel\u00b7chen", "selbst", "die", "Gem\u00b7sen", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der erschrockne J\u00e4ger bebt? \u2014", "tokens": ["Und", "der", "er\u00b7schrock\u00b7ne", "J\u00e4\u00b7ger", "bebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von seinem Gipfel schleudre du", "tokens": ["Von", "sei\u00b7nem", "Gip\u00b7fel", "schleud\u00b7re", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Steinchen spielend in die Tiefen:", "tokens": ["Ein", "Stein\u00b7chen", "spie\u00b7lend", "in", "die", "Tie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du st\u00f6rst der L\u00fcfte schwanke Ruh,", "tokens": ["Du", "st\u00f6rst", "der", "L\u00fcf\u00b7te", "schwan\u00b7ke", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Nebel steigen, die dort schliefen.", "tokens": ["Und", "Ne\u00b7bel", "stei\u00b7gen", ",", "die", "dort", "schlie\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So warfst du, seine Kraft nicht ahnend,", "tokens": ["So", "warfst", "du", ",", "sei\u00b7ne", "Kraft", "nicht", "ah\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein W\u00f6rtchen mir in meine Brust,", "tokens": ["Ein", "W\u00f6rt\u00b7chen", "mir", "in", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein W\u00f6rtchen, leise, aber mahnend,", "tokens": ["Ein", "W\u00f6rt\u00b7chen", ",", "lei\u00b7se", ",", "a\u00b7ber", "mah\u00b7nend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und sieh, nun stieg der tr\u00fcbe Wust", "tokens": ["Und", "sieh", ",", "nun", "stieg", "der", "tr\u00fc\u00b7be", "Wust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von Nebelbildern alter Kr\u00e4nkung", "tokens": ["Von", "Ne\u00b7bel\u00b7bil\u00b7dern", "al\u00b7ter", "Kr\u00e4n\u00b7kung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Aus ihrer stillen Nachtversenkung.\u201c", "tokens": ["Aus", "ih\u00b7rer", "stil\u00b7len", "Nacht\u00b7ver\u00b7sen\u00b7kung", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}