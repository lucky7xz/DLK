{"dta.poem.16113": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "167.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh, suche Menschen auf, um dich als Mensch zu f\u00fchlen", "tokens": ["Geh", ",", "su\u00b7che", "Men\u00b7schen", "auf", ",", "um", "dich", "als", "Mensch", "zu", "f\u00fch\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "PTKVZ", "$,", "KOUI", "PPER", "KOUS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In andern, ohne tr\u00fcb' im Busen dir zu w\u00fchlen.", "tokens": ["In", "an\u00b7dern", ",", "oh\u00b7ne", "tr\u00fcb'", "im", "Bu\u00b7sen", "dir", "zu", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "KOUI", "VVFIN", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Such einen Gl\u00fccklichen, wenn du es selbst nicht bist;", "tokens": ["Such", "ei\u00b7nen", "Gl\u00fcck\u00b7li\u00b7chen", ",", "wenn", "du", "es", "selbst", "nicht", "bist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei gl\u00fccklich da\u00df du siehst, da\u00df es ein andrer ist.", "tokens": ["Sei", "gl\u00fcck\u00b7lich", "da\u00df", "du", "siehst", ",", "da\u00df", "es", "ein", "an\u00b7drer", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Such auf Ungl\u00fcckliche, wenn du es w\u00e4hnst zu seyn,", "tokens": ["Such", "auf", "Un\u00b7gl\u00fcck\u00b7li\u00b7che", ",", "wenn", "du", "es", "w\u00e4hnst", "zu", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und es dich tr\u00f6sten mag, da\u00df du's nicht bist allein.", "tokens": ["Und", "es", "dich", "tr\u00f6s\u00b7ten", "mag", ",", "da\u00df", "du's", "nicht", "bist", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "VVINF", "VMFIN", "$,", "KOUS", "PIS", "PTKNEG", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Such einen auf, den du verstehst, der dich versteht;", "tokens": ["Such", "ei\u00b7nen", "auf", ",", "den", "du", "ver\u00b7stehst", ",", "der", "dich", "ver\u00b7steht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht, wenn's nur zum Ohr, wenn nicht zum Herzen, geht.", "tokens": ["Wo", "nicht", ",", "wenn's", "nur", "zum", "Ohr", ",", "wenn", "nicht", "zum", "Her\u00b7zen", ",", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "KOUS", "ADV", "APPRART", "NN", "$,", "KOUS", "PTKNEG", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Verst\u00f6ren wird ihn um so minder, was du klagst,", "tokens": ["Ver\u00b7st\u00f6\u00b7ren", "wird", "ihn", "um", "so", "min\u00b7der", ",", "was", "du", "klagst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ADV", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dich erleichterts wenn du dein Anliegen sagst.", "tokens": ["Und", "dich", "er\u00b7leich\u00b7terts", "wenn", "du", "dein", "An\u00b7lie\u00b7gen", "sagst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}