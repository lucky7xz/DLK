{"textgrid.poem.61175": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Einsiedler", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ward mir das Gew\u00fchle", "tokens": ["Wie", "ward", "mir", "das", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Welt doch gar zur Last!", "tokens": ["Der", "Welt", "doch", "gar", "zur", "Last", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es rauscht der Wald so k\u00fchle", "tokens": ["Es", "rauscht", "der", "Wald", "so", "k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lockt zu s\u00fc\u00dfer Rast.", "tokens": ["Und", "lockt", "zu", "s\u00fc\u00b7\u00dfer", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fahrt wohl denn, ihr Beschwerden,", "tokens": ["Fahrt", "wohl", "denn", ",", "ihr", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fahr wohl, o Lust der Erden!", "tokens": ["Fahr", "wohl", ",", "o", "Lust", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Siedler will ich werden,", "tokens": ["Ein", "Sied\u00b7ler", "will", "ich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Wildnis stiller Gast.", "tokens": ["Der", "Wild\u00b7nis", "stil\u00b7ler", "Gast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Wams von Purpursammet,", "tokens": ["Mein", "Wams", "von", "Pur\u00b7pur\u00b7sam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich mu\u00df dich von mir tun:", "tokens": ["Ich", "mu\u00df", "dich", "von", "mir", "tun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schwert, hast ausgeflammet,", "tokens": ["Mein", "Schwert", ",", "hast", "aus\u00b7ge\u00b7flam\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Grabscheit wirst du nun.", "tokens": ["Ein", "Grab\u00b7scheit", "wirst", "du", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fleuch auf, mein Falk, mit Schalle!", "tokens": ["Fleuch", "auf", ",", "mein", "Falk", ",", "mit", "Schal\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Trab' heim, mein Ro\u00df, zum Stalle!", "tokens": ["Trab'", "heim", ",", "mein", "Ro\u00df", ",", "zum", "Stal\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Goldsporn bricht, ich walle", "tokens": ["Der", "Gold\u00b7sporn", "bricht", ",", "ich", "wal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fortan auf Sandelschuhn.", "tokens": ["For\u00b7tan", "auf", "San\u00b7del\u00b7schuhn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich will ein Haus mir bauen", "tokens": ["Ich", "will", "ein", "Haus", "mir", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier zwischen Eich' und Tann'", "tokens": ["Hier", "zwi\u00b7schen", "Eich'", "und", "Tann'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus St\u00e4mmen unbehauen", "tokens": ["Aus", "St\u00e4m\u00b7men", "un\u00b7be\u00b7hau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Moos und Flechten dran:", "tokens": ["Mit", "Moos", "und", "Flech\u00b7ten", "dran", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Kreuzlein will ich schneiden", "tokens": ["Ein", "Kreuz\u00b7lein", "will", "ich", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus jenen H\u00e4ngeweiden", "tokens": ["Aus", "je\u00b7nen", "H\u00e4n\u00b7ge\u00b7wei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mich in Felle kleiden,", "tokens": ["Und", "mich", "in", "Fel\u00b7le", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie weiland Sankt Johann.", "tokens": ["Wie", "wei\u00b7land", "Sankt", "Jo\u00b7hann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im hohlen Baum die Waben,", "tokens": ["Im", "hoh\u00b7len", "Baum", "die", "Wa\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie reichen Honig dar;", "tokens": ["Sie", "rei\u00b7chen", "Ho\u00b7nig", "dar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach Wurzeln kann ich graben", "tokens": ["Nach", "Wur\u00b7zeln", "kann", "ich", "gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die l\u00e4ngste Zeit im Jahr;", "tokens": ["Die", "l\u00e4ngs\u00b7te", "Zeit", "im", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dort von fels'ger Schwelle", "tokens": ["Und", "dort", "von", "fels'\u00b7ger", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00fcpft braun herab die Quelle,", "tokens": ["H\u00fcpft", "braun", "her\u00b7ab", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie schimmert ihre Welle", "tokens": ["Wie", "schim\u00b7mert", "ih\u00b7re", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In hohler Hand so klar!", "tokens": ["In", "hoh\u00b7ler", "Hand", "so", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein G\u00e4rtlein soll umhegen", "tokens": ["Ein", "G\u00e4rt\u00b7lein", "soll", "um\u00b7he\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dunkle Siedelei,", "tokens": ["Die", "dunk\u00b7le", "Sie\u00b7de\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drin will ich Rosen pflegen", "tokens": ["Drin", "will", "ich", "Ro\u00b7sen", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Rosmarin dabei:", "tokens": ["Und", "Ros\u00b7ma\u00b7rin", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will aus dem Born sie tr\u00e4nken", "tokens": ["Will", "aus", "dem", "Born", "sie", "tr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und, wenn sie welk sich senken,", "tokens": ["Und", ",", "wenn", "sie", "welk", "sich", "sen\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Herzen still gedenken,", "tokens": ["Im", "Her\u00b7zen", "still", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df Lieb' ein Schatten sei.", "tokens": ["Da\u00df", "Lieb'", "ein", "Schat\u00b7ten", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und kommt zu meiner Zellen", "tokens": ["Und", "kommt", "zu", "mei\u00b7ner", "Zel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Reh die gr\u00fcne Bahn,", "tokens": ["Ein", "Reh", "die", "gr\u00fc\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das w\u00e4hl' ich zum Gesellen", "tokens": ["Das", "w\u00e4hl'", "ich", "zum", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und zieh' es treu heran.", "tokens": ["Und", "zieh'", "es", "treu", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf meinem Bett von Ranken", "tokens": ["Auf", "mei\u00b7nem", "Bett", "von", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da ruh' es seine Flanken;", "tokens": ["Da", "ruh'", "es", "sei\u00b7ne", "Flan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es wird mir besser danken,", "tokens": ["Es", "wird", "mir", "bes\u00b7ser", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als je ein Mensch getan.", "tokens": ["Als", "je", "ein", "Mensch", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So will ich Umgang pflegen", "tokens": ["So", "will", "ich", "Um\u00b7gang", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Rosen, Reh und Hain,", "tokens": ["Mit", "Ro\u00b7sen", ",", "Reh", "und", "Hain", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gegr\u00fc\u00dft auf meinen Wegen", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "auf", "mei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Sonnenstrahl allein;", "tokens": ["Vom", "Son\u00b7nen\u00b7strahl", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und jeden Abend treten", "tokens": ["Und", "je\u00b7den", "A\u00b7bend", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Will ich zum Kreuz und beten", "tokens": ["Will", "ich", "zum", "Kreuz", "und", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "\u00bbherr, nimm zu dir mich ein!\u00ab", "tokens": ["\u00bb", "herr", ",", "nimm", "zu", "dir", "mich", "ein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVIMP", "APPR", "PPER", "PRF", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und so mich Gott erh\u00f6ret,", "tokens": ["Und", "so", "mich", "Gott", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sei der Forst mein Grab,", "tokens": ["Da", "sei", "der", "Forst", "mein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo mich kein Reigen st\u00f6ret", "tokens": ["Wo", "mich", "kein", "Rei\u00b7gen", "st\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und keines Rosses Trab.", "tokens": ["Und", "kei\u00b7nes", "Ros\u00b7ses", "Trab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wildr\u00f6slein, rot und bleiche,", "tokens": ["Wild\u00b7r\u00f6s\u00b7lein", ",", "rot", "und", "blei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Bestatten fromm die Leiche,", "tokens": ["Be\u00b7stat\u00b7ten", "fromm", "die", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es singt von dunkler Eiche", "tokens": ["Es", "singt", "von", "dunk\u00b7ler", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Nachtigall herab.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}