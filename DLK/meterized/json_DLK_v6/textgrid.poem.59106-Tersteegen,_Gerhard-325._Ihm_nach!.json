{"textgrid.poem.59106": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "325. Ihm nach!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lern doch bedachtsam sein in allen;", "tokens": ["Lern", "doch", "be\u00b7dacht\u00b7sam", "sein", "in", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VAINF", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schnellen L\u00e4ufer leichtlich fallen!", "tokens": ["Die", "schnel\u00b7len", "L\u00e4u\u00b7fer", "leicht\u00b7lich", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Herzenskind auf Gott nur schaut", "tokens": ["Ein", "Her\u00b7zens\u00b7kind", "auf", "Gott", "nur", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geht ", "tokens": ["Und", "geht"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}}}}