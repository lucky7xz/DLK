{"textgrid.poem.36831": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn eures Neids und eurer niedern R\u00e4nke,", "tokens": ["Wenn", "eu\u00b7res", "Neids", "und", "eu\u00b7rer", "nie\u00b7dern", "R\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scheels\u00fccht'ge h\u00e4m'sche vaterl\u00e4nd'sche Feinde,", "tokens": ["Scheel\u00b7s\u00fccht'\u00b7ge", "h\u00e4m'\u00b7sche", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7sche", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zuweilen ich in meinem Lied gedenke,", "tokens": ["Zu\u00b7wei\u00b7len", "ich", "in", "mei\u00b7nem", "Lied", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So scheint's, da\u00df euer Ha\u00df auch mich verbittre,", "tokens": ["So", "scheint's", ",", "da\u00df", "eu\u00b7er", "Ha\u00df", "auch", "mich", "ver\u00b7bitt\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df jener S\u00fcmpfe Dunst, worin ihr r\u00f6chelt,", "tokens": ["Da\u00df", "je\u00b7ner", "S\u00fcmp\u00b7fe", "Dunst", ",", "wo\u00b7rin", "ihr", "r\u00f6\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst meine reine Inselluft durchwittre.", "tokens": ["Selbst", "mei\u00b7ne", "rei\u00b7ne", "In\u00b7sel\u00b7luft", "durch\u00b7witt\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch ist's nicht so: ich mu\u00df die Zeit verfluchen,", "tokens": ["Doch", "ist's", "nicht", "so", ":", "ich", "mu\u00df", "die", "Zeit", "ver\u00b7flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich gelernt, des Lebens Geist und W\u00fcrde", "tokens": ["Da", "ich", "ge\u00b7lernt", ",", "des", "Le\u00b7bens", "Geist", "und", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Freiheit ohne Schrank' und Maa\u00df zu suchen,", "tokens": ["In", "Frei\u00b7heit", "oh\u00b7ne", "Schrank'", "und", "Maa\u00df", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und jene nun den Furien heil'ge Kette", "tokens": ["Und", "je\u00b7ne", "nun", "den", "Fu\u00b7ri\u00b7en", "heil'\u00b7ge", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Lieb' und Irrthum, Ha\u00df, Vertrau'n und Frevel,", "tokens": ["Von", "Lieb'", "und", "Irr\u00b7thum", ",", "Ha\u00df", ",", "Ver\u00b7trau'n", "und", "Fre\u00b7vel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ich einst trug an deiner Richterst\u00e4tte,", "tokens": ["Die", "ich", "einst", "trug", "an", "dei\u00b7ner", "Rich\u00b7ter\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Befleckte Unschuld, oft mit ihrem Kummer,", "tokens": ["Be\u00b7fleck\u00b7te", "Un\u00b7schuld", ",", "oft", "mit", "ih\u00b7rem", "Kum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihrem Fluch und euern Namen kehret", "tokens": ["Mit", "ih\u00b7rem", "Fluch", "und", "eu\u00b7ern", "Na\u00b7men", "keh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wie ein Traum zur\u00fcck in wildem Schlummer.", "tokens": ["Sie", "wie", "ein", "Traum", "zu\u00b7r\u00fcck", "in", "wil\u00b7dem", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich k\u00e4mpfe mit den h\u00e4\u00dflich finstern Bildern,", "tokens": ["Ich", "k\u00e4mp\u00b7fe", "mit", "den", "h\u00e4\u00df\u00b7lich", "fins\u00b7tern", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich z\u00fcrn' und straf', und meines Liedes Weise", "tokens": ["Ich", "z\u00fcrn'", "und", "straf'", ",", "und", "mei\u00b7nes", "Lie\u00b7des", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beginnt sogleich auch wieder zu verwildern.", "tokens": ["Be\u00b7ginnt", "sog\u00b7leich", "auch", "wie\u00b7der", "zu", "ver\u00b7wil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch ich erwach', es fliehen die Gespenster,", "tokens": ["Doch", "ich", "er\u00b7wach'", ",", "es", "flie\u00b7hen", "die", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einer reinen Welt seh' ich mich wieder,", "tokens": ["In", "ei\u00b7ner", "rei\u00b7nen", "Welt", "seh'", "ich", "mich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der holde Tag lacht schon durchs Blumenfenster:", "tokens": ["Der", "hol\u00b7de", "Tag", "lacht", "schon", "durchs", "Blu\u00b7men\u00b7fens\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die frischen L\u00fcfte f\u00fchl' ich um mich wehen,", "tokens": ["Die", "fri\u00b7schen", "L\u00fcf\u00b7te", "f\u00fchl'", "ich", "um", "mich", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es gl\u00e4nzt das Meer, und in verj\u00fcngter Sch\u00f6ne", "tokens": ["Es", "gl\u00e4nzt", "das", "Meer", ",", "und", "in", "ver\u00b7j\u00fcng\u00b7ter", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh' ich den bessern Geist mir schon erstehen.", "tokens": ["Seh'", "ich", "den", "bes\u00b7sern", "Geist", "mir", "schon", "er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}