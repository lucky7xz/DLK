{"dta.poem.20564": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "22.  \n  Schlacht bei Bautzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Der Truppen F&#252;hrer haben es erwogen,", "tokens": ["Der", "Trup\u00b7pen", "F", "&#252;", "hrer", "ha\u00b7ben", "es", "er\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ADJA", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kriegsconstlium hat gezogen", "tokens": ["Ein", "Kriegs\u00b7const\u00b7li\u00b7um", "hat", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es in den reiflichsten Bedacht,", "tokens": ["Es", "in", "den", "reif\u00b7lichs\u00b7ten", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ob man auf\u2019s Neu\u2019 soll bieten Schlacht? \u2014", "tokens": ["Ob", "man", "auf's", "Neu'", "soll", "bie\u00b7ten", "Schlacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VMFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Klein ist das Heer der Bundeskrieger,", "tokens": ["Klein", "ist", "das", "Heer", "der", "Bun\u00b7des\u00b7krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Doppelten gebeut der Sieger", "tokens": ["Dem", "Dop\u00b7pel\u00b7ten", "ge\u00b7beut", "der", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von Jena und den Pyramiden. \u2014", "tokens": ["Von", "Je\u00b7na", "und", "den", "Py\u00b7ra\u00b7mi\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erw\u00e4gt zugleich man all\u2019 die m\u00fcden", "tokens": ["Er\u00b7w\u00e4gt", "zu\u00b7gleich", "man", "all'", "die", "m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIS", "PIS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Freiwill\u2019gen J\u00e4ger, schwach und kr\u00e4nklich,", "tokens": ["Frei\u00b7will'\u00b7gen", "J\u00e4\u00b7ger", ",", "schwach", "und", "kr\u00e4nk\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "So scheint zu bieten Schlacht bedenklich. \u2014", "tokens": ["So", "scheint", "zu", "bie\u00b7ten", "Schlacht", "be\u00b7denk\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Doch gilt\u2019s, das Aeu\u00dfersie zu wagen,", "tokens": ["Doch", "gilt's", ",", "das", "A\u00b7e\u00b7u\u00b7\u00dfer\u00b7sie", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Denn Deutschland krankt an bangem Zagen", "tokens": ["Denn", "Deutschland", "krankt", "an", "ban\u00b7gem", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und fallen kann\u2019s in Lethargie,", "tokens": ["Und", "fal\u00b7len", "kann's", "in", "Le\u00b7thar\u00b7gie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hilft baldigst nicht Allopathie. \u2014", "tokens": ["Hilft", "bal\u00b7digst", "nicht", "Al\u00b7lo\u00b7pa\u00b7thie", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Kriegsallopathen sind Kanonen! \u2014", "tokens": ["Kriegs\u00b7al\u00b7lo\u00b7pa\u00b7then", "sind", "Ka\u00b7no\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Nicht immer l\u00e4\u00dft sich ", "tokens": ["Nicht", "im\u00b7mer", "l\u00e4\u00dft", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PRF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Und oftmals schwindet die Gefahr,", "tokens": ["Und", "oft\u00b7mals", "schwin\u00b7det", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wenn man nur treu und muthig war.", "tokens": ["Wenn", "man", "nur", "treu", "und", "mut\u00b7hig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Stellt auf sich dicht bei Bautzens Thor,", "tokens": ["Stellt", "auf", "sich", "dicht", "bei", "Baut\u00b7zens", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Nah einem Felsen und der Spree. \u2014", "tokens": ["Nah", "ei\u00b7nem", "Fel\u00b7sen", "und", "der", "Spree", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der gr\u00f6\u00dfte Theil der Bund\u2019sarmee", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "Theil", "der", "Bun\u00b7d'\u00b7sar\u00b7mee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Stand bei Mehltheuer, Jenkowitz,", "tokens": ["Stand", "bei", "Mehl\u00b7theu\u00b7er", ",", "Jen\u00b7ko\u00b7witz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "NE", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.24": {"text": "Bei Kreckwitz und bei Plieskowitz. \u2014", "tokens": ["Bei", "Kreck\u00b7witz", "und", "bei", "Plies\u00b7ko\u00b7witz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nach Bautzen\u2019s linker Seit\u2019 gewandt", "tokens": ["Nach", "Baut\u00b7zen's", "lin\u00b7ker", "Seit'", "ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Emanuel, Jlowaisky stand", "tokens": ["E\u00b7ma\u00b7nu\u00b7el", ",", "Jlo\u00b7wais\u00b7ky", "stand"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und au\u00dferdem stand dort ", "tokens": ["Und", "au\u00b7\u00dfer\u00b7dem", "stand", "dort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "So wie nicht minder ", "tokens": ["So", "wie", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PTKNEG", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Man sah Kosacken und Baschkiren", "tokens": ["Man", "sah", "Ko\u00b7sa\u00b7cken", "und", "Baschki\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Am Spreestrand hin und her flankiren. \u2014", "tokens": ["Am", "Sprees\u00b7trand", "hin", "und", "her", "flan\u00b7ki\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.32": {"text": "Rechts von Stadt Bautzen stand ", "tokens": ["Rechts", "von", "Stadt", "Baut\u00b7zen", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NE", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.33": {"text": "Ein Mann von edlem, tapfern Geist", "tokens": ["Ein", "Mann", "von", "ed\u00b7lem", ",", "tap\u00b7fern", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Stand hinter Bautzen: Gen\u2019ral ", "tokens": ["Stand", "hin\u00b7ter", "Baut\u00b7zen", ":", "Gen'\u00b7ral"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "NN", "$.", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Befehl\u2019gend ", "tokens": ["Be\u00b7fehl'\u00b7gend"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Und Gen\u2019ral ", "tokens": ["Und", "Gen'\u00b7ral"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Jm linken Fl\u00fcgel an der ", "tokens": ["Jm", "lin\u00b7ken", "Fl\u00fc\u00b7gel", "an", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Der Gen\u2019ral ", "tokens": ["Der", "Gen'\u00b7ral"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Links, nah dem Centrum, da stand ", "tokens": ["Links", ",", "nah", "dem", "Cen\u00b7trum", ",", "da", "stand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJD", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.40": {"text": "Er war von Eisen, nicht von Kork;", "tokens": ["Er", "war", "von", "Ei\u00b7sen", ",", "nicht", "von", "Kork", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "PTKNEG", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Besetzt, verschanzt hielt er Basch\u00fctz,", "tokens": ["Be\u00b7setzt", ",", "ver\u00b7schanzt", "hielt", "er", "Ba\u00b7sch\u00fctz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Nicht minder auch Gro\u00df-Jenkowitz.", "tokens": ["Nicht", "min\u00b7der", "auch", "Gro\u00df\u00b7Jen\u00b7ko\u00b7witz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "In Kriegsspielkarten war er Trumpf!", "tokens": ["In", "Kriegs\u00b7spiel\u00b7kar\u00b7ten", "war", "er", "Trumpf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Vor ihm lag Bl\u00f6sa\u2019s tiefer Sumpf. \u2014", "tokens": ["Vor", "ihm", "lag", "Bl\u00f6sa's", "tie\u00b7fer", "Sumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Bl&#252;cher, ich denk': auf seine Bitte,", "tokens": ["Bl", "&#252;", "cher", ",", "ich", "denk'", ":", "auf", "sei\u00b7ne", "Bit\u00b7te", ","], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$,", "PPER", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Stand rechts, und zwar zun\u00e4chst der Mitte.", "tokens": ["Stand", "rechts", ",", "und", "zwar", "zu\u00b7n\u00e4chst", "der", "Mit\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Man konnt\u2019 der Spree entlang ihn sehn", "tokens": ["Man", "konnt'", "der", "Spree", "ent\u00b7lang", "ihn", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Von Gleina bis auf Kreckwitz-H\u00f6h\u2019n. \u2014", "tokens": ["Von", "Glei\u00b7na", "bis", "auf", "Kreck\u00b7witz\u00b7H\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["APPR", "NE", "ADV", "APPR", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Der K\u00fcnste Freund, wie des Gefechts,", "tokens": ["Der", "K\u00fcns\u00b7te", "Freund", ",", "wie", "des", "Ge\u00b7fechts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Barclay de Tolly stand ganz rechts.", "tokens": ["Bar\u00b7clay", "de", "Tol\u00b7ly", "stand", "ganz", "rechts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "ADV", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.51": {"text": "Viel Waldung lag vor ihm und Spree.", "tokens": ["Viel", "Wal\u00b7dung", "lag", "vor", "ihm", "und", "Spree", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Es trennt von ", "tokens": ["Es", "trennt", "von"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.53": {"text": "Wohl mancher breit\u2019 und tiefe Teich,", "tokens": ["Wohl", "man\u00b7cher", "breit'", "und", "tie\u00b7fe", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "An solchen ist dort Gegend reich.", "tokens": ["An", "sol\u00b7chen", "ist", "dort", "Ge\u00b7gend", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VAFIN", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Reserv\u2019, die ", "tokens": ["Re\u00b7ser\u00b7v'", ",", "die"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PRELS"], "meter": "+-+-", "measure": "trochaic.di"}, "line.56": {"text": "Held ", "tokens": ["Held"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.57": {"text": "Er, Gro\u00dff\u00fcrst, stand bei Klein-Purschwitz", "tokens": ["Er", ",", "Gro\u00df\u00b7f\u00fcrst", ",", "stand", "bei", "Klein\u00b7Pur\u00b7schwitz"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Und au\u00dferdem auch bei Kubsch\u00fctz.", "tokens": ["Und", "au\u00b7\u00dfer\u00b7dem", "auch", "bei", "Kub\u00b7sch\u00fctz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.59": {"text": "Er weilt dort des Succurses wegen,", "tokens": ["Er", "weilt", "dort