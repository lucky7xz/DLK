{"textgrid.poem.48997": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du gl\u00fccklichster von allen V\u00f6geln,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gl\u00fccklichster von allen V\u00f6geln,", "tokens": ["Du", "gl\u00fcck\u00b7lichs\u00b7ter", "von", "al\u00b7len", "V\u00f6\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kleines, liebes Sp\u00e4tzchen!", "tokens": ["Klei\u00b7nes", ",", "lie\u00b7bes", "Sp\u00e4tz\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Du nist'st an meiner Cloe Fenster.", "tokens": ["Du", "nist'st", "an", "mei\u00b7ner", "Cloe", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Viel gl\u00fccklicher als der Adler bist du,", "tokens": ["Viel", "gl\u00fcck\u00b7li\u00b7cher", "als", "der", "Ad\u00b7ler", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+--+-++", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dem rote Morgensonne sein Bett vergoldet.", "tokens": ["Dem", "ro\u00b7te", "Mor\u00b7gen\u00b7son\u00b7ne", "sein", "Bett", "ver\u00b7gol\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Denn ach! Du weckst sie, die Sch\u00f6ne, aus dem Schlummer,", "tokens": ["Denn", "ach", "!", "Du", "weckst", "sie", ",", "die", "Sch\u00f6\u00b7ne", ",", "aus", "dem", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Du siehst sie aus dem Bett steigen", "tokens": ["Du", "siehst", "sie", "aus", "dem", "Bett", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Und h\u00fcpfst mit deinen Jungen hervor.", "tokens": ["Und", "h\u00fcpfst", "mit", "dei\u00b7nen", "Jun\u00b7gen", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "O dann \u00f6ffnet sie selbst das Fenster", "tokens": ["O", "dann", "\u00f6ff\u00b7net", "sie", "selbst", "das", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und \u00e4tzt mit ihrer lilienwei\u00dfen Hand", "tokens": ["Und", "\u00e4tzt", "mit", "ih\u00b7rer", "li\u00b7li\u00b7en\u00b7wei\u00b7\u00dfen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Dich kleinen S\u00e4nger noch.", "tokens": ["Dich", "klei\u00b7nen", "S\u00e4n\u00b7ger", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}