{"textgrid.poem.41094": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An Helene v. Moltke", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trete als der Erste hier in Dein Heiligtum,", "tokens": ["Ich", "tre\u00b7te", "als", "der", "Ers\u00b7te", "hier", "in", "Dein", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Umher lausch' ich, doch alles ist in ihm still und stumm,", "tokens": ["Um\u00b7her", "lausch'", "ich", ",", "doch", "al\u00b7les", "ist", "in", "ihm", "still", "und", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "PIS", "VAFIN", "APPR", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Freunde z\u00f6gern, keiner ist noch herbeigekommen,", "tokens": ["Die", "Freun\u00b7de", "z\u00f6\u00b7gern", ",", "kei\u00b7ner", "ist", "noch", "her\u00b7bei\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PIS", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hat in dem goldbekr\u00e4nzten Salone Platz genommen.", "tokens": ["Hat", "in", "dem", "gold\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Sa\u00b7lo\u00b7ne", "Platz", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Verlegen steh' ich einsam. Was soll ich Armer sagen,", "tokens": ["Ver\u00b7le\u00b7gen", "steh'", "ich", "ein\u00b7sam", ".", "Was", "soll", "ich", "Ar\u00b7mer", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$.", "PWS", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Da\u00df ich so fr\u00fch gekommen, \u2013 dazu in keinem Wagen,", "tokens": ["Da\u00df", "ich", "so", "fr\u00fch", "ge\u00b7kom\u00b7men", ",", "\u2013", "da\u00b7zu", "in", "kei\u00b7nem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "$,", "$(", "PAV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Zu Fu\u00df an einem Stocke, in Stiefeln sehr befleckt,", "tokens": ["Zu", "Fu\u00df", "an", "ei\u00b7nem", "Sto\u00b7cke", ",", "in", "Stie\u00b7feln", "sehr", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "In einem \u00dcberrocke, ganz gegen den Respekt? \u2013", "tokens": ["In", "ei\u00b7nem", "\u00dc\u00b7berr\u00b7o\u00b7cke", ",", "ganz", "ge\u00b7gen", "den", "Res\u00b7pekt", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "W\u00e4r' nach der bunten Menge gekommen ich zuletzt,", "tokens": ["W\u00e4r'", "nach", "der", "bun\u00b7ten", "Men\u00b7ge", "ge\u00b7kom\u00b7men", "ich", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Dann h\u00e4tt' ich dort ins Ecke mich unbemerkt gesetzt.", "tokens": ["Dann", "h\u00e4tt'", "ich", "dort", "ins", "E\u00b7cke", "mich", "un\u00b7be\u00b7merkt", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Nun steh' ich hart am Tore, wo alles mich ersieht,", "tokens": ["Nun", "steh'", "ich", "hart", "am", "To\u00b7re", ",", "wo", "al\u00b7les", "mich", "er\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Nicht kann ich mich verstecken, so sehr ich bin bem\u00fcht.", "tokens": ["Nicht", "kann", "ich", "mich", "ver\u00b7ste\u00b7cken", ",", "so", "sehr", "ich", "bin", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "PRF", "VVINF", "$,", "ADV", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Was ist zu tun? \u2013 o Fr\u00e4ulein! von Herzen bitt' ich Dich:", "tokens": ["Was", "ist", "zu", "tun", "?", "\u2013", "o", "Fr\u00e4u\u00b7lein", "!", "von", "Her\u00b7zen", "bitt'", "ich", "Dich", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "$.", "$(", "FM", "NN", "$.", "APPR", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Werf doch in Deiner G\u00fcte jetzt Deinen Schal auf mich!", "tokens": ["Werf", "doch", "in", "Dei\u00b7ner", "G\u00fc\u00b7te", "jetzt", "Dei\u00b7nen", "Schal", "auf", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "La\u00df unter ihm mich bleiben, ganz unbemerkt im stillen,", "tokens": ["La\u00df", "un\u00b7ter", "ihm", "mich", "blei\u00b7ben", ",", "ganz", "un\u00b7be\u00b7merkt", "im", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "PRF", "VVINF", "$,", "ADV", "ADJD", "APPRART", "ADJA", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Bis da\u00df sich mit der Menge die leeren R\u00e4ume f\u00fcllen.", "tokens": ["Bis", "da\u00df", "sich", "mit", "der", "Men\u00b7ge", "die", "lee\u00b7ren", "R\u00e4u\u00b7me", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Ha! unter eines solchen herzlieben Tuches Hut", "tokens": ["Ha", "!", "un\u00b7ter", "ei\u00b7nes", "sol\u00b7chen", "herz\u00b7lie\u00b7ben", "Tu\u00b7ches", "Hut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ART", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Wallt auf in frischer Jugend ein noch so altes Blut.", "tokens": ["Wallt", "auf", "in", "fri\u00b7scher", "Ju\u00b7gend", "ein", "noch", "so", "al\u00b7tes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Dann w\u00e4hrend hier die andern gar zierlich malen, reimen,", "tokens": ["Dann", "w\u00e4h\u00b7rend", "hier", "die", "an\u00b7dern", "gar", "zier\u00b7lich", "ma\u00b7len", ",", "rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "ART", "ADJA", "ADV", "ADJD", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Will unter dem Verstecke ich selig von Dir tr\u00e4umen.", "tokens": ["Will", "un\u00b7ter", "dem", "Ver\u00b7ste\u00b7cke", "ich", "se\u00b7lig", "von", "Dir", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Bald schau' ich Dich zu Pferde wohl als verwegne Fee,", "tokens": ["Bald", "schau'", "ich", "Dich", "zu", "Pfer\u00b7de", "wohl", "als", "ver\u00b7weg\u00b7ne", "Fee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "ADV", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Bald wieder als ein Lichtbild zu Ulm auf M\u00fcnsters H\u00f6h'!", "tokens": ["Bald", "wie\u00b7der", "als", "ein", "Licht\u00b7bild", "zu", "Ulm", "auf", "M\u00fcns\u00b7ters", "H\u00f6h'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ART", "NN", "APPR", "NE", "APPR", "NE", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Dann wieder schau' ich wandeln in T\u00e4lern Dich der Schweiz,", "tokens": ["Dann", "wie\u00b7der", "schau'", "ich", "wan\u00b7deln", "in", "T\u00e4\u00b7lern", "Dich", "der", "Schweiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "PPER", "ART", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Kind der Natur voll Einfalt und holdem Liebesreiz,", "tokens": ["Kind", "der", "Na\u00b7tur", "voll", "Ein\u00b7falt", "und", "hol\u00b7dem", "Lie\u00b7bes\u00b7reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Dann unter jenem Himmel Italiens blau und klar,", "tokens": ["Dann", "un\u00b7ter", "je\u00b7nem", "Him\u00b7mel", "I\u00b7ta\u00b7li\u00b7ens", "blau", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Aus dem Du Dir geholet Dein sch\u00f6nes Augenpaar.", "tokens": ["Aus", "dem", "Du", "Dir", "ge\u00b7ho\u00b7let", "Dein", "sch\u00f6\u00b7nes", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Zuletzt werd' ich Dir schauen ins liebe Herz hinein,", "tokens": ["Zu\u00b7letzt", "werd'", "ich", "Dir", "schau\u00b7en", "ins", "lie\u00b7be", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Drin wird es wie im Kelche der wei\u00dfen Lilie sein.", "tokens": ["Drin", "wird", "es", "wie", "im", "Kel\u00b7che", "der", "wei\u00b7\u00dfen", "Li\u00b7lie", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "APPRART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Das alles la\u00df mich tr\u00e4umen da unter Deinem Tuch,", "tokens": ["Das", "al\u00b7les", "la\u00df", "mich", "tr\u00e4u\u00b7men", "da", "un\u00b7ter", "Dei\u00b7nem", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVIMP", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Bis sie gef\u00fcllt mit Reimen das liebe, dicke Buch.", "tokens": ["Bis", "sie", "ge\u00b7f\u00fcllt", "mit", "Rei\u00b7men", "das", "lie\u00b7be", ",", "di\u00b7cke", "Buch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}