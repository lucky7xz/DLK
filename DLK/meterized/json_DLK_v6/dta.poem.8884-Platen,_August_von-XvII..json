{"dta.poem.8884": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "War unsre Liebe, nicht in Staub versunken:", "tokens": ["War", "uns\u00b7re", "Lie\u00b7be", ",", "nicht", "in", "Staub", "ver\u00b7sun\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur deiner Sch\u00f6nheit bebt' ich wonnetrunken,", "tokens": ["Nur", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "bebt'", "ich", "won\u00b7ne\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und g\u00fctig warst du, gleich den Engeln oben.", "tokens": ["Und", "g\u00fc\u00b7tig", "warst", "du", ",", "gleich", "den", "En\u00b7geln", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du hattest mich zu dir emporgehoben,", "tokens": ["Du", "hat\u00b7test", "mich", "zu", "dir", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deinem Auge schwamm ein lichter Funken,", "tokens": ["In", "dei\u00b7nem", "Au\u00b7ge", "schwamm", "ein", "lich\u00b7ter", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Farben schuf, den Pinsel d'rein zu tunken,", "tokens": ["Der", "Far\u00b7ben", "schuf", ",", "den", "Pin\u00b7sel", "d'\u00b7rein", "zu", "tun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den reine Dichterh\u00e4nde Gott geloben.", "tokens": ["Den", "rei\u00b7ne", "Dich\u00b7ter\u00b7h\u00e4n\u00b7de", "Gott", "ge\u00b7lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun, da ich fern von dir den Tag verbringe,", "tokens": ["Nun", ",", "da", "ich", "fern", "von", "dir", "den", "Tag", "ver\u00b7brin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erscheinst du der Bewunderung noch reiner,", "tokens": ["Er\u00b7scheinst", "du", "der", "Be\u00b7wun\u00b7de\u00b7rung", "noch", "rei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Je mehr im Geist ich deinen Werth durchdringe.", "tokens": ["Je", "mehr", "im", "Geist", "ich", "dei\u00b7nen", "Werth", "durch\u00b7drin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, immer sehnsuchtsvoller denk' ich deiner,", "tokens": ["Ja", ",", "im\u00b7mer", "sehn\u00b7suchts\u00b7vol\u00b7ler", "denk'", "ich", "dei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und legt die Welt mir auch so manche Schlinge,", "tokens": ["Und", "legt", "die", "Welt", "mir", "auch", "so", "man\u00b7che", "Schlin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst mich nie gefangen sehn in einer.", "tokens": ["Du", "sollst", "mich", "nie", "ge\u00b7fan\u00b7gen", "sehn", "in", "ei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "APPR", "ART", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}