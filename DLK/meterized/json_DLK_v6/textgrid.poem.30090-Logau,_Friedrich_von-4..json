{"textgrid.poem.30090": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guter Wein verterbt den Beutel, b\u00f6ser schadet sehr dem Magen;", "tokens": ["Gu\u00b7ter", "Wein", "ver\u00b7terbt", "den", "Beu\u00b7tel", ",", "b\u00f6\u00b7ser", "scha\u00b7det", "sehr", "dem", "Ma\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Besser aber ist den Beutel, als den guten Magen plagen.", "tokens": ["Bes\u00b7ser", "a\u00b7ber", "ist", "den", "Beu\u00b7tel", ",", "als", "den", "gu\u00b7ten", "Ma\u00b7gen", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}