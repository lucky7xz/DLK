{"dta.poem.12088": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stand auf einem Berg und sah die Sonn' aufgehn,", "tokens": ["Ich", "stand", "auf", "ei\u00b7nem", "Berg", "und", "sah", "die", "Sonn'", "auf\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Berg schien inselgleich in einem Meer zu stehn.", "tokens": ["Der", "Berg", "schien", "in\u00b7sel\u00b7gleich", "in", "ei\u00b7nem", "Meer", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Morgennebel war durch jedes Thal ergossen,", "tokens": ["Denn", "Mor\u00b7gen\u00b7ne\u00b7bel", "war", "durch", "je\u00b7des", "Thal", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und alle Seen umher in Einen See zerflossen.", "tokens": ["Und", "al\u00b7le", "Seen", "um\u00b7her", "in", "Ei\u00b7nen", "See", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKVZ", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was wahres Wasser sei, was blo\u00dfer Wasserdunst,", "tokens": ["Was", "wah\u00b7res", "Was\u00b7ser", "sei", ",", "was", "blo\u00b7\u00dfer", "Was\u00b7ser\u00b7dunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "$,", "PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu unterscheiden klar vermochte keine Kunst.", "tokens": ["Zu", "un\u00b7ter\u00b7schei\u00b7den", "klar", "ver\u00b7moch\u00b7te", "kei\u00b7ne", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch als die Sonne stieg, ward es von selber klar,", "tokens": ["Doch", "als", "die", "Son\u00b7ne", "stieg", ",", "ward", "es", "von", "sel\u00b7ber", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was nur ein Wasserschein, was wirklich Wasser war.", "tokens": ["Was", "nur", "ein", "Was\u00b7ser\u00b7schein", ",", "was", "wirk\u00b7lich", "Was\u00b7ser", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Nebelh\u00fclle schwand, entschleiernd das Gefild,", "tokens": ["Die", "Ne\u00b7bel\u00b7h\u00fcl\u00b7le", "schwand", ",", "ent\u00b7schlei\u00b7ernd", "das", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Se'en spiegelten voll Glanz der Sonne Bild.", "tokens": ["Die", "Se'\u00b7en", "spie\u00b7gel\u00b7ten", "voll", "Glanz", "der", "Son\u00b7ne", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}