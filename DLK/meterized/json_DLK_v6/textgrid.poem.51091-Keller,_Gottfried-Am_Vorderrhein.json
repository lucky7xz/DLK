{"textgrid.poem.51091": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Am Vorderrhein", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ahnungsvoll er ausgezogen,", "tokens": ["Wie", "ah\u00b7nungs\u00b7voll", "er", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der junge Held, aus Kluft und Stein!", "tokens": ["Der", "jun\u00b7ge", "Held", ",", "aus", "Kluft", "und", "Stein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat er durstig eingesogen", "tokens": ["Wie", "hat", "er", "durs\u00b7tig", "ein\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Milch des Berges, frisch und rein!", "tokens": ["Die", "Milch", "des", "Ber\u00b7ges", ",", "frisch", "und", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun wallt der Hirtensohn hernieder,", "tokens": ["Nun", "wallt", "der", "Hir\u00b7ten\u00b7sohn", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hin in mein zweites Heimatland:", "tokens": ["Hin", "in", "mein", "zwei\u00b7tes", "Hei\u00b7mat\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O gr\u00fc\u00df mir all die deutschen Br\u00fcder,", "tokens": ["O", "gr\u00fc\u00df", "mir", "all", "die", "deut\u00b7schen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die herrlichen, l\u00e4ngs deinem Strand!", "tokens": ["Die", "herr\u00b7li\u00b7chen", ",", "l\u00e4ngs", "dei\u00b7nem", "Strand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So gr\u00fc\u00df auch all die deutschen Frauen", "tokens": ["So", "gr\u00fc\u00df", "auch", "all", "die", "deut\u00b7schen", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lerne ritterlichen Brauch;", "tokens": ["Und", "ler\u00b7ne", "rit\u00b7ter\u00b7li\u00b7chen", "Brauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn du wirst die Dome schauen,", "tokens": ["Und", "wenn", "du", "wirst", "die", "Do\u00b7me", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die krausen K\u00e4uze, gr\u00fc\u00df sie auch!", "tokens": ["Die", "krau\u00b7sen", "K\u00e4u\u00b7ze", ",", "gr\u00fc\u00df", "sie", "auch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst w\u00fc\u00dft ich niemand just zu gr\u00fc\u00dfen,", "tokens": ["Sonst", "w\u00fc\u00dft", "ich", "nie\u00b7mand", "just", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vielleicht die schlimme Lorelei", "tokens": ["Viel\u00b7leicht", "die", "schlim\u00b7me", "Lo\u00b7re\u00b7lei"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und deiner Reben freudig Sprie\u00dfen \u2013", "tokens": ["Und", "dei\u00b7ner", "Re\u00b7ben", "freu\u00b7dig", "Sprie\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Vierzigen geh still vorbei!", "tokens": ["Den", "Vier\u00b7zi\u00b7gen", "geh", "still", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es taucht ein Aar ins Wolkenlose", "tokens": ["Es", "taucht", "ein", "Aar", "ins", "Wol\u00b7ken\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch \u00fcber mir im Sonnenschein;", "tokens": ["Hoch", "\u00fc\u00b7ber", "mir", "im", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich werfe eine Alpenrose", "tokens": ["Ich", "wer\u00b7fe", "ei\u00b7ne", "Al\u00b7pen\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tief unten in den wilden Rhein:", "tokens": ["Tief", "un\u00b7ten", "in", "den", "wil\u00b7den", "Rhein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fchr nieder sie, f\u00fchr sie zu Tale,", "tokens": ["F\u00fchr", "nie\u00b7der", "sie", ",", "f\u00fchr", "sie", "zu", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "$,", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und eh du trittst zum Meerestor,", "tokens": ["Und", "eh", "du", "trittst", "zum", "Mee\u00b7res\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Vettern halt, im Eichensaale,", "tokens": ["Den", "Vet\u00b7tern", "halt", ",", "im", "Ei\u00b7chen\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den harrenden, dies Zeichen vor!", "tokens": ["Den", "har\u00b7ren\u00b7den", ",", "dies", "Zei\u00b7chen", "vor", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PDS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}