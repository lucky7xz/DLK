{"textgrid.poem.63424": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Achter in't Holt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dat is so still hier achter,", "tokens": ["Dat", "is", "so", "still", "hier", "ach\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "ADJD", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "as wir 'ne Kirch dat Holt,", "tokens": ["as", "wir", "'ne", "Kirch", "dat", "Holt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit v\u00e4l wid Bagenfinster", "tokens": ["mit", "v\u00e4l", "wid", "Ba\u00b7gen\u00b7fins\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "un Pilers, hoch un olt.", "tokens": ["un", "Pi\u00b7lers", ",", "hoch", "un", "olt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "ADJD", "FM", "FM", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dat is so still nu worren;", "tokens": ["Dat", "is", "so", "still", "nu", "wor\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "de Wold, de steiht un swiggt,", "tokens": ["de", "Wold", ",", "de", "steiht", "un", "swiggt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "as wenn dor vun sin Kanzel", "tokens": ["as", "wenn", "dor", "vun", "sin", "Kan\u00b7zel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "de Preester r\u00fcnner stiggt;", "tokens": ["de", "Prees\u00b7ter", "r\u00fcn\u00b7ner", "stiggt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "as wenn dor v\u00f6r'n Altor", "tokens": ["as", "wenn", "dor", "v\u00f6r'n", "Al\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "de Preester steiht un singt", "tokens": ["de", "Prees\u00b7ter", "steiht", "un", "singt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "un de Gemeen den S\u00e4gen", "tokens": ["un", "de", "Ge\u00b7meen", "den", "S\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "un ehr den Fr\u00e4den bringt.", "tokens": ["un", "ehr", "den", "Fr\u00e4\u00b7den", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Dat is de s\u00f6te Vagel,", "tokens": ["Dat", "is", "de", "s\u00f6\u00b7te", "Va\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "de Nachtigall is dat \u2013", "tokens": ["de", "Nach\u00b7ti\u00b7gall", "is", "dat", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dat sleiht een'n so to Harten,", "tokens": ["dat", "sleiht", "e\u00b7en'n", "so", "to", "Har\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "dat Og dat ward di natt;", "tokens": ["dat", "Og", "dat", "ward", "di", "natt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "NE", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "as k\u00fcnnst du af dat l\u00fcchten", "tokens": ["as", "k\u00fcnnst", "du", "af", "dat", "l\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "NE", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "all, wat up't Hart di liggt,", "tokens": ["all", ",", "wat", "up't", "Hart", "di", "liggt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "as ob een gahn un bichten", "tokens": ["as", "ob", "e\u00b7en", "gahn", "un", "bich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "un still eens baden m\u00fccht.", "tokens": ["un", "still", "e\u00b7ens", "ba\u00b7den", "m\u00fccht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "De hogen ollen Eeken", "tokens": ["De", "ho\u00b7gen", "ol\u00b7len", "E\u00b7e\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "de geiht dat d\u00f6rche Kron,", "tokens": ["de", "geiht", "dat", "d\u00f6r\u00b7che", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "de hogen ollen Eeken,", "tokens": ["de", "ho\u00b7gen", "ol\u00b7len", "E\u00b7e\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "as Chur un \u00d6rgelton.", "tokens": ["as", "Chur", "un", "\u00d6r\u00b7gel\u00b7ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}