{"textgrid.poem.64033": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Brennende Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brennende Einsamkeit", "tokens": ["Bren\u00b7nen\u00b7de", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Schreit,", "tokens": ["Schreit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Gestalten kommen hervor, wo V\u00f6lker modern,", "tokens": ["Ge\u00b7stal\u00b7ten", "kom\u00b7men", "her\u00b7vor", ",", "wo", "V\u00f6l\u00b7ker", "mo\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Winkend die Fackeln der Himmel lodern,", "tokens": ["Win\u00b7kend", "die", "Fa\u00b7ckeln", "der", "Him\u00b7mel", "lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und da ich noch suche die Weite,", "tokens": ["Und", "da", "ich", "noch", "su\u00b7che", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.6": {"text": "So schmiegt es sich mir an die Seite", "tokens": ["So", "schmiegt", "es", "sich", "mir", "an", "die", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und lacht mir so nah' mit lebendigen Sternen,", "tokens": ["Und", "lacht", "mir", "so", "nah'", "mit", "le\u00b7ben\u00b7di\u00b7gen", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wie du sie nicht fandest in m\u00fcdesten Fernen.", "tokens": ["Wie", "du", "sie", "nicht", "fan\u00b7dest", "in", "m\u00fc\u00b7des\u00b7ten", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}