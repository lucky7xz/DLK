{"dta.poem.19027": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Xi.   Angenehme Sterbens-Gedancken.   Jm Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00e4ume bl\u00fchen ab,", "tokens": ["Die", "B\u00e4u\u00b7me", "bl\u00fc\u00b7hen", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Bl\u00e4tter st\u00fcrtzen:", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "st\u00fcrt\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mir wird das liebe Grab", "tokens": ["Mir", "wird", "das", "lie\u00b7be", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Elend k\u00fcrtzen.", "tokens": ["Mein", "E\u00b7lend", "k\u00fcrt\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Getrost, ich sehe schon", "tokens": ["Ge\u00b7trost", ",", "ich", "se\u00b7he", "schon"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das B\u00e4umlein bl\u00fchen,", "tokens": ["Das", "B\u00e4um\u00b7lein", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und meines Leibes Thon", "tokens": ["Und", "mei\u00b7nes", "Lei\u00b7bes", "Thon"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gerader ziehen.", "tokens": ["Ge\u00b7ra\u00b7der", "zie\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mein Grab-Stein springt entzwey,", "tokens": ["Mein", "Grab\u00b7Stein", "springt", "ent\u00b7zwey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der Schlaf vergehet:", "tokens": ["Der", "Schlaf", "ver\u00b7ge\u00b7het", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Leib wird Kercker-frey,", "tokens": ["Der", "Leib", "wird", "Ker\u00b7cker\u00b7frey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Tod verwehet.", "tokens": ["Mein", "Tod", "ver\u00b7we\u00b7het", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Faulni\u00df finstre Baar,", "tokens": ["Der", "Faul\u00b7ni\u00df", "finst\u00b7re", "Baar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und die Verwesung,", "tokens": ["Und", "die", "Ver\u00b7we\u00b7sung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Verliert sich gantz und gar", "tokens": ["Ver\u00b7liert", "sich", "gantz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In der Genesung.", "tokens": ["In", "der", "Ge\u00b7ne\u00b7sung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Der Sturm, der unsern Geist", "tokens": ["Der", "Sturm", ",", "der", "un\u00b7sern", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vom Leibe treibet,", "tokens": ["Vom", "Lei\u00b7be", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und uns von hinnen rei\u00dft,", "tokens": ["Und", "uns", "von", "hin\u00b7nen", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat ausgest\u00e4ubet.", "tokens": ["Hat", "aus\u00b7ge\u00b7st\u00e4u\u00b7bet", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Man h\u00f6ret ferner nicht", "tokens": ["Man", "h\u00f6\u00b7ret", "fer\u00b7ner", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Windes Brausen:", "tokens": ["Des", "Win\u00b7des", "Brau\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Man sp\u00fcrt im stillen Licht", "tokens": ["Man", "sp\u00fcrt", "im", "stil\u00b7len", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein lieblich Sausen.", "tokens": ["Ein", "lieb\u00b7lich", "Sau\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ein Wind von Jehovah", "tokens": ["Ein", "Wind", "von", "Je\u00b7ho\u00b7vah"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Wird ausgeblasen:", "tokens": ["Wird", "aus\u00b7ge\u00b7bla\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Beine liegen da", "tokens": ["Die", "Bei\u00b7ne", "lie\u00b7gen", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In gr\u00fcnen Rasen.", "tokens": ["In", "gr\u00fc\u00b7nen", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Auf Hoffnung liegen sie", "tokens": ["Auf", "Hoff\u00b7nung", "lie\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Aufferstehung,", "tokens": ["Der", "Auf\u00b7fer\u00b7ste\u00b7hung", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und warten spat und fr\u00fch", "tokens": ["Und", "war\u00b7ten", "spat", "und", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Stands-Erh\u00f6hung.", "tokens": ["Der", "Stands\u00b7Er\u00b7h\u00f6\u00b7hung", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Jhr seyd zu Staub verbrand,", "tokens": ["Ihr", "seyd", "zu", "Staub", "ver\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jhr kahlen Beine,", "tokens": ["Ihr", "kah\u00b7len", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ist wunder kleine.", "tokens": ["Ist", "wun\u00b7der", "klei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Jhr seyd fast aufgeleckt,", "tokens": ["Ihr", "seyd", "fast", "auf\u00b7ge\u00b7leckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jhr Aschen-Haufen:", "tokens": ["Ihr", "A\u00b7schen\u00b7Hau\u00b7fen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Tiefe, die euch deckt,", "tokens": ["Die", "Tie\u00b7fe", ",", "die", "euch", "deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist angelaufen.", "tokens": ["Ist", "an\u00b7ge\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Jhr seyd aufs Feld ges\u00e4t", "tokens": ["Ihr", "seyd", "aufs", "Feld", "ge\u00b7s\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie Aeser-Knochen,", "tokens": ["Wie", "A\u00b7e\u00b7ser\u00b7Kno\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und in die Lufft verweht,", "tokens": ["Und", "in", "die", "Lufft", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zerqvetscht, zerbrochen.", "tokens": ["Zer\u00b7qvetscht", ",", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Die hat des Abgrunds Wut", "tokens": ["Die", "hat", "des", "Ab\u00b7grunds", "Wut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durchaus zerw\u00fchlet,", "tokens": ["Durc\u00b7haus", "zer\u00b7w\u00fch\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die eine schnelle Fluth", "tokens": ["Die", "ei\u00b7ne", "schnel\u00b7le", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hinweg gesp\u00fclet.", "tokens": ["Hin\u00b7weg", "ge\u00b7sp\u00fc\u00b7let", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Jhr wi\u00dft nicht, hie und d", "tokens": ["Ihr", "wi\u00dft", "nicht", ",", "hie", "und", "d"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "ADV", "KON", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verstreute Glieder!", "tokens": ["Ver\u00b7streu\u00b7te", "Glie\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie euch das Wort so nah,", "tokens": ["Wie", "euch", "das", "Wort", "so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt rufst es wieder.", "tokens": ["Jetzt", "rufst", "es", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Der Mann, in welchem es", "tokens": ["Der", "Mann", ",", "in", "wel\u00b7chem", "es"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Beschlossen ware,", "tokens": ["Be\u00b7schlos\u00b7sen", "wa\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der kommt mit Lob-Get\u00f6\u00df", "tokens": ["Der", "kommt", "mit", "Lob\u00b7Ge\u00b7t\u00f6\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Helden-Schaare.", "tokens": ["Der", "Hel\u00b7den\u00b7Schaa\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Man thut die B\u00fccher auf,", "tokens": ["Man", "thut", "die", "B\u00fc\u00b7cher", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es wird gelesen,", "tokens": ["Es", "wird", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie eines jeden Lauff", "tokens": ["Wie", "ei\u00b7nes", "je\u00b7den", "Lauff"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bewandt gewesen.", "tokens": ["Be\u00b7wandt", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Der wird als Satans Theil", "tokens": ["Der", "wird", "als", "Sa\u00b7tans", "Theil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "KOKOM", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hinweg getrieben:", "tokens": ["Hin\u00b7weg", "ge\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der steht zum Trost und Heil", "tokens": ["Der", "steht", "zum", "Trost", "und", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jm Buch geschrieben.", "tokens": ["Jm", "Buch", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.16": {"line.1": {"text": "Wie wird es mir ergehn", "tokens": ["Wie", "wird", "es", "mir", "er\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An diesem Tage?", "tokens": ["An", "die\u00b7sem", "Ta\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wo wird mein Urtheil stehn?", "tokens": ["Wo", "wird", "mein", "Ur\u00b7theil", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer h\u00e4lt die Waage?", "tokens": ["Wer", "h\u00e4lt", "die", "Waa\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.17": {"line.1": {"text": "Triumph! der hier erschein.", "tokens": ["Tri\u00b7umph", "!", "der", "hier", "er\u00b7schein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jm rothen Kleide,", "tokens": ["Jm", "ro\u00b7then", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Eins sind wir beyde.", "tokens": ["Eins", "sind", "wir", "bey\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.18": {"line.1": {"text": "Da solte ich vor mich", "tokens": ["Da", "sol\u00b7te", "ich", "vor", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nichts Gutes hoffen?", "tokens": ["Nichts", "Gu\u00b7tes", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wer so besteht, wie ich,", "tokens": ["Wer", "so", "be\u00b7steht", ",", "wie", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der hats getroffen.", "tokens": ["Der", "hats", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.19": {"line.1": {"text": "Ich war ein S\u00fcnden-Kind,", "tokens": ["Ich", "war", "ein", "S\u00fcn\u00b7den\u00b7Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie andre S\u00fcnder:", "tokens": ["Wie", "and\u00b7re", "S\u00fcn\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Allein, ich \u00fcberwind", "tokens": ["Al\u00b7lein", ",", "ich", "\u00fc\u00b7berw\u00b7ind"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jm Uberwinder.", "tokens": ["Jm", "U\u00b7ber\u00b7win\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.20": {"line.1": {"text": "Ich bin an seinem Stamm", "tokens": ["Ich", "bin", "an", "sei\u00b7nem", "Stamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hinan gedehnet:", "tokens": ["Hi\u00b7nan", "ge\u00b7deh\u00b7net", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Er ist das reine Lamm,", "tokens": ["Er", "ist", "das", "rei\u00b7ne", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das GOtt vers\u00f6hnet.", "tokens": ["Das", "Gott", "ver\u00b7s\u00f6h\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.21": {"line.1": {"text": "O Lamm, verg\u00f6nne mir", "tokens": ["O", "Lamm", ",", "ver\u00b7g\u00f6n\u00b7ne", "mir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dich zu begleiten!", "tokens": ["Dich", "zu", "be\u00b7glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mein Mann, ich weiche dir", "tokens": ["Mein", "Mann", ",", "ich", "wei\u00b7che", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht von der Seiten.", "tokens": ["Nicht", "von", "der", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.22": {"line.1": {"text": "Ich sehe schon hinein", "tokens": ["Ich", "se\u00b7he", "schon", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In deine Wonne:", "tokens": ["In", "dei\u00b7ne", "Won\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Hie blitzt der klare Schein", "tokens": ["Hie", "blitzt", "der", "kla\u00b7re", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Salems Sonne.", "tokens": ["Von", "Sa\u00b7lems", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.23": {"line.1": {"text": "Wie mancher stehet da", "tokens": ["Wie", "man\u00b7cher", "ste\u00b7het", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In reiner Seide:", "tokens": ["In", "rei\u00b7ner", "Sei\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie ist dir der so nah", "tokens": ["Wie", "ist", "dir", "der", "so", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jm wei\u00dfen Kleide!", "tokens": ["Jm", "wei\u00b7\u00dfen", "Klei\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.24": {"line.1": {"text": "Den hielt man in der Welt", "tokens": ["Den", "hielt", "man", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor einen Narren,", "tokens": ["Vor", "ei\u00b7nen", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der dort im Ruhe-Zelt", "tokens": ["Der", "dort", "im", "Ru\u00b7he\u00b7Zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zog lang im Karren.", "tokens": ["Zog", "lang", "im", "Kar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.25": {"line.1": {"text": "Wie seuffzte deine Magd", "tokens": ["Wie", "seuffz\u00b7te", "dei\u00b7ne", "Magd"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jm Krancken-Bette!", "tokens": ["Jm", "Kran\u00b7cken\u00b7Bet\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie offt hat sie gesagt:", "tokens": ["Wie", "offt", "hat", "sie", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer Fl\u00fcgel h\u00e4tte!", "tokens": ["Wer", "Fl\u00fc\u00b7gel", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.26": {"line.1": {"text": "Und ietzo seh ich sie", "tokens": ["Und", "iet\u00b7zo", "seh", "ich", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit Palmen-Zweigen,", "tokens": ["Mit", "Pal\u00b7men\u00b7Zwei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Auff Zion steigen.", "tokens": ["Auff", "Zi\u00b7on", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.27": {"line.1": {"text": "Wo ist der arme Mann,", "tokens": ["Wo", "ist", "der", "ar\u00b7me", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der hier nur thr\u00e4nte,", "tokens": ["Der", "hier", "nur", "thr\u00e4n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und sich von Jugend an", "tokens": ["Und", "sich", "von", "Ju\u00b7gend", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nach Salem sehnte.", "tokens": ["Nach", "Sa\u00b7lem", "sehn\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.28": {"line.1": {"text": "Da sitzt er Freuden-voll", "tokens": ["Da", "sitzt", "er", "Freu\u00b7den\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu deinen F\u00fcssen,", "tokens": ["Zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und giebt dir einen Zoll", "tok