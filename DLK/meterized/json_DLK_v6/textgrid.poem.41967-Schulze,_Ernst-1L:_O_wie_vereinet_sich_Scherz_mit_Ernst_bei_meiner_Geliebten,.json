{"textgrid.poem.41967": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie vereinet sich Scherz mit Ernst bei meiner Geliebten,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie vereinet sich Scherz mit Ernst bei meiner Geliebten,", "tokens": ["O", "wie", "ver\u00b7ei\u00b7net", "sich", "Scherz", "mit", "Ernst", "bei", "mei\u00b7ner", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PRF", "NN", "APPR", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wie mit den Sitten der Welt tiefer, romantischer Sinn!", "tokens": ["Wie", "mit", "den", "Sit\u00b7ten", "der", "Welt", "tie\u00b7fer", ",", "ro\u00b7man\u00b7ti\u00b7scher", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jetzt erheitert mit fr\u00f6hlichem Witz sie den gl\u00e4nzenden Cirkel,", "tokens": ["Jetzt", "er\u00b7hei\u00b7tert", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Witz", "sie", "den", "gl\u00e4n\u00b7zen\u00b7den", "Cir\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und jetzt schw\u00e4rmt sie mit mir \u00fcber die Erde hinaus.", "tokens": ["Und", "jetzt", "schw\u00e4rmt", "sie", "mit", "mir", "\u00fc\u00b7ber", "die", "Er\u00b7de", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gleich dem Cham\u00e4leon wandelt ihr Geist sich in jegliche Form um,", "tokens": ["Gleich", "dem", "Cha\u00b7m\u00e4\u00b7le\u00b7on", "wan\u00b7delt", "ihr", "Geist", "sich", "in", "jeg\u00b7li\u00b7che", "Form", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Und in jeglicher Form reizt und entz\u00fccket ihr Geist.", "tokens": ["Und", "in", "jeg\u00b7li\u00b7cher", "Form", "reizt", "und", "ent\u00b7z\u00fc\u00b7cket", "ihr", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "J\u00fcngst durchschw\u00e4rmt' ich mit ihr Ariostos g\u00f6ttliche Dichtung,", "tokens": ["J\u00fcngst", "durch\u00b7schw\u00e4rmt'", "ich", "mit", "ihr", "A\u00b7rios\u00b7tos", "g\u00f6tt\u00b7li\u00b7che", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und wir entschwanden der Welt, irrten durch Wunder dahin.", "tokens": ["Und", "wir", "ent\u00b7schwan\u00b7den", "der", "Welt", ",", "irr\u00b7ten", "durch", "Wun\u00b7der", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "PAV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Fr\u00f6hlich ergriff mich des bunten Gewirrs fantastischer Zauber,", "tokens": ["Fr\u00f6h\u00b7lich", "er\u00b7griff", "mich", "des", "bun\u00b7ten", "Ge\u00b7wirrs", "fan\u00b7tas\u00b7ti\u00b7scher", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Und mit gl\u00fchendem Blick rief ich im Taumel des Wahns:", "tokens": ["Und", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Blick", "rief", "ich", "im", "Tau\u00b7mel", "des", "Wahns", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Liebchen, o bl\u00fchete doch noch jetzt die begeisternde Zeit uns,", "tokens": ["Lieb\u00b7chen", ",", "o", "bl\u00fc\u00b7he\u00b7te", "doch", "noch", "jetzt", "die", "be\u00b7geis\u00b7tern\u00b7de", "Zeit", "uns", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "VVFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.12": {"text": "Wo nur M\u00fche den Lohn, Liebe nur Liebe gewann,", "tokens": ["Wo", "nur", "M\u00fc\u00b7he", "den", "Lohn", ",", "Lie\u00b7be", "nur", "Lie\u00b7be", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "ART", "NN", "$,", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Wo sich im Wort nicht blos auspr\u00e4gte des Herzens Empfindung,", "tokens": ["Wo", "sich", "im", "Wort", "nicht", "blos", "aus\u00b7pr\u00e4g\u00b7te", "des", "Her\u00b7zens", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "NN", "PTKNEG", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Wo auch kr\u00e4ftig die That b\u00fcrgte f\u00fcr's innre Gef\u00fchl.", "tokens": ["Wo", "auch", "kr\u00e4f\u00b7tig", "die", "That", "b\u00fcrg\u00b7te", "f\u00fcr's", "inn\u00b7re", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Ha, dann st\u00fcrzt' ich f\u00fcr dich zum Kampf beim schmetternden Schlachtruf,", "tokens": ["Ha", ",", "dann", "st\u00fcrzt'", "ich", "f\u00fcr", "dich", "zum", "Kampf", "beim", "schmet\u00b7tern\u00b7den", "Schlach\u00b7truf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "---+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Siegt' in jeglichem Kampf, holde Geliebte, f\u00fcr dich,", "tokens": ["Siegt'", "in", "jeg\u00b7li\u00b7chem", "Kampf", ",", "hol\u00b7de", "Ge\u00b7lieb\u00b7te", ",", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Denn hoch weht', an den Speer mir gekn\u00fcpft, muthwinkend die Farbe,", "tokens": ["Denn", "hoch", "weht'", ",", "an", "den", "Speer", "mir", "ge\u00b7kn\u00fcpft", ",", "mut\u00b7hwin\u00b7kend", "die", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$,", "APPR", "ART", "NN", "PPER", "VVPP", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Die du mir gabst, dein Blick schenkte mir eiserne Kraft.", "tokens": ["Die", "du", "mir", "gabst", ",", "dein", "Blick", "schenk\u00b7te", "mir", "ei\u00b7ser\u00b7ne", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Ha, dann str\u00e4ubt' ich mich nicht durch Libyas W\u00fcste zu wandern,", "tokens": ["Ha", ",", "dann", "str\u00e4ubt'", "ich", "mich", "nicht", "durch", "Li\u00b7byas", "W\u00fcs\u00b7te", "zu", "wan\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "NE", "NN", "PTKZU", "VVFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Tappte durch Kl\u00fcfte, die nie Strahlen des Tages gesehn,", "tokens": ["Tapp\u00b7te", "durch", "Kl\u00fcf\u00b7te", ",", "die", "nie", "Strah\u00b7len", "des", "Ta\u00b7ges", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PRELS", "ADV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Wollte durch's Meer, durch's st\u00fcrmische Meer hinschwimmen, ein Bl\u00fcmchen", "tokens": ["Woll\u00b7te", "durch's", "Meer", ",", "durch's", "st\u00fcr\u00b7mi\u00b7sche", "Meer", "hin\u00b7schwim\u00b7men", ",", "ein", "Bl\u00fcm\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Dir zu brechen, das fern schm\u00fcckte den feindlichen Strand.", "tokens": ["Dir", "zu", "bre\u00b7chen", ",", "das", "fern", "schm\u00fcck\u00b7te", "den", "feind\u00b7li\u00b7chen", "Strand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PRELS", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Doch dann lohnt' auch selige Ruh' die best\u00e4ndige Treue,", "tokens": ["Doch", "dann", "lohnt'", "auch", "se\u00b7li\u00b7ge", "Ruh'", "die", "be\u00b7st\u00e4n\u00b7di\u00b7ge", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Und nicht bliebe der Lohn z\u00e4rtlicher Minne mir aus.", "tokens": ["Und", "nicht", "blie\u00b7be", "der", "Lohn", "z\u00e4rt\u00b7li\u00b7cher", "Min\u00b7ne", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "NN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Friedlich wallten wir dann durch bl\u00fchende Fluren und spannten,", "tokens": ["Fried\u00b7lich", "wall\u00b7ten", "wir", "dann", "durch", "bl\u00fc\u00b7hen\u00b7de", "Flu\u00b7ren", "und", "spann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Wo es der Laune Gebot wollte, das schattige Zelt,", "tokens": ["Wo", "es", "der", "Lau\u00b7ne", "Ge\u00b7bot", "woll\u00b7te", ",", "das", "schat\u00b7ti\u00b7ge", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VMFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Wohnten auf duftigen Wiesen, wo s\u00fc\u00df rings w\u00e4rmerer Lufthauch", "tokens": ["Wohn\u00b7ten", "auf", "duf\u00b7ti\u00b7gen", "Wie\u00b7sen", ",", "wo", "s\u00fc\u00df", "rings", "w\u00e4r\u00b7me\u00b7rer", "Luft\u00b7hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADV", "ADJA", "NN"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.28": {"text": "S\u00e4uselt', und ewiges Blau schm\u00fcckte den himmlischen Dom.", "tokens": ["S\u00e4u\u00b7selt'", ",", "und", "e\u00b7wi\u00b7ges", "Blau", "schm\u00fcck\u00b7te", "den", "himm\u00b7li\u00b7schen", "Dom", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Lieder dichtet' ich dann voll Lieb' und sch\u00fcchterner Sehnsucht,", "tokens": ["Lie\u00b7der", "dich\u00b7tet'", "ich", "dann", "voll", "Lieb'", "und", "sch\u00fcch\u00b7ter\u00b7ner", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Schmelzend s\u00e4nge des Hains Muse die Weisen dazu.", "tokens": ["Schmel\u00b7zend", "s\u00e4n\u00b7ge", "des", "Hains", "Mu\u00b7se", "die", "Wei\u00b7sen", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Oft auch raubt' ich mit leiser Gewalt dir bebend ein K\u00fc\u00dfchen,", "tokens": ["Oft", "auch", "raubt'", "ich", "mit", "lei\u00b7ser", "Ge\u00b7walt", "dir", "be\u00b7bend", "ein", "K\u00fc\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Und du drohetest zwar, aber doch z\u00fcrntest du nie.", "tokens": ["Und", "du", "dro\u00b7he\u00b7test", "zwar", ",", "a\u00b7ber", "doch", "z\u00fcrn\u00b7test", "du", "nie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Doch das ist jetzt Alles vorbei; mit eisernen Banden", "tokens": ["Doch", "das", "ist", "jetzt", "Al\u00b7les", "vor\u00b7bei", ";", "mit", "ei\u00b7ser\u00b7nen", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PIS", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "H\u00e4lt uns die Welt, und der Wahn k\u00fcrzte die Schwingen uns ab.", "tokens": ["H\u00e4lt", "uns", "die", "Welt", ",", "und", "der", "Wahn", "k\u00fcrz\u00b7te", "die", "Schwin\u00b7gen", "uns", "ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Also rief ich entflammt. Sie l\u00e4chelte. Wahrlich, in Manchas", "tokens": ["Al\u00b7so", "rief", "ich", "ent\u00b7flammt", ".", "Sie", "l\u00e4\u00b7chel\u00b7te", ".", "Wahr\u00b7lich", ",", "in", "Man\u00b7chas"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "$.", "ADV", "$,", "APPR", "NE"], "meter": "--+--+-+--+-++-", "measure": "anapaest.di.plus"}, "line.36": {"text": "Luft nur, w\u00e4hnet' ich sonst, k\u00f6nn' ein Ouixote gedeihn;", "tokens": ["Luft", "nur", ",", "w\u00e4h\u00b7net'", "ich", "sonst", ",", "k\u00f6nn'", "ein", "Ou\u00b7i\u00b7xo\u00b7te", "ge\u00b7deihn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+--++-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.37": {"text": "Sprich, wie zeugte der k\u00e4ltere Nord und der ewigen Haiden", "tokens": ["Sprich", ",", "wie", "zeug\u00b7te", "der", "k\u00e4l\u00b7te\u00b7re", "Nord", "und", "der", "e\u00b7wi\u00b7gen", "Hai\u00b7den"], "token_info": ["word", "punct", "word", "word", "w