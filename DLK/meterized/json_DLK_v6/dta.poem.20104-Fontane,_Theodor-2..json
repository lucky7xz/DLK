{"dta.poem.20104": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Und \u00fcber\u2019s Dorf ging Jahr um Jahr,", "tokens": ["Und", "\u00fc\u00b7ber's", "Dorf", "ging", "Jahr", "um", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufscho\u00df manch schlanke Tanne,", "tokens": ["Auf\u00b7scho\u00df", "manch", "schlan\u00b7ke", "Tan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie aber, die \u201eSch\u00f6n-Anne\u201c war,", "tokens": ["Sie", "a\u00b7ber", ",", "die", "\u201e", "Sch\u00f6n\u00b7An\u00b7ne", "\u201c", "war", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "$(", "NN", "$(", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hei\u00dft lang nun \u201eMutter Anne\u201c;", "tokens": ["Hei\u00dft", "lang", "nun", "\u201e", "Mut\u00b7ter", "An\u00b7ne", "\u201c", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "$(", "NN", "NE", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt, wenn im Krug brav T\u00e4nzer sind,", "tokens": ["Jetzt", ",", "wenn", "im", "Krug", "brav", "T\u00e4n\u00b7zer", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPRART", "NN", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geht schon der sch\u00f6nen Anne ", "tokens": ["Geht", "schon", "der", "sch\u00f6\u00b7nen", "An\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Sonntagsschmuck zu Tanze.", "tokens": ["Im", "Sonn\u00b7tags\u00b7schmuck", "zu", "Tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was weint die Mutter Anne so,", "tokens": ["Was", "weint", "die", "Mut\u00b7ter", "An\u00b7ne", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00fctzt den Kopf in Sorgen?", "tokens": ["Und", "st\u00fctzt", "den", "Kopf", "in", "Sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schl\u00e4gt ihr das Mutterherz nicht froh", "tokens": ["Schl\u00e4gt", "ihr", "das", "Mut\u00b7ter\u00b7herz", "nicht", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An jedem neuen Morgen?", "tokens": ["An", "je\u00b7dem", "neu\u00b7en", "Mor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Tochter kommt vom Tanz nach Haus,", "tokens": ["Die", "Toch\u00b7ter", "kommt", "vom", "Tanz", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Mutter spricht: \u201ebliebst lange aus,", "tokens": ["Die", "Mut\u00b7ter", "spricht", ":", "\u201e", "bliebst", "lan\u00b7ge", "aus", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kind, halte Dich in Ehren!\u201c", "tokens": ["Kind", ",", "hal\u00b7te", "Dich", "in", "Eh\u00b7ren", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Tochter zieht ein schnippsch Gesicht,", "tokens": ["Die", "Toch\u00b7ter", "zieht", "ein", "schnippsch", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spricht: \u201ela\u00df mich nur machen!", "tokens": ["Und", "spricht", ":", "\u201e", "la\u00df", "mich", "nur", "ma\u00b7chen", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich d\u00e4cht, ich hielt\u2019 auf Ehr und Pflicht,", "tokens": ["Ich", "d\u00e4cht", ",", "ich", "hielt'", "auf", "Ehr", "und", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u2014 kann mich selbst bewachen;", "tokens": ["Und", "kann", "mich", "selbst", "be\u00b7wa\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn ich leicht und locker w\u00e4r\u2019,", "tokens": ["Und", "wenn", "ich", "leicht", "und", "lo\u00b7cker", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es k\u00e4m wohl nicht von ungef\u00e4hr,", "tokens": ["Es", "k\u00e4m", "wohl", "nicht", "von", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat alles seine Gr\u00fcnde.", "tokens": ["Hat", "al\u00b7les", "sei\u00b7ne", "Gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201edu sagst mir oft, mein Vater sei", "tokens": ["\u201e", "du", "sagst", "mir", "oft", ",", "mein", "Va\u00b7ter", "sei"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Jahren schon gestorben,", "tokens": ["Vor", "Jah\u00b7ren", "schon", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch hat mir manche Neckerei", "tokens": ["Doch", "hat", "mir", "man\u00b7che", "Ne\u00b7cke\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Glauben dran verdorben;", "tokens": ["Den", "Glau\u00b7ben", "dran", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohl schuld ich dieses Leben Dir,", "tokens": ["Wohl", "schuld", "ich", "die\u00b7ses", "Le\u00b7ben", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch, wei\u00df es Gott, oft w\u00fcnsch ich mir,", "tokens": ["Doch", ",", "wei\u00df", "es", "Gott", ",", "oft", "w\u00fcnsch", "ich", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich w\u00e4re nicht geboren.\u201c", "tokens": ["Ich", "w\u00e4\u00b7re", "nicht", "ge\u00b7bo\u00b7ren", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie spricht\u2019s, ihr schwarzes Auge gl\u00fcht,", "tokens": ["Sie", "spricht's", ",", "ihr", "schwar\u00b7zes", "Au\u00b7ge", "gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Th\u00fcr ist zugeflogen,", "tokens": ["Die", "Th\u00fcr", "ist", "zu\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und um die letzte Hoffnung sieht", "tokens": ["Und", "um", "die", "letz\u00b7te", "Hoff\u00b7nung", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Arm-Anne sich betrogen;", "tokens": ["Ar\u00b7mAn\u00b7ne", "sich", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie seufzt: \u201edas also ist der Lohn,", "tokens": ["Sie", "seufzt", ":", "\u201e", "das", "al\u00b7so", "ist", "der", "Lohn", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PDS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um den ich allen Spott und Hohn", "tokens": ["Um", "den", "ich", "al\u00b7len", "Spott", "und", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Lebelang getragen!\u201c", "tokens": ["Mein", "Le\u00b7be\u00b7lang", "ge\u00b7tra\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann aber betet sie bewegt:", "tokens": ["Dann", "a\u00b7ber", "be\u00b7tet", "sie", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201egott, es ist mein Verschulden!", "tokens": ["\u201e", "gott", ",", "es", "ist", "mein", "Ver\u00b7schul\u00b7den", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was uns Dein Wille auferlegt", "tokens": ["Was", "uns", "Dein", "Wil\u00b7le", "auf\u00b7er\u00b7legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geziemet uns zu dulden; \u2014", "tokens": ["Ge\u00b7zie\u00b7met", "uns", "zu", "dul\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Entsagen kann die wahre Lieb',", "tokens": ["Ent\u00b7sa\u00b7gen", "kann", "die", "wah\u00b7re", "Lieb'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es war die ", "tokens": ["Es", "war", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und bitter mu\u00df ich\u2019s b\u00fc\u00dfen.\u201c", "tokens": ["Und", "bit\u00b7ter", "mu\u00df", "ich's", "b\u00fc\u00b7\u00dfen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}