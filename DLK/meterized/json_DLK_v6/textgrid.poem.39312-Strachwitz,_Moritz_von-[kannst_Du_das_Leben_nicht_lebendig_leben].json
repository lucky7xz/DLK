{"textgrid.poem.39312": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[kannst Du das Leben nicht lebendig leben]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst Du das Leben nicht lebendig leben,", "tokens": ["Kannst", "Du", "das", "Le\u00b7ben", "nicht", "le\u00b7ben\u00b7dig", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Statt \u00fcber diesen feuertrunknen Farben", "tokens": ["Statt", "\u00fc\u00b7ber", "die\u00b7sen", "feu\u00b7er\u00b7trunk\u00b7nen", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf schwarzen Fittich alten Grams zu schweben?", "tokens": ["Auf", "schwar\u00b7zen", "Fit\u00b7tich", "al\u00b7ten", "Grams", "zu", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Venedig fiel und seine Helden starben! \u2013", "tokens": ["Ve\u00b7ne\u00b7dig", "fiel", "und", "sei\u00b7ne", "Hel\u00b7den", "star\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Doch sieh, der Markusplatz ist lauter Feuer", "tokens": ["Doch", "sieh", ",", "der", "Mar\u00b7kus\u00b7platz", "ist", "lau\u00b7ter", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wirft ins Wasser tausend Strahlengarben.", "tokens": ["Und", "wirft", "ins", "Was\u00b7ser", "tau\u00b7send", "Strah\u00b7len\u00b7gar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier schwimmt die Lust und wirft hinweg das Steuer,", "tokens": ["Hier", "schwimmt", "die", "Lust", "und", "wirft", "hin\u00b7weg", "das", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier wehn der Sch\u00f6nheit jugendliche Lichter", "tokens": ["Hier", "wehn", "der", "Sch\u00f6n\u00b7heit", "ju\u00b7gend\u00b7li\u00b7che", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um alter Meister herrlichstes Gem\u00e4uer.", "tokens": ["Um", "al\u00b7ter", "Meis\u00b7ter", "herr\u00b7lichs\u00b7tes", "Ge\u00b7m\u00e4u\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da unten aber wogt es dicht und dichter;", "tokens": ["Da", "un\u00b7ten", "a\u00b7ber", "wogt", "es", "dicht", "und", "dich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zwingt Dich nicht zum reizenden Vergessen", "tokens": ["Und", "zwingt", "Dich", "nicht", "zum", "rei\u00b7zen\u00b7den", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lebensf\u00fclle strahlender Gesichter?", "tokens": ["Die", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", "strah\u00b7len\u00b7der", "Ge\u00b7sich\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der tiefe Himmel w\u00f6lbt den Platz indessen", "tokens": ["Der", "tie\u00b7fe", "Him\u00b7mel", "w\u00f6lbt", "den", "Platz", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Amethyst zum s\u00e4ulenstolzen Saale,", "tokens": ["Mit", "A\u00b7met\u00b7hyst", "zum", "s\u00e4u\u00b7len\u00b7stol\u00b7zen", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In keinem sch\u00f6nern bist Du je gesessen! \u2013", "tokens": ["In", "kei\u00b7nem", "sch\u00f6\u00b7nern", "bist", "Du", "je", "ge\u00b7ses\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "VVINF", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Umsonst, umsonst! Mein Auge schmerzt vom Strahle,", "tokens": ["Um\u00b7sonst", ",", "um\u00b7sonst", "!", "Mein", "Au\u00b7ge", "schmerzt", "vom", "Strah\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz ist m\u00fcd \u2013 la\u00df schnell Dein Ruder triefen,", "tokens": ["Mein", "Herz", "ist", "m\u00fcd", "\u2013", "la\u00df", "schnell", "Dein", "Ru\u00b7der", "trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "VVIMP", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Gondolier, hinab den Gran Canale!", "tokens": ["Mein", "Gon\u00b7do\u00b7lier", ",", "hin\u00b7ab", "den", "Gran", "Ca\u00b7na\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn Licht und L\u00e4rm sich hinter uns verliefen,", "tokens": ["Wenn", "Licht", "und", "L\u00e4rm", "sich", "hin\u00b7ter", "uns", "ver\u00b7lie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann kann mein Aug' auf Mondeswellen schweifen", "tokens": ["Dann", "kann", "mein", "Aug'", "auf", "Mon\u00b7des\u00b7wel\u00b7len", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In \u00f6der Fenster schwermutsvolle Tiefen!", "tokens": ["In", "\u00f6\u00b7der", "Fens\u00b7ter", "schwer\u00b7muts\u00b7vol\u00b7le", "Tie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Hier weht von wundervollen S\u00e4ulenkn\u00e4ufen", "tokens": ["Hier", "weht", "von", "wun\u00b7der\u00b7vol\u00b7len", "S\u00e4u\u00b7len\u00b7kn\u00e4u\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schwermut Schlingkraut \u00fcber Tor und Mauer,", "tokens": ["Der", "Schwer\u00b7mut", "Schling\u00b7kraut", "\u00fc\u00b7ber", "Tor", "und", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier kann mein Herz im Stillen bl\u00fchn und reifen!", "tokens": ["Hier", "kann", "mein", "Herz", "im", "Stil\u00b7len", "bl\u00fchn", "und", "rei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Nicht kam ich her zu diesem Wonneschauer,", "tokens": ["Nicht", "kam", "ich", "her", "zu", "die\u00b7sem", "Won\u00b7ne\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Venetia, da\u00df kurzer Glanz mich bade,", "tokens": ["Ve\u00b7ne\u00b7tia", ",", "da\u00df", "kur\u00b7zer", "Glanz", "mich", "ba\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich kam zu Dir, zu teilen Deine Trauer!", "tokens": ["Ich", "kam", "zu", "Dir", ",", "zu", "tei\u00b7len", "Dei\u00b7ne", "Trau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Kein Fenster klirrt auf meinem \u00f6den Pfade,", "tokens": ["Kein", "Fens\u00b7ter", "klirrt", "auf", "mei\u00b7nem", "\u00f6\u00b7den", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nur die Welle, tr\u00e4umerisch und tr\u00fcbe,", "tokens": ["Und", "nur", "die", "Wel\u00b7le", ",", "tr\u00e4u\u00b7me\u00b7risch", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Spielt an Venedigs marmorne Gestade", "tokens": ["Spielt", "an", "Ve\u00b7ne\u00b7digs", "mar\u00b7mor\u00b7ne", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Mit leisem Schlag ein Lied \u2013 verlorner Liebe!", "tokens": ["Mit", "lei\u00b7sem", "Schlag", "ein", "Lied", "\u2013", "ver\u00b7lor\u00b7ner", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}