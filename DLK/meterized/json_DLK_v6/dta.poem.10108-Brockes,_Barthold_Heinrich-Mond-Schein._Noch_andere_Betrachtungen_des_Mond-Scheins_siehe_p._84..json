{"dta.poem.10108": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mond-Schein.   Noch andere Betrachtungen des Mond-Scheins siehe  p.  84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann wir, in einer stillen Nacht, bey heitrer Luft, von\nMonden-Schein,", "tokens": ["Wann", "wir", ",", "in", "ei\u00b7ner", "stil\u00b7len", "Nacht", ",", "bey", "hei\u00b7trer", "Luft", ",", "von", "Mon\u00b7den\u00b7Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und dessen sanftem Licht beflossen, bestrahlet und erleuchtet", "tokens": ["Und", "des\u00b7sen", "sanf\u00b7tem", "Licht", "be\u00b7flos\u00b7sen", ",", "be\u00b7strah\u00b7let", "und", "er\u00b7leuch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Gedenckt man, (ob gleich, leider! selten) wol an den Mond,", "tokens": ["Ge\u00b7denckt", "man", ",", "(", "ob", "gleich", ",", "lei\u00b7der", "!", "sel\u00b7ten", ")", "wol", "an", "den", "Mond", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "$(", "KOUS", "ADV", "$,", "ADV", "$.", "ADJD", "$(", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Blickt in den reinen Silber-Kreis, und zieht darauf den kur-", "tokens": ["Blickt", "in", "den", "rei\u00b7nen", "Sil\u00b7ber\u00b7Kreis", ",", "und", "zieht", "da\u00b7rauf", "den", "kur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PAV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Mit einer kalten L\u00e4ssigkeit, die bald sich satt gesehn, zur\u00fcck:", "tokens": ["Mit", "ei\u00b7ner", "kal\u00b7ten", "L\u00e4s\u00b7sig\u00b7keit", ",", "die", "bald", "sich", "satt", "ge\u00b7sehn", ",", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "ADJD", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Spricht auch noch wol: wie scheint der Mond so hell, so", "tokens": ["Spricht", "auch", "noch", "wol", ":", "wie", "scheint", "der", "Mond", "so", "hell", ",", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allein, wie wenig sind doch derer, die ihres geistigen", "tokens": ["Al\u00b7lein", ",", "wie", "we\u00b7nig", "sind", "doch", "de\u00b7rer", ",", "die", "ih\u00b7res", "geis\u00b7ti\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PIS", "VAFIN", "ADV", "PDS", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Vern\u00fcnftgen Blick hinaufw\u00e4rts schicken, und zu dem Ur-", "tokens": ["Ver\u00b7n\u00fcnft\u00b7gen", "Blick", "hin\u00b7auf\u00b7w\u00e4rts", "schi\u00b7cken", ",", "und", "zu", "dem", "Ur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$,", "KON", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Zur Sonnen, als dem wahren Licht, die fr\u00f6hlichen Gedan-", "tokens": ["Zur", "Son\u00b7nen", ",", "als", "dem", "wah\u00b7ren", "Licht", ",", "die", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7dan"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die, da\u00df der Mond kein Licht besitze, und da\u00df, wenn sie im", "tokens": ["Die", ",", "da\u00df", "der", "Mond", "kein", "Licht", "be\u00b7sit\u00b7ze", ",", "und", "da\u00df", ",", "wenn", "sie", "im"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$,", "KON", "KOUS", "$,", "KOUS", "PPER", "APPRART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Sie in dem wahren Sonnen-Schein spatziren; fassen und", "tokens": ["Sie", "in", "dem", "wah\u00b7ren", "Son\u00b7nen\u00b7Schein", "spat\u00b7zi\u00b7ren", ";", "fas\u00b7sen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$.", "VVINF", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Ja wie viel minder sind noch derer, die, voller Andacht,", "tokens": ["Ja", "wie", "viel", "min\u00b7der", "sind", "noch", "de\u00b7rer", ",", "die", ",", "vol\u00b7ler", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "ADV", "ADV", "VAFIN", "ADV", "PDS", "$,", "PRELS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf Den, wovon die Sonn\u2019 ihr Licht, so wie der Mond von", "tokens": ["Auf", "Den", ",", "wo\u00b7von", "die", "Sonn'", "ihr", "Licht", ",", "so", "wie", "der", "Mond", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "PWAV", "ART", "NN", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der einzig, ewig unerschaffen, im Licht, das undurchdringlich,", "tokens": ["Der", "ein\u00b7zig", ",", "e\u00b7wig", "un\u00b7er\u00b7schaf\u00b7fen", ",", "im", "Licht", ",", "das", "un\u00b7durch\u00b7dring\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "ADJD", "$,", "APPRART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und die Jhm ein ger\u00fchrtes Hertz, f\u00fcr Sein Gesch\u00f6pf, zum", "tokens": ["Und", "die", "Jhm", "ein", "ge\u00b7r\u00fchr\u00b7tes", "Hertz", ",", "f\u00fcr", "Sein", "Ge\u00b7sch\u00f6pf", ",", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART"], "meter": "--++-+-+-+-+-", "measure": "anapaest.init"}}}}}