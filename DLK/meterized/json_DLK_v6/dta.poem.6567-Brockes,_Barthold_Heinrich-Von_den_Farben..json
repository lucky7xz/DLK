{"dta.poem.6567": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der bunten Farben Glantz, so wol bebl\u00fchmter Auen,", "tokens": ["Der", "bun\u00b7ten", "Far\u00b7ben", "Glantz", ",", "so", "wol", "be\u00b7bl\u00fchm\u00b7ter", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unsern Blick mit ihren Schmeltz erfreuen,", "tokens": ["Die", "un\u00b7sern", "Blick", "mit", "ih\u00b7ren", "Sch\u00b7meltz", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als auch der Schilder-und Tapezereyen,", "tokens": ["Als", "auch", "der", "Schil\u00b7der\u00b7\u00b7und", "Ta\u00b7pe\u00b7ze\u00b7re\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die unserem Gesicht die kluge Kunst l\u00e4sst schauen,", "tokens": ["Die", "un\u00b7se\u00b7rem", "Ge\u00b7sicht", "die", "klu\u00b7ge", "Kunst", "l\u00e4sst", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat um den Unterscheid der Farben, so sie zieren,", "tokens": ["Hat", "um", "den", "Un\u00b7ter\u00b7scheid", "der", "Far\u00b7ben", ",", "so", "sie", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In Mischungen, die so verschieden, zu formiren;", "tokens": ["In", "Misc\u00b7hun\u00b7gen", ",", "die", "so", "ver\u00b7schie\u00b7den", ",", "zu", "for\u00b7mi\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Nichts, als der Fl\u00e4che Tieff\u2019 und H\u00f6h\u2019n", "tokens": ["Nichts", ",", "als", "der", "Fl\u00e4\u00b7che", "Tief\u00b7f'", "und", "H\u00f6h'n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "NE", "KON", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Aus denen sie gewebt sind und bestehn,", "tokens": ["Aus", "de\u00b7nen", "sie", "ge\u00b7webt", "sind", "und", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "VAFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Durch welche sie in unsern Augen", "tokens": ["Durch", "wel\u00b7che", "sie", "in", "un\u00b7sern", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So viel Bewegungen zu w\u00fcrcken taugen.", "tokens": ["So", "viel", "Be\u00b7we\u00b7gun\u00b7gen", "zu", "w\u00fcr\u00b7cken", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Man nenne, wie man wil, die Farben von Natur,", "tokens": ["Man", "nen\u00b7ne", ",", "wie", "man", "wil", ",", "die", "Far\u00b7ben", "von", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PIS", "VMFIN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man nenne sie durch Kunst gemacht,", "tokens": ["Man", "nen\u00b7ne", "sie", "durch", "Kunst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es schwinde bald, es dau\u2019re ihre Pracht,", "tokens": ["Es", "schwin\u00b7de", "bald", ",", "es", "dau'\u00b7re", "ih\u00b7re", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dieselbe W\u00fcrckung bringt derselben Schmuck und Zier", "tokens": ["Die\u00b7sel\u00b7be", "W\u00fcr\u00b7ckung", "bringt", "der\u00b7sel\u00b7ben", "Schmuck", "und", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PDAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allein herf\u00fcr.", "tokens": ["Al\u00b7lein", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Der Jris Farben selbst, die nur so kurtze Zeit", "tokens": ["Der", "Jris", "Far\u00b7ben", "selbst", ",", "die", "nur", "so", "kurt\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJA", "NN"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.17": {"text": "In ihren Schimmer stehn,", "tokens": ["In", "ih\u00b7ren", "Schim\u00b7mer", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Sind nicht allein so sch\u00f6n,", "tokens": ["Sind", "nicht", "al\u00b7lein", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Sie sind auch in der That von gleicher W\u00fcrcklichkeit,", "tokens": ["Sie", "sind", "auch", "in", "der", "That", "von", "glei\u00b7cher", "W\u00fcrck\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als wie der Farben Schein,", "tokens": ["Als", "wie", "der", "Far\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Der C\u00f6rper, die best\u00e4ndig seyn.", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "die", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Glantz von Bildern, die gemahlet", "tokens": ["Der", "Glantz", "von", "Bil\u00b7dern", ",", "die", "ge\u00b7mah\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ist gar nicht k\u00fcnstlicher,", "tokens": ["Ist", "gar", "nicht", "k\u00fcnst\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.24": {"text": "Als eben der,", "tokens": ["Als", "e\u00b7ben", "der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.25": {"text": "Der in Rubinen strahlet,", "tokens": ["Der", "in", "Ru\u00b7bi\u00b7nen", "strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Den man nat\u00fcrlich nennt. Das Licht allein", "tokens": ["Den", "man", "na\u00b7t\u00fcr\u00b7lich", "nennt", ".", "Das", "Licht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN", "$.", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ist alles. Der Natur Manier ist einerley", "tokens": ["Ist", "al\u00b7les", ".", "Der", "Na\u00b7tur", "Ma\u00b7nier", "ist", "ei\u00b7ner\u00b7ley"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$.", "ART", "NN", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit welcher sie formirt so manche Schilderey.", "tokens": ["Mit", "wel\u00b7cher", "sie", "for\u00b7mirt", "so", "man\u00b7che", "Schil\u00b7de\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}