{"textgrid.poem.63946": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der verurtheilte Soldat", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger, tapferer Soldat", "tokens": ["Ein", "jun\u00b7ger", ",", "tap\u00b7fe\u00b7rer", "Sol\u00b7dat"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward wegen einer Uebelthat,", "tokens": ["Ward", "we\u00b7gen", "ei\u00b7ner", "Ue\u00b7belt\u00b7hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die er in b\u00f6sem Trunk begangen,", "tokens": ["Die", "er", "in", "b\u00f6\u00b7sem", "Trunk", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daf\u00fcr sein Urtheil zu empfangen,", "tokens": ["Da\u00b7f\u00fcr", "sein", "Ur\u00b7theil", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hinausgef\u00fchrt. Sein braunes Haar,", "tokens": ["Hin\u00b7aus\u00b7ge\u00b7f\u00fchrt", ".", "Sein", "brau\u00b7nes", "Haar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der gro\u00dfen schwarzen Augen Paar,", "tokens": ["Der", "gro\u00b7\u00dfen", "schwar\u00b7zen", "Au\u00b7gen", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein gut Gesicht, die sch\u00f6ne L\u00e4nge,", "tokens": ["Sein", "gut", "Ge\u00b7sicht", ",", "die", "sch\u00f6\u00b7ne", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bewegten ringsumher die Menge;", "tokens": ["Be\u00b7weg\u00b7ten", "rings\u00b7um\u00b7her", "die", "Men\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vor Allem ward er, wie man sagt,", "tokens": ["Vor", "Al\u00b7lem", "ward", "er", ",", "wie", "man", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vom weiblichen Geschlecht beklagt.", "tokens": ["Vom", "weib\u00b7li\u00b7chen", "Ge\u00b7schlecht", "be\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Schon kniet er nieder auf den Sand,", "tokens": ["Schon", "kniet", "er", "nie\u00b7der", "auf", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und schon war von des Henkers Hand", "tokens": ["Und", "schon", "war", "von", "des", "Hen\u00b7kers", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Das scharfe Schwert gez\u00fcckt, als Halt!", "tokens": ["Das", "schar\u00b7fe", "Schwert", "ge\u00b7z\u00fcckt", ",", "als", "Halt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Durch den geschloss'nen Kreis erschallt.", "tokens": ["Durch", "den", "ge\u00b7schloss'\u00b7nen", "Kreis", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein M\u00e4dchen drang zugleich herbei,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "drang", "zu\u00b7gleich", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und rief mit \u00e4ngstlichem Geschrei:", "tokens": ["Und", "rief", "mit", "\u00e4ngst\u00b7li\u00b7chem", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Pardon! Pardon! Ihr Leute denkt,", "tokens": ["Par\u00b7don", "!", "Par\u00b7don", "!", "Ihr", "Leu\u00b7te", "denkt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Man hat sein Leben mir geschenkt.", "tokens": ["Man", "hat", "sein", "Le\u00b7ben", "mir", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich fiel dem Landesherrn zu F\u00fc\u00dfen,", "tokens": ["Ich", "fiel", "dem", "Lan\u00b7des\u00b7herrn", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und lie\u00df so lange Thr\u00e4nen flie\u00dfen,", "tokens": ["Und", "lie\u00df", "so", "lan\u00b7ge", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Bis ich vom Tod ihn losgemacht.", "tokens": ["Bis", "ich", "vom", "Tod", "ihn", "los\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ihm ist Verzeihung zugedacht,", "tokens": ["Ihm", "ist", "Ver\u00b7zei\u00b7hung", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wenn er zur Frau mich nehmen will!", "tokens": ["Wenn", "er", "zur", "Frau", "mich", "neh\u00b7men", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der arme S\u00fcnder sah sie still", "tokens": ["Der", "ar\u00b7me", "S\u00fcn\u00b7der", "sah", "sie", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und voller Ueberlegung an.", "tokens": ["Und", "vol\u00b7ler", "Ue\u00b7ber\u00b7le\u00b7gung", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Was du (sprach er) f\u00fcr mich gethan,", "tokens": ["Was", "du", "(", "sprach", "er", ")", "f\u00fcr", "mich", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$(", "VVFIN", "PPER", "$(", "APPR", "PPER", "VVPP", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.27": {"text": "Ist dankenswerth. Doch, tr\u00fcgt mich nicht", "tokens": ["Ist", "dan\u00b7kens\u00b7werth", ".", "Doch", ",", "tr\u00fcgt", "mich", "nicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$.", "KON", "$,", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Dein wildes, kupfriges Gesicht,", "tokens": ["Dein", "wil\u00b7des", ",", "kupf\u00b7ri\u00b7ges", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dein rothes Aug', dein spitzes Kinn,", "tokens": ["Dein", "ro\u00b7thes", "Aug'", ",", "dein", "spit\u00b7zes", "Kinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "So bist du eine Teufelin,", "tokens": ["So", "bist", "du", "ei\u00b7ne", "Teu\u00b7fe\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Die mir zur allerschwersten B\u00fcrde", "tokens": ["Die", "mir", "zur", "al\u00b7ler\u00b7schwers\u00b7ten", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mein elend Leben machen w\u00fcrde!", "tokens": ["Mein", "e\u00b7lend", "Le\u00b7ben", "ma\u00b7chen", "w\u00fcr\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ein b\u00f6ses St\u00fcndchen ist f\u00fcrwahr", "tokens": ["Ein", "b\u00f6\u00b7ses", "St\u00fcnd\u00b7chen", "ist", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ertr\u00e4glicher, als zwanzig Jahr'", "tokens": ["Er\u00b7tr\u00e4g\u00b7li\u00b7cher", ",", "als", "zwan\u00b7zig", "Jahr'"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Mit einem Weibe, so wie du,", "tokens": ["Mit", "ei\u00b7nem", "Wei\u00b7be", ",", "so", "wie", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "In steter Qual; d'rum haut nur zu!", "tokens": ["In", "ste\u00b7ter", "Qual", ";", "d'\u00b7rum", "haut", "nur", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PAV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}