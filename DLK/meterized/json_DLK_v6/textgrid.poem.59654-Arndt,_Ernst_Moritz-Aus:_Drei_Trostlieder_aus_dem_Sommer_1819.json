{"textgrid.poem.59654": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Aus: Drei Trostlieder aus dem Sommer 1819", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Geist, und zieh dich st\u00e4hlen an!", "tokens": ["Komm", ",", "Geist", ",", "und", "zieh", "dich", "st\u00e4h\u00b7len", "an", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, Herz, und la\u00df dich eisern kleiden!", "tokens": ["Komm", ",", "Herz", ",", "und", "la\u00df", "dich", "ei\u00b7sern", "klei\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVIMP", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es r\u00fcste sich, was streiten kann,", "tokens": ["Es", "r\u00fcs\u00b7te", "sich", ",", "was", "strei\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWS", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf harten Krieg und schwere Leiden!", "tokens": ["Auf", "har\u00b7ten", "Krieg", "und", "schwe\u00b7re", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm, Stolz, und fasse das Panier!", "tokens": ["Komm", ",", "Stolz", ",", "und", "fas\u00b7se", "das", "Pa\u00b7nier", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df wehn die Fahnen, wehn zum Himmel!", "tokens": ["La\u00df", "wehn", "die", "Fah\u00b7nen", ",", "wehn", "zum", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das rechte Bleiben ist nicht hier,", "tokens": ["Das", "rech\u00b7te", "Blei\u00b7ben", "ist", "nicht", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum wirf dich mutig ins Get\u00fcmmel!", "tokens": ["Drum", "wirf", "dich", "mu\u00b7tig", "ins", "Ge\u00b7t\u00fcm\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, Hoffnung, auch! Es soll dein Gr\u00fcn,", "tokens": ["Komm", ",", "Hoff\u00b7nung", ",", "auch", "!", "Es", "soll", "dein", "Gr\u00fcn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Maigr\u00fcn, rosenrot sich f\u00e4rben,", "tokens": ["Dein", "Mai\u00b7gr\u00fcn", ",", "ro\u00b7sen\u00b7rot", "sich", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einmal sollst du herrlich bl\u00fchn", "tokens": ["Noch", "ein\u00b7mal", "sollst", "du", "herr\u00b7lich", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann gleich roten Rosen sterben.", "tokens": ["Und", "dann", "gleich", "ro\u00b7ten", "Ro\u00b7sen", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So steht der Krieg, so ist der Zorn,", "tokens": ["So", "steht", "der", "Krieg", ",", "so", "ist", "der", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in der Mitte gar kein Bleiben:", "tokens": ["Und", "in", "der", "Mit\u00b7te", "gar", "kein", "Blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer lechzet Strom zu sein, der Born", "tokens": ["Wer", "lech\u00b7zet", "Strom", "zu", "sein", ",", "der", "Born"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "PTKZU", "VAINF", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df k\u00fchn als Dunst von Felsen st\u00e4uben.", "tokens": ["Mu\u00df", "k\u00fchn", "als", "Dunst", "von", "Fel\u00b7sen", "st\u00e4u\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "KOKOM", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So alles dran! So alles drein!", "tokens": ["So", "al\u00b7les", "dran", "!", "So", "al\u00b7les", "drein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKVZ", "$.", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und setzt das Kleine f\u00fcr das Gro\u00dfe!", "tokens": ["Und", "setzt", "das", "Klei\u00b7ne", "f\u00fcr", "das", "Gro\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gott aber soll der W\u00fcrfler sein!", "tokens": ["Gott", "a\u00b7ber", "soll", "der", "W\u00fcrf\u00b7ler", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wirft die Millionen Lose.", "tokens": ["Er", "wirft", "die", "Mil\u00b7lion\u00b7en", "Lo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "So alles dran! So alles drein!", "tokens": ["So", "al\u00b7les", "dran", "!", "So", "al\u00b7les", "drein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKVZ", "$.", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und setzt das Kurze f\u00fcr das Lange!", "tokens": ["Und", "setzt", "das", "Kur\u00b7ze", "f\u00fcr", "das", "Lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gott aber soll der W\u00fcrfler sein!", "tokens": ["Gott", "a\u00b7ber", "soll", "der", "W\u00fcrf\u00b7ler", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei dieser Schanzung seid nicht bange.", "tokens": ["Bei", "die\u00b7ser", "Schan\u00b7zung", "seid", "nicht", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}