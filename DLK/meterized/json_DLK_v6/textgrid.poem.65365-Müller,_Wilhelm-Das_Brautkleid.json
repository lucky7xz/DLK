{"textgrid.poem.65365": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Brautkleid", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flur hat angezogen", "tokens": ["Die", "Flur", "hat", "an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein gr\u00fcnes seidenes Kleid,", "tokens": ["Ein", "gr\u00fc\u00b7nes", "sei\u00b7de\u00b7nes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die leichten schillernden Falten", "tokens": ["Die", "leich\u00b7ten", "schil\u00b7lern\u00b7den", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Umfliegen sie weit und breit.", "tokens": ["Um\u00b7flie\u00b7gen", "sie", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und unter der flatternden H\u00fclle", "tokens": ["Und", "un\u00b7ter", "der", "flat\u00b7tern\u00b7den", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Schl\u00e4gt ihre warme Brust,", "tokens": ["Schl\u00e4gt", "ih\u00b7re", "war\u00b7me", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Winde wollen sie k\u00fchlen", "tokens": ["Die", "Win\u00b7de", "wol\u00b7len", "sie", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und vergl\u00fchen sich selber in Lust.", "tokens": ["Und", "ver\u00b7gl\u00fc\u00b7hen", "sich", "sel\u00b7ber", "in", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Es zucken die Sonnenstrahlen", "tokens": ["Es", "zu\u00b7cken", "die", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herunter mit blitzendem Brand,", "tokens": ["Her\u00b7un\u00b7ter", "mit", "blit\u00b7zen\u00b7dem", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Als m\u00f6chten sie gern ihr versengen", "tokens": ["Als", "m\u00f6ch\u00b7ten", "sie", "gern", "ihr", "ver\u00b7sen\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das neidische gr\u00fcne Gewand.", "tokens": ["Das", "nei\u00b7di\u00b7sche", "gr\u00fc\u00b7ne", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Sie ruft: Ihr Strahlen, ihr Winde,", "tokens": ["Sie", "ruft", ":", "Ihr", "Strah\u00b7len", ",", "ihr", "Win\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Kleid la\u00dft unversehrt!", "tokens": ["Mein", "Kleid", "la\u00dft", "un\u00b7ver\u00b7sehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ward von meinem Liebsten", "tokens": ["Es", "ward", "von", "mei\u00b7nem", "Liebs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Brautschmuck mir bescheert.", "tokens": ["Zum", "Brautsc\u00b7hmuck", "mir", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Mai, so hei\u00dft mein Liebster,", "tokens": ["Der", "Mai", ",", "so", "hei\u00dft", "mein", "Liebs\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er gab es zu tragen mir,", "tokens": ["Er", "gab", "es", "zu", "tra\u00b7gen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er sprach: Du sollst es tragen,", "tokens": ["Er", "sprach", ":", "Du", "sollst", "es", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lang' ich bleibe bei dir.", "tokens": ["So", "lang'", "ich", "blei\u00b7be", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und wenn ich von dir scheide,", "tokens": ["Und", "wenn", "ich", "von", "dir", "schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So werd' es gelb vor Gram,", "tokens": ["So", "werd'", "es", "gelb", "vor", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann la\u00df es von den Menschen", "tokens": ["Dann", "la\u00df", "es", "von", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir ausziehn ohne Scham.", "tokens": ["Dir", "aus\u00b7ziehn", "oh\u00b7ne", "Scham", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und leg' als nackte Witwe", "tokens": ["Und", "leg'", "als", "nack\u00b7te", "Wit\u00b7we"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich nieder mit deinem Leid,", "tokens": ["Dich", "nie\u00b7der", "mit", "dei\u00b7nem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis da\u00df ich wieder kehre", "tokens": ["Bis", "da\u00df", "ich", "wie\u00b7der", "keh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bring' ein neues Kleid.", "tokens": ["Und", "bring'", "ein", "neu\u00b7es", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}