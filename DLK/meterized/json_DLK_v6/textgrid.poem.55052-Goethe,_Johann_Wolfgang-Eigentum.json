{"textgrid.poem.55052": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Eigentum", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df, da\u00df mir nichts angeh\u00f6rt", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "mir", "nichts", "an\u00b7ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als der Gedanke, der ungest\u00f6rt", "tokens": ["Als", "der", "Ge\u00b7dan\u00b7ke", ",", "der", "un\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aus meiner Seele will flie\u00dfen,", "tokens": ["Aus", "mei\u00b7ner", "See\u00b7le", "will", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und jeder g\u00fcnstige Augenblick,", "tokens": ["Und", "je\u00b7der", "g\u00fcns\u00b7ti\u00b7ge", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den mich ein liebendes Geschick", "tokens": ["Den", "mich", "ein", "lie\u00b7ben\u00b7des", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Grund aus l\u00e4\u00dft genie\u00dfen.", "tokens": ["Von", "Grund", "aus", "l\u00e4\u00dft", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}