{"textgrid.poem.27792": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man sagt, man lieset viel, wie da\u00df f\u00fcr langen Jahren", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, man lieset viel, wie da\u00df f\u00fcr langen Jahren", "tokens": ["Man", "sagt", ",", "man", "lie\u00b7set", "viel", ",", "wie", "da\u00df", "f\u00fcr", "lan\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "ADV", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Zeiten ein gantz Volk aus seinem Sitz gefahren", "tokens": ["Zu", "Zei\u00b7ten", "ein", "gantz", "Volk", "aus", "sei\u00b7nem", "Sitz", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und neues Land gesucht. Hinf\u00fcro wird man sagen", "tokens": ["Und", "neu\u00b7es", "Land", "ge\u00b7sucht", ".", "Hin\u00b7f\u00fc\u00b7ro", "wird", "man", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVPP", "$.", "ADV", "VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was andres: Wie man sah bei uns in vielen Tagen", "tokens": ["Was", "and\u00b7res", ":", "Wie", "man", "sah", "bei", "uns", "in", "vie\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "$.", "PWAV", "PIS", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vom Land Holtz, Stein, Ziehn, Blei, Gold, Silber, Kupffer, Eisen,", "tokens": ["Vom", "Land", "Holtz", ",", "Stein", ",", "Ziehn", ",", "Blei", ",", "Gold", ",", "Sil\u00b7ber", ",", "Kupf\u00b7fer", ",", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "NN", "$,", "VVFIN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fleisch, Brot, Tranck und was nicht? hin in die St\u00e4te reisen.", "tokens": ["Fleisch", ",", "Brot", ",", "Tranck", "und", "was", "nicht", "?", "hin", "in", "die", "St\u00e4\u00b7te", "rei\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "PWS", "PTKNEG", "$.", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}