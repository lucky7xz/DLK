{"textgrid.poem.56997": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Weste", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lebt in S\u00fcditalien eine Weste", "tokens": ["Es", "lebt", "in", "S\u00fc\u00b7di\u00b7ta\u00b7li\u00b7en", "ei\u00b7ne", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "an einer Kirche d\u00e4mmrigem Altar.", "tokens": ["an", "ei\u00b7ner", "Kir\u00b7che", "d\u00e4mm\u00b7ri\u00b7gem", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versteht mich recht: Noch dient sie Gott aufs beste.", "tokens": ["Ver\u00b7steht", "mich", "recht", ":", "Noch", "dient", "sie", "Gott", "aufs", "bes\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PPER", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wie in Adam schon Herr Haeckel war,", "tokens": ["Doch", "wie", "in", "A\u00b7dam", "schon", "Herr", "Hae\u00b7ckel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "ADV", "NN", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "(zum Beispiel blo\u00df), so steckt in diesem Reste", "tokens": ["(", "zum", "Bei\u00b7spiel", "blo\u00df", ")", ",", "so", "steckt", "in", "die\u00b7sem", "Res\u00b7te"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "ADV", "$(", "$,", "ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Brokat voll Silberbl\u00fcmlein wunderbar", "tokens": ["Bro\u00b7kat", "voll", "Sil\u00b7ber\u00b7bl\u00fcm\u00b7lein", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "schon heut der krause \u00dcbergang verborgen", "tokens": ["schon", "heut", "der", "krau\u00b7se", "\u00dc\u00b7ber\u00b7gang", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "vom Geist von gestern auf den Leib von morgen.", "tokens": ["vom", "Geist", "von", "ge\u00b7stern", "auf", "den", "Leib", "von", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "APPR", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}