{"textgrid.poem.45194": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[als liberal einst der Verfolgung Ziel]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als liberal einst der Verfolgung Ziel,", "tokens": ["Als", "li\u00b7be\u00b7ral", "einst", "der", "Ver\u00b7fol\u00b7gung", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schilt mich der Freiheitstaumel nur servil,", "tokens": ["Schilt", "mich", "der", "Frei\u00b7heits\u00b7tau\u00b7mel", "nur", "ser\u00b7vil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht hier noch dort in den Extremen z\u00fcnftig,", "tokens": ["Nicht", "hier", "noch", "dort", "in", "den", "Ext\u00b7re\u00b7men", "z\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich glaube bald, ich bin vern\u00fcnftig.", "tokens": ["Ich", "glau\u00b7be", "bald", ",", "ich", "bin", "ver\u00b7n\u00fcnf\u00b7tig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}