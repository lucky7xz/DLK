{"dta.poem.7091": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wei\u00df allhier die Seele nicht,", "tokens": ["Es", "wei\u00df", "all\u00b7hier", "die", "See\u00b7le", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf welche Weise sie sich in den C\u00f6rper flicht,", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "sie", "sich", "in", "den", "C\u00f6r\u00b7per", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf welche Weise sie mit ihm verbunden,", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "sie", "mit", "ihm", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie wei\u00df nicht, wie sie ihm Gestalt und Wachsthum gebe,", "tokens": ["Sie", "wei\u00df", "nicht", ",", "wie", "sie", "ihm", "Ge\u00b7stalt", "und", "Wach\u00b7sthum", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie wei\u00df nicht, wie sie ihn belebe.", "tokens": ["Sie", "wei\u00df", "nicht", ",", "wie", "sie", "ihn", "be\u00b7le\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist ihr unbekannt, sie kanns hier nicht ergr\u00fcnden,", "tokens": ["Es", "ist", "ihr", "un\u00b7be\u00b7kannt", ",", "sie", "kanns", "hier", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit was vor Art dem C\u00f6rper das Empfinden,", "tokens": ["Mit", "was", "vor", "Art", "dem", "C\u00f6r\u00b7per", "das", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und die Geschicklichkeit sich zu bewegen,", "tokens": ["Und", "die", "Ge\u00b7schick\u00b7lich\u00b7keit", "sich", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sie selbst geschickt sey beyzulegen:", "tokens": ["Sie", "selbst", "ge\u00b7schickt", "sey", "bey\u00b7zu\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Kr\u00e4ffte selbst, wodurch sie seiner Meister,", "tokens": ["Die", "Kr\u00e4ff\u00b7te", "selbst", ",", "wo\u00b7durch", "sie", "sei\u00b7ner", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Erkennet ihre Schwachheit nicht,", "tokens": ["Er\u00b7ken\u00b7net", "ih\u00b7re", "Schwach\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die immer-rege Lebens-Geister,", "tokens": ["Die", "im\u00b7mer\u00b7re\u00b7ge", "Le\u00b7bens\u00b7Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sind ihr so gut als unbekannt.", "tokens": ["Sind", "ihr", "so", "gut", "als", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Jhr eigner Trieb, durch welchen sie befiehlt,", "tokens": ["Ihr", "eig\u00b7ner", "Trieb", ",", "durch", "wel\u00b7chen", "sie", "be\u00b7fiehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PWAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Verbirget sich vor ihr, sie kan ihn nicht entdecken,", "tokens": ["Ver\u00b7bir\u00b7get", "sich", "vor", "ihr", ",", "sie", "kan", "ihn", "nicht", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "So wenig als die Kr\u00e4fft\u2019, so ihren Winck vollstrecken.", "tokens": ["So", "we\u00b7nig", "als", "die", "Kr\u00e4fft'", ",", "so", "ih\u00b7ren", "Win\u00b7ck", "voll\u00b7stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}