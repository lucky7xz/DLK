{"textgrid.poem.37104": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Maylied", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tanzt dem sch\u00f6nen May entgegen,", "tokens": ["Tanzt", "dem", "sch\u00f6\u00b7nen", "May", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der des Waldes Haar verneut,", "tokens": ["Der", "des", "Wal\u00b7des", "Haar", "ver\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Roth und wei\u00dfe Farbenb\u00f6gen", "tokens": ["Roth", "und", "wei\u00b7\u00dfe", "Far\u00b7ben\u00b7b\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Fruchtbaums Wipfel streut,", "tokens": ["Auf", "des", "Frucht\u00b7baums", "Wip\u00b7fel", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem goldverbr\u00e4mten Schleier,", "tokens": ["Mit", "dem", "gold\u00b7ver\u00b7br\u00e4m\u00b7ten", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wartende Gefilde deckt!", "tokens": ["War\u00b7ten\u00b7de", "Ge\u00b7fil\u00b7de", "deckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Singt ihm Hymnen in die Leyer,", "tokens": ["Singt", "ihm", "Hym\u00b7nen", "in", "die", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Der den Schlaf der Freude weckt!", "tokens": ["Der", "den", "Schlaf", "der", "Freu\u00b7de", "weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tanzt daher, am Arm der Sch\u00f6ne,", "tokens": ["Tanzt", "da\u00b7her", ",", "am", "Arm", "der", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ein treuer Busen fr\u00f6hnt,", "tokens": ["Der", "ein", "treu\u00b7er", "Bu\u00b7sen", "fr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Menget Lieder ins Get\u00f6ne,", "tokens": ["Men\u00b7get", "Lie\u00b7der", "ins", "Ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das die Morgenglocke t\u00f6nt,", "tokens": ["Das", "die", "Mor\u00b7gen\u00b7glo\u00b7cke", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ins Geschwirr der Espenbl\u00e4tter,", "tokens": ["Ins", "Ge\u00b7schwirr", "der", "Es\u00b7pen\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erweckt den Wiederklang!", "tokens": ["Und", "er\u00b7weckt", "den", "Wie\u00b7der\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er, der Freund der Liebesg\u00f6tter,", "tokens": ["Er", ",", "der", "Freund", "der", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heischet Opfer und Gesang.", "tokens": ["Hei\u00b7schet", "Op\u00b7fer", "und", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bringet ihm in gr\u00fcnen Schatten,", "tokens": ["Brin\u00b7get", "ihm", "in", "gr\u00fc\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure Fr\u00fchlingsopfer dar,", "tokens": ["Eu\u00b7re", "Fr\u00fch\u00b7lings\u00b7op\u00b7fer", "dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Junge neuverm\u00e4hlte Gatten,", "tokens": ["Jun\u00b7ge", "neu\u00b7ver\u00b7m\u00e4hl\u00b7te", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Liebe Festaltar:", "tokens": ["Auf", "der", "Lie\u00b7be", "Fes\u00b7tal\u00b7tar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fc\u00dfe, wenn des Hahns Drommete", "tokens": ["K\u00fc\u00b7\u00dfe", ",", "wenn", "des", "Hahns", "Drom\u00b7me\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das umb\u00fcschte D\u00f6rfchen weckt,", "tokens": ["Das", "um\u00b7b\u00fcschte", "D\u00f6rf\u00b7chen", "weckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "K\u00fc\u00dfe, wenn die Abendr\u00f6the", "tokens": ["K\u00fc\u00b7\u00dfe", ",", "wenn", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeden Baum mit Purpur deckt.", "tokens": ["Je\u00b7den", "Baum", "mit", "Pur\u00b7pur", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Flieht, ihr sch\u00f6nen St\u00e4dterinnen,", "tokens": ["Flieht", ",", "ihr", "sch\u00f6\u00b7nen", "St\u00e4d\u00b7te\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eurer St\u00e4dte goldne Kluft,", "tokens": ["Eu\u00b7rer", "St\u00e4d\u00b7te", "gold\u00b7ne", "Kluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eurer Kerker hohe Zinnen,", "tokens": ["Eu\u00b7rer", "Ker\u00b7ker", "ho\u00b7he", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinket frische Mayenluft!", "tokens": ["Trin\u00b7ket", "fri\u00b7sche", "Ma\u00b7yen\u00b7luft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Irrt, mit eurem Sonnenh\u00fctgen,", "tokens": ["Irrt", ",", "mit", "eu\u00b7rem", "Son\u00b7nen\u00b7h\u00fct\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf die Fr\u00fchlingsflur hinaus!", "tokens": ["Auf", "die", "Fr\u00fch\u00b7lings\u00b7flur", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Singt ein fr\u00f6lich Mayenliedgen,", "tokens": ["Singt", "ein", "fr\u00f6\u00b7lich", "Ma\u00b7yen\u00b7lied\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pfl\u00fccket einen Busenstraus!", "tokens": ["Pfl\u00fc\u00b7cket", "ei\u00b7nen", "Bu\u00b7sen\u00b7straus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schm\u00fcckt mit Kirschenbl\u00fcthenzweigen", "tokens": ["Schm\u00fcckt", "mit", "Kir\u00b7schen\u00b7bl\u00fc\u00b7then\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euren gr\u00fcnen Sonnenhut,", "tokens": ["Eu\u00b7ren", "gr\u00fc\u00b7nen", "Son\u00b7nen\u00b7hut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fcrzt das R\u00f6ckchen, tanzet Reigen,", "tokens": ["Sch\u00fcrzt", "das", "R\u00f6ck\u00b7chen", ",", "tan\u00b7zet", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Sch\u00e4ferjugend thut!", "tokens": ["Wie", "die", "Sch\u00e4\u00b7fer\u00b7ju\u00b7gend", "thut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gaukelt in der Kirschenbl\u00fcthe,", "tokens": ["Gau\u00b7kelt", "in", "der", "Kir\u00b7schen\u00b7bl\u00fct\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zephyrn, eure Fl\u00fcgel matt,", "tokens": ["Ze\u00b7phyrn", ",", "eu\u00b7re", "Fl\u00fc\u00b7gel", "matt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Haucht auf ihre Sonnenh\u00fcte", "tokens": ["Haucht", "auf", "ih\u00b7re", "Son\u00b7nen\u00b7h\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Manches wei\u00dfe Bl\u00fcthenblatt.", "tokens": ["Man\u00b7ches", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7then\u00b7blatt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}