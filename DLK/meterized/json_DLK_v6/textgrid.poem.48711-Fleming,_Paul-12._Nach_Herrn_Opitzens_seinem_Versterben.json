{"textgrid.poem.48711": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12. Nach Herrn Opitzens seinem Versterben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um ", "tokens": ["Um"], "token_info": ["word"], "pos": ["KOUI"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "von mehren wei\u00df ich nit. Der Meister teutscher Lieder,", "tokens": ["von", "meh\u00b7ren", "wei\u00df", "ich", "nit", ".", "Der", "Meis\u00b7ter", "teut\u00b7scher", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das Wunder unsrer Zeit, legt seine Harfe nieder,", "tokens": ["das", "Wun\u00b7der", "uns\u00b7rer", "Zeit", ",", "legt", "sei\u00b7ne", "Har\u00b7fe", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "di\u00df war sein letzter Ton. Nun, Welt, bewahr dich Gott.", "tokens": ["di\u00df", "war", "sein", "letz\u00b7ter", "Ton", ".", "Nun", ",", "Welt", ",", "be\u00b7wahr", "dich", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie stehn best\u00fcrzt, erstarrt, verstummt, itzt bla\u00df, itzt rot,", "tokens": ["Sie", "stehn", "be\u00b7st\u00fcrzt", ",", "er\u00b7starrt", ",", "ver\u00b7stummt", ",", "itzt", "bla\u00df", ",", "itzt", "rot", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die deutschen Klarien um ihre sch\u00f6nsten Br\u00fcder.", "tokens": ["die", "deut\u00b7schen", "Kla\u00b7ri\u00b7en", "um", "ih\u00b7re", "sch\u00f6ns\u00b7ten", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00f6mmt ein Olivenzweig aus Persien nicht wieder,", "tokens": ["K\u00f6mmt", "ein", "O\u00b7liv\u00b7en\u00b7zweig", "aus", "Per\u00b7si\u00b7en", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so steht ihr Lorberwald in seiner letzten Not.", "tokens": ["so", "steht", "ihr", "Lor\u00b7ber\u00b7wald", "in", "sei\u00b7ner", "letz\u00b7ten", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Regt kein Geist denn sich mehr? Und ist uns andern Allen", "tokens": ["Regt", "kein", "Geist", "denn", "sich", "mehr", "?", "Und", "ist", "uns", "an\u00b7dern", "Al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "KON", "PRF", "ADV", "$.", "KON", "VAFIN", "PPER", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "in diesen Mut und Lust und Hoffnung ganz gefallen?", "tokens": ["in", "die\u00b7sen", "Mut", "und", "Lust", "und", "Hoff\u00b7nung", "ganz", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wen aber klag' ich an? Verzeih mir dieses doch,", "tokens": ["Wen", "a\u00b7ber", "klag'", "ich", "an", "?", "Ver\u00b7zeih", "mir", "die\u00b7ses", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "VVIMP", "PPER", "PDS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "da\u00df mein Ged\u00e4chtn\u00fc\u00df stutzt; es sind f\u00fcnf ganzer Jahre,", "tokens": ["da\u00df", "mein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "stutzt", ";", "es", "sind", "f\u00fcnf", "gan\u00b7zer", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df ich, o Vaterland, fast nichts von dir erfahre.", "tokens": ["da\u00df", "ich", ",", "o", "Va\u00b7ter\u00b7land", ",", "fast", "nichts", "von", "dir", "er\u00b7fah\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "$,", "ADV", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}}}}