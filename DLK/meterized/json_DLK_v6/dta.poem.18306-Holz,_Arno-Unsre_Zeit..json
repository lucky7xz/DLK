{"dta.poem.18306": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Unsre Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die sich als \u201eMistre\u00df\u201c produzirt,", "tokens": ["Die", "sich", "als", "\u201e", "Mis\u00b7tre\u00df", "\u201c", "pro\u00b7du\u00b7zirt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "KOUS", "$(", "NN", "$(", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Simpelfransen vor der Stirne", "tokens": ["Mit", "Sim\u00b7pel\u00b7fran\u00b7sen", "vor", "der", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schauderhaft decolletirt.", "tokens": ["Und", "schau\u00b7der\u00b7haft", "de\u00b7col\u00b7le\u00b7tirt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie raubt uns alle Illusionen,", "tokens": ["Sie", "raubt", "uns", "al\u00b7le", "Il\u00b7lu\u00b7si\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie turnt Trapez und paukt Klavier", "tokens": ["Sie", "turnt", "Tra\u00b7pez", "und", "paukt", "Kla\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und macht aus Fensterglas Kanonen", "tokens": ["Und", "macht", "aus", "Fens\u00b7ter\u00b7glas", "Ka\u00b7no\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Kronjuwelen aus Papier!", "tokens": ["Und", "Kron\u00b7ju\u00b7we\u00b7len", "aus", "Pa\u00b7pier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}