{"textgrid.poem.41430": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Condor und die Staaren", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Recht verhalten sich die Herren kleiner Staaten,", "tokens": ["Mit", "Recht", "ver\u00b7hal\u00b7ten", "sich", "die", "Her\u00b7ren", "klei\u00b7ner", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wie die gro\u00dfen Potentaten;", "tokens": ["So", "wie", "die", "gro\u00b7\u00dfen", "Po\u00b7ten\u00b7ta\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sind die Kl\u00fcgsten jederzeit", "tokens": ["Doch", "sind", "die", "Kl\u00fcgs\u00b7ten", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Recht auch eingedenk, in Worten und in Thaten,", "tokens": ["Mit", "Recht", "auch", "ein\u00b7ge\u00b7denk", ",", "in", "Wor\u00b7ten", "und", "in", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der unvollkommnen Aehnlichkeit.", "tokens": ["Der", "un\u00b7voll\u00b7komm\u00b7nen", "A\u00b7ehn\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es r\u00fcstete, vor vielen Jahren,", "tokens": ["Es", "r\u00fcs\u00b7te\u00b7te", ",", "vor", "vie\u00b7len", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der gro\u00dfe Condor sich zum Krieg,", "tokens": ["Der", "gro\u00b7\u00dfe", "Con\u00b7dor", "sich", "zum", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und er versammelte der edlen V\u00f6gel Schaaren,", "tokens": ["Und", "er", "ver\u00b7sam\u00b7mel\u00b7te", "der", "ed\u00b7len", "V\u00f6\u00b7gel", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die f\u00fcrchterlich, gewohnt zum Sieg,", "tokens": ["Die", "f\u00fcrch\u00b7ter\u00b7lich", ",", "ge\u00b7wohnt", "zum", "Sieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dieses Haubtes w\u00fcrdig waren.", "tokens": ["Und", "die\u00b7ses", "Haub\u00b7tes", "w\u00fcr\u00b7dig", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zugleich erschien ein Schwarm von Staaren,", "tokens": ["Zu\u00b7gleich", "er\u00b7schien", "ein", "Schwarm", "von", "Staa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und rief, einm\u00fcthig im Geschrei:", "tokens": ["Und", "rief", ",", "ein\u00b7m\u00fct\u00b7hig", "im", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir stimmen diesem Kriege bei,", "tokens": ["Wir", "stim\u00b7men", "die\u00b7sem", "Krie\u00b7ge", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Um, wie der Condor, zu verfahren.", "tokens": ["Um", ",", "wie", "der", "Con\u00b7dor", ",", "zu", "ver\u00b7fah\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "$,", "PWAV", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So waffnete sich auch Germanien zu Siegen,", "tokens": ["So", "waff\u00b7ne\u00b7te", "sich", "auch", "Ger\u00b7ma\u00b7ni\u00b7en", "zu", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und, um das Haus Bourbon begl\u00fcckter zu bekriegen,", "tokens": ["Und", ",", "um", "das", "Haus", "Bour\u00b7bon", "be\u00b7gl\u00fcck\u00b7ter", "zu", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "NE", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geht Bund und Reichstag an: der Feldzug wird beliebt.", "tokens": ["Geht", "Bund", "und", "Reichs\u00b7tag", "an", ":", "der", "Feld\u00b7zug", "wird", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als jeder Stand nun seine Stimme gibt,", "tokens": ["Als", "je\u00b7der", "Stand", "nun", "sei\u00b7ne", "Stim\u00b7me", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verhei\u00dfen Oesterreichs Gesandte", "tokens": ["Ver\u00b7hei\u00b7\u00dfen", "O\u00b7es\u00b7ter\u00b7reichs", "Ge\u00b7sand\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein Heer von drei\u00dfigtausend Mann.", "tokens": ["Ein", "Heer", "von", "drei\u00b7\u00dfig\u00b7tau\u00b7send", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein bisch\u00f6flicher h\u00f6rt es an:", "tokens": ["Ein", "bi\u00b7sch\u00f6f\u00b7li\u00b7cher", "h\u00f6rt", "es", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Und, als der Aufruf nun auch ihn zum Stimmen nannte,", "tokens": ["Und", ",", "als", "der", "Auf\u00b7ruf", "nun", "auch", "ihn", "zum", "Stim\u00b7men", "nann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hatt' er es sich gemerkt; denn er votirt sogleich:", "tokens": ["Hatt'", "er", "es", "sich", "ge\u00b7merkt", ";", "denn", "er", "vo\u00b7tirt", "sog\u00b7leich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PRF", "VVPP", "$.", "KON", "PPER", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}