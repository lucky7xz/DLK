{"dta.poem.1366": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr/ die die harte Last der schweren S\u00fcnde dr\u00fcckt/", "tokens": ["Ihr", "/", "die", "die", "har\u00b7te", "Last", "der", "schwe\u00b7ren", "S\u00fcn\u00b7de", "dr\u00fcckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihr mit Hertzens-Angst m\u00fchselig seyd beladen/", "tokens": ["Die", "ihr", "mit", "Hert\u00b7zens\u00b7Angst", "m\u00fch\u00b7se\u00b7lig", "seyd", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kommt/ spricht der HErr/ kommt her zum Tische meiner Gna-", "tokens": ["Kommt", "/", "spricht", "der", "Herr", "/", "kommt", "her", "zum", "Ti\u00b7sche", "mei\u00b7ner", "Gna"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "ART", "NN", "$(", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "So werdet ihr mit Trost und H\u00fclffe seyn erquickt/", "tokens": ["So", "wer\u00b7det", "ihr", "mit", "Trost", "und", "H\u00fclf\u00b7fe", "seyn", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Seele wird euch rein/ das Hertze leicht gemacht/", "tokens": ["Die", "See\u00b7le", "wird", "euch", "rein", "/", "das", "Hert\u00b7ze", "leicht", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$(", "PDS", "VVFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das Brod/ das ich euch will zur s\u00fcssen Speise schencken/", "tokens": ["Das", "Brod", "/", "das", "ich", "euch", "will", "zur", "s\u00fcs\u00b7sen", "Spei\u00b7se", "schen\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "PPER", "VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist mein selbst eigner Leib/ vor eure Schuld geschlacht/", "tokens": ["Ist", "mein", "selbst", "eig\u00b7ner", "Leib", "/", "vor", "eu\u00b7re", "Schuld", "ge\u00b7schlacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADV", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Tranck/ damit ich will die matten Glieder tr\u00e4ncken/", "tokens": ["Der", "Tranck", "/", "da\u00b7mit", "ich", "will", "die", "mat\u00b7ten", "Glie\u00b7der", "tr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist mein selbst eignes Blutt/ am hohen Creutz-Altar", "tokens": ["Ist", "mein", "selbst", "eig\u00b7nes", "Blutt", "/", "am", "ho\u00b7hen", "Creutz\u00b7Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADV", "ADJA", "NN", "$(", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vor eure Missethat geopffert und vergossen.", "tokens": ["Vor", "eu\u00b7re", "Mis\u00b7se\u00b7that", "ge\u00b7opf\u00b7fert", "und", "ver\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O JEsu/ was du sagst/ ist ohngezweifelt wahr/", "tokens": ["O", "Je\u00b7su", "/", "was", "du", "sagst", "/", "ist", "ohn\u00b7ge\u00b7zwei\u00b7felt", "wahr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PWS", "PPER", "VVFIN", "$(", "VAFIN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Worte/ welche selbst aus deinem Munde flossen/", "tokens": ["Die", "Wor\u00b7te", "/", "wel\u00b7che", "selbst", "aus", "dei\u00b7nem", "Mun\u00b7de", "flos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sind ohne Widerstreit. Wie schwach ich immer bin/", "tokens": ["Sind", "oh\u00b7ne", "Wi\u00b7der\u00b7streit", ".", "Wie", "schwach", "ich", "im\u00b7mer", "bin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie sehr sich die Vernunfft dir will entgegen setzen/", "tokens": ["Wie", "sehr", "sich", "die", "Ver\u00b7nunfft", "dir", "will", "ent\u00b7ge\u00b7gen", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PRF", "ART", "NN", "PPER", "VMFIN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So will ich solche doch in keinen Zweifel ziehn/", "tokens": ["So", "will", "ich", "sol\u00b7che", "doch", "in", "kei\u00b7nen", "Zwei\u00b7fel", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vor Wercke deiner Gnad und deiner Allmacht sch\u00e4tzen.", "tokens": ["Vor", "Wer\u00b7cke", "dei\u00b7ner", "Gnad", "und", "dei\u00b7ner", "All\u00b7macht", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich fasse sie mit Lust von deinen Lippen auff/", "tokens": ["Ich", "fas\u00b7se", "sie", "mit", "Lust", "von", "dei\u00b7nen", "Lip\u00b7pen", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und lasse sie erfreut durch Marck und Seele dringen.", "tokens": ["Und", "las\u00b7se", "sie", "er\u00b7freut", "durch", "Marck", "und", "See\u00b7le", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich eil/ O HErr/ zu dir/ doch hemmen meinen Lauff", "tokens": ["Ich", "eil", "/", "O", "Herr", "/", "zu", "dir", "/", "doch", "hem\u00b7men", "mei\u00b7nen", "Lauff"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$(", "NE", "NN", "$(", "APPR", "PPER", "$(", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die S\u00fcnden/ welche mich zum \u00f6fftern Falle zwingen/", "tokens": ["Die", "S\u00fcn\u00b7den", "/", "wel\u00b7che", "mich", "zum", "\u00f6ff\u00b7tern", "Fal\u00b7le", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die \u00fcberh\u00e4uffte Schuld/ so mein Gewissen nagt/", "tokens": ["Die", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7te", "Schuld", "/", "so", "mein", "Ge\u00b7wis\u00b7sen", "nagt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Spricht/ da\u00df ich deiner Gunst ohnw\u00fcrdig sey zu achten/", "tokens": ["Spricht", "/", "da\u00df", "ich", "dei\u00b7ner", "Gunst", "ohn\u00b7w\u00fcr\u00b7dig", "sey", "zu", "ach\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Macht/ da\u00df ich mich zu dir zu finden bin verzagt/", "tokens": ["Macht", "/", "da\u00df", "ich", "mich", "zu", "dir", "zu", "fin\u00b7den", "bin", "ver\u00b7zagt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und mein betr\u00fcbter Geist von Aengsten will verschmachten.", "tokens": ["Und", "mein", "be\u00b7tr\u00fcb\u00b7ter", "Geist", "von", "A\u00b7engs\u00b7ten", "will", "ver\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und dennoch wiltu/ HErr/ ich soll mich finden ein/", "tokens": ["Und", "den\u00b7noch", "wil\u00b7tu", "/", "Herr", "/", "ich", "soll", "mich", "fin\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "$(", "NN", "$(", "PPER", "VMFIN", "PPER", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Auff deine G\u00fcttigkeit vertr\u00f6sten mein Gewissen/", "tokens": ["Auff", "dei\u00b7ne", "G\u00fct\u00b7tig\u00b7keit", "ver\u00b7tr\u00f6s\u00b7ten", "mein", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und offtermahls ein Gast bey deiner Taffel seyn/", "tokens": ["Und", "off\u00b7ter\u00b7mahls", "ein", "Gast", "bey", "dei\u00b7ner", "Taf\u00b7fel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Im fall ich deiner Gnad und G\u00fctter will gen\u00fcssen/", "tokens": ["Im", "fall", "ich", "dei\u00b7ner", "Gnad", "und", "G\u00fct\u00b7ter", "will", "ge\u00b7n\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPOSAT", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dein G\u00f6ttlich Himmel-Brod soll meinen schwachen Geist", "tokens": ["Dein", "G\u00f6tt\u00b7lich", "Him\u00b7mel\u00b7Brod", "soll", "mei\u00b7nen", "schwa\u00b7chen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mit h\u00f6chster S\u00fc\u00dfigkeit zum ewgen Leben st\u00e4rcken.", "tokens": ["Mit", "h\u00f6chs\u00b7ter", "S\u00fc\u00b7\u00dfig\u00b7keit", "zum", "ew\u00b7gen", "Le\u00b7ben", "st\u00e4r\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ach HErr/ wer bin ich doch/ dem du so viel verhei\u00dft/", "tokens": ["Ach", "Herr", "/", "wer", "bin", "ich", "doch", "/", "dem", "du", "so", "viel", "ver\u00b7hei\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "PWS", "VAFIN", "PPER", "ADV", "$(", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie hab ich di\u00df verdient mit meinen schn\u00f6den Wercken!", "tokens": ["Wie", "hab", "ich", "di\u00df", "ver\u00b7dient", "mit", "mei\u00b7nen", "schn\u00f6\u00b7den", "Wer\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ihr/ die die harte Last der schweren S\u00fcnde dr\u00fcckt/", "tokens": ["Ihr", "/", "die", "die", "har\u00b7te", "Last", "der", "schwe\u00b7ren", "S\u00fcn\u00b7de", "dr\u00fcckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die ihr mit Hertzens-Angst m\u00fchselig seyd beladen/", "tokens": ["Die", "ihr", "mit", "Hert\u00b7zens\u00b7Angst", "m\u00fch\u00b7se\u00b7lig", "seyd", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Kommt/ sagstu/ her zu mir/ zum Tische meiner Gnaden/", "tokens": ["Kommt", "/", "sags\u00b7tu", "/", "her", "zu", "mir", "/", "zum", "Ti\u00b7sche", "mei\u00b7ner", "Gna\u00b7den", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "ADV", "APPR", "PPER", "$(", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So werdet ihr mit Trost und H\u00fclffe seyn erquickt/", "tokens": ["So", "wer\u00b7det", "ihr", "mit", "Trost", "und", "H\u00fclf\u00b7fe", "seyn", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Seele wird euch rein/ das Hertze leicht gemacht/", "tokens": ["Die", "See\u00b7le", "wird", "euch", "rein", "/", "das", "Hert\u00b7ze", "leicht", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$(", "PDS", "VVFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "O s\u00fcsse Liebes-Wort! O Worte voller Leben!", "tokens": ["O", "s\u00fcs\u00b7se", "Lie\u00b7bes\u00b7Wort", "!", "O", "Wor\u00b7te", "vol\u00b7ler", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein armer S\u00fcnder wird an deine Taffel bracht/", "tokens": ["Ein", "ar\u00b7mer", "S\u00fcn\u00b7der", "wird", "an", "dei\u00b7ne", "Taf\u00b7fel", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du wilt ihm selber dich zur edlen Speise geben.", "tokens": ["Du", "wilt", "ihm", "sel\u00b7ber", "dich", "zur", "ed\u00b7len", "Spei\u00b7se", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Du hast es/ HErr/ gesagt: daran begn\u00fcget mir/", "tokens": ["Du", "hast", "es", "/", "Herr", "/", "ge\u00b7sagt", ":", "da\u00b7ran", "be\u00b7gn\u00fc\u00b7get", "mir", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "NN", "$(", "VVPP", "$.", "PAV", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ob dich die Engel gleich mit h\u00f6chster Ehr-Furcht schauen/", "tokens": ["Ob", "dich", "die", "En\u00b7gel", "gleich", "mit", "h\u00f6chs\u00b7ter", "Ehr\u00b7Furcht", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ob die Gerechten gleich mit Schrecken nahn zu dir/", "tokens": ["Ob", "die", "Ge\u00b7rech\u00b7ten", "gleich", "mit", "Schre\u00b7cken", "nahn", "zu", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "NN", "ADJA", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So kan ich S\u00fcnder doch auff deine Worte trauen/", "tokens": ["So", "kan", "ich", "S\u00fcn\u00b7der", "doch", "auff", "dei\u00b7ne", "Wor\u00b7te", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und zu dir treren hin/ HErr/ nimm mich gn\u00e4dig an/", "tokens": ["Und", "zu", "dir", "tre\u00b7ren", "hin", "/", "Herr", "/", "nimm", "mich", "gn\u00e4\u00b7dig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PTKVZ", "$(", "NN", "$(", "VVIMP", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und weil ich nicht genung mit vorgenommner Reue", "tokens": ["Und", "weil", "ich", "nicht", "ge\u00b7nung", "mit", "vor\u00b7ge\u00b7nomm\u00b7ner", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Die auff geladne Schuld vor dir beweinen kan", "tokens": ["Die", "auff", "ge\u00b7lad\u00b7ne", "Schuld", "vor", "dir", "be\u00b7wei\u00b7nen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Aus S\u00fcnden-voller Art/ mich dannenher mit Scheue", "tokens": ["Aus", "S\u00fcn\u00b7den\u00b7vol\u00b7ler", "Art", "/", "mich", "dan\u00b7nen\u00b7her", "mit", "Scheu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$(", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Vor dir befinden mu\u00df/ und deiner hohen Gunst/", "tokens": ["Vor", "dir", "be\u00b7fin\u00b7den", "mu\u00df", "/", "und", "dei\u00b7ner", "ho\u00b7hen", "Gunst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "O Gnadenreicher HErr/ nur leyder! ohnwerth nennen;", "tokens": ["O", "Gna\u00b7den\u00b7rei\u00b7cher", "Herr", "/", "nur", "ley\u00b7der", "!", "ohn\u00b7werth", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ADV", "ADV", "$.", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Erwecke du in mir der wahren Busse Brunst/", "tokens": ["Er\u00b7we\u00b7cke", "du", "in", "mir", "der", "wah\u00b7ren", "Bus\u00b7se", "Brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und la\u00df mein kaltes Hertz in wahrer And acht brennen.", "tokens": ["Und", "la\u00df", "mein", "kal\u00b7tes", "Hertz", "in", "wah\u00b7rer", "And", "acht", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "CARD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Rei\u00df/ was noch weltlich ist/ von meinen Sinnen aus/", "tokens": ["Rei\u00df", "/", "was", "noch", "welt\u00b7lich", "ist", "/", "von", "mei\u00b7nen", "Sin\u00b7nen", "aus", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWS", "ADV", "ADJD", "VAFIN", "$(", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und la\u00df mich w\u00fcrdiglich zu deiner Taffel gehen/", "tokens": ["Und", "la\u00df", "mich", "w\u00fcr\u00b7dig\u00b7lich", "zu", "dei\u00b7ner", "Taf\u00b7fel", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Und wenn gereinigt ist der Seelen edles Hau\u00df/", "tokens": ["Und", "wenn", "ge\u00b7rei\u00b7nigt", "ist", "der", "See\u00b7len", "ed\u00b7les", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVPP", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "So la\u00df es dir allein zur Wohnung offen stehen!", "tokens": ["So", "la\u00df", "es", "dir", "al\u00b7lein", "zur", "Woh\u00b7nung", "of\u00b7fen", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "ADV", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}