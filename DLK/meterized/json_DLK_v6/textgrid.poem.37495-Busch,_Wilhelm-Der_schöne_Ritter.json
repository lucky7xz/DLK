{"textgrid.poem.37495": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der sch\u00f6ne Ritter", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbheut' werden s' schauen. Alles echt und sitzt famos.\u00ab", "tokens": ["\u00bb", "heut'", "wer\u00b7den", "s'", "schau\u00b7en", ".", "Al\u00b7les", "echt", "und", "sitzt", "fa\u00b7mos", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "NE", "VVINF", "$.", "PIS", "ADJD", "KON", "VVFIN", "ADJD", "$.", "$("], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "\u00bbder verflixte Harnisch, wenn er nur net so drucket!\u00ab", "tokens": ["\u00bb", "der", "ver\u00b7flix\u00b7te", "Har\u00b7nisch", ",", "wenn", "er", "nur", "net", "so", "dru\u00b7cket", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "VVFIN", "$.", "$("], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ritterliche Gro\u00dfmut, welche sich zwei plebejische Masken sehr zu Nutze machen.", "tokens": ["Rit\u00b7ter\u00b7li\u00b7che", "Gro\u00df\u00b7mut", ",", "wel\u00b7che", "sich", "zwei", "ple\u00b7be\u00b7ji\u00b7sche", "Mas\u00b7ken", "sehr", "zu", "Nut\u00b7ze", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "CARD", "ADJA", "NN", "ADV", "PTKZU", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Es kommt zum Kampfe.", "tokens": ["Es", "kommt", "zum", "Kamp\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u00bbkracks, da haben wir's!\u00ab", "tokens": ["\u00bb", "kracks", ",", "da", "ha\u00b7ben", "wir's", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ADV", "VAFIN", "PIS", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbo liebe Moni, helfen S' mir doch um Gottes willen, da\u00df ich das Ding vom Leib krieg'!\u00ab", "tokens": ["\u00bb", "o", "lie\u00b7be", "Mo\u00b7ni", ",", "hel\u00b7fen", "S'", "mir", "doch", "um", "Got\u00b7tes", "wil\u00b7len", ",", "da\u00df", "ich", "das", "Ding", "vom", "Leib", "krieg'", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "FM", "$,", "VVINF", "$(", "PPER", "ADV", "APPR", "NN", "NN", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Schmerzliche Vergi\u00dfmeinnicht!", "tokens": ["Schmerz\u00b7li\u00b7che", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}