{"textgrid.poem.46971": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Widersacher des deutschen Steins", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid gewi\u00df nicht echtes deutsches Gold,", "tokens": ["Ihr", "seid", "ge\u00b7wi\u00df", "nicht", "ech\u00b7tes", "deut\u00b7sches", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und scheut euch vor der Probe,", "tokens": ["Und", "scheut", "euch", "vor", "der", "Pro\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil ihr davon durchaus nichts h\u00f6ren wollt,", "tokens": ["Weil", "ihr", "da\u00b7von", "durc\u00b7haus", "nichts", "h\u00f6\u00b7ren", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df man den Pr\u00fcfstein lobe.", "tokens": ["Da\u00df", "man", "den", "Pr\u00fcf\u00b7stein", "lo\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den, der den Busen hat voll Eisenerzen,", "tokens": ["Den", ",", "der", "den", "Bu\u00b7sen", "hat", "voll", "Ei\u00b7se\u00b7ner\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zieht der Magnetstein an;", "tokens": ["Zieht", "der", "Mag\u00b7net\u00b7stein", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "Ihr habt gewi\u00df nur Kot in euren Herzen,", "tokens": ["Ihr", "habt", "ge\u00b7wi\u00df", "nur", "Kot", "in", "eu\u00b7ren", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil er nicht ziehn euch kann.", "tokens": ["Weil", "er", "nicht", "ziehn", "euch", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}