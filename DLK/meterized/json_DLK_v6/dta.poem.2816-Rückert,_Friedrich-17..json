{"dta.poem.2816": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur eine Waffe gab jedwedem Thier Natur,", "tokens": ["Nur", "ei\u00b7ne", "Waf\u00b7fe", "gab", "jed\u00b7we\u00b7dem", "Thier", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht allen alle, dir, o Mensch, gar keine nur.", "tokens": ["Nicht", "al\u00b7len", "al\u00b7le", ",", "dir", ",", "o", "Mensch", ",", "gar", "kei\u00b7ne", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "PIS", "$,", "PPER", "$,", "FM", "NN", "$,", "ADV", "PIAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie gab auch eine Kunst nur einem, und nicht allen", "tokens": ["Sie", "gab", "auch", "ei\u00b7ne", "Kunst", "nur", "ei\u00b7nem", ",", "und", "nicht", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "ART", "$,", "KON", "PTKNEG", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedwede, wieder dir ist keine zugefallen.", "tokens": ["Jed\u00b7we\u00b7de", ",", "wie\u00b7der", "dir", "ist", "kei\u00b7ne", "zu\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "ADV", "PPER", "VAFIN", "PIAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum? w\u00e4r' eine Waff' und Kunst dir angeboren,", "tokens": ["Wa\u00b7rum", "?", "w\u00e4r'", "ei\u00b7ne", "Waff'", "und", "Kunst", "dir", "an\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "ART", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00e4re der Gebrauch der andern dir verloren.", "tokens": ["So", "w\u00e4\u00b7re", "der", "Ge\u00b7brauch", "der", "an\u00b7dern", "dir", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch brauchen solltest du so alle K\u00fcnst' als Waffen,", "tokens": ["Doch", "brau\u00b7chen", "soll\u00b7test", "du", "so", "al\u00b7le", "K\u00fcnst'", "als", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "PIAT", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir selber schaffend, was dir nicht ist anerschaffen.", "tokens": ["Dir", "sel\u00b7ber", "schaf\u00b7fend", ",", "was", "dir", "nicht", "ist", "a\u00b7ner\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "PWS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}