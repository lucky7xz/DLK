{"dta.poem.12286": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von zw\u00f6lf Knaben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Mutter zeihet mich,               ", "tokens": ["Mein", "Mut\u00b7ter", "zei\u00b7het", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zw\u00f6lf Knaben freyen mich.", "tokens": ["Zw\u00f6lf", "Kna\u00b7ben", "frey\u00b7en", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Erst der th\u00e4t mir winken,", "tokens": ["Der", "Erst", "der", "th\u00e4t", "mir", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ander mein gedenken,", "tokens": ["Der", "an\u00b7der", "mein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Dritt der tratt mir auf den Fu\u00df,", "tokens": ["Der", "Dritt", "der", "tratt", "mir", "auf", "den", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Viert bot mir einen freundlichen Gru\u00df,", "tokens": ["Der", "Viert", "bot", "mir", "ei\u00b7nen", "freund\u00b7li\u00b7chen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der F\u00fcnft bot mir das Fingerlein,", "tokens": ["Der", "F\u00fcnft", "bot", "mir", "das", "Fin\u00b7ger\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sechst der mu\u00df mein eigen seyn,", "tokens": ["Der", "Sechst", "der", "mu\u00df", "mein", "ei\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Siebent bot mir das rothe Gold,", "tokens": ["Der", "Sie\u00b7bent", "bot", "mir", "das", "ro\u00b7the", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der Acht war mir von Herzen hold,", "tokens": ["Der", "Acht", "war", "mir", "von", "Her\u00b7zen", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VAFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Neunt lag mir an meinem Arm,", "tokens": ["Der", "Neunt", "lag", "mir", "an", "mei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zehnt der war noch nicht erwarmt,", "tokens": ["Der", "Zehnt", "der", "war", "noch", "nicht", "er\u00b7warmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Elfte war mein ehlich Mann,", "tokens": ["Der", "Elf\u00b7te", "war", "mein", "eh\u00b7lich", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zw\u00f6lft ging in der Still davon.", "tokens": ["Der", "Zw\u00f6lft", "ging", "in", "der", "Still", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die zw\u00f6lf Knaben gut,", "tokens": ["Die", "zw\u00f6lf", "Kna\u00b7ben", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Zw\u00f6lf Knaben gut,", "tokens": ["Zw\u00f6lf", "Kna\u00b7ben", "gut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Dieselbigen zw\u00f6lf Knaben gut,", "tokens": ["Die\u00b7sel\u00b7bi\u00b7gen", "zw\u00f6lf", "Kna\u00b7ben", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die f\u00fchrten einen guten frischen freien Muth.", "tokens": ["Die", "f\u00fchr\u00b7ten", "ei\u00b7nen", "gu\u00b7ten", "fri\u00b7schen", "frei\u00b7en", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Was machen zw\u00f6lfe hie?", "tokens": ["Was", "ma\u00b7chen", "zw\u00f6l\u00b7fe", "hie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Dutzend machen sie.", "tokens": ["Ein", "Dut\u00b7zend", "ma\u00b7chen", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}