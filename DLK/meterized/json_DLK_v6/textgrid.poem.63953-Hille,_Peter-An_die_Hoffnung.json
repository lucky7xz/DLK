{"textgrid.poem.63953": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "An die Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als geschwunden der kindische Wahn,", "tokens": ["Als", "ge\u00b7schwun\u00b7den", "der", "kin\u00b7di\u00b7sche", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Es w\u00fcrde sich kl\u00e4ren", "tokens": ["Es", "w\u00fcr\u00b7de", "sich", "kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das Chaos, die Tr\u00e4ume,", "tokens": ["Das", "Chaos", ",", "die", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sich kl\u00e4ren zur Dichtung,", "tokens": ["Sich", "kl\u00e4\u00b7ren", "zur", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Werden zur Wahrheit, \u2013 \u2013", "tokens": ["Wer\u00b7den", "zur", "Wahr\u00b7heit", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,", "$(", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Als gewichen der Wahn,", "tokens": ["Als", "ge\u00b7wi\u00b7chen", "der", "Wahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wie stand ich verzweifelt,", "tokens": ["Wie", "stand", "ich", "ver\u00b7zwei\u00b7felt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Starrte ins Leere,", "tokens": ["Starr\u00b7te", "ins", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "In trostlose Nacht!", "tokens": ["In", "trost\u00b7lo\u00b7se", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Sollte mein Auge", "tokens": ["Soll\u00b7te", "mein", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geworfen nur haben", "tokens": ["Ge\u00b7wor\u00b7fen", "nur", "ha\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VAFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den Ungl\u00fccksblick", "tokens": ["Den", "Un\u00b7gl\u00fccks\u00b7blick"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ins Strahlenmeer der heiligen Dichtung,", "tokens": ["Ins", "Strah\u00b7len\u00b7meer", "der", "hei\u00b7li\u00b7gen", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da\u00df ich wanke", "tokens": ["Da\u00df", "ich", "wan\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ins Dunkel,", "tokens": ["Ins", "Dun\u00b7kel", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Wanke ins graue", "tokens": ["Wan\u00b7ke", "ins", "grau\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Leben des Alltags?", "tokens": ["Le\u00b7ben", "des", "All\u00b7tags", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Tiefe, traurig tiefe Nacht!", "tokens": ["Tie\u00b7fe", ",", "trau\u00b7rig", "tie\u00b7fe", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da seh' ich ein Licht,", "tokens": ["Da", "seh'", "ich", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ein schwaches schwankendes Licht,", "tokens": ["Ein", "schwa\u00b7ches", "schwan\u00b7ken\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es wird gr\u00f6\u00dfer, wird heller. \u2013", "tokens": ["Es", "wird", "gr\u00f6\u00b7\u00dfer", ",", "wird", "hel\u00b7ler", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Verschwunden ist's. \u2013", "tokens": ["Ver\u00b7schwun\u00b7den", "ist'", "s.", "\u2013"], "token_info": ["word", "word", "abbreviation", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da leuchtet es wieder,", "tokens": ["Da", "leuch\u00b7tet", "es", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gr\u00f6\u00dfer und gr\u00f6\u00dfer,", "tokens": ["Gr\u00f6\u00b7\u00dfer", "und", "gr\u00f6\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ich sehe den Stern,", "tokens": ["Ich", "se\u00b7he", "den", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Der tr\u00f6stend mir winkt.", "tokens": ["Der", "tr\u00f6s\u00b7tend", "mir", "winkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Sehe beleuchtet von ihm", "tokens": ["Se\u00b7he", "be\u00b7leuch\u00b7tet", "von", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVPP", "APPR", "PPER"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Deine rosigen Finger,", "tokens": ["Dei\u00b7ne", "ro\u00b7si\u00b7gen", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Dein holdes Gesicht,", "tokens": ["Dein", "hol\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "O du meine Hoffnung!", "tokens": ["O", "du", "mei\u00b7ne", "Hoff\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Es lichtet schon mehr sich und mehr", "tokens": ["Es", "lich\u00b7tet", "schon", "mehr", "sich", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PRF", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das tr\u00fcbe Dunkel.", "tokens": ["Das", "tr\u00fc\u00b7be", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Entgegen schon seh ich mir schimmern", "tokens": ["Ent\u00b7ge\u00b7gen", "schon", "seh", "ich", "mir", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Den Himmel des Ruhmes,", "tokens": ["Den", "Him\u00b7mel", "des", "Ruh\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "An deiner Hand", "tokens": ["An", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Erreich ich ihn bald.", "tokens": ["Er\u00b7reich", "ich", "ihn", "bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Gl\u00fcck nur und Dank und strebender Eifer", "tokens": ["Gl\u00fcck", "nur", "und", "Dank", "und", "stre\u00b7ben\u00b7der", "Ei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Schwellt die freudige Seele,", "tokens": ["Schwellt", "die", "freu\u00b7di\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Noch eben umnachtet!", "tokens": ["Noch", "e\u00b7ben", "um\u00b7nach\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Dank dir, innigster Dank", "tokens": ["Dank", "dir", ",", "in\u00b7nigs\u00b7ter", "Dank"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Dir, Tr\u00f6sterin Hoffnung.", "tokens": ["Dir", ",", "Tr\u00f6s\u00b7te\u00b7rin", "Hoff\u00b7nung", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}