{"textgrid.poem.51358": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach M\u00e4gdlein, deine Zier", "tokens": ["Ach", "M\u00e4gd\u00b7lein", ",", "dei\u00b7ne", "Zier"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sieht wie ein Bl\u00fcmlein f\u00fcr,", "tokens": ["Sieht", "wie", "ein", "Bl\u00fcm\u00b7lein", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das zart und neu gebohren:", "tokens": ["Das", "zart", "und", "neu", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sich so bald verlohren,", "tokens": ["Und", "sich", "so", "bald", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bald ein k\u00fchler Wind", "tokens": ["So", "bald", "ein", "k\u00fch\u00b7ler", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu wittern sich beginnt:", "tokens": ["Zu", "wit\u00b7tern", "sich", "be\u00b7ginnt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durch stille seyn und schweigen", "tokens": ["Durch", "stil\u00b7le", "seyn", "und", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VAINF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bekleibt und bleibt es eigen:", "tokens": ["Be\u00b7kleibt", "und", "bleibt", "es", "ei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Erf\u00e4hrst du, was es sey,", "tokens": ["Er\u00b7f\u00e4hrst", "du", ",", "was", "es", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So ist sie schon vorbey.", "tokens": ["So", "ist", "sie", "schon", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Glaub, eh als du es funden,", "tokens": ["Glaub", ",", "eh", "als", "du", "es", "fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ist es bereit verschwunden.", "tokens": ["Ist", "es", "be\u00b7reit", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}