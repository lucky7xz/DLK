{"textgrid.poem.46117": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wan, Myrta, reden und stillschweigen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wan, Myrta, reden und stillschweigen", "tokens": ["Wan", ",", "Myr\u00b7ta", ",", "re\u00b7den", "und", "still\u00b7schwei\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "NE", "$,", "VVINF", "KON", "VVFIN"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "zumal verhindert unser gl\u00fcck,", "tokens": ["zu\u00b7mal", "ver\u00b7hin\u00b7dert", "un\u00b7ser", "gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so la\u00df uns unser herz bezeugen", "tokens": ["so", "la\u00df", "uns", "un\u00b7ser", "herz", "be\u00b7zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "durch sich besprachende anblick;", "tokens": ["durch", "sich", "be\u00b7spra\u00b7chen\u00b7de", "an\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "dan Amor, den wir allzeit ehren,", "tokens": ["dan", "A\u00b7mor", ",", "den", "wir", "all\u00b7zeit", "eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wird solche stumme sprach uns lehren.", "tokens": ["wird", "sol\u00b7che", "stum\u00b7me", "sprach", "uns", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df die anblick hin und her fliegen,", "tokens": ["La\u00df", "die", "an\u00b7blick", "hin", "und", "her", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "getreue boten deiner gunst,", "tokens": ["ge\u00b7treu\u00b7e", "bo\u00b7ten", "dei\u00b7ner", "gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der neider torheit zu betriegen,", "tokens": ["der", "nei\u00b7der", "tor\u00b7heit", "zu", "be\u00b7trie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die doll und d\u00f6lpisch zu der kunst,", "tokens": ["die", "doll", "und", "d\u00f6l\u00b7pisch", "zu", "der", "kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "dan Amor, welchen sie nicht ehren,", "tokens": ["dan", "A\u00b7mor", ",", "wel\u00b7chen", "sie", "nicht", "eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PWAT", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wird sie die stumme sprach nicht lehren.", "tokens": ["wird", "sie", "die", "stum\u00b7me", "sprach", "nicht", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Solt aber jemand sich verdrie\u00dfen", "tokens": ["Solt", "a\u00b7ber", "je\u00b7mand", "sich", "ver\u00b7drie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PIS", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ab unsrer lieb anblicken fahrt,", "tokens": ["ab", "uns\u00b7rer", "lieb", "an\u00b7bli\u00b7cken", "fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so m\u00fcssen wir uns dan begr\u00fc\u00dfen", "tokens": ["so", "m\u00fcs\u00b7sen", "wir", "uns", "dan", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit dem geist, nach der engeln art;", "tokens": ["mit", "dem", "geist", ",", "nach", "der", "en\u00b7geln", "art", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "dan Amor, welchen wir stets ehren,", "tokens": ["dan", "A\u00b7mor", ",", "wel\u00b7chen", "wir", "stets", "eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PWAT", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wird solche stumme sprach uns lehren.", "tokens": ["wird", "sol\u00b7che", "stum\u00b7me", "sprach", "uns", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und also w\u00f6llen wir betriegen", "tokens": ["Und", "al\u00b7so", "w\u00f6l\u00b7len", "wir", "be\u00b7trie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der falschen schwetzer m\u00fch und leid", "tokens": ["der", "fal\u00b7schen", "schwet\u00b7zer", "m\u00fch", "und", "leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und doppelt uns nach lust vern\u00fcgen", "tokens": ["und", "dop\u00b7pelt", "uns", "nach", "lust", "ver\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "APPR", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "in ihrem neid und unsrer freud;", "tokens": ["in", "ih\u00b7rem", "neid", "und", "uns\u00b7rer", "freud", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "weil sie, torecht, Amorn nicht ehren,", "tokens": ["weil", "sie", ",", "to\u00b7recht", ",", "A\u00b7morn", "nicht", "eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "$,", "NE", "PTKNEG", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "wird er sie dise sprach nicht lehren.", "tokens": ["wird", "er", "sie", "di\u00b7se", "sprach", "nicht", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PDS", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}