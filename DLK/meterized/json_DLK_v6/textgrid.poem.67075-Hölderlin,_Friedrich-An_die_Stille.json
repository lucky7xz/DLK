{"textgrid.poem.67075": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Stille", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort im waldumkr\u00e4nzten Schattentale", "tokens": ["Dort", "im", "wald\u00b7um\u00b7kr\u00e4nz\u00b7ten", "Schat\u00b7ten\u00b7ta\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schl\u00fcrft ich, schlummernd unterm Rosenstrauch,", "tokens": ["Schl\u00fcrft", "ich", ",", "schlum\u00b7mernd", "un\u00b7term", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Trunkenheit aus deiner G\u00f6tterschale,", "tokens": ["Trun\u00b7ken\u00b7heit", "aus", "dei\u00b7ner", "G\u00f6t\u00b7ter\u00b7scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Angeweht von deinem Liebeshauch.", "tokens": ["An\u00b7ge\u00b7weht", "von", "dei\u00b7nem", "Lie\u00b7bes\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sieh, es brennt an deines J\u00fcnglings Wange", "tokens": ["Sieh", ",", "es", "brennt", "an", "dei\u00b7nes", "J\u00fcng\u00b7lings", "Wan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hei\u00df und gl\u00fchend noch Begeisterung,", "tokens": ["Hei\u00df", "und", "gl\u00fc\u00b7hend", "noch", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Voll ist mir das Herz vom Lobgesange,", "tokens": ["Voll", "ist", "mir", "das", "Herz", "vom", "Lob\u00b7ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und der Fittig heischet Adlerschwung.", "tokens": ["Und", "der", "Fit\u00b7tig", "hei\u00b7schet", "Ad\u00b7ler\u00b7schwung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Stieg ich k\u00fchnen Sinns zum Hades nieder,", "tokens": ["Stieg", "ich", "k\u00fch\u00b7nen", "Sinns", "zum", "Ha\u00b7des", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo kein Sterblicher dich noch ersah,", "tokens": ["Wo", "kein", "Sterb\u00b7li\u00b7cher", "dich", "noch", "er\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schw\u00e4nge sich das mutige Gefieder", "tokens": ["Schw\u00e4n\u00b7ge", "sich", "das", "mu\u00b7ti\u00b7ge", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zum Orion auf, so w\u00e4rst du da;", "tokens": ["Zum", "O\u00b7rion", "auf", ",", "so", "w\u00e4rst", "du", "da", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ins weite Meer die Str\u00f6me gleiten,", "tokens": ["Wie", "ins", "wei\u00b7te", "Meer", "die", "Str\u00f6\u00b7me", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "St\u00fcrzen dir die Zeiten alle zu,", "tokens": ["St\u00fcr\u00b7zen", "dir", "die", "Zei\u00b7ten", "al\u00b7le", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "In dem Scho\u00df der alten Ewigkeiten,", "tokens": ["In", "dem", "Scho\u00df", "der", "al\u00b7ten", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In des Chaos Tiefen wohntest du.", "tokens": ["In", "des", "Chaos", "Tie\u00b7fen", "wohn\u00b7test", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In der W\u00fcste d\u00fcrrem Schreckgefilde,", "tokens": ["In", "der", "W\u00fcs\u00b7te", "d\u00fcr\u00b7rem", "Schreck\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Hungertod des Wallers harrt,", "tokens": ["Wo", "der", "Hun\u00b7ger\u00b7tod", "des", "Wal\u00b7lers", "harrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In der St\u00fcrme Land, wo schwarz und wilde", "tokens": ["In", "der", "St\u00fcr\u00b7me", "Land", ",", "wo", "schwarz", "und", "wil\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PWAV", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das Gebirg im kalten Panzer starrt,", "tokens": ["Das", "Ge\u00b7birg", "im", "kal\u00b7ten", "Pan\u00b7zer", "starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "In der Sommernacht, in Morgenl\u00fcften,", "tokens": ["In", "der", "Som\u00b7mer\u00b7nacht", ",", "in", "Mor\u00b7gen\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In den Hainen weht dein Schwestergru\u00df,", "tokens": ["In", "den", "Hai\u00b7nen", "weht", "dein", "Schwes\u00b7ter\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "\u00dcber schauerlichen Schlummergr\u00fcften", "tokens": ["\u00dc\u00b7ber", "schau\u00b7er\u00b7li\u00b7chen", "Schlum\u00b7mer\u00b7gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "St\u00e4rkt die Lieblinge dein G\u00f6tterku\u00df.", "tokens": ["St\u00e4rkt", "die", "Lieb\u00b7lin\u00b7ge", "dein", "G\u00f6t\u00b7ter\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ruhe f\u00e4chelst du der Heldenseele", "tokens": ["Ru\u00b7he", "f\u00e4\u00b7chelst", "du", "der", "Hel\u00b7den\u00b7see\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Halle, wann die Schlacht beginnt,", "tokens": ["In", "der", "Hal\u00b7le", ",", "wann", "die", "Schlacht", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hauchst Begeistrung in der Felsenh\u00f6hle,", "tokens": ["Hauchst", "Be\u00b7geis\u00b7trung", "in", "der", "Fel\u00b7sen\u00b7h\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo um Mitternacht der Denker sinnt,", "tokens": ["Wo", "um", "Mit\u00b7ter\u00b7nacht", "der", "Den\u00b7ker", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Schlummer tr\u00e4ufst du auf die d\u00fcstre Zelle,", "tokens": ["Schlum\u00b7mer", "tr\u00e4ufst", "du", "auf", "die", "d\u00fcst\u00b7re", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df der Dulder seines Grams vergi\u00dft,", "tokens": ["Da\u00df", "der", "Dul\u00b7der", "sei\u00b7nes", "Grams", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "L\u00e4chelst traulich aus der Schattenquelle,", "tokens": ["L\u00e4\u00b7chelst", "trau\u00b7lich", "aus", "der", "Schat\u00b7ten\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wo den ersten Ku\u00df das M\u00e4dchen k\u00fc\u00dft.", "tokens": ["Wo", "den", "ers\u00b7ten", "Ku\u00df", "das", "M\u00e4d\u00b7chen", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Ha! dir tr\u00e4uft die wonnetrunkne Z\u00e4hre", "tokens": ["Ha", "!", "dir", "tr\u00e4uft", "die", "won\u00b7ne\u00b7trunk\u00b7ne", "Z\u00e4h\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und Entz\u00fcckung str\u00f6mt in mein Gebein,", "tokens": ["Und", "Ent\u00b7z\u00fc\u00b7ckung", "str\u00f6mt", "in", "mein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Millionen bauen dir Alt\u00e4re,", "tokens": ["Mil\u00b7lion\u00b7en", "bau\u00b7en", "dir", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Z\u00fcrne nicht! auch dieses Herz ist dein!", "tokens": ["Z\u00fcr\u00b7ne", "nicht", "!", "auch", "die\u00b7ses", "Herz", "ist", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$.", "ADV", "PDAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dort im Tale will ich Wonne trinken,", "tokens": ["Dort", "im", "Ta\u00b7le", "will", "ich", "Won\u00b7ne", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wiederkehren in die Schattenkluft,", "tokens": ["Wie\u00b7der\u00b7keh\u00b7ren", "in", "die", "Schat\u00b7ten\u00b7kluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Bis der G\u00f6ttin Arme trauter winken,", "tokens": ["Bis", "der", "G\u00f6t\u00b7tin", "Ar\u00b7me", "trau\u00b7ter", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Bis die Braut zum stillen Bunde ruft.", "tokens": ["Bis", "die", "Braut", "zum", "stil\u00b7len", "Bun\u00b7de", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Keine Lauscher nahn der Schlummerst\u00e4tte,", "tokens": ["Kei\u00b7ne", "Lau\u00b7scher", "nahn", "der", "Schlum\u00b7mer\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00fchl und schattig ists im Leichentuch,", "tokens": ["K\u00fchl", "und", "schat\u00b7tig", "ists", "im", "Lei\u00b7chen\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Abgesch\u00fcttelt ist die Sklavenkette,", "tokens": ["Ab\u00b7ge\u00b7sch\u00fct\u00b7telt", "ist", "die", "Skla\u00b7ven\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Maiges\u00e4usel wird Gewitterfluch;", "tokens": ["Mai\u00b7ge\u00b7s\u00e4u\u00b7sel", "wird", "Ge\u00b7wit\u00b7ter\u00b7fluch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sch\u00f6ner rauscht die tr\u00e4ge Flut der Zeiten,", "tokens": ["Sch\u00f6\u00b7ner", "rauscht", "die", "tr\u00e4\u00b7ge", "Flut", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Rings umd\u00fcstert von der Sorgen Schwarm;", "tokens": ["Rings", "um\u00b7d\u00fcs\u00b7tert", "von", "der", "Sor\u00b7gen", "Schwarm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie ein Traum verfliegen Ewigkeiten,", "tokens": ["Wie", "ein", "Traum", "ver\u00b7flie\u00b7gen", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schl\u00e4ft der J\u00fcngling seiner Braut im Arm.", "tokens": ["Schl\u00e4ft", "der", "J\u00fcng\u00b7ling", "sei\u00b7ner", "Braut", "im", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}