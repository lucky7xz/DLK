{"textgrid.poem.24904": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Inesilla", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Inesilla, das fr\u00f6hliche Kind,", "tokens": ["I\u00b7ne\u00b7sil\u00b7la", ",", "das", "fr\u00f6h\u00b7li\u00b7che", "Kind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wiegt sich im schlanken Ge\u00e4ste,", "tokens": ["Wiegt", "sich", "im", "schlan\u00b7ken", "Ge\u00b7\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Auf und nieder geschaukelt vom Wind,", "tokens": ["Auf", "und", "nie\u00b7der", "ge\u00b7schau\u00b7kelt", "vom", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gleich dem Vogel im Neste.", "tokens": ["Gleich", "dem", "Vo\u00b7gel", "im", "Nes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Wie sie von Wipfel zu Wipfel klimmt,", "tokens": ["Wie", "sie", "von", "Wip\u00b7fel", "zu", "Wip\u00b7fel", "klimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo in den Bl\u00e4tterkronen", "tokens": ["Wo", "in", "den", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Granate, die purpurne, glimmt", "tokens": ["Die", "Gra\u00b7na\u00b7te", ",", "die", "pur\u00b7pur\u00b7ne", ",", "glimmt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+--+++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Neben den bleichen Limonen!", "tokens": ["Ne\u00b7ben", "den", "blei\u00b7chen", "Li\u00b7mo\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Will ich ihr nahn, schnell nimmt sie die Flucht", "tokens": ["Will", "ich", "ihr", "nahn", ",", "schnell", "nimmt", "sie", "die", "Flucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "$,", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ueber die Aeste, die vollen,", "tokens": ["Ue\u00b7ber", "die", "A\u00b7es\u00b7te", ",", "die", "vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da\u00df vor die F\u00fc\u00dfe mir, Frucht an Frucht,", "tokens": ["Da\u00df", "vor", "die", "F\u00fc\u00b7\u00dfe", "mir", ",", "Frucht", "an", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Goldene Aepfel rollen.", "tokens": ["Gol\u00b7de\u00b7ne", "A\u00b7e\u00b7pfel", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "O genug schon! genug schon, Kind!", "tokens": ["O", "ge\u00b7nug", "schon", "!", "ge\u00b7nug", "schon", ",", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$.", "ADV", "ADV", "$,", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "S\u00fc\u00dferes gieb mir zu nippen!", "tokens": ["S\u00fc\u00b7\u00dfe\u00b7res", "gieb", "mir", "zu", "nip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Reifer, als alle die Fr\u00fcchte, sind", "tokens": ["Rei\u00b7fer", ",", "als", "al\u00b7le", "die", "Fr\u00fcch\u00b7te", ",", "sind"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "KOUS", "PIS", "ART", "NN", "$,", "VAFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "F\u00fcr den Ku\u00df deine Lippen!", "tokens": ["F\u00fcr", "den", "Ku\u00df", "dei\u00b7ne", "Lip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Doch aus dem Wipfel, dichtbelaubt,", "tokens": ["Doch", "aus", "dem", "Wip\u00b7fel", ",", "dicht\u00b7be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lacht mir die Kleine entgegen,", "tokens": ["Lacht", "mir", "die", "Klei\u00b7ne", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sch\u00fcttelt die Zweige, und \u00fcber mein Haupt", "tokens": ["Sch\u00fct\u00b7telt", "die", "Zwei\u00b7ge", ",", "und", "\u00fc\u00b7ber", "mein", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "F\u00e4llt von Orangen ein Regen.", "tokens": ["F\u00e4llt", "von", "O\u00b7ran\u00b7gen", "ein", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Durch das Ge\u00e4st fort springt sie leis,", "tokens": ["Durch", "das", "Ge\u00b7\u00e4st", "fort", "springt", "sie", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "W\u00e4hrend die Fr\u00fcchte noch fallen;", "tokens": ["W\u00e4h\u00b7rend", "die", "Fr\u00fcch\u00b7te", "noch", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Fernhin \u00fcber die Felder von Mais", "tokens": ["Fern\u00b7hin", "\u00fc\u00b7ber", "die", "Fel\u00b7der", "von", "Mais"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "H\u00f6r' ich ihr Lachen verhallen.", "tokens": ["H\u00f6r'", "ich", "ihr", "La\u00b7chen", "ver\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "H\u00fcte dich, M\u00e4dchen! Entgingst du mir auch", "tokens": ["H\u00fc\u00b7te", "dich", ",", "M\u00e4d\u00b7chen", "!", "Ent\u00b7gingst", "du", "mir", "auch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "PPER", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Heut durch die Schritte, die raschen,", "tokens": ["Heut", "durch", "die", "Schrit\u00b7te", ",", "die", "ra\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie die Libelle auf zitterndem Strauch", "tokens": ["Wie", "die", "Li\u00b7bel\u00b7le", "auf", "zit\u00b7tern\u00b7dem", "Strauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Werd' ich morgen dich haschen!", "tokens": ["Werd'", "ich", "mor\u00b7gen", "dich", "ha\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}