{"textgrid.poem.55785": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Getr\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Himmels veilchenblaue W\u00f6lbung spannte", "tokens": ["Des", "Him\u00b7mels", "veil\u00b7chen\u00b7blau\u00b7e", "W\u00f6l\u00b7bung", "spann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich sterneglitzernd \u00fcber mir ... ein Dampf", "tokens": ["Sich", "ster\u00b7neg\u00b7lit\u00b7zernd", "\u00fc\u00b7ber", "mir", "...", "ein", "Dampf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "PPER", "$(", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von \u00fcberreifen, \u00fcppigen Gardenien", "tokens": ["Von", "\u00fc\u00b7berr\u00b7ei\u00b7fen", ",", "\u00fcp\u00b7pi\u00b7gen", "Gar\u00b7de\u00b7ni\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Umquoll mit weichem F\u00e4cheln Stirn und Wange,", "tokens": ["Um\u00b7quoll", "mit", "wei\u00b7chem", "F\u00e4\u00b7cheln", "Stirn", "und", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und meine Glieder, schwer und schlummers\u00fcchtig,", "tokens": ["Und", "mei\u00b7ne", "Glie\u00b7der", ",", "schwer", "und", "schlum\u00b7mer\u00b7s\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie ruhten reglos auf den Marmorplatten", "tokens": ["Sie", "ruh\u00b7ten", "reg\u00b7los", "auf", "den", "Mar\u00b7mor\u00b7plat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Des Sarkophages, der mich k\u00fchl umfing.", "tokens": ["Des", "Sar\u00b7ko\u00b7pha\u00b7ges", ",", "der", "mich", "k\u00fchl", "um\u00b7fing", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und auf des Sarges Ranft, \u2013 da sa\u00dfest du.", "tokens": ["Und", "auf", "des", "Sar\u00b7ges", "Ranft", ",", "\u2013", "da", "sa\u00b7\u00dfest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "$(", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Um deinen rosenbraunen K\u00f6rper bebte", "tokens": ["Um", "dei\u00b7nen", "ro\u00b7sen\u00b7brau\u00b7nen", "K\u00f6r\u00b7per", "beb\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein goldiggr\u00fcner, seidenzarter Flor,", "tokens": ["Ein", "gol\u00b7dig\u00b7gr\u00fc\u00b7ner", ",", "sei\u00b7denz\u00b7ar\u00b7ter", "Flor", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und aus dem trotzig schwarzen Haar erglei\u00dfte", "tokens": ["Und", "aus", "dem", "trot\u00b7zig", "schwar\u00b7zen", "Haar", "er\u00b7glei\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bleichgr\u00fcnlicher Smaragde kalter Schein.", "tokens": ["Bleich\u00b7gr\u00fcn\u00b7li\u00b7cher", "Sma\u00b7rag\u00b7de", "kal\u00b7ter", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.13": {"text": "Dein Antlitz war vertr\u00e4umt und weltverloren,", "tokens": ["Dein", "Ant\u00b7litz", "war", "ver\u00b7tr\u00e4umt", "und", "welt\u00b7ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und deine Augen starrten weit hinaus,", "tokens": ["Und", "dei\u00b7ne", "Au\u00b7gen", "starr\u00b7ten", "weit", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "So suchend, so begehrend und so schmerzlich,", "tokens": ["So", "su\u00b7chend", ",", "so", "be\u00b7geh\u00b7rend", "und", "so", "schmerz\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVPP", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und leise zuckten deine schweren Brauen,", "tokens": ["Und", "lei\u00b7se", "zuck\u00b7ten", "dei\u00b7ne", "schwe\u00b7ren", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und um die stolzgepressten Lippen glitt", "tokens": ["Und", "um", "die", "stolz\u00b7ge\u00b7press\u00b7ten", "Lip\u00b7pen", "glitt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ein wundes L\u00e4cheln, ein ged\u00e4mpfter Hohn", "tokens": ["Ein", "wun\u00b7des", "L\u00e4\u00b7cheln", ",", "ein", "ge\u00b7d\u00e4mpf\u00b7ter", "Hohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und \u00dcberdruss und Ekel und Verzweiflung,", "tokens": ["Und", "\u00dc\u00b7berd\u00b7russ", "und", "E\u00b7kel", "und", "Ver\u00b7zwei\u00b7flung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und ungesprochen traf mein Ohr die Frage:", "tokens": ["Und", "un\u00b7ge\u00b7spro\u00b7chen", "traf", "mein", "Ohr", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wie lange noch soll ich dich h\u00fcten \u2013 ich,", "tokens": ["Wie", "lan\u00b7ge", "noch", "soll", "ich", "dich", "h\u00fc\u00b7ten", "\u2013", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$(", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das hei\u00dfe, wilde, starke Leben \u2013 dich,", "tokens": ["Das", "hei\u00b7\u00dfe", ",", "wil\u00b7de", ",", "star\u00b7ke", "Le\u00b7ben", "\u2013", "dich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "$,", "ADJA", "NN", "$(", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Den Markerkrankten, Todessiechen, sprich,", "tokens": ["Den", "Mar\u00b7ker\u00b7krank\u00b7ten", ",", "To\u00b7des\u00b7sie\u00b7chen", ",", "sprich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wie lange noch?", "tokens": ["Wie", "lan\u00b7ge", "noch", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}