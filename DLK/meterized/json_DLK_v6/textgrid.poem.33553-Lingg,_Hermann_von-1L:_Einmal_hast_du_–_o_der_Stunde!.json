{"textgrid.poem.33553": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einmal hast du \u2013 o der Stunde!", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal hast du \u2013 o der Stunde!", "tokens": ["Ein\u00b7mal", "hast", "du", "\u2013", "o", "der", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "FM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlummernd mir im Arm geruht,", "tokens": ["Schlum\u00b7mernd", "mir", "im", "Arm", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen Ku\u00df noch auf dem Munde,", "tokens": ["Mei\u00b7nen", "Ku\u00df", "noch", "auf", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Wangen welche Glut!", "tokens": ["Auf", "den", "Wan\u00b7gen", "wel\u00b7che", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie da die Pulse flogen!", "tokens": ["O", "wie", "da", "die", "Pul\u00b7se", "flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauschend jedem Atemzug,", "tokens": ["Lau\u00b7schend", "je\u00b7dem", "A\u00b7tem\u00b7zug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlt' ich an des Busens Wogen,", "tokens": ["F\u00fchlt'", "ich", "an", "des", "Bu\u00b7sens", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dein Herz an meines schlug.", "tokens": ["Wie", "dein", "Herz", "an", "mei\u00b7nes", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das wird nie vergessen werden,", "tokens": ["Das", "wird", "nie", "ver\u00b7ges\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das verl\u00f6scht kein andrer Tag,", "tokens": ["Das", "ver\u00b7l\u00f6scht", "kein", "an\u00b7drer", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht das gr\u00f6\u00dfte Gl\u00fcck auf Erden,", "tokens": ["Nicht", "das", "gr\u00f6\u00df\u00b7te", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht des Angl\u00fccks schwerster Schlag.", "tokens": ["Nicht", "des", "An\u00b7gl\u00fccks", "schwers\u00b7ter", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Flamme, nie vergl\u00fchend,", "tokens": ["Ei\u00b7ne", "Flam\u00b7me", ",", "nie", "ver\u00b7gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein lebend'ger Edelstein,", "tokens": ["Ein", "le\u00b7ben\u00b7d'\u00b7ger", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt mir der Gedanke bl\u00fchend:", "tokens": ["Lebt", "mir", "der", "Ge\u00b7dan\u00b7ke", "bl\u00fc\u00b7hend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einmal so und ewig mein!", "tokens": ["Ein\u00b7mal", "so", "und", "e\u00b7wig", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}