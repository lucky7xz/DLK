{"dta.poem.21388": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201evorgestern fand ich ihn traurig geb\u00fcckt", "tokens": ["\u201e", "vor\u00b7ges\u00b7tern", "fand", "ich", "ihn", "trau\u00b7rig", "ge\u00b7b\u00fcckt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "VVPP"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Am Camin, in der Morgenstunde;", "tokens": ["Am", "Ca\u00b7min", ",", "in", "der", "Mor\u00b7gen\u00b7stun\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er kochte h\u00f6chstselbst ein Lavement", "tokens": ["Er", "koch\u00b7te", "h\u00f6chst\u00b7selbst", "ein", "La\u00b7ve\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "F\u00fcr seine kranken Hunde.\u201c", "tokens": ["F\u00fcr", "sei\u00b7ne", "kran\u00b7ken", "Hun\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}