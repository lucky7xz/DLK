{"textgrid.poem.47430": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[f\u00fcr die b\u00f6se Liebe braucht' ich]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr die b\u00f6se Liebe braucht' ich", "tokens": ["F\u00fcr", "die", "b\u00f6\u00b7se", "Lie\u00b7be", "braucht'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon so manche Medicin;", "tokens": ["Schon", "so", "man\u00b7che", "Me\u00b7di\u00b7cin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es wurde mir die Ruhe,", "tokens": ["Doch", "es", "wur\u00b7de", "mir", "die", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ersehnte, nicht verlieh'n.", "tokens": ["Die", "er\u00b7sehn\u00b7te", ",", "nicht", "ver\u00b7lieh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "$,", "PTKNEG", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn es ist in mir die Liebe", "tokens": ["Denn", "es", "ist", "in", "mir", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht ein blo\u00dfer Gastbesuch,", "tokens": ["Nicht", "ein", "blo\u00b7\u00dfer", "Gast\u00b7be\u00b7such", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie er wohl in andern Herzen", "tokens": ["Wie", "er", "wohl", "in", "an\u00b7dern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pfleget ein- und auszuzieh'n,", "tokens": ["Pfle\u00b7get", "ein", "und", "aus\u00b7zu\u00b7zieh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist mir Odem, Geist und Seele \u2013", "tokens": ["Ist", "mir", "O\u00b7dem", ",", "Geist", "und", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden Odem, Seele, Geist,", "tokens": ["Wer\u00b7den", "O\u00b7dem", ",", "See\u00b7le", ",", "Geist", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine letzten Hauche schwinden,", "tokens": ["Mei\u00b7ne", "letz\u00b7ten", "Hau\u00b7che", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dahin auch jene flieh'n.", "tokens": ["Wird", "da\u00b7hin", "auch", "je\u00b7ne", "flieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "PAV", "ADV", "PDS", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}