{"textgrid.poem.33404": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Kettenlied gesungen am St. Johannisfeste", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir singen und schlingen zur Wette", "tokens": ["Wir", "sin\u00b7gen", "und", "schlin\u00b7gen", "zur", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Eintracht unendliche Kette,", "tokens": ["Der", "Ein\u00b7tracht", "un\u00b7end\u00b7li\u00b7che", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und feiern der Br\u00fcderschaft Fest.", "tokens": ["Und", "fei\u00b7ern", "der", "Br\u00fc\u00b7der\u00b7schaft", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "O, messet die Kette, ihr findet", "tokens": ["O", ",", "mes\u00b7set", "die", "Ket\u00b7te", ",", "ihr", "fin\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Kein Ende daran, sie umwindet", "tokens": ["Kein", "En\u00b7de", "da\u00b7ran", ",", "sie", "um\u00b7win\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "PAV", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Erde von Osten bis West.", "tokens": ["Die", "Er\u00b7de", "von", "Os\u00b7ten", "bis", "West", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und wie an der Kette die Glieder,", "tokens": ["Und", "wie", "an", "der", "Ket\u00b7te", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So schlingen sich Br\u00fcder an Br\u00fcder,", "tokens": ["So", "schlin\u00b7gen", "sich", "Br\u00fc\u00b7der", "an", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und leben im ewigen Bund.", "tokens": ["Und", "le\u00b7ben", "im", "e\u00b7wi\u00b7gen", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Sie laden und feuern im Kreise,", "tokens": ["Sie", "la\u00b7den", "und", "feu\u00b7ern", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und singen nach einerlei Weise:", "tokens": ["Und", "sin\u00b7gen", "nach", "ei\u00b7ner\u00b7lei", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Was ewig ist, Br\u00fcder, ist rund.", "tokens": ["Was", "e\u00b7wig", "ist", ",", "Br\u00fc\u00b7der", ",", "ist", "rund", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}