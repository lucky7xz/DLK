{"textgrid.poem.33893": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Hedwig", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war in sch\u00f6ner Fr\u00fchlingszeit,", "tokens": ["Es", "war", "in", "sch\u00f6\u00b7ner", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich dich fand bei Spiel und Scherz,", "tokens": ["Als", "ich", "dich", "fand", "bei", "Spiel", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da dr\u00e4ngte all' die Lieblichkeit", "tokens": ["Da", "dr\u00e4ng\u00b7te", "all'", "die", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind lind, wie nie noch, an mein Herz.", "tokens": ["Sind", "lind", ",", "wie", "nie", "noch", ",", "an", "mein", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du selber warst dem Fr\u00fchling gleich,", "tokens": ["Du", "sel\u00b7ber", "warst", "dem", "Fr\u00fch\u00b7ling", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nur verspricht, doch nicht gew\u00e4hrt,", "tokens": ["Der", "nur", "ver\u00b7spricht", ",", "doch", "nicht", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum ward ich nicht vor Sehnsucht bleich", "tokens": ["Drum", "ward", "ich", "nicht", "vor", "Sehn\u00b7sucht", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von Entz\u00fccken nicht verkl\u00e4rt.", "tokens": ["Und", "von", "Ent\u00b7z\u00fc\u00b7cken", "nicht", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es war der Morgen vor dem Fest,", "tokens": ["Es", "war", "der", "Mor\u00b7gen", "vor", "dem", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An dem man nur noch Tr\u00e4ume tauscht,", "tokens": ["An", "dem", "man", "nur", "noch", "Tr\u00e4u\u00b7me", "tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Weh, das keinen Stachel l\u00e4\u00dft,", "tokens": ["Das", "Weh", ",", "das", "kei\u00b7nen", "Sta\u00b7chel", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freude, welche nicht berauscht.", "tokens": ["Die", "Freu\u00b7de", ",", "wel\u00b7che", "nicht", "be\u00b7rauscht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie nur noch gr\u00fcn der Rosenstrauch,", "tokens": ["Wie", "nur", "noch", "gr\u00fcn", "der", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch auch schon gr\u00fcn die Nessel war,", "tokens": ["Doch", "auch", "schon", "gr\u00fcn", "die", "Nes\u00b7sel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So glichen sich die Stunden auch,", "tokens": ["So", "gli\u00b7chen", "sich", "die", "Stun\u00b7den", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die uns begl\u00fcckten, wunderbar.", "tokens": ["Die", "uns", "be\u00b7gl\u00fcck\u00b7ten", ",", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nach manchem Tag kam dann der Tag,", "tokens": ["Nach", "man\u00b7chem", "Tag", "kam", "dann", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns, vielleicht auf ewig, schied;", "tokens": ["Der", "uns", ",", "viel\u00b7leicht", "auf", "e\u00b7wig", ",", "schied", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "APPR", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich trug es, wie man's tragen mag,", "tokens": ["Ich", "trug", "es", ",", "wie", "man's", "tra\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man den Fr\u00fchling scheiden sieht.", "tokens": ["Wenn", "man", "den", "Fr\u00fch\u00b7ling", "schei\u00b7den", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nur selten stieg dein holdes Bild", "tokens": ["Nur", "sel\u00b7ten", "stieg", "dein", "hol\u00b7des", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir auf in der erstarrten Brust,", "tokens": ["Mir", "auf", "in", "der", "er\u00b7starr\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, ward ich einmal weich und mild,", "tokens": ["Doch", ",", "ward", "ich", "ein\u00b7mal", "weich", "und", "mild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So war ich gleich mir dein bewu\u00dft.", "tokens": ["So", "war", "ich", "gleich", "mir", "dein", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und dieses f\u00fchl' ich: blick' ich einst", "tokens": ["Und", "die\u00b7ses", "f\u00fchl'", "ich", ":", "blick'", "ich", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von meinem Sterbebett zur\u00fcck,", "tokens": ["Von", "mei\u00b7nem", "Ster\u00b7be\u00b7bett", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist, da\u00df du mir noch erscheinst,", "tokens": ["So", "ist", ",", "da\u00df", "du", "mir", "noch", "er\u00b7scheinst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein letzter Wunsch, mein letztes Gl\u00fcck.", "tokens": ["Mein", "letz\u00b7ter", "Wunsch", ",", "mein", "letz\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du warst mein Lebensengel, sei", "tokens": ["Du", "warst", "mein", "Le\u00b7ben\u00b7sen\u00b7gel", ",", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn du mein Todesengel auch,", "tokens": ["Denn", "du", "mein", "To\u00b7de\u00b7sen\u00b7gel", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann mischt noch in den Herbst der Mai", "tokens": ["Dann", "mischt", "noch", "in", "den", "Herbst", "der", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den \u00fcberquellend-vollen Hauch.", "tokens": ["Den", "\u00fc\u00b7ber\u00b7quel\u00b7len\u00b7dvol\u00b7len", "Hauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Am Morgen, wo der Mensch ersteht", "tokens": ["Am", "Mor\u00b7gen", ",", "wo", "der", "Mensch", "er\u00b7steht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr seinen schweren Tageslauf,", "tokens": ["F\u00fcr", "sei\u00b7nen", "schwe\u00b7ren", "Ta\u00b7ges\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Abends, wenn er schlafen geht,", "tokens": ["Und", "A\u00b7bends", ",", "wenn", "er", "schla\u00b7fen", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da schaut er gern zum Himmel auf!", "tokens": ["Da", "schaut", "er", "gern", "zum", "Him\u00b7mel", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}