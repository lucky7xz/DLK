{"textgrid.poem.31481": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie? hat die Allheit sich in dieses nichts begeben!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? hat die Allheit sich in dieses nichts begeben!", "tokens": ["Wie", "?", "hat", "die", "All\u00b7heit", "sich", "in", "die\u00b7ses", "nichts", "be\u00b7ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "ART", "NN", "PRF", "APPR", "PDAT", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Vnaussprechlichkeit so alles sprichet aus /", "tokens": ["die", "Vn\u00b7aus\u00b7sprech\u00b7lich\u00b7keit", "so", "al\u00b7les", "spri\u00b7chet", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "macht die die Schwachheit dann zu ihrer Allmacht Haus?", "tokens": ["macht", "die", "die", "Schwach\u00b7heit", "dann", "zu", "ih\u00b7rer", "All\u00b7macht", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "will den Fu\u00dfsch\u00e4mel sie zum Ehren-Thron erheben?", "tokens": ["will", "den", "Fu\u00df\u00b7sch\u00e4\u00b7mel", "sie", "zum", "Eh\u00b7ren\u00b7Thron", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "der Vnbegreiffliche / will auf den H\u00e4nden schweben.", "tokens": ["der", "Vn\u00b7be\u00b7greif\u00b7fli\u00b7che", "/", "will", "auf", "den", "H\u00e4n\u00b7den", "schwe\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der Erd' und Himmel regt mit seines Donners Brau\u00df /", "tokens": ["Der", "Erd'", "und", "Him\u00b7mel", "regt", "mit", "sei\u00b7nes", "Don\u00b7ners", "Brau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "was mehr! das Meer verfelst mit seines Athems Sau\u00df /", "tokens": ["was", "mehr", "!", "das", "Meer", "ver\u00b7felst", "mit", "sei\u00b7nes", "A\u00b7thems", "Sau\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "denkt in dem keuschen Glas / der Jungfrau Leib / zu leben.", "tokens": ["denkt", "in", "dem", "keu\u00b7schen", "Glas", "/", "der", "Jung\u00b7frau", "Leib", "/", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "$(", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "De\u00df Wort-und Wei\u00dfheit-Krafft das ganze Seyn gebohren /", "tokens": ["De\u00df", "Wor\u00b7t\u00b7und", "Wei\u00df\u00b7heit\u00b7Krafft", "das", "gan\u00b7ze", "Seyn", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "l\u00e4st aus dem Staub der Zeit selb selbst geb\u00e4hren sich.", "tokens": ["l\u00e4st", "aus", "dem", "Staub", "der", "Zeit", "selb", "selbst", "ge\u00b7b\u00e4h\u00b7ren", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "ADV", "VVFIN", "PRF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Gott wird ein Mensch / auf da\u00df der Mensch sey unverlohren.", "tokens": ["Gott", "wird", "ein", "Mensch", "/", "auf", "da\u00df", "der", "Mensch", "sey", "un\u00b7ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$(", "APPR", "KOUS", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein Elends-Abgrund Stand / ehrt Himmelhebend dich.", "tokens": ["Sein", "E\u00b7lends\u00b7Ab\u00b7grund", "Stand", "/", "ehrt", "Him\u00b7mel\u00b7he\u00b7bend", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$(", "VVFIN", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "La\u00df allen Hoffarts Pracht! weil Demut ihm erkohren", "tokens": ["La\u00df", "al\u00b7len", "Hof\u00b7farts", "Pracht", "!", "weil", "De\u00b7mut", "ihm", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "NN", "$.", "KOUS", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der H\u00f6chste / folgt / sie sey die Hochheit wesentlich.", "tokens": ["der", "H\u00f6chs\u00b7te", "/", "folgt", "/", "sie", "sey", "die", "Hoch\u00b7heit", "we\u00b7sent\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "$(", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}