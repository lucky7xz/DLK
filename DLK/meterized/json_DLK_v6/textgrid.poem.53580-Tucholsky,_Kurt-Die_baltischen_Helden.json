{"textgrid.poem.53580": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die baltischen Helden", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollt ich einem Bauern dienen", "tokens": ["Sollt", "ich", "ei\u00b7nem", "Bau\u00b7ern", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mein Brot mit Schwei\u00df verdienen?", "tokens": ["und", "mein", "Brot", "mit", "Schwei\u00df", "ver\u00b7die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bruder, nein, das mag ich nicht!", "tokens": ["Bru\u00b7der", ",", "nein", ",", "das", "mag", "ich", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich will lieber in dem Felde", "tokens": ["Ich", "will", "lie\u00b7ber", "in", "dem", "Fel\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "mir verschaffen Brot und Gelde,", "tokens": ["mir", "ver\u00b7schaf\u00b7fen", "Brot", "und", "Gel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wo man von den Waffen spricht.", "tokens": ["wo", "man", "von", "den", "Waf\u00b7fen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Keinem Bauern dien ich nicht.", "tokens": ["Kei\u00b7nem", "Bau\u00b7ern", "di\u00b7en", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Die Sonne blinkt golden, der Himmel ist blau,", "tokens": ["Die", "Son\u00b7ne", "blinkt", "gol\u00b7den", ",", "der", "Him\u00b7mel", "ist", "blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und doch stehen wir drin im Oktober.", "tokens": ["und", "doch", "ste\u00b7hen", "wir", "drin", "im", "Ok\u00b7to\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Leb wohl, s\u00fc\u00dfes M\u00e4del! Leb wohl, kleine Frau!", "tokens": ["Leb", "wohl", ",", "s\u00fc\u00b7\u00dfes", "M\u00e4\u00b7del", "!", "Leb", "wohl", ",", "klei\u00b7ne", "Frau", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ADJA", "NN", "$.", "VVIMP", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und gr\u00fc\u00dft mir unsern Heuschober!", "tokens": ["Und", "gr\u00fc\u00dft", "mir", "un\u00b7sern", "Heu\u00b7scho\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Hauptmann verlas heut den Tagesbefehl:", "tokens": ["Der", "Haupt\u00b7mann", "ver\u00b7las", "heut", "den", "Ta\u00b7ges\u00b7be\u00b7fehl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u00bbmarsch\u2013marsch gegen den Bolschewiken!\u00ab", "tokens": ["\u00bb", "marsch", "\u2013", "marsch", "ge\u00b7gen", "den", "Bol\u00b7sche\u00b7wi\u00b7ken", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$(", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wir bleuen ihm die Jacke, wir schie\u00dfen nie fehl", "tokens": ["Wir", "bleu\u00b7en", "ihm", "die", "Ja\u00b7cke", ",", "wir", "schie\u00b7\u00dfen", "nie", "fehl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und klopfen ihm dann auf den R\u00fccken.", "tokens": ["und", "klop\u00b7fen", "ihm", "dann", "auf", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Sommer durch lagen wir gern im Quartier", "tokens": ["Den", "Som\u00b7mer", "durch", "la\u00b7gen", "wir", "gern", "im", "Quar\u00b7tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und griffen manch M\u00e4del ums Mieder;", "tokens": ["und", "grif\u00b7fen", "manch", "M\u00e4\u00b7del", "ums", "Mie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "doch wir sind Soldaten, nicht Puppen zur Zier.", "tokens": ["doch", "wir", "sind", "Sol\u00b7da\u00b7ten", ",", "nicht", "Pup\u00b7pen", "zur", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$,", "PTKNEG", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Vom Rasten rosten die Glieder.", "tokens": ["Vom", "Ras\u00b7ten", "ros\u00b7ten", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich nehm die Patronen und lad mein Gewehr.", "tokens": ["Ich", "nehm", "die", "Pat\u00b7ro\u00b7nen", "und", "lad", "mein", "Ge\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NE", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Platz da, du Sau-Bolschewike!", "tokens": ["Platz", "da", ",", "du", "Sau\u00b7Bol\u00b7sche\u00b7wi\u00b7ke", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es pfeift meine Kugel, der Kerl f\u00e4llt verquer,", "tokens": ["Es", "pfeift", "mei\u00b7ne", "Ku\u00b7gel", ",", "der", "Kerl", "f\u00e4llt", "ver\u00b7quer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ich treff in den Kopf jede M\u00fccke.", "tokens": ["ich", "treff", "in", "den", "Kopf", "je\u00b7de", "M\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Potzbombengranaten! Potzschockschwernot!", "tokens": ["Potz\u00b7bom\u00b7ben\u00b7gra\u00b7na\u00b7ten", "!", "Potz\u00b7schock\u00b7schwer\u00b7not", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nun schert euch zum Teufel, ihr Luder!", "tokens": ["Nun", "schert", "euch", "zum", "Teu\u00b7fel", ",", "ihr", "Lu\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ihr stehlt unserm Herrgott jedes Pfund Brot,", "tokens": ["Ihr", "stehlt", "un\u00b7serm", "Herr\u00b7gott", "je\u00b7des", "Pfund", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIAT", "NN", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "solang eure Juden am Ruder.", "tokens": ["so\u00b7lang", "eu\u00b7re", "Ju\u00b7den", "am", "Ru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Nie wart ihr Soldaten, ihr Helden vom Maul,", "tokens": ["Nie", "wart", "ihr", "Sol\u00b7da\u00b7ten", ",", "ihr", "Hel\u00b7den", "vom", "Maul", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "nur Meuchelm\u00f6rder und R\u00e4uber,", "tokens": ["nur", "Meu\u00b7chel\u00b7m\u00f6r\u00b7der", "und", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "eure Knobloch-Regenten, stinkend und faul,", "tokens": ["eu\u00b7re", "Kno\u00b7bloch\u00b7Re\u00b7gen\u00b7ten", ",", "stin\u00b7kend", "und", "faul", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "sind Schuster und allerhand Schreiber!", "tokens": ["sind", "Schus\u00b7ter", "und", "al\u00b7ler\u00b7hand", "Schrei\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Wir weisen euch, Schuster, den w\u00fcrdigen Platz", "tokens": ["Wir", "wei\u00b7sen", "euch", ",", "Schus\u00b7ter", ",", "den", "w\u00fcr\u00b7di\u00b7gen", "Platz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "und stellen euch an die W\u00e4nde . . .", "tokens": ["und", "stel\u00b7len", "euch", "an", "die", "W\u00e4n\u00b7de", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Petersburg sind wir mit m\u00e4chtigem Satz", "tokens": ["In", "Pe\u00b7ters\u00b7burg", "sind", "wir", "mit", "m\u00e4ch\u00b7ti\u00b7gem", "Satz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "und sch\u00fctteln dort Br\u00fcdern die H\u00e4nde.", "tokens": ["und", "sch\u00fct\u00b7teln", "dort", "Br\u00fc\u00b7dern", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}