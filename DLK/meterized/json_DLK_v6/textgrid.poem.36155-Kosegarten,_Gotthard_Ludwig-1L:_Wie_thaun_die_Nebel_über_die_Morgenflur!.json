{"textgrid.poem.36155": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie thaun die Nebel \u00fcber die Morgenflur!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie thaun die Nebel \u00fcber die Morgenflur!", "tokens": ["Wie", "thaun", "die", "Ne\u00b7bel", "\u00fc\u00b7ber", "die", "Mor\u00b7gen\u00b7flur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie freundlich nickt der regnende Bl\u00fcthenbaum", "tokens": ["Wie", "freund\u00b7lich", "nickt", "der", "reg\u00b7nen\u00b7de", "Bl\u00fc\u00b7then\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mir seinen Morgengru\u00df durchs Fenster,", "tokens": ["Mir", "sei\u00b7nen", "Mor\u00b7gen\u00b7gru\u00df", "durchs", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Golden vom j\u00fcngsten der Sonnenstrahlen.", "tokens": ["Gol\u00b7den", "vom", "j\u00fcng\u00b7sten", "der", "Son\u00b7nen\u00b7strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ich will dich feiern, heiliger, sch\u00f6ner Tag!", "tokens": ["Ich", "will", "dich", "fei\u00b7ern", ",", "hei\u00b7li\u00b7ger", ",", "sch\u00f6\u00b7ner", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Wonn' und Wehmuth feiern \u2013 So feierten", "tokens": ["Mit", "Wonn'", "und", "Weh\u00b7muth", "fei\u00b7ern", "\u2013", "So", "fei\u00b7er\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$(", "ADV", "VVFIN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dich Christus J\u00fcnger, als ihr Meister", "tokens": ["Dich", "Chris\u00b7tus", "J\u00fcn\u00b7ger", ",", "als", "ihr", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NE", "NE", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihnen entschwebte vom hohen Tabor.", "tokens": ["Ih\u00b7nen", "ent\u00b7schweb\u00b7te", "vom", "ho\u00b7hen", "Ta\u00b7bor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch stand er unter ihnen. Itzt segnete", "tokens": ["Noch", "stand", "er", "un\u00b7ter", "ih\u00b7nen", ".", "Itzt", "seg\u00b7ne\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$.", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie seine Rechte. R\u00fchrender scholl sein Wort \u2013", "tokens": ["Sie", "sei\u00b7ne", "Rech\u00b7te", ".", "R\u00fch\u00b7ren\u00b7der", "scholl", "sein", "Wort", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "NN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und immer heller ward sein Antlitz,", "tokens": ["Und", "im\u00b7mer", "hel\u00b7ler", "ward", "sein", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Strahlender immer, und immer hehrer.", "tokens": ["Strah\u00b7len\u00b7der", "im\u00b7mer", ",", "und", "im\u00b7mer", "heh\u00b7rer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Itzt s\u00e4uselt' es, wie S\u00e4usel im Zedernhain \u2013", "tokens": ["Itzt", "s\u00e4u\u00b7selt'", "es", ",", "wie", "S\u00e4u\u00b7sel", "im", "Ze\u00b7dern\u00b7hain", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "NN", "APPRART", "NN", "$("], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Itzt h\u00fcllten Wolken, duftig und goldbes\u00e4umt,", "tokens": ["Itzt", "h\u00fcll\u00b7ten", "Wol\u00b7ken", ",", "duf\u00b7tig", "und", "gold\u00b7be\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Gottgeliebten. \u2013 Itzt entschwebt' er", "tokens": ["Den", "Gott\u00b7ge\u00b7lieb\u00b7ten", ".", "\u2013", "Itzt", "ent\u00b7schwebt'", "er"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Feierlich langsam seinen Freunden!", "tokens": ["Fei\u00b7er\u00b7lich", "lang\u00b7sam", "sei\u00b7nen", "Freun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Wie standen die Verla\u00dfnen! Wie streckten sie", "tokens": ["Wie", "stan\u00b7den", "die", "Ver\u00b7la\u00df\u00b7nen", "!", "Wie", "streck\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihm nach die hei\u00dfen Arme! Wie starrt' ihr Blick!", "tokens": ["Ihm", "nach", "die", "hei\u00b7\u00dfen", "Ar\u00b7me", "!", "Wie", "starrt'", "ihr", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun sah'n sie ihn nicht mehr. \u2013 Nun st\u00fcrzten", "tokens": ["Nun", "sah'n", "sie", "ihn", "nicht", "mehr", ".", "\u2013", "Nun", "st\u00fcrz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$.", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Thr\u00e4nen der Wehmuth und Wonnethr\u00e4nen!", "tokens": ["Thr\u00e4\u00b7nen", "der", "Weh\u00b7muth", "und", "Won\u00b7net\u00b7hr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Ich will dich feiern, heiliger, sch\u00f6ner Tag!", "tokens": ["Ich", "will", "dich", "fei\u00b7ern", ",", "hei\u00b7li\u00b7ger", ",", "sch\u00f6\u00b7ner", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bist du es nicht, an welchem vor zwanzig sechs", "tokens": ["Bist", "du", "es", "nicht", ",", "an", "wel\u00b7chem", "vor", "zwan\u00b7zig", "sechs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "$,", "APPR", "PRELS", "APPR", "CARD", "CARD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verbl\u00fchten Lenzen meine Mutter", "tokens": ["Ver\u00b7bl\u00fch\u00b7ten", "Len\u00b7zen", "mei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieder zur himmlischen Heimath kehrte?", "tokens": ["Wie\u00b7der", "zur", "himm\u00b7li\u00b7schen", "Hei\u00b7math", "kehr\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Noch rang sie auf dem Lager \u2013 es trauerte", "tokens": ["Noch", "rang", "sie", "auf", "dem", "La\u00b7ger", "\u2013", "es", "trau\u00b7er\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der erste Gatt'! Es wimmerten um ihr Bett", "tokens": ["Der", "ers\u00b7te", "Gatt'", "!", "Es", "wim\u00b7mer\u00b7ten", "um", "ihr", "Bett"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die Kinder ihres Herzens \u2013 Weinend", "tokens": ["Die", "Kin\u00b7der", "ih\u00b7res", "Her\u00b7zens", "\u2013", "Wei\u00b7nend"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sah sie gen Himmel. Der Himmel sah sie.", "tokens": ["Sah", "sie", "gen", "Him\u00b7mel", ".", "Der", "Him\u00b7mel", "sah", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie sprach zum Gatten: Friede, mein Trauter, dir!", "tokens": ["Sie", "sprach", "zum", "Gat\u00b7ten", ":", "Frie\u00b7de", ",", "mein", "Trau\u00b7ter", ",", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und: Friede sey mit euch! zu den Wimmernden!", "tokens": ["Und", ":", "Frie\u00b7de", "sey", "mit", "euch", "!", "zu", "den", "Wim\u00b7mern\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$.", "NN", "VAFIN", "APPR", "PPER", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und immer stiller ward ihr Antlitz,", "tokens": ["Und", "im\u00b7mer", "stil\u00b7ler", "ward", "ihr", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruhiger immer und immer heller! \u2013", "tokens": ["Ru\u00b7hi\u00b7ger", "im\u00b7mer", "und", "im\u00b7mer", "hel\u00b7ler", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "ADJD", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und Engelstimmen flisterten: \u00bbKomm hinweg!", "tokens": ["Und", "En\u00b7gel\u00b7stim\u00b7men", "flis\u00b7ter\u00b7ten", ":", "\u00bb", "Komm", "hin\u00b7weg", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$.", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Komm, theure Schwester! Dulderinn! komm hinweg!\u00ab", "tokens": ["Komm", ",", "theu\u00b7re", "Schwes\u00b7ter", "!", "Dul\u00b7de\u00b7rinn", "!", "komm", "hin\u00b7weg", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "NN", "$.", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da brach ihr m\u00fcdes Aug'. Ihr Schutzgeist", "tokens": ["Da", "brach", "ihr", "m\u00fc\u00b7des", "Aug'", ".", "Ihr", "Schutz\u00b7geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trug sie in Veilchengew\u00f6lk gen Himmel.", "tokens": ["Trug", "sie", "in", "Veil\u00b7chen\u00b7ge\u00b7w\u00f6lk", "gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Ich will dich feiern, heiliger, sch\u00f6ner Tag!", "tokens": ["Ich", "will", "dich", "fei\u00b7ern", ",", "hei\u00b7li\u00b7ger", ",", "sch\u00f6\u00b7ner", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Wonn' und Wehmuth feiern, so oft du kehrst \u2013", "tokens": ["Mit", "Wonn'", "und", "Weh\u00b7muth", "fei\u00b7ern", ",", "so", "oft", "du", "kehrst", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,", "ADV", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und kehrst du mir nicht mehr im Staube \u2013", "tokens": ["Und", "kehrst", "du", "mir", "nicht", "mehr", "im", "Stau\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wonne, so feir' ich dich ihr am Busen!", "tokens": ["Won\u00b7ne", ",", "so", "feir'", "ich", "dich", "ihr", "am", "Bu\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "APPRART", "NN", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}}}}