{"textgrid.poem.61447": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Mein Friedensschlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl netzt' ich hei\u00df mit Tr\u00e4nen meine Pf\u00fchle", "tokens": ["Wohl", "netzt'", "ich", "hei\u00df", "mit", "Tr\u00e4\u00b7nen", "mei\u00b7ne", "Pf\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und rang in Qualen, mich emporzuhalten;", "tokens": ["Und", "rang", "in", "Qua\u00b7len", ",", "mich", "em\u00b7por\u00b7zu\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn furchtbar brannte dieser Zeiten Schw\u00fcle.", "tokens": ["Denn", "furcht\u00b7bar", "brann\u00b7te", "die\u00b7ser", "Zei\u00b7ten", "Schw\u00fc\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es lag die Welt in grimmem Kampf zerspalten,", "tokens": ["Es", "lag", "die", "Welt", "in", "grim\u00b7mem", "Kampf", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zu der Heere keinem konnt' ich stehen;", "tokens": ["Und", "zu", "der", "Hee\u00b7re", "kei\u00b7nem", "konnt'", "ich", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIS", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier sah ich Wahnsinn, dort Verstocktheit walten.", "tokens": ["Hier", "sah", "ich", "Wahn\u00b7sinn", ",", "dort", "Ver\u00b7stock\u00b7theit", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das allertiefste Weh war mir geschehen;", "tokens": ["Das", "al\u00b7ler\u00b7tiefs\u00b7te", "Weh", "war", "mir", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn meiner Sehnsucht Bild, nun war's gekommen,", "tokens": ["Denn", "mei\u00b7ner", "Sehn\u00b7sucht", "Bild", ",", "nun", "wa\u00b7r's", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch w\u00fcst verzerrt, ein Greuel anzusehen.", "tokens": ["Doch", "w\u00fcst", "ver\u00b7zerrt", ",", "ein", "Greu\u00b7el", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das trieb mich rastlos um, von Gram beklommen;", "tokens": ["Das", "trieb", "mich", "rast\u00b7los", "um", ",", "von", "Gram", "be\u00b7klom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch endlich, als ich lange N\u00e4cht' und Tage", "tokens": ["Doch", "end\u00b7lich", ",", "als", "ich", "lan\u00b7ge", "N\u00e4cht'", "und", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gerungen, ward von mir die Last genommen.", "tokens": ["Ge\u00b7run\u00b7gen", ",", "ward", "von", "mir", "die", "Last", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nur wem das Schicksal stumm ist, der verzage;", "tokens": ["Nur", "wem", "das", "Schick\u00b7sal", "stumm", "ist", ",", "der", "ver\u00b7za\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "ADJD", "VAFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu wem der Gott spricht aus der Weltgeschichte,", "tokens": ["Zu", "wem", "der", "Gott", "spricht", "aus", "der", "Welt\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem singt er Trost zuletzt zur Zeit der Plage.", "tokens": ["Dem", "singt", "er", "Trost", "zu\u00b7letzt", "zur", "Zeit", "der", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Durch blasse D\u00e4mmrung f\u00fchrt er ihn zum Lichte", "tokens": ["Durch", "blas\u00b7se", "D\u00e4mm\u00b7rung", "f\u00fchrt", "er", "ihn", "zum", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zeigt ihm wie von hoher Bergeszinne,", "tokens": ["Und", "zeigt", "ihm", "wie", "von", "ho\u00b7her", "Ber\u00b7ges\u00b7zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergangnes und Zuk\u00fcnft'ges im Gesichte.", "tokens": ["Ver\u00b7gang\u00b7nes", "und", "Zu\u00b7k\u00fcnft'\u00b7ges", "im", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und so von ihm geleitet ward ich inne:", "tokens": ["Und", "so", "von", "ihm", "ge\u00b7lei\u00b7tet", "ward", "ich", "in\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es k\u00e4mpft sich ein Gedank' in br\u00fcnst'gem Hoffen", "tokens": ["Es", "k\u00e4mpft", "sich", "ein", "Ge\u00b7dank'", "in", "br\u00fcnst'\u00b7gem", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch jede Zeit, da\u00df er Gestalt gewinne.", "tokens": ["Durch", "je\u00b7de", "Zeit", ",", "da\u00df", "er", "Ge\u00b7stalt", "ge\u00b7win\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch in den Staub geboren weist er offen", "tokens": ["Doch", "in", "den", "Staub", "ge\u00b7bo\u00b7ren", "weist", "er", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht gleich sein Antlitz; Geist und Bild sind zweie;", "tokens": ["Nicht", "gleich", "sein", "Ant\u00b7litz", ";", "Geist", "und", "Bild", "sind", "zwei\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "$.", "NN", "KON", "NN", "VAFIN", "CARD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verh\u00fcllt erst gl\u00fcht er unter niedern Stoffen.", "tokens": ["Ver\u00b7h\u00fcllt", "erst", "gl\u00fcht", "er", "un\u00b7ter", "nie\u00b7dern", "Stof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Durch mi\u00dfgeschaffner Formen lange Reihe", "tokens": ["Durch", "mi\u00df\u00b7ge\u00b7schaff\u00b7ner", "For\u00b7men", "lan\u00b7ge", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Seelenwandrung hat er zu vollenden,", "tokens": ["Die", "See\u00b7len\u00b7wand\u00b7rung", "hat", "er", "zu", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis er verkl\u00e4rt ergl\u00e4nzt im Licht der Weihe.", "tokens": ["Bis", "er", "ver\u00b7kl\u00e4rt", "er\u00b7gl\u00e4nzt", "im", "Licht", "der", "Wei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "So rang der Vorwelt Sehnsucht aller Enden", "tokens": ["So", "rang", "der", "Vor\u00b7welt", "Sehn\u00b7sucht", "al\u00b7ler", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Sch\u00f6nen; doch bis sie's gelernt zu fassen,", "tokens": ["Zum", "Sch\u00f6\u00b7nen", ";", "doch", "bis", "sie's", "ge\u00b7lernt", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "APPR", "NE", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie tastete sie lang mit schweren H\u00e4nden!", "tokens": ["Wie", "tas\u00b7te\u00b7te", "sie", "lang", "mit", "schwe\u00b7ren", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wie lange band sie Dinge, die sich hassen,", "tokens": ["Wie", "lan\u00b7ge", "band", "sie", "Din\u00b7ge", ",", "die", "sich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Bau der Sphinx, im Zwitterleib des Greifen", "tokens": ["Im", "Bau", "der", "Sphinx", ",", "im", "Zwit\u00b7ter\u00b7leib", "des", "Grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und t\u00fcrmte schwunglos tr\u00fcbgedr\u00fcckte Massen.", "tokens": ["Und", "t\u00fcrm\u00b7te", "schwung\u00b7los", "tr\u00fcb\u00b7ge\u00b7dr\u00fcck\u00b7te", "Mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und dennoch lag im Wilden, Rohen, Steifen", "tokens": ["Und", "den\u00b7noch", "lag", "im", "Wil\u00b7den", ",", "Ro\u00b7hen", ",", "Stei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Keim schon, der bestimmt war, einst im Bilde", "tokens": ["Der", "Keim", "schon", ",", "der", "be\u00b7stimmt", "war", ",", "einst", "im", "Bil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "VVPP", "VAFIN", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Schaumgebornen wonnig auszureifen,", "tokens": ["Der", "Schaum\u00b7ge\u00b7bor\u00b7nen", "won\u00b7nig", "aus\u00b7zu\u00b7rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Wie sie mit G\u00f6tterl\u00e4cheln die Gefilde", "tokens": ["Wie", "sie", "mit", "G\u00f6t\u00b7ter\u00b7l\u00e4\u00b7cheln", "die", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchzieht und tausend Blumen weckt im Schreiten,", "tokens": ["Durch\u00b7zieht", "und", "tau\u00b7send", "Blu\u00b7men", "weckt", "im", "Schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "CARD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ganz Liebreiz, ganz Holdseligkeit und Milde. \u2013", "tokens": ["Ganz", "Lieb\u00b7reiz", ",", "ganz", "Hold\u00b7se\u00b7lig\u00b7keit", "und", "Mil\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.14": {"line.1": {"text": "Nun geht der Freiheit Geist durch ", "tokens": ["Nun", "geht", "der", "Frei\u00b7heit", "Geist", "durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Massen r\u00fchrt er, da\u00df sie sich getrauen,", "tokens": ["Die", "Mas\u00b7sen", "r\u00fchrt", "er", ",", "da\u00df", "sie", "sich", "ge\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach dumpfem Sinn den Leib ihm zu bereiten.", "tokens": ["Nach", "dum\u00b7pfem", "Sinn", "den", "Leib", "ihm", "zu", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Doch eine Binde liegt um ihre Brauen,", "tokens": ["Doch", "ei\u00b7ne", "Bin\u00b7de", "liegt", "um", "ih\u00b7re", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Tun ist ma\u00dflos, fiebrisch ihr Geb\u00e4rden;", "tokens": ["Ihr", "Tun", "ist", "ma\u00df\u00b7los", ",", "fie\u00b7brisch", "ihr", "Ge\u00b7b\u00e4r\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur eine G\u00f6tzin schaffen sie voll Grauen.", "tokens": ["Nur", "ei\u00b7ne", "G\u00f6t\u00b7zin", "schaf\u00b7fen", "sie", "voll", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Und tausend Opfer fallen ihr auf Erden,", "tokens": ["Und", "tau\u00b7send", "Op\u00b7fer", "fal\u00b7len", "ihr", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word",