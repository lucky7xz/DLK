{"dta.poem.12382": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der eifers\u00fcchtige Knabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es stehen drey Stern' am Himmel,               ", "tokens": ["Es", "ste\u00b7hen", "drey", "Stern'", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die geben der Lieb' ihren Schein:", "tokens": ["Die", "ge\u00b7ben", "der", "Lieb'", "ih\u00b7ren", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u201egott gr\u00fc\u00df euch, sch\u00f6nes Jungfr\u00e4ulein,", "tokens": ["\u201e", "gott", "gr\u00fc\u00df", "euch", ",", "sch\u00f6\u00b7nes", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ewo bind' ich mein R\u00f6sselein hin?\u201c", "tokens": ["\u201e", "wo", "bind'", "ich", "mein", "R\u00f6s\u00b7sel\u00b7ein", "hin", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "\u201enimm du es, dein R\u00f6\u00dflein, beim Z\u00fcgel, beim Zaum,", "tokens": ["\u201e", "nimm", "du", "es", ",", "dein", "R\u00f6\u00df\u00b7lein", ",", "beim", "Z\u00fc\u00b7gel", ",", "beim", "Zaum", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201ebind's an den Feigenbaum.", "tokens": ["\u201e", "bin\u00b7d's", "an", "den", "Fei\u00b7gen\u00b7baum", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201esetz dich ein' kleine Weile nieder,", "tokens": ["\u201e", "setz", "dich", "ein'", "klei\u00b7ne", "Wei\u00b7le", "nie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eund mach mir eine kleine Kurzweil.\u201c", "tokens": ["\u201e", "und", "mach", "mir", "ei\u00b7ne", "klei\u00b7ne", "Kurz\u00b7weil", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eich kann und mag nicht sitzen,", "tokens": ["\u201e", "ich", "kann", "und", "mag", "nicht", "sit\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201emag auch nicht lustig seyn,", "tokens": ["\u201e", "mag", "auch", "nicht", "lus\u00b7tig", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADV", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u201emein Herz ist mir betr\u00fcbet,", "tokens": ["\u201e", "mein", "Herz", "ist", "mir", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201efeinslieb von wegen dein.\u201c", "tokens": ["\u201e", "feins\u00b7lieb", "von", "we\u00b7gen", "dein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "APPR", "APPR", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was zog er aus der Taschen?", "tokens": ["Was", "zog", "er", "aus", "der", "Ta\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Messer, war scharf und spitz,", "tokens": ["Ein", "Mes\u00b7ser", ",", "war", "scharf", "und", "spitz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er stachs seiner Lieben durchs Herze,", "tokens": ["Er", "stachs", "sei\u00b7ner", "Lie\u00b7ben", "durchs", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "APPRART", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das rothe Blut gegen ihn spritzt.", "tokens": ["Das", "ro\u00b7the", "Blut", "ge\u00b7gen", "ihn", "spritzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und da ers wieder herausser zog,", "tokens": ["Und", "da", "ers", "wie\u00b7der", "her\u00b7aus\u00b7ser", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Von Blut war es so roth:", "tokens": ["Von", "Blut", "war", "es", "so", "roth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201each reicher Gott vom Himmel,", "tokens": ["\u201e", "ach", "rei\u00b7cher", "Gott", "vom", "Him\u00b7mel", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ewie bitter wird mir der Tod!\u201c", "tokens": ["\u201e", "wie", "bit\u00b7ter", "wird", "mir", "der", "Tod", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Was zog er ihr abe vom Finger?", "tokens": ["Was", "zog", "er", "ihr", "a\u00b7be", "vom", "Fin\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein rothes Goldringelein,", "tokens": ["Ein", "ro\u00b7thes", "Gold\u00b7rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er warfs in fliessend Wasser,", "tokens": ["Er", "warfs", "in", "flies\u00b7send", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es gab seinen klaren Schein:", "tokens": ["Es", "gab", "sei\u00b7nen", "kla\u00b7ren", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201eschwimm hin, schwimm her, Goldringelein!", "tokens": ["\u201e", "schwimm", "hin", ",", "schwimm", "her", ",", "Gold\u00b7rin\u00b7ge\u00b7lein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "\u201ebis an den tiefen See!", "tokens": ["\u201e", "bis", "an", "den", "tie\u00b7fen", "See", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201emein Feinslieb ist mir gestorben,", "tokens": ["\u201e", "mein", "Feins\u00b7lieb", "ist", "mir", "ge\u00b7stor\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ejzt hab ich kein Feinslieb mehr.\u201c", "tokens": ["\u201e", "jzt", "hab", "ich", "kein", "Feins\u00b7lieb", "mehr", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "So gehts, wenn ein M\u00e4del zwei Knaben lieb hat,", "tokens": ["So", "gehts", ",", "wenn", "ein", "M\u00e4\u00b7del", "zwei", "Kna\u00b7ben", "lieb", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "CARD", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Thut wunderselten gut;", "tokens": ["Thut", "wun\u00b7der\u00b7sel\u00b7ten", "gut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das haben wir Beyd' erfahren,", "tokens": ["Das", "ha\u00b7ben", "wir", "Beyd'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was falsche Liebe thut.", "tokens": ["Was", "fal\u00b7sche", "Lie\u00b7be", "thut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}