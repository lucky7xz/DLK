{"dta.poem.5500": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00e4ntzen der Vernunft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald ein Feuer-reicher Geist sich auf ein tieffes", "tokens": ["So", "bald", "ein", "Feu\u00b7er\u00b7rei\u00b7cher", "Geist", "sich", "auf", "ein", "tief\u00b7fes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "raisoniren,", "tokens": ["rai\u00b7so\u00b7ni\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und von ", "tokens": ["Und", "von"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Mit angespannten Kr\u00e4ften leget; begiebet er sich auf ein", "tokens": ["Mit", "an\u00b7ge\u00b7spann\u00b7ten", "Kr\u00e4f\u00b7ten", "le\u00b7get", ";", "be\u00b7gie\u00b7bet", "er", "sich", "auf", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "VVFIN", "PPER", "PRF", "APPR", "ART"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Meer,", "tokens": ["Meer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wo Zweifels-Wirbel, Meynungs-Wellen, ihn unaufh\u00f6r-", "tokens": ["Wo", "Zwei\u00b7fels\u00b7Wir\u00b7bel", ",", "Mey\u00b7nungs\u00b7Wel\u00b7len", ",", "ihn", "un\u00b7auf\u00b7h\u00f6r"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$,", "NN", "$,", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "lich hin und her", "tokens": ["lich", "hin", "und", "her"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "In stetem Jrthum schlenckern werden; wo Vorurtheile", "tokens": ["In", "ste\u00b7tem", "Jr\u00b7thum", "schlen\u00b7ckern", "wer\u00b7den", ";", "wo", "Vor\u00b7urt\u00b7hei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VAFIN", "$.", "PWAV", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "sich bem\u00fchn,", "tokens": ["sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "In tausend Boden-losen Strudeln, ihn in des Abgrunds", "tokens": ["In", "tau\u00b7send", "Bo\u00b7den\u00b7lo\u00b7sen", "Stru\u00b7deln", ",", "ihn", "in", "des", "Ab\u00b7grunds"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "NN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.11": {"text": "Gruft zu ziehn.", "tokens": ["Gruft", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Will er nun nicht der Seelen Ruh, die Seele, ja, fast", "tokens": ["Will", "er", "nun", "nicht", "der", "See\u00b7len", "Ruh", ",", "die", "See\u00b7le", ",", "ja", ",", "fast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "PTKANT", "$,", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Gott, verlieren,", "tokens": ["Gott", ",", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "(wie, wenn er sich auf eigne Kr\u00e4fte verl\u00e4sset, leider", "tokens": ["(", "wie", ",", "wenn", "er", "sich", "auf", "eig\u00b7ne", "Kr\u00e4f\u00b7te", "ver\u00b7l\u00e4s\u00b7set", ",", "lei\u00b7der"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "PWAV", "$,", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,", "ADV"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.15": {"text": "oft geschicht,)", "tokens": ["oft", "ge\u00b7schicht", ",", ")"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADV", "VVPP", "$,", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "So wehl\u2019 er in der Finsterni\u00df nur blos die Demuth sich", "tokens": ["So", "wehl'", "er", "in", "der", "Fins\u00b7ter\u00b7ni\u00df", "nur", "blos", "die", "De\u00b7muth", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "zum Licht.", "tokens": ["zum", "Licht", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Nur die allein kann unverletzt ihn in den sichern Haven", "tokens": ["Nur", "die", "al\u00b7lein", "kann", "un\u00b7ver\u00b7letzt", "ihn", "in", "den", "si\u00b7chern", "Ha\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "VMFIN", "ADJD", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.19": {"text": "f\u00fchren.", "tokens": ["f\u00fch\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Ich hab\u2019 es, GOtt sey Danck, erfahren, was, wenn ich", "tokens": ["Ich", "hab'", "es", ",", "Gott", "sey", "Danck", ",", "er\u00b7fah\u00b7ren", ",", "was", ",", "wenn", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "VAFIN", "NN", "$,", "VVINF", "$,", "PRELS", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "sonst versuncken w\u00e4re,", "tokens": ["sonst", "ver\u00b7sun\u00b7cken", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Sie mir vor H\u00fclf\u2019 und Rath geschafft. Durch diese sanft\u2019", "tokens": ["Sie", "mir", "vor", "H\u00fcl\u00b7f'", "und", "Rath", "ge\u00b7schafft", ".", "Durch", "die\u00b7se", "sanft'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$.", "APPR", "PDS", "VVFIN"], "meter": "-+-+--+-+---+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "und wahre Lehre:", "tokens": ["und", "wah\u00b7re", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Wer bist du? was ist dein Verstand? ist er von solcher", "tokens": ["Wer", "bist", "du", "?", "was", "ist", "dein", "Ver\u00b7stand", "?", "ist", "er", "von", "sol\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$.", "VAFIN", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sch\u00e4rff' und Kraft,", "tokens": ["Sch\u00e4rff'", "und", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Da\u00df er das innerste der Dinge, des Geists, der C\u00f6rper", "tokens": ["Da\u00df", "er", "das", "in\u00b7ners\u00b7te", "der", "Din\u00b7ge", ",", "des", "Geists", ",", "der", "C\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Eigenschaft", "tokens": ["Ei\u00b7gen\u00b7schaft"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Und die Natur zu fassen f\u00e4hig? GOtt hat ihn dir in die-", "tokens": ["Und", "die", "Na\u00b7tur", "zu", "fas\u00b7sen", "f\u00e4\u00b7hig", "?", "Gott", "hat", "ihn", "dir", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "ADJD", "$.", "NN", "VAFIN", "PPER", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.29": {"text": "sem Leben", "tokens": ["sem", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Gewi\u00df in einem reichen Maa\u00df und in so hohem Grad ge-", "tokens": ["Ge\u00b7wi\u00df", "in", "ei\u00b7nem", "rei\u00b7chen", "Maa\u00df", "und", "in", "so", "ho\u00b7hem", "Grad", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ADV", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.31": {"text": "geben,", "tokens": ["ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}}}}}