{"dta.poem.12926": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort der Sylvia an den Flo-  \n rindo, der ihr in einem schreiben  \n seine liebe entdecket.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Florindo schertzet nur: Ich sehe zwar sein schreiben;", "tokens": ["Flo\u00b7rin\u00b7do", "schert\u00b7zet", "nur", ":", "Ich", "se\u00b7he", "zwar", "sein", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein sein hertze liegt noch nicht in meiner hand,", "tokens": ["Al\u00b7lein", "sein", "hert\u00b7ze", "liegt", "noch", "nicht", "in", "mei\u00b7ner", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und w\u00e4r es auch schon da, wie lange w\u00fcrd\u2019 es bleiben?", "tokens": ["Und", "w\u00e4r", "es", "auch", "schon", "da", ",", "wie", "lan\u00b7ge", "w\u00fcrd'", "es", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,", "PWAV", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wei\u00df gewi\u00df, es liebt den s\u00fc\u00dfen unbestand.", "tokens": ["Ich", "wei\u00df", "ge\u00b7wi\u00df", ",", "es", "liebt", "den", "s\u00fc\u00b7\u00dfen", "un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn die ver\u00e4nderung bringt allezeit ergetzen.", "tokens": ["Denn", "die", "ver\u00b7\u00e4n\u00b7de\u00b7rung", "bringt", "al\u00b7le\u00b7zeit", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es schleicht bey einer kost sich bald der eckel ein,", "tokens": ["Es", "schleicht", "bey", "ei\u00b7ner", "kost", "sich", "bald", "der", "ec\u00b7kel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "VVFIN", "PRF", "ADV", "ART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und was man anfangs pflag dem golde gleich zu sch\u00e4tzen.", "tokens": ["Und", "was", "man", "an\u00b7fangs", "pflag", "dem", "gol\u00b7de", "gleich", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das mu\u00df nach kurtzer zeit wie altes eisen seyn.", "tokens": ["Das", "mu\u00df", "nach", "kurt\u00b7zer", "zeit", "wie", "al\u00b7tes", "ei\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ADJA", "NN", "KOKOM", "ADJA", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die fessel, so mein blick Florinden angeleget,", "tokens": ["Die", "fes\u00b7sel", ",", "so", "mein", "blick", "Flo\u00b7rin\u00b7den", "an\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Sind dem papiere nur, nicht dir und mir bewust.", "tokens": ["Sind", "dem", "pa\u00b7pie\u00b7re", "nur", ",", "nicht", "dir", "und", "mir", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADV", "$,", "PTKNEG", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja weil mein auge nichts von herrschsucht in sich heget,", "tokens": ["Ja", "weil", "mein", "au\u00b7ge", "nichts", "von", "herrschsucht", "in", "sich", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPOSAT", "NN", "PIS", "APPR", "VVFIN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "So nenn\u2019 ich deine band\u2019 ein spielwerck deiner lust.", "tokens": ["So", "nenn'", "ich", "dei\u00b7ne", "band'", "ein", "spiel\u00b7werck", "dei\u00b7ner", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein so galanter kopff kan leichtlich was ersinnen,", "tokens": ["Ein", "so", "ga\u00b7lan\u00b7ter", "kopff", "kan", "leicht\u00b7lich", "was", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VMFIN", "ADJD", "PWS", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das dichten steht ohndem den besten dichtern frey.", "tokens": ["Das", "dich\u00b7ten", "steht", "ohn\u00b7dem", "den", "bes\u00b7ten", "dich\u00b7tern", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PAV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Tr\u00fcg\u2019 ich den lorbeer-crantz, ich wolt\u2019 es auch beginnen,", "tokens": ["Tr\u00fcg'", "ich", "den", "lor\u00b7beer\u00b7crantz", ",", "ich", "wolt'", "es", "auch", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich schriebe, da\u00df mein hertz wie deines, br\u00fcnstig sey.", "tokens": ["Ich", "schrie\u00b7be", ",", "da\u00df", "mein", "hertz", "wie", "dei\u00b7nes", ",", "br\u00fcns\u00b7tig", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "KOKOM", "PPOSAT", "$,", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So aber, weil mich noch Apollo nicht gecr\u00f6net,", "tokens": ["So", "a\u00b7ber", ",", "weil", "mich", "noch", "A\u00b7pol\u00b7lo", "nicht", "ge\u00b7cr\u00f6\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "NE", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "So setz\u2019 ich weiter nichts, als was die wahrheit ist.", "tokens": ["So", "setz'", "ich", "wei\u00b7ter", "nichts", ",", "als", "was", "die", "wahr\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "$,", "KOUS", "PIS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich wei\u00df es ohnedem, da\u00df mich Florindo h\u00f6hnet,", "tokens": ["Ich", "wei\u00df", "es", "oh\u00b7ne\u00b7dem", ",", "da\u00df", "mich", "Flo\u00b7rin\u00b7do", "h\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "$,", "KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil man in meiner nacht gantz keinen glantz erkiest.", "tokens": ["Weil", "man", "in", "mei\u00b7ner", "nacht", "gantz", "kei\u00b7nen", "glantz", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie kan des himmels krafft in meinen augen stecken?", "tokens": ["Wie", "kan", "des", "him\u00b7mels", "krafft", "in", "mei\u00b7nen", "au\u00b7gen", "ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Legt eine g\u00f6ttin sich auch kranck, wie ich, dahin?", "tokens": ["Legt", "ei\u00b7ne", "g\u00f6t\u00b7tin", "sich", "auch", "kranck", ",", "wie", "ich", ",", "da\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mein glaube wei\u00df sich nicht so weit hinaus zu strecken,", "tokens": ["Mein", "glau\u00b7be", "wei\u00df", "sich", "nicht", "so", "weit", "hin\u00b7aus", "zu", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "ADV", "ADJD", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So leichtlich als ich sonst des glaubens f\u00e4hig bin.", "tokens": ["So", "leicht\u00b7lich", "als", "ich", "sonst", "des", "glau\u00b7bens", "f\u00e4\u00b7hig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein geist, wie deiner ist, der alles kan verachten,", "tokens": ["Ein", "geist", ",", "wie", "dei\u00b7ner", "ist", ",", "der", "al\u00b7les", "kan", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "VAFIN", "$,", "PRELS", "PIS", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der aus den perlen sand, und gold zu kothe macht,", "tokens": ["Der", "aus", "den", "per\u00b7len", "sand", ",", "und", "gold", "zu", "ko\u00b7the", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wird wol ein weibes-bild nicht zu verg\u00f6ttern trachten,", "tokens": ["Wird", "wol", "ein", "wei\u00b7bes\u00b7bild", "nicht", "zu", "ver\u00b7g\u00f6t\u00b7tern", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der selbst der Sch\u00f6pffer nur das dienen zugedacht.", "tokens": ["Der", "selbst", "der", "Sch\u00f6pf\u00b7fer", "nur", "das", "die\u00b7nen", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "PDS", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drum sage mir nicht vor: da\u00df ich dir was befohlen;", "tokens": ["Drum", "sa\u00b7ge", "mir", "nicht", "vor", ":", "da\u00df", "ich", "dir", "was", "be\u00b7foh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "KOUS", "PPER", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das bitten kommt uns zwar, doch kein befehlen zu.", "tokens": ["Das", "bit\u00b7ten", "kommt", "uns", "zwar", ",", "doch", "kein", "be\u00b7feh\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "PPER", "ADV", "$,", "ADV", "PIAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dein liebes-feuer ist ein feuer kalter kohlen,", "tokens": ["Dein", "lie\u00b7bes\u00b7feu\u00b7er", "ist", "ein", "feu\u00b7er", "kal\u00b7ter", "koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ART", "ADJA", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und deine leidenschafft l\u00e4st dich in guter ruh.", "tokens": ["Und", "dei\u00b7ne", "lei\u00b7den\u00b7schafft", "l\u00e4st", "dich", "in", "gu\u00b7ter", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Indessen kan ich es vor keine k\u00fchnheit schelten,", "tokens": ["In\u00b7des\u00b7sen", "kan", "ich", "es", "vor", "kei\u00b7ne", "k\u00fchn\u00b7heit", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df mir Florindens hand so nette verse schickt;", "tokens": ["Da\u00df", "mir", "Flo\u00b7rin\u00b7dens", "hand", "so", "net\u00b7te", "ver\u00b7se", "schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja kan mein bitten was bey deiner Muse gelten,", "tokens": ["Ja", "kan", "mein", "bit\u00b7ten", "was", "bey", "dei\u00b7ner", "Mu\u00b7se", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PPOSAT", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So werd\u2019 ich k\u00fcnfftig mehr durch ihren schertz erqvickt.", "tokens": ["So", "werd'", "ich", "k\u00fcnff\u00b7tig", "mehr", "durch", "ih\u00b7ren", "schertz", "er\u00b7qvickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}