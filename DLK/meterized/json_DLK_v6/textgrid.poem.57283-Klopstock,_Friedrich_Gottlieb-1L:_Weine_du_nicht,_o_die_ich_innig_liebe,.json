{"textgrid.poem.57283": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weine du nicht, o die ich innig liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weine du nicht, o die ich innig liebe,", "tokens": ["Wei\u00b7ne", "du", "nicht", ",", "o", "die", "ich", "in\u00b7nig", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "FM", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dass ein trauriger Tag von dir mich scheidet!", "tokens": ["Dass", "ein", "trau\u00b7ri\u00b7ger", "Tag", "von", "dir", "mich", "schei\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wenn nun wieder Hesperus dir dort l\u00e4chelt,", "tokens": ["Wenn", "nun", "wie\u00b7der", "Hes\u00b7pe\u00b7rus", "dir", "dort", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NE", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Komm', ich Gl\u00fccklicher, wieder!", "tokens": ["Komm'", ",", "ich", "Gl\u00fcck\u00b7li\u00b7cher", ",", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "NN", "$,", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Aber in dunkler Nacht ersteigst du Felsen,", "tokens": ["A\u00b7ber", "in", "dunk\u00b7ler", "Nacht", "er\u00b7steigst", "du", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schwebst in t\u00e4uschender dunkler Nacht auf Wassern!", "tokens": ["Schwebst", "in", "t\u00e4u\u00b7schen\u00b7der", "dunk\u00b7ler", "Nacht", "auf", "Was\u00b7sern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Theilt' ich nur mit dir die Gefahr zu sterben;", "tokens": ["Theilt'", "ich", "nur", "mit", "dir", "die", "Ge\u00b7fahr", "zu", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "W\u00fcrd', ich Gl\u00fcckliche, weinen?", "tokens": ["W\u00fcrd'", ",", "ich", "Gl\u00fcck\u00b7li\u00b7che", ",", "wei\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "NN", "$,", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}