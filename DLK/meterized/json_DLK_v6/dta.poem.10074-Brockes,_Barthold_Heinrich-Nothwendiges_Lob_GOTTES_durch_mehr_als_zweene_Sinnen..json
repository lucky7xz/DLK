{"dta.poem.10074": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendiges Lob GOTTES durch  \n mehr als zweene Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsre Seele hat bisher durch das Ohr, in guten Lehren,", "tokens": ["Uns\u00b7re", "See\u00b7le", "hat", "bis\u00b7her", "durch", "das", "Ohr", ",", "in", "gu\u00b7ten", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Vieles, unserm GOTT zum Ruhm, in der Kirchen", "tokens": ["Vie\u00b7les", ",", "un\u00b7serm", "GoTT", "zum", "Ruhm", ",", "in", "der", "Kir\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Unsre Seele hat auch noch von der Gottheit Wunder-Wesen", "tokens": ["Uns\u00b7re", "See\u00b7le", "hat", "auch", "noch", "von", "der", "Got\u00b7theit", "Wun\u00b7der\u00b7We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "--+----+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Durch das Aug\u2019 in vielen Schriften etwas Gutes wol ge-", "tokens": ["Durch", "das", "Aug'", "in", "vie\u00b7len", "Schrif\u00b7ten", "et\u00b7was", "Gu\u00b7tes", "wol", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PIAT", "NN", "PIAT", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Aber es ist einmahl Zeit, unser\u2019 andre Sinnen auch", "tokens": ["A\u00b7ber", "es", "ist", "ein\u00b7mahl", "Zeit", ",", "un\u00b7ser'", "and\u00b7re", "Sin\u00b7nen", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "$,", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "+--+-+++-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.6": {"text": "Dem allgegenw\u00e4rtgen Sch\u00f6pfer zum gef\u00e4lligen Gebrauch", "tokens": ["Dem", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "Sch\u00f6p\u00b7fer", "zum", "ge\u00b7f\u00e4l\u00b7li\u00b7gen", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Anzuwenden, und der Seelen auch durch sie was zuzuf\u00fchren,", "tokens": ["An\u00b7zu\u00b7wen\u00b7den", ",", "und", "der", "See\u00b7len", "auch", "durch", "sie", "was", "zu\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "ADV", "APPR", "PPER", "PIS", "VVIZU", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Da\u00df sie, unserm GOTT zu Ehren, m\u00f6ge schmecken, f\u00fchlen,", "tokens": ["Da\u00df", "sie", ",", "un\u00b7serm", "GoTT", "zu", "Eh\u00b7ren", ",", "m\u00f6\u00b7ge", "schme\u00b7cken", ",", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "$,", "VMFIN", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Denn da\u00df dieses Sein Verlangen, und Sein Endzweck,", "tokens": ["Denn", "da\u00df", "die\u00b7ses", "Sein", "Ver\u00b7lan\u00b7gen", ",", "und", "Sein", "End\u00b7zweck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDAT", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Durch die Gabe nicht allein, sondern durch Sein Wort.", "tokens": ["Durch", "die", "Ga\u00b7be", "nicht", "al\u00b7lein", ",", "son\u00b7dern", "durch", "Sein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ADV", "$,", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da\u00df wir sehn und f\u00fchlen sollen im Gesch\u00f6pf Sein G\u00f6ttlichs", "tokens": ["Da\u00df", "wir", "sehn", "und", "f\u00fch\u00b7len", "sol\u00b7len", "im", "Ge\u00b7sch\u00f6pf", "Sein", "G\u00f6tt\u00b7lichs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "KON", "VVINF", "VMFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Und beym David: ", "tokens": ["Und", "beym", "Da\u00b7vid", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}