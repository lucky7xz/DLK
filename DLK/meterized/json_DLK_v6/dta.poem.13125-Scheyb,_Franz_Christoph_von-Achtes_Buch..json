{"dta.poem.13125": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich bin stets um den Leib der K\u00f6niginn geschwebt,", "tokens": ["\u201e", "ich", "bin", "stets", "um", "den", "Leib", "der", "K\u00f6\u00b7ni\u00b7ginn", "ge\u00b7schwebt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eund habe selbigen durch meine Kraft belebt;", "tokens": ["\u201e", "und", "ha\u00b7be", "sel\u00b7bi\u00b7gen", "durch", "mei\u00b7ne", "Kraft", "be\u00b7lebt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eich will auch k\u00fcnftighin kein Sorgen unterlassen,", "tokens": ["\u201e", "ich", "will", "auch", "k\u00fcnf\u00b7tig\u00b7hin", "kein", "Sor\u00b7gen", "un\u00b7ter\u00b7las\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201esie stets, wie meine Frucht, in meinen Arm zu fassen.", "tokens": ["\u201e", "sie", "stets", ",", "wie", "mei\u00b7ne", "Frucht", ",", "in", "mei\u00b7nen", "Arm", "zu", "fas\u00b7sen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "$,", "PWAV", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eich habe, was ihr Leib von meinem Schaz enth\u00e4lt,", "tokens": ["\u201e", "ich", "ha\u00b7be", ",", "was", "ihr", "Leib", "von", "mei\u00b7nem", "Schaz", "ent\u00b7h\u00e4lt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "620\u201dMir schon zur Sicherheit und Vorsicht beygesellt,", "tokens": ["\"", "Mir", "schon", "zur", "Si\u00b7cher\u00b7heit", "und", "Vor\u00b7sicht", "bey\u00b7ge\u00b7sellt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "APPRART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edamit wir jederzeit gemeinsam \u00fcberlegen,", "tokens": ["\u201e", "da\u00b7mit", "wir", "je\u00b7der\u00b7zeit", "ge\u00b7mein\u00b7sam", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewie wir in ihre Brust ein langes Leben pr\u00e4gen.", "tokens": ["\u201e", "wie", "wir", "in", "ih\u00b7re", "Brust", "ein", "lan\u00b7ges", "Le\u00b7ben", "pr\u00e4\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}