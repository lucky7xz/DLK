{"dta.poem.7152": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ja wol der M\u00fche werth,", "tokens": ["Es", "ist", "ja", "wol", "der", "M\u00fc\u00b7he", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Geister einst zur\u00fcck zu lencken,", "tokens": ["Die", "Geis\u00b7ter", "einst", "zu\u00b7r\u00fcck", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und voller Inbrunst zu gedencken", "tokens": ["Und", "vol\u00b7ler", "In\u00b7brunst", "zu", "ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf das, was uns der HERR im vor\u2019gen Jahr beschehrt.", "tokens": ["Auf", "das", ",", "was", "uns", "der", "HeRR", "im", "vor'\u00b7gen", "Jahr", "be\u00b7schehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was in dem Geistlichen, durch den geweihten Orden,", "tokens": ["Was", "in", "dem", "Geist\u00b7li\u00b7chen", ",", "durch", "den", "ge\u00b7weih\u00b7ten", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Und sonderlich durch ", "tokens": ["Und", "son\u00b7der\u00b7lich", "durch"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie in dem Leiblichen auch unsre gantze Stadt", "tokens": ["Wie", "in", "dem", "Leib\u00b7li\u00b7chen", "auch", "uns\u00b7re", "gant\u00b7ze", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des H\u00f6chsten Herrschers Schutz so starck versp\u00fcret hat,", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Herr\u00b7schers", "Schutz", "so", "starck", "ver\u00b7sp\u00fc\u00b7ret", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df, ob gleich \u00fcberall und von so manchem Ort", "tokens": ["Da\u00df", ",", "ob", "gleich", "\u00fc\u00b7be\u00b7rall", "und", "von", "so", "man\u00b7chem", "Ort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ADV", "ADV", "KON", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Drangsals-Sturm, ein Ungl\u00fccks-Nord", "tokens": ["Ein", "Drang\u00b7sals\u00b7Sturm", ",", "ein", "Un\u00b7gl\u00fccks\u00b7Nord"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das Wohlfahrts-Schiff ergrimmt genung best\u00fcrmet;", "tokens": ["Das", "Wohl\u00b7fahrts\u00b7Schiff", "er\u00b7grimmt", "ge\u00b7nung", "be\u00b7st\u00fcr\u00b7met", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Es unter GOTTES Hut dennoch beschirmet,", "tokens": ["Es", "un\u00b7ter", "GoT\u00b7TES", "Hut", "den\u00b7noch", "be\u00b7schir\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wohlbehalten blieb.", "tokens": ["Und", "wohl\u00b7be\u00b7hal\u00b7ten", "blieb", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}