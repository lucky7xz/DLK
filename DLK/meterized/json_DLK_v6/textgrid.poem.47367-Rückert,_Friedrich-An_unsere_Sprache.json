{"textgrid.poem.47367": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An unsere Sprache", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reine Jungfrau, ewig sch\u00f6ne,", "tokens": ["Rei\u00b7ne", "Jung\u00b7frau", ",", "e\u00b7wig", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geist'ge Mutter deiner S\u00f6hne,", "tokens": ["Geist'\u00b7ge", "Mut\u00b7ter", "dei\u00b7ner", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4chtige von Zauberbann,", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", "von", "Zau\u00b7ber\u00b7bann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du, in der ich leb' und brenne,", "tokens": ["Du", ",", "in", "der", "ich", "leb'", "und", "bren\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Br\u00fcder kenn' und nenne", "tokens": ["Mei\u00b7ne", "Br\u00fc\u00b7der", "kenn'", "und", "nen\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dich selber preisen kann!", "tokens": ["Und", "dich", "sel\u00b7ber", "prei\u00b7sen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da ich aus dem Schlaf erwachte,", "tokens": ["Da", "ich", "aus", "dem", "Schlaf", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch nicht wu\u00dfte, da\u00df ich dachte,", "tokens": ["Noch", "nicht", "wu\u00df\u00b7te", ",", "da\u00df", "ich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gabest du mich selber mir,", "tokens": ["Ga\u00b7best", "du", "mich", "sel\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00dfest mich die Welt erbeuten,", "tokens": ["Lie\u00b7\u00dfest", "mich", "die", "Welt", "er\u00b7beu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lehrtest mich die R\u00e4tsel deuten", "tokens": ["Lehr\u00b7test", "mich", "die", "R\u00e4t\u00b7sel", "deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mich spielen selbst mit dir.", "tokens": ["Und", "mich", "spie\u00b7len", "selbst", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Spenderin aus reichem Horne,", "tokens": ["Spen\u00b7de\u00b7rin", "aus", "rei\u00b7chem", "Hor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6pferin aus vollem Borne,", "tokens": ["Sch\u00f6p\u00b7fe\u00b7rin", "aus", "vol\u00b7lem", "Bor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohnerin im Sternenzelt!", "tokens": ["Woh\u00b7ne\u00b7rin", "im", "Ster\u00b7nen\u00b7zelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle H\u00f6hn hast du erfl\u00fcgelt,", "tokens": ["Al\u00b7le", "H\u00f6hn", "hast", "du", "er\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Tiefen du entsiegelt", "tokens": ["Al\u00b7le", "Tie\u00b7fen", "du", "ent\u00b7sie\u00b7gelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und durchwandelt alle Welt.", "tokens": ["Und", "durch\u00b7wan\u00b7delt", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch der Eichenw\u00e4lder Bogen", "tokens": ["Durch", "der", "Ei\u00b7chen\u00b7w\u00e4l\u00b7der", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du brausend hingezogen,", "tokens": ["Bist", "du", "brau\u00b7send", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der letzte Wipfel barst;", "tokens": ["Bis", "der", "letz\u00b7te", "Wip\u00b7fel", "barst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der F\u00fcrstenschl\u00f6sser Prangen", "tokens": ["Durch", "der", "F\u00fcrs\u00b7ten\u00b7schl\u00f6s\u00b7ser", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du klingend hergegangen,", "tokens": ["Bist", "du", "klin\u00b7gend", "her\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und noch bist du, die du warst.", "tokens": ["Und", "noch", "bist", "du", ",", "die", "du", "warst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "St\u00fcrme, rausche, lispl' und s\u00e4usle!", "tokens": ["St\u00fcr\u00b7me", ",", "rau\u00b7sche", ",", "lispl'", "und", "s\u00e4us\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zimmre, gl\u00e4tte, hau' und mei\u00dfle,", "tokens": ["Zimm\u00b7re", ",", "gl\u00e4t\u00b7te", ",", "hau'", "und", "mei\u00df\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffe fort mit Sch\u00f6pfergeist!", "tokens": ["Schaf\u00b7fe", "fort", "mit", "Sch\u00f6p\u00b7fer\u00b7geist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir l\u00e4\u00dft gern der Stoff sich zwingen,", "tokens": ["Dir", "l\u00e4\u00dft", "gern", "der", "Stoff", "sich", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und dir mu\u00df der Bau gelingen,", "tokens": ["Und", "dir", "mu\u00df", "der", "Bau", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Den kein Zeitstrom niederrei\u00dft.", "tokens": ["Den", "kein", "Zeits\u00b7trom", "nie\u00b7der\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mach' uns stark an Geistesh\u00e4nden,", "tokens": ["Mach'", "uns", "stark", "an", "Geis\u00b7tes\u00b7h\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wir sie zum Rechten wenden,", "tokens": ["Da\u00df", "wir", "sie", "zum", "Rech\u00b7ten", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einzugreifen in die Reih'n.", "tokens": ["Ein\u00b7zu\u00b7grei\u00b7fen", "in", "die", "Reih'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Viel Gesellen sind gesetzet,", "tokens": ["Viel", "Ge\u00b7sel\u00b7len", "sind", "ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner wird gering gesch\u00e4tzet,", "tokens": ["Kei\u00b7ner", "wird", "ge\u00b7ring", "ge\u00b7sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wer kann, soll Meister sein.", "tokens": ["Und", "wer", "kann", ",", "soll", "Meis\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "$,", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}