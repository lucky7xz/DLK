{"textgrid.poem.63354": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Leiferde", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir leben ganz im Dunkeln,", "tokens": ["Wir", "le\u00b7ben", "ganz", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns bl\u00fchen nicht Ranunkeln", "tokens": ["Uns", "bl\u00fc\u00b7hen", "nicht", "Ra\u00b7nun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und M\u00e4dchen gl\u00fchn uns nicht.", "tokens": ["Und", "M\u00e4d\u00b7chen", "gl\u00fchn", "uns", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir sind von Gott verworfen", "tokens": ["Wir", "sind", "von", "Gott", "ver\u00b7wor\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und unter Schmutz und Schorfen", "tokens": ["Und", "un\u00b7ter", "Schmutz", "und", "Schor\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist unsre Brust mit Schwefel ausgepicht.", "tokens": ["Ist", "uns\u00b7re", "Brust", "mit", "Schwe\u00b7fel", "aus\u00b7ge\u00b7picht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Rucksack, der ist leer,", "tokens": ["Der", "Ruck\u00b7sack", ",", "der", "ist", "leer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Hirn von Pl\u00e4nen schwer,", "tokens": ["Das", "Hirn", "von", "Pl\u00e4\u00b7nen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit uns will's niemand wagen.", "tokens": ["Mit", "uns", "will's", "nie\u00b7mand", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir finden Stell' und Arbeit nicht,", "tokens": ["Wir", "fin\u00b7den", "Stell'", "und", "Ar\u00b7beit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hunger wie mit Messern sticht", "tokens": ["Der", "Hun\u00b7ger", "wie", "mit", "Mes\u00b7sern", "sticht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Magen.", "tokens": ["Den", "Ma\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Wir sind dahingezogen", "tokens": ["Wir", "sind", "da\u00b7hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Not und Kot und Dreck.", "tokens": ["Durch", "Not", "und", "Kot", "und", "Dreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wind hat uns verbogen,", "tokens": ["Der", "Wind", "hat", "uns", "ver\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Leben uns belogen,", "tokens": ["Das", "Le\u00b7ben", "uns", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Menschheit warf uns weg.", "tokens": ["Die", "Menschheit", "warf", "uns", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wir wateten im Schlamm,", "tokens": ["Wir", "wa\u00b7te\u00b7ten", "im", "Schlamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir kamen an den Damm,", "tokens": ["Wir", "ka\u00b7men", "an", "den", "Damm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Zug flog hell vor\u00fcber,", "tokens": ["Ein", "Zug", "flog", "hell", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach, niemand rief: Hol \u00fcber!", "tokens": ["Ach", ",", "nie\u00b7mand", "rief", ":", "Hol", "\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "$.", "NN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hol \u00fcber!", "tokens": ["Hol", "\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "APPR", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Es tranken Kavaliere", "tokens": ["Es", "tran\u00b7ken", "Ka\u00b7va\u00b7lie\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Speisewagen Mumm.", "tokens": ["Im", "Spei\u00b7se\u00b7wa\u00b7gen", "Mumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind nicht einmal Tiere,", "tokens": ["Wir", "sind", "nicht", "ein\u00b7mal", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns wandern Herz und Niere", "tokens": ["Uns", "wan\u00b7dern", "Herz", "und", "Nie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ziellos im Leib herum.", "tokens": ["Ziel\u00b7los", "im", "Leib", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den Klotz nun auf die Schienen,", "tokens": ["Den", "Klotz", "nun", "auf", "die", "Schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Qualen ists genug,", "tokens": ["Der", "Qua\u00b7len", "ists", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald kommt der n\u00e4chste Zug,", "tokens": ["Bald", "kommt", "der", "n\u00e4chs\u00b7te", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir wollen was verdienen", "tokens": ["Wir", "wol\u00b7len", "was", "ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u2013 Und sei's auch nur das Hochgericht.", "tokens": ["\u2013", "Und", "sei's", "auch", "nur", "das", "Hoch\u00b7ge\u00b7richt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn wir im \u00c4ther baumeln", "tokens": ["Wenn", "wir", "im", "\u00c4\u00b7ther", "bau\u00b7meln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und zu den Sternen taumeln,", "tokens": ["Und", "zu", "den", "Ster\u00b7nen", "tau\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Sehn wir zum erstenmal das Licht \u2013", "tokens": ["Sehn", "wir", "zum", "ers\u00b7ten\u00b7mal", "das", "Licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Licht.", "tokens": ["Das", "Licht", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}