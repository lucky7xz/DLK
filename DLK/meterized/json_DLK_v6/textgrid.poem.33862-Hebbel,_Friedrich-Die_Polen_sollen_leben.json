{"textgrid.poem.33862": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Polen sollen leben", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Hamburg in dem Saale,", "tokens": ["Zu", "Ham\u00b7burg", "in", "dem", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll Lichterglanz und Pracht,", "tokens": ["Voll", "Lich\u00b7ter\u00b7glanz", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sitzt mancher Gast beim Mahle", "tokens": ["Sitzt", "man\u00b7cher", "Gast", "beim", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In heil'ger Neujahrsnacht;", "tokens": ["In", "heil'\u00b7ger", "Neu\u00b7jahrs\u00b7nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Fremden sind's, sie w\u00e4ren gern", "tokens": ["Die", "Frem\u00b7den", "sin\u00b7d's", ",", "sie", "w\u00e4\u00b7ren", "gern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Im Vaterland, doch das ist fern,", "tokens": ["Im", "Va\u00b7ter\u00b7land", ",", "doch", "das", "ist", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun wird denn sein gedacht.", "tokens": ["Nun", "wird", "denn", "sein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erst haben sich die G\u00e4ste", "tokens": ["Erst", "ha\u00b7ben", "sich", "die", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kalt in's Gesicht geschaut,", "tokens": ["Kalt", "in's", "Ge\u00b7sicht", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch werden sie bei'm Feste", "tokens": ["Doch", "wer\u00b7den", "sie", "bei'm", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald froh und wohlvertraut.", "tokens": ["Bald", "froh", "und", "wohl\u00b7ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur Einer, welchen Niemand kennt,", "tokens": ["Nur", "Ei\u00b7ner", ",", "wel\u00b7chen", "Nie\u00b7mand", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PWAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Blickt stumm in's Licht, wie's niederbrennt,", "tokens": ["Blickt", "stumm", "in's", "Licht", ",", "wie's", "nie\u00b7der\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jung, aber schon ergraut.", "tokens": ["Jung", ",", "a\u00b7ber", "schon", "er\u00b7graut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihm d\u00fcnken sie Gespenster", "tokens": ["Ihm", "d\u00fcn\u00b7ken", "sie", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihrer Lust zu sein;", "tokens": ["In", "ih\u00b7rer", "Lust", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kehrt sich ab; in's Fenster", "tokens": ["Er", "kehrt", "sich", "ab", ";", "in's", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$.", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirft hell der Mond den Schein.", "tokens": ["Wirft", "hell", "der", "Mond", "den", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er spricht: du \u00fcberschaust die Welt,", "tokens": ["Er", "spricht", ":", "du", "\u00fc\u00b7bersc\u00b7haust", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sag', ob Polen steht, ob f\u00e4llt! \u2013", "tokens": ["So", "sag'", ",", "ob", "Po\u00b7len", "steht", ",", "ob", "f\u00e4llt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "NE", "VVFIN", "$,", "KOUS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Wolke h\u00fcllt ihn ein.", "tokens": ["Die", "Wol\u00b7ke", "h\u00fcllt", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sein Herz will zornig wallen,", "tokens": ["Sein", "Herz", "will", "zor\u00b7nig", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schw\u00f6rt er still sich zu:", "tokens": ["Da", "schw\u00f6rt", "er", "still", "sich", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VMFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Magst steh'n, mein Volk, magst fallen,", "tokens": ["Magst", "steh'n", ",", "mein", "Volk", ",", "magst", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich steh' und fall', wie du!", "tokens": ["Ich", "steh'", "und", "fall'", ",", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gewi\u00df der Erste, w\u00e4r' ich dort,", "tokens": ["Ge\u00b7wi\u00df", "der", "Ers\u00b7te", ",", "w\u00e4r'", "ich", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Letzte hier am fremden Ort,", "tokens": ["Der", "Letz\u00b7te", "hier", "am", "frem\u00b7den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Dolch bringt mich zur Ruh.", "tokens": ["Mein", "Dolch", "bringt", "mich", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Glockenthurm thut eben", "tokens": ["Der", "Glo\u00b7cken\u00b7thurm", "thut", "e\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die zw\u00f6lfte Stunde kund,", "tokens": ["Die", "zw\u00f6lf\u00b7te", "Stun\u00b7de", "kund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Polen sollen leben!", "tokens": ["Die", "Po\u00b7len", "sol\u00b7len", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ruft er mit lautem Mund.", "tokens": ["Ruft", "er", "mit", "lau\u00b7tem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Jeder greift, wie er, zum Glas,", "tokens": ["Ein", "Je\u00b7der", "greift", ",", "wie", "er", ",", "zum", "Glas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie All' ergl\u00fch'n, doch er sinkt bla\u00df", "tokens": ["Sie", "All'", "er\u00b7gl\u00fch'n", ",", "doch", "er", "sinkt", "bla\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVINF", "$,", "KON", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur\u00fcck, ist todt zur Stund'.", "tokens": ["Zu\u00b7r\u00fcck", ",", "ist", "todt", "zur", "Stund'", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie gie\u00dfen, statt zu trinken,", "tokens": ["Sie", "gie\u00b7\u00dfen", ",", "statt", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Wein jetzt in den Sand;", "tokens": ["Den", "Wein", "jetzt", "in", "den", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sah'n das Schicksal winken", "tokens": ["Sie", "sah'n", "das", "Schick\u00b7sal", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und haben's wohl erkannt,", "tokens": ["Und", "ha\u00b7ben's", "wohl", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df Polen bald dem Todten gleicht,", "tokens": ["Da\u00df", "Po\u00b7len", "bald", "dem", "Tod\u00b7ten", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Keiner ahnt, wie bald vielleicht", "tokens": ["Doch", "Kei\u00b7ner", "ahnt", ",", "wie", "bald", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWAV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Welt dem Polenland.", "tokens": ["Die", "Welt", "dem", "Po\u00b7len\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}