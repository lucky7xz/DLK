{"dta.poem.675": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mich (dem die grosse Welt vorhin zu enge war/) ", "tokens": ["Mich", "(", "dem", "die", "gros\u00b7se", "Welt", "vor\u00b7hin", "zu", "en\u00b7ge", "war", "/", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$(", "ART", "ART", "ADJA", "NN", "ADV", "APPR", "ADJA", "VAFIN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt dieses enge grab verrigelt", "tokens": ["H\u00e4lt", "die\u00b7ses", "en\u00b7ge", "grab", "ver\u00b7ri\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin in dieser klufft versiegelt/", "tokens": ["Ich", "bin", "in", "die\u00b7ser", "klufft", "ver\u00b7sie\u00b7gelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd fest verstrickt in angst/ in wehmut vnd gefahr.", "tokens": ["Vnd", "fest", "ver\u00b7strickt", "in", "angst", "/", "in", "weh\u00b7mut", "vnd", "ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "NE", "$(", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo ist die g\u00fcldne zeit gebliben/", "tokens": ["Wo", "ist", "die", "g\u00fcld\u00b7ne", "zeit", "ge\u00b7bli\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da ich mit freyem munde sang? ", "tokens": ["Da", "ich", "mit", "frey\u00b7em", "mun\u00b7de", "sang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da meiner s\u00fcssen seiten klang/", "tokens": ["Da", "mei\u00b7ner", "s\u00fcs\u00b7sen", "sei\u00b7ten", "klang", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich in des h\u00f6chsten lob must \u00fcben.", "tokens": ["Sich", "in", "des", "h\u00f6chs\u00b7ten", "lob", "must", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ach wie hat alles sich verkehret/", "tokens": ["Ach", "wie", "hat", "al\u00b7les", "sich", "ver\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VAFIN", "PIS", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie hefftig hat mich GOTT beschweret.", "tokens": ["Wie", "heff\u00b7tig", "hat", "mich", "GoTT", "be\u00b7schwe\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}