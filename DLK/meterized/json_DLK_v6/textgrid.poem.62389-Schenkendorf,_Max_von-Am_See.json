{"textgrid.poem.62389": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am See", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn ich hier am Wasser steh',", "tokens": ["Und", "wenn", "ich", "hier", "am", "Was\u00b7ser", "steh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem klaren Spiegel seh'", "tokens": ["In", "die\u00b7sem", "kla\u00b7ren", "Spie\u00b7gel", "seh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Himmel und die B\u00e4ume,", "tokens": ["Den", "Him\u00b7mel", "und", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So zieht mich's wol hinab, hinab,", "tokens": ["So", "zieht", "mich's", "wol", "hin\u00b7ab", ",", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gern s\u00e4nken in das feuchte Grab", "tokens": ["Gern", "s\u00e4n\u00b7ken", "in", "das", "feuch\u00b7te", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sehnsucht und die Tr\u00e4ume.", "tokens": ["Die", "Sehn\u00b7sucht", "und", "die", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch ist es nur ein eitler Wahn,", "tokens": ["Doch", "ist", "es", "nur", "ein", "eit\u00b7ler", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein eigen Bildni\u00df schaust du an.", "tokens": ["Dein", "ei\u00b7gen", "Bild\u00b7ni\u00df", "schaust", "du", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all das Sterngefunkel,", "tokens": ["Und", "all", "das", "Stern\u00b7ge\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mag's locken dich zu Lust und Ku\u00df,", "tokens": ["Mag's", "lo\u00b7cken", "dich", "zu", "Lust", "und", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steig' nicht hinab zum kalten Flu\u00df,", "tokens": ["Steig'", "nicht", "hin\u00b7ab", "zum", "kal\u00b7ten", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn unten ist es dunkel.", "tokens": ["Denn", "un\u00b7ten", "ist", "es", "dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wenn ich vor der Liebsten steh'", "tokens": ["Doch", "wenn", "ich", "vor", "der", "Liebs\u00b7ten", "steh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr in die klaren Augen seh',", "tokens": ["Ihr", "in", "die", "kla\u00b7ren", "Au\u00b7gen", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist kein Traum, kein W\u00e4hnen.", "tokens": ["Das", "ist", "kein", "Traum", ",", "kein", "W\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du mildes, frommes Angesicht,", "tokens": ["Du", "mil\u00b7des", ",", "from\u00b7mes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Himmelsblick, du reines Licht,", "tokens": ["Du", "Him\u00b7mels\u00b7blick", ",", "du", "rei\u00b7nes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du t\u00e4uschest nicht mein Sehnen.", "tokens": ["Du", "t\u00e4u\u00b7schest", "nicht", "mein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist nicht mehr mein armes Ich,", "tokens": ["Es", "ist", "nicht", "mehr", "mein", "ar\u00b7mes", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das eitel in dem Spiegel sich,", "tokens": ["Das", "ei\u00b7tel", "in", "dem", "Spie\u00b7gel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur ewig sich beschauet:", "tokens": ["Nur", "e\u00b7wig", "sich", "be\u00b7schau\u00b7et", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein zweites Leben, das mir bl\u00fcht,", "tokens": ["Ein", "zwei\u00b7tes", "Le\u00b7ben", ",", "das", "mir", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein be\u00dfres, dran sich mein Gem\u00fcth", "tokens": ["Ein", "be\u00df\u00b7res", ",", "dran", "sich", "mein", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PAV", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Ewigkeit erbauet.", "tokens": ["In", "E\u00b7wig\u00b7keit", "er\u00b7bau\u00b7et", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O s\u00fc\u00dfer Bund von Ich und Du,", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Bund", "von", "Ich", "und", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun flie\u00dfe hin in Lust und Ruh", "tokens": ["Nun", "flie\u00b7\u00dfe", "hin", "in", "Lust", "und", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein liebes, sch\u00f6nes Leben!", "tokens": ["Mein", "lie\u00b7bes", ",", "sch\u00f6\u00b7nes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O starker Bund von Eins und Zwei,", "tokens": ["O", "star\u00b7ker", "Bund", "von", "Eins", "und", "Zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "KON", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Daraus wird sich der heil'gen Drei", "tokens": ["Da\u00b7raus", "wird", "sich", "der", "heil'\u00b7gen", "Drei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Vollkommne Zahl erheben.", "tokens": ["Voll\u00b7komm\u00b7ne", "Zahl", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}