{"textgrid.poem.45773": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Wehrgesetz", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwir haben nun achtmalhunderttausend Mann,", "tokens": ["\u00bb", "wir", "ha\u00b7ben", "nun", "acht\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das w\u00e4re genug, sollt ich meinen!\u00ab", "tokens": ["Das", "w\u00e4\u00b7re", "ge\u00b7nug", ",", "sollt", "ich", "mei\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "VMFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich w\u00fcnschte, k\u00e4m auf mich es an,", "tokens": ["Ich", "w\u00fcnschte", ",", "k\u00e4m", "auf", "mich", "es", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Achtmalhunderttausend \u2013 und einen.", "tokens": ["Acht\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "\u2013", "und", "ei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["CARD", "$(", "KON", "ART", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}