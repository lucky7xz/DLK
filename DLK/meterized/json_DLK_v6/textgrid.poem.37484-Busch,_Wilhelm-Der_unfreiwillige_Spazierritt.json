{"textgrid.poem.37484": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der unfreiwillige Spazierritt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Pumps labt sich im k\u00fchlen Bad,", "tokens": ["Herr", "Pumps", "labt", "sich", "im", "k\u00fch\u00b7len", "Bad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PRF", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als hoch zu Ro\u00df ein Leutnant naht.", "tokens": ["Als", "hoch", "zu", "Ro\u00df", "ein", "Leut\u00b7nant", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Leutnants Ro\u00df wird pl\u00f6tzlich scheu,", "tokens": ["Des", "Leut\u00b7nants", "Ro\u00df", "wird", "pl\u00f6tz\u00b7lich", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Pumps eilt gleich zur Hilf' herbei;", "tokens": ["Herr", "Pumps", "eilt", "gleich", "zur", "Hil\u00b7f'", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ergreift den Zaum mit starker Hand", "tokens": ["Er\u00b7greift", "den", "Zaum", "mit", "star\u00b7ker", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwingt sich dann aufs Ro\u00df gewandt.", "tokens": ["Und", "schwingt", "sich", "dann", "aufs", "Ro\u00df", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kaum sitzt er \u2013 zu des Leutnants Schreck \u2013", "tokens": ["Kaum", "sitzt", "er", "\u2013", "zu", "des", "Leut\u00b7nants", "Schreck", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da rennt das Ro\u00df mit ihm vom Fleck;", "tokens": ["Da", "rennt", "das", "Ro\u00df", "mit", "ihm", "vom", "Fleck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und rennt und st\u00fcrmt vorbei mit Wut", "tokens": ["Und", "rennt", "und", "st\u00fcrmt", "vor\u00b7bei", "mit", "Wut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einem Fr\u00e4ulein-Institut.", "tokens": ["An", "ei\u00b7nem", "Fr\u00e4u\u00b7lein\u00b7In\u00b7sti\u00b7tut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und rennt hinein zur Stadt \u2013 o Graus \u2013", "tokens": ["Und", "rennt", "hin\u00b7ein", "zur", "Stadt", "\u2013", "o", "Graus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$(", "FM", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo eben ward die Schule aus;", "tokens": ["Wo", "e\u00b7ben", "ward", "die", "Schu\u00b7le", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und rennt, bis es gerannt genug,", "tokens": ["Und", "rennt", ",", "bis", "es", "ge\u00b7rannt", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirft Herrn Pumps noch ab im Flug.", "tokens": ["Und", "wirft", "Herrn", "Pumps", "noch", "ab", "im", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "ADV", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Jugend sieht's und lacht ihn aus;", "tokens": ["Die", "Ju\u00b7gend", "sieht's", "und", "lacht", "ihn", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnell eilt Herr Pumps ins n\u00e4chste Haus.", "tokens": ["Schnell", "eilt", "Herr", "Pumps", "ins", "n\u00e4chs\u00b7te", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und fa\u00dft die n\u00e4chste T\u00fcr' \u2013 o weh \u2013", "tokens": ["Und", "fa\u00dft", "die", "n\u00e4chs\u00b7te", "T\u00fcr'", "\u2013", "o", "weh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "FM", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Fr\u00e4ulein sitzen beim Kaffee;", "tokens": ["Zwei", "Fr\u00e4u\u00b7lein", "sit\u00b7zen", "beim", "Kaf\u00b7fee", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Doch wie er schnell den R\u00fcckzug sucht,", "tokens": ["Doch", "wie", "er", "schnell", "den", "R\u00fcck\u00b7zug", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hemmt 's Stubenm\u00e4del seine Flucht.", "tokens": ["Hemmt", "'s", "Stu\u00b7ben\u00b7m\u00e4\u00b7del", "sei\u00b7ne", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Endlich erreicht er ein Gemach,", "tokens": ["End\u00b7lich", "er\u00b7reicht", "er", "ein", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein Diener bringt ihm Kleider nach,", "tokens": ["Ein", "Die\u00b7ner", "bringt", "ihm", "Klei\u00b7der", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Schafft eine Droschke dann zur Stell',", "tokens": ["Schafft", "ei\u00b7ne", "Droschke", "dann", "zur", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Drin f\u00e4hrt Herr Pumps nach Hause schnell.", "tokens": ["Drin", "f\u00e4hrt", "Herr", "Pumps", "nach", "Hau\u00b7se", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}