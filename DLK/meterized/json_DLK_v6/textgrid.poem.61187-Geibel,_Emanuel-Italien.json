{"textgrid.poem.61187": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Italien", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie eigen wird dem Wandrer, der, entflohn des Nordens Haft,", "tokens": ["O", "wie", "ei\u00b7gen", "wird", "dem", "Wand\u00b7rer", ",", "der", ",", "ent\u00b7flohn", "des", "Nor\u00b7dens", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nach dem hei\u00dfersehnten S\u00fcden lenkt die frohe Pilgerschaft,", "tokens": ["Nach", "dem", "hei\u00b7\u00dfer\u00b7sehn\u00b7ten", "S\u00fc\u00b7den", "lenkt", "die", "fro\u00b7he", "Pil\u00b7ger\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn er von des Gotthards Gipfel, der in ew'gem Eise schweigt,", "tokens": ["Wenn", "er", "von", "des", "Got\u00b7thards", "Gip\u00b7fel", ",", "der", "in", "ew'\u00b7gem", "Ei\u00b7se", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Langsam durch die Morgend\u00e4mmrung gen Italien niedersteigt.", "tokens": ["Lang\u00b7sam", "durch", "die", "Mor\u00b7gen\u00b7d\u00e4mm\u00b7rung", "gen", "I\u00b7ta\u00b7li\u00b7en", "nie\u00b7ders\u00b7teigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "+-+-+-+---+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Leise teilen sich die Nebel, und es wird so lau die Luft,", "tokens": ["Lei\u00b7se", "tei\u00b7len", "sich", "die", "Ne\u00b7bel", ",", "und", "es", "wird", "so", "lau", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aus der Tiefe wie ein Gr\u00fc\u00dfen weht empor verlorner Duft;", "tokens": ["Aus", "der", "Tie\u00b7fe", "wie", "ein", "Gr\u00fc\u00b7\u00dfen", "weht", "em\u00b7por", "ver\u00b7lor\u00b7ner", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "PTKVZ", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Noch ein Vorsprung! - sieh, und unten weit und bl\u00fchend lacht das Tal,", "tokens": ["Noch", "ein", "Vor\u00b7sprung", "!", "sieh", ",", "und", "un\u00b7ten", "weit", "und", "bl\u00fc\u00b7hend", "lacht", "das", "Tal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "$(", "VVIMP", "$,", "KON", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dichte G\u00e4rten, Silberseen, \u00fcbergl\u00e4nzt vom Morgenstrahl.", "tokens": ["Dich\u00b7te", "G\u00e4r\u00b7ten", ",", "Sil\u00b7ber\u00b7seen", ",", "\u00fc\u00b7ber\u00b7gl\u00e4nzt", "vom", "Mor\u00b7gen\u00b7strahl", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus den H\u00fcgeln quellen Rosen, um die Ulmen rankt der Wein,", "tokens": ["Aus", "den", "H\u00fc\u00b7geln", "quel\u00b7len", "Ro\u00b7sen", ",", "um", "die", "Ul\u00b7men", "rankt", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "KOUI", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schlanke Marmors\u00e4ulen schimmern winkend im Zypressenhain,", "tokens": ["Schlan\u00b7ke", "Mar\u00b7mor\u00b7s\u00e4u\u00b7len", "schim\u00b7mern", "win\u00b7kend", "im", "Zyp\u00b7res\u00b7sen\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Dort die Berge lorbeerwaldig, hier das blaukristallne Meer,", "tokens": ["Dort", "die", "Ber\u00b7ge", "lor\u00b7beer\u00b7wal\u00b7dig", ",", "hier", "das", "blau\u00b7kris\u00b7tall\u00b7ne", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und der Himmel wie ein liebend Mutterauge dr\u00fcber her.", "tokens": ["Und", "der", "Him\u00b7mel", "wie", "ein", "lie\u00b7bend", "Mut\u00b7ter\u00b7au\u00b7ge", "dr\u00fc\u00b7ber", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOKOM", "ART", "ADJD", "NN", "PAV", "PTKVZ", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Und dazwischen buntgekleidet buntes Volk in Tal und H\u00f6hn,", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "bunt\u00b7ge\u00b7klei\u00b7det", "bun\u00b7tes", "Volk", "in", "Tal", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Braune Buben, stolze Frauen, wie des Landes Rosen sch\u00f6n,", "tokens": ["Brau\u00b7ne", "Bu\u00b7ben", ",", "stol\u00b7ze", "Frau\u00b7en", ",", "wie", "des", "Lan\u00b7des", "Ro\u00b7sen", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Winzertanz auf allen Bergen, in den H\u00e4usern Zitherschall,", "tokens": ["Win\u00b7zer\u00b7tanz", "auf", "al\u00b7len", "Ber\u00b7gen", ",", "in", "den", "H\u00e4u\u00b7sern", "Zi\u00b7ther\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Lust'ge Lieder in den Barken, Klang und Jubel \u00fcberall.", "tokens": ["Lust'\u00b7ge", "Lie\u00b7der", "in", "den", "Bar\u00b7ken", ",", "Klang", "und", "Ju\u00b7bel", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Wahrlich, solltest du nicht meinen, ausgest\u00fcrzt auf dieses Land", "tokens": ["Wahr\u00b7lich", ",", "soll\u00b7test", "du", "nicht", "mei\u00b7nen", ",", "aus\u00b7ge\u00b7st\u00fcrzt", "auf", "die\u00b7ses", "Land"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seiner Freuden vollsten Becher hab' ein Gott mit trunkner Hand?", "tokens": ["Sei\u00b7ner", "Freu\u00b7den", "volls\u00b7ten", "Be\u00b7cher", "hab'", "ein", "Gott", "mit", "trunk\u00b7ner", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "An dem L\u00e4nderbaum Europens sei's der bl\u00fctenvollste Zweig,", "tokens": ["An", "dem", "L\u00e4n\u00b7der\u00b7baum", "Eu\u00b7ro\u00b7pens", "sei's", "der", "bl\u00fc\u00b7ten\u00b7volls\u00b7te", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie an gr\u00fcnen Laubgewinden, so an goldnen Fr\u00fcchten reich?", "tokens": ["Wie", "an", "gr\u00fc\u00b7nen", "Laub\u00b7ge\u00b7win\u00b7den", ",", "so", "an", "gold\u00b7nen", "Fr\u00fcch\u00b7ten", "reich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Aber ach, der bittern T\u00e4uschung! Unter diesem farb'gen Scherz,", "tokens": ["A\u00b7ber", "ach", ",", "der", "bit\u00b7tern", "T\u00e4u\u00b7schung", "!", "Un\u00b7ter", "die\u00b7sem", "fa\u00b7rb'\u00b7gen", "Scherz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "ADJA", "NN", "$.", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Wie die Natter unter Blumen, lauscht ein tief verborgner Schmerz,", "tokens": ["Wie", "die", "Nat\u00b7ter", "un\u00b7ter", "Blu\u00b7men", ",", "lauscht", "ein", "tief", "ver\u00b7borg\u00b7ner", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jener Schmerz, der nimmer rastet, da\u00df die alte Tugend starb,", "tokens": ["Je\u00b7ner", "Schmerz", ",", "der", "nim\u00b7mer", "ras\u00b7tet", ",", "da\u00df", "die", "al\u00b7te", "Tu\u00b7gend", "starb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df die Freiheit ging verloren, und ein Heldenvolk verdarb.", "tokens": ["Da\u00df", "die", "Frei\u00b7heit", "ging", "ver\u00b7lo\u00b7ren", ",", "und", "ein", "Hel\u00b7den\u00b7volk", "ver\u00b7darb", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VVPP", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "O Italien, du der K\u00fcnste Mutter, stolzes, sch\u00f6nes Weib,", "tokens": ["O", "I\u00b7ta\u00b7li\u00b7en", ",", "du", "der", "K\u00fcns\u00b7te", "Mut\u00b7ter", ",", "stol\u00b7zes", ",", "sch\u00f6\u00b7nes", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "ART", "ADJA", "NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Tr\u00e4grin einst der h\u00f6chsten Kronen, siech und elend ward dein Leib,", "tokens": ["Tr\u00e4g\u00b7rin", "einst", "der", "h\u00f6chs\u00b7ten", "Kro\u00b7nen", ",", "siech", "und", "e\u00b7lend", "ward", "dein", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,", "VVIMP", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Dieser holde Rosenschimmer, der so reizend dich umbl\u00fcht,", "tokens": ["Die\u00b7ser", "hol\u00b7de", "Ro\u00b7sen\u00b7schim\u00b7mer", ",", "der", "so", "rei\u00b7zend", "dich", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ach, es ist des Fiebers Hitze, das in deinen Adern gl\u00fcht.", "tokens": ["Ach", ",", "es", "ist", "des", "Fie\u00b7bers", "Hit\u00b7ze", ",", "das", "in", "dei\u00b7nen", "A\u00b7dern", "gl\u00fcht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Ja, es will mich oft gemahnen, aller deiner Blumen Glanz", "tokens": ["Ja", ",", "es", "will", "mich", "oft", "ge\u00b7mah\u00b7nen", ",", "al\u00b7ler", "dei\u00b7ner", "Blu\u00b7men", "Glanz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,", "PIAT", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Lieg' um deine kranken Schl\u00e4fe fertig schon als Totenkranz,", "tokens": ["Lieg'", "um", "dei\u00b7ne", "kran\u00b7ken", "Schl\u00e4\u00b7fe", "fer\u00b7tig", "schon", "als", "To\u00b7ten\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "ADV", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ja, als sei'n Vesuv und \u00c4tna lodernd nur dahingestellt", "tokens": ["Ja", ",", "als", "sei'n", "Ve\u00b7suv", "und", "\u00c4t\u00b7na", "lo\u00b7dernd", "nur", "da\u00b7hin\u00b7ge\u00b7stellt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPOSAT", "NN", "KON", "NE", "ADJD", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Fackeln an dem Sterbelager einer K\u00f6nigin der Welt. -", "tokens": ["Fa\u00b7ckeln", "an", "dem", "Ster\u00b7be\u00b7la\u00b7ger", "ei\u00b7ner", "K\u00f6\u00b7ni\u00b7gin", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Aber nein! Noch lebt die Hoffnung, ob auch tief versteckt im Weh;", "tokens": ["A\u00b7ber", "nein", "!", "Noch", "lebt", "die", "Hoff\u00b7nung", ",", "ob", "auch", "tief", "ver\u00b7steckt", "im", "Weh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "ADV", "ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Kennst du nicht das Lied vom herben Kummer der Penelope?", "tokens": ["Kennst", "du", "nicht", "das", "Lied", "vom", "her\u00b7ben", "Kum\u00b7mer", "der", "Pe\u00b7ne\u00b7lo\u00b7pe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sch\u00f6n wie du vor allen andern ward wie du sie viel umfreit,", "tokens": ["Sch\u00f6n", "wie", "du", "vor", "al\u00b7len", "an\u00b7dern", "ward", "wie", "du", "sie", "viel", "um\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "APPR", "PIAT", "PIS", "VAFIN", "KOKOM", "PPER", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und der Fremden Schwarm verpra\u00dfte frech des Hauses Herrlichkeit.", "tokens": ["Und", "der", "Frem\u00b7den", "Schwarm", "ver\u00b7pra\u00df\u00b7te", "frech", "des", "Hau\u00b7ses", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word",