{"dta.poem.6019": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie eines K\u00f6nigs Macht und Krafft ins gantze Reich", "tokens": ["Wie", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Macht", "und", "Krafft", "ins", "gant\u00b7ze", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich aus der Residenz, als dessen Hertz, verbreiten;", "tokens": ["Sich", "aus", "der", "Re\u00b7si\u00b7denz", ",", "als", "des\u00b7sen", "Hertz", ",", "ver\u00b7brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "KOUS", "PRELAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00fcrckt dies herrliche Gestirn von allen Seiten.", "tokens": ["So", "w\u00fcrckt", "dies", "herr\u00b7li\u00b7che", "Ge\u00b7stirn", "von", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es schickt den holden Blick, recht eingetheilt, und gleich", "tokens": ["Es", "schickt", "den", "hol\u00b7den", "Blick", ",", "recht", "ein\u00b7ge\u00b7theilt", ",", "und", "gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf alles, was sich regt, und was man wachsen sieht,", "tokens": ["Auf", "al\u00b7les", ",", "was", "sich", "regt", ",", "und", "was", "man", "wach\u00b7sen", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PRF", "VVFIN", "$,", "KON", "PWS", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf alles, welches Athem zieht.", "tokens": ["Auf", "al\u00b7les", ",", "wel\u00b7ches", "A\u00b7them", "zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}