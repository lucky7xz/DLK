{"dta.poem.11702": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An  \u2014", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df doch dein Dichten! hast ja Geld;", "tokens": ["La\u00df", "doch", "dein", "Dich\u00b7ten", "!", "hast", "ja", "Geld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "$.", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tropf! brauch's, die Poesie lebendig zu betreiben!", "tokens": ["Tropf", "!", "brauch's", ",", "die", "Poe\u00b7sie", "le\u00b7ben\u00b7dig", "zu", "be\u00b7trei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was gilt's? dich freut das Sch\u00f6nste in der Welt", "tokens": ["Was", "gilt's", "?", "dich", "freut", "das", "Sch\u00f6ns\u00b7te", "in", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur halb, vor lauter Angst, du m\u00fcssest es beschreiben.", "tokens": ["Nur", "halb", ",", "vor", "lau\u00b7ter", "Angst", ",", "du", "m\u00fcs\u00b7sest", "es", "be\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "PIAT", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}