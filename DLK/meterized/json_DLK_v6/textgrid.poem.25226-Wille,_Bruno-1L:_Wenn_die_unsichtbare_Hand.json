{"textgrid.poem.25226": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die unsichtbare Hand", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die unsichtbare Hand", "tokens": ["Wenn", "die", "un\u00b7sicht\u00b7ba\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Dich aus meinen Armen wand,", "tokens": ["Dich", "aus", "mei\u00b7nen", "Ar\u00b7men", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fragt dein Gr\u00fcbeln wohl beklommen,", "tokens": ["Fragt", "dein", "Gr\u00fc\u00b7beln", "wohl", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ins \u00d6de du gekommen ...", "tokens": ["Wie", "ins", "\u00d6\u00b7de", "du", "ge\u00b7kom\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00dft du, Liebster? Wei\u00dft du wohl?", "tokens": ["Wei\u00dft", "du", ",", "Liebs\u00b7ter", "?", "Wei\u00dft", "du", "wohl", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ums Schlo\u00df der Nachtwind rauscht,", "tokens": ["Wenn", "ums", "Schlo\u00df", "der", "Nacht\u00b7wind", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seufzend deine Sehnsucht lauscht/", "tokens": ["Seuf\u00b7zend", "dei\u00b7ne", "Sehn\u00b7sucht", "lauscht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch, ein Riegel geht verstohlen,", "tokens": ["Horch", ",", "ein", "Rie\u00b7gel", "geht", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es schleicht auf scheuen Sohlen ...", "tokens": ["Und", "es", "schleicht", "auf", "scheu\u00b7en", "Soh\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00dft du, Liebster? Wei\u00dft du wohl?", "tokens": ["Wei\u00dft", "du", ",", "Liebs\u00b7ter", "?", "Wei\u00dft", "du", "wohl", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Traufe wimmernd tropft,", "tokens": ["Wenn", "die", "Trau\u00b7fe", "wim\u00b7mernd", "tropft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Herz zum Springen klopft,", "tokens": ["Und", "das", "Herz", "zum", "Sprin\u00b7gen", "klopft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn vom Schluchzen hingerissen", "tokens": ["Wenn", "vom", "Schluch\u00b7zen", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich dein Antlitz birgt im Kissen/", "tokens": ["Sich", "dein", "Ant\u00b7litz", "birgt", "im", "Kis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00dft du, Liebster? Wei\u00dft du wohl?", "tokens": ["Wei\u00dft", "du", ",", "Liebs\u00b7ter", "?", "Wei\u00dft", "du", "wohl", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn im Regensturm der See", "tokens": ["Wenn", "im", "Re\u00b7gen\u00b7sturm", "der", "See"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wogend raunt, wie alles Weh", "tokens": ["Wo\u00b7gend", "raunt", ",", "wie", "al\u00b7les", "Weh"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "PWAV", "PIS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiegeselig dir entschliefe", "tokens": ["Wie\u00b7ge\u00b7se\u00b7lig", "dir", "ent\u00b7schlie\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der todesk\u00fchlen Tiefe ...", "tokens": ["In", "der", "to\u00b7des\u00b7k\u00fch\u00b7len", "Tie\u00b7fe", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00dft du, Liebster? Wei\u00dft du wohl?", "tokens": ["Wei\u00dft", "du", ",", "Liebs\u00b7ter", "?", "Wei\u00dft", "du", "wohl", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn dann bl\u00fcht aus Wolkendunkel", "tokens": ["Wenn", "dann", "bl\u00fcht", "aus", "Wol\u00b7ken\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trostgesang und Sterngefunkel/", "tokens": ["Trost\u00b7ge\u00b7sang", "und", "Stern\u00b7ge\u00b7fun\u00b7kel", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00dft du, was so z\u00e4rtlich zittert", "tokens": ["Wei\u00dft", "du", ",", "was", "so", "z\u00e4rt\u00b7lich", "zit\u00b7tert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie Odem dich umwittert?", "tokens": ["Und", "wie", "O\u00b7dem", "dich", "um\u00b7wit\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00dft du, Liebster? Wei\u00dft du wohl?", "tokens": ["Wei\u00dft", "du", ",", "Liebs\u00b7ter", "?", "Wei\u00dft", "du", "wohl", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}