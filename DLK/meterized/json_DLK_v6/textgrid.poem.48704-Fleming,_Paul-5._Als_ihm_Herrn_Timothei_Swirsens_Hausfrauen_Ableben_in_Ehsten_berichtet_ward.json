{"textgrid.poem.48704": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Als ihm Herrn Timothei Swirsens Hausfrauen Ableben in Ehsten berichtet ward", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin betr\u00fcbt mit ihm, dem Freunde jener Zeit.", "tokens": ["Ich", "bin", "be\u00b7tr\u00fcbt", "mit", "ihm", ",", "dem", "Freun\u00b7de", "je\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPER", "$,", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es tauret mich sein Fall, in dem er mu\u00df verlieren", "tokens": ["Es", "tau\u00b7ret", "mich", "sein", "Fall", ",", "in", "dem", "er", "mu\u00df", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des Herzens halben Teil und hin zum Grabe f\u00fchren.", "tokens": ["des", "Her\u00b7zens", "hal\u00b7ben", "Teil", "und", "hin", "zum", "Gra\u00b7be", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bin betr\u00fcbt mit ihm, es tauret mich sein Leid.", "tokens": ["Ich", "bin", "be\u00b7tr\u00fcbt", "mit", "ihm", ",", "es", "tau\u00b7ret", "mich", "sein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach, da\u00df di\u00df Beileid doch von ihm nun ist so weit!", "tokens": ["Ach", ",", "da\u00df", "di\u00df", "Bei\u00b7leid", "doch", "von", "ihm", "nun", "ist", "so", "weit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PDS", "NN", "ADV", "APPR", "PPER", "ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch da\u00df er nach der Zeit dasselbe m\u00f6ge sp\u00fcren,", "tokens": ["Doch", "da\u00df", "er", "nach", "der", "Zeit", "das\u00b7sel\u00b7be", "m\u00f6\u00b7ge", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "PDAT", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so will ich auch allhier ihm seinen Toten zieren", "tokens": ["so", "will", "ich", "auch", "all\u00b7hier", "ihm", "sei\u00b7nen", "To\u00b7ten", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und etwas setzen auf von keiner Sterblichkeit.", "tokens": ["und", "et\u00b7was", "set\u00b7zen", "auf", "von", "kei\u00b7ner", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir Menschen pflegen oft zu klagen \u00fcber Leichen", "tokens": ["Wir", "Men\u00b7schen", "pfle\u00b7gen", "oft", "zu", "kla\u00b7gen", "\u00fc\u00b7ber", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und wissen selber nicht, wie nah' uns unsre Zeichen", "tokens": ["und", "wis\u00b7sen", "sel\u00b7ber", "nicht", ",", "wie", "nah'", "uns", "uns\u00b7re", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des Todes sind gesteckt. Wer wei\u00df, ob auch um dich", "tokens": ["des", "To\u00b7des", "sind", "ge\u00b7steckt", ".", "Wer", "wei\u00df", ",", "ob", "auch", "um", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PWS", "VVFIN", "$,", "KOUS", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "ein Auge wird genetzt? Drum will ich Andre klagen", "tokens": ["ein", "Au\u00b7ge", "wird", "ge\u00b7netzt", "?", "Drum", "will", "ich", "And\u00b7re", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PAV", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und hierbeineben auch von meiner Schwachheit sagen:", "tokens": ["und", "hier\u00b7bei\u00b7ne\u00b7ben", "auch", "von", "mei\u00b7ner", "Schwach\u00b7heit", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "so hab' ich recht beweint in einem Fremden mich.", "tokens": ["so", "hab'", "ich", "recht", "be\u00b7weint", "in", "ei\u00b7nem", "Frem\u00b7den", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}