{"textgrid.poem.52136": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt Matho wil ein Artzt und Artzneymacher seyn,", "tokens": ["H\u00f6rt", "Ma\u00b7tho", "wil", "ein", "Artzt", "und", "Artz\u00b7ney\u00b7mac\u00b7her", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "VMFIN", "ART", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der manchen hat gebracht in Ungemach und Pein:", "tokens": ["Der", "man\u00b7chen", "hat", "ge\u00b7bracht", "in", "Un\u00b7ge\u00b7mach", "und", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie steigt die Kunst, vor satzt er Leut' in Angst und N\u00f6then,", "tokens": ["Wie", "steigt", "die", "Kunst", ",", "vor", "satzt", "er", "Leut'", "in", "Angst", "und", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "APPR", "VVFIN", "PPER", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nahm Ihnen, was ihm fehlt: Itzt lernt er sie gar t\u00f6dten.", "tokens": ["Nahm", "Ih\u00b7nen", ",", "was", "ihm", "fehlt", ":", "Itzt", "lernt", "er", "sie", "gar", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}