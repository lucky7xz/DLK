{"dta.poem.3285": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Geliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Die ich mir zum M\u00e4dchen w\u00e4hle,", "tokens": ["Die", "ich", "mir", "zum", "M\u00e4d\u00b7chen", "w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll von aufgeweckter Seele,", "tokens": ["Soll", "von", "auf\u00b7ge\u00b7weck\u00b7ter", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll von schlanker L\u00e4nge seyn.", "tokens": ["Soll", "von", "schlan\u00b7ker", "L\u00e4n\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanfte G\u00fcte, Witz im Scherze", "tokens": ["Sanf\u00b7te", "G\u00fc\u00b7te", ",", "Witz", "im", "Scher\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "R\u00fchrt mein Herze;", "tokens": ["R\u00fchrt", "mein", "Her\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nicht ein glatt Gesicht allein.", "tokens": ["Nicht", "ein", "glatt", "Ge\u00b7sicht", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJD", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Allzujung taugt nur zum Spielen!", "tokens": ["All\u00b7zu\u00b7jung", "taugt", "nur", "zum", "Spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fleischigt sey sie anzuf\u00fchlen,", "tokens": ["Flei\u00b7schigt", "sey", "sie", "an\u00b7zu\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gew\u00f6lbt die weisse Brust.", "tokens": ["Und", "ge\u00b7w\u00f6lbt", "die", "weis\u00b7se", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Brunette soll vor allen", "tokens": ["Die", "Bru\u00b7net\u00b7te", "soll", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mir gefallen:", "tokens": ["Mir", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sie ist dauerhaft zur Lust.", "tokens": ["Sie", "ist", "dau\u00b7er\u00b7haft", "zur", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Setzt noch unter diese Dinge,", "tokens": ["Setzt", "noch", "un\u00b7ter", "die\u00b7se", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie artig tanz\u2019 und singe:", "tokens": ["Da\u00df", "sie", "ar\u00b7tig", "tanz'", "und", "sin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches M\u00e4dchen ist ihr gleich?", "tokens": ["Wel\u00b7ches", "M\u00e4d\u00b7chen", "ist", "ihr", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O ihr M\u00e4dchenkenner! saget:", "tokens": ["O", "ihr", "M\u00e4d\u00b7chen\u00b7ken\u00b7ner", "!", "sa\u00b7get", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wers erjaget,", "tokens": ["Wers", "er\u00b7ja\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Hat der nicht ein K\u00f6nigreich?", "tokens": ["Hat", "der", "nicht", "ein", "K\u00f6\u00b7nig\u00b7reich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}