{"textgrid.poem.58247": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "285. Die Abgeschiedenheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer viel begehrt,", "tokens": ["Wer", "viel", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wird viel gest\u00f6rt,", "tokens": ["Wird", "viel", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer gar nichts will,", "tokens": ["Wer", "gar", "nichts", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bleibt immer still:", "tokens": ["Bleibt", "im\u00b7mer", "still", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In Trost und Freuden,", "tokens": ["In", "Trost", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In Furcht und Leiden", "tokens": ["In", "Furcht", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bleib abgeschieden", "tokens": ["Bleib", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "In Gottes Frieden!", "tokens": ["In", "Got\u00b7tes", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}