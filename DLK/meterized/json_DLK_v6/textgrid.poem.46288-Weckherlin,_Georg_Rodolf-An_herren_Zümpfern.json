{"textgrid.poem.46288": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An herren Z\u00fcmpfern", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wan nichts mu\u00df deinen leib ber\u00fchren,", "tokens": ["Wan", "nichts", "mu\u00df", "dei\u00b7nen", "leib", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was keinen lieblichen geruch,", "tokens": ["was", "kei\u00b7nen", "lieb\u00b7li\u00b7chen", "ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wan deine handschuch, stifel, schuch,", "tokens": ["wan", "dei\u00b7ne", "hand\u00b7schuch", ",", "sti\u00b7fel", ",", "schuch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dein kleid von seiden oder tuch", "tokens": ["dein", "kleid", "von", "sei\u00b7den", "o\u00b7der", "tuch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit lieblichem geruch prachtieren:", "tokens": ["mit", "lieb\u00b7li\u00b7chem", "ge\u00b7ruch", "prach\u00b7tie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wan wol ger\u00e4uchert dein gemach", "tokens": ["Wan", "wol", "ge\u00b7r\u00e4u\u00b7chert", "dein", "ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVPP", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "und stets wolriechend jede sach", "tokens": ["und", "stets", "wol\u00b7ri\u00b7e\u00b7chend", "je\u00b7de", "sach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "an dir, dein bart, haut, haar, hut, feder,", "tokens": ["an", "dir", ",", "dein", "bart", ",", "haut", ",", "haar", ",", "hut", ",", "fe\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.9": {"text": "ja auch die brief, der s\u00e4cken leder:", "tokens": ["ja", "auch", "die", "brief", ",", "der", "s\u00e4\u00b7cken", "le\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "VVFIN", "$,", "PRELS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wan deine w\u00e4scherin mu\u00df auch", "tokens": ["Wan", "dei\u00b7ne", "w\u00e4\u00b7sche\u00b7rin", "mu\u00df", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "wolriechend deine leinwat waschen,", "tokens": ["wol\u00b7ri\u00b7e\u00b7chend", "dei\u00b7ne", "lein\u00b7wat", "wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "mein? was geruch, Zumpf, oder rauch", "tokens": ["mein", "?", "was", "ge\u00b7ruch", ",", "Zumpf", ",", "o\u00b7der", "rauch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "$.", "PWS", "ADV", "$,", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "hast du von gestriger maultaschen?", "tokens": ["hast", "du", "von", "ge\u00b7stri\u00b7ger", "maul\u00b7ta\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}