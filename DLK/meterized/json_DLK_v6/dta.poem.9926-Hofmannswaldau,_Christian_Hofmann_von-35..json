{"dta.poem.9926": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wasser das ver\u00e4ndert mich offt in einem augenblicke/", "tokens": ["Was\u00b7ser", "das", "ver\u00b7\u00e4n\u00b7dert", "mich", "offt", "in", "ei\u00b7nem", "au\u00b7gen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ohne fl\u00fcgel flieg ich hoch/ niedrig/ vor mich/ und zur\u00fccke/", "tokens": ["Oh\u00b7ne", "fl\u00fc\u00b7gel", "flieg", "ich", "hoch", "/", "nied\u00b7rig", "/", "vor", "mich", "/", "und", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$(", "ADJD", "$(", "APPR", "PPER", "$(", "KON", "VVFIN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Alle von sich weg mich jagen/ sprechen da\u00df ich fressen kan/", "tokens": ["Al\u00b7le", "von", "sich", "weg", "mich", "ja\u00b7gen", "/", "spre\u00b7chen", "da\u00df", "ich", "fres\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PRF", "ADV", "PPER", "VVINF", "$(", "VVFIN", "KOUS", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da ich doch kein leben habe/ noch viel minder maul und zahn.", "tokens": ["Da", "ich", "doch", "kein", "le\u00b7ben", "ha\u00b7be", "/", "noch", "viel", "min\u00b7der", "maul", "und", "zahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "VVINF", "VAFIN", "$(", "ADV", "ADV", "ADV", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}