{"textgrid.poem.39326": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "31. St\u00e4rkung in tiefer Dunkelheit", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fortgek\u00e4mpft und fortgerungen,", "tokens": ["Fort\u00b7ge\u00b7k\u00e4mpft", "und", "fort\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zum Lichte", "tokens": ["Bis", "zum", "Lich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mu\u00df es, bange Seele, seyn!", "tokens": ["Mu\u00df", "es", ",", "ban\u00b7ge", "See\u00b7le", ",", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "NN", "$,", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die tiefsten Dunkelheiten", "tokens": ["Durch", "die", "tiefs\u00b7ten", "Dun\u00b7kel\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann dich Jesus hinbegleiten;", "tokens": ["Kann", "dich", "Je\u00b7sus", "hin\u00b7be\u00b7glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Muth spricht er den Schwachen ein.", "tokens": ["Muth", "spricht", "er", "den", "Schwa\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Bei der Hand will er dich fassen", "tokens": ["Bei", "der", "Hand", "will", "er", "dich", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheinst du gleich von ihm verlassen;", "tokens": ["Scheinst", "du", "gleich", "von", "ihm", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaube nur, und zweifle nicht!", "tokens": ["Glau\u00b7be", "nur", ",", "und", "zweif\u00b7le", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bethe, k\u00e4mpfe ohne Wanken", "tokens": ["Be\u00b7the", ",", "k\u00e4mp\u00b7fe", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald wirst du voll Freude danken!", "tokens": ["Bald", "wirst", "du", "voll", "Freu\u00b7de", "dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bald umgiebt dich Kraft und Licht", "tokens": ["Bald", "um\u00b7giebt", "dich", "Kraft", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Bald wird dir sein Antlitz funkeln", "tokens": ["Bald", "wird", "dir", "sein", "Ant\u00b7litz", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hoffe, harre! Glaub im Dunkeln!", "tokens": ["Hof\u00b7fe", ",", "har\u00b7re", "!", "Glaub", "im", "Dun\u00b7keln", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie gereut ihn seiner Wahl", "tokens": ["Nie", "ge\u00b7reut", "ihn", "sei\u00b7ner", "Wahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er will dich im Glauben \u00fcben;", "tokens": ["Er", "will", "dich", "im", "Glau\u00b7ben", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gott, die Liebe, kann nur lieben", "tokens": ["Gott", ",", "die", "Lie\u00b7be", ",", "kann", "nur", "lie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wonne bald wird deine Quaal", "tokens": ["Won\u00b7ne", "bald", "wird", "dei\u00b7ne", "Qua\u00b7al"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weg von aller Welt die Blicke", "tokens": ["Weg", "von", "al\u00b7ler", "Welt", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau nicht seitw\u00e4rts, nicht zur\u00fccke;", "tokens": ["Schau", "nicht", "seit\u00b7w\u00e4rts", ",", "nicht", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur auf Gott und Ewigkeit!", "tokens": ["Nur", "auf", "Gott", "und", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur zu deinem Jesus wende", "tokens": ["Nur", "zu", "dei\u00b7nem", "Je\u00b7sus", "wen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aug und Herz und Sinn und H\u00e4nde,", "tokens": ["Aug", "und", "Herz", "und", "Sinn", "und", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis er himmlisch dich erfreut.", "tokens": ["Bis", "er", "himm\u00b7lisch", "dich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus des Jammers wilden Wogen", "tokens": ["Aus", "des", "Jam\u00b7mers", "wil\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat dich oft herausgezogen", "tokens": ["Hat", "dich", "oft", "her\u00b7aus\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Allmacht treue Hand.", "tokens": ["Sei\u00b7ner", "All\u00b7macht", "treu\u00b7e", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie zu kurz ist seine Rechte;", "tokens": ["Nie", "zu", "kurz", "ist", "sei\u00b7ne", "Rech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ist einer seiner Knechte,", "tokens": ["Wo", "ist", "ei\u00b7ner", "sei\u00b7ner", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der bey ihm nicht Rettung fand?", "tokens": ["Der", "bey", "ihm", "nicht", "Ret\u00b7tung", "fand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Schlie\u00df dich ein in deine Kammer;", "tokens": ["Schlie\u00df", "dich", "ein", "in", "dei\u00b7ne", "Kam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh und sch\u00fctte deinen Jammer", "tokens": ["Geh", "und", "sch\u00fct\u00b7te", "dei\u00b7nen", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus in Gottes Vaterherz:", "tokens": ["Aus", "in", "Got\u00b7tes", "Va\u00b7ter\u00b7herz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kannst du gleich ihn nicht empfinden,", "tokens": ["Kannst", "du", "gleich", "ihn", "nicht", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Worte nicht, nicht Thr\u00e4nen finden,", "tokens": ["Wor\u00b7te", "nicht", ",", "nicht", "Thr\u00e4\u00b7nen", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PTKNEG", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klage schweigend deinen Schmerz!", "tokens": ["Kla\u00b7ge", "schwei\u00b7gend", "dei\u00b7nen", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kr\u00e4ftig ist dein tiefes Schweigen;", "tokens": ["Kr\u00e4f\u00b7tig", "ist", "dein", "tie\u00b7fes", "Schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott wird sich als Vater zeigen;", "tokens": ["Gott", "wird", "sich", "als", "Va\u00b7ter", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "KOUS", "NN", "VVINF", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Glaube nur, da\u00df er dich h\u00f6rt!", "tokens": ["Glau\u00b7be", "nur", ",", "da\u00df", "er", "dich", "h\u00f6rt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaub, da\u00df Jesus dich vertrittet;", "tokens": ["Glaub", ",", "da\u00df", "Je\u00b7sus", "dich", "ver\u00b7trit\u00b7tet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NE", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Glaub, da\u00df alles, was er bittet,", "tokens": ["Glaub", ",", "da\u00df", "al\u00b7les", ",", "was", "er", "bit\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott, sein Vater, ihm gew\u00e4hrt.", "tokens": ["Gott", ",", "sein", "Va\u00b7ter", ",", "ihm", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Drum, so will ich nicht verzagen,", "tokens": ["Drum", ",", "so", "will", "ich", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich vor Gottes Antlitz wagen;", "tokens": ["Mich", "vor", "Got\u00b7tes", "Ant\u00b7litz", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm ich um, so komm ich um!", "tokens": ["Komm", "ich", "um", ",", "so", "komm", "ich", "um", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch: Ich werd ihn \u00fcberwinden;", "tokens": ["Doch", ":", "Ich", "werd", "ihn", "\u00fc\u00b7berw\u00b7in\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer ihn sucht, der wird ihn finden.", "tokens": ["Wer", "ihn", "sucht", ",", "der", "wird", "ihn", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er bringt nur die Heuchler um!", "tokens": ["Er", "bringt", "nur", "die", "Heuch\u00b7ler", "um", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}