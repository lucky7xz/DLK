{"textgrid.poem.45834": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[sie bl\u00fcht mir nicht in T\u00e4lern]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie bl\u00fcht mir nicht in T\u00e4lern, nicht auf H\u00f6hen,", "tokens": ["Sie", "bl\u00fcht", "mir", "nicht", "in", "T\u00e4\u00b7lern", ",", "nicht", "auf", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht in dem Wolkenflug; nicht in der Flut,", "tokens": ["Nicht", "in", "dem", "Wol\u00b7ken\u00b7flug", ";", "nicht", "in", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$.", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die fort wie Sehnsucht eilt, kann ich sie sehen,", "tokens": ["Die", "fort", "wie", "Sehn\u00b7sucht", "eilt", ",", "kann", "ich", "sie", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "KOKOM", "NN", "VVFIN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aus dem stillen See, der ewig ruht,", "tokens": ["Und", "aus", "dem", "stil\u00b7len", "See", ",", "der", "e\u00b7wig", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Steigt nicht ihr Bild. Es ist schon l\u00e4ngst geschehen,", "tokens": ["Steigt", "nicht", "ihr", "Bild", ".", "Es", "ist", "schon", "l\u00e4ngst", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df die Natur verlor, was ich mit Mut", "tokens": ["Da\u00df", "die", "Na\u00b7tur", "ver\u00b7lor", ",", "was", "ich", "mit", "Mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "PWS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Erringen soll. Drum mu\u00df mit meinen Sinnen", "tokens": ["Er\u00b7rin\u00b7gen", "soll", ".", "Drum", "mu\u00df", "mit", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "$.", "PAV", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich ewig der Entflohenen Netze spinnen.", "tokens": ["Ich", "e\u00b7wig", "der", "Ent\u00b7flo\u00b7he\u00b7nen", "Net\u00b7ze", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}