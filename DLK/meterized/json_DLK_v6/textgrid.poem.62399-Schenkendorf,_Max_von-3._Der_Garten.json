{"textgrid.poem.62399": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "3. Der Garten", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den Garten mu\u00df ich blicken,", "tokens": ["In", "den", "Gar\u00b7ten", "mu\u00df", "ich", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das frische stille Gr\u00fcn,", "tokens": ["In", "das", "fri\u00b7sche", "stil\u00b7le", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend W\u00fcnsche mu\u00df ich schicken", "tokens": ["Tau\u00b7send", "W\u00fcn\u00b7sche", "mu\u00df", "ich", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fernhin wo die Schwalben ziehn.", "tokens": ["Fern\u00b7hin", "wo", "die", "Schwal\u00b7ben", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fliegt nur mit den Morgenwinden,", "tokens": ["Fliegt", "nur", "mit", "den", "Mor\u00b7gen\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Wolken flieget fort,", "tokens": ["Mit", "den", "Wol\u00b7ken", "flie\u00b7get", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure Heimat sollt ihr finden,", "tokens": ["Eu\u00b7re", "Hei\u00b7mat", "sollt", "ihr", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieben W\u00fcnsche, Ziel und Ort.", "tokens": ["Lie\u00b7ben", "W\u00fcn\u00b7sche", ",", "Ziel", "und", "Ort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00fcckw\u00e4rts will ja nicht mein Sehnen,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "will", "ja", "nicht", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer in die Eitelkeit;", "tokens": ["Nim\u00b7mer", "in", "die", "Ei\u00b7tel\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Seufzer, diese Thr\u00e4nen", "tokens": ["Die\u00b7se", "Seuf\u00b7zer", ",", "die\u00b7se", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gelten keinem Erdenleid.", "tokens": ["Gel\u00b7ten", "kei\u00b7nem", "Er\u00b7den\u00b7leid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ueber Wolken, \u00fcber Sterne", "tokens": ["Ue\u00b7ber", "Wol\u00b7ken", ",", "\u00fc\u00b7ber", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufw\u00e4rts, aufw\u00e4rts, himmelw\u00e4rts,", "tokens": ["Auf\u00b7w\u00e4rts", ",", "auf\u00b7w\u00e4rts", ",", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Neubelebt, in sel'ger Ferne", "tokens": ["Neu\u00b7be\u00b7lebt", ",", "in", "sel'\u00b7ger", "Fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sink' ich an das gro\u00dfe Herz!", "tokens": ["Sink'", "ich", "an", "das", "gro\u00b7\u00dfe", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo die Wunden nicht mehr dr\u00fccken,", "tokens": ["Wo", "die", "Wun\u00b7den", "nicht", "mehr", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Heer der W\u00fcnsche schweigt,", "tokens": ["Wo", "das", "Heer", "der", "W\u00fcn\u00b7sche", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu mir mit s\u00fc\u00dfen Blicken", "tokens": ["Und", "zu", "mir", "mit", "s\u00fc\u00b7\u00dfen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die ew'ge Liebe neigt.", "tokens": ["Sich", "die", "ew'\u00b7ge", "Lie\u00b7be", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aus den Wipfeln will es steigen", "tokens": ["Aus", "den", "Wip\u00b7feln", "will", "es", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein geliebtes Wunderbild,", "tokens": ["Mein", "ge\u00b7lieb\u00b7tes", "Wun\u00b7der\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach des Gartens gr\u00fcnen Zweigen", "tokens": ["Nach", "des", "Gar\u00b7tens", "gr\u00fc\u00b7nen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blick' ich still und lusterf\u00fcllt.", "tokens": ["Blick'", "ich", "still", "und", "lus\u00b7ter\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}