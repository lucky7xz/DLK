{"textgrid.poem.67790": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Lehrers", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vater, Vater, Dich soll ich", "tokens": ["O", "Va\u00b7ter", ",", "Va\u00b7ter", ",", "Dich", "soll", "ich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "PPER", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menschenkinder lehren!", "tokens": ["Die", "Men\u00b7schen\u00b7kin\u00b7der", "leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O lehre mich, erhebe mich,", "tokens": ["O", "leh\u00b7re", "mich", ",", "er\u00b7he\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Wort nur ", "tokens": ["Dein", "Wort", "nur"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und la\u00df es denn im Munde mein", "tokens": ["Und", "la\u00df", "es", "denn", "im", "Mun\u00b7de", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPRART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Br\u00fcdern Milch und Honig sein,", "tokens": ["Den", "Br\u00fc\u00b7dern", "Milch", "und", "Ho\u00b7nig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ewig zu genie\u00dfen,", "tokens": ["Es", "e\u00b7wig", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es ewig fort zu gie\u00dfen!", "tokens": ["Es", "e\u00b7wig", "fort", "zu", "gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Gott, o Gott, verdiente ich,", "tokens": ["O", "Gott", ",", "o", "Gott", ",", "ver\u00b7dien\u00b7te", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich, Deinen Sohn zu kennen?", "tokens": ["Ich", ",", "Dei\u00b7nen", "Sohn", "zu", "ken\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du w\u00e4hltest, Du geliebtest mich,", "tokens": ["Du", "w\u00e4hl\u00b7test", ",", "Du", "ge\u00b7lieb\u00b7test", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihm nur Dich zu nennen,", "tokens": ["In", "ihm", "nur", "Dich", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und zeigtest mir Dein Himmelreich,", "tokens": ["Und", "zeig\u00b7test", "mir", "Dein", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verborgen sonst uns allzugleich;", "tokens": ["Ver\u00b7bor\u00b7gen", "sonst", "uns", "all\u00b7zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was Witz und Weisheit schweiget,", "tokens": ["Was", "Witz", "und", "Weis\u00b7heit", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hast Du mir, Herr, gezeiget.", "tokens": ["Hast", "Du", "mir", ",", "Herr", ",", "ge\u00b7zei\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Geheimni\u00df, Gottes Menschenplan,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df", ",", "Got\u00b7tes", "Men\u00b7schen\u00b7plan", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Schatz der Ewigkeiten!", "tokens": ["Du", "Schatz", "der", "E\u00b7wig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie lieb' ich Dich und bete an", "tokens": ["Wie", "lieb'", "ich", "Dich", "und", "be\u00b7te", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und d\u00fcrste, fortzuschreiten", "tokens": ["Und", "d\u00fcrs\u00b7te", ",", "fort\u00b7zu\u00b7schrei\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu Dir, der Du mir Alles bist", "tokens": ["Zu", "Dir", ",", "der", "Du", "mir", "Al\u00b7les", "bist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "PPER", "PIS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dies mein armes Ich erkiest,", "tokens": ["Und", "dies", "mein", "ar\u00b7mes", "Ich", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PPOSAT", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir drin in Himmelsstille", "tokens": ["Mir", "drin", "in", "Him\u00b7mels\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu werden Licht und F\u00fclle!", "tokens": ["Zu", "wer\u00b7den", "Licht", "und", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6rte den Gesang der Nacht,", "tokens": ["Ich", "h\u00f6r\u00b7te", "den", "Ge\u00b7sang", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lied der stillen Sterne;", "tokens": ["Das", "Lied", "der", "stil\u00b7len", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah den Schauplatz Deiner Macht,", "tokens": ["Ich", "sah", "den", "Schau\u00b7platz", "Dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ahnend Dich von ferne.", "tokens": ["Nur", "ah\u00b7nend", "Dich", "von", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und, Herr, da wardst Du mir so nah,", "tokens": ["Und", ",", "Herr", ",", "da", "wardst", "Du", "mir", "so", "nah", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dort \u00fcberall und hier mir da,", "tokens": ["Dort", "\u00fc\u00b7be\u00b7rall", "und", "hier", "mir", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo sich's im Herzen reget,", "tokens": ["Wo", "sich's", "im", "Her\u00b7zen", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo dieser Puls Dich schl\u00e4get.", "tokens": ["Wo", "die\u00b7ser", "Puls", "Dich", "schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich fragte Sonn' und Mond und Stern,", "tokens": ["Ich", "frag\u00b7te", "Sonn'", "und", "Mond", "und", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort Himmel und hier Erden:", "tokens": ["Dort", "Him\u00b7mel", "und", "hier", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbsaht Ihr ihn?\u00ab \u00bbSahn ihn nur von fern;", "tokens": ["\u00bb", "saht", "Ihr", "ihn", "?", "\u00ab", "\u00bb", "Sahn", "ihn", "nur", "von", "fern", ";"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPER", "$.", "$(", "$(", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Hauch nur hie\u00df uns werden;", "tokens": ["Sein", "Hauch", "nur", "hie\u00df", "uns", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Wink aus jener Dunkelheit,", "tokens": ["Ein", "Wink", "aus", "je\u00b7ner", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo er sich selbst ist Licht und Kleid,", "tokens": ["Wo", "er", "sich", "selbst", "ist", "Licht", "und", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur dieses Winkes Zeugen,", "tokens": ["Nur", "die\u00b7ses", "Win\u00b7kes", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verk\u00fcnden wir und schweigen.\u00ab", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "wir", "und", "schwei\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Umringt von lichter Dunkelheit,", "tokens": ["Um\u00b7ringt", "von", "lich\u00b7ter", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Gott, sank ich danieder,", "tokens": ["O", "Gott", ",", "sank", "ich", "da\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fand Alles voll von Dir allweit", "tokens": ["Fand", "Al\u00b7les", "voll", "von", "Dir", "all\u00b7weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles \u00f6de wieder,", "tokens": ["Und", "Al\u00b7les", "\u00f6\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00e4chzete, Dich nah zu sehn,", "tokens": ["Und", "\u00e4ch\u00b7ze\u00b7te", ",", "Dich", "nah", "zu", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und, Herz, tief in Dein Herz zu gehn,", "tokens": ["Und", ",", "Herz", ",", "tief", "in", "Dein", "Herz", "zu", "gehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am Brunnquell aller Gaben", "tokens": ["Am", "Brunn\u00b7quell", "al\u00b7ler", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich innig satt zu laben.", "tokens": ["Mich", "in\u00b7nig", "satt", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da sprach statt Sonne mir und Stern", "tokens": ["Da", "sprach", "statt", "Son\u00b7ne", "mir", "und", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bruder mir auf Erden:", "tokens": ["Ein", "Bru\u00b7der", "mir", "auf", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwas tappst Du da, so matt und fern!", "tokens": ["\u00bb", "was", "tappst", "Du", "da", ",", "so", "matt", "und", "fern", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hie\u00df er nicht Mensch Dich werden?", "tokens": ["Hie\u00df", "er", "nicht", "Mensch", "Dich", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "PPER", "VAINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und hat Dir in Dein Menschenbild", "tokens": ["Und", "hat", "Dir", "in", "Dein", "Men\u00b7schen\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Gottheit Kr\u00e4fte tief verh\u00fcllt,", "tokens": ["Der", "Got\u00b7theit", "Kr\u00e4f\u00b7te", "tief", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sollst am Quell der Gaben", "tokens": ["Und", "sollst", "am", "Quell", "der", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich satt, o satt einst laben!\u00ab", "tokens": ["Dich", "satt", ",", "o", "satt", "einst", "la\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "$,", "FM", "ADJD", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bber ward, wie Du!\u00ab Mein Sch\u00f6pfer dort,", "tokens": ["\u00bb", "er", "ward", ",", "wie", "Du", "!", "\u00ab", "Mein", "Sch\u00f6p\u00b7fer", "dort", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$("