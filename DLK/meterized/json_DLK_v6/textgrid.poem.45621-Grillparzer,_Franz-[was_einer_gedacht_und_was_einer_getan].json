{"textgrid.poem.45621": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[was einer gedacht und was einer getan]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was einer gedacht und was einer getan,", "tokens": ["Was", "ei\u00b7ner", "ge\u00b7dacht", "und", "was", "ei\u00b7ner", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "KON", "PWS", "PIS", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ist ungleich an Wert und Lohne;", "tokens": ["Ist", "un\u00b7gleich", "an", "Wert", "und", "Loh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NE", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem, was ich gedacht, schufst du Leben an,", "tokens": ["Dem", ",", "was", "ich", "ge\u00b7dacht", ",", "schufst", "du", "Le\u00b7ben", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "So nimm denn immer die Krone.", "tokens": ["So", "nimm", "denn", "im\u00b7mer", "die", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}