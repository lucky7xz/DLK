{"textgrid.poem.28364": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu etwas grossem noch wird Sordalus wol werden;", "tokens": ["Zu", "et\u00b7was", "gros\u00b7sem", "noch", "wird", "Sor\u00b7da\u00b7lus", "wol", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "ADV", "VAFIN", "NE", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann seinerley Geburt ist nicht gemein auff Erden;", "tokens": ["Dann", "sei\u00b7ner\u00b7ley", "Ge\u00b7burt", "ist", "nicht", "ge\u00b7mein", "auff", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist ihm selbst bewust, (man denckt ihm auch sehr dran)", "tokens": ["Es", "ist", "ihm", "selbst", "be\u00b7wust", ",", "(", "man", "denckt", "ihm", "auch", "sehr", "dran", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$,", "$(", "PIS", "VVFIN", "PPER", "ADV", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Mutter hat ihn bracht und hatte keinen Mann.", "tokens": ["Die", "Mut\u00b7ter", "hat", "ihn", "bracht", "und", "hat\u00b7te", "kei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}