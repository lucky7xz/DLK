{"textgrid.poem.26566": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Allerseelen.", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allerseelen.", "tokens": ["Al\u00b7ler\u00b7see\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Grauer, feuchtkalter Nebel", "tokens": ["Grau\u00b7er", ",", "feucht\u00b7kal\u00b7ter", "Ne\u00b7bel"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Umh\u00fcllt das Land,", "tokens": ["Um\u00b7h\u00fcllt", "das", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und in dem kleinen Friedhof dort am Bergeshang", "tokens": ["Und", "in", "dem", "klei\u00b7nen", "Fried\u00b7hof", "dort", "am", "Ber\u00b7ges\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schimmern Grablaternen,", "tokens": ["Schim\u00b7mern", "Grab\u00b7la\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schimmern auf den H\u00fcgeln Immortellen und Astern.", "tokens": ["Schim\u00b7mern", "auf", "den", "H\u00fc\u00b7geln", "Im\u00b7mor\u00b7tel\u00b7len", "und", "As\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Ja, heute gedenkt", "tokens": ["Ja", ",", "heu\u00b7te", "ge\u00b7denkt"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Jeder seiner Todten,", "tokens": ["Je\u00b7der", "sei\u00b7ner", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schm\u00fcckt die St\u00e4tten,", "tokens": ["Schm\u00fcckt", "die", "St\u00e4t\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wo sie vermodern oder vermodert sind \u2013", "tokens": ["Wo", "sie", "ver\u00b7mo\u00b7dern", "o\u00b7der", "ver\u00b7mo\u00b7dert", "sind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "KON", "VVPP", "VAFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und das Ged\u00e4chtni\u00df der Menschheit", "tokens": ["Und", "das", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Umwindet Male aus Erz und Marmor", "tokens": ["Um\u00b7win\u00b7det", "Ma\u00b7le", "aus", "Erz", "und", "Mar\u00b7mor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit den Kr\u00e4nzen des Nachruhms.", "tokens": ["Mit", "den", "Kr\u00e4n\u00b7zen", "des", "Nach\u00b7ruhms", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Wer aber gedenkt", "tokens": ["Wer", "a\u00b7ber", "ge\u00b7denkt"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Ungenannten,", "tokens": ["Der", "Un\u00b7ge\u00b7nann\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Ungeliebten,", "tokens": ["Der", "Un\u00b7ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Gr\u00e4berlosen?", "tokens": ["Der", "Gr\u00e4\u00b7ber\u00b7lo\u00b7sen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Derer, die dahingegangen", "tokens": ["De\u00b7rer", ",", "die", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fernab vom Herzen der Mitwelt", "tokens": ["Fern\u00b7ab", "vom", "Her\u00b7zen", "der", "Mit\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und unvermi\u00dft,", "tokens": ["Und", "un\u00b7ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Unbetrauert,", "tokens": ["Un\u00b7be\u00b7trau\u00b7ert", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ihre Atome dem All zur\u00fcckgegeben?", "tokens": ["Ih\u00b7re", "A\u00b7to\u00b7me", "dem", "All", "zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ihrer gedenkt heute", "tokens": ["Ih\u00b7rer", "ge\u00b7denkt", "heu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "ADV"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ein Mensch,", "tokens": ["Ein", "Mensch", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der, gleich ihnen,", "tokens": ["Der", ",", "gleich", "ih\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Gelebt,", "tokens": ["Ge\u00b7lebt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Gek\u00e4mpft,", "tokens": ["Ge\u00b7k\u00e4mpft", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Gelitten", "tokens": ["Ge\u00b7lit\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und schaudernd oft mit ihrem Schicksal", "tokens": ["Und", "schau\u00b7dernd", "oft", "mit", "ih\u00b7rem", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das eig'ne vorausempfunden.", "tokens": ["Das", "eig'\u00b7ne", "vor\u00b7aus\u00b7em\u00b7pfun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}