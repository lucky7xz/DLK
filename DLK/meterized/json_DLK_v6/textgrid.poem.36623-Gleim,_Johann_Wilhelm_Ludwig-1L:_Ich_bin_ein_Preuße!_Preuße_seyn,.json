{"textgrid.poem.36623": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin ein Preu\u00dfe! Preu\u00dfe seyn,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Preu\u00dfe! Preu\u00dfe seyn,", "tokens": ["Ich", "bin", "ein", "Preu\u00b7\u00dfe", "!", "Preu\u00b7\u00dfe", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist seyn: Ein freyer Mann,", "tokens": ["Ist", "seyn", ":", "Ein", "frey\u00b7er", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAINF", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der seiner Freyheit sich erfreun", "tokens": ["Der", "sei\u00b7ner", "Frey\u00b7heit", "sich", "er\u00b7freun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen St\u00e4nden kann!", "tokens": ["In", "al\u00b7len", "St\u00e4n\u00b7den", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In allen St\u00e4nden ist Gesetz!", "tokens": ["In", "al\u00b7len", "St\u00e4n\u00b7den", "ist", "Ge\u00b7setz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer nach Gesetzen lebt,", "tokens": ["Wer", "nach", "Ge\u00b7set\u00b7zen", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zieht all' die Freyheit in sein Netz,", "tokens": ["Zieht", "all'", "die", "Frey\u00b7heit", "in", "sein", "Netz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Herz und Geist erhebt!", "tokens": ["Die", "Herz", "und", "Geist", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich, ein Soldat, ich freue mich,", "tokens": ["Ich", ",", "ein", "Sol\u00b7dat", ",", "ich", "freu\u00b7e", "mich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kriegsgesetzes-Zucht!", "tokens": ["Der", "Kriegs\u00b7ge\u00b7set\u00b7zes\u00b7Zucht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie macht mich brav! Mit ihr schlag' ich", "tokens": ["Sie", "macht", "mich", "brav", "!", "Mit", "ihr", "schlag'", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "APPR", "PPER", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Feindmann in die Flucht!", "tokens": ["Den", "Feind\u00b7mann", "in", "die", "Flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Durch sie sch\u00fctz' ich das Vaterland,", "tokens": ["Durch", "sie", "sch\u00fctz'", "ich", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und Weib und Kind und Freund!", "tokens": ["Und", "Weib", "und", "Kind", "und", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und drohe mit bewehrter Hand,", "tokens": ["Und", "dro\u00b7he", "mit", "be\u00b7wehr\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem der's nicht ehrlich meint!", "tokens": ["Dem", "der's", "nicht", "ehr\u00b7lich", "meint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und werde, was ich noch nicht bin,", "tokens": ["Und", "wer\u00b7de", ",", "was", "ich", "noch", "nicht", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In t\u00f6dtlicher Gefahr", "tokens": ["In", "t\u00f6dt\u00b7li\u00b7cher", "Ge\u00b7fahr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von vestem unbesorgtem Sinn,", "tokens": ["Von", "ves\u00b7tem", "un\u00b7be\u00b7sorg\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und besser als ich war!", "tokens": ["Und", "bes\u00b7ser", "als", "ich", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich, etwa nur ein Grenadier,", "tokens": ["Ich", ",", "et\u00b7wa", "nur", "ein", "Gre\u00b7na\u00b7di\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Feinde schlagen kann;", "tokens": ["Der", "Fein\u00b7de", "schla\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Todgefahren schein ich mir,", "tokens": ["In", "Tod\u00b7ge\u00b7fah\u00b7ren", "schein", "ich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erst recht ein braver Mann!", "tokens": ["Erst", "recht", "ein", "bra\u00b7ver", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und thu' ich alle meine Pflicht,", "tokens": ["Und", "thu'", "ich", "al\u00b7le", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bin ich brav, und gut,", "tokens": ["Und", "bin", "ich", "brav", ",", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So tret' ich jedem ins Gesicht,", "tokens": ["So", "tret'", "ich", "je\u00b7dem", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwinge meinen Hut!", "tokens": ["Und", "schwin\u00b7ge", "mei\u00b7nen", "Hut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und schaue, wenn der K\u00f6nig k\u00f6mmt,", "tokens": ["Und", "schau\u00b7e", ",", "wenn", "der", "K\u00f6\u00b7nig", "k\u00f6mmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem K\u00f6nig ins Gesicht!", "tokens": ["Dem", "K\u00f6\u00b7nig", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meines Herzens Freude hemmt", "tokens": ["Und", "mei\u00b7nes", "Her\u00b7zens", "Freu\u00b7de", "hemmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Hauptmanns: ", "tokens": ["Des", "Haupt\u00b7manns", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Gehorsam jeglichem Befehl", "tokens": ["Ge\u00b7hor\u00b7sam", "jeg\u00b7li\u00b7chem", "Be\u00b7fehl"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seinem Eigensinn,", "tokens": ["Und", "sei\u00b7nem", "Ei\u00b7gen\u00b7sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' ichs sonst aber keinen Hehl,", "tokens": ["Hab'", "ichs", "sonst", "a\u00b7ber", "kei\u00b7nen", "Hehl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich ihm b\u00f6se bin;", "tokens": ["Da\u00df", "ich", "ihm", "b\u00f6\u00b7se", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn er gerecht zu seyn vergi\u00dft,", "tokens": ["Wenn", "er", "ge\u00b7recht", "zu", "seyn", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das er schon oft verga\u00df,", "tokens": ["Das", "er", "schon", "oft", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df er z\u00fcrnt, wenns Noth nicht ist,", "tokens": ["Und", "da\u00df", "er", "z\u00fcrnt", ",", "wenns", "Noth", "nicht", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und streng' ist ohne Maa\u00df!", "tokens": ["Und", "streng'", "ist", "oh\u00b7ne", "Maa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Inde\u00df, was ists in dieser Welt?", "tokens": ["In\u00b7de\u00df", ",", "was", "ists", "in", "die\u00b7ser", "Welt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frag' ich, der Grenadier;", "tokens": ["Frag'", "ich", ",", "der", "Gre\u00b7na\u00b7di\u00b7er", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist wohl, wenn man's am Licht erhellt,", "tokens": ["Ist", "wohl", ",", "wenn", "man's", "am", "Licht", "er\u00b7hellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vollkommenheit in ihr?", "tokens": ["Voll\u00b7kom\u00b7men\u00b7heit", "in", "ihr", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Vollkommenheit ist aber doch", "tokens": ["Voll\u00b7kom\u00b7men\u00b7heit", "ist", "a\u00b7ber", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jener! Das ist wahr!", "tokens": ["In", "je\u00b7ner", "!", "Das", "ist", "wahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$.", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Zeit geht hin! Wie lang' ists noch?", "tokens": ["Die", "Zeit", "geht", "hin", "!", "Wie", "lang'", "ists", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PWAV", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur etwa drey\u00dfig Jahr!", "tokens": ["Nur", "et\u00b7wa", "drey\u00b7\u00dfig", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die drey\u00dfig also wollen wir", "tokens": ["Die", "drey\u00b7\u00dfig", "al\u00b7so", "wol\u00b7len", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vesthalten unsern Eid,", "tokens": ["Vest\u00b7hal\u00b7ten", "un\u00b7sern", "Eid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gehn, als brave Grenadier,", "tokens": ["Und", "gehn", ",", "als", "bra\u00b7ve", "Gre\u00b7na\u00b7di\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Zeit in Ewigkeit!", "tokens": ["Aus", "Zeit", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}