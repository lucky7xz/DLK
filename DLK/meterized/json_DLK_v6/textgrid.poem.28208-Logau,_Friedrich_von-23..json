{"textgrid.poem.28208": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Poeten, die sollen die G\u00f6tter nicht nennen,", "tokens": ["Po\u00b7et\u00b7en", ",", "die", "sol\u00b7len", "die", "G\u00f6t\u00b7ter", "nicht", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Christen verlachen, die Heyden bekennen;", "tokens": ["Die", "Chris\u00b7ten", "ver\u00b7la\u00b7chen", ",", "die", "Hey\u00b7den", "be\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wird ihnen man Venus und Bacchus nur schencken,", "tokens": ["Wird", "ih\u00b7nen", "man", "Ve\u00b7nus", "und", "Bac\u00b7chus", "nur", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "NN", "KON", "NE", "ADV", "VVINF", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie werden der andren nicht leichte gedencken.", "tokens": ["Sie", "wer\u00b7den", "der", "an\u00b7dren", "nicht", "leich\u00b7te", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}