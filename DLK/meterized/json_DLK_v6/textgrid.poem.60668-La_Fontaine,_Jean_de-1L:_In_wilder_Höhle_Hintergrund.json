{"textgrid.poem.60668": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: In wilder H\u00f6hle Hintergrund", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In wilder H\u00f6hle Hintergrund", "tokens": ["In", "wil\u00b7der", "H\u00f6h\u00b7le", "Hin\u00b7ter\u00b7grund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Satyr sa\u00df mit Weib und Kindern,", "tokens": ["Ein", "Sa\u00b7tyr", "sa\u00df", "mit", "Weib", "und", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Suppennapf vor Hand und Mund,", "tokens": ["Den", "Sup\u00b7pen\u00b7napf", "vor", "Hand", "und", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um ihres Magens Not zu lindern.", "tokens": ["Um", "ih\u00b7res", "Ma\u00b7gens", "Not", "zu", "lin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie hockten auf geh\u00e4uftem Moos", "tokens": ["Sie", "hock\u00b7ten", "auf", "ge\u00b7h\u00e4uf\u00b7tem", "Moos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00fcnschten keine be\u00dfren Pf\u00fchle.", "tokens": ["Und", "w\u00fcnschten", "kei\u00b7ne", "be\u00df\u00b7ren", "Pf\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr Appetit war gut und gro\u00df", "tokens": ["Ihr", "Ap\u00b7pe\u00b7tit", "war", "gut", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ohne Tisch und Tuch und St\u00fchle.", "tokens": ["Auch", "oh\u00b7ne", "Tisch", "und", "Tuch", "und", "St\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da trat ein Wandersmann herein,", "tokens": ["Da", "trat", "ein", "Wan\u00b7ders\u00b7mann", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchn\u00e4\u00dft von kalten Regenschauern.", "tokens": ["Durc\u00b7hn\u00e4\u00dft", "von", "kal\u00b7ten", "Re\u00b7gen\u00b7schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man lud ihn gleich zum Essen ein", "tokens": ["Man", "lud", "ihn", "gleich", "zum", "Es\u00b7sen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bat ihn, sich aufs Moos zu kauern.", "tokens": ["Und", "bat", "ihn", ",", "sich", "aufs", "Moos", "zu", "kau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Gast nahm gern die Sch\u00fcssel an,", "tokens": ["Der", "Gast", "nahm", "gern", "die", "Sch\u00fcs\u00b7sel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erfreut ob solch erw\u00fcnschter Spende.", "tokens": ["Er\u00b7freut", "ob", "solch", "er\u00b7w\u00fcnschter", "Spen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit Atempusten er begann", "tokens": ["Mit", "A\u00b7tem\u00b7pus\u00b7ten", "er", "be\u00b7gann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zun\u00e4chst zu w\u00e4rmen seine H\u00e4nde.", "tokens": ["Zu\u00b7n\u00e4chst", "zu", "w\u00e4r\u00b7men", "sei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann blies er auf die Suppe hin,", "tokens": ["Dann", "blies", "er", "auf", "die", "Sup\u00b7pe", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihm noch allzu hei\u00df erschienen.", "tokens": ["Die", "ihm", "noch", "all\u00b7zu", "hei\u00df", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Satyr schien's ein Widersinn,", "tokens": ["Dem", "Sa\u00b7tyr", "schien's", "ein", "Wi\u00b7der\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sprach: \u00bbWozu soll beides dienen?\u00ab", "tokens": ["Er", "sprach", ":", "\u00bb", "Wo\u00b7zu", "soll", "bei\u00b7des", "die\u00b7nen", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWAV", "VMFIN", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bberstmals erw\u00e4rmt's die kalte Haut,", "tokens": ["\u00bb", "erst\u00b7mals", "er\u00b7w\u00e4rmt's", "die", "kal\u00b7te", "Haut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Zum andern k\u00fchlt's die warme Speise.\u00ab", "tokens": ["Zum", "an\u00b7dern", "k\u00fchlt's", "die", "war\u00b7me", "Spei\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da hat's den wilden Mann gegraut:", "tokens": ["Da", "hat's", "den", "wil\u00b7den", "Mann", "ge\u00b7graut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbsteh auf und mach dich auf die Reise.", "tokens": ["\u00bb", "steh", "auf", "und", "mach", "dich", "auf", "die", "Rei\u00b7se", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bewahr mich Zeus vor solchem Bund!", "tokens": ["Be\u00b7wahr", "mich", "Zeus", "vor", "sol\u00b7chem", "Bund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich schauert \u00fcber alle Ma\u00dfen.", "tokens": ["Mich", "schau\u00b7ert", "\u00fc\u00b7ber", "al\u00b7le", "Ma\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fort mit den Menschen, deren Mund", "tokens": ["Fort", "mit", "den", "Men\u00b7schen", ",", "de\u00b7ren", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So warm wie kalt versteht zu blasen.\u00ab", "tokens": ["So", "warm", "wie", "kalt", "ver\u00b7steht", "zu", "bla\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "VVFIN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}