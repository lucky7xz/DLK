{"textgrid.poem.59310": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Freiwerber", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchmorgens durch die Winde k\u00fchl", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", "durch", "die", "Win\u00b7de", "k\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zwei Ritter hergeritten sind,", "tokens": ["Zwei", "Rit\u00b7ter", "her\u00b7ge\u00b7rit\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Garten klingt ihr Saitenspiel,", "tokens": ["Im", "Gar\u00b7ten", "klingt", "ihr", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wach auf, wach auf, mein sch\u00f6nes Kind!", "tokens": ["Wach", "auf", ",", "wach", "auf", ",", "mein", "sch\u00f6\u00b7nes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ringsum viel Schl\u00f6sser schimmernd stehn,", "tokens": ["Ring\u00b7sum", "viel", "Schl\u00f6s\u00b7ser", "schim\u00b7mernd", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So silbern geht der Str\u00f6me Lauf,", "tokens": ["So", "sil\u00b7bern", "geht", "der", "Str\u00f6\u00b7me", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch, weit rings Lerchenlieder wehn,", "tokens": ["Hoch", ",", "weit", "rings", "Ler\u00b7chen\u00b7lie\u00b7der", "wehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlie\u00df Fenster, Herz und \u00c4uglein auf!", "tokens": ["Schlie\u00df", "Fens\u00b7ter", ",", "Herz", "und", "\u00c4ug\u00b7lein", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So wie du bist, verschlafen hei\u00df,", "tokens": ["So", "wie", "du", "bist", ",", "ver\u00b7schla\u00b7fen", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df allen Putz und Zier zu Haus,", "tokens": ["La\u00df", "al\u00b7len", "Putz", "und", "Zier", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tritt nur herf\u00fcr im Hemdlein wei\u00df,", "tokens": ["Tritt", "nur", "her\u00b7f\u00fcr", "im", "Hemd\u00b7lein", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Siehst so gar sch\u00f6n verliebet aus.", "tokens": ["Siehst", "so", "gar", "sch\u00f6n", "ver\u00b7lie\u00b7bet", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ich hab einen Fremden wohl bei mir,", "tokens": ["Ich", "hab", "ei\u00b7nen", "Frem\u00b7den", "wohl", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der lauert unten auf der Wacht,", "tokens": ["Der", "lau\u00b7ert", "un\u00b7ten", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der bittet sch\u00f6n dich um Quartier,", "tokens": ["Der", "bit\u00b7tet", "sch\u00f6n", "dich", "um", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verschlafnes Kind, nimm dich in acht!", "tokens": ["Ver\u00b7schlaf\u00b7nes", "Kind", ",", "nimm", "dich", "in", "acht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}