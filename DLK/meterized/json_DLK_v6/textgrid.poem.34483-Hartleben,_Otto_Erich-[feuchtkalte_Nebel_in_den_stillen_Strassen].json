{"textgrid.poem.34483": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[feuchtkalte Nebel in den stillen Strassen]", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feuchtkalte Nebel in den stillen Strassen \u2013", "tokens": ["Feucht\u00b7kal\u00b7te", "Ne\u00b7bel", "in", "den", "stil\u00b7len", "Stras\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fr\u00fchmorgenschein \u2013 ob wir uns ganz vergassen?", "tokens": ["Fr\u00fch\u00b7mor\u00b7gen\u00b7schein", "\u2013", "ob", "wir", "uns", "ganz", "ver\u00b7gas\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Ich fahre lautlos \u00fcbers weiche Pflaster \u2013", "tokens": ["Ich", "fah\u00b7re", "laut\u00b7los", "\u00fc\u00b7bers", "wei\u00b7che", "Pflas\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "verfr\u00fchte Arbeit und versp\u00e4tet Laster.", "tokens": ["ver\u00b7fr\u00fch\u00b7te", "Ar\u00b7beit", "und", "ver\u00b7sp\u00e4\u00b7tet", "Las\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "An jener Th\u00fcr vor\u00fcber? \u2013 Fass dich: hier,", "tokens": ["An", "je\u00b7ner", "Th\u00fcr", "vor\u00b7\u00fc\u00b7ber", "?", "\u2013", "Fass", "dich", ":", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$.", "$(", "VVIMP", "PPER", "$.", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hier links, da schlug es ehmals. Pocht es dir?", "tokens": ["hier", "links", ",", "da", "schlug", "es", "eh\u00b7mals", ".", "Pocht", "es", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "NN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zertreten und zerfahren ist der Schnee:", "tokens": ["Zer\u00b7tre\u00b7ten", "und", "zer\u00b7fah\u00b7ren", "ist", "der", "Schnee", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schmutz \u00fcberdeckt die zugefrorne Spree.", "tokens": ["Schmutz", "\u00fc\u00b7berd\u00b7eckt", "die", "zu\u00b7ge\u00b7fror\u00b7ne", "Spree", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dort schl\u00e4ft die Jungfrau. Ihre Stirn ist rein.", "tokens": ["Dort", "schl\u00e4ft", "die", "Jung\u00b7frau", ".", "Ih\u00b7re", "Stirn", "ist", "rein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du sollst auf diesen Strassen th\u00e4tig sein.", "tokens": ["Du", "sollst", "auf", "die\u00b7sen", "Stras\u00b7sen", "th\u00e4\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vorbei! Vorbei! Schon wird der Morgen laut.", "tokens": ["Vor\u00b7bei", "!", "Vor\u00b7bei", "!", "Schon", "wird", "der", "Mor\u00b7gen", "laut", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Feuchtkalte Nebel \u2013 doch es thaut \u2013 es thaut!", "tokens": ["Feucht\u00b7kal\u00b7te", "Ne\u00b7bel", "\u2013", "doch", "es", "thaut", "\u2013", "es", "thaut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KON", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}