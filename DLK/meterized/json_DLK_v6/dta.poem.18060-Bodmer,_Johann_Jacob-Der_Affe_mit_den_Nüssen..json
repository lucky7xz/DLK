{"dta.poem.18060": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Der Affe mit den N\u00fcssen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Affe kam auf einen Platz gerannt,", "tokens": ["Ein", "Af\u00b7fe", "kam", "auf", "ei\u00b7nen", "Platz", "ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo er viel guter N\u00fcsse fand.", "tokens": ["Wo", "er", "viel", "gu\u00b7ter", "N\u00fcs\u00b7se", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von diesen \u00e4\u00df er nur zu gern;", "tokens": ["Von", "die\u00b7sen", "\u00e4\u00df", "er", "nur", "zu", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man hatte ihm gesagt, der Kern", "tokens": ["Man", "hat\u00b7te", "ihm", "ge\u00b7sagt", ",", "der", "Kern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4r angenehm und k\u00f6stlich gut.", "tokens": ["W\u00e4r", "an\u00b7ge\u00b7nehm", "und", "k\u00f6st\u00b7lich", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verwirrt ward ihm sein dummer Muth", "tokens": ["Ver\u00b7wirrt", "ward", "ihm", "sein", "dum\u00b7mer", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als er die Bitterkeit entdeckt,", "tokens": ["Als", "er", "die", "Bit\u00b7ter\u00b7keit", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die in den gr\u00fcnen H\u00e4uten steckt: ", "tokens": ["Die", "in", "den", "gr\u00fc\u00b7nen", "H\u00e4u\u00b7ten", "steckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch mehr, als er die Schalen nagt.", "tokens": ["Noch", "mehr", ",", "als", "er", "die", "Scha\u00b7len", "nagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von N\u00fcssen ist mir viel gesagt,", "tokens": ["Von", "N\u00fcs\u00b7sen", "ist", "mir", "viel", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sprach er, izt ist mir anders kund;", "tokens": ["Sprach", "er", ",", "izt", "ist", "mir", "an\u00b7ders", "kund", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie haben mir verderbt den Mund. ", "tokens": ["Sie", "ha\u00b7ben", "mir", "ver\u00b7derbt", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er warff die Nu\u00df ver\u00e4chtlich auf die Erden,", "tokens": ["Er", "warff", "die", "Nu\u00df", "ver\u00b7\u00e4cht\u00b7lich", "auf", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von welcher ihm der Kern nicht mogte werden.", "tokens": ["Von", "wel\u00b7cher", "ihm", "der", "Kern", "nicht", "mog\u00b7te", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}