{"textgrid.poem.32377": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "55. Auf den Tod eines Affen", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt er nun, der kleine, liebe Pavian,", "tokens": ["Hier", "liegt", "er", "nun", ",", "der", "klei\u00b7ne", ",", "lie\u00b7be", "Pa\u00b7vi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der uns so manches nachgetan!", "tokens": ["Der", "uns", "so", "man\u00b7ches", "nach\u00b7ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wette, was er itzt getan,", "tokens": ["Ich", "wet\u00b7te", ",", "was", "er", "itzt", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tun wir ihm alle nach, dem lieben Pavian.", "tokens": ["Tun", "wir", "ihm", "al\u00b7le", "nach", ",", "dem", "lie\u00b7ben", "Pa\u00b7vi\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIS", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}