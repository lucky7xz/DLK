{"textgrid.poem.27591": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df, der liebe Flachs ist gar ein n\u00fctzes wesen;", "tokens": ["Ge\u00b7wi\u00df", ",", "der", "lie\u00b7be", "Flachs", "ist", "gar", "ein", "n\u00fct\u00b7zes", "we\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, der es wo nicht glaubt, mag diese Reime lesen:", "tokens": ["Der", ",", "der", "es", "wo", "nicht", "glaubt", ",", "mag", "die\u00b7se", "Rei\u00b7me", "le\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "PWAV", "PTKNEG", "VVFIN", "$,", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein M\u00e4gdlein gieng zu Stuhl und th\u00e4t, ich wei\u00df nicht was,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "gieng", "zu", "Stuhl", "und", "th\u00e4t", ",", "ich", "wei\u00df", "nicht", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da, war das Hembd ihr gut, sonst w\u00e4r sie noch wol na\u00df.", "tokens": ["Da", ",", "war", "das", "Hembd", "ihr", "gut", ",", "sonst", "w\u00e4r", "sie", "noch", "wol", "na\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}