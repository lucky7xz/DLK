{"textgrid.poem.66351": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Ehelicher Gutermorgen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guten Morgen!", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Gro\u00df' und kleine Sorgen,", "tokens": ["Gro\u00df'", "und", "klei\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weibchen, theilen wir,", "tokens": ["Weib\u00b7chen", ",", "thei\u00b7len", "wir", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Du, die mir im Leben", "tokens": ["Du", ",", "die", "mir", "im", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Gott zum Trost gegeben:", "tokens": ["Gott", "zum", "Trost", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "O wie theuer bist du mir!", "tokens": ["O", "wie", "theu\u00b7er", "bist", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Guten Morgen!", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hehr und unverborgen", "tokens": ["Hehr", "und", "un\u00b7ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gl\u00e4nzt das Morgenlicht.", "tokens": ["Gl\u00e4nzt", "das", "Mor\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und das Gold der Sonne,", "tokens": ["Und", "das", "Gold", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Weibchen, welche Wonne!", "tokens": ["Weib\u00b7chen", ",", "wel\u00b7che", "Won\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Spielt auf deinem Angesicht.", "tokens": ["Spielt", "auf", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O, der Freude!", "tokens": ["O", ",", "der", "Freu\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hier an meiner Seite", "tokens": ["Hier", "an", "mei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sitzt das holde Kind.", "tokens": ["Sitzt", "das", "hol\u00b7de", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ihre Finger schweben", "tokens": ["Ih\u00b7re", "Fin\u00b7ger", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ueber Goldgeweben,", "tokens": ["Ue\u00b7ber", "Gold\u00b7ge\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie im Bl\u00fcthenbusch der Wind.", "tokens": ["Wie", "im", "Bl\u00fc\u00b7then\u00b7busch", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freudenz\u00e4hren", "tokens": ["Freu\u00b7den\u00b7z\u00e4h\u00b7ren"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Flie\u00dfen dir zu Ehren,", "tokens": ["Flie\u00b7\u00dfen", "dir", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der mein Weibchen schuf.", "tokens": ["Der", "mein", "Weib\u00b7chen", "schuf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Leicht ist mir der Tage", "tokens": ["Leicht", "ist", "mir", "der", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Zugeme\u00dfne Plage", "tokens": ["Zu\u00b7ge\u00b7me\u00df\u00b7ne", "Pla\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und ein Spiel ist mein Beruf.", "tokens": ["Und", "ein", "Spiel", "ist", "mein", "Be\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Guten Morgen!", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Alle unsre Sorgen,", "tokens": ["Al\u00b7le", "uns\u00b7re", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weibchen, theilen wir.", "tokens": ["Weib\u00b7chen", ",", "thei\u00b7len", "wir", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gerne will ich leben,", "tokens": ["Ger\u00b7ne", "will", "ich", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gern in Eden schweben;", "tokens": ["Gern", "in", "E\u00b7den", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Aber, Engel, nur mit dir.", "tokens": ["A\u00b7ber", ",", "En\u00b7gel", ",", "nur", "mit", "dir", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}