{"textgrid.poem.38948": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: O weh! o weh!", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weh! o weh!", "tokens": ["O", "weh", "!", "o", "weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "FM", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie mir das durch die ganze Seele rei\u00dft!", "tokens": ["Wie", "mir", "das", "durch", "die", "gan\u00b7ze", "See\u00b7le", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In's Henkers Nahmen, ich bin keine Fl\u00f6te!", "tokens": ["In's", "Hen\u00b7kers", "Nah\u00b7men", ",", "ich", "bin", "kei\u00b7ne", "Fl\u00f6\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie kann man mich so qu\u00e4len,", "tokens": ["Wie", "kann", "man", "mich", "so", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Alle meine T\u00f6ne unterdr\u00fccken,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "T\u00f6\u00b7ne", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und kneifen und schaben und kratzen,", "tokens": ["Und", "knei\u00b7fen", "und", "scha\u00b7ben", "und", "krat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Bis ein fremdes quinkelirendes Geschrey herausschnarrt?", "tokens": ["Bis", "ein", "frem\u00b7des", "quin\u00b7ke\u00b7li\u00b7ren\u00b7des", "Ge\u00b7schrey", "her\u00b7aus\u00b7schnarrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Ich kenne meine eigene Stimme nicht wieder,", "tokens": ["Ich", "ken\u00b7ne", "mei\u00b7ne", "ei\u00b7ge\u00b7ne", "Stim\u00b7me", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ich erschrecke vor mir selber", "tokens": ["Ich", "er\u00b7schre\u00b7cke", "vor", "mir", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In diesen unwohlth\u00e4tigen Passagen.", "tokens": ["In", "die\u00b7sen", "un\u00b7wohlt\u00b7h\u00e4\u00b7ti\u00b7gen", "Pas\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ei! ei! da\u00df ein andrer Geist", "tokens": ["Ei", "!", "ei", "!", "da\u00df", "ein", "an\u00b7drer", "Geist"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Doch auch einmal so mit dir umspringen m\u00f6chte,", "tokens": ["Doch", "auch", "ein\u00b7mal", "so", "mit", "dir", "um\u00b7sprin\u00b7gen", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Damit du alle Menschlichkeit verl\u00e4ugnen m\u00fc\u00dftest", "tokens": ["Da\u00b7mit", "du", "al\u00b7le", "Menschlich\u00b7keit", "ver\u00b7l\u00e4ug\u00b7nen", "m\u00fc\u00df\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und dich dem Thiere gleich gebehrden.", "tokens": ["Und", "dich", "dem", "Thie\u00b7re", "gleich", "ge\u00b7behr\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Innerlich schmerzt mich die Musik", "tokens": ["In\u00b7ner\u00b7lich", "schmerzt", "mich", "die", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Die da unten wohnt und von wilden Kl\u00e4ngen vernichtet wird,", "tokens": ["Die", "da", "un\u00b7ten", "wohnt", "und", "von", "wil\u00b7den", "Kl\u00e4n\u00b7gen", "ver\u00b7nich\u00b7tet", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "KON", "APPR", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "--+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Eine Kolik \u00e4ngstigt mich durch und durch,", "tokens": ["Ei\u00b7ne", "Ko\u00b7lik", "\u00e4ngs\u00b7tigt", "mich", "durch", "und", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "KON", "APPR", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Der Resonanzboden wird von Gicht befallen,", "tokens": ["Der", "Re\u00b7so\u00b7nanz\u00b7bo\u00b7den", "wird", "von", "Gicht", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Der Steg winselt und wimmert.", "tokens": ["Der", "Steg", "win\u00b7selt", "und", "wim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Wie ein Clarinett soll ich mich gebehrden,", "tokens": ["Wie", "ein", "Cla\u00b7ri\u00b7nett", "soll", "ich", "mich", "ge\u00b7behr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.21": {"text": "Jetzt dem Basson verglichen werden,", "tokens": ["Jetzt", "dem", "Bas\u00b7son", "ver\u00b7gli\u00b7chen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Er rei\u00dft mir noch die melodische Zunge aus,", "tokens": ["Er", "rei\u00dft", "mir", "noch", "die", "me\u00b7lo\u00b7di\u00b7sche", "Zun\u00b7ge", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Lange werd' ich liegen m\u00fcssen und mich besinnen,", "tokens": ["Lan\u00b7ge", "werd'", "ich", "lie\u00b7gen", "m\u00fcs\u00b7sen", "und", "mich", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VMFIN", "KON", "PPER", "VVINF", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Eh' ich diesen Schrecken verwinden kann.", "tokens": ["Eh'", "ich", "die\u00b7sen", "Schre\u00b7cken", "ver\u00b7win\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Ei so kneif, du kneifender Satan!", "tokens": ["Ei", "so", "kneif", ",", "du", "knei\u00b7fen\u00b7der", "Sa\u00b7tan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Es wird ihm selber sauer,", "tokens": ["Es", "wird", "ihm", "sel\u00b7ber", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Es neigt zu Ende mit der verfluchten Sonate,", "tokens": ["Es", "neigt", "zu", "En\u00b7de", "mit", "der", "ver\u00b7fluch\u00b7ten", "So\u00b7na\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Ach weh! o weh'! o! welche Gef\u00fchle!", "tokens": ["Ach", "weh", "!", "o", "weh'", "!", "o", "!", "wel\u00b7che", "Ge\u00b7f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "FM", "PTKVZ", "$.", "FM", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Die Ribben, die Seiten, der R\u00fccken,", "tokens": ["Die", "Rib\u00b7ben", ",", "die", "Sei\u00b7ten", ",", "der", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "Alles wie zerschlagen! \u2013\u2013", "tokens": ["Al\u00b7les", "wie", "zer\u00b7schla\u00b7gen", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "KOKOM", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}