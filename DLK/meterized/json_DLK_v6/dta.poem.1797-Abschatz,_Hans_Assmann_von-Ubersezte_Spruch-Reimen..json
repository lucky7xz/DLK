{"dta.poem.1797": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ohne Widerkunfft der Strom das Wasser f\u00fchrt/", "tokens": ["Wie", "oh\u00b7ne", "Wi\u00b7der\u00b7kunfft", "der", "Strom", "das", "Was\u00b7ser", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So eilt der schwache Mensch/ der Jahr auff Jahr", "tokens": ["So", "eilt", "der", "schwa\u00b7che", "Mensch", "/", "der", "Jahr", "auff", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "liert/", "tokens": ["liert", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dem lezten Ziele zu: die schleichende Gewalt", "tokens": ["Dem", "lez\u00b7ten", "Zie\u00b7le", "zu", ":", "die", "schlei\u00b7chen\u00b7de", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Schicksals w\u00e4lzt sein Rad ohn allen Auffenthalt.", "tokens": ["Des", "Schick\u00b7sals", "w\u00e4lzt", "sein", "Rad", "ohn", "al\u00b7len", "Auf\u00b7fent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}