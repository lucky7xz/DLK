{"textgrid.poem.66313": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bleibe vor dem Sarge stehen,", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleibe vor dem Sarge stehen,", "tokens": ["Blei\u00b7be", "vor", "dem", "Sar\u00b7ge", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eitles, jugendliches Chor!", "tokens": ["Eit\u00b7les", ",", "ju\u00b7gend\u00b7li\u00b7ches", "Chor", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fei'rlich steige zu den H\u00f6hen", "tokens": ["Fei'\u00b7rlich", "stei\u00b7ge", "zu", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heut dein Leichenlied empor.", "tokens": ["Heut", "dein", "Lei\u00b7chen\u00b7lied", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese schwarze Bahre beut", "tokens": ["Die\u00b7se", "schwar\u00b7ze", "Bah\u00b7re", "beut"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir ein Bild der Eitelkeit;", "tokens": ["Dir", "ein", "Bild", "der", "Ei\u00b7tel\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Drum so sing mit vollen Ch\u00f6ren,", "tokens": ["Drum", "so", "sing", "mit", "vol\u00b7len", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es alle Menschen h\u00f6ren.", "tokens": ["Da\u00df", "es", "al\u00b7le", "Men\u00b7schen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00e4nderingen, Klagen, Thr\u00e4nen,", "tokens": ["H\u00e4n\u00b7de\u00b7rin\u00b7gen", ",", "Kla\u00b7gen", ",", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt mit eurem Lied empor.", "tokens": ["Steigt", "mit", "eu\u00b7rem", "Lied", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stumme, bla\u00dfgetraurte Mienen", "tokens": ["Stum\u00b7me", ",", "bla\u00df\u00b7ge\u00b7traur\u00b7te", "Mie\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schauen aus dem Trauerflor.", "tokens": ["Schau\u00b7en", "aus", "dem", "Trau\u00b7er\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welches freche Angesicht,", "tokens": ["Wel\u00b7ches", "fre\u00b7che", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welcher Busen schauert nicht?", "tokens": ["Wel\u00b7cher", "Bu\u00b7sen", "schau\u00b7ert", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wem erbeben nicht die Glieder", "tokens": ["Wem", "er\u00b7be\u00b7ben", "nicht", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bei den Leichen seiner Br\u00fcder?", "tokens": ["Bei", "den", "Lei\u00b7chen", "sei\u00b7ner", "Br\u00fc\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hebt, mit Thr\u00e4nen in den Blicken,", "tokens": ["Hebt", ",", "mit", "Thr\u00e4\u00b7nen", "in", "den", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4ger, diesen Todten auf;", "tokens": ["Tr\u00e4\u00b7ger", ",", "die\u00b7sen", "Tod\u00b7ten", "auf", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tragt ihn nur mit starkem R\u00fccken", "tokens": ["Tragt", "ihn", "nur", "mit", "star\u00b7kem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Todtenberg hinauf.", "tokens": ["Auf", "den", "Tod\u00b7ten\u00b7berg", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denkt, wie unter dieser Last", "tokens": ["Denkt", ",", "wie", "un\u00b7ter", "die\u00b7ser", "Last"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch ein Todesschrecken fa\u00dft:", "tokens": ["Euch", "ein", "To\u00b7des\u00b7schre\u00b7cken", "fa\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, wann wird die Stunde schlagen,", "tokens": ["Ach", ",", "wann", "wird", "die", "Stun\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da uns andre Schultern tragen?", "tokens": ["Da", "uns", "and\u00b7re", "Schul\u00b7tern", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau, mit einem edlen Schauer,", "tokens": ["Schau", ",", "mit", "ei\u00b7nem", "ed\u00b7len", "Schau\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockre Erde, schaue du", "tokens": ["Lock\u00b7re", "Er\u00b7de", ",", "schau\u00b7e", "du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Klagen, dieser Trauer,", "tokens": ["Die\u00b7sen", "Kla\u00b7gen", ",", "die\u00b7ser", "Trau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesem Leichenzuge zu.", "tokens": ["Die\u00b7sem", "Lei\u00b7chen\u00b7zu\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch zu deinem dicken Ohr", "tokens": ["Auch", "zu", "dei\u00b7nem", "di\u00b7cken", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Steiget der Gesang empor,", "tokens": ["Stei\u00b7get", "der", "Ge\u00b7sang", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Diese Stimme voll Verderben:", "tokens": ["Die\u00b7se", "Stim\u00b7me", "voll", "Ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Menschen m\u00fcssen sterben!", "tokens": ["Al\u00b7le", "Men\u00b7schen", "m\u00fcs\u00b7sen", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Boy und Flor und Trauerkleider,", "tokens": ["Boy", "und", "Flor", "und", "Trau\u00b7er\u00b7klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herzen von Beklemmung schwer,", "tokens": ["Her\u00b7zen", "von", "Be\u00b7klem\u00b7mung", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Langsam wandelnde Begleiter", "tokens": ["Lang\u00b7sam", "wan\u00b7deln\u00b7de", "Be\u00b7glei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Hinter diesem Sarge her;", "tokens": ["Hin\u00b7ter", "die\u00b7sem", "Sar\u00b7ge", "her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blasse Schwermuth, stumme Pein,", "tokens": ["Blas\u00b7se", "Schwer\u00b7muth", ",", "stum\u00b7me", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Klagen, die sich m\u00fcde schrein,", "tokens": ["Kla\u00b7gen", ",", "die", "sich", "m\u00fc\u00b7de", "schrein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Freunde, die mit schwachen F\u00fc\u00dfen", "tokens": ["Freun\u00b7de", ",", "die", "mit", "schwa\u00b7chen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihrem Todten folgen m\u00fcssen.", "tokens": ["Ih\u00b7rem", "Tod\u00b7ten", "fol\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freunde! l\u00f6scht der Tod den Schimmer", "tokens": ["Freun\u00b7de", "!", "l\u00f6scht", "der", "Tod", "den", "Schim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser feuchten Blicke aus:", "tokens": ["Die\u00b7ser", "feuch\u00b7ten", "Bli\u00b7cke", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O! so traget meine Tr\u00fcmmer", "tokens": ["O", "!", "so", "tra\u00b7get", "mei\u00b7ne", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Pracht zum Thor hinaus.", "tokens": ["Oh\u00b7ne", "Pracht", "zum", "Thor", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ziehet ungesehn vorbei;", "tokens": ["Zie\u00b7het", "un\u00b7ge\u00b7sehn", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Christen ist es einerlei,", "tokens": ["Chris\u00b7ten", "ist", "es", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob sie schwarzbeh\u00e4ngte Wagen", "tokens": ["Ob", "sie", "schwarz\u00b7be\u00b7h\u00e4ng\u00b7te", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder Bettlerschultern tragen.", "tokens": ["O\u00b7der", "Bett\u00b7ler\u00b7schul\u00b7tern", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}