{"dta.poem.1": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "mein Trieb, der waget warlich", "tokens": ["mein", "Trieb", ",", "der", "wa\u00b7get", "war\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ich mich untersteh der Andacht Saiten-", "tokens": ["Da", "ich", "mich", "un\u00b7ter\u00b7steh", "der", "An\u00b7dacht", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das meine Einfalt r\u00fchrt, mit dem verstimm-", "tokens": ["Das", "mei\u00b7ne", "Ein\u00b7falt", "r\u00fchrt", ",", "mit", "dem", "ver\u00b7stim\u00b7m"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr dein bemerkend Ohr, in matten Thon", "tokens": ["F\u00fcr", "dein", "be\u00b7mer\u00b7kend", "Ohr", ",", "in", "mat\u00b7ten", "Thon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Schuldigkeit befahl, die arme Sch\u00fcch-", "tokens": ["Die", "Schul\u00b7dig\u00b7keit", "be\u00b7fahl", ",", "die", "ar\u00b7me", "Sch\u00fcch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die wiederrieth es mir: in diesem Wette", "tokens": ["Die", "wie\u00b7der\u00b7rieth", "es", "mir", ":", "in", "die\u00b7sem", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Frug mein bewegt Gem\u00fcth: Ob ich auch werth", "tokens": ["Frug", "mein", "be\u00b7wegt", "Ge\u00b7m\u00fcth", ":", "Ob", "ich", "auch", "werth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "VVFIN", "NN", "$.", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Lieder Dir zu weihn, die niedrig, kl\u00e4glich", "tokens": ["Die", "Lie\u00b7der", "Dir", "zu", "weihn", ",", "die", "nied\u00b7rig", ",", "kl\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,", "PRELS", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und \u00f6fters knarrend gehn; da s\u00fcsse Melo-", "tokens": ["Und", "\u00f6f\u00b7ters", "knar\u00b7rend", "gehn", ";", "da", "s\u00fcs\u00b7se", "Me\u00b7lo"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVINF", "$.", "KOUS", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Dein Ohr schon l\u00e4ngst vergn\u00fcgt, dein Her-", "tokens": ["Dein", "Ohr", "schon", "l\u00e4ngst", "ver\u00b7gn\u00fcgt", ",", "dein", "Her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da was nicht lieblich schallt, in denen Dich-", "tokens": ["Da", "was", "nicht", "lieb\u00b7lich", "schallt", ",", "in", "de\u00b7nen", "Dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PWS", "PTKNEG", "ADJD", "VVFIN", "$,", "APPR", "PRELS", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Auch Dir, nicht angenehm, nicht reitzend", "tokens": ["Auch", "Dir", ",", "nicht", "an\u00b7ge\u00b7nehm", ",", "nicht", "reit\u00b7zend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die Sch\u00fcchternheit die sprach: zur\u00fck mit", "tokens": ["Die", "Sch\u00fcch\u00b7tern\u00b7heit", "die", "sprach", ":", "zu\u00b7r\u00fck", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "ADV", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der matt und elend geht: der Gottheit Lob-", "tokens": ["Der", "matt", "und", "e\u00b7lend", "geht", ":", "der", "Got\u00b7theit", "Lob"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Mu\u00df, wenn er soll gefalln, das Herze feu-", "tokens": ["Mu\u00df", ",", "wenn", "er", "soll", "ge\u00b7falln", ",", "das", "Her\u00b7ze", "feu"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,", "PDS", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und im entflammten Trieb zum Siz des", "tokens": ["Und", "im", "ent\u00b7flamm\u00b7ten", "Trieb", "zum", "Siz", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Den Liedern fehlt das Feur, das in die Her-", "tokens": ["Den", "Lie\u00b7dern", "fehlt", "das", "Feur", ",", "das", "in", "die", "Her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ob sie aus Andacht gleich, zu ", "tokens": ["Ob", "sie", "aus", "An\u00b7dacht", "gleich", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "$,", "APPR"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Sie r\u00fchren kein Gem\u00fcth, da\u00df jemahls an-", "tokens": ["Sie", "r\u00fch\u00b7ren", "kein", "Ge\u00b7m\u00fcth", ",", "da\u00df", "je\u00b7mahls", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "KOUS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wie man die Gottheit recht, in Lobges\u00e4n-", "tokens": ["Wie", "man", "die", "Got\u00b7theit", "recht", ",", "in", "Lob\u00b7ge\u00b7s\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "ADJD", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Sie wandte ferner ein: bedenkest du denn", "tokens": ["Sie", "wand\u00b7te", "fer\u00b7ner", "ein", ":", "be\u00b7den\u00b7kest", "du", "denn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$.", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Wie manch erhabnes Lied und herrliches Ge-", "tokens": ["Wie", "manch", "er\u00b7hab\u00b7nes", "Lied", "und", "herr\u00b7li\u00b7ches", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Von erster Jugend an in dessen Ohr erklun-", "tokens": ["Von", "ers\u00b7ter", "Ju\u00b7gend", "an", "in", "des\u00b7sen", "Ohr", "er\u00b7klun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "APPR", "PRELAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da jener Elbe-Schwan der Gottheit Ruhm", "tokens": ["Da", "je\u00b7ner", "El\u00b7be\u00b7Schwan", "der", "Got\u00b7theit", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NE", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da Brokkes Saitenspiel, so oft das Herze", "tokens": ["Da", "Brok\u00b7kes", "Sai\u00b7ten\u00b7spiel", ",", "so", "oft", "das", "Her\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "$,", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Das jederman gef\u00e4llt, in heiligs Feuer", "tokens": ["Das", "je\u00b7der\u00b7man", "ge\u00b7f\u00e4llt", ",", "in", "hei\u00b7ligs", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PIS", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wie kan beim hellen Thon, ein heisch und", "tokens": ["Wie", "kan", "beim", "hel\u00b7len", "Thon", ",", "ein", "heisch", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPRART", "ADJA", "NN", "$,", "ART", "ADJD", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Bei einem Schwang-Gesang ein schnatternd", "tokens": ["Bei", "ei\u00b7nem", "Schwang\u00b7Ge\u00b7sang", "ein", "schnat\u00b7ternd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "So sprach ", "tokens": ["So", "sprach"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Da ich mir vorgesezt, mit deines Nahmens", "tokens": ["Da", "ich", "mir", "vor\u00b7ge\u00b7sezt", ",", "mit", "dei\u00b7nes", "Nah\u00b7mens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Aus Ehrfurcht blos erwekt, mein Liederbuch", "tokens": ["Aus", "Ehr\u00b7furcht", "blos", "er\u00b7wekt", ",", "mein", "Lie\u00b7der\u00b7buch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Um Dir dadurch den Trieb des Herzens ab-", "tokens": ["Um", "Dir", "da\u00b7durch", "den", "Trieb", "des", "Her\u00b7zens", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PAV", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Des Herzens Trieb befahl und zeigte deine", "tokens": ["Des", "Her\u00b7zens", "Trieb", "be\u00b7fahl", "und", "zeig\u00b7te", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Die Dankbarkeit wies mir die Summe mei-", "tokens": ["Die", "Dank\u00b7bar\u00b7keit", "wies", "mir", "die", "Sum\u00b7me", "mei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Und darum wage ichs den Abtrag meiner", "tokens": ["Und", "da\u00b7rum", "wa\u00b7ge", "ichs", "den", "Ab\u00b7trag", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PIS", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "F\u00fcr die geno\u00dfne Gunst, im Wollen zu ent-", "tokens": ["F\u00fcr", "die", "ge\u00b7no\u00df\u00b7ne", "Gunst", ",", "im", "Wol\u00b7len", "zu", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Du kennest meinen Trieb auch in den nie-", "tokens": ["Du", "ken\u00b7nest", "mei\u00b7nen", "Trieb", "auch", "in", "den", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Den hocherhabnen ", "tokens": ["Den", "ho\u00b7cher\u00b7hab\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Du billigest den Zwek, den HErrn der Herr-", "tokens": ["Du", "bil\u00b7li\u00b7gest", "den", "Zwek", ",", "den", "Herrn", "der", "Herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Den die Natur und Schrift uns zeiget, aus-", "tokens": ["Den", "die", "Na\u00b7tur", "und", "Schrift", "uns", "zei\u00b7get", ",", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ART", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Nim was die Redlichkeit, in reiner Neigung", "tokens": ["Nim", "was", "die", "Red\u00b7lich\u00b7keit", ",", "in", "rei\u00b7ner", "Nei\u00b7gung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PWS", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.42": {"text": "Bis ihr ein be\u00dfres Lied in h\u00f6hern Thon ge-", "tokens": ["Bis", "ihr", "ein", "be\u00df\u00b7res", "Lied", "in", "h\u00f6\u00b7hern", "Thon", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Nim es zum Unterpfand, da\u00df der der die-", "tokens": ["Nim", "es", "zum", "Un\u00b7ter\u00b7pfand", ",", "da\u00df", "der", "der", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPRART", "NN", "$,", "KOUS", "ART", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "In wahrer Hochachtung stets dein Verehrer", "tokens": ["In", "wah\u00b7rer", "Hoch\u00b7ach\u00b7tung", "stets", "dein", "Ver\u00b7eh\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Mein Blat das redet nichts, zu deines Nah-", "tokens": ["Mein", "Blat", "das", "re\u00b7det", "nichts", ",", "zu", "dei\u00b7nes", "Nah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "PIS", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Du brauchst mein Loblied nicht, weil Fa-", "tokens": ["Du", "brauchst", "mein", "Lob\u00b7lied", "nicht", ",", "weil", "Fa"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "KOUS", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Schon dein Verdienst erkannt; des Him-", "tokens": ["Schon", "dein", "Ver\u00b7dienst", "er\u00b7kannt", ";", "des", "Him"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$.", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Sei Deine Sonn und Schild, dich bl\u00fchend", "tokens": ["Sei", "Dei\u00b7ne", "Sonn", "und", "Schild", ",", "dich", "bl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}