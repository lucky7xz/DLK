{"dta.poem.6889": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df unsere Natur so zugerichtet sey,", "tokens": ["Da\u00df", "un\u00b7se\u00b7re", "Na\u00b7tur", "so", "zu\u00b7ge\u00b7rich\u00b7tet", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df vom Geist die Pein den Gliedern zuzuschreiben,", "tokens": ["Und", "da\u00df", "vom", "Geist", "die", "Pein", "den", "Glie\u00b7dern", "zu\u00b7zu\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ins Gehirn die Nerven treiben;", "tokens": ["Die", "ins", "Ge\u00b7hirn", "die", "Ner\u00b7ven", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem stimmet diese Probe bey.", "tokens": ["Dem", "stim\u00b7met", "die\u00b7se", "Pro\u00b7be", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach einer blutgen Schlacht, woselbst Bellonen Wuht,", "tokens": ["Nach", "ei\u00b7ner", "blut\u00b7gen", "Schlacht", ",", "wo\u00b7selbst", "Bel\u00b7lo\u00b7nen", "Wuht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch die zerschmetternd-wilde Blitze", "tokens": ["Durch", "die", "zer\u00b7schmet\u00b7tern\u00b7dwil\u00b7de", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der donnernden Gesch\u00fctze,", "tokens": ["Der", "don\u00b7nern\u00b7den", "Ge\u00b7sch\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So Arm, als Beine, raubt; wenn Helden, deren Muht", "tokens": ["So", "Arm", ",", "als", "Bei\u00b7ne", ",", "raubt", ";", "wenn", "Hel\u00b7den", ",", "de\u00b7ren", "Muht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$.", "KOUS", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Parcen Stahl getrotzt, mit dem betr\u00fcbten Zeichen", "tokens": ["Der", "Par\u00b7cen", "Stahl", "ge\u00b7trotzt", ",", "mit", "dem", "be\u00b7tr\u00fcb\u00b7ten", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von ihrer Tapfferkeit zur\u00fccke kommen,", "tokens": ["Von", "ih\u00b7rer", "Tapf\u00b7fer\u00b7keit", "zu\u00b7r\u00fc\u00b7cke", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und denen Esculap das Leben,", "tokens": ["Und", "de\u00b7nen", "E\u00b7scu\u00b7lap", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das durch die Wunden fast geraubt war, wiedergeben,", "tokens": ["Das", "durch", "die", "Wun\u00b7den", "fast", "ge\u00b7raubt", "war", ",", "wie\u00b7der\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wird eine neue Pein von ihnen wahrgenommen:", "tokens": ["Wird", "ei\u00b7ne", "neu\u00b7e", "Pein", "von", "ih\u00b7nen", "wahr\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da vom zerst\u00fcckten Arm und Bein,", "tokens": ["Da", "vom", "zer\u00b7st\u00fcck\u00b7ten", "Arm", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Nerven Rest im Hirn so starck ersch\u00fcttert seyn,", "tokens": ["Der", "Ner\u00b7ven", "Rest", "im", "Hirn", "so", "starck", "er\u00b7sch\u00fct\u00b7tert", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "ADV", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als w\u00e4ren sie noch gantz. Die Seele, die ger\u00fchret", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "noch", "gantz", ".", "Die", "See\u00b7le", ",", "die", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADV", "$.", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Durch solche scharffe Pein,", "tokens": ["Durch", "sol\u00b7che", "scharf\u00b7fe", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Bringt das, was sie versp\u00fcret,", "tokens": ["Bringt", "das", ",", "was", "sie", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "An Orten wieder hin, die w\u00fcrcklich nicht mehr seyn,", "tokens": ["An", "Or\u00b7ten", "wie\u00b7der", "hin", ",", "die", "w\u00fcrck\u00b7lich", "nicht", "mehr", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKVZ", "$,", "PRELS", "ADJD", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zu dem verlohrnen Arm und Bein.", "tokens": ["Zu", "dem", "ver\u00b7lohr\u00b7nen", "Arm", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}