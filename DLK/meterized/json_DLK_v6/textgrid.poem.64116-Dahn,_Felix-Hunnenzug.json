{"textgrid.poem.64116": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Hunnenzug", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber den Tanais, \u00fcber den Ister", "tokens": ["\u00dc\u00b7ber", "den", "Ta\u00b7nais", ",", "\u00fc\u00b7ber", "den", "Is\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Winket der Tod mit der Sense der Pest:", "tokens": ["Win\u00b7ket", "der", "Tod", "mit", "der", "Sen\u00b7se", "der", "Pest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u00bbg\u00fcrte dich, sch\u00fcrze dich, schwarzes Geschwister!", "tokens": ["\u00bb", "g\u00fcr\u00b7te", "dich", ",", "sch\u00fcr\u00b7ze", "dich", ",", "schwar\u00b7zes", "Ge\u00b7schwis\u00b7ter", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ferne nach Gallien ruft uns ein Fest.", "tokens": ["Fer\u00b7ne", "nach", "Gal\u00b7li\u00b7en", "ruft", "uns", "ein", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "H\u00f6re mich, hagerer Bruder du, Hunger!", "tokens": ["H\u00f6\u00b7re", "mich", ",", "ha\u00b7ge\u00b7rer", "Bru\u00b7der", "du", ",", "Hun\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "NE", "$,", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "R\u00fcttle dich, schlafender Geier du, Krieg,", "tokens": ["R\u00fctt\u00b7le", "dich", ",", "schla\u00b7fen\u00b7der", "Gei\u00b7er", "du", ",", "Krieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "PPER", "$,", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Alluners\u00e4ttlicher, immer noch junger,", "tokens": ["Al\u00b7lu\u00b7ner\u00b7s\u00e4tt\u00b7li\u00b7cher", ",", "im\u00b7mer", "noch", "jun\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sch\u00fcttle die blutigen Schwingen und flieg!\u00ab", "tokens": ["Sch\u00fctt\u00b7le", "die", "blu\u00b7ti\u00b7gen", "Schwin\u00b7gen", "und", "flieg", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh da, in Wolken, den V\u00f6lkern ein Grauen,", "tokens": ["Sieh", "da", ",", "in", "Wol\u00b7ken", ",", "den", "V\u00f6l\u00b7kern", "ein", "Grau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "APPR", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ballt sich ein schwarzer, ein schrecklicher Zug:", "tokens": ["Ballt", "sich", "ein", "schwar\u00b7zer", ",", "ein", "schreck\u00b7li\u00b7cher", "Zug", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Riesen und Schlangen, entsetzlich zu schauen,", "tokens": ["Rie\u00b7sen", "und", "Schlan\u00b7gen", ",", "ent\u00b7setz\u00b7lich", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Rasende Rosse mit Fl\u00fcgeln am Bug!", "tokens": ["Ra\u00b7sen\u00b7de", "Ros\u00b7se", "mit", "Fl\u00fc\u00b7geln", "am", "Bug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Allen voran der verderbliche Geier,", "tokens": ["Al\u00b7len", "vo\u00b7ran", "der", "ver\u00b7derb\u00b7li\u00b7che", "Gei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Kreischend nach Fra\u00df und die F\u00e4nge gespannt:", "tokens": ["Krei\u00b7schend", "nach", "Fra\u00df", "und", "die", "F\u00e4n\u00b7ge", "ge\u00b7spannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sonneverfinsternd erstrecket der Schreier", "tokens": ["Son\u00b7ne\u00b7ver\u00b7fins\u00b7ternd", "er\u00b7stre\u00b7cket", "der", "Schrei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Schattende Schwingen vom Meere zum Land.", "tokens": ["Schat\u00b7ten\u00b7de", "Schwin\u00b7gen", "vom", "Mee\u00b7re", "zum", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Flammendes Z\u00fcngelein schl\u00e4gt er zuweilen", "tokens": ["Flam\u00b7men\u00b7des", "Z\u00fcn\u00b7ge\u00b7lein", "schl\u00e4gt", "er", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADV"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Rot aus des Schnabels, des klaffenden, Ritz:", "tokens": ["Rot", "aus", "des", "Schna\u00b7bels", ",", "des", "klaf\u00b7fen\u00b7den", ",", "Ritz", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u2013 Hinter ihm Nacht \u2013: doch in zischenden Keilen", "tokens": ["\u2013", "Hin\u00b7ter", "ihm", "Nacht", "\u2013", ":", "doch", "in", "zi\u00b7schen\u00b7den", "Kei\u00b7len"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPER", "NN", "$(", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Zuckt aus dem Schnabel dann z\u00fcndender Blitz.", "tokens": ["Zuckt", "aus", "dem", "Schna\u00b7bel", "dann", "z\u00fcn\u00b7den\u00b7der", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber noch grausiger als an dem Himmel", "tokens": ["A\u00b7ber", "noch", "grau\u00b7si\u00b7ger", "als", "an", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "W\u00e4lzt sich auf Erden ein flutender Streif:", "tokens": ["W\u00e4lzt", "sich", "auf", "Er\u00b7den", "ein", "flu\u00b7ten\u00b7der", "Streif", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Drachenvergleichlich, ein V\u00f6lkergewimmel,", "tokens": ["Dra\u00b7chen\u00b7ver\u00b7gleich\u00b7lich", ",", "ein", "V\u00f6l\u00b7ker\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Feuer im Rachen und Gift in dem Schweif!", "tokens": ["Feu\u00b7er", "im", "Ra\u00b7chen", "und", "Gift", "in", "dem", "Schweif", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Blies da ein Mann auf gewundenem Horne", "tokens": ["Blies", "da", "ein", "Mann", "auf", "ge\u00b7wun\u00b7de\u00b7nem", "Hor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "An der Alutha vor felligem Zelt:", "tokens": ["An", "der", "A\u00b7lut\u00b7ha", "vor", "fel\u00b7li\u00b7gem", "Zelt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Schauernd in Lust und in Schreck und in Zorne", "tokens": ["Schau\u00b7ernd", "in", "Lust", "und", "in", "Schreck", "und", "in", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Bebt da der Okzident, zittert die Welt.", "tokens": ["Bebt", "da", "der", "Ok\u00b7zi\u00b7dent", ",", "zit\u00b7tert", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbhunnen, die Erde, mir gab sie der Kriegsgott!", "tokens": ["\u00bb", "hun\u00b7nen", ",", "die", "Er\u00b7de", ",", "mir", "gab", "sie", "der", "Kriegs\u00b7gott", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Hunnen, euch schenk' ich sie, mordet sie aus!\u00ab", "tokens": ["Hun\u00b7nen", ",", "euch", "schenk'", "ich", "sie", ",", "mor\u00b7det", "sie", "aus", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u00bbattila, scholl es da, V\u00e4terlein, Siegsgott,", "tokens": ["\u00bb", "at\u00b7ti\u00b7la", ",", "scholl", "es", "da", ",", "V\u00e4\u00b7ter\u00b7lein", ",", "Siegs\u00b7gott", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "NE", "$,", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Danke dir, danke dir! Richten es aus.\u00ab", "tokens": ["Dan\u00b7ke", "dir", ",", "dan\u00b7ke", "dir", "!", "Rich\u00b7ten", "es", "aus", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "PPER", "$,", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Horch! Von dem Kaukasus bebt bis nach B\u00f6hmen", "tokens": ["Horch", "!", "Von", "dem", "Kau\u00b7ka\u00b7sus", "bebt", "bis", "nach", "B\u00f6h\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "ART", "NN", "VVFIN", "ADV", "APPR", "NE"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Dr\u00f6hnend Europa von Hufengestampf,", "tokens": ["Dr\u00f6h\u00b7nend", "Eu\u00b7ro\u00b7pa", "von", "Hu\u00b7fen\u00b7ge\u00b7stampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Hoch auf den Bergen und tief in den Str\u00f6men", "tokens": ["Hoch", "auf", "den", "Ber\u00b7gen", "und", "tief", "in", "den", "Str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Woget und w\u00fctet und w\u00fcrget der Kampf.", "tokens": ["Wo\u00b7get", "und", "w\u00fc\u00b7tet", "und", "w\u00fcr\u00b7get", "der", "Kampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbattila, Attila, Spender der Beute!", "tokens": ["\u00bb", "at\u00b7ti\u00b7la", ",", "At\u00b7ti\u00b7la", ",", "Spen\u00b7der", "der", "Beu\u00b7te", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$,", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "V\u00e4terlein, sage nur, machen wir's recht?", "tokens": ["V\u00e4\u00b7ter\u00b7lein", ",", "sa\u00b7ge", "nur", ",", "ma\u00b7chen", "wir's", "recht", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,", "VVINF", "VAFIN", "ADJD", "$."], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Pf\u00e4hlen die J\u00fcnglinge, schleifen die Br\u00e4ute,", "tokens": ["Pf\u00e4h\u00b7len", "die", "J\u00fcng\u00b7lin\u00b7ge", ",", "schlei\u00b7fen", "die", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "B\u00fcgelgebunden, am Lockengeflecht.", "tokens": ["B\u00fc\u00b7gel\u00b7ge\u00b7bun\u00b7den", ",", "am", "Lo\u00b7cken\u00b7ge\u00b7flecht", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.11": {"line.1": {"text": "Attila, willst du so? Nieder die R\u00f6mer!", "tokens": ["At\u00b7ti\u00b7la", ",", "willst", "du", "so", "?", "Nie\u00b7der", "die", "R\u00f6\u00b7mer", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADV", "$.", "ADV", "ART", "NN", "$."], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Siebenfach nieder Germanengeschlecht!", "tokens": ["Sie\u00b7ben\u00b7fach", "nie\u00b7der", "Ger\u00b7ma\u00b7nen\u00b7ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "V\u00f6lkerzermalmender L\u00e4nderdurchstr\u00f6mer,", "tokens": ["V\u00f6l\u00b7ker\u00b7zer\u00b7mal\u00b7men\u00b7der", "L\u00e4n\u00b7der\u00b7durch\u00b7str\u00f6\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Attila, sag' es uns, machen wir's recht?\u00ab", "tokens": ["At\u00b7ti\u00b7la", ",", "sag'", "es", "uns", ",", "ma\u00b7chen", "wir's", "recht", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "$,", "VVINF", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Aber die Gei\u00dfel, neunstr\u00e4ngig, mit Blute,", "tokens": ["A\u00b7ber", "die", "Gei\u00b7\u00dfel", ",", "neuns\u00b7tr\u00e4n\u00b7gig", ",", "mit", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Hebet gen Himmel der Chan im Gebet:", "tokens": ["He\u00b7bet", "gen", "Him\u00b7mel", "der", "Chan", "im", "Ge\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00bbseht ihr in Wolken die stammende Rute?",