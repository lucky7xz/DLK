eln die Welt?\u00ab", "tokens": ["Klag\u00b7te", "der", "Ei\u00b7ne", ",", "\u00bb", "ver\u00b7r\u00f6\u00b7cheln", "die", "Welt", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$(", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.14": {"line.1": {"text": "\u00bbnein doch, A\u00ebtius,\u00ab \u2013 lachte der Zweite,", "tokens": ["\u00bb", "nein", "doch", ",", "A\u00b7\u00ebti\u00b7us", ",", "\u00ab", "\u2013", "lach\u00b7te", "der", "Zwei\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "ADV", "$,", "NE", "$,", "$(", "$(", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Warf in den Nacken das goldene Haar \u2013", "tokens": ["Warf", "in", "den", "Na\u00b7cken", "das", "gol\u00b7de\u00b7ne", "Haar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u00bbla\u00df uns vergessen verstrittener Streite:", "tokens": ["\u00bb", "la\u00df", "uns", "ver\u00b7ges\u00b7sen", "ver\u00b7strit\u00b7te\u00b7ner", "Strei\u00b7te", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "VVPP", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Sage, wen f\u00fcrchten wir, \u2013 wir: \u2013 wenn ein ", "tokens": ["Sa\u00b7ge", ",", "wen", "f\u00fcrch\u00b7ten", "wir", ",", "\u2013", "wir", ":", "\u2013", "wenn", "ein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "$,", "$(", "PPER", "$.", "$(", "KOUS", "ART"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.15": {"line.1": {"text": "Rufe vom Tiber durch fliegende Boten", "tokens": ["Ru\u00b7fe", "vom", "Ti\u00b7ber", "durch", "flie\u00b7gen\u00b7de", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NE", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Deiner Legionen gepanzerte Wehr,", "tokens": ["Dei\u00b7ner", "Le\u00b7gi\u00b7o\u00b7nen", "ge\u00b7pan\u00b7zer\u00b7te", "Wehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Traue Theoderichs freudigen Goten:", "tokens": ["Trau\u00b7e", "The\u00b7o\u00b7de\u00b7richs", "freu\u00b7di\u00b7gen", "Go\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "R\u00f6mischer Schild und germanischer Speer!", "tokens": ["R\u00f6\u00b7mi\u00b7scher", "Schild", "und", "ger\u00b7ma\u00b7ni\u00b7scher", "Speer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.16": {"line.1": {"text": "La\u00df sie nur kommen auf zottigen G\u00e4ulen!", "tokens": ["La\u00df", "sie", "nur", "kom\u00b7men", "auf", "zot\u00b7ti\u00b7gen", "G\u00e4u\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "La\u00df sie empfahn uns mit Schild und mit Schaft:", "tokens": ["La\u00df", "sie", "em\u00b7pfahn", "uns", "mit", "Schild", "und", "mit", "Schaft