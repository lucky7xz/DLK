{"dta.poem.12633": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XxII.  \n  W\u00fcnsche und Gesundheiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verdoppelt euch, ihr Freuden-T\u00f6ne!", "tokens": ["Ver\u00b7dop\u00b7pelt", "euch", ",", "ihr", "Freu\u00b7den\u00b7T\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begleitet diesen Reben-Saft.", "tokens": ["Be\u00b7glei\u00b7tet", "die\u00b7sen", "Re\u00b7ben\u00b7Saft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf! auf! Es lebe jede Sch\u00f6ne", "tokens": ["Auf", "!", "auf", "!", "Es", "le\u00b7be", "je\u00b7de", "Sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "PTKVZ", "$.", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00fcsser Reitzung Wunder-Kraft.", "tokens": ["Und", "s\u00fcs\u00b7ser", "Reit\u00b7zung", "Wun\u00b7der\u00b7Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verehret anmuthsvolle Blicke", "tokens": ["Ver\u00b7eh\u00b7ret", "an\u00b7muths\u00b7vol\u00b7le", "Bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihre stumme Redner-Kunst;", "tokens": ["Und", "ih\u00b7re", "stum\u00b7me", "Red\u00b7ner\u00b7Kunst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcnscht geliebten Augen Gl\u00fccke", "tokens": ["Und", "w\u00fcnscht", "ge\u00b7lieb\u00b7ten", "Au\u00b7gen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und uns geliebter Augen Gunst.", "tokens": ["Und", "uns", "ge\u00b7lieb\u00b7ter", "Au\u00b7gen", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die holden Lippen m\u00fcssen bl\u00fchen,", "tokens": ["Die", "hol\u00b7den", "Lip\u00b7pen", "m\u00fcs\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Jugend f\u00e4rbt und Scherz belebt;", "tokens": ["Die", "Ju\u00b7gend", "f\u00e4rbt", "und", "Scherz", "be\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nicht zu sehr dem Ku\u00df entfliehen;", "tokens": ["Die", "nicht", "zu", "sehr", "dem", "Ku\u00df", "ent\u00b7flie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PTKA", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um die der Ku\u00df mit Freude schwebt.", "tokens": ["Um", "die", "der", "Ku\u00df", "mit", "Freu\u00b7de", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Viel Gl\u00fcck dem weiblichen Geschlechte,", "tokens": ["Viel", "Gl\u00fcck", "dem", "weib\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Hoffnungs-voller Jungfern-Stand!", "tokens": ["Dir", ",", "Hoff\u00b7nungs\u00b7vol\u00b7ler", "Jung\u00b7fern\u00b7Stand", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Frauen ehelichem Rechte", "tokens": ["Der", "Frau\u00b7en", "e\u00b7he\u00b7li\u00b7chem", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder sch\u00f6nen Witwen-Hand.", "tokens": ["Und", "je\u00b7der", "sch\u00f6\u00b7nen", "Wit\u00b7wen\u00b7Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jhr Sch\u00f6nen! die in dieser Stunde", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", "!", "die", "in", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ART", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein holdes Gl\u00fcck uns zugesellt,", "tokens": ["Ein", "hol\u00b7des", "Gl\u00fcck", "uns", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trinkt oder schlurft mit frohem Munde:", "tokens": ["Trinkt", "o\u00b7der", "schlurft", "mit", "fro\u00b7hem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Worauf? Auf das, was euch gef\u00e4llt.", "tokens": ["Wo\u00b7rauf", "?", "Auf", "das", ",", "was", "euch", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es leben die beliebten Alten,", "tokens": ["Es", "le\u00b7ben", "die", "be\u00b7lieb\u00b7ten", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fr\u00f6hlichkeiten g\u00fcnstig sind.", "tokens": ["Die", "Fr\u00f6h\u00b7lich\u00b7kei\u00b7ten", "g\u00fcns\u00b7tig", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Geist der Lust hat sie erhalten;", "tokens": ["Ein", "Geist", "der", "Lust", "hat", "sie", "er\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der geh auf Kind und Kindes-Kind.", "tokens": ["Der", "geh", "auf", "Kind", "und", "Kin\u00b7des\u00b7Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O tr\u00e4nkt euch, jugendliche Kr\u00e4fte!", "tokens": ["O", "tr\u00e4nkt", "euch", ",", "ju\u00b7gend\u00b7li\u00b7che", "Kr\u00e4f\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den V\u00e4tern besser beyzustehn.", "tokens": ["Den", "V\u00e4\u00b7tern", "bes\u00b7ser", "bey\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft euch so wacker zum Gesch\u00e4fte", "tokens": ["La\u00dft", "euch", "so", "wa\u00b7cker", "zum", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als muthig zum Vergn\u00fcgen sehn.", "tokens": ["Als", "mut\u00b7hig", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn keine Lieb und Freundschaft w\u00e4ren,", "tokens": ["Wenn", "kei\u00b7ne", "Lieb", "und", "Freund\u00b7schaft", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was h\u00e4tt\u2019 uns Menschen nicht gefehlt?", "tokens": ["Was", "h\u00e4tt'", "uns", "Men\u00b7schen", "nicht", "ge\u00b7fehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man singe jedem Tag zu Ehren,", "tokens": ["Man", "sin\u00b7ge", "je\u00b7dem", "Tag", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der aus Bekannten Freunde w\u00e4hlt.", "tokens": ["Der", "aus", "Be\u00b7kann\u00b7ten", "Freun\u00b7de", "w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Es leben wohlgew\u00e4hlte Freunde!", "tokens": ["Es", "le\u00b7ben", "wohl\u00b7ge\u00b7w\u00e4hl\u00b7te", "Freun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die\u00df Glas soll ihnen heilig seyn.", "tokens": ["Die\u00df", "Glas", "soll", "ih\u00b7nen", "hei\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt gleichfalls, halb\u2019 und ganze Feinde!", "tokens": ["Lebt", "gleich\u00b7falls", ",", "halb'", "und", "gan\u00b7ze", "Fein\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch fern von uns, und ohne Wein.", "tokens": ["Doch", "fern", "von", "uns", ",", "und", "oh\u00b7ne", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Vergn\u00fcgter Stunden Angedenken", "tokens": ["Ver\u00b7gn\u00fcg\u00b7ter", "Stun\u00b7den", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht diese Stunden doppelt sch\u00f6n.", "tokens": ["Macht", "die\u00b7se", "Stun\u00b7den", "dop\u00b7pelt", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADJD", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Du Zeit, die wir der Freude schenken,", "tokens": ["Du", "Zeit", ",", "die", "wir", "der", "Freu\u00b7de", "schen\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O la\u00df dich \u00f6fters wieder sehn!", "tokens": ["O", "la\u00df", "dich", "\u00f6f\u00b7ters", "wie\u00b7der", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Du Pfand des Gl\u00fcckes, g\u00fcldner Friede!", "tokens": ["Du", "Pfand", "des", "Gl\u00fc\u00b7ckes", ",", "g\u00fcld\u00b7ner", "Frie\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O kehr in Deutschland wieder ein!", "tokens": ["O", "kehr", "in", "Deutschland", "wie\u00b7der", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "ADV", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So trinken wir, bey manchem Liede,", "tokens": ["So", "trin\u00b7ken", "wir", ",", "bey", "man\u00b7chem", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit ausges\u00f6hnten Feinden Wein.", "tokens": ["Mit", "aus\u00b7ge\u00b7s\u00f6hn\u00b7ten", "Fein\u00b7den", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Uns treffe nie der Fluch der Zeiten,", "tokens": ["Uns", "tref\u00b7fe", "nie", "der", "Fluch", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo gut Getr\u00e4nke bitter ist,", "tokens": ["Wo", "gut", "Ge\u00b7tr\u00e4n\u00b7ke", "bit\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und man beym Wein der Fr\u00f6hlichkeiten,", "tokens": ["Und", "man", "beym", "Wein", "der", "Fr\u00f6h\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Singens und des Klangs vergi\u00dft!", "tokens": ["Des", "Sin\u00b7gens", "und", "des", "Klangs", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Lebt, \u00fcberlebt die Splitter-Richter,", "tokens": ["Lebt", ",", "\u00fc\u00b7ber\u00b7lebt", "die", "Split\u00b7ter\u00b7Rich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Freunde! die ihr weislich lacht,", "tokens": ["Ihr", "Freun\u00b7de", "!", "die", "ihr", "weis\u00b7lich", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einem aufgeweckten Dichter", "tokens": ["Und", "ei\u00b7nem", "auf\u00b7ge\u00b7weck\u00b7ten", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht jeden Scherz zum Frevel macht.", "tokens": ["Nicht", "je\u00b7den", "Scherz", "zum", "Fre\u00b7vel", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}