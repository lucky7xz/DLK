{"textgrid.poem.32964": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: So freu dich doch, da\u00df es Fr\u00fchling wird", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So freu dich doch, da\u00df es Fr\u00fchling wird", "tokens": ["So", "freu", "dich", "doch", ",", "da\u00df", "es", "Fr\u00fch\u00b7ling", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADV", "$,", "KOUS", "PPER", "NN", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und la\u00df die Wintergedanken,", "tokens": ["und", "la\u00df", "die", "Win\u00b7ter\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "la\u00df keimen, was der Sonnenschein", "tokens": ["la\u00df", "kei\u00b7men", ",", "was", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dir in die Seele will ranken.", "tokens": ["dir", "in", "die", "See\u00b7le", "will", "ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "All\u00fcberall alles voll Jubelget\u00f6n,", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "al\u00b7les", "voll", "Ju\u00b7bel\u00b7ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "ADJD", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "voll Mailust-entgegen-Genesen", "tokens": ["voll", "Mai\u00b7lust\u00b7ent\u00b7ge\u00b7gen\u00b7Ge\u00b7ne\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "und die Luft so lau und der Himmel so blau", "tokens": ["und", "die", "Luft", "so", "lau", "und", "der", "Him\u00b7mel", "so", "blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "KON", "ART", "NN", "ADV", "ADJD"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und die Welt so sch\u00f6n, o, so wundersch\u00f6n,", "tokens": ["und", "die", "Welt", "so", "sch\u00f6n", ",", "o", ",", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$,", "FM", "$,", "ADV", "ADJD", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "wie sie noch nie gewesen.", "tokens": ["wie", "sie", "noch", "nie", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es wird schon werden, es wird schon werden!", "tokens": ["Es", "wird", "schon", "wer\u00b7den", ",", "es", "wird", "schon", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VAINF", "$,", "PPER", "VAFIN", "ADV", "VAINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein kleines Weilchen nur noch, und:", "tokens": ["ein", "klei\u00b7nes", "Weil\u00b7chen", "nur", "noch", ",", "und", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$,", "KON", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mit bl\u00fchenden Rosen steht es am Weg", "tokens": ["mit", "bl\u00fc\u00b7hen\u00b7den", "Ro\u00b7sen", "steht", "es", "am", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und k\u00fc\u00dft auf die Stirn dich mit seligem Mund.", "tokens": ["und", "k\u00fc\u00dft", "auf", "die", "Stirn", "dich", "mit", "se\u00b7li\u00b7gem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}