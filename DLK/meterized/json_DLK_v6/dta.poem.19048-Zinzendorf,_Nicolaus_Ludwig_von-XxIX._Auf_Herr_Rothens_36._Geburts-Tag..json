{"dta.poem.19048": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.  Auf Herr Rothens 36. Geburts-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wer von der Erde ist, den hat die Erde lieb;", "tokens": ["Wer", "von", "der", "Er\u00b7de", "ist", ",", "den", "hat", "die", "Er\u00b7de", "lieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "$,", "ART", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn was sich angeh\u00f6rt, da\u00df mu\u00df sich wohlgefallen;", "tokens": ["Denn", "was", "sich", "an\u00b7ge\u00b7h\u00f6rt", ",", "da\u00df", "mu\u00df", "sich", "wohl\u00b7ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "VVPP", "$,", "KOUS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was der Natur gerecht, das w\u00e4hlt sie sich f\u00fcr allen;", "tokens": ["Was", "der", "Na\u00b7tur", "ge\u00b7recht", ",", "das", "w\u00e4hlt", "sie", "sich", "f\u00fcr", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "$,", "PDS", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da kommt die Freundschaft her, ein eingepflantzter Trieb:", "tokens": ["Da", "kommt", "die", "Freund\u00b7schaft", "her", ",", "ein", "ein\u00b7ge\u00b7pflantz\u00b7ter", "Trieb", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Je weiter sich ein Ding von unsrer Art entfernet,", "tokens": ["Je", "wei\u00b7ter", "sich", "ein", "Ding", "von", "uns\u00b7rer", "Art", "ent\u00b7fer\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Je leichter wird der Ha\u00df dagegen angelernet.", "tokens": ["Je", "leich\u00b7ter", "wird", "der", "Ha\u00df", "da\u00b7ge\u00b7gen", "an\u00b7ge\u00b7ler\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Abgrund trennet GOtt vom Feinde Belial", "tokens": ["Der", "Ab\u00b7grund", "tren\u00b7net", "Gott", "vom", "Fein\u00b7de", "Be\u00b7li\u00b7al"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die S\u00fcnde, (welche Kluft!) des HErrn und unser Wesen,", "tokens": ["Die", "S\u00fcn\u00b7de", ",", "(", "wel\u00b7che", "Kluft", "!", ")", "des", "Herrn", "und", "un\u00b7ser", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "PWAT", "NN", "$.", "$(", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir hassen seinen Geist, bi\u00df wir davon genesen:", "tokens": ["Wir", "has\u00b7sen", "sei\u00b7nen", "Geist", ",", "bi\u00df", "wir", "da\u00b7von", "ge\u00b7ne\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn unsere Statur zertrennete der Fall.", "tokens": ["Denn", "un\u00b7se\u00b7re", "Sta\u00b7tur", "zer\u00b7tren\u00b7ne\u00b7te", "der", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn aber Christus uns von neuen erst verbunden;", "tokens": ["Wenn", "a\u00b7ber", "Chris\u00b7tus", "uns", "von", "neu\u00b7en", "erst", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "PPER", "APPR", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So ist die Freundschaft auch mit leichter M\u00fch gefunden.", "tokens": ["So", "ist", "die", "Freund\u00b7schaft", "auch", "mit", "leich\u00b7ter", "M\u00fch", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da liebt die Seele GOtt, und GOtt die Seele dann,", "tokens": ["Da", "liebt", "die", "See\u00b7le", "Gott", ",", "und", "Gott", "die", "See\u00b7le", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "KON", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und alle, die zugleich in di\u00df Verb\u00fcndni\u00df treten,", "tokens": ["Und", "al\u00b7le", ",", "die", "zu\u00b7gleich", "in", "di\u00df", "Ver\u00b7b\u00fcn\u00b7dni\u00df", "tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "APPR", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den HErrn als ihren Freund und Vater anzubeten,", "tokens": ["Den", "Herrn", "als", "ih\u00b7ren", "Freund", "und", "Va\u00b7ter", "an\u00b7zu\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPOSAT", "NN", "KON", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die sehn einander nun, als liebe Br\u00fcder an.", "tokens": ["Die", "sehn", "ein\u00b7an\u00b7der", "nun", ",", "als", "lie\u00b7be", "Br\u00fc\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "$,", "KOUS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gewi\u00df! man glaubets nicht, man hab\u2019 es denn erfahren,", "tokens": ["Ge\u00b7wi\u00df", "!", "man", "glau\u00b7bets", "nicht", ",", "man", "hab'", "es", "denn", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIS", "VVFIN", "PTKNEG", "$,", "PIS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie sich die Glaubigen im Geist zusammen paaren.", "tokens": ["Wie", "sich", "die", "Glau\u00b7bi\u00b7gen", "im", "Geist", "zu\u00b7sam\u00b7men", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein jeder siehet sich ein klares Merckmahl aus,", "tokens": ["Ein", "je\u00b7der", "sie\u00b7het", "sich", "ein", "kla\u00b7res", "Merck\u00b7mahl", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Woran er Christi Geist und Christi Sinn erkennet,", "tokens": ["Wo\u00b7ran", "er", "Chris\u00b7ti", "Geist", "und", "Chris\u00b7ti", "Sinn", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "NN", "KON", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und ob der andre sey, was er sich gleichwol nennet,", "tokens": ["Und", "ob", "der", "and\u00b7re", "sey", ",", "was", "er", "sich", "gleich\u00b7wol", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIS", "VAFIN", "$,", "PWS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Ein Glied am Br\u00e4utigam, ein Stein zum Tempel-Hau\u00df;", "tokens": ["Ein", "Glied", "am", "Br\u00e4u\u00b7ti\u00b7gam", ",", "ein", "Stein", "zum", "Tem\u00b7pel\u00b7Hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und kan er dieses nur nach aller Wahrheit finden,", "tokens": ["Und", "kan", "er", "die\u00b7ses", "nur", "nach", "al\u00b7ler", "Wahr\u00b7heit", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDS", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So eilt er d\u00fcrstiglich mit ihm sich zu verbinden.", "tokens": ["So", "eilt", "er", "d\u00fcrs\u00b7tig\u00b7lich", "mit", "ihm", "sich", "zu", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das Auge thr\u00e4nt, es schl\u00e4gt das Hertz, die Hand ergreift,", "tokens": ["Das", "Au\u00b7ge", "thr\u00e4nt", ",", "es", "schl\u00e4gt", "das", "Hertz", ",", "die", "Hand", "er\u00b7greift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Fu\u00df beweget uns von einem Ort zum andern,", "tokens": ["Der", "Fu\u00df", "be\u00b7we\u00b7get", "uns", "von", "ei\u00b7nem", "Ort", "zum", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und macht, da\u00df Hertz und Hand und alle Glieder wandern;", "tokens": ["Und", "macht", ",", "da\u00df", "Hertz", "und", "Hand", "und", "al\u00b7le", "Glie\u00b7der", "wan\u00b7dern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das Haupt ist mehrentheils mit Dencken \u00fcberh\u00e4uft.", "tokens": ["Das", "Haupt", "ist", "meh\u00b7ren\u00b7theils", "mit", "Den\u00b7cken", "\u00fc\u00b7berh\u00b7\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das Ohr vernimmt das Wort, und tr\u00e4gt es zu den Sinnen,", "tokens": ["Das", "Ohr", "ver\u00b7nimmt", "das", "Wort", ",", "und", "tr\u00e4gt", "es", "zu", "den", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die durch den ofnen Mund ein Gegen-Wort beginnen.", "tokens": ["Die", "durch", "den", "of\u00b7nen", "Mund", "ein", "Ge\u00b7gen\u00b7Wort", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So sehr ver\u00e4nderlich, so mancherley Gestalt", "tokens": ["So", "sehr", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ",", "so", "man\u00b7cher\u00b7ley", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sind Glieder, die zugleich nur einen Leib bedeuten,", "tokens": ["Sind", "Glie\u00b7der", ",", "die", "zu\u00b7gleich", "nur", "ei\u00b7nen", "Leib", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Das Haupt betrachtet sie, die nahen und die weiten;", "tokens": ["Das", "Haupt", "be\u00b7trach\u00b7tet", "sie", ",", "die", "na\u00b7hen", "und", "die", "wei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "KON", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Inde\u00df, da ein Gebl\u00fct in ihnen allen wallt.", "tokens": ["In\u00b7de\u00df", ",", "da", "ein", "Ge\u00b7bl\u00fct", "in", "ih\u00b7nen", "al\u00b7len", "wallt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Seele Sorgfalt wei\u00df sie alle zu verbinden,", "tokens": ["Der", "See\u00b7le", "Sorg\u00b7falt", "wei\u00df", "sie", "al\u00b7le", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df keines seine Ruh kan ohne jenes finden.", "tokens": ["Da\u00df", "kei\u00b7nes", "sei\u00b7ne", "Ruh", "kan", "oh\u00b7ne", "je\u00b7nes", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VMFIN", "APPR", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jm Leibe Christi siehts von in- und aussen so,", "tokens": ["Jm", "Lei\u00b7be", "Chris\u00b7ti", "siehts", "von", "in", "und", "aus\u00b7sen", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "APPR", "TRUNC", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie in dem Menschen aus; sie haben gleiche Kr\u00e4fte,", "tokens": ["Wie", "in", "dem", "Men\u00b7schen", "aus", ";", "sie", "ha\u00b7ben", "glei\u00b7che", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und doch so mancherley zergliederte Gesch\u00e4fte,", "tokens": ["Und", "doch", "so", "man\u00b7cher\u00b7ley", "zer\u00b7glie\u00b7der\u00b7te", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und ohne eines wird das andre sein nicht froh.", "tokens": ["Und", "oh\u00b7ne", "ei\u00b7nes", "wird", "das", "and\u00b7re", "sein", "nicht", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VAFIN", "ART", "ADJA", "VAINF", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Leib wird munter, da der Seelen-Kr\u00e4fte schlafen,", "tokens": ["Der", "Leib", "wird", "mun\u00b7ter", ",", "da", "der", "See\u00b7len\u00b7Kr\u00e4f\u00b7te", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und wenn er schl\u00e4frig wird, f\u00e4ngt jene an zu schaffen:", "tokens": ["Und", "wenn", "er", "schl\u00e4f\u00b7rig", "wird", ",", "f\u00e4ngt", "je\u00b7ne", "an", "zu", "schaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PDS", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "O Liebe! du hast dir hie einen Leib gebaut,", "tokens": ["O", "Lie\u00b7be", "!", "du", "hast", "dir", "hie", "ei\u00b7nen", "Leib", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ein liebes Tempel-Hau\u00df von auserwehlten Steinen;", "tokens": ["Ein", "lie\u00b7bes", "Tem\u00b7pel\u00b7Hau\u00df", "von", "au\u00b7ser\u00b7wehl\u00b7ten", "Stei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die Pfort ist aufgethan, und seine Fenster scheinen,", "tokens": ["Die", "Pfort", "ist", "auf\u00b7ge\u00b7than", ",", "und", "sei\u00b7ne", "Fens\u00b7ter", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die jener nur verh\u00f6hnt, der mit Verwundrung schaut,", "tokens": ["Die", "je\u00b7ner", "nur", "ver\u00b7h\u00f6hnt", ",", "der", "mit", "Ver\u00b7wund\u00b7rung", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADV", "VVPP", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wir haben dich dabey in GOttes grossen Nahmen", "tokens": ["Wir", "ha\u00b7ben", "dich", "da\u00b7bey", "in", "Got\u00b7tes", "gros\u00b7sen", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "PAV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Zum Eckstein hingelegt, als wir gen Beth El kamen.", "tokens": ["Zum", "E\u00b7ckstein", "hin\u00b7ge\u00b7legt", ",", "als", "wir", "gen", "Be\u00b7th", "El", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KOUS", "PPER", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.49": {"text": "So kr\u00f6ne diesen Bau: und weil du je gewolt,", "tokens": ["So", "kr\u00f6\u00b7ne", "die\u00b7sen", "Bau", ":", "und", "weil", "du", "je", "ge\u00b7wolt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$.", "KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Der liebe ", "tokens": ["Der", "lie\u00b7be"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Und ihn nach eigner Wahl dir dazu ausersehen,", "tokens": ["Und", "ihn", "nach", "eig\u00b7ner", "Wahl", "dir", "da\u00b7zu", "au\u00b7ser\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Eh man noch einen Stein zu diesem Bau gerollt.", "tokens": ["Eh", "man", "noch", "ei\u00b7nen", "Stein", "zu", "die\u00b7sem", "Bau", "ge\u00b7rollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.53": {"text": "O Haupt! so lehre uns mit Beten und mit Wachen", "tokens": ["O", "Haupt", "!", "so", "leh\u00b7re", "uns", "mit", "Be\u00b7ten", "und", "mit", "Wa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Den gantzen Saphir-Grund des Tempels fertig machen!", "tokens": ["Den", "gant\u00b7zen", "Sa\u00b7phir\u00b7Grund", "des", "Tem\u00b7pels", "fer\u00b7tig", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}