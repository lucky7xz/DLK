{"textgrid.poem.54096": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Oh Frau!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh Frau!", "tokens": ["Oh", "Frau", "!"], "token_info": ["word", "word", "punct"], "pos": ["XY", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Lerne du das Flugzeug steuern,", "tokens": ["Ler\u00b7ne", "du", "das", "Flug\u00b7zeug", "steu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "lerne Vollmatrosen heuern,", "tokens": ["ler\u00b7ne", "Voll\u00b7mat\u00b7ro\u00b7sen", "heu\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "lenke nur ein Viergespann", "tokens": ["len\u00b7ke", "nur", "ein", "Vier\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wie ein Mann.", "tokens": ["wie", "ein", "Mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "M\u00e4nner werden immer kleiner,", "tokens": ["M\u00e4n\u00b7ner", "wer\u00b7den", "im\u00b7mer", "klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unerreichbar ist nichts mehr \u2013:", "tokens": ["un\u00b7er\u00b7reich\u00b7bar", "ist", "nichts", "mehr", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "ADV", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe Frau! Es fliegt dir einer", "tokens": ["Lie\u00b7be", "Frau", "!", "Es", "fliegt", "dir", "ei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "immer hinterher.", "tokens": ["im\u00b7mer", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Rechne du Gehaltstabellen,", "tokens": ["Rech\u00b7ne", "du", "Ge\u00b7halt\u00b7sta\u00b7bel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dirigiere du Kapellen,", "tokens": ["di\u00b7ri\u00b7gie\u00b7re", "du", "Ka\u00b7pel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "weil die Frau ja alles kann", "tokens": ["weil", "die", "Frau", "ja", "al\u00b7les", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "PIS", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wie ein Mann.", "tokens": ["wie", "ein", "Mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Tu das alles. Doch ein Kleiner", "tokens": ["Tu", "das", "al\u00b7les", ".", "Doch", "ein", "Klei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "PIS", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "folgt dir \u00fcber Land und Meer.", "tokens": ["folgt", "dir", "\u00fc\u00b7ber", "Land", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es fliegt dir immer einer", "tokens": ["Und", "es", "fliegt", "dir", "im\u00b7mer", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "immer hinterher.", "tokens": ["im\u00b7mer", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Krieche in die Bergwerksstollen,", "tokens": ["Krie\u00b7che", "in", "die", "Berg\u00b7werks\u00b7stol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "flieh die hei\u00dfen Liebestollen;", "tokens": ["flieh", "die", "hei\u00b7\u00dfen", "Lie\u00b7be\u00b7stol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "du bleibst noch im Himmelsblau", "tokens": ["du", "bleibst", "noch", "im", "Him\u00b7mels\u00b7blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "eine Frau", "tokens": ["ei\u00b7ne", "Frau"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Noch das st\u00e4rkste Frauenzimmer", "tokens": ["Noch", "das", "st\u00e4rks\u00b7te", "Frau\u00b7en\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hats in dieser Sache schwer . . .", "tokens": ["hats", "in", "die\u00b7ser", "Sa\u00b7che", "schwer", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "ADJD", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es folgt ihr immer, immer,", "tokens": ["Denn", "es", "folgt", "ihr", "im\u00b7mer", ",", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "immer, immer, immer, immer", "tokens": ["im\u00b7mer", ",", "im\u00b7mer", ",", "im\u00b7mer", ",", "im\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "einer hinterher.", "tokens": ["ei\u00b7ner", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}