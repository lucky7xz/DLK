{"textgrid.poem.65143": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nichts kommt in des Lebens Reiche", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts kommt in des Lebens Reiche", "tokens": ["Nichts", "kommt", "in", "des", "Le\u00b7bens", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nahe jener Lust,", "tokens": ["Na\u00b7he", "je\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die mich fa\u00dft, schmieg' ich die bleiche", "tokens": ["Die", "mich", "fa\u00dft", ",", "schmieg'", "ich", "die", "blei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stirn an deine Brust!", "tokens": ["Stirn", "an", "dei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die mein Innerstes ersch\u00fcttert,", "tokens": ["Die", "mein", "In\u00b7ners\u00b7tes", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn dein Blick mich gr\u00fc\u00dft,", "tokens": ["Wenn", "dein", "Blick", "mich", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Todesselig mich durchzittert,", "tokens": ["To\u00b7des\u00b7se\u00b7lig", "mich", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn dein Mund mich k\u00fc\u00dft! \u2013", "tokens": ["Wenn", "dein", "Mund", "mich", "k\u00fc\u00dft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie sie magisch mich umweben,", "tokens": ["Wie", "sie", "ma\u00b7gisch", "mich", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blick und Hauch und Wort!", "tokens": ["Blick", "und", "Hauch", "und", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ja! du k\u00fcssest mir das Leben", "tokens": ["Ja", "!", "du", "k\u00fcs\u00b7sest", "mir", "das", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Lippe fort!", "tokens": ["Von", "der", "Lip\u00b7pe", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Du zerbrichst des K\u00f6rpers Schranke,", "tokens": ["Du", "zer\u00b7brichst", "des", "K\u00f6r\u00b7pers", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die Seele, frei,", "tokens": ["Da\u00df", "die", "See\u00b7le", ",", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nichts mehr als nur ein Gedanke", "tokens": ["Nichts", "mehr", "als", "nur", "ein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PIS", "KOKOM", "ADV", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Reinster Liebe sei!", "tokens": ["Reins\u00b7ter", "Lie\u00b7be", "sei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}