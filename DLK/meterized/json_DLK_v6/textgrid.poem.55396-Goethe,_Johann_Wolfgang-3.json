{"textgrid.poem.55396": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf schweres Gewitter und Regengu\u00df", "tokens": ["Auf", "schwe\u00b7res", "Ge\u00b7wit\u00b7ter", "und", "Re\u00b7gen\u00b7gu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Blickt' ein Philister, zum Beschlu\u00df,", "tokens": ["Blickt'", "ein", "Phi\u00b7lis\u00b7ter", ",", "zum", "Be\u00b7schlu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins weiterziehende Grause nach,", "tokens": ["Ins", "wei\u00b7ter\u00b7zie\u00b7hen\u00b7de", "Grau\u00b7se", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und so zu seinesgleichen sprach:", "tokens": ["Und", "so", "zu", "sei\u00b7nes\u00b7glei\u00b7chen", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbder Donner hat uns sehr erschreckt,", "tokens": ["\u00bb", "der", "Don\u00b7ner", "hat", "uns", "sehr", "er\u00b7schreckt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Blitz die Scheunen angesteckt,", "tokens": ["Der", "Blitz", "die", "Scheu\u00b7nen", "an\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und das war unsrer S\u00fcnden Teil!", "tokens": ["Und", "das", "war", "uns\u00b7rer", "S\u00fcn\u00b7den", "Teil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Dagegen hat zu frischem Heil", "tokens": ["Da\u00b7ge\u00b7gen", "hat", "zu", "fri\u00b7schem", "Heil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Regen fruchtbar uns erquickt", "tokens": ["Der", "Re\u00b7gen", "frucht\u00b7bar", "uns", "er\u00b7quickt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und f\u00fcr den n\u00e4chsten Herbst begl\u00fcckt.", "tokens": ["Und", "f\u00fcr", "den", "n\u00e4chs\u00b7ten", "Herbst", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was kommt nun aber der Regenbogen", "tokens": ["Was", "kommt", "nun", "a\u00b7ber", "der", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "An grauer Wand herangezogen?", "tokens": ["An", "grau\u00b7er", "Wand", "her\u00b7an\u00b7ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der mag wohl zu entbehren sein,", "tokens": ["Der", "mag", "wohl", "zu", "ent\u00b7beh\u00b7ren", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der bunte Trug! der leere Schein!\u00ab", "tokens": ["Der", "bun\u00b7te", "Trug", "!", "der", "lee\u00b7re", "Schein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Frau Iris aber dagegen sprach:", "tokens": ["Frau", "I\u00b7ris", "a\u00b7ber", "da\u00b7ge\u00b7gen", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "PAV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bberk\u00fchnst du dich zu meiner Schmach?", "tokens": ["\u00bb", "er\u00b7k\u00fchnst", "du", "dich", "zu", "mei\u00b7ner", "Schmach", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bin ich hier ins All gestellt", "tokens": ["Doch", "bin", "ich", "hier", "ins", "All", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Zeugnis einer bessern Welt,", "tokens": ["Als", "Zeug\u00b7nis", "ei\u00b7ner", "bes\u00b7sern", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr Augen, die vom Erdenlauf", "tokens": ["F\u00fcr", "Au\u00b7gen", ",", "die", "vom", "Er\u00b7den\u00b7lauf"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Getrost sich wenden zum Himmel auf", "tokens": ["Ge\u00b7trost", "sich", "wen\u00b7den", "zum", "Him\u00b7mel", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "VVINF", "APPRART", "NN", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und in der D\u00fcnste tr\u00fcbem Netz", "tokens": ["Und", "in", "der", "D\u00fcns\u00b7te", "tr\u00fc\u00b7bem", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erkennen Gott und sein Gesetz.", "tokens": ["Er\u00b7ken\u00b7nen", "Gott", "und", "sein", "Ge\u00b7setz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Drum w\u00fchle du, ein andres Schwein,", "tokens": ["Drum", "w\u00fch\u00b7le", "du", ",", "ein", "and\u00b7res", "Schwein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nur immer den R\u00fcssel in den Boden hinein,", "tokens": ["Nur", "im\u00b7mer", "den", "R\u00fcs\u00b7sel", "in", "den", "Bo\u00b7den", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und g\u00f6nne dem verkl\u00e4rten Blick", "tokens": ["Und", "g\u00f6n\u00b7ne", "dem", "ver\u00b7kl\u00e4r\u00b7ten", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "An meiner Herrlichkeit sein Gl\u00fcck.\u00ab", "tokens": ["An", "mei\u00b7ner", "Herr\u00b7lich\u00b7keit", "sein", "Gl\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}