{"textgrid.poem.49626": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein frischer Luftzug weht im Reiche,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein frischer Luftzug weht im Reiche,", "tokens": ["Ein", "fri\u00b7scher", "Luft\u00b7zug", "weht", "im", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Herr von B\u00fclow oft betont.", "tokens": ["Wie", "Herr", "von", "B\u00fc\u00b7low", "oft", "be\u00b7tont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es gr\u00fcnt die alte, deutsche Eiche,", "tokens": ["Es", "gr\u00fcnt", "die", "al\u00b7te", ",", "deut\u00b7sche", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wer noch hoffte, wird belohnt.", "tokens": ["Und", "wer", "noch", "hoff\u00b7te", ",", "wird", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So \u00f6ffnet fr\u00f6hlich alle Fenster", "tokens": ["So", "\u00f6ff\u00b7net", "fr\u00f6h\u00b7lich", "al\u00b7le", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00dft den neuen Fr\u00fchling ein!", "tokens": ["Und", "la\u00dft", "den", "neu\u00b7en", "Fr\u00fch\u00b7ling", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verjagt die Zweifel und Gespenster", "tokens": ["Ver\u00b7jagt", "die", "Zwei\u00b7fel", "und", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und freut euch an dem Sonnenschein!", "tokens": ["Und", "freut", "euch", "an", "dem", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein! Bernhard kann uns so nicht t\u00e4uschen;", "tokens": ["Nein", "!", "Bern\u00b7hard", "kann", "uns", "so", "nicht", "t\u00e4u\u00b7schen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch vorw\u00e4rts geht der Dinge Lauf.", "tokens": ["Noch", "vor\u00b7w\u00e4rts", "geht", "der", "Din\u00b7ge", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wachen aus den Freiheitsr\u00e4uschen", "tokens": ["Wir", "wa\u00b7chen", "aus", "den", "Frei\u00b7heits\u00b7r\u00e4u\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht wieder mit dem Kater auf.", "tokens": ["Nicht", "wie\u00b7der", "mit", "dem", "Ka\u00b7ter", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dennoch schwant mir's im Gem\u00fcte:", "tokens": ["Und", "den\u00b7noch", "schwant", "mir's", "im", "Ge\u00b7m\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gar bald weht diese Fr\u00fchlingsluft", "tokens": ["Gar", "bald", "weht", "die\u00b7se", "Fr\u00fch\u00b7lings\u00b7luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PDAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Um manche abgestorb'ne Bl\u00fcte", "tokens": ["Um", "man\u00b7che", "ab\u00b7ge\u00b7stor\u00b7b'\u00b7ne", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und auch um eine Kanzlergruft.", "tokens": ["Und", "auch", "um", "ei\u00b7ne", "Kanz\u00b7ler\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}