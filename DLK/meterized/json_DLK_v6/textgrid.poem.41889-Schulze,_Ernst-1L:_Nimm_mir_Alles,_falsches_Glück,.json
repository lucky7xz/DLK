{"textgrid.poem.41889": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimm mir Alles, falsches Gl\u00fcck,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm mir Alles, falsches Gl\u00fcck,", "tokens": ["Nimm", "mir", "Al\u00b7les", ",", "fal\u00b7sches", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb mir T\u00e4uschung, Freud' und Schmerzen;", "tokens": ["Gieb", "mir", "T\u00e4u\u00b7schung", ",", "Freud'", "und", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines bleibt mir doch zur\u00fcck:", "tokens": ["Ei\u00b7nes", "bleibt", "mir", "doch", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hohe Lieb' in treuem Herzen.", "tokens": ["Ho\u00b7he", "Lieb'", "in", "treu\u00b7em", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinem Zorn erbeb' ich nicht,", "tokens": ["Dei\u00b7nem", "Zorn", "er\u00b7beb'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Klage nicht um Ruhm und Freude;", "tokens": ["Kla\u00b7ge", "nicht", "um", "Ruhm", "und", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Muthig ist, wie Morgenlicht,", "tokens": ["Mut\u00b7hig", "ist", ",", "wie", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lieb' im Leide.", "tokens": ["Lieb'", "im", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Was sie schenkte, was sie nahm,", "tokens": ["Was", "sie", "schenk\u00b7te", ",", "was", "sie", "nahm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles ist mir lieb und theuer,", "tokens": ["Al\u00b7les", "ist", "mir", "lieb", "und", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr tiefster, l\u00e4ngster Gram", "tokens": ["Und", "ihr", "tiefs\u00b7ter", ",", "l\u00e4ngs\u00b7ter", "Gram"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht mich k\u00fchner nur und treuer.", "tokens": ["Macht", "mich", "k\u00fch\u00b7ner", "nur", "und", "treu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gern erduld' ich ihre Noth,", "tokens": ["Gern", "er\u00b7duld'", "ich", "ih\u00b7re", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4chle, wenn ich mich betr\u00fcbe;", "tokens": ["L\u00e4ch\u00b7le", ",", "wenn", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Freundlich ist, wie Abendroth,", "tokens": ["Freund\u00b7lich", "ist", ",", "wie", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Leid in Liebe.", "tokens": ["Leid", "in", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}