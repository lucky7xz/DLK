{"dta.poem.11641": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Unser Friz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wollt' ein Meislein fangen,", "tokens": ["Wollt'", "ein", "Meis\u00b7lein", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Doch weil ihm denselben Tag", "tokens": ["Doch", "weil", "ihm", "den\u00b7sel\u00b7ben", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Keines drein gegangen,", "tokens": ["Kei\u00b7nes", "drein", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wird dem Friz zu lang die Zeit,", "tokens": ["Wird", "dem", "Friz", "zu", "lang", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denkt: ich hab' umsonst gestreut,", "tokens": ["Denkt", ":", "ich", "hab'", "um\u00b7sonst", "ge\u00b7streut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ja keine kommen.", "tokens": ["Will", "ja", "kei\u00b7ne", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nach acht Tagen f\u00e4llt ihm ein,", "tokens": ["Nach", "acht", "Ta\u00b7gen", "f\u00e4llt", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Garten zu spazieren:", "tokens": ["Im", "Gar\u00b7ten", "zu", "spa\u00b7zie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist sch\u00f6ner Sonnenschein,", "tokens": ["Es", "ist", "sch\u00f6\u00b7ner", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Man kann nicht erfrieren;", "tokens": ["Man", "kann", "nicht", "er\u00b7frie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Und am alten Apfelbaum", "tokens": ["Und", "am", "al\u00b7ten", "Ap\u00b7fel\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Kommt's ihm pl\u00f6tzlich wie im Traum:", "tokens": ["Kommt's", "ihm", "pl\u00f6tz\u00b7lich", "wie", "im", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob der Schlag gefallen?", "tokens": ["Ob", "der", "Schlag", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eja! es sizt ein Vogel drinn!", "tokens": ["\u201e", "ja", "!", "es", "sizt", "ein", "Vo\u00b7gel", "drinn", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber, weh! o wehe!", "tokens": ["A\u00b7ber", ",", "weh", "!", "o", "we\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PTKVZ", "$.", "FM", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das ist trauriger Gewinn:", "tokens": ["Das", "ist", "trau\u00b7ri\u00b7ger", "Ge\u00b7winn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Todt, so viel ich sehe!", "tokens": ["Todt", ",", "so", "viel", "ich", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "\u2014 Aber was kann ich daf\u00fcr?", "tokens": ["A\u00b7ber", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sicher hat das dumme Thier", "tokens": ["Si\u00b7cher", "hat", "das", "dum\u00b7me", "Thier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich zu todt gefressen!\u201c", "tokens": ["Sich", "zu", "todt", "ge\u00b7fres\u00b7sen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "PTKA", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "So tr\u00f6st't sich dein M\u00f6rder wohl,", "tokens": ["So", "tr\u00f6st't", "sich", "dein", "M\u00f6r\u00b7der", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der dich hungern lassen,", "tokens": ["Der", "dich", "hun\u00b7gern", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber ich vor Leid und Groll", "tokens": ["A\u00b7ber", "ich", "vor", "Leid", "und", "Groll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df mich nicht zu fassen!", "tokens": ["Wei\u00df", "mich", "nicht", "zu", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hast alle Br\u00f6slein aufgepickt,", "tokens": ["Hast", "al\u00b7le", "Br\u00f6s\u00b7lein", "auf\u00b7ge\u00b7pickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hast dann vergebens umgeblickt,", "tokens": ["Hast", "dann", "ver\u00b7ge\u00b7bens", "um\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo noch ein K\u00f6rnlein w\u00e4re!", "tokens": ["Wo", "noch", "ein", "K\u00f6rn\u00b7lein", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr andern V\u00f6glein allesammt,", "tokens": ["Ihr", "an\u00b7dern", "V\u00f6\u00b7glein", "al\u00b7le\u00b7sammt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl unterm blauen Himmel!", "tokens": ["Wohl", "un\u00b7term", "blau\u00b7en", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr habt mit Wehgesang verdammt", "tokens": ["Ihr", "habt", "mit", "Weh\u00b7ge\u00b7sang", "ver\u00b7dammt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Vogelsteller-L\u00fcmmel.", "tokens": ["Den", "Vo\u00b7gel\u00b7stel\u00b7ler\u00b7L\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach, Eines starb so balde, bald!", "tokens": ["Ach", ",", "Ei\u00b7nes", "starb", "so", "bal\u00b7de", ",", "bald", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "ADV", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eben da in Feld und Wald", "tokens": ["E\u00b7ben", "da", "in", "Feld", "und", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Fr\u00fchling wollte kommen.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "woll\u00b7te", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}