{"textgrid.poem.47596": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es d\u00fcnket dir, ich w\u00e4re nichts; ich aber]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es d\u00fcnket dir, ich w\u00e4re nichts; ich aber", "tokens": ["Es", "d\u00fcn\u00b7ket", "dir", ",", "ich", "w\u00e4\u00b7re", "nichts", ";", "ich", "a\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PIS", "$.", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Mann im Staat,", "tokens": ["Ein", "Mann", "im", "Staat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Mann von Rang, ich bin im Reich der Liebe", "tokens": ["Ein", "Mann", "von", "Rang", ",", "ich", "bin", "im", "Reich", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geheimer Rath.", "tokens": ["Ge\u00b7hei\u00b7mer", "Rath", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Dann bin ich auch im Kreise der Betrunknen", "tokens": ["Dann", "bin", "ich", "auch", "im", "Krei\u00b7se", "der", "Be\u00b7trunk\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Veteran,", "tokens": ["Ein", "Ve\u00b7te\u00b7ran", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der hier so manche Jahre schon die Wege", "tokens": ["Der", "hier", "so", "man\u00b7che", "Jah\u00b7re", "schon", "die", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Herrn betrat.", "tokens": ["Des", "Herrn", "be\u00b7trat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Es ist ein Ausbund adeliger Geister", "tokens": ["Es", "ist", "ein", "Aus\u00b7bund", "a\u00b7de\u00b7li\u00b7ger", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allhier vereint;", "tokens": ["All\u00b7hier", "ver\u00b7eint", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.3": {"text": "Ein jeder ist Prophete, Sofi, Seher", "tokens": ["Ein", "je\u00b7der", "ist", "Pro\u00b7phe\u00b7te", ",", "So\u00b7fi", ",", "Se\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "PIS", "VAFIN", "NN", "$,", "NE", "$,", "NN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und Potentat.", "tokens": ["Und", "Po\u00b7ten\u00b7tat", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sie sitzen und studiren freudestrahlend", "tokens": ["Sie", "sit\u00b7zen", "und", "stu\u00b7di\u00b7ren", "freu\u00b7des\u00b7trah\u00b7lend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Glase Dschem's,", "tokens": ["Im", "Gla\u00b7se", "Dschem's", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und besser ist, als das der Philosophen,", "tokens": ["Und", "bes\u00b7ser", "ist", ",", "als", "das", "der", "Phi\u00b7lo\u00b7so\u00b7phen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUS", "ART", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr Resultat.", "tokens": ["Ihr", "Re\u00b7sul\u00b7tat", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Doch fehlet einer und mit ihm die reinste", "tokens": ["Doch", "feh\u00b7let", "ei\u00b7ner", "und", "mit", "ihm", "die", "reins\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "KON", "APPR", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beseligung.", "tokens": ["Be\u00b7se\u00b7li\u00b7gung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.3": {"text": "Wo bleibt Hafis? Wo lieber, als bei'm Becher", "tokens": ["Wo", "bleibt", "Ha\u00b7fis", "?", "Wo", "lie\u00b7ber", ",", "als", "bei'm", "Be\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "NE", "$.", "PWAV", "ADV", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weilt er so spat?", "tokens": ["Weilt", "er", "so", "spat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Verschm\u00e4het er des Weines edle Labe", "tokens": ["Ver\u00b7schm\u00e4\u00b7het", "er", "des", "Wei\u00b7nes", "ed\u00b7le", "La\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil er entz\u00fcckt", "tokens": ["Weil", "er", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Von einer Huri-Lippe just den Nektar", "tokens": ["Von", "ei\u00b7ner", "Hu\u00b7ri\u00b7\u00b7Lip\u00b7pe", "just", "den", "Nek\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Der Huld empfaht?", "tokens": ["Der", "Huld", "em\u00b7pfaht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Doch nein, er kommt; er eilt mit raschem Fu\u00dfe", "tokens": ["Doch", "nein", ",", "er", "kommt", ";", "er", "eilt", "mit", "ra\u00b7schem", "Fu\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schenke zu;", "tokens": ["Der", "Schen\u00b7ke", "zu", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Jubelruf begr\u00fc\u00dft den alten Meister,", "tokens": ["Ein", "Ju\u00b7bel\u00b7ruf", "be\u00b7gr\u00fc\u00dft", "den", "al\u00b7ten", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wie er naht.", "tokens": ["So", "wie", "er", "naht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Ja, wenn du sehen willst, was ich bedeute,", "tokens": ["Ja", ",", "wenn", "du", "se\u00b7hen", "willst", ",", "was", "ich", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm in mein Reich,", "tokens": ["Komm", "in", "mein", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein jauchzendes! Du weigerst meiner Gr\u00f6\u00dfe", "tokens": ["Mein", "jauch\u00b7zen\u00b7des", "!", "Du", "wei\u00b7gerst", "mei\u00b7ner", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Attestat. \u2013", "tokens": ["Kein", "At\u00b7tes\u00b7tat", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Mir auf die Kutte deutest du, die alte,", "tokens": ["Mir", "auf", "die", "Kut\u00b7te", "deu\u00b7test", "du", ",", "die", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sch\u00e4bige?", "tokens": ["Die", "sch\u00e4\u00b7bi\u00b7ge", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du Ignorant! So prangen \u00e4chte Kaiser", "tokens": ["Du", "Ig\u00b7no\u00b7rant", "!", "So", "pran\u00b7gen", "\u00e4ch\u00b7te", "Kai\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$.", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Festornat.", "tokens": ["Im", "Fes\u00b7tor\u00b7nat", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}