{"textgrid.poem.63137": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "31", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man liest zu Hause meine B\u00fccher,", "tokens": ["Man", "liest", "zu", "Hau\u00b7se", "mei\u00b7ne", "B\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mancher freut sich meiner Schrift.", "tokens": ["Und", "man\u00b7cher", "freut", "sich", "mei\u00b7ner", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich decken schon die schwarzen T\u00fccher,", "tokens": ["Mich", "de\u00b7cken", "schon", "die", "schwar\u00b7zen", "T\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Lippen speien Gift.", "tokens": ["Und", "mei\u00b7ne", "Lip\u00b7pen", "spei\u00b7en", "Gift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Maulwurf nagt an meiner Wange,", "tokens": ["Der", "Maul\u00b7wurf", "nagt", "an", "mei\u00b7ner", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wurm betritt des Leibes Pflicht.", "tokens": ["Der", "Wurm", "be\u00b7tritt", "des", "Lei\u00b7bes", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon zerrt des ewigen Arztes Zange", "tokens": ["Schon", "zerrt", "des", "e\u00b7wi\u00b7gen", "Arz\u00b7tes", "Zan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Leidenden in neues Licht.", "tokens": ["Den", "Lei\u00b7den\u00b7den", "in", "neu\u00b7es", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}