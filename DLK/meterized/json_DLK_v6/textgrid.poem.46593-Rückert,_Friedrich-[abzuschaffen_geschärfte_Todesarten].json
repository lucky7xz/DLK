{"textgrid.poem.46593": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[abzuschaffen gesch\u00e4rfte Todesarten]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abzuschaffen gesch\u00e4rfte Todesarten,", "tokens": ["Ab\u00b7zu\u00b7schaf\u00b7fen", "ge\u00b7sch\u00e4rf\u00b7te", "To\u00b7des\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Abzustellen den Graus der Folterkammern,", "tokens": ["Ab\u00b7zu\u00b7stel\u00b7len", "den", "Graus", "der", "Fol\u00b7ter\u00b7kam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "War wohl unseren aufgekl\u00e4rten Zeiten", "tokens": ["War", "wohl", "un\u00b7se\u00b7ren", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Vorbehalten zu einem Ruhm. Doch leider", "tokens": ["Vor\u00b7be\u00b7hal\u00b7ten", "zu", "ei\u00b7nem", "Ruhm", ".", "Doch", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "KON", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Da\u00df unschuldige Menschenleben gleichwohl,", "tokens": ["Da\u00df", "un\u00b7schul\u00b7di\u00b7ge", "Men\u00b7schen\u00b7le\u00b7ben", "gleich\u00b7wohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "$,"], "meter": "-++--+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Von Krankheiten gespannt auf Folterbetten,", "tokens": ["Von", "Krank\u00b7hei\u00b7ten", "ge\u00b7spannt", "auf", "Fol\u00b7ter\u00b7bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Schwerem langsamem Tod entgegenschmachten!", "tokens": ["Schwe\u00b7rem", "lang\u00b7sa\u00b7mem", "Tod", "ent\u00b7ge\u00b7gen\u00b7schmach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ach wenn menschlicher auch die Menschen wurden,", "tokens": ["Ach", "wenn", "menschli\u00b7cher", "auch", "die", "Men\u00b7schen", "wur\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PIS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Unsre Mutter Natur, sie ist bei ihrer", "tokens": ["Uns\u00b7re", "Mut\u00b7ter", "Na\u00b7tur", ",", "sie", "ist", "bei", "ih\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Alten heiligen Barbarei geblieben.", "tokens": ["Al\u00b7ten", "hei\u00b7li\u00b7gen", "Bar\u00b7ba\u00b7rei", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}