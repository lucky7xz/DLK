{"textgrid.poem.55190": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "R\u00e4tsel", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel M\u00e4nner sind hoch zu verehren,", "tokens": ["Viel", "M\u00e4n\u00b7ner", "sind", "hoch", "zu", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wohlt\u00e4tige durch Werk und Lehren;", "tokens": ["Wohl\u00b7t\u00e4\u00b7ti\u00b7ge", "durch", "Werk", "und", "Leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wer uns zu erstatten wagt,", "tokens": ["Doch", "wer", "uns", "zu", "er\u00b7stat\u00b7ten", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was die Natur uns ganz versagt,", "tokens": ["Was", "die", "Na\u00b7tur", "uns", "ganz", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den darf ich wohl den gr\u00f6\u00dften nennen:", "tokens": ["Den", "darf", "ich", "wohl", "den", "gr\u00f6\u00df\u00b7ten", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich denke doch, ihr m\u00fc\u00dft ihn kennen?", "tokens": ["Ich", "den\u00b7ke", "doch", ",", "ihr", "m\u00fc\u00dft", "ihn", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}