{"dta.poem.1684": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin das reine Gla\u00df/ das noch kein Staub befleckt/", "tokens": ["Ich", "bin", "das", "rei\u00b7ne", "Gla\u00df", "/", "das", "noch", "kein", "Staub", "be\u00b7fleckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PDS", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lilje/ welche noch die Bienen nicht besessen/", "tokens": ["Die", "Lil\u00b7je", "/", "wel\u00b7che", "noch", "die", "Bie\u00b7nen", "nicht", "be\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Rose/ deren Blatt kein K\u00e4fer noch durchfressen/", "tokens": ["Die", "Ro\u00b7se", "/", "de\u00b7ren", "Blatt", "kein", "K\u00e4\u00b7fer", "noch", "durch\u00b7fres\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELAT", "NN", "PIAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Muschel-Kind/ das noch der Mutter Schale deckt/", "tokens": ["Das", "Mu\u00b7schel\u00b7Kind", "/", "das", "noch", "der", "Mut\u00b7ter", "Scha\u00b7le", "deckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Ziel/ auff welches noch kein fremder Pfeil gezweckt/", "tokens": ["Das", "Ziel", "/", "auff", "wel\u00b7ches", "noch", "kein", "frem\u00b7der", "Pfeil", "ge\u00b7zweckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "ADV", "PIAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der s\u00fcsse Freuden-Brunn/ den noch kein Bley gemessen/", "tokens": ["Der", "s\u00fcs\u00b7se", "Freu\u00b7den\u00b7Brunn", "/", "den", "noch", "kein", "Bley", "ge\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Nu\u00df/ aus der man kan das Lebens-Oele pressen.", "tokens": ["Die", "Nu\u00df", "/", "aus", "der", "man", "kan", "das", "Le\u00b7bens\u00b7O\u00b7ele", "pres\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PIS", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Schnecke/ welche noch die H\u00f6rner nie gestreckt/", "tokens": ["Die", "Schne\u00b7cke", "/", "wel\u00b7che", "noch", "die", "H\u00f6r\u00b7ner", "nie", "ge\u00b7streckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Tochter keuscher Scham/ der Unschuld liebstes Kind/", "tokens": ["Die", "Toch\u00b7ter", "keu\u00b7scher", "Scham", "/", "der", "Un\u00b7schuld", "liebs\u00b7tes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Aepffel-Bl\u00fctte/ die nun fertig auff zuspringen/", "tokens": ["Die", "A\u00b7e\u00b7pf\u00b7fel\u00b7B\u00b7l\u00fct\u00b7te", "/", "die", "nun", "fer\u00b7tig", "auff", "zu\u00b7sprin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "ADJD", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Das M\u00fcntzhau\u00df/ drinnen man Vergn\u00fcgungs-Sch\u00e4tze find/", "tokens": ["Das", "M\u00fcntz\u00b7hau\u00df", "/", "drin\u00b7nen", "man", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7Sch\u00e4t\u00b7ze", "find", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PIS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Festung/ welche durch ein einig Thor zu zwingen/", "tokens": ["Die", "Fes\u00b7tung", "/", "wel\u00b7che", "durch", "ein", "ei\u00b7nig", "Thor", "zu", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "APPR", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Liebe Feuer-Zeug/ die Artzney deren Krafft", "tokens": ["Der", "Lie\u00b7be", "Feu\u00b7er\u00b7Zeug", "/", "die", "Artz\u00b7ney", "de\u00b7ren", "Krafft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "PRELAT", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Auch Todte lebend macht/ die edle Jungfranschafft.", "tokens": ["Auch", "Tod\u00b7te", "le\u00b7bend", "macht", "/", "die", "ed\u00b7le", "Jung\u00b7fran\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "VVFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie geht mirs aber nun? man fl\u00f6st mir Nectar ein/", "tokens": ["Wie", "geht", "mirs", "a\u00b7ber", "nun", "?", "man", "fl\u00f6st", "mir", "Nec\u00b7tar", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "ADV", "ADV", "$.", "PIS", "VVFIN", "PPER", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Cupido will bey mir den Jungfern-Honig finden/", "tokens": ["Cu\u00b7pi\u00b7do", "will", "bey", "mir", "den", "Jung\u00b7fern\u00b7Ho\u00b7nig", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.17": {"text": "Der Liebe Brand mu\u00df mir die R\u00f6the mehr entz\u00fcnden/", "tokens": ["Der", "Lie\u00b7be", "Brand", "mu\u00df", "mir", "die", "R\u00f6\u00b7the", "mehr", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Perle kan auch in der See nicht sicher seyn/", "tokens": ["Die", "Per\u00b7le", "kan", "auch", "in", "der", "See", "nicht", "si\u00b7cher", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Bogen scharff bes\u00e4hnt zielt auff mein Helffenbein/", "tokens": ["Ein", "Bo\u00b7gen", "scharff", "be\u00b7s\u00e4hnt", "zielt", "auff", "mein", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was ich verborgen hielt/ will F\u00fcrwitz doch ergr\u00fcnden/", "tokens": ["Was", "ich", "ver\u00b7bor\u00b7gen", "hielt", "/", "will", "F\u00fcr\u00b7witz", "doch", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VVFIN", "$(", "VMFIN", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Die Liebes-Presse sucht aus mir den Safft zu winden/", "tokens": ["Die", "Lie\u00b7bes\u00b7Pres\u00b7se", "sucht", "aus", "mir", "den", "Safft", "zu", "win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mein Wohnhau\u00df wird best\u00fcrmt/ und w\u00e4r es noch so klein.", "tokens": ["Mein", "Wohn\u00b7hau\u00df", "wird", "be\u00b7st\u00fcrmt", "/", "und", "w\u00e4r", "es", "noch", "so", "klein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Mutter l\u00e4st ihr Kind/ und weichet selbst darvon/", "tokens": ["Die", "Mut\u00b7ter", "l\u00e4st", "ihr", "Kind", "/", "und", "wei\u00b7chet", "selbst", "dar\u00b7von", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Knospe platzet auff/ der Schatz wird mir gestohlen/", "tokens": ["Die", "Knos\u00b7pe", "plat\u00b7zet", "auff", "/", "der", "Schatz", "wird", "mir", "ge\u00b7stoh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$(", "ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Aus meiner Brustwehr wird des Uberwinders Thron/", "tokens": ["Aus", "mei\u00b7ner", "Brust\u00b7wehr", "wird", "des", "U\u00b7ber\u00b7win\u00b7ders", "Thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Venus-Zunder f\u00e4ngt und brennet endlich Kohlen/", "tokens": ["Der", "Ve\u00b7nus\u00b7Zun\u00b7der", "f\u00e4ngt", "und", "bren\u00b7net", "end\u00b7lich", "Koh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den Arzt ert\u00f6dtet selbst die fremd-erweckte Krafft/", "tokens": ["Den", "Arzt", "er\u00b7t\u00f6d\u00b7tet", "selbst", "die", "frem\u00b7der\u00b7weck\u00b7te", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und endlich stirbt dahin die edle Jungfrauschafft:", "tokens": ["Und", "end\u00b7lich", "stirbt", "da\u00b7hin", "die", "ed\u00b7le", "Jung\u00b7frausc\u00b7hafft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So hab ich viel Gefahr von aussen und von innen/", "tokens": ["So", "hab", "ich", "viel", "Ge\u00b7fahr", "von", "aus\u00b7sen", "und", "von", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "APPR", "VVINF", "KON", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und sehe wie der Tod schon auff der Zunge sizt/", "tokens": ["Und", "se\u00b7he", "wie", "der", "Tod", "schon", "auff", "der", "Zun\u00b7ge", "sizt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Poltzen/ welche man mich zu verletzen spizt/", "tokens": ["Die", "Polt\u00b7zen", "/", "wel\u00b7che", "man", "mich", "zu", "ver\u00b7let\u00b7zen", "spizt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "PRF", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die List/ durch welche man mich trachtet zu gewinnen/", "tokens": ["Die", "List", "/", "durch", "wel\u00b7che", "man", "mich", "trach\u00b7tet", "zu", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PIS", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die stets gen\u00e4hrte Glutt der angefeurten Sinnen/", "tokens": ["Die", "stets", "ge\u00b7n\u00e4hr\u00b7te", "Glutt", "der", "an\u00b7ge\u00b7feur\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Thr\u00e4nen/ welche man bey meinem Grabe schwizt/", "tokens": ["Die", "Thr\u00e4\u00b7nen", "/", "wel\u00b7che", "man", "bey", "mei\u00b7nem", "Gra\u00b7be", "schwizt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Vortheil/ welche man mich zu verletzen/ n\u00fczt.", "tokens": ["Die", "Vor\u00b7theil", "/", "wel\u00b7che", "man", "mich", "zu", "ver\u00b7let\u00b7zen", "/", "n\u00fczt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "PRF", "PTKZU", "VVINF", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sind Feinde die ja leicht mich Schwache f\u00e4lleu k\u00fcnnen.", "tokens": ["Sind", "Fein\u00b7de", "die", "ja", "leicht", "mich", "Schwa\u00b7che", "f\u00e4l\u00b7leu", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "ADV", "ADJD", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich mu\u00df/ doch will ich nicht gantz ungerochen sterben/", "tokens": ["Ich", "mu\u00df", "/", "doch", "will", "ich", "nicht", "gantz", "un\u00b7ge\u00b7ro\u00b7chen", "ster\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Den Sieger soll sein Schwei\u00df nebst meinem Blutte f\u00e4rben/", "tokens": ["Den", "Sie\u00b7ger", "soll", "sein", "Schwei\u00df", "nebst", "mei\u00b7nem", "Blut\u00b7te", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wird mir der Ring entf\u00fchrt/ die Lantze biegt vom Streit/", "tokens": ["Wird", "mir", "der", "Ring", "ent\u00b7f\u00fchrt", "/", "die", "Lant\u00b7ze", "biegt", "vom", "Streit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$(", "ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Man mu\u00df mir diesen Ruhm der Gro\u00dfmutt hinterlassen/", "tokens": ["Man", "mu\u00df", "mir", "die\u00b7sen", "Ruhm", "der", "Gro\u00df\u00b7mutt", "hin\