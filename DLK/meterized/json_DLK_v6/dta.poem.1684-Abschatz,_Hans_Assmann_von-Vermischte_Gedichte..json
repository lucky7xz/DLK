: "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Im Fall ich nicht bey dir zu lange Zeit gehafft/", "tokens": ["Im", "Fall", "ich", "nicht", "bey", "dir", "zu", "lan\u00b7ge", "Zeit", "ge\u00b7hafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "So la\u00df mir auff mein Grab mit steiffem Griffel schreiben/", "tokens": ["So", "la\u00df", "mir", "auff", "mein", "Grab", "mit", "steif\u00b7fem", "Grif\u00b7fel", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "ier liegt/ was m\u00fchens werth/ Julindens Jungfrauschafft.", "tokens": ["ier", "liegt", "/", "was", "m\u00fc\u00b7hens", "werth", "/", "Ju\u00b7lin\u00b7dens", "Jung\u00b7frausc\u00b7hafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWS", "ADV", "ADJD", "$(", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}