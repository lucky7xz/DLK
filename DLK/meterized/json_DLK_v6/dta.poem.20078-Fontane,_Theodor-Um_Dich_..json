{"dta.poem.20078": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Sag an \u201ees f\u00e4llt von Deinem Haupte", "tokens": ["Sag", "an", "\u201e", "es", "f\u00e4llt", "von", "Dei\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Haar, von welchem Gott nicht wei\u00df\u201c \u2014", "tokens": ["Kein", "Haar", ",", "von", "wel\u00b7chem", "Gott", "nicht", "wei\u00df", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "PWAT", "NN", "PTKNEG", "VVFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was der Tag uns Gr\u00f6\u00dfres raubte,", "tokens": ["Und", "was", "der", "Tag", "uns", "Gr\u00f6\u00df\u00b7res", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das fiele nicht auf Sein Gehei\u00df?!", "tokens": ["Das", "fie\u00b7le", "nicht", "auf", "Sein", "Ge\u00b7hei\u00df", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Trag es, wenn seinen Schnee der Winter", "tokens": ["Trag", "es", ",", "wenn", "sei\u00b7nen", "Schnee", "der", "Win\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In unser Hoffen niederstiebt,", "tokens": ["In", "un\u00b7ser", "Hof\u00b7fen", "nie\u00b7ders\u00b7tiebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ganzer Fr\u00fchling lacht dahinter:", "tokens": ["Ein", "gan\u00b7zer", "Fr\u00fch\u00b7ling", "lacht", "da\u00b7hin\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott z\u00fcchtigt immer, wen er liebt.", "tokens": ["Gott", "z\u00fcch\u00b7tigt", "im\u00b7mer", ",", "wen", "er", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df in dem Leid, das Er beschieden,", "tokens": ["La\u00df", "in", "dem", "Leid", ",", "das", "Er", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Keim uns k\u00fcnftgen Gl\u00fcckes schaun,", "tokens": ["Den", "Keim", "uns", "k\u00fcnft\u00b7gen", "Gl\u00fc\u00b7ckes", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann kann der Tag, wo Freud und Frieden", "tokens": ["Dann", "kann", "der", "Tag", ",", "wo", "Freud", "und", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unsrem Herzen H\u00fctten baun.", "tokens": ["In", "uns\u00b7rem", "Her\u00b7zen", "H\u00fct\u00b7ten", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}