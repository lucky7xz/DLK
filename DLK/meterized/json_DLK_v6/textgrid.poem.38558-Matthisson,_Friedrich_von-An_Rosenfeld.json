{"textgrid.poem.38558": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Rosenfeld", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trauter! dessen Bruderhand", "tokens": ["Trau\u00b7ter", "!", "des\u00b7sen", "Bru\u00b7der\u00b7hand"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Jugend Feenland,", "tokens": ["Durch", "der", "Ju\u00b7gend", "Fe\u00b7en\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches leichtbeschwingte Jahr,", "tokens": ["Man\u00b7ches", "leicht\u00b7be\u00b7schwing\u00b7te", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trost und Schuz und Stab mir war;", "tokens": ["Trost", "und", "Schuz", "und", "Stab", "mir", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dessen Auge sich ergo\u00df,", "tokens": ["Des\u00b7sen", "Au\u00b7ge", "sich", "er\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mir Nacht die Seel' umflo\u00df,", "tokens": ["Wenn", "mir", "Nacht", "die", "Seel'", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen Brust, wenn Freude quoll,", "tokens": ["Des\u00b7sen", "Brust", ",", "wenn", "Freu\u00b7de", "quoll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sympathetisch \u00fcberschwoll;", "tokens": ["Sym\u00b7pa\u00b7the\u00b7tisch", "\u00fc\u00b7bersc\u00b7hwoll", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau! der Trennung Stunde blikt", "tokens": ["Schau", "!", "der", "Tren\u00b7nung", "Stun\u00b7de", "blikt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcrchterlich hernieder, z\u00fckt", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "her\u00b7nie\u00b7der", ",", "z\u00fckt"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "PTKVZ", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon den Seelendolch nach mir,", "tokens": ["Schon", "den", "See\u00b7len\u00b7dolch", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fernt, du Lieber, mich von dir!", "tokens": ["Fernt", ",", "du", "Lie\u00b7ber", ",", "mich", "von", "dir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJD", "$,", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Allgewaltig gr\u00e4bt der Schmerz", "tokens": ["All\u00b7ge\u00b7wal\u00b7tig", "gr\u00e4bt", "der", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wund' auf Wunde mir ins Herz,", "tokens": ["Wund'", "auf", "Wun\u00b7de", "mir", "ins", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie zu heilen, ach! vermag", "tokens": ["Sie", "zu", "hei\u00b7len", ",", "ach", "!", "ver\u00b7mag"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ITJ", "$.", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur des Wiedersehens Tag!", "tokens": ["Nur", "des", "Wie\u00b7der\u00b7se\u00b7hens", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn in \u00f6der Ferne nun,", "tokens": ["Wenn", "in", "\u00f6\u00b7der", "Fer\u00b7ne", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Freundes Arm zu ruhn,", "tokens": ["In", "des", "Freun\u00b7des", "Arm", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den kein Erdenlied besingt,", "tokens": ["Den", "kein", "Er\u00b7den\u00b7lied", "be\u00b7singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine ganze Seele ringt:", "tokens": ["Mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "ringt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Webe dann sein Angesicht,", "tokens": ["We\u00b7be", "dann", "sein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Phantasie! aus Mondenlicht,", "tokens": ["Phan\u00b7ta\u00b7sie", "!", "aus", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Blik aus Aetherblau,", "tokens": ["Sei\u00b7nen", "Blik", "aus", "A\u00b7e\u00b7ther\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mir zur s\u00fc\u00dfen Wonneschau!", "tokens": ["Mir", "zur", "s\u00fc\u00b7\u00dfen", "Won\u00b7ne\u00b7schau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df in d\u00fcstrer Trennungsnacht,", "tokens": ["Da\u00df", "in", "d\u00fcst\u00b7rer", "Tren\u00b7nungs\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo kein Stern der Freude lacht,", "tokens": ["Wo", "kein", "Stern", "der", "Freu\u00b7de", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch sein L\u00e4cheln himmelan", "tokens": ["Noch", "sein", "L\u00e4\u00b7cheln", "him\u00b7me\u00b7lan"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Seele fl\u00fcgeln kann!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "fl\u00fc\u00b7geln", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Horch! Geliebter, da umscholl", "tokens": ["Horch", "!", "Ge\u00b7lieb\u00b7ter", ",", "da", "um\u00b7scholl"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$.", "NN", "$,", "KOUS", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich mein leztes Lebewohl!", "tokens": ["Dich", "mein", "lez\u00b7tes", "Le\u00b7be\u00b7wohl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dank f\u00fcr jede Wonne, Dank \u2013", "tokens": ["Dank", "f\u00fcr", "je\u00b7de", "Won\u00b7ne", ",", "Dank", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "APPR", "PIAT", "NN", "$,", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier versieg', o mein Gesang!", "tokens": ["Hier", "ver\u00b7sieg'", ",", "o", "mein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}