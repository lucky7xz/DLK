{"dta.poem.16188": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "242.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Glauben gehst du aus, und kehrst zur\u00fcck zum Glauben;", "tokens": ["Vom", "Glau\u00b7ben", "gehst", "du", "aus", ",", "und", "kehrst", "zu\u00b7r\u00fcck", "zum", "Glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zweifel steht am Weg, die Ruhe dir zu rauben.", "tokens": ["Der", "Zwei\u00b7fel", "steht", "am", "Weg", ",", "die", "Ru\u00b7he", "dir", "zu", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gehst du ihm aus dem Weg? er ist auf allen Wegen,", "tokens": ["Gehst", "du", "ihm", "aus", "dem", "Weg", "?", "er", "ist", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In anderer Gestalt tritt er dir dort entgegen.", "tokens": ["In", "an\u00b7de\u00b7rer", "Ge\u00b7stalt", "tritt", "er", "dir", "dort", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum flieh nicht vor dem Feind, und such' ihn auch nicht auf;", "tokens": ["Drum", "flieh", "nicht", "vor", "dem", "Feind", ",", "und", "such'", "ihn", "auch", "nicht", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo er dir aufst\u00f6\u00dft, r\u00e4um' ihn fort aus deinem Lauf!", "tokens": ["Wo", "er", "dir", "auf\u00b7st\u00f6\u00dft", ",", "r\u00e4um'", "ihn", "fort", "aus", "dei\u00b7nem", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bek\u00e4mpfen mu\u00dft du ihn, du mu\u00dft ihn \u00fcberwinden,", "tokens": ["Be\u00b7k\u00e4mp\u00b7fen", "mu\u00dft", "du", "ihn", ",", "du", "mu\u00dft", "ihn", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willst du durch sein Gebiet den Weg zur Wahrheit finden.", "tokens": ["Willst", "du", "durch", "sein", "Ge\u00b7biet", "den", "Weg", "zur", "Wahr\u00b7heit", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du zweifelst nicht, weil du geworden weiser bist;", "tokens": ["Du", "zwei\u00b7felst", "nicht", ",", "weil", "du", "ge\u00b7wor\u00b7den", "wei\u00b7ser", "bist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VAPP", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du zweifelst, weil noch reif nicht deine Weisheit ist.", "tokens": ["Du", "zwei\u00b7felst", ",", "weil", "noch", "reif", "nicht", "dei\u00b7ne", "Weis\u00b7heit", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ADJD", "PTKNEG", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Zweifel ist die H\u00fcll', in der die Frucht soll reifen,", "tokens": ["Der", "Zwei\u00b7fel", "ist", "die", "H\u00fcll'", ",", "in", "der", "die", "Frucht", "soll", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die gereifte Frucht wird ihre H\u00fcll' abstreifen.", "tokens": ["Und", "die", "ge\u00b7reif\u00b7te", "Frucht", "wird", "ih\u00b7re", "H\u00fcll'", "ab\u00b7strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}