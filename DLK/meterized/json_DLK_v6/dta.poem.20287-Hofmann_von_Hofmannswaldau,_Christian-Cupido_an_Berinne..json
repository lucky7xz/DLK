{"dta.poem.20287": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Cupido an Berinne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus meiner mutter mund ist dieser brieff geflossen/", "tokens": ["Aus", "mei\u00b7ner", "mut\u00b7ter", "mund", "ist", "die\u00b7ser", "brieff", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So dich/ Berinne/ mehr als ihre tauben liebt/", "tokens": ["So", "dich", "/", "Be\u00b7rin\u00b7ne", "/", "mehr", "als", "ih\u00b7re", "tau\u00b7ben", "liebt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "NE", "$(", "PIAT", "KOKOM", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch ihren segen ist dein warmer schnee entsprossen/", "tokens": ["Durch", "ih\u00b7ren", "se\u00b7gen", "ist", "dein", "war\u00b7mer", "schnee", "ent\u00b7spros\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem iede schwanen-brust sich gantz gefangen giebt.", "tokens": ["Dem", "ie\u00b7de", "schwa\u00b7nen\u00b7brust", "sich", "gantz", "ge\u00b7fan\u00b7gen", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist der mutter wort/ was ich dir \u00fcbersende/", "tokens": ["Es", "ist", "der", "mut\u00b7ter", "wort", "/", "was", "ich", "dir", "\u00fc\u00b7ber\u00b7sen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PWS", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich hab es nur aus lust in diese reimen bracht.", "tokens": ["Ich", "hab", "es", "nur", "aus", "lust", "in", "die\u00b7se", "rei\u00b7men", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was nimmt nicht eine frau mit freuden in die h\u00e4nde/", "tokens": ["Was", "nimmt", "nicht", "ei\u00b7ne", "frau", "mit", "freu\u00b7den", "in", "die", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was nach der Venus wunsch Cupido fertig macht.", "tokens": ["Was", "nach", "der", "Ve\u00b7nus", "wunsch", "Cu\u00b7pi\u00b7do", "fer\u00b7tig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJD", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dir ist nicht unbekandt/ was man die liebe nennet/", "tokens": ["Dir", "ist", "nicht", "un\u00b7be\u00b7kandt", "/", "was", "man", "die", "lie\u00b7be", "nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "PWS", "PIS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es ist vor deinen geist nicht eine fremde glut;", "tokens": ["Es", "ist", "vor", "dei\u00b7nen", "geist", "nicht", "ei\u00b7ne", "frem\u00b7de", "glut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du hast viel angesteckt/ und bist auch selbst entbrennet/", "tokens": ["Du", "hast", "viel", "an\u00b7ge\u00b7steckt", "/", "und", "bist", "auch", "selbst", "ent\u00b7bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$(", "KON", "VAFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und kennest/ was mein pfeil vor grosse wunder thut;", "tokens": ["Und", "ken\u00b7nest", "/", "was", "mein", "pfeil", "vor", "gros\u00b7se", "wun\u00b7der", "thut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du weist/ da\u00df menschen sich nicht recht entmenschen k\u00f6nnen/", "tokens": ["Du", "weist", "/", "da\u00df", "men\u00b7schen", "sich", "nicht", "recht", "ent\u00b7men\u00b7schen", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIS", "PRF", "PTKNEG", "ADJD", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und die begierde sie als alte kinder wiegt;", "tokens": ["Und", "die", "be\u00b7gier\u00b7de", "sie", "als", "al\u00b7te", "kin\u00b7der", "wiegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich wei\u00df/ so gut als du/ den zunder deiner sinnen/", "tokens": ["Ich", "wei\u00df", "/", "so", "gut", "als", "du", "/", "den", "zun\u00b7der", "dei\u00b7ner", "sin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADJD", "KOKOM", "PPER", "$(", "ART", "NN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und da\u00df nicht ei\u00df und stahl dir um das hertze liegt.", "tokens": ["Und", "da\u00df", "nicht", "ei\u00df", "und", "stahl", "dir", "um", "das", "hert\u00b7ze", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "ADJD", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich habe dir zun\u00e4chst zwey schreiben wegger\u00fccket/", "tokens": ["Ich", "ha\u00b7be", "dir", "zu\u00b7n\u00e4chst", "zwey", "schrei\u00b7ben", "weg\u00b7ge\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "CARD", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So an den Criton du hast zierlich auffgesetzt;", "tokens": ["So", "an", "den", "Cri\u00b7ton", "du", "hast", "zier\u00b7lich", "auff\u00b7ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich habe sie aus pflicht der mutter zugeschicket/", "tokens": ["Ich", "ha\u00b7be", "sie", "aus", "pflicht", "der", "mut\u00b7ter", "zu\u00b7ge\u00b7schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So sie von gr\u00f6ssern werth als ihre perlen sch\u00e4tzt.", "tokens": ["So", "sie", "von", "gr\u00f6s\u00b7sern", "werth", "als", "ih\u00b7re", "per\u00b7len", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "ADJD", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie hat sie alle zwey in einen schrein verschlossen/", "tokens": ["Sie", "hat", "sie", "al\u00b7le", "zwey", "in", "ei\u00b7nen", "schrein", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "CARD", "APPR", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dahin der diamant nur will verwahret seyn.", "tokens": ["Da\u00b7hin", "der", "di\u00b7a\u00b7mant", "nur", "will", "ver\u00b7wah\u00b7ret", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie sprach: Berinn\u2019 ist selbst in diesen brieff geflossen/", "tokens": ["Sie", "sprach", ":", "Be\u00b7rinn'", "ist", "selbst", "in", "die\u00b7sen", "brieff", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VAFIN", "ADV", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und druckt ihr ebenbild den sch\u00f6nen worten ein/", "tokens": ["Und", "druckt", "ihr", "e\u00b7ben\u00b7bild", "den", "sch\u00f6\u00b7nen", "wor\u00b7ten", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich wei\u00df die edle glut nicht hoch genug zu preisen/", "tokens": ["Ich", "wei\u00df", "die", "ed\u00b7le", "glut", "nicht", "hoch", "ge\u00b7nug", "zu", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es steht das lieben dir ja gar zu zierlich an:", "tokens": ["Es", "steht", "das", "lie\u00b7ben", "dir", "ja", "gar", "zu", "zier\u00b7lich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "PPER", "ADV", "ADV", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und du/ Berinne/ kanst in einem spiegel weisen/", "tokens": ["Und", "du", "/", "Be\u00b7rin\u00b7ne", "/", "kanst", "in", "ei\u00b7nem", "spie\u00b7gel", "wei\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "NE", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie gold und lieb allein im feuer dauren kan.", "tokens": ["Wie", "gold", "und", "lieb", "al\u00b7lein", "im", "feu\u00b7er", "dau\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dein unbefleckter mund verschencket keine k\u00fcsse/", "tokens": ["Dein", "un\u00b7be\u00b7fleck\u00b7ter", "mund", "ver\u00b7schen\u00b7cket", "kei\u00b7ne", "k\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Daran nicht trinckbar gold und lebens-nectar klebt/", "tokens": ["Da\u00b7ran", "nicht", "trin\u00b7ck\u00b7bar", "gold", "und", "le\u00b7bens\u00b7\u00b7nec\u00b7tar", "klebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ADJD", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Es ist die s\u00fc\u00dfigkeit vor ihnen selbst nicht s\u00fcsse/", "tokens": ["Es", "ist", "die", "s\u00fc\u00b7\u00dfig\u00b7keit", "vor", "ih\u00b7nen", "selbst", "nicht", "s\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Weil mehr als honigseim auff deinen lippen schwebt.", "tokens": ["Weil", "mehr", "als", "ho\u00b7ni\u00b7gseim", "auff", "dei\u00b7nen", "lip\u00b7pen", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die schnelle zauberey/ so du im munde f\u00fchrest/", "tokens": ["Die", "schnel\u00b7le", "zau\u00b7be\u00b7rey", "/", "so", "du", "im", "mun\u00b7de", "f\u00fch\u00b7rest", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPER", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Macht/ da\u00df dich Criton mehr als seine seele liebt/", "tokens": ["Macht", "/", "da\u00df", "dich", "Cri\u00b7ton", "mehr", "als", "sei\u00b7ne", "see\u00b7le", "liebt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "NE", "PIAT", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Da\u00df er/ so bald du nur die lippen ihm ber\u00fchrest", "tokens": ["Da\u00df", "er", "/", "so", "bald", "du", "nur", "die", "lip\u00b7pen", "ihm", "be\u00b7r\u00fch\u00b7rest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "ADV", "ADV", "PPER", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sich selber ihm entzieht/ und dir sich eigen giebt.", "tokens": ["Sich", "sel\u00b7ber", "ihm", "ent\u00b7zieht", "/", "und", "dir", "sich", "ei\u00b7gen", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPER", "VVFIN", "$(", "KON", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der menschen liebe wird von vieler art gefunden/", "tokens": ["Der", "men\u00b7schen", "lie\u00b7be", "wird", "von", "vie\u00b7ler", "art", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Doch deucht mich/ diese sey viel mehr als andre werth/", "tokens": ["Doch", "deucht", "mich", "/", "die\u00b7se", "sey", "viel", "mehr", "als", "and\u00b7re", "werth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "PDS", "VAFIN", "ADV", "ADV", "KOUS", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dieweil die tugend selbst den brandzeug hat gebunden/", "tokens": ["Die\u00b7weil", "die", "tu\u00b7gend", "selbst", "den", "brand\u00b7zeug", "hat", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und diese reine glut durch ihren schweffel nehrt.", "tokens": ["Und", "die\u00b7se", "rei\u00b7ne", "glut", "durch", "ih\u00b7ren", "schwef\u00b7fel", "nehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du weist/ als Criton dir den ersten ku\u00df gegeben/", "tokens": ["Du", "weist", "/", "als", "Cri\u00b7ton", "dir", "den", "ers\u00b7ten", "ku\u00df", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da\u00df dieses keusche wort aus seinen lippen brach:", "tokens": ["Da\u00df", "die\u00b7ses", "keu\u00b7sche", "wort", "aus", "sei\u00b7nen", "lip\u00b7pen", "brach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich will allhier veracht/ und dort verdammet leben/", "tokens": ["Ich", "will", "all\u00b7hier", "ver\u00b7acht", "/", "und", "dort", "ver\u00b7dam\u00b7met", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "$(", "KON", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Rennt meines geistes trieb verbotnen l\u00fcsten nach!", "tokens": ["Rennt", "mei\u00b7nes", "geis\u00b7tes", "trieb", "ver\u00b7bot\u00b7nen", "l\u00fcs\u00b7ten", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "VVFIN", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich f\u00fchl in meiner brust die allerreinsten flammen/", "tokens": ["Ich", "f\u00fchl", "in", "mei\u00b7ner", "brust", "die", "al\u00b7ler\u00b7reins\u00b7ten", "flam\u00b7men", "/"], "token_info": [