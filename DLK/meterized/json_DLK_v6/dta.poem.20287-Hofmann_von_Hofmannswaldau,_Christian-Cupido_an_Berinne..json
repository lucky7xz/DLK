er": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und wo der abend nicht den tag zuschanden macht.", "tokens": ["Und", "wo", "der", "a\u00b7bend", "nicht", "den", "tag", "zu\u00b7schan\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADV", "PTKNEG", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wer seinen hunger nicht mit voller kost bestillet/", "tokens": ["Wer", "sei\u00b7nen", "hun\u00b7ger", "nicht", "mit", "vol\u00b7ler", "kost", "be\u00b7stil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Der wird der s\u00fc\u00dfigkeit alleine werth geacht.", "tokens": ["Der", "wird", "der", "s\u00fc\u00b7\u00dfig\u00b7keit", "al\u00b7lei\u00b7ne", "werth", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ich will hink\u00fcnfftig dich als meine schwester lieben/", "tokens": ["Ich", "will", "hin\u00b7k\u00fcnff\u00b7tig", "dich", "als", "mei\u00b7ne", "schwes\u00b7ter", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "KOUS", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Mit der ein bruder stets vern\u00fcnfftig schertzen mu\u00df;", "tokens": ["Mit", "der", "ein", "bru\u00b7der", "stets", "ver\u00b7n\u00fcnff\u00b7tig", "schert\u00b7zen", "mu\u00df", ";"], "token_info"