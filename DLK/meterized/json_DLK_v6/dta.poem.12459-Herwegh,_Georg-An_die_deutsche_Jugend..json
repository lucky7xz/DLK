{"dta.poem.12459": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die deutsche Jugend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr spottet unser, stolze W\u00fcrdentr\u00e4ger?", "tokens": ["Ihr", "spot\u00b7tet", "un\u00b7ser", ",", "stol\u00b7ze", "W\u00fcr\u00b7den\u00b7tr\u00e4\u00b7ger", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Baut nicht zu viel auf Euer Ahnenschild!", "tokens": ["Baut", "nicht", "zu", "viel", "auf", "Eu\u00b7er", "Ah\u00b7nen\u00b7schild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKA", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht noch ", "tokens": ["Viel\u00b7leicht", "noch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "++-", "measure": "unknown.measure.di"}, "line.4": {"text": "Vielleicht schon morgen das gejagte Wild!", "tokens": ["Viel\u00b7leicht", "schon", "mor\u00b7gen", "das", "ge\u00b7jag\u00b7te", "Wild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit manchem Worte wollt' Er Euch bedeuten,", "tokens": ["Mit", "man\u00b7chem", "Wor\u00b7te", "wollt'", "Er", "Euch", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit manchem Wort zu Frommen Euch und Nutz:", "tokens": ["Mit", "man\u00b7chem", "Wort", "zu", "From\u00b7men", "Euch", "und", "Nutz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr aber ", "tokens": ["Ihr", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Nimm, deutsche Jugend, nimm sein Lied in Schutz!", "tokens": ["Nimm", ",", "deut\u00b7sche", "Ju\u00b7gend", ",", "nimm", "sein", "Lied", "in", "Schutz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVIMP", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich spielte freilich nur auf Einer Saite,", "tokens": ["Ich", "spiel\u00b7te", "frei\u00b7lich", "nur", "auf", "Ei\u00b7ner", "Sai\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Euch, erlauchte Herren, stets mi\u00dff\u00e4llt:", "tokens": ["Die", "Euch", ",", "er\u00b7lauch\u00b7te", "Her\u00b7ren", ",", "stets", "mi\u00df\u00b7f\u00e4llt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch rief nicht ", "tokens": ["Doch", "rief", "nicht"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Zum Streite ruft der neue Geist der Welt!", "tokens": ["Zum", "Strei\u00b7te", "ruft", "der", "neu\u00b7e", "Geist", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jauchzt das Volk und schwingt es seine M\u00fctzen,", "tokens": ["Und", "jauchzt", "das", "Volk", "und", "schwingt", "es", "sei\u00b7ne", "M\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wollt Ihr den Leiermann drum \u00e4chten? Thut's!", "tokens": ["Wollt", "Ihr", "den", "Lei\u00b7er\u00b7mann", "drum", "\u00e4ch\u00b7ten", "?", "Thut's", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PAV", "VVINF", "$.", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Adler wei\u00df die Nachtigall zu sch\u00fctzen \u2014", "tokens": ["Der", "Ad\u00b7ler", "wei\u00df", "die", "Nach\u00b7ti\u00b7gall", "zu", "sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nimm, deutsche Jugend, unser Lied in Schutz!", "tokens": ["Nimm", ",", "deut\u00b7sche", "Ju\u00b7gend", ",", "un\u00b7ser", "Lied", "in", "Schutz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Leicht k\u00f6nnen wir der F\u00fcrsten Gunst entbehren", "tokens": ["Leicht", "k\u00f6n\u00b7nen", "wir", "der", "F\u00fcrs\u00b7ten", "Gunst", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Sie soll mich auch in Zukunft singen lehren,", "tokens": ["Sie", "soll", "mich", "auch", "in", "Zu\u00b7kunft", "sin\u00b7gen", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mir die Hand zum ersten Lied gef\u00fchrt;", "tokens": ["Die", "mir", "die", "Hand", "zum", "ers\u00b7ten", "Lied", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "All' meine Sch\u00e4tze leg' ich ihr zu F\u00fc\u00dfen:", "tokens": ["All'", "mei\u00b7ne", "Sch\u00e4t\u00b7ze", "leg'", "ich", "ihr", "zu", "F\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Ja wohl! ich werd' ihr Sklave bleiben m\u00fcssen, \u2014", "tokens": ["Ja", "wohl", "!", "ich", "werd'", "ihr", "Skla\u00b7ve", "blei\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "ADV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "VMINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nimm, deutsche Jugend, nimm mein Lied in Schutz!", "tokens": ["Nimm", ",", "deut\u00b7sche", "Ju\u00b7gend", ",", "nimm", "mein", "Lied", "in", "Schutz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVIMP", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie, die kein Wetter aus dem Schlafe r\u00fcttelt,", "tokens": ["Sie", ",", "die", "kein", "Wet\u00b7ter", "aus", "dem", "Schla\u00b7fe", "r\u00fct\u00b7telt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Treibhauspflanzen, die ein M\u00e4dchen hegt,", "tokens": ["Die", "Treib\u00b7haus\u00b7pflan\u00b7zen", ",", "die", "ein", "M\u00e4d\u00b7chen", "hegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df der Sturm die Br\u00fcder drau\u00dfen sch\u00fcttelt:", "tokens": ["In\u00b7de\u00df", "der", "Sturm", "die", "Br\u00fc\u00b7der", "drau\u00b7\u00dfen", "sch\u00fct\u00b7telt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du l\u00e4chelst ob der Demuth unsrer Alten,", "tokens": ["Du", "l\u00e4\u00b7chelst", "ob", "der", "De\u00b7muth", "uns\u00b7rer", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und willst nur Zorn und k\u00fchner Worte Trutz;", "tokens": ["Und", "willst", "nur", "Zorn", "und", "k\u00fch\u00b7ner", "Wor\u00b7te", "Trutz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zwar hinkt mein Vers, doch ist er ohne ", "tokens": ["Zwar", "hinkt", "mein", "Vers", ",", "doch", "ist", "er", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nimm, deutsche Jugend, nimm mein Lied in Schutz!", "tokens": ["Nimm", ",", "deut\u00b7sche", "Ju\u00b7gend", ",", "nimm", "mein", "Lied", "in", "Schutz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVIMP", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gleich wie die Lerche gr\u00fc\u00dft den ersten Funken,", "tokens": ["Gleich", "wie", "die", "Ler\u00b7che", "gr\u00fc\u00dft", "den", "ers\u00b7ten", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der aus dem Aug' des jungen Tages bricht:", "tokens": ["Der", "aus", "dem", "Aug'", "des", "jun\u00b7gen", "Ta\u00b7ges", "bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So macht ein Strahl von Hoffnung schon mich trunken,", "tokens": ["So", "macht", "ein", "Strahl", "von", "Hoff\u00b7nung", "schon", "mich", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich brauch' die Sonne der Erf\u00fcllung nicht.", "tokens": ["Ich", "brauch'", "die", "Son\u00b7ne", "der", "Er\u00b7f\u00fcl\u00b7lung", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201ees ", "tokens": ["\u201e", "es"], "token_info": ["punct", "word"], "pos": ["$(", "PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Schriebst Du nicht also, mein geliebter Prutz?", "tokens": ["Schriebst", "Du", "nicht", "al\u00b7so", ",", "mein", "ge\u00b7lieb\u00b7ter", "Prutz", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kein Korn der Freiheit darf verloren gehen \u2014", "tokens": ["Kein", "Korn", "der", "Frei\u00b7heit", "darf", "ver\u00b7lo\u00b7ren", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nimm, deutsche Jugend, unser Lied in Schutz!", "tokens": ["Nimm", ",", "deut\u00b7sche", "Ju\u00b7gend", ",", "un\u00b7ser", "Lied", "in", "Schutz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}