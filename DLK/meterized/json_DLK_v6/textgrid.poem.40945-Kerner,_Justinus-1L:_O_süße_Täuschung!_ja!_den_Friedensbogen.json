{"textgrid.poem.40945": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: O s\u00fc\u00dfe T\u00e4uschung! ja! den Friedensbogen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O s\u00fc\u00dfe T\u00e4uschung! ja! den Friedensbogen", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "T\u00e4u\u00b7schung", "!", "ja", "!", "den", "Frie\u00b7dens\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "PTKANT", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du wohl oft ums kampfesm\u00fcde Haupt,", "tokens": ["Hast", "du", "wohl", "oft", "ums", "kamp\u00b7fes\u00b7m\u00fc\u00b7de", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich nicht mehr gehoffet und geglaubt,", "tokens": ["Wenn", "ich", "nicht", "mehr", "ge\u00b7hof\u00b7fet", "und", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Engel, mir mit milder Hand gezogen.", "tokens": ["Ein", "En\u00b7gel", ",", "mir", "mit", "mil\u00b7der", "Hand", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie man \u00d6l gie\u00dft in die st\u00fcrm'schen Wogen", "tokens": ["Und", "wie", "man", "\u00d6l", "gie\u00dft", "in", "die", "st\u00fcr\u00b7m'\u00b7schen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Des Meeres, da\u00df sich lege ihre Wut,", "tokens": ["Des", "Mee\u00b7res", ",", "da\u00df", "sich", "le\u00b7ge", "ih\u00b7re", "Wut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.7": {"text": "So gossest du mir oft ins st\u00fcrm'sche Blut", "tokens": ["So", "gos\u00b7sest", "du", "mir", "oft", "ins", "st\u00fcr\u00b7m'\u00b7sche", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Ein \u00d6l, das es zur Ruhe hat bewogen.", "tokens": ["Ein", "\u00d6l", ",", "das", "es", "zur", "Ru\u00b7he", "hat", "be\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch sieh! der Grundton meines Lebens ist", "tokens": ["Doch", "sieh", "!", "der", "Grund\u00b7ton", "mei\u00b7nes", "Le\u00b7bens", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Schmerz, den du mir scheinbar nur entrissen,", "tokens": ["Der", "Schmerz", ",", "den", "du", "mir", "schein\u00b7bar", "nur", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Im Innern fort der Born des Schmerzes flie\u00dft,", "tokens": ["Im", "In\u00b7nern", "fort", "der", "Born", "des", "Schmer\u00b7zes", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wenn au\u00dfen auch die Lippen l\u00e4cheln m\u00fcssen.", "tokens": ["Wenn", "au\u00b7\u00dfen", "auch", "die", "Lip\u00b7pen", "l\u00e4\u00b7cheln", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mein kleines Lied, das nur des Schmerzens Kind,", "tokens": ["Mein", "klei\u00b7nes", "Lied", ",", "das", "nur", "des", "Schmer\u00b7zens", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wird wie der Born des Schmerzens niemals stocken,", "tokens": ["Wird", "wie", "der", "Born", "des", "Schmer\u00b7zens", "nie\u00b7mals", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wird t\u00f6nen fort, verhallend in die Glocken,", "tokens": ["Wird", "t\u00f6\u00b7nen", "fort", ",", "ver\u00b7hal\u00b7lend", "in", "die", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PTKVZ", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die euch Verk\u00fcnd'ger meines Todes sind.", "tokens": ["Die", "euch", "Ver\u00b7k\u00fcn\u00b7d'\u00b7ger", "mei\u00b7nes", "To\u00b7des", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}