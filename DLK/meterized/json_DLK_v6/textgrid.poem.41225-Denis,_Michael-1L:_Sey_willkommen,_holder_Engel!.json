{"textgrid.poem.41225": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sey willkommen, holder Engel!", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey willkommen, holder Engel!", "tokens": ["Sey", "will\u00b7kom\u00b7men", ",", "hol\u00b7der", "En\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der ersten Lebensbl\u00fcthe.", "tokens": ["In", "der", "ers\u00b7ten", "Le\u00b7bens\u00b7bl\u00fct\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kleines, allerliebstes Wesen!", "tokens": ["Klei\u00b7nes", ",", "al\u00b7ler\u00b7liebs\u00b7tes", "We\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey dem Freunde deines Vaters", "tokens": ["Sey", "dem", "Freun\u00b7de", "dei\u00b7nes", "Va\u00b7ters"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend, tausendmal willkommen!", "tokens": ["Tau\u00b7send", ",", "tau\u00b7send\u00b7mal", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["CARD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Edler Hauch der Gottheit! lerne", "tokens": ["Ed\u00b7ler", "Hauch", "der", "Got\u00b7theit", "!", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner zarten K\u00f6rperh\u00fclle", "tokens": ["Dei\u00b7ner", "zar\u00b7ten", "K\u00f6r\u00b7per\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach und nach dich zu gew\u00f6hnen!", "tokens": ["Nach", "und", "nach", "dich", "zu", "ge\u00b7w\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Blicke bald nach deiner Mutter,", "tokens": ["Bli\u00b7cke", "bald", "nach", "dei\u00b7ner", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die auf deinen Wangen l\u00e4chelt:", "tokens": ["Die", "auf", "dei\u00b7nen", "Wan\u00b7gen", "l\u00e4\u00b7chelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Blicke bald nach deinem Vater,", "tokens": ["Bli\u00b7cke", "bald", "nach", "dei\u00b7nem", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Der in deinem Auge lebet;", "tokens": ["Der", "in", "dei\u00b7nem", "Au\u00b7ge", "le\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schmiege dich bald an den weichen", "tokens": ["Schmie\u00b7ge", "dich", "bald", "an", "den", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Busen, dem du dich entwandest,", "tokens": ["Bu\u00b7sen", ",", "dem", "du", "dich", "ent\u00b7wan\u00b7dest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und umschling mit frohem Stammeln", "tokens": ["Und", "um\u00b7schling", "mit", "fro\u00b7hem", "Stam\u00b7meln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Deiner Mutter sanften Nacken.", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", "sanf\u00b7ten", "Na\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Wann nun dein bem\u00fchter Vater", "tokens": ["Wann", "nun", "dein", "be\u00b7m\u00fch\u00b7ter", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Dem Gedr\u00e4nge seiner Pflichten", "tokens": ["Dem", "Ge\u00b7dr\u00e4n\u00b7ge", "sei\u00b7ner", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Auf ein Weilchen nur entschl\u00fcpfet,", "tokens": ["Auf", "ein", "Weil\u00b7chen", "nur", "ent\u00b7schl\u00fcp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und sich deiner Mutter nahend", "tokens": ["Und", "sich", "dei\u00b7ner", "Mut\u00b7ter", "na\u00b7hend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Dich von ihrem Halse fodert,", "tokens": ["Dich", "von", "ih\u00b7rem", "Hal\u00b7se", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Damal str\u00e4ube dich ein wenig", "tokens": ["Da\u00b7mal", "str\u00e4u\u00b7be", "dich", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "(denn die\u00df m\u00fc\u00dfen junge Sch\u00f6nen),", "tokens": ["(", "denn", "die\u00df", "m\u00fc\u00b7\u00dfen", "jun\u00b7ge", "Sch\u00f6\u00b7nen", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PDS", "VMFIN", "ADJA", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Bis er mit Gewalt dich raubet.", "tokens": ["Bis", "er", "mit", "Ge\u00b7walt", "dich", "rau\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Aber dann, o Engel! k\u00fc\u00df' ihm", "tokens": ["A\u00b7ber", "dann", ",", "o", "En\u00b7gel", "!", "k\u00fc\u00df'", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "FM", "NE", "$.", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Seine m\u00e4nnlichbraunen Wangen,", "tokens": ["Sei\u00b7ne", "m\u00e4nn\u00b7lich\u00b7brau\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "K\u00fc\u00df' ihm weg den Philosophen,", "tokens": ["K\u00fc\u00df'", "ihm", "weg", "den", "Phi\u00b7lo\u00b7so\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "K\u00fc\u00df' ihm weg den Staatsgelehrten,", "tokens": ["K\u00fc\u00df'", "ihm", "weg", "den", "Staats\u00b7ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "K\u00fc\u00df' ihm alles aus der Seele,", "tokens": ["K\u00fc\u00df'", "ihm", "al\u00b7les", "aus", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Was ihm noch vom Krieger anh\u00e4ngt,", "tokens": ["Was", "ihm", "noch", "vom", "Krie\u00b7ger", "an\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Da\u00df er nichts als Vater bleibe. \u2013", "tokens": ["Da\u00df", "er", "nichts", "als", "Va\u00b7ter", "blei\u00b7be", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PIS", "KOKOM", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Doch ich singe schon zu lange.", "tokens": ["Doch", "ich", "sin\u00b7ge", "schon", "zu", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Du beginnest einzuschlummern.", "tokens": ["Du", "be\u00b7gin\u00b7nest", "ein\u00b7zu\u00b7schlum\u00b7mern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Sollte die\u00df mein Liedchen wirken? \u2013", "tokens": ["Soll\u00b7te", "die\u00df", "mein", "Lied\u00b7chen", "wir\u00b7ken", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PDS", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Ja! die\u00df wirken manche Lieder. \u2013", "tokens": ["Ja", "!", "die\u00df", "wir\u00b7ken", "man\u00b7che", "Lie\u00b7der", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$.", "PDS", "VVFIN", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Schlumm're s\u00fc\u00df, mein holder Engel!", "tokens": ["Schlum\u00b7m'\u00b7re", "s\u00fc\u00df", ",", "mein", "hol\u00b7der", "En\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}