{"textgrid.poem.36788": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Carneval", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie, du wunderst dich, Freund, wie so urpl\u00f6tzlich ein Volk sich", "tokens": ["Wie", ",", "du", "wun\u00b7derst", "dich", ",", "Freund", ",", "wie", "so", "ur\u00b7pl\u00f6tz\u00b7lich", "ein", "Volk", "sich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PPER", "VAFIN", "PPER", "$,", "NN", "$,", "PWAV", "ADV", "ADJD", "ART", "NN", "PRF"], "meter": "--+--+--++--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wochenlang wie toll, n\u00e4rrisch und albern betr\u00e4gt?", "tokens": ["Wo\u00b7chen\u00b7lang", "wie", "toll", ",", "n\u00e4r\u00b7risch", "und", "al\u00b7bern", "be\u00b7tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "O mein Theurer, du irrest dich sehr, schilt keinen, der heut sich", "tokens": ["O", "mein", "Theu\u00b7rer", ",", "du", "ir\u00b7rest", "dich", "sehr", ",", "schilt", "kei\u00b7nen", ",", "der", "heut", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PIAT", "$,", "PRELS", "ADV", "PRF"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Auf dem Corso herum wie ein Besessener treibt,", "tokens": ["Auf", "dem", "Cor\u00b7so", "he\u00b7rum", "wie", "ein", "Be\u00b7ses\u00b7se\u00b7ner", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "So erscheint mir am wahrsten der Mensch, dies Carneval steht ihm,", "tokens": ["So", "er\u00b7scheint", "mir", "am", "wahrs\u00b7ten", "der", "Mensch", ",", "dies", "Car\u00b7ne\u00b7val", "steht", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "ART", "NN", "$,", "PDS", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+--+-+-+--", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Aber das Schlimmere folgt, wenn er kein M\u00e4skchen mehr hat.", "tokens": ["A\u00b7ber", "das", "Schlim\u00b7me\u00b7re", "folgt", ",", "wenn", "er", "kein", "M\u00e4sk\u00b7chen", "mehr", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "ADV", "VAFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}