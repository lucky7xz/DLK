{"textgrid.poem.31101": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "215.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr Lauten hat und f\u00fcr Violen T\u00f6lplin den polschen Bock erkohren;", "tokens": ["F\u00fcr", "Lau\u00b7ten", "hat", "und", "f\u00fcr", "Vi\u00b7o\u00b7len", "T\u00f6l\u00b7plin", "den", "pol\u00b7schen", "Bock", "er\u00b7koh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "KON", "APPR", "NN", "NE", "ART", "ADJA", "NE", "VVPP", "$."], "meter": "-+-+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Denn jene kunten ihm nicht f\u00fcllen die hohen, weiten, tieffen Ohren.", "tokens": ["Denn", "je\u00b7ne", "kun\u00b7ten", "ihm", "nicht", "f\u00fcl\u00b7len", "die", "ho\u00b7hen", ",", "wei\u00b7ten", ",", "tief\u00b7fen", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}