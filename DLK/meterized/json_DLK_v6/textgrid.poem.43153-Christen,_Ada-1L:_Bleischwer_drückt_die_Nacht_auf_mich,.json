{"textgrid.poem.43153": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bleischwer dr\u00fcckt die Nacht auf mich,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleischwer dr\u00fcckt die Nacht auf mich,", "tokens": ["Blei\u00b7schwer", "dr\u00fcckt", "die", "Nacht", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolken jagen rasch vor\u00fcber,", "tokens": ["Wol\u00b7ken", "ja\u00b7gen", "rasch", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00fcbe schon und immer tr\u00fcber", "tokens": ["Tr\u00fc\u00b7be", "schon", "und", "im\u00b7mer", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcllt der Mond in Nebel sich.", "tokens": ["H\u00fcllt", "der", "Mond", "in", "Ne\u00b7bel", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den Zweigen \u00e4chzt der Wind", "tokens": ["In", "den", "Zwei\u00b7gen", "\u00e4chzt", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es rauschen scheu die Bl\u00e4tter,", "tokens": ["Und", "es", "rau\u00b7schen", "scheu", "die", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald vom dumpfen nahen Wetter", "tokens": ["Bald", "vom", "dum\u00b7pfen", "na\u00b7hen", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgel\u00f6scht die Sterne sind.", "tokens": ["Aus\u00b7ge\u00b7l\u00f6scht", "die", "Ster\u00b7ne", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unkenruf im nahen See", "tokens": ["Un\u00b7ken\u00b7ruf", "im", "na\u00b7hen", "See"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im Gras ein leis' Gefl\u00fcster;", "tokens": ["Und", "im", "Gras", "ein", "leis'", "Ge\u00b7fl\u00fcs\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "\u00d6de starrt der Himmel, d\u00fcster \u2013", "tokens": ["\u00d6\u00b7de", "starrt", "der", "Him\u00b7mel", ",", "d\u00fcs\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weint er stumm \u2013 ob unserm Weh?", "tokens": ["Weint", "er", "stumm", "\u2013", "ob", "un\u00b7serm", "Weh", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$(", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}