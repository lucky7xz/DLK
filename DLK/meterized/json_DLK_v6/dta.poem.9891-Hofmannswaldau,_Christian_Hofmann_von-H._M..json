{"dta.poem.9891": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "H. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach lache nicht du prei\u00df der damen/", "tokens": ["Ach", "la\u00b7che", "nicht", "du", "prei\u00df", "der", "da\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "PPER", "VVFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df deine gunst ein knecht begehrt/", "tokens": ["Da\u00df", "dei\u00b7ne", "gunst", "ein", "knecht", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein feuer/ das der busem n\u00e4hrt/", "tokens": ["Mein", "feu\u00b7er", "/", "das", "der", "bu\u00b7sem", "n\u00e4hrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchrt von der tugend seinen saamen.", "tokens": ["F\u00fchrt", "von", "der", "tu\u00b7gend", "sei\u00b7nen", "saa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was schadt es/ da\u00df ein dorn sich um die rose flicht/", "tokens": ["Was", "schadt", "es", "/", "da\u00df", "ein", "dorn", "sich", "um", "die", "ro\u00b7se", "flicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "KOUS", "ART", "ADV", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in der stoltzen lust die rothe bl\u00e4tter bricht.", "tokens": ["Und", "in", "der", "stolt\u00b7zen", "lust", "die", "ro\u00b7the", "bl\u00e4t\u00b7ter", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wahr ists/ ich habe mich verstiegen;", "tokens": ["Wahr", "ists", "/", "ich", "ha\u00b7be", "mich", "ver\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch weil die lieb ein feuer ist/", "tokens": ["Doch", "weil", "die", "lieb", "ein", "feu\u00b7er", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sich in keinen kercker schliest/", "tokens": ["Das", "sich", "in", "kei\u00b7nen", "ker\u00b7cker", "schliest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan es wohl an den himmel fliegen:", "tokens": ["Kan", "es", "wohl", "an", "den", "him\u00b7mel", "flie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist mein himmel/ nur dein auge ist der pol/", "tokens": ["Du", "bist", "mein", "him\u00b7mel", "/", "nur", "dein", "au\u00b7ge", "ist", "der", "pol", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "ADV", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dahin mein hertze sich auf ewig lencken soll.", "tokens": ["Da\u00b7hin", "mein", "hert\u00b7ze", "sich", "auf", "e\u00b7wig", "len\u00b7cken", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "VVFIN", "PRF", "APPR", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du sprichst/ wo weisse liljen blitzen/", "tokens": ["Du", "sprichst", "/", "wo", "weis\u00b7se", "lil\u00b7jen", "blit\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da darff kein wilder schwartz-dorn stehn:", "tokens": ["Da", "darff", "kein", "wil\u00b7der", "schwartz\u00b7dorn", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo aber bunter tausendsch\u00f6n/", "tokens": ["Wo", "a\u00b7ber", "bun\u00b7ter", "tau\u00b7send\u00b7sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pflegt keine nessel nicht zu sitzen.", "tokens": ["Pflegt", "kei\u00b7ne", "nes\u00b7sel", "nicht", "zu", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wohl! aber dennoch deckt die zwiebel schwartzer sand/", "tokens": ["Wohl", "!", "a\u00b7ber", "den\u00b7noch", "deckt", "die", "zwie\u00b7bel", "schwart\u00b7zer", "sand", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ADV", "VVFIN", "ART", "ADJD", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ihre zierde bricht des g\u00e4rtners treue hand.", "tokens": ["Und", "ih\u00b7re", "zier\u00b7de", "bricht", "des", "g\u00e4rt\u00b7ners", "treu\u00b7e", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df/ du wilt mit deinen dingen", "tokens": ["Ich", "wei\u00df", "/", "du", "wilt", "mit", "dei\u00b7nen", "din\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vortrefflich weit und hoch hinau\u00df:", "tokens": ["Vor\u00b7treff\u00b7lich", "weit", "und", "hoch", "hin\u00b7au\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df ich mich nun zu weit heraus", "tokens": ["La\u00df", "ich", "mich", "nun", "zu", "weit", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "PTKA", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und werde es zu wege bringen/", "tokens": ["Und", "wer\u00b7de", "es", "zu", "we\u00b7ge", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bleibt mir doch der ruhm/ da\u00df nur ein himmels-kind", "tokens": ["So", "bleibt", "mir", "doch", "der", "ruhm", "/", "da\u00df", "nur", "ein", "him\u00b7mels\u00b7kind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$(", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In reiner liebe mich hat gegen dich entz\u00fcndt.", "tokens": ["In", "rei\u00b7ner", "lie\u00b7be", "mich", "hat", "ge\u00b7gen", "dich", "ent\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Verh\u00e4rtst du aber deine sinnen/", "tokens": ["Ver\u00b7h\u00e4rtst", "du", "a\u00b7ber", "dei\u00b7ne", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wilst ein alabaster seyn/", "tokens": ["Und", "wilst", "ein", "a\u00b7la\u00b7bas\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So g\u00f6nne mir zum leichen-stein", "tokens": ["So", "g\u00f6n\u00b7ne", "mir", "zum", "lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein hertze/ wenn ich mu\u00df von hinnen/", "tokens": ["Dein", "hert\u00b7ze", "/", "wenn", "ich", "mu\u00df", "von", "hin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PPER", "VMFIN", "APPR", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und schreibe diese schrifft mit stahl und eisen an/", "tokens": ["Und", "schrei\u00b7be", "die\u00b7se", "schrifft", "mit", "stahl", "und", "ei\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "APPR", "NN", "KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo anders auch ein stahl dein hertze zwingen kan.", "tokens": ["Wo", "an\u00b7ders", "auch", "ein", "stahl", "dein", "hert\u00b7ze", "zwin\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Adanis konte sonst nicht lieben", "tokens": ["A\u00b7da\u00b7nis", "kon\u00b7te", "sonst", "nicht", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "PTKNEG", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Als was der himmel selber liebt;", "tokens": ["Als", "was", "der", "him\u00b7mel", "sel\u00b7ber", "liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was einem steten nahmen giebt", "tokens": ["Was", "ei\u00b7nem", "ste\u00b7ten", "nah\u00b7men", "giebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War nur sein eintziges vergn\u00fcgen:", "tokens": ["War", "nur", "sein", "eint\u00b7zi\u00b7ges", "ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Deswegen/ als er fiel/ so konte nur allein", "tokens": ["Des\u00b7we\u00b7gen", "/", "als", "er", "fiel", "/", "so", "kon\u00b7te", "nur", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$(", "KOUS", "PPER", "VVFIN", "$(", "ADV", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vom himmel/ dem er gleich/ sein letztes fallen seyn.", "tokens": ["Vom", "him\u00b7mel", "/", "dem", "er", "gleich", "/", "sein", "letz\u00b7tes", "fal\u00b7len", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "PRELS", "PPER", "ADV", "$(", "PPOSAT", "ADJA", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}