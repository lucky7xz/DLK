{"textgrid.poem.24495": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein gro\u00dfer Kasten stund voll Thaler und ", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gro\u00dfer Kasten stund voll Thaler und ", "tokens": ["Ein", "gro\u00b7\u00dfer", "Kas\u00b7ten", "stund", "voll", "Tha\u00b7ler", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "NN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach Kasten w\u00e4rst du mein/ so w\u00e4re mir gerathen.", "tokens": ["Ach", "Kas\u00b7ten", "w\u00e4rst", "du", "mein", "/", "so", "w\u00e4\u00b7re", "mir", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VAFIN", "PPER", "PPOSAT", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich aber sagte gleich/ der Geitz besitzt das Hau\u00df.", "tokens": ["Ich", "a\u00b7ber", "sag\u00b7te", "gleich", "/", "der", "Geitz", "be\u00b7sitzt", "das", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum steht der Kasten zwar alhier an seiner Stelle/", "tokens": ["Drum", "steht", "der", "Kas\u00b7ten", "zwar", "al\u00b7hier", "an", "sei\u00b7ner", "Stel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Schl\u00fc\u00dfel aber hat der Teuffel in der H\u00f6lle.", "tokens": ["Den", "Schl\u00fc\u00b7\u00dfel", "a\u00b7ber", "hat", "der", "Teuf\u00b7fel", "in", "der", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}