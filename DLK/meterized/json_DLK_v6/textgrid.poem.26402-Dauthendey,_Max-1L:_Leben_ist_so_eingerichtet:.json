"word"], "pos": ["ADJD", "ADV", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hin, wo sich die Wege teilen", "tokens": ["Hin", ",", "wo", "sich", "die", "We\u00b7ge", "tei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PRF", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zum Pavillone gehen.", "tokens": ["Und", "zum", "Pa\u00b7vil\u00b7lo\u00b7ne", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Will die Absag' auf der Stelle", "tokens": ["Will", "die", "Ab\u00b7sag'", "auf", "der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Ritz' der T\u00fcre schieben,", "tokens": ["Durch", "die", "Ritz'", "der", "T\u00fc\u00b7re", "schie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber einmal vor der Schwelle,", "tokens": ["A\u00b7ber", "ein\u00b7mal", "vor", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es nicht dabei geblieben.", "tokens": ["Ist", "es", "nicht", "da\u00b7bei", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Selbst durchs Brett der wei\u00dfen T\