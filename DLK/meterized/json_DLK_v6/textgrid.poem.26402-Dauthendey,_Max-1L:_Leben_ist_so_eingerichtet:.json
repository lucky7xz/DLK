{"textgrid.poem.26402": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leben ist so eingerichtet:", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leben ist so eingerichtet:", "tokens": ["Le\u00b7ben", "ist", "so", "ein\u00b7ge\u00b7rich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G'scheit ist Jedermann im stillen,", "tokens": ["G'\u00b7scheit", "ist", "Je\u00b7der\u00b7mann", "im", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wer noch so klug es sichtet,", "tokens": ["Doch", "wer", "noch", "so", "klug", "es", "sich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Handelt dumm meist widerwillen.", "tokens": ["Han\u00b7delt", "dumm", "meist", "wi\u00b7der\u00b7wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast Du Dir auch vorgenommen,", "tokens": ["Hast", "Du", "Dir", "auch", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkle Kr\u00e4fte nicht zu leiden,", "tokens": ["Dunk\u00b7le", "Kr\u00e4f\u00b7te", "nicht", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlst sie mit verstopften Ohren.", "tokens": ["F\u00fchlst", "sie", "mit", "ver\u00b7stopf\u00b7ten", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn Gef\u00fchl l\u00e4\u00dft sich nicht meiden.", "tokens": ["Denn", "Ge\u00b7f\u00fchl", "l\u00e4\u00dft", "sich", "nicht", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leda war bei der Gesandtin", "tokens": ["Le\u00b7da", "war", "bei", "der", "Ge\u00b7sand\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu Besuch, mit G\u00f6nnermiene.", "tokens": ["Zu", "Be\u00b7such", ",", "mit", "G\u00f6n\u00b7ner\u00b7mie\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah sie Einen und erfand ihn", "tokens": ["Sah", "sie", "Ei\u00b7nen", "und", "er\u00b7fand", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb, geno\u00df sie's mit Routine.", "tokens": ["Lieb", ",", "ge\u00b7no\u00df", "sie's", "mit", "Rou\u00b7ti\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIS", "APPR", "NE", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Immer lag auf langen St\u00fchlen", "tokens": ["Im\u00b7mer", "lag", "auf", "lan\u00b7gen", "St\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie in den Salons herum", "tokens": ["Sie", "in", "den", "Sa\u00b7lons", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "APZR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und lie\u00df ihre Wimpern f\u00fchlen,", "tokens": ["Und", "lie\u00df", "ih\u00b7re", "Wim\u00b7pern", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie ein Impfer sein Serum.", "tokens": ["Wie", "ein", "Imp\u00b7fer", "sein", "Se\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lange St\u00fchle wie Alt\u00e4re", "tokens": ["Lan\u00b7ge", "St\u00fch\u00b7le", "wie", "Al\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trugen festlich Ledas Glut.", "tokens": ["Tru\u00b7gen", "fest\u00b7lich", "Le\u00b7das", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Stuhl mal k\u00fcrzer w\u00e4re,", "tokens": ["Wenn", "der", "Stuhl", "mal", "k\u00fcr\u00b7zer", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Machte es sich nicht so gut.", "tokens": ["Mach\u00b7te", "es", "sich", "nicht", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ledas indische Muss'line", "tokens": ["Le\u00b7das", "in\u00b7di\u00b7sche", "Muss'\u00b7li\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "H\u00fcllten gutgepflegte Reize,", "tokens": ["H\u00fcll\u00b7ten", "gut\u00b7ge\u00b7pfleg\u00b7te", "Rei\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verkappt lebten die Sinne,", "tokens": ["Und", "ver\u00b7kappt", "leb\u00b7ten", "die", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie die Falken bei der Beize.", "tokens": ["Wie", "die", "Fal\u00b7ken", "bei", "der", "Bei\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hatte sie mal klar bekommen,", "tokens": ["Hat\u00b7te", "sie", "mal", "klar", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wen sie w\u00fcnschte sich als Sieger,", "tokens": ["Wen", "sie", "w\u00fcnschte", "sich", "als", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PRF", "KOUS", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Hat sie R\u00fccksicht nie genommen,", "tokens": ["Hat", "sie", "R\u00fcck\u00b7sicht", "nie", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Machte J\u00fcnglinge zu Tiger.", "tokens": ["Mach\u00b7te", "J\u00fcng\u00b7lin\u00b7ge", "zu", "Ti\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Nun bei der Gesandtin sollte", "tokens": ["Nun", "bei", "der", "Ge\u00b7sand\u00b7tin", "soll\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Heut' man eine Hochzeit geben.", "tokens": ["Heut'", "man", "ei\u00b7ne", "Hoch\u00b7zeit", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Tochter trauen wollte", "tokens": ["Ih\u00b7re", "Toch\u00b7ter", "trau\u00b7en", "woll\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einem Grafen man f\u00fcrs Leben.", "tokens": ["Ei\u00b7nem", "Gra\u00b7fen", "man", "f\u00fcrs", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sorglos kam man vom Altare,", "tokens": ["Sorg\u00b7los", "kam", "man", "vom", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sp\u00e4t erhob man sich vom Mahle.", "tokens": ["Sp\u00e4t", "er\u00b7hob", "man", "sich", "vom", "Mah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leda dann wie Liebesware", "tokens": ["Le\u00b7da", "dann", "wie", "Lie\u00b7bes\u00b7wa\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem l\u00e4ngsten Stuhl im Saale", "tokens": ["Auf", "dem", "l\u00e4ngs\u00b7ten", "Stuhl", "im", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sich hinlegt; tut mit den Wimpern", "tokens": ["Sich", "hin\u00b7legt", ";", "tut", "mit", "den", "Wim\u00b7pern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVPP", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Durch die festerhitzte Menge", "tokens": ["Durch", "die", "fes\u00b7ter\u00b7hitz\u00b7te", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Br\u00e4utigame klimpern \u2013", "tokens": ["Nach", "dem", "Br\u00e4u\u00b7ti\u00b7ga\u00b7me", "klim\u00b7pern", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen zieht es aus der Enge,", "tokens": ["Die\u00b7sen", "zieht", "es", "aus", "der", "En\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "F\u00fchlt gleich seinen Absatz wanken,", "tokens": ["F\u00fchlt", "gleich", "sei\u00b7nen", "Ab\u00b7satz", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt laut seine Lackschuh knarren,", "tokens": ["H\u00f6rt", "laut", "sei\u00b7ne", "Lack\u00b7schuh", "knar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht, da\u00df sie ganz in Gedanken", "tokens": ["Sieht", ",", "da\u00df", "sie", "ganz", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mit ihm fortgegangen waren.", "tokens": ["Mit", "ihm", "fort\u00b7ge\u00b7gan\u00b7gen", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Leda hat mit schw\u00fcler Wange", "tokens": ["Le\u00b7da", "hat", "mit", "schw\u00fc\u00b7ler", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum ihr Auge aufgehoben,", "tokens": ["Kaum", "ihr", "Au\u00b7ge", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Hochzeitsnacht ward lange,", "tokens": ["Und", "die", "Hoch\u00b7zeits\u00b7nacht", "ward", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn nicht ewig, dann verschoben.", "tokens": ["Wenn", "nicht", "e\u00b7wig", ",", "dann", "ver\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJD", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Weil sich Ledas Augen dehnen,", "tokens": ["Weil", "sich", "Le\u00b7das", "Au\u00b7gen", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlt er seines Blutes Schw\u00e4chen,", "tokens": ["F\u00fchlt", "er", "sei\u00b7nes", "Blu\u00b7tes", "Schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht am langen Stuhl sich lehnen,", "tokens": ["Sieht", "am", "lan\u00b7gen", "Stuhl", "sich", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6cht' den langen Stuhl zerbrechen.", "tokens": ["M\u00f6cht'", "den", "lan\u00b7gen", "Stuhl", "zer\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "\u00bbheute Nacht la\u00df mich nicht warten,\u00ab", "tokens": ["\u00bb", "heu\u00b7te", "Nacht", "la\u00df", "mich", "nicht", "war\u00b7ten", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "VVIMP", "PPER", "PTKNEG", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft sich Ledas Stimm' vernehmen,", "tokens": ["L\u00e4\u00dft", "sich", "Le\u00b7das", "Stimm'", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbrechts der Pavillon im Garten\u00ab, \u2013", "tokens": ["\u00bb", "rechts", "der", "Pa\u00b7vil\u00b7lon", "im", "Gar\u00b7ten", "\u00ab", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "ART", "NN", "APPRART", "NN", "$(", "$,", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Nochmals tat ihr Aug' ihn l\u00e4hmen.", "tokens": ["Noch\u00b7mals", "tat", "ihr", "Aug'", "ihn", "l\u00e4h\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Dann erhob sie sich vom langen", "tokens": ["Dann", "er\u00b7hob", "sie", "sich", "vom", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stuhl, er durft' sie nicht ber\u00fchren,", "tokens": ["Stuhl", ",", "er", "durft'", "sie", "nicht", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist vom Br\u00e4utigam gegangen;", "tokens": ["Ist", "vom", "Br\u00e4u\u00b7ti\u00b7gam", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der konnt' kaum die Braut noch sp\u00fcren.", "tokens": ["Der", "konnt'", "kaum", "die", "Braut", "noch", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Wackelnder als ging's auf Eier,", "tokens": ["Wa\u00b7ckeln\u00b7der", "als", "ging's", "auf", "Ei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlug sein Herz, das Neugetraute,", "tokens": ["Schlug", "sein", "Herz", ",", "das", "Neu\u00b7ge\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihm vor der Hochzeitsfeier", "tokens": ["Da\u00df", "ihm", "vor", "der", "Hoch\u00b7zeits\u00b7fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinter