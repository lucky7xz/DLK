"Hin\u00b7ter", "ihm", "st\u00fcrzt", "was", "zu\u00b7sam\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PTKVZ", "$,", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ach, sein Brautstand tat versinken.", "tokens": ["Ach", ",", "sein", "Brauts\u00b7tand", "tat", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Schnell steht er im Handumdrehen", "tokens": ["Schnell", "steht", "er", "im", "Han\u00b7dum\u00b7dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In dem Pavillon, dem gro\u00dfen,", "tokens": ["In", "dem", "Pa\u00b7vil\u00b7lon", ",", "dem", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mu\u00df im Dunkeln weitergehen,", "tokens": ["Mu\u00df", "im", "Dun\u00b7keln", "wei\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen langen Stuhl umsto\u00dfen.", "tokens": ["Ei\u00b7nen", "lan\u00b7gen", "Stuhl", "um\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Er greift zu mit beiden H\u00e4nden,", "tokens": ["Er", "greift", "zu", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Leda tut vor Wonnest\u00f6hnen \u2013", "tokens": ["Le\u00b7da", "tut", "vor", "Won\u00b7nes\u00b7t\u00f6h\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es war nicht mehr abzuwenden,", "tokens": ["Es", "war", "nicht", "mehr", "ab\u00b7zu\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er mu\u00df ihrer Liebe fr\u00f6hnen.", "tokens": ["Er", "mu\u00df", "ih\u00b7rer", "Lie\u00b7be", "fr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.24": {"line.1": {"text": "Wenn er nur Gedanken h\u00e4tte \u2013", "tokens": ["Wenn", "er", "nur", "Ge\u00b7dan\u00b7ken", "h\u00e4t\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber Liebe kann nicht denken,", "tokens": ["A\u00b7ber", "Lie\u00b7be", "kann", "nicht", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkt nicht an die Braut, die nette.", "tokens": ["Denkt", "nicht", "an", "die", "Braut", ",", "die", "net\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Widerwillen mu\u00df er kr\u00e4nken.", "tokens": ["Wi\u00b7der\u00b7wil\u00b7len", "mu\u00df", "er", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Nie mehr hat er heimgefunden,", "tokens": ["Nie", "mehr", "hat", "er", "heim\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Floh mit Leda vor dem Morgen,", "tokens": ["Floh", "mit", "Le\u00b7da", "vor", "dem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Widerwillig schlug er Wunden", "tokens": ["Wi\u00b7der\u00b7wil\u00b7lig", "schlug", "er", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und macht' andern Leuten Sorgen. \u2013", "tokens": ["Und", "macht'", "an\u00b7dern", "Leu\u00b7ten", "Sor\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.26": {"line.1": {"text": "So kann's Leben an Dir handeln,", "tokens": ["So", "kann's", "Le\u00b7ben", "an", "Dir", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "G