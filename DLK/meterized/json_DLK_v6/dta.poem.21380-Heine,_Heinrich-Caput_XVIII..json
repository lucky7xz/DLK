{"dta.poem.21380": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Minden ist eine feste Burg,", "tokens": ["Min\u00b7den", "ist", "ei\u00b7ne", "fes\u00b7te", "Burg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hat gute Wehr\u2019 und Waffen!", "tokens": ["Hat", "gu\u00b7te", "Wehr'", "und", "Waf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit preu\u00dfischen Festungen hab\u2019 ich jedoch", "tokens": ["Mit", "preu\u00b7\u00dfi\u00b7schen", "Fes\u00b7tun\u00b7gen", "hab'", "ich", "je\u00b7doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nicht gerne was zu schaffen.", "tokens": ["Nicht", "ger\u00b7ne", "was", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}