{"textgrid.poem.64075": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Streben in die Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du blaue Ferne, die mir lieblich winket,", "tokens": ["Du", "blau\u00b7e", "Fer\u00b7ne", ",", "die", "mir", "lieb\u00b7lich", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was birgst Du wohl in Deinen Nebelduft?", "tokens": ["Was", "birgst", "Du", "wohl", "in", "Dei\u00b7nen", "Ne\u00b7bel\u00b7duft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist's ein Phantom, was mir entz\u00fcckend blinket", "tokens": ["Ist's", "ein", "Phan\u00b7tom", ",", "was", "mir", "ent\u00b7z\u00fc\u00b7ckend", "blin\u00b7ket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PWS", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Stern der Ahndung dort in fremder Luft?", "tokens": ["Als", "Stern", "der", "Ahn\u00b7dung", "dort", "in", "frem\u00b7der", "Luft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was ist's, das m\u00e4chtig mich in's Weite ruft,", "tokens": ["Was", "ist's", ",", "das", "m\u00e4ch\u00b7tig", "mich", "in's", "Wei\u00b7te", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn still verl\u00f6scht die Abendsonne sinket?", "tokens": ["Wenn", "still", "ver\u00b7l\u00f6scht", "die", "A\u00b7bend\u00b7son\u00b7ne", "sin\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn der Trennung unermessne Kluft", "tokens": ["Und", "wenn", "der", "Tren\u00b7nung", "un\u00b7er\u00b7mess\u00b7ne", "Kluft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den tr\u00fcben Sinn in Grabestiefen winket?", "tokens": ["Den", "tr\u00fc\u00b7ben", "Sinn", "in", "Gra\u00b7bes\u00b7tie\u00b7fen", "win\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist der Hoffnung wunderbares Wehen", "tokens": ["Es", "ist", "der", "Hoff\u00b7nung", "wun\u00b7der\u00b7ba\u00b7res", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das weit entlegne L\u00e4nder mir verkl\u00e4rt,", "tokens": ["Das", "weit", "ent\u00b7leg\u00b7ne", "L\u00e4n\u00b7der", "mir", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und goldnen Schimmer webt um ferne H\u00f6hen.", "tokens": ["Und", "gold\u00b7nen", "Schim\u00b7mer", "webt", "um", "fer\u00b7ne", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Nur eine bessre Zukunft zu ersp\u00e4hen,", "tokens": ["Nur", "ei\u00b7ne", "bess\u00b7re", "Zu\u00b7kunft", "zu", "er\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6cht' ich heran genaht die Ferne sehen.", "tokens": ["M\u00f6cht'", "ich", "he\u00b7ran", "ge\u00b7naht", "die", "Fer\u00b7ne", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}