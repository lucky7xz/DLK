{"textgrid.poem.54351": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "33. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Edle Freyheit, la\u00df dich k\u00fcssen,", "tokens": ["Ed\u00b7le", "Frey\u00b7heit", ",", "la\u00df", "dich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du mein liebstes auf der Welt,", "tokens": ["Du", "mein", "liebs\u00b7tes", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich will ich ins Herze schliessen,", "tokens": ["Dich", "will", "ich", "ins", "Her\u00b7ze", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil dein Wesen mir gef\u00e4llt.", "tokens": ["Weil", "dein", "We\u00b7sen", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O du Nahrung meiner Seelen,", "tokens": ["O", "du", "Nah\u00b7rung", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie wohl schmeckt deine Kost!", "tokens": ["O", "wie", "wohl", "schmeckt", "dei\u00b7ne", "Kost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ich will alle Tropfen z\u00e4hlen,", "tokens": ["Ich", "will", "al\u00b7le", "Trop\u00b7fen", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von dem zuckers\u00fcssen Most.", "tokens": ["Von", "dem", "zu\u00b7cker\u00b7s\u00fcs\u00b7sen", "Most", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deine Gunst, dein reizend Wesen", "tokens": ["Dei\u00b7ne", "Gunst", ",", "dein", "rei\u00b7zend", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterh\u00e4lt Geist und Verstand,", "tokens": ["Un\u00b7ter\u00b7h\u00e4lt", "Geist", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ja du bleibest auserlesen,", "tokens": ["Ja", "du", "blei\u00b7best", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm mich an zum Unterpfand.", "tokens": ["Nimm", "mich", "an", "zum", "Un\u00b7ter\u00b7pfand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Andre m\u00f6gen sich verlieben,", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "sich", "ver\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn Cupido zielt und trifft,", "tokens": ["Wenn", "Cu\u00b7pi\u00b7do", "zielt", "und", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Entschlu\u00df ist l\u00e4ngst geschrieben:", "tokens": ["Mein", "Ent\u00b7schlu\u00df", "ist", "l\u00e4ngst", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df dies kein Vergn\u00fcgen stift.", "tokens": ["Da\u00df", "dies", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "stift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn Verliebte seufzend klagen,", "tokens": ["Wenn", "Ver\u00b7lieb\u00b7te", "seuf\u00b7zend", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So bin ich stets aufger\u00e4umt:", "tokens": ["So", "bin", "ich", "stets", "auf\u00b7ge\u00b7r\u00e4umt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich weis nichts von den Plagen,", "tokens": ["Denn", "ich", "weis", "nichts", "von", "den", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ein solcher Thore tr\u00e4umt.", "tokens": ["Die", "ein", "sol\u00b7cher", "Tho\u00b7re", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Blo\u00df mein einiges Vergn\u00fcgen", "tokens": ["Blo\u00df", "mein", "ei\u00b7ni\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Freyheit ganz allein;", "tokens": ["Ist", "die", "Frey\u00b7heit", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihr schmeicheln kann nicht tr\u00fcgen;", "tokens": ["Denn", "ihr", "schmei\u00b7cheln", "kann", "nicht", "tr\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So mu\u00df ich erkenntlich seyn.", "tokens": ["So", "mu\u00df", "ich", "er\u00b7kennt\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Sie soll meine Losung bleiben", "tokens": ["Sie", "soll", "mei\u00b7ne", "Lo\u00b7sung", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis mich Gruft und Stein bedeckt;", "tokens": ["Bis", "mich", "Gruft", "und", "Stein", "be\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf mein Grabmal soll man schreiben,", "tokens": ["Auf", "mein", "Grab\u00b7mal", "soll", "man", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Freyheit Lust erweckt.", "tokens": ["Da\u00df", "die", "Frey\u00b7heit", "Lust", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}