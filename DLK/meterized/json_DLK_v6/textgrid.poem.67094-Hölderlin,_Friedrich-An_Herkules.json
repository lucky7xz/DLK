{"textgrid.poem.67094": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Herkules", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Kindheit Schlaf begraben", "tokens": ["In", "der", "Kind\u00b7heit", "Schlaf", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag ich, wie das Erz im Schacht;", "tokens": ["Lag", "ich", ",", "wie", "das", "Erz", "im", "Schacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dank, mein Herkules! den Knaben", "tokens": ["Dank", ",", "mein", "Her\u00b7ku\u00b7les", "!", "den", "Kna\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hast zum Manne du gemacht,", "tokens": ["Hast", "zum", "Man\u00b7ne", "du", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reif bin ich zum K\u00f6nigssitze", "tokens": ["Reif", "bin", "ich", "zum", "K\u00f6\u00b7nigs\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mir brechen stark und gro\u00df", "tokens": ["Und", "mir", "bre\u00b7chen", "stark", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Taten, wie Kronions Blitze,", "tokens": ["Ta\u00b7ten", ",", "wie", "Kro\u00b7nions", "Blit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Aus der Jugend Wolke los.", "tokens": ["Aus", "der", "Ju\u00b7gend", "Wol\u00b7ke", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der Adler seine Jungen,", "tokens": ["Wie", "der", "Ad\u00b7ler", "sei\u00b7ne", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Funk im Auge klimmt,", "tokens": ["Wenn", "der", "Funk", "im", "Au\u00b7ge", "klimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die k\u00fchnen Wanderungen", "tokens": ["Auf", "die", "k\u00fch\u00b7nen", "Wan\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den frohen Aether nimmt,", "tokens": ["In", "den", "fro\u00b7hen", "A\u00b7e\u00b7ther", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nimmst du aus der Kinderwiege,", "tokens": ["Nimmst", "du", "aus", "der", "Kin\u00b7der\u00b7wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Mutter Tisch und Haus", "tokens": ["Von", "der", "Mut\u00b7ter", "Tisch", "und", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In die Flamme deiner Kriege,", "tokens": ["In", "die", "Flam\u00b7me", "dei\u00b7ner", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hoher Halbgott, mich hinaus.", "tokens": ["Ho\u00b7her", "Halb\u00b7gott", ",", "mich", "hin\u00b7aus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4hntest du, dein K\u00e4mpferwagen", "tokens": ["W\u00e4hn\u00b7test", "du", ",", "dein", "K\u00e4mp\u00b7fer\u00b7wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rolle mir umsonst ins Ohr?", "tokens": ["Rol\u00b7le", "mir", "um\u00b7sonst", "ins", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jede Last, die du getragen,", "tokens": ["Je\u00b7de", "Last", ",", "die", "du", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hub die Seele mir empor,", "tokens": ["Hub", "die", "See\u00b7le", "mir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwar der Sch\u00fcler mu\u00dfte zahlen;", "tokens": ["Zwar", "der", "Sch\u00fc\u00b7ler", "mu\u00df\u00b7te", "zah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmerzlich brannten, stolzes Licht,", "tokens": ["Schmerz\u00b7lich", "brann\u00b7ten", ",", "stol\u00b7zes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir im Busen deine Strahlen,", "tokens": ["Mir", "im", "Bu\u00b7sen", "dei\u00b7ne", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber sie verzehrten nicht.", "tokens": ["A\u00b7ber", "sie", "ver\u00b7zehr\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn f\u00fcr deines Schicksals Wogen", "tokens": ["Wenn", "f\u00fcr", "dei\u00b7nes", "Schick\u00b7sals", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hohe G\u00f6tterkr\u00e4fte dich,", "tokens": ["Ho\u00b7he", "G\u00f6t\u00b7ter\u00b7kr\u00e4f\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fchner Schwimmer! auferzogen,", "tokens": ["K\u00fch\u00b7ner", "Schwim\u00b7mer", "!", "auf\u00b7er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was erzog dem Siege mich?", "tokens": ["Was", "er\u00b7zog", "dem", "Sie\u00b7ge", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was berief den Vaterlosen,", "tokens": ["Was", "be\u00b7rief", "den", "Va\u00b7ter\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der in dunkler Halle sa\u00df,", "tokens": ["Der", "in", "dunk\u00b7ler", "Hal\u00b7le", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu dem G\u00f6ttlichen und Gro\u00dfen,", "tokens": ["Zu", "dem", "G\u00f6tt\u00b7li\u00b7chen", "und", "Gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df er k\u00fchn an dir sich ma\u00df?", "tokens": ["Da\u00df", "er", "k\u00fchn", "an", "dir", "sich", "ma\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "PRF", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ergriff und zog vom Schwarme", "tokens": ["Was", "er\u00b7griff", "und", "zog", "vom", "Schwar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Gespielen mich hervor?", "tokens": ["Der", "Ge\u00b7spie\u00b7len", "mich", "her\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was bewog des B\u00e4umchens Arme", "tokens": ["Was", "be\u00b7wog", "des", "B\u00e4um\u00b7chens", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach des Aethers Tag empor?", "tokens": ["Nach", "des", "A\u00b7e\u00b7thers", "Tag", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Freundlich nahm des jungen Lebens", "tokens": ["Freund\u00b7lich", "nahm", "des", "jun\u00b7gen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keines G\u00e4rtners Hand sich an,", "tokens": ["Kei\u00b7nes", "G\u00e4rt\u00b7ners", "Hand", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber kraft des eignen Strebens", "tokens": ["A\u00b7ber", "kraft", "des", "eig\u00b7nen", "Stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blickt und wuchs ich himmelan.", "tokens": ["Blickt", "und", "wuchs", "ich", "him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sohn Kronions! an die Seite", "tokens": ["Sohn", "Kro\u00b7nions", "!", "an", "die", "Sei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tret ich nun err\u00f6tend dir,", "tokens": ["Tret", "ich", "nun", "er\u00b7r\u00f6\u00b7tend", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Olymp ist deine Beute;", "tokens": ["Der", "O\u00b7lymp", "ist", "dei\u00b7ne", "Beu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Komm und teile sie mit mir!", "tokens": ["Komm", "und", "tei\u00b7le", "sie", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sterblich bin ich zwar geboren,", "tokens": ["Sterb\u00b7lich", "bin", "ich", "zwar", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dennoch hat Unsterblichkeit", "tokens": ["Den\u00b7noch", "hat", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Meine Seele sich geschworen,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "sich", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sie h\u00e4lt, was sie gebeut.", "tokens": ["Und", "sie", "h\u00e4lt", ",", "was", "sie", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}