{"dta.poem.38": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Grab einer Persohn die  \n in Kindesn\u00f6then gestorben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh! Wandrer! schaue hier der Rahel\nGrabmahl an,", "tokens": ["Sieh", "!", "Wand\u00b7rer", "!", "schau\u00b7e", "hier", "der", "Ra\u00b7hel", "Grab\u00b7mahl", "an", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die in der Erde liegt, der Mutter", "tokens": ["Die", "in", "der", "Er\u00b7de", "liegt", ",", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die selbst ein Todten Grab des Kindes heissen kan;", "tokens": ["Die", "selbst", "ein", "Tod\u00b7ten", "Grab", "des", "Kin\u00b7des", "heis\u00b7sen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie den Tod hier mu\u00df, an dessen Tode haben.", "tokens": ["Weil", "sie", "den", "Tod", "hier", "mu\u00df", ",", "an", "des\u00b7sen", "To\u00b7de", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "$,", "APPR", "PRELAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie starb nach ", "tokens": ["Sie", "starb", "nach"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Damit sie ewiglich bey ihm dort k\u00f6nte leben;", "tokens": ["Da\u00b7mit", "sie", "e\u00b7wig\u00b7lich", "bey", "ihm", "dort", "k\u00f6n\u00b7te", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Gott lies sie in der Angst; darin sie ist entleibt:", "tokens": ["Gott", "lies", "sie", "in", "der", "Angst", ";", "da\u00b7rin", "sie", "ist", "ent\u00b7leibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Damit er ihr darauf k\u00f6nnt immer Freude geben,", "tokens": ["Da\u00b7mit", "er", "ihr", "da\u00b7rauf", "k\u00f6nnt", "im\u00b7mer", "Freu\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PAV", "VVFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er hat ihr alle Lust bey ihrer Last versagt:", "tokens": ["Er", "hat", "ihr", "al\u00b7le", "Lust", "bey", "ih\u00b7rer", "Last", "ver\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Damit er solche bald mit Himmels-Lust verpflege:", "tokens": ["Da\u00b7mit", "er", "sol\u00b7che", "bald", "mit", "Him\u00b7mels\u00b7Lust", "ver\u00b7pfle\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie wunderlich scheint ", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "scheint"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Der h\u00f6rt: zwar wunderbar doch gut sind GOttes", "tokens": ["Der", "h\u00f6rt", ":", "zwar", "wun\u00b7der\u00b7bar", "doch", "gut", "sind", "Got\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "ADV", "ADJD", "ADV", "ADJD", "VAFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nun Wandrer gehe hin bemerke dieses Grab", "tokens": ["Nun", "Wand\u00b7rer", "ge\u00b7he", "hin", "be\u00b7mer\u00b7ke", "die\u00b7ses", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ein erstorbnes Kind der eignen Mutter gab", "tokens": ["Da\u00df", "ein", "ers\u00b7torb\u00b7nes", "Kind", "der", "eig\u00b7nen", "Mut\u00b7ter", "gab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hier liegt ein todtes Kind in Mutterleib begraben,", "tokens": ["Hier", "liegt", "ein", "tod\u00b7tes", "Kind", "in", "Mut\u00b7ter\u00b7leib", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die beide ihre Ruh in Mutter Schoosse ", "tokens": ["Die", "bei\u00b7de", "ih\u00b7re", "Ruh", "in", "Mut\u00b7ter", "Schoos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "PPOSAT", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}