{"dta.poem.9930": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df als kreide bin ich erstlich/ werde bald als wie ein mohr;", "tokens": ["Wei\u00df", "als", "krei\u00b7de", "bin", "ich", "erst\u00b7lich", "/", "wer\u00b7de", "bald", "als", "wie", "ein", "mohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "VVFIN", "VAFIN", "PPER", "ADJD", "$(", "VAFIN", "ADV", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Denn die hitze endert mich/ die mein armer leib mu\u00df leiden:", "tokens": ["Denn", "die", "hit\u00b7ze", "en\u00b7dert", "mich", "/", "die", "mein", "ar\u00b7mer", "leib", "mu\u00df", "lei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$(", "ART", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "--+----+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Nach belieben kommet man zu mir/ pflegt auch so zu scheiden/", "tokens": ["Nach", "be\u00b7lie\u00b7ben", "kom\u00b7met", "man", "zu", "mir", "/", "pflegt", "auch", "so", "zu", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PIS", "APPR", "PPER", "$(", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+----+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Keiner wird mich leichtlich hassen/ es sey denn ein albrer thor.", "tokens": ["Kei\u00b7ner", "wird", "mich", "leicht\u00b7lich", "has\u00b7sen", "/", "es", "sey", "denn", "ein", "alb\u00b7rer", "thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVINF", "$(", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}}}}}