{"textgrid.poem.40523": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Bach tief unterm Klippenhang", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Bach tief unterm Klippenhang", "tokens": ["Der", "Bach", "tief", "un\u00b7term", "Klip\u00b7pen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rauscht in Sirenens\u00e4ngen,", "tokens": ["Rauscht", "in", "Si\u00b7re\u00b7nen\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da\u00df, hart am Felsrand, schwindelbang,", "tokens": ["Da\u00df", ",", "hart", "am", "Fels\u00b7rand", ",", "schwin\u00b7del\u00b7bang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gekr\u00fcmmt, die Fichten h\u00e4ngen.", "tokens": ["Ge\u00b7kr\u00fcmmt", ",", "die", "Fich\u00b7ten", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Kreuz von Holz spricht noch davon", "tokens": ["Am", "Kreuz", "von", "Holz", "spricht", "noch", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schrift mit trunk'nem Lallen,", "tokens": ["Die", "Schrift", "mit", "trunk'\u00b7nem", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ein bezechter Alpensohn", "tokens": ["Da\u00df", "ein", "be\u00b7zech\u00b7ter", "Al\u00b7pen\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich hier zu Tod gefallen.", "tokens": ["Sich", "hier", "zu", "Tod", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wie ich lauschend Aug' und Ohr", "tokens": ["Und", "wie", "ich", "lau\u00b7schend", "Aug'", "und", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geneigt zur Abgrundstiefe,", "tokens": ["Ge\u00b7neigt", "zur", "Ab\u00b7grund\u00b7stie\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da war mir's, als ob draus empor", "tokens": ["Da", "war", "mir's", ",", "als", "ob", "draus", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "$,", "KOKOM", "KOUS", "PAV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dumpf eine Stimme riefe:", "tokens": ["Dumpf", "ei\u00b7ne", "Stim\u00b7me", "rie\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbzechbr\u00fcderlein, hilf mir doch aus", "tokens": ["\u00bb", "zech\u00b7br\u00fc\u00b7derl\u00b7ein", ",", "hilf", "mir", "doch", "aus"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "VVIMP", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Felsenkeller wallen!", "tokens": ["Dem", "Fel\u00b7sen\u00b7kel\u00b7ler", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sieh, in ein leeres Fa\u00df, o Graus,", "tokens": ["Sieh", ",", "in", "ein", "lee\u00b7res", "Fa\u00df", ",", "o", "Graus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich dahier gefallen!", "tokens": ["Bin", "ich", "da\u00b7hier", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Durchs Spundloch leuchtet karg und gelb", "tokens": ["Durchs", "Spund\u00b7loch", "leuch\u00b7tet", "karg", "und", "gelb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag in meine Tonne:", "tokens": ["Der", "Tag", "in", "mei\u00b7ne", "Ton\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Himmel ist mein Fa\u00dfgew\u00f6lb,", "tokens": ["Dein", "Him\u00b7mel", "ist", "mein", "Fa\u00df\u00b7ge\u00b7w\u00f6lb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Spundloch deine Sonne!", "tokens": ["Mein", "Spund\u00b7loch", "dei\u00b7ne", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn die\u00df karge Licht verschwand,", "tokens": ["Und", "wenn", "die\u00df", "kar\u00b7ge", "Licht", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann funkelt Weinsteinglimmer", "tokens": ["Dann", "fun\u00b7kelt", "Wein\u00b7stein\u00b7glim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An meines Fasses dunkler Wand!", "tokens": ["An", "mei\u00b7nes", "Fas\u00b7ses", "dunk\u00b7ler", "Wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du nennst es Sternenschimmer!", "tokens": ["Du", "nennst", "es", "Ster\u00b7nen\u00b7schim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was rauscht da? Weinfluth h\u00f6r' ich voll", "tokens": ["Was", "rauscht", "da", "?", "Wein\u00b7fluth", "h\u00f6r'", "ich", "voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "$.", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus offnen Zapfen jagen!", "tokens": ["Aus", "off\u00b7nen", "Zap\u00b7fen", "ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dir ist's ein Bach! ein, Wasser soll", "tokens": ["Dir", "ist's", "ein", "Bach", "!", "ein", ",", "Was\u00b7ser", "soll"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PTKVZ", "$,", "PWS", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich doch zu mir nicht wagen!", "tokens": ["Sich", "doch", "zu", "mir", "nicht", "wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Tr\u00e4um ich im gr\u00fcnen Friedhofraum", "tokens": ["Tr\u00e4um", "ich", "im", "gr\u00fc\u00b7nen", "Fried\u00b7ho\u00b7fraum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Br\u00fcdern und Gespielen,", "tokens": ["Bei", "Br\u00fc\u00b7dern", "und", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Engel unsrer Stirne Saum", "tokens": ["Wo", "En\u00b7gel", "uns\u00b7rer", "Stir\u00b7ne", "Saum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Tannenreisern k\u00fchlen?", "tokens": ["Mit", "Tan\u00b7nen\u00b7rei\u00b7sern", "k\u00fch\u00b7len", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nein, Weinlaub seh' ich \u00fcber mir", "tokens": ["Nein", ",", "Wein\u00b7laub", "seh'", "ich", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Kr\u00e4nzen lieblich schwanken!", "tokens": ["In", "Kr\u00e4n\u00b7zen", "lieb\u00b7lich", "schwan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sprich, oder wehn um Klippen hier", "tokens": ["Sprich", ",", "o\u00b7der", "wehn", "um", "Klip\u00b7pen", "hier"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KON", "VVFIN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur lose Epheuranken?", "tokens": ["Nur", "lo\u00b7se", "E\u00b7pheu\u00b7ran\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ach, und zerfiel sich nicht mein Leib", "tokens": ["Ach", ",", "und", "zer\u00b7fiel", "sich", "nicht", "mein", "Leib"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Klippen und in L\u00fcften?", "tokens": ["An", "Klip\u00b7pen", "und", "in", "L\u00fcf\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Weinesbl\u00fcth' und Most zerst\u00e4ub'", "tokens": ["Wie", "Wei\u00b7nes\u00b7bl\u00fcth'", "und", "Most", "zer\u00b7st\u00e4ub'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er froh in Schaum und D\u00fcften!", "tokens": ["Er", "froh", "in", "Schaum", "und", "D\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Doch du, herabgeneigt zum treu'n", "tokens": ["Doch", "du", ",", "her\u00b7ab\u00b7ge\u00b7neigt", "zum", "treu'n"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "VVPP", "APPRART", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vasallen m\u00e4cht'ger F\u00e4sser,", "tokens": ["Va\u00b7sal\u00b7len", "m\u00e4cht'\u00b7ger", "F\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Dein Rausch von Lenz und Sonnenschein", "tokens": ["Dein", "Rausch", "von", "Lenz", "und", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er so gar viel besser?", "tokens": ["Ist", "er", "so", "gar", "viel", "bes\u00b7ser", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wohl bist, wo strauchelnd ich geschwankt,", "tokens": ["Wohl", "bist", ",", "wo", "strau\u00b7chelnd", "ich", "ge\u00b7schwankt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sacht vorbeigeglitten;", "tokens": ["Du", "sacht", "vor\u00b7bei\u00b7ge\u00b7glit\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch bin ich oft, wo du gewankt,", "tokens": ["Doch", "bin", "ich", "oft", ",", "wo", "du", "ge\u00b7wankt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufrecht und fest geschritten.", "tokens": ["Auf\u00b7recht", "und", "fest", "ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "O schl\u00fcrf' ihn ganz, den Goldpokal", "tokens": ["O", "schl\u00fcr\u00b7f'", "ihn", "ganz", ",", "den", "Gold\u00b7po\u00b7kal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Fr\u00fchlingsduft und Rose,", "tokens": ["Von", "Fr\u00fch\u00b7lings\u00b7duft", "und", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Freiheit, Licht und Sonnenstrahl", "tokens": ["Von", "Frei\u00b7heit", ",", "Licht", "und", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Nachtigallgekose!", "tokens": ["Und", "Nach\u00b7ti\u00b7gall\u00b7ge\u00b7ko\u00b7se", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ein s\u00fc\u00dfer Taumel hebt den Schritt", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Tau\u00b7mel", "hebt", "den", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Zechern und den Dichtern,", "tokens": ["Den", "Ze\u00b7chern", "und", "den", "Dich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo scharfer Kies die Fersen schnitt", "tokens": ["Wo", "schar\u00b7fer", "Kies", "die", "Fer\u00b7sen", "schnitt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Armen, die da n\u00fcchtern!", "tokens": ["Den", "Ar\u00b7men", ",", "die", "da", "n\u00fcch\u00b7tern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "In diesen Abgrund sinkst du nicht,", "tokens": ["In", "die\u00b7sen", "Ab\u00b7grund", "sinkst", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch anderswo in einen!", "tokens": ["Doch", "an\u00b7ders\u00b7wo", "in", "ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geb' einen Traum, so sch\u00f6n und licht,", "tokens": ["Geb'", "ei\u00b7nen", "Traum", ",", "so", "sch\u00f6n", "und", "licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herr dir dann, wie meinen!\u00ab", "tokens": ["Der", "Herr", "dir", "dann", ",", "wie", "mei\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}