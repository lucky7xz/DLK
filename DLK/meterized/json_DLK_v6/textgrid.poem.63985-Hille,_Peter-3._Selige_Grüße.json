{"textgrid.poem.63985": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "3. Selige Gr\u00fc\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00e4ulicher Flieder.", "tokens": ["Bl\u00e4u\u00b7li\u00b7cher", "Flie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ist das ein Gr\u00fc\u00dfen!", "tokens": ["Ist", "das", "ein", "Gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wirbelnde Lieder", "tokens": ["Wir\u00b7beln\u00b7de", "Lie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wehen her\u00fcber, \u2013", "tokens": ["We\u00b7hen", "her\u00b7\u00fc\u00b7ber", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$,", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "St\u00fcrben lieber.", "tokens": ["St\u00fcr\u00b7ben", "lie\u00b7ber", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Seligsein \u2013 und das hei\u00dft b\u00fc\u00dfen.", "tokens": ["Se\u00b7lig\u00b7sein", "\u2013", "und", "das", "hei\u00dft", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "KON", "PDS", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}