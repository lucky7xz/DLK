{"textgrid.poem.43187": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du gutes altes M\u00fctterlein,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gutes altes M\u00fctterlein,", "tokens": ["Du", "gu\u00b7tes", "al\u00b7tes", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hattest mich so lieb,", "tokens": ["Du", "hat\u00b7test", "mich", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verlie\u00df Dich auch Dein wildes Kind \u2013", "tokens": ["Ver\u00b7lie\u00df", "Dich", "auch", "Dein", "wil\u00b7des", "Kind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergieb mir doch \u2013 vergieb!", "tokens": ["Ver\u00b7gieb", "mir", "doch", "\u2013", "ver\u00b7gieb", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$(", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie gerne kehrt ich heim zu Dir,", "tokens": ["Wie", "ger\u00b7ne", "kehrt", "ich", "heim", "zu", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Deinen stillen Heerd,", "tokens": ["An", "Dei\u00b7nen", "stil\u00b7len", "Heerd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie gern vermi\u00dft ich Alles jetzt,", "tokens": ["Wie", "gern", "ver\u00b7mi\u00dft", "ich", "Al\u00b7les", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was einst ich hei\u00df begehrt.", "tokens": ["Was", "einst", "ich", "hei\u00df", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie gern l\u00e4g' ich an Deiner Brust,", "tokens": ["Wie", "gern", "l\u00e4g'", "ich", "an", "Dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem letzten, wahren Hort \u2013", "tokens": ["Dem", "letz\u00b7ten", ",", "wah\u00b7ren", "Hort", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie gern l\u00e4g' ich zu F\u00fc\u00dfen Dir", "tokens": ["Wie", "gern", "l\u00e4g'", "ich", "zu", "F\u00fc\u00b7\u00dfen", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lauschte Deinem Wort.", "tokens": ["Und", "lauschte", "Dei\u00b7nem", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und kl\u00e4ng' Dein Wort auch noch so hart,", "tokens": ["Und", "kl\u00e4ng'", "Dein", "Wort", "auch", "noch", "so", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst hattest Du mich lieb \u2013", "tokens": ["Einst", "hat\u00b7test", "Du", "mich", "lieb", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du gutes altes M\u00fctterlein", "tokens": ["Du", "gu\u00b7tes", "al\u00b7tes", "M\u00fct\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergieb mir doch \u2013 vergieb! ...", "tokens": ["Ver\u00b7gieb", "mir", "doch", "\u2013", "ver\u00b7gieb", "!", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$(", "VVIMP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}