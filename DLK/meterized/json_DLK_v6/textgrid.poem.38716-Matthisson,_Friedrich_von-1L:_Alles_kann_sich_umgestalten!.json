{"textgrid.poem.38716": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles kann sich umgestalten!", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles kann sich umgestalten!", "tokens": ["Al\u00b7les", "kann", "sich", "um\u00b7ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag das dunkle Schicksal walten.", "tokens": ["Mag", "das", "dunk\u00b7le", "Schick\u00b7sal", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Muthig! auf der steilsten Bahn.", "tokens": ["Mut\u00b7hig", "!", "auf", "der", "steils\u00b7ten", "Bahn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trau dem Gl\u00fccke! trau den G\u00f6ttern!", "tokens": ["Trau", "dem", "Gl\u00fc\u00b7cke", "!", "trau", "den", "G\u00f6t\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Steig, trotz Wogendrang und Wettern,", "tokens": ["Steig", ",", "trotz", "Wo\u00b7gen\u00b7drang", "und", "Wet\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fchn, wie C\u00e4sar, in den Kahn.", "tokens": ["K\u00fchn", ",", "wie", "C\u00e4\u00b7sar", ",", "in", "den", "Kahn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NE", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df den Schw\u00e4chling angstvoll zagen!", "tokens": ["La\u00df", "den", "Schw\u00e4ch\u00b7ling", "angst\u00b7voll", "za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer um Hohes k\u00e4mpft, mu\u00df wagen!", "tokens": ["Wer", "um", "Ho\u00b7hes", "k\u00e4mpft", ",", "mu\u00df", "wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Leben gelt' es oder Tod!", "tokens": ["Le\u00b7ben", "gelt'", "es", "o\u00b7der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df die Woge donnernd branden!", "tokens": ["La\u00df", "die", "Wo\u00b7ge", "don\u00b7nernd", "bran\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur bleib immer, magst du landen", "tokens": ["Nur", "bleib", "im\u00b7mer", ",", "magst", "du", "lan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder scheitern, selbst Pilot!", "tokens": ["O\u00b7der", "schei\u00b7tern", ",", "selbst", "Pi\u00b7lot", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}