{"textgrid.poem.33608": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie blitzen im Tau die Blumen,", "tokens": ["Wie", "blit\u00b7zen", "im", "Tau", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie hell die Sonne scheint!", "tokens": ["Wie", "hell", "die", "Son\u00b7ne", "scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es haben Nachts die Sterne", "tokens": ["Es", "ha\u00b7ben", "Nachts", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So goldene Tr\u00e4nen geweint.", "tokens": ["So", "gol\u00b7de\u00b7ne", "Tr\u00e4\u00b7nen", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Was mochte sie betr\u00fcben", "tokens": ["Was", "moch\u00b7te", "sie", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So tief und insgeheim?", "tokens": ["So", "tief", "und", "ins\u00b7ge\u00b7heim", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zog im Sturm vor\u00fcber", "tokens": ["Es", "zog", "im", "Sturm", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Fr\u00fchling wieder heim.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wie\u00b7der", "heim", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum l\u00e4cheln auch die Rosen", "tokens": ["Drum", "l\u00e4\u00b7cheln", "auch", "die", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor Liebe und Verdru\u00df;", "tokens": ["Vor", "Lie\u00b7be", "und", "Ver\u00b7dru\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch bebt um ihre Lippen", "tokens": ["Noch", "bebt", "um", "ih\u00b7re", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Fr\u00fchlings Abschiedku\u00df.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Ab\u00b7schied\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}