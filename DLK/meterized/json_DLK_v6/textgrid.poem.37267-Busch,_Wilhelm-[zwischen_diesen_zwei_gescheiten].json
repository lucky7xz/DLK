{"textgrid.poem.37267": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[zwischen diesen zwei gescheiten]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen diesen zwei gescheiten", "tokens": ["Zwi\u00b7schen", "die\u00b7sen", "zwei", "ge\u00b7schei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "CARD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen, Anna und Dorette,", "tokens": ["M\u00e4d\u00b7chen", ",", "An\u00b7na", "und", "Do\u00b7ret\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist zu allen Tageszeiten", "tokens": ["Ist", "zu", "al\u00b7len", "Ta\u00b7ges\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ein ewiges Gekrette.", "tokens": ["Doch", "ein", "e\u00b7wi\u00b7ges", "Ge\u00b7kret\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch dazu um Kleinigkeiten \u2013", "tokens": ["Noch", "da\u00b7zu", "um", "Klei\u00b7nig\u00b7kei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "APPR", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gestern gingen sie zu Bette,", "tokens": ["Ge\u00b7stern", "gin\u00b7gen", "sie", "zu", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sie fingen an zu streiten,", "tokens": ["Und", "sie", "fin\u00b7gen", "an", "zu", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer die dicksten Waden h\u00e4tte.", "tokens": ["Wer", "die", "dicks\u00b7ten", "Wa\u00b7den", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}