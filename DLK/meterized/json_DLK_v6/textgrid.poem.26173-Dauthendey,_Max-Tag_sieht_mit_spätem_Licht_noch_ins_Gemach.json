{"textgrid.poem.26173": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Tag sieht mit sp\u00e4tem Licht noch ins Gemach", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die V\u00f6gel br\u00fcten, und der Abend schweigt", "tokens": ["Die", "V\u00f6\u00b7gel", "br\u00fc\u00b7ten", ",", "und", "der", "A\u00b7bend", "schweigt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Liedlos, als mu\u00df er ruhen von dem Mai.", "tokens": ["Lied\u00b7los", ",", "als", "mu\u00df", "er", "ru\u00b7hen", "von", "dem", "Mai", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur eine Amsel singt ins sommergr\u00fcne Einerlei", "tokens": ["Nur", "ei\u00b7ne", "Am\u00b7sel", "singt", "ins", "som\u00b7mer\u00b7gr\u00fc\u00b7ne", "Ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Als letzte, die noch keine Ruhe zeigt.", "tokens": ["Als", "letz\u00b7te", ",", "die", "noch", "kei\u00b7ne", "Ru\u00b7he", "zeigt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Berg ist gr\u00fcn, und alle Bl\u00fcten fielen fort.", "tokens": ["Der", "Berg", "ist", "gr\u00fcn", ",", "und", "al\u00b7le", "Bl\u00fc\u00b7ten", "fie\u00b7len", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Flu\u00df flie\u00dft endlos ohne Sang und Wort.", "tokens": ["Der", "Flu\u00df", "flie\u00dft", "end\u00b7los", "oh\u00b7ne", "Sang", "und", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Abend horcht der letzten Amsel nach,", "tokens": ["Der", "A\u00b7bend", "horcht", "der", "letz\u00b7ten", "Am\u00b7sel", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Tag sieht mit sp\u00e4tem Licht noch ins Gemach,", "tokens": ["Tag", "sieht", "mit", "sp\u00e4\u00b7tem", "Licht", "noch", "ins", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und Tag und Abend Arm in Arm liegen vereint am Dach.", "tokens": ["Und", "Tag", "und", "A\u00b7bend", "Arm", "in", "Arm", "lie\u00b7gen", "ver\u00b7eint", "am", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "NN", "APPR", "NN", "VVFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+---+-+", "measure": "unknown.measure.hexa"}}}}}