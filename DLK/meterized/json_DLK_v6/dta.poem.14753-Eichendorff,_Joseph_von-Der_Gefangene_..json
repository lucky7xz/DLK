{"dta.poem.14753": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie pr\u00e4chtig gl\u00e4nzt die Aue!", "tokens": ["Wie", "pr\u00e4ch\u00b7tig", "gl\u00e4nzt", "die", "Au\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Gold der Quell nun flo\u00df,", "tokens": ["Wie", "Gold", "der", "Quell", "nun", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und einer s\u00fc\u00dfen Fraue", "tokens": ["Und", "ei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Frau\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lag er im weichen Schoo\u00df.", "tokens": ["Lag", "er", "im", "wei\u00b7chen", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}