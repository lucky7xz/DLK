{"textgrid.poem.33848": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Schiffers Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier steh'n wir unter'm Apfelbaum,", "tokens": ["Hier", "steh'n", "wir", "un\u00b7ter'm", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier will ich von dir scheiden,", "tokens": ["Hier", "will", "ich", "von", "dir", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier tr\u00e4umte ich so manchen Traum,", "tokens": ["Hier", "tr\u00e4um\u00b7te", "ich", "so", "man\u00b7chen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier tr\u00e4gt sich auch ein Leiden.", "tokens": ["Hier", "tr\u00e4gt", "sich", "auch", "ein", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier sah ich dich zum ersten Mal,", "tokens": ["Hier", "sah", "ich", "dich", "zum", "ers\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In winterlicher Oede!", "tokens": ["In", "win\u00b7ter\u00b7li\u00b7cher", "O\u00b7e\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wie war der Baum so nackt und kahl,", "tokens": ["Wie", "war", "der", "Baum", "so", "nackt", "und", "kahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie warst du kalt und spr\u00f6de!", "tokens": ["Wie", "warst", "du", "kalt", "und", "spr\u00f6\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch bald ergr\u00fcnte Zweig nach Zweig,", "tokens": ["Doch", "bald", "er\u00b7gr\u00fcn\u00b7te", "Zweig", "nach", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle Knospen trieben.", "tokens": ["Und", "al\u00b7le", "Knos\u00b7pen", "trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sprang dein Herz, den Knospen gleich,", "tokens": ["Da", "sprang", "dein", "Herz", ",", "den", "Knos\u00b7pen", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da fingst du an, zu lieben.", "tokens": ["Da", "fingst", "du", "an", ",", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie ist er jetzt von Bl\u00fcten voll!", "tokens": ["Wie", "ist", "er", "jetzt", "von", "Bl\u00fc\u00b7ten", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wird er reichlich tragen!", "tokens": ["Wie", "wird", "er", "reich\u00b7lich", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, wer ihn f\u00fcr dich sch\u00fctteln soll,", "tokens": ["Doch", ",", "wer", "ihn", "f\u00fcr", "dich", "sch\u00fct\u00b7teln", "soll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das w\u00fc\u00dft' ich nicht zu sagen.", "tokens": ["Das", "w\u00fc\u00dft'", "ich", "nicht", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hei! Wie dich s\u00e4uselnd jener Ast", "tokens": ["Hei", "!", "Wie", "dich", "s\u00e4u\u00b7selnd", "je\u00b7ner", "Ast"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "PPER", "ADJD", "PDAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit rothem Schnee bestreute,", "tokens": ["Mit", "ro\u00b7them", "Schnee", "be\u00b7streu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ob er schon die schwere Last", "tokens": ["Als", "ob", "er", "schon", "die", "schwe\u00b7re", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der k\u00fcnft'gen Fr\u00fcchte scheute!", "tokens": ["Der", "k\u00fcnft'\u00b7gen", "Fr\u00fcch\u00b7te", "scheu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn \u00fcber's Meer der Herbstwind pfeift", "tokens": ["Wenn", "\u00fc\u00b7ber's", "Meer", "der", "Herbst\u00b7wind", "pfeift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und an dem Mast mir r\u00fcttelt,", "tokens": ["Und", "an", "dem", "Mast", "mir", "r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So denke ich: sie sind gereift,", "tokens": ["So", "den\u00b7ke", "ich", ":", "sie", "sind", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und er ist's, der sie sch\u00fcttelt!", "tokens": ["Und", "er", "ist's", ",", "der", "sie", "sch\u00fct\u00b7telt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Und mu\u00df mein Schiff vor seinem Braus", "tokens": ["Und", "mu\u00df", "mein", "Schiff", "vor", "sei\u00b7nem", "Braus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar an ein Felsriff prallen,", "tokens": ["Gar", "an", "ein", "Fel\u00b7sriff", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So ruf' ich noch im Scheitern aus:", "tokens": ["So", "ruf'", "ich", "noch", "im", "Schei\u00b7tern", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sch\u00f6nste will nicht fallen!", "tokens": ["Die", "sch\u00f6ns\u00b7te", "will", "nicht", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}