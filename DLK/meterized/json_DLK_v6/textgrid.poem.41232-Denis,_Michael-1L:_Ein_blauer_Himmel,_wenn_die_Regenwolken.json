{"textgrid.poem.41232": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein blauer Himmel, wenn die Regenwolken", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein blauer Himmel, wenn die Regenwolken", "tokens": ["Ein", "blau\u00b7er", "Him\u00b7mel", ",", "wenn", "die", "Re\u00b7gen\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun endlich hinter Berge ziehn,", "tokens": ["Nun", "end\u00b7lich", "hin\u00b7ter", "Ber\u00b7ge", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ruhend Meer nach St\u00fcrmen ist in Menschenherzen", "tokens": ["Ein", "ru\u00b7hend", "Meer", "nach", "St\u00fcr\u00b7men", "ist", "in", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein jetzt erf\u00fcllter Wunsch.", "tokens": ["Ein", "jetzt", "er\u00b7f\u00fcll\u00b7ter", "Wunsch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O fei're du, das ganze Spiel herunter,", "tokens": ["O", "fei'\u00b7re", "du", ",", "das", "gan\u00b7ze", "Spiel", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den jetzt erf\u00fcllten Wunsch, mein Lied!", "tokens": ["Den", "jetzt", "er\u00b7f\u00fcll\u00b7ten", "Wunsch", ",", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Den, nach dessen Antlitz mich verlangte,", "tokens": ["Ich", "ha\u00b7be", "Den", ",", "nach", "des\u00b7sen", "Ant\u00b7litz", "mich", "ver\u00b7lang\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich habe Den gesehn!", "tokens": ["Ich", "ha\u00b7be", "Den", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Helden, welcher als das letzte Wetter", "tokens": ["Den", "Hel\u00b7den", ",", "wel\u00b7cher", "als", "das", "letz\u00b7te", "Wet\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Krieges \u00fcber Deutschland zog,", "tokens": ["Des", "Krie\u00b7ges", "\u00fc\u00b7ber", "Deutschland", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein unverseh'ner Blitz aus finst'ren Eisenwolken", "tokens": ["Ein", "un\u00b7ver\u00b7seh'\u00b7ner", "Blitz", "aus", "finst'\u00b7ren", "Ei\u00b7sen\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf uns're Gegner fuhr,", "tokens": ["Auf", "un\u00b7s'\u00b7re", "Geg\u00b7ner", "fuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und schneller fast, als unsere Gedanken,", "tokens": ["Und", "schnel\u00b7ler", "fast", ",", "als", "un\u00b7se\u00b7re", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Fl\u00e4chen, H\u00fcgel, Berg und Wald,", "tokens": ["Durch", "Fl\u00e4\u00b7chen", ",", "H\u00fc\u00b7gel", ",", "Berg", "und", "Wald", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch gedr\u00e4ngte Kriegerreihen fra\u00df, und Vesten", "tokens": ["Und", "durch", "ge\u00b7dr\u00e4ng\u00b7te", "Krie\u00b7ger\u00b7rei\u00b7hen", "fra\u00df", ",", "und", "Ves\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Voll stolzer Sicherheit;", "tokens": ["Voll", "stol\u00b7zer", "Si\u00b7cher\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df alles, alles, eh der Retter umsah", "tokens": ["Da\u00df", "al\u00b7les", ",", "al\u00b7les", ",", "eh", "der", "Ret\u00b7ter", "um\u00b7sah"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PIS", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Empor in Feuerwogen schlug,", "tokens": ["Em\u00b7por", "in", "Feu\u00b7er\u00b7wo\u00b7gen", "schlug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Laudons Name, das Geprassel \u00fcberstimmend,", "tokens": ["Und", "Lau\u00b7dons", "Na\u00b7me", ",", "das", "Ge\u00b7pras\u00b7sel", "\u00fc\u00b7bers\u00b7tim\u00b7mend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis an die Meere scholl;", "tokens": ["Bis", "an", "die", "Mee\u00b7re", "scholl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den Helden, den der Kriegesarbeit Kenner,", "tokens": ["Den", "Hel\u00b7den", ",", "den", "der", "Krie\u00b7ge\u00b7sar\u00b7beit", "Ken\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Zeugen unverd\u00e4chtigster,", "tokens": ["Der", "Zeu\u00b7gen", "un\u00b7ver\u00b7d\u00e4ch\u00b7tigs\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Den selbst der hohe Brennenherrscher ohne Fehler,", "tokens": ["Den", "selbst", "der", "ho\u00b7he", "Bren\u00b7nen\u00b7herr\u00b7scher", "oh\u00b7ne", "Feh\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie seinen Bruder, fand;", "tokens": ["Wie", "sei\u00b7nen", "Bru\u00b7der", ",", "fand", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Den Helden, dessen Anblick in den Herzen", "tokens": ["Den", "Hel\u00b7den", ",", "des\u00b7sen", "An\u00b7blick", "in", "den", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Eisentr\u00e4ger Muth emp\u00f6rt,", "tokens": ["Der", "Ei\u00b7sen\u00b7tr\u00e4\u00b7ger", "Muth", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wie der Stral aus Osten in der Haine Wipfeln", "tokens": ["So", "wie", "der", "Stral", "aus", "Os\u00b7ten", "in", "der", "Hai\u00b7ne", "Wip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Federkehlen weckt;", "tokens": ["Die", "Fe\u00b7der\u00b7keh\u00b7len", "weckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Den sah ich im Gefolge seiner Thaten", "tokens": ["Den", "sah", "ich", "im", "Ge\u00b7fol\u00b7ge", "sei\u00b7ner", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(ein langer, liederwerther Zug,),", "tokens": ["(", "ein", "lan\u00b7ger", ",", "lie\u00b7der\u00b7wert\u00b7her", "Zug", ",", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "$,", "ADJA", "NN", "$,", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Schimmerkleide seines Ruhmes, tausend Schatten", "tokens": ["Im", "Schim\u00b7mer\u00b7klei\u00b7de", "sei\u00b7nes", "Ruh\u00b7mes", ",", "tau\u00b7send", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gefallener um ihn.", "tokens": ["Ge\u00b7fal\u00b7le\u00b7ner", "um", "ihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie gierig hing mein Aug' an ihm! Wie jauchzte", "tokens": ["Wie", "gie\u00b7rig", "hing", "mein", "Aug'", "an", "ihm", "!", "Wie", "jauchz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Geist in seiner Gegenwart!", "tokens": ["Mein", "Geist", "in", "sei\u00b7ner", "Ge\u00b7gen\u00b7wart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Sineds h\u00f6chste Lust ist Th\u00e4ter gro\u00dfer Thaten,", "tokens": ["Denn", "Si\u00b7neds", "h\u00f6chs\u00b7te", "Lust", "ist", "Th\u00e4\u00b7ter", "gro\u00b7\u00dfer", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Menschenwerth zu sehn.", "tokens": ["Und", "Men\u00b7schen\u00b7werth", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch er, er sieht den Nachzug seiner Thaten,", "tokens": ["Doch", "er", ",", "er", "sieht", "den", "Nach\u00b7zug", "sei\u00b7ner", "Tha\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schimmer seines Ruhmes nicht,", "tokens": ["Den", "Schim\u00b7mer", "sei\u00b7nes", "Ruh\u00b7mes", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die tausend Schatten auf der Stahlbahn hingestreckter", "tokens": ["Die", "tau\u00b7send", "Schat\u00b7ten", "auf", "der", "Stahl\u00b7bahn", "hin\u00b7ge\u00b7streck\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "APPR", "ART", "NN", "APPO"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Versuchten sieht er nicht.", "tokens": ["Ver\u00b7such\u00b7ten", "sieht", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Sonne gleich (sie blicket nie zur\u00fccke", "tokens": ["Der", "Son\u00b7ne", "gleich", "(", "sie", "bli\u00b7cket", "nie", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$(", "PPER", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Luftgebiethen, die sie ma\u00df,", "tokens": ["Nach", "Luft\u00b7ge\u00b7bie\u00b7then", ",", "die", "sie", "ma\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie h\u00f6ret den nur, der sie wandeln hie\u00df, und h\u00f6ret", "tokens": ["Sie", "h\u00f6\u00b7ret", "den", "nur", ",", "der", "sie", "wan\u00b7deln", "hie\u00df", ",", "und", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "$,", "PRELS", "PPER", "VVFIN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Dank der Erde nicht)", "tokens": ["Den", "Dank", "der", "Er\u00b7de", "nicht", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Sonne gleich verfolget unverwendet", "tokens": ["Der", "Son\u00b7ne", "gleich", "ver\u00b7fol\u00b7get", "un\u00b7ver\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Aug' der Ehre steilen Pfad;", "tokens": ["Sein", "Aug'", "der", "Eh\u00b7re", "stei\u00b7len", "Pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Menschenlob, die Pflicht, Theresien geschworen,", "tokens": ["Nicht", "Men\u00b7schen\u00b7lob", ",", "die", "Pflicht", ",", "The\u00b7re\u00b7si\u00b7en", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ART", "NN", "$,", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Joseph, h\u00f6rt er nur.", "tokens": ["Und", "Jo\u00b7se\u00b7ph", ",", "h\u00f6rt", "er", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Tiefdenkend schweiget er von eig'nen Thaten,", "tokens": ["Tief\u00b7den\u00b7kend", "schwei\u00b7get", "er", "von", "eig'\u00b7nen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Kreise der Gewaltigen.", "tokens": ["Im", "Krei\u00b7se", "der", "Ge\u00b7wal\u00b7ti\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ha, Gegner! reize den gedankenvollen Schweiger,", "tokens": ["Ha", ",", "Geg\u00b7ner", "!", "rei\u00b7ze", "den", "ge\u00b7dan\u00b7ken\u00b7vol\u00b7len", "Schwei\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$