{"textgrid.poem.61933": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam trauert Apoll. Wann denkt noch seiner ein J\u00fcngling?", "tokens": ["Ein\u00b7sam", "trau\u00b7ert", "A\u00b7poll", ".", "Wann", "denkt", "noch", "sei\u00b7ner", "ein", "J\u00fcng\u00b7ling", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "$.", "PWAV", "VVFIN", "ADV", "PPOSAT", "ART", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Heute beherrscht den Parna\u00df Plutus, der blendende Gott;", "tokens": ["Heu\u00b7te", "be\u00b7herrscht", "den", "Par\u00b7na\u00df", "Plu\u00b7tus", ",", "der", "blen\u00b7den\u00b7de", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Siehe, mit Schaufel und Karst, kalifornische Minen zu w\u00fchlen,", "tokens": ["Sie\u00b7he", ",", "mit", "Schau\u00b7fel", "und", "Karst", ",", "ka\u00b7li\u00b7for\u00b7ni\u00b7sche", "Mi\u00b7nen", "zu", "w\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "APPR", "NN", "KON", "NN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nach dem entheiligten Berg ziehn sie begehrlich hinaus.", "tokens": ["Nach", "dem", "en\u00b7thei\u00b7lig\u00b7ten", "Berg", "ziehn", "sie", "be\u00b7gehr\u00b7lich", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Deutsche Muse, du weinst? \u2013 \u00bbEinst war ich die Tochter des Himmels", "tokens": ["Deut\u00b7sche", "Mu\u00b7se", ",", "du", "weinst", "?", "\u2013", "\u00bb", "Einst", "war", "ich", "die", "Toch\u00b7ter", "des", "Him\u00b7mels"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "$.", "$(", "$(", "ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Eueren Dichtern; ein Fest bracht' ich, sobald ich erschien.", "tokens": ["Eu\u00b7e\u00b7ren", "Dich\u00b7tern", ";", "ein", "Fest", "bracht'", "ich", ",", "so\u00b7bald", "ich", "er\u00b7schien", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jetzt im Gewande der Magd, auf der Stirn unw\u00fcrdige Tropfen,", "tokens": ["Jetzt", "im", "Ge\u00b7wan\u00b7de", "der", "Magd", ",", "auf", "der", "Stirn", "un\u00b7w\u00fcr\u00b7di\u00b7ge", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mu\u00df ich um schn\u00f6den Gewinn fr\u00f6hnen im Qualm der Fabrik.\u00ab", "tokens": ["Mu\u00df", "ich", "um", "schn\u00f6\u00b7den", "Ge\u00b7winn", "fr\u00f6h\u00b7nen", "im", "Qualm", "der", "Fab\u00b7rik", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus dem Tempel der Kunst wann gei\u00dfelt ein anderer Lessing", "tokens": ["Aus", "dem", "Tem\u00b7pel", "der", "Kunst", "wann", "gei\u00b7\u00dfelt", "ein", "an\u00b7de\u00b7rer", "Les\u00b7sing"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Z\u00fcrnend wieder den Schwarm feilschender Kr\u00e4mer hinaus?", "tokens": ["Z\u00fcr\u00b7nend", "wie\u00b7der", "den", "Schwarm", "feil\u00b7schen\u00b7der", "Kr\u00e4\u00b7mer", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nicht um die Gunst mehr frein sie der Muse, sie frein um die Mitgift,", "tokens": ["Nicht", "um", "die", "Gunst", "mehr", "frein", "sie", "der", "Mu\u00b7se", ",", "sie", "frein", "um", "die", "Mit\u00b7gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "PIAT", "NN", "PPER", "ART", "NN", "$,", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und im gemeinen Erwerb stirbt das entweihte Talent.", "tokens": ["Und", "im", "ge\u00b7mei\u00b7nen", "Er\u00b7werb", "stirbt", "das", "ent\u00b7weih\u00b7te", "Ta\u00b7lent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Neue Theater zu baun, stets zeigt ihr euch willig und schm\u00fcckt sie", "tokens": ["Neu\u00b7e", "The\u00b7a\u00b7ter", "zu", "baun", ",", "stets", "zeigt", "ihr", "euch", "wil\u00b7lig", "und", "schm\u00fcckt", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "KON", "VVFIN", "PPER"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Pr\u00e4chtig von au\u00dfen und stellt eure Poeten davor;", "tokens": ["Pr\u00e4ch\u00b7tig", "von", "au\u00b7\u00dfen", "und", "stellt", "eu\u00b7re", "Po\u00b7et\u00b7en", "da\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "KON", "VVFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Aber im Inneren bleibt's, wie es war, und der prunkende Becher", "tokens": ["A\u00b7ber", "im", "In\u00b7ne\u00b7ren", "bleibt's", ",", "wie", "es", "war", ",", "und", "der", "prun\u00b7ken\u00b7de", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wird mit schalem Getr\u00e4nk heute wie gestern gef\u00fcllt.", "tokens": ["Wird", "mit", "scha\u00b7lem", "Ge\u00b7tr\u00e4nk", "heu\u00b7te", "wie", "ge\u00b7stern", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADV", "KOKOM", "ADV", "VVPP", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Sorgt doch lieber f\u00fcr edleren Wein! Wir w\u00fcrden mit besserm", "tokens": ["Sorgt", "doch", "lie\u00b7ber", "f\u00fcr", "ed\u00b7le\u00b7ren", "Wein", "!", "Wir", "w\u00fcr\u00b7den", "mit", "bes\u00b7serm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$.", "PPER", "VAFIN", "APPR", "ADJA"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Dank ihn schl\u00fcrfen, und w\u00e4r's aus dem bescheidensten Krug.", "tokens": ["Dank", "ihn", "schl\u00fcr\u00b7fen", ",", "und", "w\u00e4r's", "aus", "dem", "be\u00b7schei\u00b7dens\u00b7ten", "Krug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KON", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Seit der Gewinnanteil euch zufiel, treibt ihr das Dichten", "tokens": ["Seit", "der", "Ge\u00b7win\u00b7nan\u00b7teil", "euch", "zu\u00b7fiel", ",", "treibt", "ihr", "das", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nur als Gesch\u00e4ft noch und bringt, was dem Philister behagt:", "tokens": ["Nur", "als", "Ge\u00b7sch\u00e4ft", "noch", "und", "bringt", ",", "was", "dem", "Phi\u00b7lis\u00b7ter", "be\u00b7hagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "ADV", "KON", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Possen und schl\u00fcpfrige Sp\u00e4\u00dfe, versetzt mit moralischer R\u00fchrung,", "tokens": ["Pos\u00b7sen", "und", "schl\u00fcpf\u00b7ri\u00b7ge", "Sp\u00e4\u00b7\u00dfe", ",", "ver\u00b7setzt", "mit", "mo\u00b7ra\u00b7li\u00b7scher", "R\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Oder auf Stelzen dahin klappernde d\u00fcrre Tendenz.", "tokens": ["O\u00b7der", "auf", "Stel\u00b7zen", "da\u00b7hin", "klap\u00b7pern\u00b7de", "d\u00fcr\u00b7re", "Ten\u00b7denz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PAV", "ADJA", "ADJA", "NN", "$."], "meter": "+--+---+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Freilich, der Kasse gedeiht's, und ihr schafft euch jedes Behagen,", "tokens": ["Frei\u00b7lich", ",", "der", "Kas\u00b7se", "ge\u00b7deiht's", ",", "und", "ihr", "schafft", "euch", "je\u00b7des", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "NE", "$,", "KON", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Aber ein Lorbeerblatt tr\u00e4gt das Gewerbe nicht ein.", "tokens": ["A\u00b7ber", "ein", "Lor\u00b7beer\u00b7blatt", "tr\u00e4gt", "das", "Ge\u00b7wer\u00b7be", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "La\u00dft vom barbarischen Brauch und ruft zu der tragischen Muse", "tokens": ["La\u00dft", "vom", "bar\u00b7ba\u00b7ri\u00b7schen", "Brauch", "und", "ruft", "zu", "der", "tra\u00b7gi\u00b7schen", "Mu\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPRART", "ADJA", "NN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Festlich geschm\u00fcckten Altar wieder die Schwester herein!", "tokens": ["Fest\u00b7lich", "ge\u00b7schm\u00fcck\u00b7ten", "Al\u00b7tar", "wie\u00b7der", "die", "Schwes\u00b7ter", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Von dem Gew\u00fchle des Tags zu Melpomenes reinen Gestalten", "tokens": ["Von", "dem", "Ge\u00b7w\u00fch\u00b7le", "des", "Tags", "zu", "Mel\u00b7po\u00b7me\u00b7nes", "rei\u00b7nen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "NE", "ADJA", "NN"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Kann euch die Br\u00fccke von Gold nur Polyhymnia baun.", "tokens": ["Kann", "euch", "die", "Br\u00fc\u00b7cke", "von", "Gold", "nur", "Po\u00b7ly\u00b7hym\u00b7nia", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Wie der Gewaltigste selbst im Kampf mit den M\u00e4chten des Schicksals", "tokens": ["Wie", "der", "Ge\u00b7wal\u00b7tigs\u00b7te", "selbst", "im", "Kampf", "mit", "den", "M\u00e4ch\u00b7ten", "des", "Schick\u00b7sals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Hinsinkt, wenn er vom Pfad irrend in Schuld sich verstrickt,", "tokens": ["Hin\u00b7sinkt", ",", "wenn", "er", "vom", "Pfad", "ir\u00b7rend", "in", "Schuld", "sich", "ver\u00b7strickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Zeigt die Trag\u00f6die dir und ersch\u00fcttert in Furcht dich und Mitleid,", "tokens": ["Zeigt", "die", "Tra\u00b7g\u00f6\u00b7die", "dir", "und", "er\u00b7sch\u00fct\u00b7tert", "in", "Furcht", "dich", "und", "Mit\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "KON", "VVFIN", "APPR", "NN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Weil der Verirrung auch du f\u00e4hig dich f\u00fchlst und der Schuld.", "tokens": ["Weil", "der", "Ver\u00b7ir\u00b7rung", "auch", "du", "f\u00e4\u00b7hig", "dich", "f\u00fchlst", "und", "der", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "ADJD", "PPER", "VVFIN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-++--+", "measure": "iambic.septa.invert"}}, "stanza.8": {"line.1": {"text": "K\u00f6nige f\u00fchr' uns der Tragiker vor und vergangene Zeiten,", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "f\u00fchr'", "uns", "der", "Tra\u00b7gi\u00b7ker", "vor", "und", "ver\u00b7gan\u00b7ge\u00b7ne", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Doch der Kom\u00f6de das Volk, wie es sich heute gebart.", "tokens": ["Doch", "der", "Ko\u00b7m\u00f6\u00b7de", "das", "Volk", ",", "wie", "es", "sich", "heu\u00b7te", "ge\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+--+--+---+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Tief zu ersch\u00fcttern vermag uns ein b\u00fcrgerlich Drama, doch bleibt ihm", "tokens": ["Tief", "zu", "er\u00b7sch\u00fct\u00b7tern", "ver\u00b7mag", "uns", "ein", "b\u00fcr\u00b7ger\u00b7lich", "Dra\u00b7ma", ",", "doch", "bleibt", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Eines versagt: das Gem\u00fct wieder vom Druck zu befrein,", "tokens": ["Ei\u00b7nes", "ver\u00b7sagt", ":", "das", "Ge\u00b7m\u00fct", "wie\u00b7der", "vom", "Druck", "zu", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "$.", "ART", "NN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Weil uns die N\u00e4he des Stoffs zudringlich beklemmt und im engen", "tokens": ["Weil", "uns", "die", "N\u00e4\u00b7he", "des", "Stoffs", "zu\u00b7dring\u0