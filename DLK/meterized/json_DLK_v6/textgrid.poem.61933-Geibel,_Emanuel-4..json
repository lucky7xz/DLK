rd", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "ART", "NN", "$,", "ADV", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Immer bem\u00e4ngeln, der Kunst hat es Gen\u00fcge getan.", "tokens": ["Im\u00b7mer", "be\u00b7m\u00e4n\u00b7geln", ",", "der", "Kunst", "hat", "es", "Ge\u00b7n\u00fc\u00b7ge", "ge\u00b7tan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ART", "NN", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Episch ist fertige Tat, der Dramatiker zeigt den", "tokens": ["E\u00b7pisch", "ist", "fer\u00b7ti\u00b7ge", "Tat", ",", "der", "Dra\u00b7ma\u00b7ti\u00b7ker", "zeigt", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie er im Kampfe der Brust reift und zur Handlung erw\u00e4chst.", "tokens": ["Wie", "er", "im", "Kamp\u00b7fe", "der", "Brust", "reift", "und", "zur", "Hand\u00b7lung", "er\u00b7w\u00e4chst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Zweifelt, so lang ihr entwerft, doch mitten im Gusse des Kunstwerks", "tokens": ["Zwei\u00b7felt", ",", "so", "lang", "ihr", "ent\u00b7werft", ",", "doch", "mit\u00b7ten", "im", "Gus\u00b7se", "des", "Kunst\u00b7werks"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "w