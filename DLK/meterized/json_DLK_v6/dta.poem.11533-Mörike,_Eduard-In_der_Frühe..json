{"dta.poem.11533": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "In der Fr\u00fche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Schlaf noch k\u00fchlt das Auge mir,", "tokens": ["Kein", "Schlaf", "noch", "k\u00fchlt", "das", "Au\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort gehet schon der Tag herf\u00fcr", "tokens": ["Dort", "ge\u00b7het", "schon", "der", "Tag", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meinem Kammerfenster.", "tokens": ["An", "mei\u00b7nem", "Kam\u00b7mer\u00b7fens\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es w\u00fchlet mein verst\u00f6rter Sinn", "tokens": ["Es", "w\u00fch\u00b7let", "mein", "ver\u00b7st\u00f6r\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch zwischen Zweifeln her und hin", "tokens": ["Noch", "zwi\u00b7schen", "Zwei\u00b7feln", "her", "und", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schaffet Nachtgespenster.", "tokens": ["Und", "schaf\u00b7fet", "Nacht\u00b7ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aengste, qu\u00e4le", "tokens": ["A\u00b7engs\u00b7te", ",", "qu\u00e4\u00b7le"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Dich nicht l\u00e4nger, meine Seele!", "tokens": ["Dich", "nicht", "l\u00e4n\u00b7ger", ",", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Freu' dich! schon sind da und dorten", "tokens": ["Freu'", "dich", "!", "schon", "sind", "da", "und", "dor\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Morgenglocken wach geworden.", "tokens": ["Mor\u00b7gen\u00b7glo\u00b7cken", "wach", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}