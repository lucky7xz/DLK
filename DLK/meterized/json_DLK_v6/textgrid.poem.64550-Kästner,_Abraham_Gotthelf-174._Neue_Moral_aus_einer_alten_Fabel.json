{"textgrid.poem.64550": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "174. Neue Moral aus einer alten Fabel", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die L\u00f6wenhaut dem Esel abzuziehn,", "tokens": ["Die", "L\u00f6\u00b7wen\u00b7haut", "dem", "E\u00b7sel", "ab\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War doch gewi\u00df von jenem M\u00fcller k\u00fchn.", "tokens": ["War", "doch", "ge\u00b7wi\u00df", "von", "je\u00b7nem", "M\u00fcl\u00b7ler", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PDAT", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn wu\u00dfte sich auch sonst der Esel nicht zu sch\u00fctzen,", "tokens": ["Denn", "wu\u00df\u00b7te", "sich", "auch", "sonst", "der", "E\u00b7sel", "nicht", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00e4lzt er sich im Koth, den R\u00e4uber zu bespritzen.", "tokens": ["So", "w\u00e4lzt", "er", "sich", "im", "Koth", ",", "den", "R\u00e4u\u00b7ber", "zu", "be\u00b7sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}