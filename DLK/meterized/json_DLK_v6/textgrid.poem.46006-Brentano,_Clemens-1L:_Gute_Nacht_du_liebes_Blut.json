{"textgrid.poem.46006": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gute Nacht du liebes Blut", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gute Nacht du liebes Blut", "tokens": ["Gu\u00b7te", "Nacht", "du", "lie\u00b7bes", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rinn noch einmal zu dem lieben Herzen", "tokens": ["Rinn", "noch", "ein\u00b7mal", "zu", "dem", "lie\u00b7ben", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tu dir heut noch was zu gut,", "tokens": ["Tu", "dir", "heut", "noch", "was", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "PIS", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen wirst du deine Lust verscherzen.", "tokens": ["Mor\u00b7gen", "wirst", "du", "dei\u00b7ne", "Lust", "ver\u00b7scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O du sel'ger Feuerquell!", "tokens": ["O", "du", "sel'\u00b7ger", "Feu\u00b7er\u00b7quell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Manchen Wundertraum hast du geschaukelt,", "tokens": ["Man\u00b7chen", "Wun\u00b7der\u00b7traum", "hast", "du", "ge\u00b7schau\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein trunkener Gesell", "tokens": ["Wie", "ein", "trun\u00b7ke\u00b7ner", "Ge\u00b7sell"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "--+---+", "measure": "anapaest.init"}, "line.4": {"text": "Bist du durch das liebste Herz gegaukelt.", "tokens": ["Bist", "du", "durch", "das", "liebs\u00b7te", "Herz", "ge\u00b7gau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Warst so \u00fcberwohl zu Haus,", "tokens": ["Warst", "so", "\u00fc\u00b7ber\u00b7wohl", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den lieben reinen blauen Adern,", "tokens": ["In", "den", "lie\u00b7ben", "rei\u00b7nen", "blau\u00b7en", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Machtest du dich gar zu kraus,", "tokens": ["Mach\u00b7test", "du", "dich", "gar", "zu", "kraus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wu\u00dftest gar nichts von fatalen Badern.", "tokens": ["Wu\u00df\u00b7test", "gar", "nichts", "von", "fa\u00b7ta\u00b7len", "Ba\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Doch du machtest dich zu breit,", "tokens": ["Doch", "du", "mach\u00b7test", "dich", "zu", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hieltest nicht die Flut und nicht die Ebbe,", "tokens": ["Hiel\u00b7test", "nicht", "die", "Flut", "und", "nicht", "die", "Eb\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie das Meer, das seiner Zeit", "tokens": ["Wie", "das", "Meer", ",", "das", "sei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Demutsvoll dem Monde k\u00fc\u00dft die Schleppe.", "tokens": ["De\u00b7muts\u00b7voll", "dem", "Mon\u00b7de", "k\u00fc\u00dft", "die", "Schlep\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Hieltest dich auch nimmer still", "tokens": ["Hiel\u00b7test", "dich", "auch", "nim\u00b7mer", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Triebst ein H\u00e4mmern, Brennen Stechen,", "tokens": ["Triebst", "ein", "H\u00e4m\u00b7mern", ",", "Bren\u00b7nen", "Ste\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer im Hause bleiben will,", "tokens": ["Wer", "im", "Hau\u00b7se", "blei\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df dem Herrn nicht immer widersprechen.", "tokens": ["Mu\u00df", "dem", "Herrn", "nicht", "im\u00b7mer", "wi\u00b7der\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Lang sah man der Unart zu,", "tokens": ["Lang", "sah", "man", "der", "Un\u00b7art", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch verloren ist an dir so Malz als Hopfen,", "tokens": ["Doch", "ver\u00b7lo\u00b7ren", "ist", "an", "dir", "so", "Malz", "als", "Hop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "APPR", "PPER", "ADV", "NE", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Welchen trifft die Strafe nu,", "tokens": ["Wel\u00b7chen", "trifft", "die", "Stra\u00b7fe", "nu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach ihr dauert mich ihr armen Tropfen.", "tokens": ["Ach", "ihr", "dau\u00b7ert", "mich", "ihr", "ar\u00b7men", "Trop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und weil keiner von euch wei\u00df,", "tokens": ["Und", "weil", "kei\u00b7ner", "von", "euch", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob er morgen ins Gericht wird treten", "tokens": ["Ob", "er", "mor\u00b7gen", "ins", "Ge\u00b7richt", "wird", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "VVINF"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sollt ihr alle gleicherweis", "tokens": ["Sollt", "ihr", "al\u00b7le", "glei\u00b7cher\u00b7weis"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heut noch einmal mit der Lieben beten.", "tokens": ["Heut", "noch", "ein\u00b7mal", "mit", "der", "Lie\u00b7ben", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Seid fein still und tuet Bu\u00df',", "tokens": ["Seid", "fein", "still", "und", "tu\u00b7et", "Bu\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJD", "KON", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer von euch sie im Gebete st\u00f6ret", "tokens": ["Wer", "von", "euch", "sie", "im", "Ge\u00b7be\u00b7te", "st\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "PPER", "APPRART", "NN", "VVFIN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Morgen aus dem Hause mu\u00df,", "tokens": ["Mor\u00b7gen", "aus", "dem", "Hau\u00b7se", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlen mu\u00df zuletzt wer nicht geh\u00f6ret.", "tokens": ["F\u00fch\u00b7len", "mu\u00df", "zu\u00b7letzt", "wer", "nicht", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PWS", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Gute Nacht du liebes Blut", "tokens": ["Gu\u00b7te", "Nacht", "du", "lie\u00b7bes", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rinn noch einmal zu dem lieben Herzen,", "tokens": ["Rinn", "noch", "ein\u00b7mal", "zu", "dem", "lie\u00b7ben", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wem es einmal ward so gut,", "tokens": ["Wem", "es", "ein\u00b7mal", "ward", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der kann alles andre leicht verschmerzen.", "tokens": ["Der", "kann", "al\u00b7les", "and\u00b7re", "leicht", "ver\u00b7schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "PIS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}