{"textgrid.poem.33440": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bleibst du Trost der ganzen Welt?", "tokens": ["Wo", "bleibst", "du", "Trost", "der", "gan\u00b7zen", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herberg' ist dir schon l\u00e4ngst bestellt.", "tokens": ["Her\u00b7ber\u00b7g'", "ist", "dir", "schon", "l\u00e4ngst", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Verlangend sieht ein jedes dich,", "tokens": ["Ver\u00b7lan\u00b7gend", "sieht", "ein", "je\u00b7des", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "PIAT", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00f6ffnet deinem Segen sich.", "tokens": ["Und", "\u00f6ff\u00b7net", "dei\u00b7nem", "Se\u00b7gen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geu\u00df, Vater, ihn gewaltig aus,", "tokens": ["Geu\u00df", ",", "Va\u00b7ter", ",", "ihn", "ge\u00b7wal\u00b7tig", "aus", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb ihn aus deinem Arm heraus:", "tokens": ["Gieb", "ihn", "aus", "dei\u00b7nem", "Arm", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Unschuld, Lieb' und s\u00fc\u00dfe Schaam", "tokens": ["Nur", "Un\u00b7schuld", ",", "Lieb'", "und", "s\u00fc\u00b7\u00dfe", "Schaam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hielt ihn, da\u00df er nicht l\u00e4ngst schon kam.", "tokens": ["Hielt", "ihn", ",", "da\u00df", "er", "nicht", "l\u00e4ngst", "schon", "kam", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Treib ihn von dir in unsern Arm,", "tokens": ["Treib", "ihn", "von", "dir", "in", "un\u00b7sern", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er von deinem Hauch noch warm;", "tokens": ["Da\u00df", "er", "von", "dei\u00b7nem", "Hauch", "noch", "warm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In schweren Wolken sammle ihn", "tokens": ["In", "schwe\u00b7ren", "Wol\u00b7ken", "samm\u00b7le", "ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und la\u00df ihn so hernieder ziehn.", "tokens": ["Und", "la\u00df", "ihn", "so", "her\u00b7nie\u00b7der", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In k\u00fchlen Str\u00f6men send' ihn her,", "tokens": ["In", "k\u00fch\u00b7len", "Str\u00f6\u00b7men", "send'", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Feuerflammen lodre er,", "tokens": ["In", "Feu\u00b7er\u00b7flam\u00b7men", "lod\u00b7re", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Luft und Oel, in Klang und Thau", "tokens": ["In", "Luft", "und", "O\u00b7el", ",", "in", "Klang", "und", "Thau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durchdring' er unsrer Erde Bau.", "tokens": ["Durch\u00b7dring'", "er", "uns\u00b7rer", "Er\u00b7de", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So wird der heil'ge Kampf gek\u00e4mpft,", "tokens": ["So", "wird", "der", "heil'\u00b7ge", "Kampf", "ge\u00b7k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wird der H\u00f6lle Grimm ged\u00e4mpft,", "tokens": ["So", "wird", "der", "H\u00f6l\u00b7le", "Grimm", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ewig bl\u00fchend geht allhier", "tokens": ["Und", "e\u00b7wig", "bl\u00fc\u00b7hend", "geht", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das alte Paradies herf\u00fcr.", "tokens": ["Das", "al\u00b7te", "Pa\u00b7ra\u00b7dies", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Erde regt sich, gr\u00fcnt und lebt,", "tokens": ["Die", "Er\u00b7de", "regt", "sich", ",", "gr\u00fcnt", "und", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Geistes voll ein jedes strebt", "tokens": ["Des", "Geis\u00b7tes", "voll", "ein", "je\u00b7des", "strebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ART", "PIAT", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Heiland lieblich zu empfahn", "tokens": ["Den", "Hei\u00b7land", "lieb\u00b7lich", "zu", "em\u00b7pfahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und beut die vollen Br\u00fcst' ihm an.", "tokens": ["Und", "beut", "die", "vol\u00b7len", "Br\u00fcst'", "ihm", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Winter weicht, ein neues Jahr", "tokens": ["Der", "Win\u00b7ter", "weicht", ",", "ein", "neu\u00b7es", "Jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht an der Krippe Hochaltar.", "tokens": ["Steht", "an", "der", "Krip\u00b7pe", "Hoc\u00b7hal\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist das erste Jahr der Welt.", "tokens": ["Es", "ist", "das", "ers\u00b7te", "Jahr", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich dies Kind erst selbst bestellt.", "tokens": ["Die", "sich", "dies", "Kind", "erst", "selbst", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PDS", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Augen sehn den Heiland wohl,", "tokens": ["Die", "Au\u00b7gen", "sehn", "den", "Hei\u00b7land", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch sind sie des Heilands voll,", "tokens": ["Und", "doch", "sind", "sie", "des", "Hei\u00b7lands", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Blumen wird sein Haupt geschm\u00fcckt,", "tokens": ["Von", "Blu\u00b7men", "wird", "sein", "Haupt", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus denen er selbst holdselig blickt.", "tokens": ["Aus", "de\u00b7nen", "er", "selbst", "hold\u00b7se\u00b7lig", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Er ist der Stern, er ist die Sonn',", "tokens": ["Er", "ist", "der", "Stern", ",", "er", "ist", "die", "Sonn'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist des ewgen Lebens Bronn,", "tokens": ["Er", "ist", "des", "ew\u00b7gen", "Le\u00b7bens", "Bronn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Kraut und Stein und Meer und Licht", "tokens": ["Aus", "Kraut", "und", "Stein", "und", "Meer", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schimmert sein kindlich Angesicht.", "tokens": ["Schim\u00b7mert", "sein", "kind\u00b7lich", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "In allen Dingen sein kindlich Thun.", "tokens": ["In", "al\u00b7len", "Din\u00b7gen", "sein", "kind\u00b7lich", "Thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seine hei\u00dfe Liebe wird nimmer ruhn,", "tokens": ["Sei\u00b7ne", "hei\u00b7\u00dfe", "Lie\u00b7be", "wird", "nim\u00b7mer", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Er schmiegt sich seiner unbewu\u00dft", "tokens": ["Er", "schmiegt", "sich", "sei\u00b7ner", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unendlich fest an jede Brust.", "tokens": ["Un\u00b7end\u00b7lich", "fest", "an", "je\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ein Gott f\u00fcr uns, ein Kind f\u00fcr sich", "tokens": ["Ein", "Gott", "f\u00fcr", "uns", ",", "ein", "Kind", "f\u00fcr", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ART", "NN", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liebt er uns all' herzinniglich,", "tokens": ["Liebt", "er", "uns", "all'", "her\u00b7zin\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PIS", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wird unsre Speis' und unser Trank,", "tokens": ["Wird", "uns\u00b7re", "Speis'", "und", "un\u00b7ser", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Treusinn ist ihm der liebste Dank.", "tokens": ["Treu\u00b7sinn", "ist", "ihm", "der", "liebs\u00b7te", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Das Elend w\u00e4chst je mehr und mehr,", "tokens": ["Das", "E\u00b7lend", "w\u00e4chst", "je", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein d\u00fcstrer Gram bedr\u00fcckt uns sehr,", "tokens": ["Ein", "d\u00fcst\u00b7rer", "Gram", "be\u00b7dr\u00fcckt", "uns", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df, Vater, den Geliebten gehn,", "tokens": ["La\u00df", ",", "Va\u00b7ter", ",", "den", "Ge\u00b7lieb\u00b7ten", "gehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit uns wirst du ihn wieder sehn.", "tokens": ["Mit", "uns", "wirst", "du", "ihn", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}