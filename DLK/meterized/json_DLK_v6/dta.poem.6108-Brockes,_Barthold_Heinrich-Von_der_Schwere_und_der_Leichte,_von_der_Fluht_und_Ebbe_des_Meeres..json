{"dta.poem.6108": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch zwo Bewegungen erf\u00fcllen die Gedancken.", "tokens": ["Noch", "zwo", "Be\u00b7we\u00b7gun\u00b7gen", "er\u00b7f\u00fcl\u00b7len", "die", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die, so der C\u00f6rper Fall verursacht und die Schwere:", "tokens": ["Die", ",", "so", "der", "C\u00f6r\u00b7per", "Fall", "ver\u00b7ur\u00b7sacht", "und", "die", "Schwe\u00b7re", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ART", "NN", "NN", "VVPP", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die, wodurch im Gegentheil die Meere", "tokens": ["Und", "die", ",", "wo\u00b7durch", "im", "Ge\u00b7gen\u00b7theil", "die", "Mee\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "$,", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedruckt sind und gedr\u00e4ngt aus ihres Ufers Schrancken.", "tokens": ["Ge\u00b7druckt", "sind", "und", "ge\u00b7dr\u00e4ngt", "aus", "ih\u00b7res", "U\u00b7fers", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}