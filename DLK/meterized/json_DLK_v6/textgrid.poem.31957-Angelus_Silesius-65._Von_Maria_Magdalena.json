{"textgrid.poem.31957": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "65. Von Maria Magdalena", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tr\u00e4nen, welche du bei unsers Herren F\u00fc\u00dfen", "tokens": ["Die", "Tr\u00e4\u00b7nen", ",", "wel\u00b7che", "du", "bei", "un\u00b7sers", "Her\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nasse Magdalen so h\u00e4ufig siehst vergie\u00dfen,", "tokens": ["Die", "nas\u00b7se", "Mag\u00b7da\u00b7len", "so", "h\u00e4u\u00b7fig", "siehst", "ver\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind ihr zerschmolznes Herz. Dies kr\u00e4nket sie allein,", "tokens": ["Sind", "ihr", "zer\u00b7schmolz\u00b7nes", "Herz", ".", "Dies", "kr\u00e4n\u00b7ket", "sie", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$.", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df nicht ihr Seel und Leib ganz sollen Tr\u00e4nen sein. ", "tokens": ["Da\u00df", "nicht", "ihr", "Seel", "und", "Leib", "ganz", "sol\u00b7len", "Tr\u00e4\u00b7nen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "KON", "NN", "ADV", "PIA