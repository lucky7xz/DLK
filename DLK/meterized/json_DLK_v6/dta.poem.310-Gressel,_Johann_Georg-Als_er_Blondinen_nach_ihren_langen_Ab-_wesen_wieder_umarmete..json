{"dta.poem.310": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er  Blondinen  nach ihren langen Ab-  \n wesen wieder umarmete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckliches Schicksahl/ himmlische Freude", "tokens": ["Gl\u00fcck\u00b7li\u00b7ches", "Schick\u00b7sahl", "/", "himm\u00b7li\u00b7sche", "Freu\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die ihr nach \u00fcberstandenen Leide", "tokens": ["Die", "ihr", "nach", "\u00fc\u00b7bers\u00b7tan\u00b7de\u00b7nen", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Meine bek\u00fcmmerte Geister erquickt/", "tokens": ["Mei\u00b7ne", "be\u00b7k\u00fcm\u00b7mer\u00b7te", "Geis\u00b7ter", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und mich dadurch \u00fcberm\u00e4\u00dfig begl\u00fcckt.", "tokens": ["Und", "mich", "da\u00b7durch", "\u00fc\u00b7berm\u00b7\u00e4\u00b7\u00dfig", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "ADJD", "VVPP", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Da\u00df ich ", "tokens": ["Da\u00df", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und ihres Schnee-Geb\u00fcrgs Albasterne Ballen", "tokens": ["Und", "ih\u00b7res", "Schnee\u00b7Ge\u00b7b\u00fcrgs", "Al\u00b7bas\u00b7ter\u00b7ne", "Bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wieder anschauen und k\u00fcssen kan.", "tokens": ["Wie\u00b7der", "an\u00b7schau\u00b7en", "und", "k\u00fcs\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schencke mir Himmel ferner die Blicke", "tokens": ["Schen\u00b7cke", "mir", "Him\u00b7mel", "fer\u00b7ner", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "ADV", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "H\u00e4uffe die Anmuth/ mehre mein Gl\u00fccke/", "tokens": ["H\u00e4uf\u00b7fe", "die", "An\u00b7muth", "/", "meh\u00b7re", "mein", "Gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "PIAT", "PPOSAT", "NN", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schencke mir Zucker der Lieblichkeit ein/", "tokens": ["Schen\u00b7cke", "mir", "Zu\u00b7cker", "der", "Lieb\u00b7lich\u00b7keit", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "ART", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "La\u00df die ", "tokens": ["La\u00df", "die"], "token_info": ["word", "word"], "pos": ["VVIMP", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die mir Vergn\u00fcgung und gl\u00fcckseliges Leben/", "tokens": ["Die", "mir", "Ver\u00b7gn\u00fc\u00b7gung", "und", "gl\u00fcck\u00b7se\u00b7li\u00b7ges", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Durch ihre erquickende Blicke kan geben/", "tokens": ["Durch", "ih\u00b7re", "er\u00b7qui\u00b7cken\u00b7de", "Bli\u00b7cke", "kan", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wenn mich ihr holdes Augen-Paar k\u00fc\u00dft.", "tokens": ["Wenn", "mich", "ihr", "hol\u00b7des", "Au\u00b7gen\u00b7Paar", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "K\u00fcsse mich Sch\u00f6nste/ schencke mir Strahlen", "tokens": ["K\u00fcs\u00b7se", "mich", "Sch\u00f6ns\u00b7te", "/", "schen\u00b7cke", "mir", "Strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "$(", "VVFIN", "PPER", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die mir erquickende Blicke zahlen", "tokens": ["Die", "mir", "er\u00b7qui\u00b7cken\u00b7de", "Bli\u00b7cke", "zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Fl\u00f6sse den ", "tokens": ["Fl\u00f6s\u00b7se", "den"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Lasse die Wangen ein Himmels-Brodt seyn/", "tokens": ["Las\u00b7se", "die", "Wan\u00b7gen", "ein", "Him\u00b7mels\u00b7Brodt", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VAINF", "$("], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Und speise die Seele mit Manna der L\u00fcste", "tokens": ["Und", "spei\u00b7se", "die", "See\u00b7le", "mit", "Man\u00b7na", "der", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Welches sich befindet am H\u00fcgel der Br\u00fcste/", "tokens": ["Wel\u00b7ches", "sich", "be\u00b7fin\u00b7det", "am", "H\u00fc\u00b7gel", "der", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Wo mir ein ", "tokens": ["Wo", "mir", "ein"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}}}}}