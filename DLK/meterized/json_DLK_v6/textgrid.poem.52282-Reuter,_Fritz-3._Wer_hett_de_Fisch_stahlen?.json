{"textgrid.poem.52282": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "3. Wer hett de Fisch stahlen?", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ut unsen Dik, dor wiren Fisch ens stahlen.", "tokens": ["Ut", "un\u00b7sen", "Dik", ",", "dor", "wi\u00b7ren", "Fisch", "ens", "stah\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "ADV", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "En Jung', de 's Abends sp\u00e4t de Fahlen", "tokens": ["En", "Jung'", ",", "de", "'s", "A\u00b7bends", "sp\u00e4t", "de", "Fah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "ADV", "ADJD", "NE", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In ehre Koppel br\u00f6cht, de hadd tauf\u00e4llig grad", "tokens": ["In", "eh\u00b7re", "Kop\u00b7pel", "br\u00f6cht", ",", "de", "hadd", "tau\u00b7f\u00e4l\u00b7lig", "grad"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drei Kirls seihn, de mit 'ne Wad'", "tokens": ["Drei", "Kirls", "seihn", ",", "de", "mit", "'", "ne", "Wad", "'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,", "NE", "APPR", "$(", "FM", "FM", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi't Fischlock wiren;", "tokens": ["Bi't", "Fisc\u00b7hlock", "wi\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ok hadd hei sei sick n\u00e4umen h\u00fcren.", "tokens": ["Ok", "hadd", "hei", "sei", "sick", "n\u00e4u\u00b7men", "h\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Doch \u00bbKorl\u00ab un \u00bbKrischan\u00ab un \u00bbJehann\u00ab,", "tokens": ["Doch", "\u00bb", "Korl", "\u00ab", "un", "\u00bb", "Krisc\u00b7han", "\u00ab", "un", "\u00bb", "Je\u00b7hann", "\u00ab", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "$(", "NE", "$(", "FM", "$(", "NN", "$(", "FM", "$(", "NE", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dat s\u00fcnd so'n Namen, de f\u00fchrt jedermann.", "tokens": ["Dat", "s\u00fcnd", "so'n", "Na\u00b7men", ",", "de", "f\u00fchrt", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "VVFIN", "PIS", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Indessen was't doch wat,", "tokens": ["In\u00b7des\u00b7sen", "was't", "doch", "wat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Un endlich kamm't ok rute, dat", "tokens": ["Un", "end\u00b7lich", "kam\u00b7m't", "ok", "ru\u00b7te", ",", "dat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "VVFIN", "NE", "VVFIN", "$,", "PRELS"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sei alle drei ut unsen D\u00f6rpen wiren.", "tokens": ["Sei", "al\u00b7le", "drei", "ut", "un\u00b7sen", "D\u00f6r\u00b7pen", "wi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "CARD", "FM", "FM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dunn heit dat denn: kein Tid verliren!", "tokens": ["Dunn", "heit", "dat", "denn", ":", "kein", "Tid", "ver\u00b7li\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "$.", "PIAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Un all de Korls un all de Krischans und", "tokens": ["Un", "all", "de", "Korls", "un", "all", "de", "Krisc\u00b7hans", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "FM", "FM", "FM", "PIAT", "NE", "NE", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "All de Jehanns, de w\u00fcrden ingespunnt.", "tokens": ["All", "de", "Je\u00b7hanns", ",", "de", "w\u00fcr\u00b7den", "in\u00b7ge\u00b7spunnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,", "NE", "VAFIN", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Na, dat was gaud; de Amtmann kreg sei v\u00f6r,", "tokens": ["Na", ",", "dat", "was", "gaud", ";", "de", "Amt\u00b7mann", "kreg", "sei", "v\u00f6r", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "PIS", "VVFIN", "$.", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Hei frog sei in de Kr\u00fcz un Quer;", "tokens": ["Hei", "frog", "sei", "in", "de", "Kr\u00fcz", "un", "Quer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "De Kirls, de logen as gedruckt.", "tokens": ["De", "Kirls", ",", "de", "lo\u00b7gen", "as", "ge\u00b7druckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nu w\u00fcrd em eklich upgespuckt,", "tokens": ["Nu", "w\u00fcrd", "em", "ek\u00b7lich", "up\u00b7ge\u00b7spuckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Un as de Amtmann gl\u00f6wt, nu wiren s' m\u00f6r,", "tokens": ["Un", "as", "de", "Amt\u00b7mann", "gl\u00f6wt", ",", "nu", "wi\u00b7ren", "s'", "m\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VVFIN", "$,", "ADV", "VAFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Dunn kreg hei sei von frischen v\u00f6r.", "tokens": ["Dunn", "kreg", "hei", "sei", "von", "fri\u00b7schen", "v\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Je Kuchen! Unse Kirls, de logen,", "tokens": ["Je", "Ku\u00b7chen", "!", "Un\u00b7se", "Kirls", ",", "de", "lo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "PPOSAT", "NN", "$,", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Dat sick de Balken dorvon bogen.", "tokens": ["Dat", "sick", "de", "Bal\u00b7ken", "dor\u00b7von", "bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "De Amtmann lep de Stuw woll up un dal,", "tokens": ["De", "Amt\u00b7mann", "lep", "de", "Stuw", "woll", "up", "un", "dal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "De Amtmann kratzt sick achtr'e Uhren;", "tokens": ["De", "Amt\u00b7mann", "kratzt", "sick", "a\u00b7chtr'e", "Uh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "De Sak, de w\u00fcrd em ganz fatal,", "tokens": ["De", "Sak", ",", "de", "w\u00fcrd", "em", "ganz", "fa\u00b7tal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VAFIN", "ADJA", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Hei k\u00fcnn de Kirls nich beluren.", "tokens": ["Hei", "k\u00fcnn", "de", "Kirls", "nich", "be\u00b7lu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.27": {"text": "Hei schow sin Brill woll up un nedder,", "tokens": ["Hei", "schow", "sin", "Brill", "woll", "up", "un", "ned\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "Kek denn in sine Akten wedder,", "tokens": ["Kek", "denn", "in", "si\u00b7ne", "Ak\u00b7ten", "wed\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.29": {"text": "Kek in de allerdicksten B\u00e4uker,", "tokens": ["Kek", "in", "de", "al\u00b7ler\u00b7dicks\u00b7ten", "B\u00e4u\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Hei w\u00fcrd ok nich en beten kl\u00e4uker;", "tokens": ["Hei", "w\u00fcrd", "ok", "nich", "en", "be\u00b7ten", "kl\u00e4u\u00b7ker", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "PTKNEG", "NE", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.31": {"text": "Un't wull d\u00f6rchut em nich gelingen,", "tokens": ["Un't", "wull", "d\u00f6r\u00b7chut", "em", "nich", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Von ehr de Wohrheit 'rut tau bringen.", "tokens": ["Von", "ehr", "de", "Wohr\u00b7heit", "'rut", "tau", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NN", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Hal jug verfluchten Kirls de Deuker!", "tokens": ["Hal", "jug", "ver\u00b7fluch\u00b7ten", "Kirls", "de", "Deu\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.34": {"text": "In sine Not g\u00fcng hei taum letzten", "tokens": ["In", "si\u00b7ne", "Not", "g\u00fcng", "hei", "taum", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.35": {"text": "Un frog \u00fcm Rat bi sinen V\u00f6rgesetzten.", "tokens": ["Un", "frog", "\u00fcm", "Rat", "bi", "si\u00b7nen", "V\u00f6r\u00b7ge\u00b7setz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.36": {"text": "\u00bbmein lieber Freund, Sie haben Ihre Sachen", "tokens": ["\u00bb", "mein", "lie\u00b7ber", "Freund", ",", "Sie", "ha\u00b7ben", "Ih\u00b7re", "Sa\u00b7chen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Nicht gut gemacht, Sie m\u00fcssen's anders machen.", "tokens": ["Nicht", "gut", "ge\u00b7macht", ",", "Sie", "m\u00fcs\u00b7sen's", "an\u00b7ders", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Oh, gah man einer hen und raupt man", "tokens": ["Oh", ",", "gah", "man", "ei\u00b7ner", "hen", "und", "raupt", "man"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PIS", "ART", "ADJA", "KON", "VVFIN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "De Kirls hir mal 'rin\u00ab, s\u00e4d de Amtshauptmann.", "tokens": ["De", "Kirls", "hir", "mal", "'rin", "\u00ab", ",", "s\u00e4d", "de", "Amts\u00b7haupt\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADV", "NE", "$(", "$,", "FM", "FM", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "De Kirls kemen 'rin. \u00bbNa, h\u00fcrt mal\u00ab, s\u00e4d de Oll,", "tokens": ["De", "Kirls", "ke\u00b7men", "'rin", ".", "\u00bb", "Na", ",", "h\u00fcrt", "mal", "\u00ab", ",", "s\u00e4d", "de", "Oll", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "$.", "$(", "ITJ", "$,", "VVFIN", "ADV", "$(", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "\u00bbick b\u00fcn Amtshauptmann hir, ji kennt mi woll,", "tokens": ["\u00bb", "ick", "b\u00fcn", "Amts\u00b7haupt\u00b7mann", "hir", ",", "ji", "kennt", "mi", "woll", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "VVFIN", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Ji Sl\u00fcngels staht hir v\u00f6r Gericht,", "tokens": ["Ji", "Sl\u00fcn\u00b7gels", "staht", "hir", "v\u00f6r", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Ji staht hir v\u00f6r Amtshauptmann Wewern!\u00ab", "tokens": ["Ji", "staht", "hir", "v\u00f6r", "Amts\u00b7haupt\u00b7mann", "We\u00b7wern", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Un dorbi makt hei so'n vergritzt Gesicht,", "tokens": ["Un", "dor\u00b7bi", "makt", "hei", "so'n", "ver\u00b7gritzt", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVPP", "NN", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.45": {"text": "Dat all de Kirls fung'n an tau bewern.", "tokens": ["Dat", "all", "de", "Kirls", "fung'n", "an", "tau", "be\u00b7wern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "\u00bbnu pa\u00dft mal up un h\u00fcrt mal tau!", "tokens": ["\u00bb", "nu", "pa\u00dft", "mal", "up", "un", "h\u00fcrt", "mal", "tau", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Un dauht, wat ick jug heiten dauh:", "tokens": ["Un", "dauht", ",", "wat", "ick", "jug", "hei\u00b7ten", "dauh", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.48": {"text": "De stahlen hewwen, bliwen stahn,", "tokens": ["De", "stah\u00b7len", "hew\u00b7wen", ",", "bli\u00b7wen", "stahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$