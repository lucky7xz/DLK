{"dta.poem.12890": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er stellt der Amarillis ihre grau-  \n samkeit vor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer kan der s\u00fc\u00dfen macht der liebe widerstehn?", "tokens": ["Wer", "kan", "der", "s\u00fc\u00b7\u00dfen", "macht", "der", "lie\u00b7be", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "VVFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es mu\u00df der strenge Mars in ihren banden gehn.", "tokens": ["Es", "mu\u00df", "der", "stren\u00b7ge", "Mars", "in", "ih\u00b7ren", "ban\u00b7den", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist wol in der welt, das ihr nicht dienen m\u00fcsse?", "tokens": ["Was", "ist", "wol", "in", "der", "welt", ",", "das", "ihr", "nicht", "die\u00b7nen", "m\u00fcs\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lufft, erd\u2019 und himmel liebt. So weit die sonne sticht,", "tokens": ["Lufft", ",", "erd'", "und", "him\u00b7mel", "liebt", ".", "So", "weit", "die", "son\u00b7ne", "sticht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "NN", "VVFIN", "$.", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Legt ihr ein ieder mensch das hertze vor die f\u00fc\u00dfe;", "tokens": ["Legt", "ihr", "ein", "ie\u00b7der", "mensch", "das", "hert\u00b7ze", "vor", "die", "f\u00fc\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PIAT", "NN", "ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur Amarillis nicht.", "tokens": ["Nur", "A\u00b7ma\u00b7ril\u00b7lis", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer l\u00e4st der seuffzer schall, so ein getreuer geist,", "tokens": ["Wer", "l\u00e4st", "der", "seuff\u00b7zer", "schall", ",", "so", "ein", "ge\u00b7treu\u00b7er", "geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(der in der unschuld sich mit angst und kummer speist,)", "tokens": ["(", "der", "in", "der", "un\u00b7schuld", "sich", "mit", "angst", "und", "kum\u00b7mer", "speist", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "APPR", "ART", "NN", "PRF", "APPR", "VVPP", "KON", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus d\u00fcrrem munde st\u00f6st, ihm nicht zu hertzen steigen?", "tokens": ["Aus", "d\u00fcr\u00b7rem", "mun\u00b7de", "st\u00f6st", ",", "ihm", "nicht", "zu", "hert\u00b7zen", "stei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des himmels starcker zorn, so durch die wolcken bricht,", "tokens": ["Des", "him\u00b7mels", "star\u00b7cker", "zorn", ",", "so", "durch", "die", "wol\u00b7cken", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "L\u00e4st sich, so gro\u00df er ist, durch ach und wehmuth beugen;", "tokens": ["L\u00e4st", "sich", ",", "so", "gro\u00df", "er", "ist", ",", "durch", "ach", "und", "weh\u00b7muth", "beu\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,", "APPR", "XY", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur Amarillis nicht.", "tokens": ["Nur", "A\u00b7ma\u00b7ril\u00b7lis", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein stein wird nach und nach vom regen ausgeschweifft:", "tokens": ["Ein", "stein", "wird", "nach", "und", "nach", "vom", "re\u00b7gen", "aus\u00b7ge\u00b7schweifft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "KON", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wird der steruen grimm durch thr\u00e4nen-flut erf\u00e4ufft.", "tokens": ["Es", "wird", "der", "ste\u00b7ru\u00b7en", "grimm", "durch", "thr\u00e4\u00b7nen\u00b7flut", "er\u00b7f\u00e4ufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die wellen haben offt auch felsen umgeschmissen.", "tokens": ["Die", "wel\u00b7len", "ha\u00b7ben", "offt", "auch", "fel\u00b7sen", "um\u00b7ge\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein wenig wasser l\u00f6scht der flammen hei\u00dfes licht:", "tokens": ["Ein", "we\u00b7nig", "was\u00b7ser", "l\u00f6scht", "der", "flam\u00b7men", "hei\u00b7\u00dfes", "licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der mensch wird durch die krafft der thr\u00e4uen hingerissen;", "tokens": ["Der", "mensch", "wird", "durch", "die", "krafft", "der", "thr\u00e4u\u00b7en", "hin\u00b7ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur Amarillis nicht.", "tokens": ["Nur", "A\u00b7ma\u00b7ril\u00b7lis", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, sch\u00f6ne Grausame! so wilst du mehr als stein,", "tokens": ["Ach", ",", "sch\u00f6\u00b7ne", "Grau\u00b7sa\u00b7me", "!", "so", "wilst", "du", "mehr", "als", "stein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "ADV", "VMFIN", "PPER", "PIAT", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und himmel, und dabey ein wilder unmensch seyn?", "tokens": ["Und", "him\u00b7mel", ",", "und", "da\u00b7bey", "ein", "wil\u00b7der", "un\u00b7mensch", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "PAV", "ART", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df thr\u00e4nen, lieb und ach doch deinen grimm vertilgen;", "tokens": ["La\u00df", "thr\u00e4\u00b7nen", ",", "lieb", "und", "ach", "doch", "dei\u00b7nen", "grimm", "ver\u00b7til\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "ADJD", "KON", "ADV", "ADV", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit der himmel dir nicht dieses urtheil spricht:", "tokens": ["Da\u00b7mit", "der", "him\u00b7mel", "dir", "nicht", "die\u00b7ses", "ur\u00b7theil", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "PTKNEG", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es geh die gantze welt auf rosen und auf lilgen;", "tokens": ["Es", "geh", "die", "gant\u00b7ze", "welt", "auf", "ro\u00b7sen", "und", "auf", "lil\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur Amarillis nicht.", "tokens": ["Nur", "A\u00b7ma\u00b7ril\u00b7lis", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}