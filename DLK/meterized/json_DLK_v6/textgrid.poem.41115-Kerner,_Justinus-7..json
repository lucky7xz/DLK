{"textgrid.poem.41115": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon lieget sie in tiefem Schlummer,", "tokens": ["Schon", "lie\u00b7get", "sie", "in", "tie\u00b7fem", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O w\u00fcrden sel'ge Tr\u00e4ume ihr!", "tokens": ["O", "w\u00fcr\u00b7den", "sel'\u00b7ge", "Tr\u00e4u\u00b7me", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indessen ich in herbem Kummer", "tokens": ["In\u00b7des\u00b7sen", "ich", "in", "her\u00b7bem", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch wach' an ihrem Lager hier.", "tokens": ["Noch", "wach'", "an", "ih\u00b7rem", "La\u00b7ger", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchle ihres Busens Wallen,", "tokens": ["Ich", "f\u00fch\u00b7le", "ih\u00b7res", "Bu\u00b7sens", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00f6r' das Atmen ihrer Brust,", "tokens": ["Ich", "h\u00f6r'", "das", "At\u00b7men", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meines Auges Tr\u00e4nen fallen", "tokens": ["Und", "mei\u00b7nes", "Au\u00b7ges", "Tr\u00e4\u00b7nen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hei\u00df auf ihr Herz, ihr unbewu\u00dft.", "tokens": ["Hei\u00df", "auf", "ihr", "Herz", ",", "ihr", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Tr\u00e4nen! st\u00f6ret nicht ihr Tr\u00e4umen,", "tokens": ["Ihr", "Tr\u00e4\u00b7nen", "!", "st\u00f6\u00b7ret", "nicht", "ihr", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf da\u00df sie nicht zum Schmerz erwacht,", "tokens": ["Auf", "da\u00df", "sie", "nicht", "zum", "Schmerz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Die Welt verschwind' ihr bis zum Morgen", "tokens": ["Die", "Welt", "ver\u00b7schwind'", "ihr", "bis", "zum", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihren Menschen, ihrer Pein.", "tokens": ["Mit", "ih\u00b7ren", "Men\u00b7schen", ",", "ih\u00b7rer", "Pein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erwacht, da brennt ihr Herz voll Sorgen;", "tokens": ["Er\u00b7wacht", ",", "da", "brennt", "ihr", "Herz", "voll", "Sor\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlief' ich mit ihr auf ewig ein!", "tokens": ["Schlie\u00b7f'", "ich", "mit", "ihr", "auf", "e\u00b7wig", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "APPR", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}