{"dta.poem.218": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet sie \u00fcber alles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lebe blo\u00df in dir mein auserw\u00e4hltes Leben;", "tokens": ["Ich", "le\u00b7be", "blo\u00df", "in", "dir", "mein", "au\u00b7ser\u00b7w\u00e4hl\u00b7tes", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lange mir die Lufft wird Geist und Oden geben/", "tokens": ["So", "lan\u00b7ge", "mir", "die", "Lufft", "wird", "Geist", "und", "O\u00b7den", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "VAFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lange soll mein Hertz/ mein Kind best\u00e4ndig seyn/", "tokens": ["So", "lan\u00b7ge", "soll", "mein", "Hertz", "/", "mein", "Kind", "be\u00b7st\u00e4n\u00b7dig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es reist noch Noht noch Tod die veste Treue ein.", "tokens": ["Es", "reist", "noch", "Noht", "noch", "Tod", "die", "ves\u00b7te", "Treu\u00b7e", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Einander mag sein Gl\u00fcck auf Gold und Silber setzen/", "tokens": ["Ein\u00b7an\u00b7der", "mag", "sein", "Gl\u00fcck", "auf", "Gold", "und", "Sil\u00b7ber", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich suche blo\u00df in dir mein eintziges Erg\u00f6tzen/", "tokens": ["Ich", "su\u00b7che", "blo\u00df", "in", "dir", "mein", "eint\u00b7zi\u00b7ges", "Er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Gold der reinen Gunst macht mich vor allen reich/", "tokens": ["Das", "Gold", "der", "rei\u00b7nen", "Gunst", "macht", "mich", "vor", "al\u00b7len", "reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PIAT", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und vor der Liljen-Brust wird helles Silber bleich.", "tokens": ["Und", "vor", "der", "Lil\u00b7jen\u00b7Brust", "wird", "hel\u00b7les", "Sil\u00b7ber", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie der ", "tokens": ["Wie", "der"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Wie nach dem Agtstein sich ein leichtes Spreu bem\u00fchet/", "tokens": ["Wie", "nach", "dem", "Agt\u00b7stein", "sich", "ein", "leich\u00b7tes", "Spreu", "be\u00b7m\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kehrt mein Hertze sich zu deiner Sch\u00f6nheit hin/", "tokens": ["So", "kehrt", "mein", "Hert\u00b7ze", "sich", "zu", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zeiget/ da\u00df ich dir als Knecht ergeben bin/", "tokens": ["Und", "zei\u00b7get", "/", "da\u00df", "ich", "dir", "als", "Knecht", "er\u00b7ge\u00b7ben", "bin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "PPER", "KOUS", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So wie dein Auge will/ so lasse ich mich f\u00fchren/", "tokens": ["So", "wie", "dein", "Au\u00b7ge", "will", "/", "so", "las\u00b7se", "ich", "mich", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wie ein Schiff das Steur mu\u00df lencken und regieren/", "tokens": ["So", "wie", "ein", "Schiff", "das", "Steur", "mu\u00df", "len\u00b7cken", "und", "re\u00b7gie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird mein Hertz gef\u00fchrt durch deiner Sch\u00f6nheit-Zier/", "tokens": ["So", "wird", "mein", "Hertz", "ge\u00b7f\u00fchrt", "durch", "dei\u00b7ner", "Sch\u00f6n\u00b7heit\u00b7Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bist mein Seelen-Licht ich lebe blo\u00df in dir.", "tokens": ["Du", "bist", "mein", "See\u00b7len\u00b7Licht", "ich", "le\u00b7be", "blo\u00df", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}