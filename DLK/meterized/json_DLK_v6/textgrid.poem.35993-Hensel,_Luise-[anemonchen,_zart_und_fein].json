{"textgrid.poem.35993": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[anemonchen, zart und fein]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will sich auch des Lebens freun,", "tokens": ["Will", "sich", "auch", "des", "Le\u00b7bens", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will mit seinem wei\u00dfen Glanz", "tokens": ["Will", "mit", "sei\u00b7nem", "wei\u00b7\u00dfen", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schm\u00fccken deinen Lebenskranz.", "tokens": ["Schm\u00fc\u00b7cken", "dei\u00b7nen", "Le\u00b7bens\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Anemonchen, sch\u00f6n und fein,", "tokens": ["A\u00b7ne\u00b7mon\u00b7chen", ",", "sch\u00f6n", "und", "fein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht so frisch und freundlich drein,", "tokens": ["Sieht", "so", "frisch", "und", "freund\u00b7lich", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Guckt so klug aus zartem Gras,", "tokens": ["Guckt", "so", "klug", "aus", "zar\u00b7tem", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fragt so gern um dies und das,", "tokens": ["Fragt", "so", "gern", "um", "dies", "und", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PDS", "KON", "PDS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir zur Lust will's freundlich bl\u00fchn,", "tokens": ["Dir", "zur", "Lust", "will's", "freund\u00b7lich", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir vergelten deine M\u00fchn.", "tokens": ["Dir", "ver\u00b7gel\u00b7ten", "dei\u00b7ne", "M\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}