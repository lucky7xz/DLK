{"textgrid.poem.59208": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren zwei junge Grafen", "tokens": ["Es", "wa\u00b7ren", "zwei", "jun\u00b7ge", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verliebt bis in den Tod,", "tokens": ["Ver\u00b7liebt", "bis", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die konnten nicht ruhn, noch schlafen", "tokens": ["Die", "konn\u00b7ten", "nicht", "ruhn", ",", "noch", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVINF", "$,", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis an den Morgen rot.", "tokens": ["Bis", "an", "den", "Mor\u00b7gen", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O trau den zwei Gesellen,", "tokens": ["O", "trau", "den", "zwei", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Liebchen, nimmermehr,", "tokens": ["Mein", "Lieb\u00b7chen", ",", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die gehn wie Wind und Wellen,", "tokens": ["Die", "gehn", "wie", "Wind", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott wei\u00df: wohin, woher. \u2013", "tokens": ["Gott", "wei\u00df", ":", "wo\u00b7hin", ",", "wo\u00b7her", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$.", "PWAV", "$,", "PWAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir gr\u00fc\u00dfen Land und Sterne", "tokens": ["Wir", "gr\u00fc\u00b7\u00dfen", "Land", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit wunderbarem Klang", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7rem", "Klang"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer uns sp\u00fcrt von ferne,", "tokens": ["Und", "wer", "uns", "sp\u00fcrt", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem wird so wohl und bang.", "tokens": ["Dem", "wird", "so", "wohl", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir haben wohl hienieden", "tokens": ["Wir", "ha\u00b7ben", "wohl", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Haus an keinem Ort,", "tokens": ["Kein", "Haus", "an", "kei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es reisen die Gedanken", "tokens": ["Es", "rei\u00b7sen", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Heimat ewig fort.", "tokens": ["Zur", "Hei\u00b7mat", "e\u00b7wig", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie eines Stromes Dringen", "tokens": ["Wie", "ei\u00b7nes", "Stro\u00b7mes", "Drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht unser Lebenslauf,", "tokens": ["Geht", "un\u00b7ser", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesanges Macht und Ringen", "tokens": ["Ge\u00b7san\u00b7ges", "Macht", "und", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tut helle Augen auf.", "tokens": ["Tut", "hel\u00b7le", "Au\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und Ufer, Wolkenfl\u00fcgel,", "tokens": ["Und", "U\u00b7fer", ",", "Wol\u00b7ken\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Liebe hoch und mild \u2013", "tokens": ["Die", "Lie\u00b7be", "hoch", "und", "mild", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird in diesem Spiegel", "tokens": ["Es", "wird", "in", "die\u00b7sem", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ganze Welt zum Bild.", "tokens": ["Die", "gan\u00b7ze", "Welt", "zum", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dich r\u00fchrt die frische Helle,", "tokens": ["Dich", "r\u00fchrt", "die", "fri\u00b7sche", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Rauschen heimlich k\u00fchl,", "tokens": ["Das", "Rau\u00b7schen", "heim\u00b7lich", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das lockt dich zu der Welle,", "tokens": ["Das", "lockt", "dich", "zu", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil's drau\u00dfen leer und schw\u00fcl.", "tokens": ["Weil's", "drau\u00b7\u00dfen", "leer", "und", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch wolle nie dir halten", "tokens": ["Doch", "wol\u00b7le", "nie", "dir", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Bilder Wunderfest,", "tokens": ["Der", "Bil\u00b7der", "Wun\u00b7der\u00b7fest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tot wird ihr freies Walten,", "tokens": ["Tot", "wird", "ihr", "frei\u00b7es", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4ltst du es weltlich fest.", "tokens": ["H\u00e4ltst", "du", "es", "welt\u00b7lich", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Kein Bett darf er hier finden.", "tokens": ["Kein", "Bett", "darf", "er", "hier", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in den T\u00e4lern sch\u00f6n", "tokens": ["Wohl", "in", "den", "T\u00e4\u00b7lern", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Siehst du sein Gold sich winden,", "tokens": ["Siehst", "du", "sein", "Gold", "sich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann pl\u00f6tzlich meerw\u00e4rts drehn.", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", "meer\u00b7w\u00e4rts", "drehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}