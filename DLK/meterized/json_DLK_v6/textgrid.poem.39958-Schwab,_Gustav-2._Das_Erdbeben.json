{"textgrid.poem.39958": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2. Das Erdbeben", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was regt sich sanfter, als die Mutterbrust,", "tokens": ["Was", "regt", "sich", "sanf\u00b7ter", ",", "als", "die", "Mut\u00b7ter\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die, s\u00e4ugend, von des Kindes Zug sich hebt?", "tokens": ["Die", ",", "s\u00e4u\u00b7gend", ",", "von", "des", "Kin\u00b7des", "Zug", "sich", "hebt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was regt sich wilder, als wenn, glutbewu\u00dft,", "tokens": ["Was", "regt", "sich", "wil\u00b7der", ",", "als", "wenn", ",", "glut\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADJA", "$,", "KOKOM", "KOUS", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Erde von Vulkanenathem bebt?", "tokens": ["Die", "Er\u00b7de", "von", "Vul\u00b7ka\u00b7ne\u00b7na\u00b7them", "bebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Gr\u00e4fin sitzt auf ihrem festen Schlo\u00df,", "tokens": ["Die", "Gr\u00e4\u00b7fin", "sitzt", "auf", "ih\u00b7rem", "fes\u00b7ten", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tr\u00e4nkt am Busen ihren ersten Sohn.", "tokens": ["Und", "tr\u00e4nkt", "am", "Bu\u00b7sen", "ih\u00b7ren", "ers\u00b7ten", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da kocht's im Boden, und das Thurmgescho\u00df", "tokens": ["Da", "kocht's", "im", "Bo\u00b7den", ",", "und", "das", "Thurm\u00b7ge\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kracht ein, und nieder sinkt der Felsenthron.", "tokens": ["Kracht", "ein", ",", "und", "nie\u00b7der", "sinkt", "der", "Fel\u00b7sen\u00b7thron", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kein Stein blieb aufrecht auf dem Stein, kein Baum", "tokens": ["Kein", "Stein", "blieb", "auf\u00b7recht", "auf", "dem", "Stein", ",", "kein", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Boden fest; und keine Stimme klagt,", "tokens": ["Im", "Bo\u00b7den", "fest", ";", "und", "kei\u00b7ne", "Stim\u00b7me", "klagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Seufzer weht; ein Wandrer w\u00fc\u00dfte kaum,", "tokens": ["Kein", "Seuf\u00b7zer", "weht", ";", "ein", "Wand\u00b7rer", "w\u00fc\u00df\u00b7te", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df in der Oede hier ein Schlo\u00df geragt: \u2013", "tokens": ["Da\u00df", "in", "der", "O\u00b7e\u00b7de", "hier", "ein", "Schlo\u00df", "ge\u00b7ragt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Erz\u00e4hlt' es nicht ", "tokens": ["Er\u00b7z\u00e4hlt'", "es", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Getragen ward in stillem Engelflug,", "tokens": ["Ge\u00b7tra\u00b7gen", "ward", "in", "stil\u00b7lem", "En\u00b7gel\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht ", "tokens": ["Und", "nicht"], "token_info": ["word", "word"], "pos": ["KON", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Der neuen Burg die ersten Steine trug. \u2013", "tokens": ["Der", "neu\u00b7en", "Burg", "die", "ers\u00b7ten", "Stei\u00b7ne", "trug", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}