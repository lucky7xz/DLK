{"textgrid.poem.41424": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Hahn und der Fuchs", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein alter Haushahn hielt auf einer Scheune Wache;", "tokens": ["Ein", "al\u00b7ter", "Haus\u00b7hahn", "hielt", "auf", "ei\u00b7ner", "Scheu\u00b7ne", "Wa\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da k\u00f6mmt ein Fuchs mit schnellem Schritt,", "tokens": ["Da", "k\u00f6mmt", "ein", "Fuchs", "mit", "schnel\u00b7lem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ruft: O kr\u00e4he, Freund, nun ich dich fr\u00f6hlich mache;", "tokens": ["Und", "ruft", ":", "O", "kr\u00e4\u00b7he", ",", "Freund", ",", "nun", "ich", "dich", "fr\u00f6h\u00b7lich", "ma\u00b7che", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "VVFIN", "$,", "NN", "$,", "ADV", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bringe gute Zeitung mit.", "tokens": ["Ich", "brin\u00b7ge", "gu\u00b7te", "Zei\u00b7tung", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Thiere Krieg h\u00f6rt auf: man ist der Zwietracht m\u00fcde.", "tokens": ["Der", "Thie\u00b7re", "Krieg", "h\u00f6rt", "auf", ":", "man", "ist", "der", "Zwiet\u00b7racht", "m\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$.", "PIS", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In unserm Reich ist Ruh' und Friede.", "tokens": ["In", "un\u00b7serm", "Reich", "ist", "Ruh'", "und", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich selber trag' ihn dir von allen F\u00fcchsen an.", "tokens": ["Ich", "sel\u00b7ber", "trag'", "ihn", "dir", "von", "al\u00b7len", "F\u00fcch\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "O Freund, komm' bald herab, da\u00df ich dich herzen kann.", "tokens": ["O", "Freund", ",", "komm'", "bald", "her\u00b7ab", ",", "da\u00df", "ich", "dich", "her\u00b7zen", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie guckst du so herum? Greif, Halt und Bellart kommen,", "tokens": ["Wie", "guckst", "du", "so", "he\u00b7rum", "?", "Greif", ",", "Halt", "und", "Be\u00b7llart", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "NE", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Hunde, die du kennst, versetzt der alte Hahn;", "tokens": ["Die", "Hun\u00b7de", ",", "die", "du", "kennst", ",", "ver\u00b7setzt", "der", "al\u00b7te", "Hahn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und, als der Fuchs entl\u00e4uft: was, fragt er, ficht dich an?", "tokens": ["Und", ",", "als", "der", "Fuchs", "ent\u00b7l\u00e4uft", ":", "was", ",", "fragt", "er", ",", "ficht", "dich", "an", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NE", "VVFIN", "$.", "PWS", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nichts, Bruder, spricht der Fuchs; der Streit ist abgethan;", "tokens": ["Nichts", ",", "Bru\u00b7der", ",", "spricht", "der", "Fuchs", ";", "der", "Streit", "ist", "ab\u00b7ge\u00b7than", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "$,", "VVFIN", "ART", "NE", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein, ich zweifle noch, ob die es schon vernommen.", "tokens": ["Al\u00b7lein", ",", "ich", "zweif\u00b7le", "noch", ",", "ob", "die", "es", "schon", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}