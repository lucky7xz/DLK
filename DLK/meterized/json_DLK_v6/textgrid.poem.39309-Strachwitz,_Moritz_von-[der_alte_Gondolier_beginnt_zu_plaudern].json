{"textgrid.poem.39309": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[der alte Gondolier beginnt zu plaudern]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte Gondolier beginnt zu plaudern,", "tokens": ["Der", "al\u00b7te", "Gon\u00b7do\u00b7lier", "be\u00b7ginnt", "zu", "plau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Liebkosend, sanfter streichelt er die Fluten,", "tokens": ["Lieb\u00b7ko\u00b7send", ",", "sanf\u00b7ter", "strei\u00b7chelt", "er", "die", "Flu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die leicht im kalten Mondenlichte schaudern!", "tokens": ["Die", "leicht", "im", "kal\u00b7ten", "Mon\u00b7den\u00b7lich\u00b7te", "schau\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Indes verlodern der Piazza Gluten,", "tokens": ["In\u00b7des", "ver\u00b7lo\u00b7dern", "der", "Piaz\u00b7za", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist, als h\u00f6rte man in Tropfen leise", "tokens": ["Es", "ist", ",", "als", "h\u00f6r\u00b7te", "man", "in", "Trop\u00b7fen", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PIS", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da Herz Venedigs durch die Stille bluten!", "tokens": ["Da", "Herz", "Ve\u00b7ne\u00b7digs", "durch", "die", "Stil\u00b7le", "blu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und mit ged\u00e4mpfter Stimme spricht der Greise:", "tokens": ["Und", "mit", "ge\u00b7d\u00e4mpf\u00b7ter", "Stim\u00b7me", "spricht", "der", "Grei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwenn so der Vollmond durch die Kolonnade", "tokens": ["\u00bb", "wenn", "so", "der", "Voll\u00b7mond", "durch", "die", "Ko\u00b7lon\u00b7na\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Dogen zittert, wie verloren Weise,", "tokens": ["Der", "Do\u00b7gen", "zit\u00b7tert", ",", "wie", "ver\u00b7lo\u00b7ren", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann kommt entlang die schattende Arkade", "tokens": ["Dann", "kommt", "ent\u00b7lang", "die", "schat\u00b7ten\u00b7de", "Ar\u00b7ka\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein schwarzes Weib, den schwarzen Zendel tragend,", "tokens": ["Ein", "schwar\u00b7zes", "Weib", ",", "den", "schwar\u00b7zen", "Zen\u00b7del", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lehnt sich schweigend an die Balustrade.", "tokens": ["Und", "lehnt", "sich", "schwei\u00b7gend", "an", "die", "Ba\u00b7lust\u00b7ra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Darauf, den Schleier aus der Stirne schlagend,", "tokens": ["Da\u00b7rauf", ",", "den", "Schlei\u00b7er", "aus", "der", "Stir\u00b7ne", "schla\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ART", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hebt mit dem Antlitz sie, dem stolzen, bleichen,", "tokens": ["Hebt", "mit", "dem", "Ant\u00b7litz", "sie", ",", "dem", "stol\u00b7zen", ",", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$,", "PRELS", "VVINF", "$,", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zum Mond empor ihr dunkles Auge klagend.", "tokens": ["Zum", "Mond", "em\u00b7por", "ihr", "dunk\u00b7les", "Au\u00b7ge", "kla\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und ob des Meers mondschimmernden Bereichen", "tokens": ["Und", "ob", "des", "Meers", "mond\u00b7schim\u00b7mern\u00b7den", "Be\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erteilt sie so mit z\u00e4rtlich bangem Blicke", "tokens": ["Er\u00b7teilt", "sie", "so", "mit", "z\u00e4rt\u00b7lich", "ban\u00b7gem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus schlanker Hand des Kreuzes Segenzeichen.", "tokens": ["Aus", "schlan\u00b7ker", "Hand", "des", "Kreu\u00b7zes", "Se\u00b7gen\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Gesenkten Hauptes wallt sie dann zur\u00fccke,", "tokens": ["Ge\u00b7senk\u00b7ten", "Haup\u00b7tes", "wallt", "sie", "dann", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mancher meint, da\u00df aus des Schleiers Welle", "tokens": ["Und", "man\u00b7cher", "meint", ",", "da\u00df", "aus", "des", "Schlei\u00b7ers", "Wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das goldne Horn der Dogenkrone blicke.", "tokens": ["Das", "gold\u00b7ne", "Horn", "der", "Do\u00b7gen\u00b7kro\u00b7ne", "bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Durchs Tor La Casta wandelt sie ins Helle,", "tokens": ["Durchs", "Tor", "La", "Cas\u00b7ta", "wan\u00b7delt", "sie", "ins", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von der Riva S\u00e4ulen sieht man schieben", "tokens": ["Und", "von", "der", "Ri\u00b7va", "S\u00e4u\u00b7len", "sieht", "man", "schie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich eine Gondel, schlank wie die Gazelle.", "tokens": ["Sich", "ei\u00b7ne", "Gon\u00b7del", ",", "schlank", "wie", "die", "Ga\u00b7zel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Noch auf der Puppa r\u00fcckw\u00e4rts stehn geblieben,", "tokens": ["Noch", "auf", "der", "Pup\u00b7pa", "r\u00fcck\u00b7w\u00e4rts", "stehn", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirft auf die L\u00f6wenstadt die blasse Fraue", "tokens": ["Wirft", "auf", "die", "L\u00f6\u00b7wen\u00b7stadt", "die", "blas\u00b7se", "Frau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den letzten Blick mit schwermutsvollem Lieben \u2013", "tokens": ["Den", "letz\u00b7ten", "Blick", "mit", "schwer\u00b7muts\u00b7vol\u00b7lem", "Lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Dann schwimmt die Gondel rasch hinaus ins Blaue!\u00ab", "tokens": ["Dann", "schwimmt", "die", "Gon\u00b7del", "rasch", "hin\u00b7aus", "ins", "Blau\u00b7e", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "ADV", "APPRART", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}