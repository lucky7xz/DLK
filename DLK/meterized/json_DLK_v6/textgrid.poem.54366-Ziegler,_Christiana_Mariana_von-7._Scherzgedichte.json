{"textgrid.poem.54366": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "7. Scherzgedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Bruder, deine H\u00f6flichkeit", "tokens": ["Herr", "Bru\u00b7der", ",", "dei\u00b7ne", "H\u00f6f\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist allzu gro\u00df, ich bin bereit", "tokens": ["Ist", "all\u00b7zu", "gro\u00df", ",", "ich", "bin", "be\u00b7reit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKA", "ADJD", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Dank davor zusagen.", "tokens": ["Den", "Dank", "da\u00b7vor", "zu\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das, was du mir hast zugeschickt,", "tokens": ["Das", ",", "was", "du", "mir", "hast", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und durch der Floren Hand gepfl\u00fcckt,", "tokens": ["Und", "durch", "der", "Flo\u00b7ren", "Hand", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich recht sch\u00f6ne tragen.", "tokens": ["Will", "ich", "recht", "sch\u00f6\u00b7ne", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ey seht mir doch dies Str\u00e4u\u00dfchen an,", "tokens": ["Ey", "seht", "mir", "doch", "dies", "Str\u00e4u\u00df\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PDS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob man es netter binden kann", "tokens": ["Ob", "man", "es", "net\u00b7ter", "bin\u00b7den", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dreyer Herren Landen?", "tokens": ["In", "drey\u00b7er", "Her\u00b7ren", "Lan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dergleichen hat kein Sch\u00e4fer leicht,", "tokens": ["Derg\u00b7lei\u00b7chen", "hat", "kein", "Sch\u00e4\u00b7fer", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So weit und breit man umher schleicht,", "tokens": ["So", "weit", "und", "breit", "man", "um\u00b7her", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PIS", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Phyllis zugestanden.", "tokens": ["Der", "Phyl\u00b7lis", "zu\u00b7ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin zwar wohl, wie mancher meynt,", "tokens": ["Ich", "bin", "zwar", "wohl", ",", "wie", "man\u00b7cher", "meynt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von den Geschenken gar kein Freund,", "tokens": ["Von", "den", "Ge\u00b7schen\u00b7ken", "gar", "kein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie k\u00f6nnen mich nicht blenden.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "mich", "nicht", "blen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Gro\u00dfmuthvolles Herz und Sinn", "tokens": ["Mein", "Gro\u00df\u00b7mu\u00b7th\u00b7vol\u00b7les", "Herz", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Flieht Eigennutz und auch Gewinn,", "tokens": ["Flieht", "Ei\u00b7gen\u00b7nutz", "und", "auch", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An allen Ort und Enden.", "tokens": ["An", "al\u00b7len", "Ort", "und", "En\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch da mir deine Redlichkeit", "tokens": ["Doch", "da", "mir", "dei\u00b7ne", "Red\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sch\u00f6n gewachsne Blumen weyht,", "tokens": ["So", "sch\u00f6n", "ge\u00b7wachs\u00b7ne", "Blu\u00b7men", "weyht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So darf ichs nicht versagen,", "tokens": ["So", "darf", "ichs", "nicht", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du n\u00e4hmst es vor ein K\u00f6rbchen an,", "tokens": ["Du", "n\u00e4hmst", "es", "vor", "ein", "K\u00f6rb\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Worinnen man dies Str\u00e4u\u00dfchen kann", "tokens": ["Wo\u00b7rin\u00b7nen", "man", "dies", "Str\u00e4u\u00df\u00b7chen", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PDS", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Hause wieder tragen.", "tokens": ["Nach", "Hau\u00b7se", "wie\u00b7der", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nein, dieses wahrlich mu\u00df nicht seyn,", "tokens": ["Nein", ",", "die\u00b7ses", "wahr\u00b7lich", "mu\u00df", "nicht", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich theile dies Geschenke ein", "tokens": ["Ich", "thei\u00b7le", "dies", "Ge\u00b7schen\u00b7ke", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Schl\u00e4ff und Brust zu winden,", "tokens": ["Um", "Schl\u00e4ff", "und", "Brust", "zu", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und welkt der Blumen Herrlichkeit,", "tokens": ["Und", "welkt", "der", "Blu\u00b7men", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wirst du dennoch allezeit", "tokens": ["So", "wirst", "du", "den\u00b7noch", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Freundschaft gr\u00fcnend finden.", "tokens": ["Die", "Freund\u00b7schaft", "gr\u00fc\u00b7nend", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}