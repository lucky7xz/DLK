{"dta.poem.6794": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Strahlen von dem Licht, ohn abw\u00e4rts sich zu sencken,", "tokens": ["Die", "Strah\u00b7len", "von", "dem", "Licht", ",", "ohn", "ab\u00b7w\u00e4rts", "sich", "zu", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KOUI", "ADV", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begeben sich zur\u00fcck; da sie sich zu uns lencken,", "tokens": ["Be\u00b7ge\u00b7ben", "sich", "zu\u00b7r\u00fcck", ";", "da", "sie", "sich", "zu", "uns", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "$.", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In ihrer Ordnung, Lag\u2019 und Abstand, und behalten", "tokens": ["In", "ih\u00b7rer", "Ord\u00b7nung", ",", "Lag'", "und", "Ab\u00b7stand", ",", "und", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von ihrem Gegenwurff die Weit\u2019 und die Gestalten.", "tokens": ["Von", "ih\u00b7rem", "Ge\u00b7gen\u00b7wurff", "die", "Weit'", "und", "die", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er stellet unserm Blick so deutlich und so klar", "tokens": ["Er", "stel\u00b7let", "un\u00b7serm", "Blick", "so", "deut\u00b7lich", "und", "so", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den vorgesetzten C\u00f6rper dar,", "tokens": ["Den", "vor\u00b7ge\u00b7setz\u00b7ten", "C\u00f6r\u00b7per", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df auch der k\u00fcnstlichste Copist", "tokens": ["Da\u00df", "auch", "der", "k\u00fcnst\u00b7lichs\u00b7te", "Co\u00b7pist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit allem Flei\u00df,", "tokens": ["Mit", "al\u00b7lem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So \u00e4hnlich sein Modell nicht nachzuschildern wei\u00df.", "tokens": ["So", "\u00e4hn\u00b7lich", "sein", "Mo\u00b7dell", "nicht", "nach\u00b7zu\u00b7schil\u00b7dern", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "VMFIN", "PTKNEG", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gleichwol befindet sichs, da\u00df von der Schilderey", "tokens": ["Gleich\u00b7wol", "be\u00b7fin\u00b7det", "sichs", ",", "da\u00df", "von", "der", "Schil\u00b7de\u00b7rey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man doch geteuschet sey.", "tokens": ["Man", "doch", "ge\u00b7teu\u00b7schet", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Wenn unser Auge sie im Spiegel-Glas  erblicket;", "tokens": ["Wenn", "un\u00b7ser", "Au\u00b7ge", "sie", "im", "Spie\u00b7gel\u00b7Glas", "er\u00b7bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So scheinet das Gem\u00e4hld\u2019 so weit hinein ger\u00fccket,", "tokens": ["So", "schei\u00b7net", "das", "Ge\u00b7m\u00e4hld'", "so", "weit", "hin\u00b7ein", "ge\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als wie der Gegenwurff von aussen abw\u00e4rts steht.", "tokens": ["Als", "wie", "der", "Ge\u00b7gen\u00b7wurff", "von", "aus\u00b7sen", "ab\u00b7w\u00e4rts", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was ihr darinn ver\u00e4nderliches seht,", "tokens": ["Was", "ihr", "da\u00b7rinn", "ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7ches", "seht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ist, da\u00df das Rechte lincks, das Lincke rechts sich dreht.", "tokens": ["Ist", ",", "da\u00df", "das", "Rech\u00b7te", "lincks", ",", "das", "Lin\u00b7cke", "rechts", "sich", "dreht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}