{"textgrid.poem.50513": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "38. Auf die Verfolgung der Christen unter Diocletian", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es dachte ", "tokens": ["Es", "dach\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Durch Marter Quahl und Todt von der ", "tokens": ["Durch", "Mar\u00b7ter", "Quahl", "und", "Todt", "von", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "NN", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Man sahe manchen Held sein willig Haubt ausstrecken,", "tokens": ["Man", "sa\u00b7he", "man\u00b7chen", "Held", "sein", "wil\u00b7lig", "Haubt", "aus\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und mancher rauhe Weg ward zu der Grufft geb\u00e4hnt:", "tokens": ["Und", "man\u00b7cher", "rau\u00b7he", "Weg", "ward", "zu", "der", "Grufft", "ge\u00b7b\u00e4hnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch hat der ", "tokens": ["Doch", "hat", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}