{"dta.poem.21180": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Empfindungen christlicher Dank-  \n barkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich mein Geist, Allm\u00e4chtiger!", "tokens": ["Wenn", "sich", "mein", "Geist", ",", "All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gnaden Menge denkt,", "tokens": ["Der", "Gna\u00b7den", "Men\u00b7ge", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Womit du mich, mein GOtt und HErr,", "tokens": ["Wo\u00b7mit", "du", "mich", ",", "mein", "Gott", "und", "Herr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So unverdient beschenkt:", "tokens": ["So", "un\u00b7ver\u00b7di\u00b7ent", "be\u00b7schenkt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Dann ist mein Herz, so hoch erfreut,", "tokens": ["Dann", "ist", "mein", "Herz", ",", "so", "hoch", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz deiner G\u00fcte voll,", "tokens": ["Ganz", "dei\u00b7ner", "G\u00fc\u00b7te", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wei\u00df f\u00fcr heisser Dankbarkeit", "tokens": ["Und", "wei\u00df", "f\u00fcr", "heis\u00b7ser", "Dank\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht, wie es danken soll.", "tokens": ["Nicht", ",", "wie", "es", "dan\u00b7ken", "soll", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als ich noch in der Mutter Schoos,", "tokens": ["Als", "ich", "noch", "in", "der", "Mut\u00b7ter", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Nacht verborgen, schlief;", "tokens": ["In", "Nacht", "ver\u00b7bor\u00b7gen", ",", "schlief", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bestimmtest du, o HErr, mein Loos,", "tokens": ["Be\u00b7stimm\u00b7test", "du", ",", "o", "Herr", ",", "mein", "Loos", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mich zum Leben rief.", "tokens": ["Das", "mich", "zum", "Le\u00b7ben", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du sprichst des Sterblichen Geschick,", "tokens": ["Du", "sprichst", "des", "Sterb\u00b7li\u00b7chen", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh er geboren ist;", "tokens": ["Eh", "er", "ge\u00b7bo\u00b7ren", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und so ward ich, (o welch ein Gl\u00fcck!)", "tokens": ["Und", "so", "ward", "ich", ",", "(", "o", "welch", "ein", "Gl\u00fcck", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "$(", "FM", "PWAT", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die Geburt, ein Christ.", "tokens": ["Durch", "die", "Ge\u00b7burt", ",", "ein", "Christ", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Schwach an der Brust, vernahmst du schon,", "tokens": ["Schwach", "an", "der", "Brust", ",", "ver\u00b7nahmst", "du", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was kein Gebet noch war,", "tokens": ["Was", "kein", "Ge\u00b7bet", "noch", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und neigtest zu des Weinens Ton", "tokens": ["Und", "neig\u00b7test", "zu", "des", "Wei\u00b7nens", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Ohr gef\u00e4llig dar.", "tokens": ["Dein", "Ohr", "ge\u00b7f\u00e4l\u00b7lig", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn ich als J\u00fcngling von dem Pfad", "tokens": ["Wenn", "ich", "als", "J\u00fcng\u00b7ling", "von", "dem", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tugend mich verirrt;", "tokens": ["Der", "Tu\u00b7gend", "mich", "ver\u00b7irrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat mich unsichtbar, HErr, dein Rath", "tokens": ["Hat", "mich", "un\u00b7sicht\u00b7bar", ",", "Herr", ",", "dein", "Rath"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Oft wieder drauf gef\u00fchrt.", "tokens": ["Oft", "wie\u00b7der", "drauf", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du warst mein Schutz, und meine Wehr", "tokens": ["Du", "warst", "mein", "Schutz", ",", "und", "mei\u00b7ne", "Wehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Ungl\u00fcck und Gefahr;", "tokens": ["F\u00fcr", "Un\u00b7gl\u00fcck", "und", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcr dem Laster, das noch mehr,", "tokens": ["Und", "f\u00fcr", "dem", "Las\u00b7ter", ",", "das", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie, zu f\u00fcrchten war.", "tokens": ["Wie", "sie", ",", "zu", "f\u00fcrch\u00b7ten", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich sah, von Krankheit bleich, durch dich", "tokens": ["Ich", "sah", ",", "von", "Krank\u00b7heit", "bleich", ",", "durch", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben hergestellt;", "tokens": ["Mein", "Le\u00b7ben", "her\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deine Gnade schm\u00fcckte mich,", "tokens": ["Und", "dei\u00b7ne", "Gna\u00b7de", "schm\u00fcck\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn S\u00fcnde mich entstellt.", "tokens": ["Wenn", "S\u00fcn\u00b7de", "mich", "ent\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Von Freudenstrahlen gl\u00e4nzt mein Blick,", "tokens": ["Von", "Freu\u00b7den\u00b7strah\u00b7len", "gl\u00e4nzt", "mein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da du so hoch mich liebst,", "tokens": ["Da", "du", "so", "hoch", "mich", "liebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mir in wahrer Freundschaft Gl\u00fcck", "tokens": ["Und", "mir", "in", "wah\u00b7rer", "Freund\u00b7schaft", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr, als ich w\u00fcnschte, giebst!", "tokens": ["Mehr", ",", "als", "ich", "w\u00fcnschte", ",", "giebst", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Und welche Wohlthat, HErr, ist nicht", "tokens": ["Und", "wel\u00b7che", "Wohlt\u00b7hat", ",", "Herr", ",", "ist", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PWAT", "NN", "$,", "NN", "$,", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Herz, das f\u00fchlen kan!", "tokens": ["Dies", "Herz", ",", "das", "f\u00fch\u00b7len", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies Herz, ganz dein, das dankbar spricht,", "tokens": ["Dies", "Herz", ",", "ganz", "dein", ",", "das", "dank\u00b7bar", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "ADV", "PPOSAT", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was du an mir gethan!", "tokens": ["Was", "du", "an", "mir", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Kein Tag soll w\u00fcrdger mir vergehn,", "tokens": ["Kein", "Tag", "soll", "w\u00fcrd\u00b7ger", "mir", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als, Ewger, dir zum Preis;", "tokens": ["Als", ",", "Ew\u00b7ger", ",", "dir", "zum", "Preis", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will mit Hymnen dich erh\u00f6hn,", "tokens": ["Ich", "will", "mit", "Hym\u00b7nen", "dich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als J\u00fcngling, und als Greis.", "tokens": ["Als", "J\u00fcng\u00b7ling", ",", "und", "als", "Greis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KON", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "In Schrecken, Angst, Gefahr und Noth,", "tokens": ["In", "Schre\u00b7cken", ",", "Angst", ",", "Ge\u00b7fahr", "und", "Noth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trau ich allein auf dich.", "tokens": ["Trau", "ich", "al\u00b7lein", "auf", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Durch dich gest\u00e4rkt, ist selbst der Tod", "tokens": ["Durch", "dich", "ge\u00b7st\u00e4rkt", ",", "ist", "selbst", "der", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "VAFIN", "ADV", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mir nicht mehr f\u00fcrchterlich.", "tokens": ["Mir", "nicht", "mehr", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wenn krachend ietzt, der Bau der Welt,", "tokens": ["Wenn", "kra\u00b7chend", "ietzt", ",", "der", "Bau", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich aus den Angeln rei\u00dft:", "tokens": ["Sich", "aus", "den", "An\u00b7geln", "rei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will ich den preisen, der mich h\u00e4lt,", "tokens": ["Will", "ich", "den", "prei\u00b7sen", ",", "der", "mich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich, der mich leben hei\u00dft;", "tokens": ["Dich", ",", "der", "mich", "le\u00b7ben", "hei\u00dft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PRF", "VVINF", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.14": {"line.1": {"text": "Dich, der mich bey der Welten Sturz", "tokens": ["Dich", ",", "der", "mich", "bey", "der", "Wel\u00b7ten", "Sturz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit starkem Arm erhob! \u2014", "tokens": ["Mit", "star\u00b7kem", "Arm", "er\u00b7hob", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Selbst Ewigkeit, HErr! ist  zu kurz,", "tokens": ["Selbst", "E\u00b7wig\u00b7keit", ",", "Herr", "!", "ist", "zu", "kurz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$.", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu preisen all dein Lob!", "tokens": ["Zu", "prei\u00b7sen", "all", "dein", "Lob", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}