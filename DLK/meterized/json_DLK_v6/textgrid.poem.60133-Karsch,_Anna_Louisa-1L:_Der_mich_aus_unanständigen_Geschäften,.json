{"textgrid.poem.60133": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der mich aus unanst\u00e4ndigen Gesch\u00e4ften,", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der mich aus unanst\u00e4ndigen Gesch\u00e4ften,", "tokens": ["Der", "mich", "aus", "un\u00b7an\u00b7st\u00e4n\u00b7di\u00b7gen", "Ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus einem p\u00f6belhaften Leben ohne Ruh", "tokens": ["Aus", "ei\u00b7nem", "p\u00f6\u00b7bel\u00b7haf\u00b7ten", "Le\u00b7ben", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herausgerissen, mit des Menschenfreundes Kr\u00e4ften,", "tokens": ["Her\u00b7aus\u00b7ge\u00b7ris\u00b7sen", ",", "mit", "des", "Men\u00b7schen\u00b7freun\u00b7des", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Theurer Kottwitz! der bist Du.", "tokens": ["Mein", "Theu\u00b7rer", "Kott\u00b7witz", "!", "der", "bist", "Du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$.", "ART", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df mich, zu meines Vaterlandes Ehre,", "tokens": ["Da\u00df", "mich", ",", "zu", "mei\u00b7nes", "Va\u00b7ter\u00b7lan\u00b7des", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der zungenvolle Ruf in fremden L\u00e4ndern nennt,", "tokens": ["Der", "zun\u00b7gen\u00b7vol\u00b7le", "Ruf", "in", "frem\u00b7den", "L\u00e4n\u00b7dern", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df mein Saytenspiel nun th\u00f6nt, bis zum Geh\u00f6re", "tokens": ["Und", "da\u00df", "mein", "Say\u00b7ten\u00b7spiel", "nun", "th\u00f6nt", ",", "bis", "zum", "Ge\u00b7h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Weisen, der die Geister kennt;", "tokens": ["Des", "Wei\u00b7sen", ",", "der", "die", "Geis\u00b7ter", "kennt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df Friedrich j\u00fcngst des Musengottes Fl\u00f6te", "tokens": ["Da\u00df", "Fried\u00b7rich", "j\u00fcngst", "des", "Mu\u00b7sen\u00b7got\u00b7tes", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Seinen liederreichen Lippen nahm und mir", "tokens": ["Von", "Sei\u00b7nen", "lie\u00b7der\u00b7rei\u00b7chen", "Lip\u00b7pen", "nahm", "und", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "KON", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entgegen l\u00e4chelte, wie Fr\u00fchlingsmorgenr\u00f6the;", "tokens": ["Ent\u00b7ge\u00b7gen", "l\u00e4\u00b7chel\u00b7te", ",", "wie", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen\u00b7r\u00f6\u00b7the", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Freund! dis alles dank ich Dir.", "tokens": ["O", "Freund", "!", "dis", "al\u00b7les", "dank", "ich", "Dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PDS", "PIS", "APPR", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn ohne Dich w\u00e4r, an dem Oderstrande", "tokens": ["Denn", "oh\u00b7ne", "Dich", "w\u00e4r", ",", "an", "dem", "O\u00b7der\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00fchselig unterdr\u00fcckt mein gl\u00fcckliches Genie;", "tokens": ["M\u00fch\u00b7se\u00b7lig", "un\u00b7ter\u00b7dr\u00fcckt", "mein", "gl\u00fcck\u00b7li\u00b7ches", "Ge\u00b7nie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ein Blumen-Saame stirbt in unbetautem Sande,", "tokens": ["Ein", "Blu\u00b7men\u00b7Saa\u00b7me", "stirbt", "in", "un\u00b7be\u00b7tau\u00b7tem", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Keimt auf des Steines R\u00fccken nie.", "tokens": ["Keimt", "auf", "des", "Stei\u00b7nes", "R\u00fc\u00b7cken", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Pflanze stirbt, von Wolken unbegossen,", "tokens": ["Die", "Pflan\u00b7ze", "stirbt", ",", "von", "Wol\u00b7ken", "un\u00b7be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom G\u00e4rtner unbespr\u00fctzt, wenn Erndte-Sonne gl\u00fcht;", "tokens": ["Vom", "G\u00e4rt\u00b7ner", "un\u00b7be\u00b7spr\u00fctzt", ",", "wenn", "Ernd\u00b7te\u00b7Son\u00b7ne", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der edle Fruchtkern treibt zum wilden Apfelsprossen,", "tokens": ["Der", "ed\u00b7le", "Frucht\u00b7kern", "treibt", "zum", "wil\u00b7den", "Ap\u00b7fel\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn nicht die Kunst den Baum erzieht.", "tokens": ["Wenn", "nicht", "die", "Kunst", "den", "Baum", "er\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So w\u00e4r auch ich verwildert; aber Deine,", "tokens": ["So", "w\u00e4r", "auch", "ich", "ver\u00b7wil\u00b7dert", ";", "a\u00b7ber", "Dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VVPP", "$.", "KON", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von einem Gott gelenkte, rechte Freundes Hand,", "tokens": ["Von", "ei\u00b7nem", "Gott", "ge\u00b7lenk\u00b7te", ",", "rech\u00b7te", "Freun\u00b7des", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zog mich zum grossen Sitz des K\u00f6niges, der seine", "tokens": ["Zog", "mich", "zum", "gros\u00b7sen", "Sitz", "des", "K\u00f6\u00b7ni\u00b7ges", ",", "der", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gecr\u00f6nte Schl\u00e4fe gr\u00fcn umwand.", "tokens": ["Ge\u00b7cr\u00f6n\u00b7te", "Schl\u00e4\u00b7fe", "gr\u00fcn", "um\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du dachtest nicht die Thaten fremder Krieger,", "tokens": ["Du", "dach\u00b7test", "nicht", "die", "Tha\u00b7ten", "frem\u00b7der", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Heerden, die der Feind trieb von zertretner Trift,", "tokens": ["Nicht", "Heer\u00b7den", ",", "die", "der", "Feind", "trieb", "von", "zer\u00b7tret\u00b7ner", "Trift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du nanntest den Verlust ein Opfer f\u00fcr den Sieger,", "tokens": ["Du", "nann\u00b7test", "den", "Ver\u00b7lust", "ein", "Op\u00b7fer", "f\u00fcr", "den", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der alle Sieger \u00fcbertrift.", "tokens": ["Der", "al\u00b7le", "Sie\u00b7ger", "\u00fc\u00b7bert\u00b7rift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du machtest mir in sorgenlosen Tagen", "tokens": ["Du", "mach\u00b7test", "mir", "in", "sor\u00b7gen\u00b7lo\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Elis\u00e4er Sitz, das pr\u00e4chtige Berlin.", "tokens": ["Zum", "E\u00b7li\u00b7s\u00e4\u00b7er", "Sitz", ",", "das", "pr\u00e4ch\u00b7ti\u00b7ge", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ward einst \u00fcbers Meer ein Dichter fortgetragen,", "tokens": ["So", "ward", "einst", "\u00fc\u00b7bers", "Meer", "ein", "Dich\u00b7ter", "fort\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von einem freundlichen Delphin;", "tokens": ["Von", "ei\u00b7nem", "freund\u00b7li\u00b7chen", "Del\u00b7phin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und ward von viel hinzugest\u00fcrmtem Volke", "tokens": ["Und", "ward", "von", "viel", "hin\u00b7zu\u00b7ge\u00b7st\u00fcrm\u00b7tem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewundert und gelobt; ich aber streife schon", "tokens": ["Be\u00b7wun\u00b7dert", "und", "ge\u00b7lobt", ";", "ich", "a\u00b7ber", "strei\u00b7fe", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$.", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit stolzem Nacken an die lufterf\u00fcllte Wolke,", "tokens": ["Mit", "stol\u00b7zem", "Na\u00b7cken", "an", "die", "luf\u00b7ter\u00b7f\u00fcll\u00b7te", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Getheilt von meiner Leyer Thon.", "tokens": ["Ge\u00b7theilt", "von", "mei\u00b7ner", "Le\u00b7yer", "Thon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Auf \u00fcberlebtes Elend blick ich nieder,", "tokens": ["Auf", "\u00fc\u00b7ber\u00b7leb\u00b7tes", "E\u00b7lend", "blick", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nenne Deinen Nahmen laut, vor einer Welt,", "tokens": ["Und", "nen\u00b7ne", "Dei\u00b7nen", "Nah\u00b7men", "laut", ",", "vor", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der dieses, Dir geweyhte, Opfer meiner Lieder", "tokens": ["Der", "die\u00b7ses", ",", "Dir", "ge\u00b7weyh\u00b7te", ",", "Op\u00b7fer", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PDAT", "$,", "PPER", "VVFIN", "$,", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie Deine sch\u00f6ne That gef\u00e4llt.", "tokens": ["Wie", "Dei\u00b7ne", "sch\u00f6\u00b7ne", "That", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}