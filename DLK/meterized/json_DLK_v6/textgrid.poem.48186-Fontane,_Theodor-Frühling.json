{"textgrid.poem.48186": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist er endlich kommen doch", "tokens": ["Nun", "ist", "er", "end\u00b7lich", "kom\u00b7men", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In gr\u00fcnem Knospenschuh;", "tokens": ["In", "gr\u00fc\u00b7nem", "Knos\u00b7pen\u00b7schuh", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bber kam, er kam ja immer noch\u00ab,", "tokens": ["\u00bb", "er", "kam", ",", "er", "kam", "ja", "im\u00b7mer", "noch", "\u00ab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die B\u00e4ume nicken sich's zu.", "tokens": ["Die", "B\u00e4u\u00b7me", "ni\u00b7cken", "sich's", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie konnten ihn all erwarten kaum,", "tokens": ["Sie", "konn\u00b7ten", "ihn", "all", "er\u00b7war\u00b7ten", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "ADJA", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun treiben sie Schu\u00df auf Schu\u00df;", "tokens": ["Nun", "trei\u00b7ben", "sie", "Schu\u00df", "auf", "Schu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Garten der alte Apfelbaum,", "tokens": ["Im", "Gar\u00b7ten", "der", "al\u00b7te", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er str\u00e4ubt sich, aber er mu\u00df.", "tokens": ["Er", "str\u00e4ubt", "sich", ",", "a\u00b7ber", "er", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KON", "PPER", "VMFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wohl z\u00f6gert auch das alte Herz", "tokens": ["Wohl", "z\u00f6\u00b7gert", "auch", "das", "al\u00b7te", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und atmet noch nicht frei,", "tokens": ["Und", "at\u00b7met", "noch", "nicht", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es bangt und sorgt: \u00bbEs ist erst M\u00e4rz,", "tokens": ["Es", "bangt", "und", "sorgt", ":", "\u00bb", "Es", "ist", "erst", "M\u00e4rz", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und M\u00e4rz ist noch nicht Mai.\u00ab", "tokens": ["Und", "M\u00e4rz", "ist", "noch", "nicht", "Mai", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "PTKNEG", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O sch\u00fcttle ab den schweren Traum", "tokens": ["O", "sch\u00fctt\u00b7le", "ab", "den", "schwe\u00b7ren", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die lange Winterruh:", "tokens": ["Und", "die", "lan\u00b7ge", "Win\u00b7ter\u00b7ruh", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es wagt es der alte Apfelbaum,", "tokens": ["Es", "wagt", "es", "der", "al\u00b7te", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herze, wag's auch ", "tokens": ["Her\u00b7ze", ",", "wag's", "auch"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}}}}}