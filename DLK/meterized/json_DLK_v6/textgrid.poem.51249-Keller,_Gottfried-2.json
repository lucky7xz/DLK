{"textgrid.poem.51249": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenne dich, o Ungl\u00fcck, ganz und gar", "tokens": ["Ich", "ken\u00b7ne", "dich", ",", "o", "Un\u00b7gl\u00fcck", ",", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sehe jedes Glied an deiner Kette!", "tokens": ["Und", "se\u00b7he", "je\u00b7des", "Glied", "an", "dei\u00b7ner", "Ket\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist vern\u00fcnftig, zum Bewundern klar,", "tokens": ["Du", "bist", "ver\u00b7n\u00fcnf\u00b7tig", ",", "zum", "Be\u00b7wun\u00b7dern", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob ein Denker dich geordnet h\u00e4tte!", "tokens": ["Als", "ob", "ein", "Den\u00b7ker", "dich", "ge\u00b7ord\u00b7net", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht mehr noch weniger hat mir geb\u00fchrt,", "tokens": ["Nicht", "mehr", "noch", "we\u00b7ni\u00b7ger", "hat", "mir", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir ist gerecht die Schale zugemessen;", "tokens": ["Mir", "ist", "ge\u00b7recht", "die", "Scha\u00b7le", "zu\u00b7ge\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dennoch hab ich bittrer sie versp\u00fcrt,", "tokens": ["Und", "den\u00b7noch", "hab", "ich", "bit\u00b7trer", "sie", "ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als niemals ich getrunken noch gegessen.", "tokens": ["Als", "nie\u00b7mals", "ich", "ge\u00b7trun\u00b7ken", "noch", "ge\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVPP", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt aber bring ich leichter sie zum Mund,", "tokens": ["Jetzt", "a\u00b7ber", "bring", "ich", "leich\u00b7ter", "sie", "zum", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als einst die m\u00fcde Seele noch wird wissen;", "tokens": ["Als", "einst", "die", "m\u00fc\u00b7de", "See\u00b7le", "noch", "wird", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der quellenklare Perltrank ist gesund,", "tokens": ["Der", "quel\u00b7len\u00b7kla\u00b7re", "Perl\u00b7trank", "ist", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich lieb ihn drum mit d\u00fcrstendem Gewissen!", "tokens": ["Ich", "lieb", "ihn", "drum", "mit", "d\u00fcrs\u00b7ten\u00b7dem", "Ge\u00b7wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}