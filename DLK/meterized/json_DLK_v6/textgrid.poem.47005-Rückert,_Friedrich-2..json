{"textgrid.poem.47005": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling kocht sich aus des Winters Reifen", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kocht", "sich", "aus", "des", "Win\u00b7ters", "Rei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Tau, den seine Kinder sollen trinken;", "tokens": ["Den", "Tau", ",", "den", "sei\u00b7ne", "Kin\u00b7der", "sol\u00b7len", "trin\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er stimmt zum Morgenlied die muntern Zinken", "tokens": ["Er", "stimmt", "zum", "Mor\u00b7gen\u00b7lied", "die", "mun\u00b7tern", "Zin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schm\u00fcckt sein gr\u00fcnes Haus mit Bl\u00fctenschleifen.", "tokens": ["Und", "schm\u00fcckt", "sein", "gr\u00fc\u00b7nes", "Haus", "mit", "Bl\u00fc\u00b7ten\u00b7schlei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohlauf, mein Herz, la\u00df deine Blicke schweifen", "tokens": ["Wohl\u00b7auf", ",", "mein", "Herz", ",", "la\u00df", "dei\u00b7ne", "Bli\u00b7cke", "schwei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Blumen, die auf allen Fluren winken!", "tokens": ["Nach", "Blu\u00b7men", ",", "die", "auf", "al\u00b7len", "Flu\u00b7ren", "win\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Landm\u00e4dchen sind's, zur Rechten und zur Linken", "tokens": ["Land\u00b7m\u00e4d\u00b7chen", "sin\u00b7d's", ",", "zur", "Rech\u00b7ten", "und", "zur", "Lin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Stehn sie geputzt; nach welcher willst du greifen?", "tokens": ["Stehn", "sie", "ge\u00b7putzt", ";", "nach", "wel\u00b7cher", "willst", "du", "grei\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$.", "APPR", "PWAT", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach weh! statt zu ergreifen, selbst ergriffen", "tokens": ["Ach", "weh", "!", "statt", "zu", "er\u00b7grei\u00b7fen", ",", "selbst", "er\u00b7grif\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "$.", "ADJD", "PTKZU", "VVINF", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bist du von einer jungen wilden Hecke,", "tokens": ["Bist", "du", "von", "ei\u00b7ner", "jun\u00b7gen", "wil\u00b7den", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die scheint, sie wolle k\u00fcnftig Rosen tragen.", "tokens": ["Die", "scheint", ",", "sie", "wol\u00b7le", "k\u00fcnf\u00b7tig", "Ro\u00b7sen", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VMFIN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt tr\u00e4gt sie Dorne nur f\u00fcr dich geschliffen.", "tokens": ["Jetzt", "tr\u00e4gt", "sie", "Dor\u00b7ne", "nur", "f\u00fcr", "dich", "ge\u00b7schlif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, armes Herz, mir ahnt, es wird die kecke", "tokens": ["Ach", ",", "ar\u00b7mes", "Herz", ",", "mir", "ahnt", ",", "es", "wird", "die", "ke\u00b7cke"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir bitter dieses Sommers Lust zernagen.", "tokens": ["Dir", "bit\u00b7ter", "die\u00b7ses", "Som\u00b7mers", "Lust", "zer\u00b7na\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PDAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}