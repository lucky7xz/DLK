{"dta.poem.21396": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eman schickte uns Kleider und Betten genug,", "tokens": ["\u201e", "man", "schick\u00b7te", "uns", "Klei\u00b7der", "und", "Bet\u00b7ten", "ge\u00b7nug", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PPER", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auch Brod und Fleisch und Suppen!", "tokens": ["Auch", "Brod", "und", "Fleisch", "und", "Sup\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig von Preu\u00dfen wollte sogar", "tokens": ["Der", "K\u00f6\u00b7nig", "von", "Preu\u00b7\u00dfen", "woll\u00b7te", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VMFIN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns schicken seine Truppen.", "tokens": ["Uns", "schi\u00b7cken", "sei\u00b7ne", "Trup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eder materielle Schaden ward", "tokens": ["\u201e", "der", "ma\u00b7te\u00b7ri\u00b7el\u00b7le", "Scha\u00b7den", "ward"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Verg\u00fctet, das lie\u00df sich sch\u00e4tzen \u2013", "tokens": ["Ver\u00b7g\u00fc\u00b7tet", ",", "das", "lie\u00df", "sich", "sch\u00e4t\u00b7zen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PDS", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jedoch den Schrecken, unseren Schreck,", "tokens": ["Je\u00b7doch", "den", "Schre\u00b7cken", ",", "un\u00b7se\u00b7ren", "Schreck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Den kann uns niemand ersetzen!\u201c", "tokens": ["Den", "kann", "uns", "nie\u00b7mand", "er\u00b7set\u00b7zen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VMFIN", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Aufmunternd sprach ich: Ihr lieben Leut,", "tokens": ["Auf\u00b7mun\u00b7ternd", "sprach", "ich", ":", "Ihr", "lie\u00b7ben", "Leut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr m\u00fc\u00dft nicht jammern und flennen,", "tokens": ["Ihr", "m\u00fc\u00dft", "nicht", "jam\u00b7mern", "und", "flen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Troya war eine bessere Stadt", "tokens": ["Troy\u00b7a", "war", "ei\u00b7ne", "bes\u00b7se\u00b7re", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und mu\u00dfte doch verbrennen.", "tokens": ["Und", "mu\u00df\u00b7te", "doch", "ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Baut Eure H\u00e4user wieder auf", "tokens": ["Baut", "Eu\u00b7re", "H\u00e4u\u00b7ser", "wie\u00b7der", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trocknet Eure Pf\u00fctzen,", "tokens": ["Und", "trock\u00b7net", "Eu\u00b7re", "Pf\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schafft Euch bess\u2019re Gesetze an,", "tokens": ["Und", "schafft", "Euch", "bess'\u00b7re", "Ge\u00b7set\u00b7ze", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und be\u00df\u2019re Feuerspritzen.", "tokens": ["Und", "be\u00df'\u00b7re", "Feu\u00b7er\u00b7sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}