{"textgrid.poem.40904": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Bettler", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bben alte Ma, en arme Ma,", "tokens": ["\u00bb", "en", "al\u00b7te", "Ma", ",", "en", "ar\u00b7me", "Ma", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er sprichtich um e Wohltat a.", "tokens": ["er", "sprich\u00b7tich", "um", "e", "Wohl\u00b7tat", "a."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "PRF", "APPR", "NE", "NE", "APPRART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "E St\u00fcckli Brot ab euem Tisch,", "tokens": ["E", "St\u00fcck\u00b7li", "Brot", "ab", "eu\u00b7em", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wenn's eue guete Willen isch!", "tokens": ["wenn's", "eu\u00b7e", "gue\u00b7te", "Wil\u00b7len", "isch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "He jo, dur Gotts Wille!", "tokens": ["He", "jo", ",", "dur", "Gotts", "Wil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "In Sturm und Wetter, arm und blo\u00df,", "tokens": ["In", "Sturm", "und", "Wet\u00b7ter", ",", "arm", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "gibore bini uf der Stro\u00df,", "tokens": ["gi\u00b7bo\u00b7re", "bi\u00b7ni", "uf", "der", "Stro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und uf der Stro\u00df in Sturm und Wind", "tokens": ["und", "uf", "der", "Stro\u00df", "in", "Sturm", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "erzogen, arm, e Bettelchind.", "tokens": ["er\u00b7zo\u00b7gen", ",", "arm", ",", "e", "Bet\u00b7tel\u00b7chind", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Druf woni chr\u00e4ftig worde bi,", "tokens": ["Druf", "wo\u00b7ni", "chr\u00e4f\u00b7tig", "wor\u00b7de", "bi", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "und d'Eltere sin gstorbe gsi,", "tokens": ["und", "d'\u00b7El\u00b7te\u00b7re", "sin", "gs\u00b7tor\u00b7be", "gsi", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "se hani denkt: Saldatetod", "tokens": ["se", "ha\u00b7ni", "denkt", ":", "Sal\u00b7da\u00b7te\u00b7tod"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "isch besser weder Bettelbrot.", "tokens": ["isch", "bes\u00b7ser", "we\u00b7der", "Bet\u00b7tel\u00b7brot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "I ha in schwarzer Wetternacht", "tokens": ["I", "ha", "in", "schwar\u00b7zer", "Wet\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["XY", "XY", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "vor Laudons Zelt und Fahne gwacht,", "tokens": ["vor", "Lau\u00b7dons", "Zelt", "und", "Fah\u00b7ne", "gwacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "i bi bim Paschal Paoli", "tokens": ["i", "bi", "bim", "Pa\u00b7schal", "Pao\u00b7li"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.17": {"text": "in Korsika Draguner gsi,", "tokens": ["in", "Kor\u00b7si\u00b7ka", "Dra\u00b7gu\u00b7ner", "gsi", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "und gfochte hani, wie ne Ma,", "tokens": ["und", "gfoch\u00b7te", "ha\u00b7ni", ",", "wie", "ne", "Ma", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PWAV", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "und Bluet an Gurt und Sebel gha.", "tokens": ["und", "Blu\u00b7et", "an", "Gurt", "und", "Se\u00b7bel", "gha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "KON", "NE", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "I bi vor menger Batterie,", "tokens": ["I", "bi", "vor", "men\u00b7ger", "Bat\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["XY", "XY", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "i bi in zwenzig Schlachte gsi,", "tokens": ["i", "bi", "in", "zwen\u00b7zig", "Schlach\u00b7te", "gsi", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "APPR", "CARD", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "und ha mit Treu und Tapferkeit", "tokens": ["und", "ha", "mit", "Treu", "und", "Tap\u00b7fer\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "dur Schwert und Chugle 's Lebe treit.", "tokens": ["dur", "Schwert", "und", "Chug\u00b7le", "'s", "Le\u00b7be", "treit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Zletzt hen sie mi mit lahmem Arm", "tokens": ["Zletzt", "hen", "sie", "mi", "mit", "lah\u00b7mem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "ins Elend gschickt. Dass Gott erbarm!", "tokens": ["ins", "E\u00b7lend", "gschickt", ".", "Dass", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "He jo, dur Gotts Wille!\u00ab", "tokens": ["He", "jo", ",", "dur", "Gotts", "Wil\u00b7le", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "\u00bbchumm arme Ma!", "tokens": ["\u00bb", "chumm", "ar\u00b7me", "Ma", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJA", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "I gunn der's wienis selber ha.", "tokens": ["I", "gunn", "der's", "wie\u00b7nis", "sel\u00b7ber", "ha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.29": {"text": "Und helf der Gott us diner Not,", "tokens": ["Und", "helf", "der", "Gott", "us", "di\u00b7ner", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "und tr\u00f6st di, bis es besser goht.\u00ab", "tokens": ["und", "tr\u00f6st", "di", ",", "bis", "es", "bes\u00b7ser", "goht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NE", "$,", "KOUS", "PPER", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "\u00bbvergelt's der Her, und dank der Gott", "tokens": ["\u00bb", "ver\u00b7gelt's", "der", "Her", ",", "und", "dank", "der", "Gott"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "du zarten Engel wii\u00df und rot,", "tokens": ["du", "zar\u00b7ten", "En\u00b7gel", "wii\u00df", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "und geb der Gott e brave Ma! \u2013", "tokens": ["und", "geb", "der", "Gott", "e", "bra\u00b7ve", "Ma", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "VVFIN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Was luegsch mi so biwegli a?", "tokens": ["Was", "lu\u00b7egsch", "mi", "so", "bi\u00b7weg\u00b7li", "a", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Hesch \u00f6bben au e Schatz im Zelt,", "tokens": ["Hesch", "\u00f6b\u00b7ben", "au", "e", "Schatz", "im", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "mit Schwert und Ro\u00df im wite Feld?", "tokens": ["mit", "Schwert", "und", "Ro\u00df", "im", "wi\u00b7te", "Feld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Biwahr di Gott vor Weh und Leid,", "tokens": ["Bi\u00b7wahr", "di", "Gott", "vor", "Weh", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "und geb dim Schatz e sicher Gleit,", "tokens": ["und", "geb", "dim", "Schatz", "e", "si\u00b7cher", "Gleit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "und bring der bald e gsunde Ma!", "tokens": ["und", "bring", "der", "bald", "e", "gsun\u00b7de", "Ma", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "'s goht ziemli scharf vor Mantua.", "tokens": ["'s", "goht", "ziem\u00b7li", "scharf", "vor", "Man\u00b7tua", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "VVFIN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "'s cha si, i ch\u00f6nnt der Meldig ge. \u2013", "tokens": ["'s", "cha", "si", ",", "i", "ch\u00f6nnt", "der", "Mel\u00b7dig", "ge", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "FM", "FM", "$,", "FM", "FM", "ART", "NE", "NE", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Was luegsch mi a, und wirsch wie Schnee?", "tokens": ["Was", "lu\u00b7egsch", "mi", "a", ",", "und", "wirsch", "wie", "Schnee", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "FM", "FM", "$,", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.43": {"text": "Denkwol, i henk mi Bettelgwand", "tokens": ["Denk\u00b7wol", ",", "i", "henk", "mi", "Bet\u00b7tel\u00b7gwand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "mi falsche graue Bart an d'Wand!", "tokens": ["mi", "fal\u00b7sche", "grau\u00b7e", "Bart", "an", "d'\u00b7Wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.45": {"text": "Jez bschau mi recht, und chennsch mi no?", "tokens": ["Jez", "bschau", "mi", "recht", ",", "und", "chennsch", "mi", "no", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJD", "$,", "KON", "ADJD", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Geb Gott, i seig Gottwilche do!\u00ab", "tokens": ["Geb", "Gott", ",", "i", "seig", "Gott\u00b7wil\u00b7che", "do", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "$,", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "\u00bbher Jesis, der Friedli, mi Friedli isch do!", "tokens": ["\u00bb", "her", "Je\u00b7sis", ",", "der", "Fried\u00b7li", ",", "mi", "Fried\u00b7li", "isch", "do", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "$,", "ART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.48": {"text": "Gottwilche, Gottwilche, wohl chenni di no!", "tokens": ["Gott\u00b7wil\u00b7che", ",", "Gott\u00b7wil\u00b7che", ",", "wohl", "chen\u00b7ni", "di", "no", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "FM", "FM", "FM", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.49": {"text": "Wohl het mi bigleitet di liebligi Gstalt", "tokens": ["Wohl", "het", "mi", "big\u00b7lei\u00b7tet", "di", "lieb\u00b7li\u00b7gi", "Gstalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "FM", "FM", "FM", "FM", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.50": {"text": "uf duftige Matten, im schattige Wald.", "tokens": ["uf", "duf\u00b7ti\u00b7ge", "Mat\u00b7ten", ",", "im", "schat\u00b7ti\u00b7ge", "Wald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.51": {"text": "Wohl het di bigleitet mi bch\u00fcmmeret Herz", "tokens": ["Wohl", "het", "di", "big\u00b7lei\u00b7tet", "mi", "bch\u00fcm\u00b7me\u00b7ret", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "VVFIN", "NE", "VVFIN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.52": {"text": "dur Schwerter und Chugle mit Hoffnig und Schmerz,", "tokens": ["dur", "Schwer\u00b7ter", "und", "Chug\u00b7le", "mit", "Hoff\u00b7nig", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.53": {"text": "und brieget und betet. Gott het mer willfahrt,", "tokens": ["und", "brie\u00b7get", "und", "be\u00b7tet", ".", "Gott", "het", "mer", "will\u00b7fahrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$.", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.54": {"text": "und het mer mi Friedli und het mer en gspart.", "tokens": ["und"