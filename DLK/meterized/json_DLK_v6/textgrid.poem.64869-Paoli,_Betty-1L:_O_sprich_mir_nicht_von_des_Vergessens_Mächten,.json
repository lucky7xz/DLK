{"textgrid.poem.64869": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O sprich mir nicht von des Vergessens M\u00e4chten,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sprich mir nicht von des Vergessens M\u00e4chten,", "tokens": ["O", "sprich", "mir", "nicht", "von", "des", "Ver\u00b7ges\u00b7sens", "M\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die, wenn sie leis sich um die Seele schlingen,", "tokens": ["Die", ",", "wenn", "sie", "leis", "sich", "um", "die", "See\u00b7le", "schlin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Schutt mit frischem Epheu \u00fcberflechten", "tokens": ["Den", "Schutt", "mit", "fri\u00b7schem", "E\u00b7pheu", "\u00fc\u00b7berf\u00b7lech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und heitre Tage sp\u00e4t im Herbste bringen.", "tokens": ["Und", "heit\u00b7re", "Ta\u00b7ge", "sp\u00e4t", "im", "Herbs\u00b7te", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir hat der Herr ein wildres Herz gegeben,", "tokens": ["Mir", "hat", "der", "Herr", "ein", "wild\u00b7res", "Herz", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit W\u00fcnschen st\u00fcrmisch, hei\u00df und unermessen;", "tokens": ["Mit", "W\u00fcn\u00b7schen", "st\u00fcr\u00b7misch", ",", "hei\u00df", "und", "un\u00b7er\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ich als falsches Gl\u00fcck sah von mir schweben,", "tokens": ["Was", "ich", "als", "fal\u00b7sches", "Gl\u00fcck", "sah", "von", "mir", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "ADJA", "NN", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als treuen Jammer mu\u00df ich's in mich pressen.", "tokens": ["Als", "treu\u00b7en", "Jam\u00b7mer", "mu\u00df", "ich's", "in", "mich", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VMFIN", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum bist und bleibst du mein f\u00fcr alle Zeiten,", "tokens": ["Drum", "bist", "und", "bleibst", "du", "mein", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "KON", "VVFIN", "PPER", "PPOSAT", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freude nur ist eilig im Vergessen;", "tokens": ["Die", "Freu\u00b7de", "nur", "ist", "ei\u00b7lig", "im", "Ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich aber wird durch's Leben still geleiten", "tokens": ["Mich", "a\u00b7ber", "wird", "durch's", "Le\u00b7ben", "still", "ge\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der dunkle Gram, da\u00df ich dich nie besessen.", "tokens": ["Der", "dunk\u00b7le", "Gram", ",", "da\u00df", "ich", "dich", "nie", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}