{"textgrid.poem.67428": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "In das Gesangbuch der Frau von *", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wunsch, der still f\u00fcr uns und Andre fleht,", "tokens": ["Ein", "Wunsch", ",", "der", "still", "f\u00fcr", "uns", "und", "And\u00b7re", "fleht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Seufzer, der dem Herzen leis entweht,", "tokens": ["Ein", "Seuf\u00b7zer", ",", "der", "dem", "Her\u00b7zen", "leis", "ent\u00b7weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den keine Lippe spricht, ist ein Gebet.", "tokens": ["Den", "kei\u00b7ne", "Lip\u00b7pe", "spricht", ",", "ist", "ein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Die Freude, die in unsrer Brust erklang,", "tokens": ["Die", "Freu\u00b7de", ",", "die", "in", "uns\u00b7rer", "Brust", "er\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die neu sich f\u00fchlt mit Jubel, Preis und Dank,", "tokens": ["Die", "neu", "sich", "f\u00fchlt", "mit", "Ju\u00b7bel", ",", "Preis", "und", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Himmel steigt sie auf und wird Gesang.", "tokens": ["Zum", "Him\u00b7mel", "steigt", "sie", "auf", "und", "wird", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn sich Dein Sinn im Streben einsam m\u00fcht,", "tokens": ["Wenn", "sich", "Dein", "Sinn", "im", "Stre\u00b7ben", "ein\u00b7sam", "m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verschleiert und umw\u00f6lkt sich Dein Gem\u00fcth,", "tokens": ["Ver\u00b7schlei\u00b7ert", "und", "um\u00b7w\u00f6lkt", "sich", "Dein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erheb, entw\u00f6lk es durch ein heilig Lied!", "tokens": ["Er\u00b7heb", ",", "ent\u00b7w\u00f6lk", "es", "durch", "ein", "hei\u00b7lig", "Lied", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ger\u00fchrt von Freude, voll von s\u00fc\u00dfem Dank,", "tokens": ["Ge\u00b7r\u00fchrt", "von", "Freu\u00b7de", ",", "voll", "von", "s\u00fc\u00b7\u00dfem", "Dank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ert\u00f6ne dann des Herzens Silberklang,", "tokens": ["Er\u00b7t\u00f6\u00b7ne", "dann", "des", "Her\u00b7zens", "Sil\u00b7ber\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und all Dein Leben werde Lobgesang!", "tokens": ["Und", "all", "Dein", "Le\u00b7ben", "wer\u00b7de", "Lob\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}