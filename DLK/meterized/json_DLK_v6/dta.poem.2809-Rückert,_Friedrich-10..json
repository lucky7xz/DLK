{"dta.poem.2809": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum hast du dich gefreut fehlloser Jugendbl\u00fcthe", "tokens": ["Kaum", "hast", "du", "dich", "ge\u00b7freut", "fehl\u00b7lo\u00b7ser", "Ju\u00b7gend\u00b7bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des sch\u00f6nsten, theuersten, mit dankbarem Gem\u00fcthe;", "tokens": ["Des", "sch\u00f6ns\u00b7ten", ",", "theu\u00b7ers\u00b7ten", ",", "mit", "dank\u00b7ba\u00b7rem", "Ge\u00b7m\u00fc\u00b7the", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So haucht ein Unheil, und der Lustglanz ist vorbei,", "tokens": ["So", "haucht", "ein", "Un\u00b7heil", ",", "und", "der", "Lust\u00b7glanz", "ist", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob gefallen drein ein b\u00f6ser Mehlthau sei.", "tokens": ["Al\u00b7sob", "ge\u00b7fal\u00b7len", "drein", "ein", "b\u00f6\u00b7ser", "Mehlt\u00b7hau", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Darf man sich loben nichts, aus Furcht es zu berufen?", "tokens": ["Darf", "man", "sich", "lo\u00b7ben", "nichts", ",", "aus", "Furcht", "es", "zu", "be\u00b7ru\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "VVFIN", "PIS", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An nichts sich freuen, was zur Freude G\u00f6tter schufen?", "tokens": ["An", "nichts", "sich", "freu\u00b7en", ",", "was", "zur", "Freu\u00b7de", "G\u00f6t\u00b7ter", "schu\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PRF", "VVINF", "$,", "PRELS", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nein, danke Gott, da\u00df dir nicht lastet aufs Genick", "tokens": ["Nein", ",", "dan\u00b7ke", "Gott", ",", "da\u00df", "dir", "nicht", "las\u00b7tet", "aufs", "Ge\u00b7nick"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Feindselige G\u00f6ttermacht und neidisches Geschick.", "tokens": ["Feind\u00b7se\u00b7li\u00b7ge", "G\u00f6t\u00b7ter\u00b7macht", "und", "nei\u00b7di\u00b7sches", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Nein, danke Gott, der dir die Freude g\u00f6nnen wollte", "tokens": ["Nein", ",", "dan\u00b7ke", "Gott", ",", "der", "dir", "die", "Freu\u00b7de", "g\u00f6n\u00b7nen", "woll\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Sch\u00f6nen eben noch als es verbl\u00fchen sollte.", "tokens": ["Am", "Sch\u00f6\u00b7nen", "e\u00b7ben", "noch", "als", "es", "ver\u00b7bl\u00fc\u00b7hen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nicht deine Freude hat den Schaden angebahnt,", "tokens": ["Nicht", "dei\u00b7ne", "Freu\u00b7de", "hat", "den", "Scha\u00b7den", "an\u00b7ge\u00b7bahnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du freutest dich nur so, vom droh'nden vorgemahnt.", "tokens": ["Du", "freu\u00b7test", "dich", "nur", "so", ",", "vom", "droh'n\u00b7den", "vor\u00b7ge\u00b7mahnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}