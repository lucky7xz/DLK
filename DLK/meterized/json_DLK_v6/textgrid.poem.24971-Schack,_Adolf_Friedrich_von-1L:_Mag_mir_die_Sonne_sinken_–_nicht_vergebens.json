{"textgrid.poem.24971": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mag mir die Sonne sinken \u2013 nicht vergebens", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag mir die Sonne sinken \u2013 nicht vergebens", "tokens": ["Mag", "mir", "die", "Son\u00b7ne", "sin\u00b7ken", "\u2013", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$(", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat sie auf Erden mir gegl\u00e4nzt;", "tokens": ["Hat", "sie", "auf", "Er\u00b7den", "mir", "ge\u00b7gl\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reich ward der gro\u00dfe Wunderkelch des Lebens,", "tokens": ["Reich", "ward", "der", "gro\u00b7\u00dfe", "Wun\u00b7der\u00b7kelch", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zum Rande sch\u00e4umend, mir kredenzt.", "tokens": ["Zum", "Ran\u00b7de", "sch\u00e4u\u00b7mend", ",", "mir", "kre\u00b7denzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im hehren Mai, wenn \u00fcber Berg' und Thale", "tokens": ["Im", "heh\u00b7ren", "Mai", ",", "wenn", "\u00fc\u00b7ber", "Ber\u00b7g'", "und", "Tha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Siegeszug der Fr\u00fchling h\u00e4lt,", "tokens": ["Den", "Sie\u00b7ges\u00b7zug", "der", "Fr\u00fch\u00b7ling", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stand ich anbetend nun schon drei\u00dfig Male", "tokens": ["Stand", "ich", "an\u00b7be\u00b7tend", "nun", "schon", "drei\u00b7\u00dfig", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "ADV", "ADV", "CARD", "NN"], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Vor dieser wundervollen Welt.", "tokens": ["Vor", "die\u00b7ser", "wun\u00b7der\u00b7vol\u00b7len", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Klangvoll zog hin durch meiner Seele Saiten,", "tokens": ["Klang\u00b7voll", "zog", "hin", "durch", "mei\u00b7ner", "See\u00b7le", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was nur mit Werderuf", "tokens": ["Was", "nur", "mit", "Wer\u00b7de\u00b7ruf"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Glorreich-Unsterbliches zu allen Zeiten", "tokens": ["Glor\u00b7reich\u00b7Un\u00b7sterb\u00b7li\u00b7ches", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Des Menschen Genius erschuf.", "tokens": ["Des", "Men\u00b7schen", "Ge\u00b7nius", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Liebe vollstes Gl\u00fcck hab' ich genossen", "tokens": ["Der", "Lie\u00b7be", "volls\u00b7tes", "Gl\u00fcck", "hab'", "ich", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Herz und Sinnen, Leib und Geist;", "tokens": ["An", "Herz", "und", "Sin\u00b7nen", ",", "Leib", "und", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Freunden einen Seelenbund geschlossen,", "tokens": ["Mit", "Freun\u00b7den", "ei\u00b7nen", "See\u00b7len\u00b7bund", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den keine Ewigkeit zerrei\u00dft.", "tokens": ["Den", "kei\u00b7ne", "E\u00b7wig\u00b7keit", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Am goldnen Tag, im Sturm und in der Stille,", "tokens": ["Am", "gold\u00b7nen", "Tag", ",", "im", "Sturm", "und", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus sternenhellem Nachtazur,", "tokens": ["Aus", "ster\u00b7nen\u00b7hel\u00b7lem", "Nach\u00b7ta\u00b7zur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sprach mit dem heil'gen Munde der Sibylle", "tokens": ["Sprach", "mit", "dem", "heil'\u00b7gen", "Mun\u00b7de", "der", "Si\u00b7byl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir Seherworte die Natur.", "tokens": ["Mir", "Se\u00b7her\u00b7wor\u00b7te", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich wei\u00df, da\u00df \u00fcber mir und mir zu F\u00fc\u00dfen", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "\u00fc\u00b7ber", "mir", "und", "mir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PPER", "KON", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und um mich Welt an Welt sich reiht;", "tokens": ["Und", "um", "mich", "Welt", "an", "Welt", "sich", "reiht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fernher ert\u00f6nt zu meinem Ohr ein Gr\u00fc\u00dfen", "tokens": ["Fern\u00b7her", "er\u00b7t\u00f6nt", "zu", "mei\u00b7nem", "Ohr", "ein", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus d\u00e4mmernder Unendlichkeit.", "tokens": ["Aus", "d\u00e4m\u00b7mern\u00b7der", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und mu\u00df es sein, mu\u00df nun im Grenzenlosen", "tokens": ["Und", "mu\u00df", "es", "sein", ",", "mu\u00df", "nun", "im", "Gren\u00b7zen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VAINF", "$,", "VMFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lebensatem mir verwehn:", "tokens": ["Der", "Le\u00b7ben\u00b7sa\u00b7tem", "mir", "ver\u00b7wehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich klage nicht; das Haupt bekr\u00e4nzt mit Rosen,", "tokens": ["Ich", "kla\u00b7ge", "nicht", ";", "das", "Haupt", "be\u00b7kr\u00e4nzt", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Will ich von hinnen gehn.", "tokens": ["Will", "ich", "von", "hin\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}