{"textgrid.poem.56971": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Nein!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pfeift der Sturm?", "tokens": ["Pfeift", "der", "Sturm", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Keift ein Wurm?", "tokens": ["Keift", "ein", "Wurm", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Heulen", "tokens": ["Heu\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Eulen", "tokens": ["Eu\u00b7len"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "hoch vom Turm?", "tokens": ["hoch", "vom", "Turm", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Nein!", "tokens": ["Nein", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Es ist des Galgenstrickes", "tokens": ["Es", "ist", "des", "Gal\u00b7gen\u00b7stri\u00b7ckes"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dickes", "tokens": ["di\u00b7ckes"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ende, welches \u00e4chzte,", "tokens": ["En\u00b7de", ",", "wel\u00b7ches", "\u00e4chz\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "gleich als ob", "tokens": ["gleich", "als", "ob"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "im Galopp", "tokens": ["im", "Ga\u00b7lopp"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "eine m\u00fcdgehetzte M\u00e4hre", "tokens": ["ei\u00b7ne", "m\u00fcd\u00b7ge\u00b7hetz\u00b7te", "M\u00e4h\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "nach dem n\u00e4chsten Brunnen lechzte", "tokens": ["nach", "dem", "n\u00e4chs\u00b7ten", "Brun\u00b7nen", "lechz\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "(der vielleicht noch ferne w\u00e4re).", "tokens": ["(", "der", "viel\u00b7leicht", "noch", "fer\u00b7ne", "w\u00e4\u00b7re", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADV", "ADV", "ADV", "VAFIN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}