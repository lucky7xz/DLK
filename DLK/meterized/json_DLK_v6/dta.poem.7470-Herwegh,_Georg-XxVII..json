{"dta.poem.7470": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tot ist die Freundschaft! wer mag sie noch singen?", "tokens": ["Tot", "ist", "die", "Freund\u00b7schaft", "!", "wer", "mag", "sie", "noch", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Mit manchen G\u00f6ttern ward in unsern Tagen", "tokens": ["Mit", "man\u00b7chen", "G\u00f6t\u00b7tern", "ward", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch diese G\u00f6ttin von dem Volk erschlagen,", "tokens": ["Auch", "die\u00b7se", "G\u00f6t\u00b7tin", "von", "dem", "Volk", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Niemand will ihr mehr ein Opfer bringen.", "tokens": ["Und", "Nie\u00b7mand", "will", "ihr", "mehr", "ein", "Op\u00b7fer", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Alten denkt man wohl in manchen Stunden,", "tokens": ["Der", "Al\u00b7ten", "denkt", "man", "wohl", "in", "man\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auch ihr Geist, so gern man sich's verhehlte,", "tokens": ["Und", "auch", "ihr", "Geist", ",", "so", "gern", "man", "sich's", "ver\u00b7hehl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,", "ADV", "ADV", "PIS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist aus der Jugend noch nicht ganz verschwunden;", "tokens": ["Ist", "aus", "der", "Ju\u00b7gend", "noch", "nicht", "ganz", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch hin das Herrlichste, was sie beseelte;", "tokens": ["Doch", "hin", "das", "Herr\u00b7lichs\u00b7te", ",", "was", "sie", "be\u00b7seel\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00fcrd' ein Aristogiton heut' gefunden,", "tokens": ["W\u00fcrd'", "ein", "A\u00b7ris\u00b7to\u00b7gi\u00b7ton", "heut'", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "ADV", "VVPP", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich glaube, da\u00df ihm der Harmodius fehlte.", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", "ihm", "der", "Har\u00b7mo\u00b7dius", "fehl\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}