{"textgrid.poem.36695": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wahrlich, o Roma, du bist an bezauberndem Wechsel ein Wunder,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrlich, o Roma, du bist an bezauberndem Wechsel ein Wunder,", "tokens": ["Wahr\u00b7lich", ",", "o", "Ro\u00b7ma", ",", "du", "bist", "an", "be\u00b7zau\u00b7bern\u00b7dem", "Wech\u00b7sel", "ein", "Wun\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "FM", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Nur wer dich siehet, erkennt, was du dem Gl\u00fccklichen bist.", "tokens": ["Nur", "wer", "dich", "sie\u00b7het", ",", "er\u00b7kennt", ",", "was", "du", "dem", "Gl\u00fcck\u00b7li\u00b7chen", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "PWS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Selbst der schweigende Gott, wenn der staunende Wandrer ihn fraget,", "tokens": ["Selbst", "der", "schwei\u00b7gen\u00b7de", "Gott", ",", "wenn", "der", "stau\u00b7nen\u00b7de", "Wand\u00b7rer", "ihn", "fra\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Deutet aufs ewige Buch, das die Geschichte sich nennt,", "tokens": ["Deu\u00b7tet", "aufs", "e\u00b7wi\u00b7ge", "Buch", ",", "das", "die", "Ge\u00b7schich\u00b7te", "sich", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Denn, was der Sch\u00f6pfung er ist, das ist Roma der Welt, und ihr Schicksal", "tokens": ["Denn", ",", "was", "der", "Sch\u00f6p\u00b7fung", "er", "ist", ",", "das", "ist", "Ro\u00b7ma", "der", "Welt", ",", "und", "ihr", "Schick\u00b7sal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "PPER", "VAFIN", "$,", "PDS", "VAFIN", "NN", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Fiel aus der Urne, wie nur Einer Kronion es gab.", "tokens": ["Fiel", "aus", "der", "Ur\u00b7ne", ",", "wie", "nur", "Ei\u00b7ner", "Kro\u00b7ni\u00b7on", "es", "gab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Schaue die Tempel nur an, und die m\u00e4chtigen S\u00e4ulen, die herrlich", "tokens": ["Schau\u00b7e", "die", "Tem\u00b7pel", "nur", "an", ",", "und", "die", "m\u00e4ch\u00b7ti\u00b7gen", "S\u00e4u\u00b7len", ",", "die", "herr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "KON", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Unterm erhabenen Schutt zweier Jahrtausende stehn!", "tokens": ["Un\u00b7term", "er\u00b7ha\u00b7be\u00b7nen", "Schutt", "zwei\u00b7er", "Jahr\u00b7tau\u00b7sen\u00b7de", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Tritt nur ins Pantheon ein, da l\u00e4chelt's ins heilige Dunkel,", "tokens": ["Tritt", "nur", "ins", "Pan\u00b7the\u00b7on", "ein", ",", "da", "l\u00e4\u00b7chelt's", "ins", "hei\u00b7li\u00b7ge", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,", "KOUS", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Oben voll heiterem Licht, sch\u00f6n wie der Himmel herab.", "tokens": ["O\u00b7ben", "voll", "hei\u00b7te\u00b7rem", "Licht", ",", "sch\u00f6n", "wie", "der", "Him\u00b7mel", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Und kein verwegenes Wort, das empfindende Herz nur erreicht es;", "tokens": ["Und", "kein", "ver\u00b7we\u00b7ge\u00b7nes", "Wort", ",", "das", "emp\u00b7fin\u00b7den\u00b7de", "Herz", "nur", "er\u00b7reicht", "es", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Aber das sch\u00f6nste ist Rom, was mir in Rom noch gefiel.", "tokens": ["A\u00b7ber", "das", "sch\u00f6ns\u00b7te", "ist", "Rom", ",", "was", "mir", "in", "Rom", "noch", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VAFIN", "NE", "$,", "PWS", "PPER", "APPR", "NE", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Darum erw\u00e4hlet mein Herz mit deiner Pinienh\u00fcgel", "tokens": ["Da\u00b7rum", "er\u00b7w\u00e4h\u00b7let", "mein", "Herz", "mit", "dei\u00b7ner", "Pi\u00b7ni\u00b7en\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Bl\u00fchenden G\u00e4rten so gern, s\u00fc\u00dfer Gianicolo, dich!", "tokens": ["Bl\u00fc\u00b7hen\u00b7den", "G\u00e4r\u00b7ten", "so", "gern", ",", "s\u00fc\u00b7\u00dfer", "Gi\u00b7a\u00b7ni\u00b7co\u00b7lo", ",", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "$,", "ADJA", "NN", "$,", "PPER", "$."], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.15": {"text": "Und ich entwandle dem Schwarm der rauschenden Stra\u00dfen am Abend,", "tokens": ["Und", "ich", "ent\u00b7wand\u00b7le", "dem", "Schwarm", "der", "rau\u00b7schen\u00b7den", "Stra\u00b7\u00dfen", "am", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Bis dein erquickendes Bild \u00fcber dem Tiber erscheint.", "tokens": ["Bis", "dein", "er\u00b7qui\u00b7cken\u00b7des", "Bild", "\u00fc\u00b7ber", "dem", "Ti\u00b7ber", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Dann ergl\u00fcht mir das stumme Gem\u00fcth, und ich fliege dir sehnend,", "tokens": ["Dann", "er\u00b7gl\u00fcht", "mir", "das", "stum\u00b7me", "Ge\u00b7m\u00fcth", ",", "und", "ich", "flie\u00b7ge", "dir", "seh\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Wie der Mutter das Kind, heil'ger Onofrio, zu.", "tokens": ["Wie", "der", "Mut\u00b7ter", "das", "Kind", ",", "heil'\u00b7ger", "O\u00b7no\u00b7frio", ",", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Und du labst mich mit friedlichem Gr\u00fcn und einsamen Schatten,", "tokens": ["Und", "du", "labst", "mich", "mit", "fried\u00b7li\u00b7chem", "Gr\u00fcn", "und", "ein\u00b7sa\u00b7men", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Wo ich so selig dich einst, Kloster und Kirche, begr\u00fc\u00dft.", "tokens": ["Wo", "ich", "so", "se\u00b7lig", "dich", "einst", ",", "Klos\u00b7ter", "und", "Kir\u00b7che", ",", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "PPER", "ADV", "$,", "NN", "KON", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Da ist Ruhe, da lispelt es kaum im zitternden Laube,", "tokens": ["Da", "ist", "Ru\u00b7he", ",", "da", "lis\u00b7pelt", "es", "kaum", "im", "zit\u00b7tern\u00b7den", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Still, wie des Dichters Grab breitet das Pl\u00e4tzchen sich aus.", "tokens": ["Still", ",", "wie", "des", "Dich\u00b7ters", "Grab", "brei\u00b7tet", "das", "Pl\u00e4tz\u00b7chen", "sich", "aus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "---+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Da mit unendlicher Lust eil' ich ans moos'ge Gem\u00e4uer,", "tokens": ["Da", "mit", "un\u00b7end\u00b7li\u00b7cher", "Lust", "eil'", "ich", "ans", "moos'\u00b7ge", "Ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Feuer und Nebel im Blick \u2013 Himmel und Roma vor mir!", "tokens": ["Feu\u00b7er", "und", "Ne\u00b7bel", "im", "Blick", "\u2013", "Him\u00b7mel", "und", "Ro\u00b7ma", "vor", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$(", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "+--+--++--++-+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Und ich knie auf die steinerne Bank, und hinunter, hinunter", "tokens": ["Und", "ich", "knie", "auf", "die", "stei\u00b7ner\u00b7ne", "Bank", ",", "und", "hin\u00b7un\u00b7ter", ",", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "PTKVZ", "$,", "ADV"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Schau' ich wie Zeus im Olymp, \u00fcber die Herrliche hin.", "tokens": ["Schau'", "ich", "wie", "Zeus", "im", "O\u00b7lymp", ",", "\u00fc\u00b7ber", "die", "Herr\u00b7li\u00b7che", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOKOM", "NE", "APPRART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "O we\u00df Auge das Meer nie erblickt, we\u00df Auge nicht Rom sah,", "tokens": ["O", "we\u00df", "Au\u00b7ge", "das", "Meer", "nie", "er\u00b7blickt", ",", "we\u00df", "Au\u00b7ge", "nicht", "Rom", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "ART", "NN", "ADV", "VVPP", "$,", "PWAT", "NN", "PTKNEG", "NE", "VVFIN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Der hat die Welt und in ihr auch nicht den Sch\u00f6pfer gesehen.", "tokens": ["Der", "hat", "die", "Welt", "und", "in", "ihr", "auch", "nicht", "den", "Sch\u00f6p\u00b7fer", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "APPR", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Schweiget, ihr Worte, mir ist als erst\u00e4nden die Geister vom Grabe,", "tokens": ["Schwei\u00b7get", ",", "ihr", "Wor\u00b7te", ",", "mir", "ist", "als", "er\u00b7st\u00e4n\u00b7den", "die", "Geis\u00b7ter", "vom", "Gra\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "KOKOM", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Die ihr erhabenes Werk hier f\u00fcr die Nachwelt gebaut,", "tokens": ["Die", "ihr", "er\u00b7ha\u00b7be\u00b7nes", "Werk", "hier", "f\u00fcr", "die", "Nach\u00b7welt", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Als erbraust' ihr rauschendes Lied hoch \u00fcber den Tr\u00fcmmern,", "tokens": ["Als", "er\u00b7braust'", "ihr", "rau\u00b7schen\u00b7des", "Lied", "hoch", "\u00fc\u00b7ber", "den", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Als erh\u00fcbe die Zeit selber den Schicksalsgesang!", "tokens": ["Als", "er\u00b7h\u00fc\u00b7be", "die", "Zeit", "sel\u00b7ber", "den", "Schick\u00b7sals\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Und doch l\u00e4chelt der Himmel so voll unaussprechlicher Liebe,", "tokens": ["Und", "doch", "l\u00e4\u00b7chelt", "der", "Him\u00b7mel", "so", "voll", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "Ueber dem bl\u00fchenden Kind, \u00fcber der s\u00fc\u00dfen Natur,", "tokens": ["Ue\u00b7ber", "dem", "bl\u00fc\u00b7hen\u00b7den", "Kind", ",", "\u00fc\u00b7ber", "der", "s\u00fc\u00b7\u00dfen", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.35": {"text": "Wie er's, das blaue Auge voll tief woll\u00fcstigem Lichte", "tokens": ["Wie", "er's", ",", "das", "blau\u00b7e", "Au\u00b7ge", "voll", "tief", "wol\u00b7l\u00fcs\u00b7ti\u00b7gem", "Lich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "ART", "ADJA", "NN", "ADJD", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Selig am Sch\u00f6pfungstag einst auf die Stirne gek\u00fc\u00dft.", "tokens": ["Se\u00b7lig", "am", "Sch\u00f6p\u00b7fungs\u00b7tag", "einst", "auf", "die", "Stir\u00b7ne", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Sieh nur hinunter