zen zerschellt;", "tokens": ["Furcht\u00b7bar", ",", "wie", "Fel\u00b7sen", ",", "die", "Gott", "stra\u00b7fend", "mit", "Blit\u00b7zen", "zer\u00b7schellt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "ART", "NN", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.51": {"text": "Ueberall Tempel im Gr\u00fcn und entlang die unendlichen Gr\u00fcnde", "tokens": ["Ue\u00b7be\u00b7rall", "Tem\u00b7pel", "im", "Gr\u00fcn", "und", "ent\u00b7lang", "die", "un\u00b7end\u00b7li\u00b7chen", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPRART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.52": {"text": "B\u00f6gen, in rosige Fluth himmlisch vom Abend getaucht.", "tokens": ["B\u00f6\u00b7gen", ",", "in", "ro\u00b7si\u00b7ge", "Fluth", "himm\u00b7lisch", "vom", "A\u00b7bend", "ge\u00b7taucht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.53": {"text": "G\u00f6tter, was all'? und das duft'ge Gebirg in versch\u00e4mtem Err\u00f6then,", "tokens": ["G\u00f6t\u00b7ter", ",", "was", "all'", "?", "und", "das", "duft'\u00b7ge", "Ge\u00b7birg", "in", "ver\u00b7sch\u00e4m\u00b7tem", "Er\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "$.", "KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.54": {"text": "Zart und herrlich, wie nur Claud' und der Sch\u00f6pfer gemalt!", "t