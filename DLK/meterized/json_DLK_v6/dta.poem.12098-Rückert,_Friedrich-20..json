{"dta.poem.12098": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah ein sch\u00f6nes Haus, reich von der Kunst geschm\u00fcckt,", "tokens": ["Ich", "sah", "ein", "sch\u00f6\u00b7nes", "Haus", ",", "reich", "von", "der", "Kunst", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Bilder Farbenglut den W\u00e4nden aufgedr\u00fcckt.", "tokens": ["Der", "Bil\u00b7der", "Far\u00b7beng\u00b7lut", "den", "W\u00e4n\u00b7den", "auf\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch war die gr\u00f6ste Kunst, da\u00df sich die Kunst so breit", "tokens": ["Doch", "war", "die", "gr\u00f6s\u00b7te", "Kunst", ",", "da\u00df", "sich", "die", "Kunst", "so", "breit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PRF", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht machte drin, um auszuschlie\u00dfen Wohnlichkeit.", "tokens": ["Nicht", "mach\u00b7te", "drin", ",", "um", "aus\u00b7zu\u00b7schlie\u00b7\u00dfen", "Wohn\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PTKVZ", "$,", "KOUI", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist die rechte Kunst, die, ohne Raum dem Leben", "tokens": ["Das", "ist", "die", "rech\u00b7te", "Kunst", ",", "die", ",", "oh\u00b7ne", "Raum", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "$,", "KOUI", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu nehmen, sich begn\u00fcgt, ihm heitern Schmuck zu geben.", "tokens": ["Zu", "neh\u00b7men", ",", "sich", "be\u00b7gn\u00fcgt", ",", "ihm", "hei\u00b7tern", "Schmuck", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRF", "VVPP", "$,", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was hilft es dem, der ganz sein Haus lie\u00df malen an,", "tokens": ["Was", "hilft", "es", "dem", ",", "der", "ganz", "sein", "Haus", "lie\u00df", "ma\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er vor lauter Glanz es nicht bewohnen kan?", "tokens": ["Wenn", "er", "vor", "lau\u00b7ter", "Glanz", "es", "nicht", "be\u00b7woh\u00b7nen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}