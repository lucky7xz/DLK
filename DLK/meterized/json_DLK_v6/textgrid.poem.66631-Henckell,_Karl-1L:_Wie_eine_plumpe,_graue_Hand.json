{"textgrid.poem.66631": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie eine plumpe, graue Hand", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine plumpe, graue Hand", "tokens": ["Wie", "ei\u00b7ne", "plum\u00b7pe", ",", "grau\u00b7e", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt heut der Himmel ausgespannt;", "tokens": ["Liegt", "heut", "der", "Him\u00b7mel", "aus\u00b7ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Riesin pre\u00dft mich nieder.", "tokens": ["Die", "Rie\u00b7sin", "pre\u00dft", "mich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Atem stockt im Busen schier,", "tokens": ["Der", "A\u00b7tem", "stockt", "im", "Bu\u00b7sen", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erstickt der Mut, ein r\u00f6chelnd Tier,", "tokens": ["Er\u00b7stickt", "der", "Mut", ",", "ein", "r\u00f6\u00b7chelnd", "Tier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erdrosselt Lust und Lieder.", "tokens": ["Er\u00b7dros\u00b7selt", "Lust", "und", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein Laut, kein Licht, kein Hauch. \u2013 Mir graust.", "tokens": ["Kein", "Laut", ",", "kein", "Licht", ",", "kein", "Hauch", ".", "\u2013", "Mir", "graust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O du verfluchte Riesenfaust,", "tokens": ["O", "du", "ver\u00b7fluch\u00b7te", "Rie\u00b7sen\u00b7faust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie l\u00e4hmst du mir den Nacken!", "tokens": ["Wie", "l\u00e4hmst", "du", "mir", "den", "Na\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unheimlich \u00f6de, dumpf und starr.", "tokens": ["Un\u00b7heim\u00b7lich", "\u00f6\u00b7de", ",", "dumpf", "und", "starr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist das der Tod? Bin ich ein Narr?", "tokens": ["Ist", "das", "der", "Tod", "?", "Bin", "ich", "ein", "Narr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$.", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird mich der Wahnsinn packen?", "tokens": ["Wird", "mich", "der", "Wahn\u00b7sinn", "pa\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Z\u00fcnd an! Ich mu\u00df was lodern sehn!", "tokens": ["Z\u00fcnd", "an", "!", "Ich", "mu\u00df", "was", "lo\u00b7dern", "sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VMFIN", "PIS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}