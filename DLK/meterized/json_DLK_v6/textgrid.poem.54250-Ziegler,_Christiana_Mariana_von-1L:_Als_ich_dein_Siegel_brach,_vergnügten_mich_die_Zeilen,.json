{"textgrid.poem.54250": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich dein Siegel brach, vergn\u00fcgten mich die Zeilen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich dein Siegel brach, vergn\u00fcgten mich die Zeilen,", "tokens": ["Als", "ich", "dein", "Sie\u00b7gel", "brach", ",", "ver\u00b7gn\u00fcg\u00b7ten", "mich", "die", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch dacht ich auch zugleich, wie kan das m\u00f6glich seyn,", "tokens": ["Doch", "dacht", "ich", "auch", "zu\u00b7gleich", ",", "wie", "kan", "das", "m\u00f6g\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "VMFIN", "PDS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist entfernt von mir auf mehr als N \u2013 \u2013 Meilen,", "tokens": ["Er", "ist", "ent\u00b7fernt", "von", "mir", "auf", "mehr", "als", "N", "\u2013", "\u2013", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "APPR", "PIAT", "KOKOM", "XY", "$(", "$(", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und doch l\u00e4ufft schon ein Blatt von seinen H\u00e4nden ein.", "tokens": ["Und", "doch", "l\u00e4ufft", "schon", "ein", "Blatt", "von", "sei\u00b7nen", "H\u00e4n\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ich hatte, glaub es, mir die Zuschrifft gantz verziehen,", "tokens": ["Ich", "hat\u00b7te", ",", "glaub", "es", ",", "mir", "die", "Zu\u00b7schrifft", "gantz", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Um desto freudiger nahm ich, mein Freund, sie an", "tokens": ["Um", "des\u00b7to", "freu\u00b7di\u00b7ger", "nahm", "ich", ",", "mein", "Freund", ",", "sie", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "ADV", "ADJD", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "PTKVZ"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Dein Brieff versichert mir, du spahrtest kein Bem\u00fchen,", "tokens": ["Dein", "Brieff", "ver\u00b7si\u00b7chert", "mir", ",", "du", "spahr\u00b7test", "kein", "Be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zeigtest was ein Freund vor Freundschafft zeigen kan.", "tokens": ["Und", "zeig\u00b7test", "was", "ein", "Freund", "vor", "Freund\u00b7schafft", "zei\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du weist, da\u00df die Music mir Ohr und Hertz erg\u00f6tzet,", "tokens": ["Du", "weist", ",", "da\u00df", "die", "Mu\u00b7sic", "mir", "Ohr", "und", "Hertz", "er\u00b7g\u00f6t\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Drum hat auch deine Hand mir etwas \u00fcberschickt,", "tokens": ["Drum", "hat", "auch", "dei\u00b7ne", "Hand", "mir", "et\u00b7was", "\u00fc\u00b7bersc\u00b7hickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das ieder, der es h\u00f6rt, vor unvergleichlich sch\u00e4tzet,", "tokens": ["Das", "ie\u00b7der", ",", "der", "es", "h\u00f6rt", ",", "vor", "un\u00b7ver\u00b7gleich\u00b7lich", "sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil man an Ton und Klang den Meister gleich erblickt.", "tokens": ["Weil", "man", "an", "Ton", "und", "Klang", "den", "Meis\u00b7ter", "gleich", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich nenne mich daf\u00fcr dir iederzeit verbunden,", "tokens": ["Ich", "nen\u00b7ne", "mich", "da\u00b7f\u00fcr", "dir", "ie\u00b7der\u00b7zeit", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was k\u00f6nt, erweg es selbst, mir angenehmer seyn?", "tokens": ["Was", "k\u00f6nt", ",", "er\u00b7weg", "es", "selbst", ",", "mir", "an\u00b7ge\u00b7neh\u00b7mer", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dergleichen Zeitvertreib vers\u00fcst mir recht die Stunden,", "tokens": ["Derg\u00b7lei\u00b7chen", "Zeit\u00b7ver\u00b7treib", "ver\u00b7s\u00fcst", "mir", "recht", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die das Verh\u00e4ngni\u00df offt mit Galle will bestreun.", "tokens": ["Die", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "offt", "mit", "Gal\u00b7le", "will", "be\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch hast du mir und dir gar nicht dabey zu schmeicheln,", "tokens": ["Doch", "hast", "du", "mir", "und", "dir", "gar", "nicht", "da\u00b7bey", "zu", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "KON", "PPER", "ADV", "PTKNEG", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als w\u00e4r ich in der Kunst bereits ein grosses Licht;", "tokens": ["Als", "w\u00e4r", "ich", "in", "der", "Kunst", "be\u00b7reits", "ein", "gros\u00b7ses", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Freund, der redlich heist, mu\u00df nicht dem andern heucheln,", "tokens": ["Ein", "Freund", ",", "der", "red\u00b7lich", "heist", ",", "mu\u00df", "nicht", "dem", "an\u00b7dern", "heu\u00b7cheln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VMFIN", "PTKNEG", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich bin, du weist es wohl, kein Virtuose nicht.", "tokens": ["Ich", "bin", ",", "du", "weist", "es", "wohl", ",", "kein", "Vir\u00b7tu\u00b7o\u00b7se", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein Lauten-Spiel mu\u00df sich noch vor der Welt verstecken,", "tokens": ["Mein", "Lau\u00b7ten\u00b7Spiel", "mu\u00df", "sich", "noch", "vor", "der", "Welt", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und sing ich auch darein, so kan doch mein Gesang,", "tokens": ["Und", "sing", "ich", "auch", "da\u00b7rein", ",", "so", "kan", "doch", "mein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "$,", "ADV", "VMFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der roh und heischer ist, von Beyfall nichts erwecken,", "tokens": ["Der", "roh", "und", "hei\u00b7scher", "ist", ",", "von", "Bey\u00b7fall", "nichts", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "$,", "APPR", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Fl\u00f6then Ton ist auch von nicht viel bessern Klang.", "tokens": ["Der", "Fl\u00f6\u00b7then", "Ton", "ist", "auch", "von", "nicht", "viel", "bes\u00b7sern", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch la\u00df deswegen dich, mein Freund, nicht irrig machen,", "tokens": ["Doch", "la\u00df", "des\u00b7we\u00b7gen", "dich", ",", "mein", "Freund", ",", "nicht", "ir\u00b7rig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PAV", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Von deiner klugen Hand mir ferner was zu weyhn;", "tokens": ["Von", "dei\u00b7ner", "klu\u00b7gen", "Hand", "mir", "fer\u00b7ner", "was", "zu", "weyhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mu\u00df gleich Apollo noch die St\u00fcmperey belachen,", "tokens": ["Mu\u00df", "gleich", "A\u00b7pol\u00b7lo", "noch", "die", "St\u00fcm\u00b7pe\u00b7rey", "be\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So kan der Einflu\u00df doch mir k\u00fcnfftig g\u00fcnstig seyn.", "tokens": ["So", "kan", "der", "Ein\u00b7flu\u00df", "doch", "mir", "k\u00fcnff\u00b7tig", "g\u00fcns\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PPER", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Pflantze wird zum Haupt, aus Rei\u00dfern werden B\u00e4ume,", "tokens": ["Die", "Pflant\u00b7ze", "wird", "zum", "Haupt", ",", "aus", "Rei\u00b7\u00dfern", "wer\u00b7den", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,", "APPR", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Man trifft doch durch Gedult und Zeit zuletzt das Ziel.", "tokens": ["Man", "trifft", "doch", "durch", "Ge\u00b7dult", "und", "Zeit", "zu\u00b7letzt", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Musen sehen selbst, da\u00df ich gar nichts vers\u00e4ume,", "tokens": ["Die", "Mu\u00b7sen", "se\u00b7hen", "selbst", ",", "da\u00df", "ich", "gar", "nichts", "ver\u00b7s\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was diese edle Kunst von Sch\u00fclern haben will.", "tokens": ["Was", "die\u00b7se", "ed\u00b7le", "Kunst", "von", "Sch\u00fc\u00b7lern", "ha\u00b7ben", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJA", "NN", "APPR", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Betritt dereinst dein Fu\u00df noch einmahl unsre Linden,", "tokens": ["Be\u00b7tritt", "de\u00b7reinst", "dein", "Fu\u00df", "noch", "ein\u00b7mahl", "uns\u00b7re", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "PPOSAT", "NE", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "So weist du schon das Hau\u00df, wo ich zugegen bin;", "tokens": ["So", "weist", "du", "schon", "das", "Hau\u00df", ",", "wo", "ich", "zu\u00b7ge\u00b7gen", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du wirst mich sicher noch als deine Freundin finden,", "tokens": ["Du", "wirst", "mich", "si\u00b7cher", "noch", "als", "dei\u00b7ne", "Freun\u00b7din", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "KOKOM", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sie nennet, wie bekannt, sich die von Z--", "tokens": ["Sie", "nen\u00b7net", ",", "wie", "be\u00b7kannt", ",", "sich", "die", "von", "Z\u00b7"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "$,", "PRF", "ART", "APPR", "TRUNC"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}}}}}