{"textgrid.poem.33906": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. B\u00f6ser Ort", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich ganz verloren,", "tokens": ["Ich", "ha\u00b7be", "mich", "ganz", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie ist hier Alles stumm!", "tokens": ["Wie", "ist", "hier", "Al\u00b7les", "stumm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es dr\u00e4ngen die schwarzen B\u00e4ume", "tokens": ["Es", "dr\u00e4n\u00b7gen", "die", "schwar\u00b7zen", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich t\u00fcckisch um mich herum.", "tokens": ["Sich", "t\u00fc\u00b7ckisch", "um", "mich", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie wollen mich nicht mehr lassen,", "tokens": ["Sie", "wol\u00b7len", "mich", "nicht", "mehr", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich aber treibt es fort,", "tokens": ["Mich", "a\u00b7ber", "treibt", "es", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man spricht von b\u00f6sen Orten,", "tokens": ["Man", "spricht", "von", "b\u00f6\u00b7sen", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df ist ein b\u00f6ser Ort!", "tokens": ["Die\u00df", "ist", "ein", "b\u00f6\u00b7ser", "Ort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier ist schon B\u00f6ses geschehen,", "tokens": ["Hier", "ist", "schon", "B\u00f6\u00b7ses", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hier mu\u00df mehr gescheh'n,", "tokens": ["Und", "hier", "mu\u00df", "mehr", "ge\u00b7scheh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird's nicht an ihm begangen,", "tokens": ["Wird's", "nicht", "an", "ihm", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mu\u00df es der Mensch begeh'n.", "tokens": ["So", "mu\u00df", "es", "der", "Mensch", "be\u00b7geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVFIN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Blumen, so hoch sie wachsen,", "tokens": ["Die", "Blu\u00b7men", ",", "so", "hoch", "sie", "wach\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind bla\u00df hier, wie der Tod,", "tokens": ["Sind", "bla\u00df", "hier", ",", "wie", "der", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur Eine in der Mitte", "tokens": ["Nur", "Ei\u00b7ne", "in", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht da in dunklem Roth.", "tokens": ["Steht", "da", "in", "dunk\u00b7lem", "Roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die hat es nicht von der Sonne,", "tokens": ["Die", "hat", "es", "nicht", "von", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nie traf sie deren Glut,", "tokens": ["Nie", "traf", "sie", "de\u00b7ren", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat es von der Erde,", "tokens": ["Sie", "hat", "es", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und die trank Menschenblut!", "tokens": ["Und", "die", "trank", "Men\u00b7schen\u00b7blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du sollst dich nicht l\u00e4nger br\u00fcsten", "tokens": ["Du", "sollst", "dich", "nicht", "l\u00e4n\u00b7ger", "br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf meines Bruders Grab", "tokens": ["Auf", "mei\u00b7nes", "Bru\u00b7ders", "Grab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deinem gestohl'nen Purpur,", "tokens": ["In", "dei\u00b7nem", "ge\u00b7stohl'\u00b7nen", "Pur\u00b7pur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ich r\u00e4ch' ihn und breche dich ab!", "tokens": ["Ich", "r\u00e4ch'", "ihn", "und", "bre\u00b7che", "dich", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Dort liegt sie zu meinen F\u00fc\u00dfen!", "tokens": ["Dort", "liegt", "sie", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da schwingt ein Vogel sich,", "tokens": ["Da", "schwingt", "ein", "Vo\u00b7gel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Setzt sich mir gegen\u00fcber", "tokens": ["Setzt", "sich", "mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPER", "APPR"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und pfeift und verspottet mich.", "tokens": ["Und", "pfeift", "und", "ver\u00b7spot\u00b7tet", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbjetzt l\u00e4\u00dft der Ort dich weiter,", "tokens": ["\u00bb", "jetzt", "l\u00e4\u00dft", "der", "Ort", "dich", "wei\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ihm sein Recht geschah,", "tokens": ["Da", "ihm", "sein", "Recht", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast die Blume get\u00f6dtet,", "tokens": ["Du", "hast", "die", "Blu\u00b7me", "ge\u00b7t\u00f6d\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es war nichts Anders da.\u00ab", "tokens": ["Es", "war", "nichts", "An\u00b7ders", "da", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}