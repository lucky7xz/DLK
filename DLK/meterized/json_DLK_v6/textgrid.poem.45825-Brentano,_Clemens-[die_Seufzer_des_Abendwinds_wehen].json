{"textgrid.poem.45825": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Seufzer des Abendwinds wehen]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Seufzer des Abendwinds wehen", "tokens": ["Die", "Seuf\u00b7zer", "des", "Ab\u00b7end\u00b7winds", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So jammernd und bittend im Turm;", "tokens": ["So", "jam\u00b7mernd", "und", "bit\u00b7tend", "im", "Turm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wohl h\u00f6r' ich um Rettung dich flehen,", "tokens": ["Wohl", "h\u00f6r'", "ich", "um", "Ret\u00b7tung", "dich", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du ringst mit den Wogen, versinkest im Sturm.", "tokens": ["Du", "ringst", "mit", "den", "Wo\u00b7gen", ",", "ver\u00b7sin\u00b7kest", "im", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Ich seh' dich am Ufer; es wallet", "tokens": ["Ich", "seh'", "dich", "am", "U\u00b7fer", ";", "es", "wal\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein traurendes Irrlicht einher.", "tokens": ["Ein", "trau\u00b7ren\u00b7des", "Irr\u00b7licht", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein liebendes Rufen erschallet,", "tokens": ["Mein", "lie\u00b7ben\u00b7des", "Ru\u00b7fen", "er\u00b7schal\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du h\u00f6rest, du liebest, du st\u00fcrzest ins Meer.", "tokens": ["Du", "h\u00f6\u00b7rest", ",", "du", "lie\u00b7best", ",", "du", "st\u00fcr\u00b7zest", "ins", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ich lieb' und ich st\u00fcrze verwegen", "tokens": ["Ich", "lieb'", "und", "ich", "st\u00fcr\u00b7ze", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dir nach in die Wogen hinab,", "tokens": ["Dir", "nach", "in", "die", "Wo\u00b7gen", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich komme dir sterbend entgegen,", "tokens": ["Ich", "kom\u00b7me", "dir", "ster\u00b7bend", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich ringe, du sinkest, ich teile dein Grab.", "tokens": ["Ich", "rin\u00b7ge", ",", "du", "sin\u00b7kest", ",", "ich", "tei\u00b7le", "dein", "Grab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Doch st\u00fcrzt man den St\u00fcrmen des Lebens", "tokens": ["Doch", "st\u00fcrzt", "man", "den", "St\u00fcr\u00b7men", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von neuem mich Armen nun zu.", "tokens": ["Von", "neu\u00b7em", "mich", "Ar\u00b7men", "nun", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PPER", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich sinke; ich ringe vergebens,", "tokens": ["Ich", "sin\u00b7ke", ";", "ich", "rin\u00b7ge", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ach nur in dem Abgrund des Todes ist Ruh'.", "tokens": ["Ach", "nur", "in", "dem", "Ab\u00b7grund", "des", "To\u00b7des", "ist", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Da schwinden die ewigen Fernen,", "tokens": ["Da", "schwin\u00b7den", "die", "e\u00b7wi\u00b7gen", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da endet kein Leben mit dir.", "tokens": ["Da", "en\u00b7det", "kein", "Le\u00b7ben", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich kenn' deinen Blick in den Sternen,", "tokens": ["Ich", "kenn'", "dei\u00b7nen", "Blick", "in", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ach sieh nicht so traurig, hab' Mitleid mit mir!", "tokens": ["Ach", "sieh", "nicht", "so", "trau\u00b7rig", ",", "hab'", "Mit\u00b7leid", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,", "VAFIN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}