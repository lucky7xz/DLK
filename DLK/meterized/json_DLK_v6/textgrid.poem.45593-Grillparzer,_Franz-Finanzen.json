{"textgrid.poem.45593": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Finanzen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sie sich in ihrem Hauptbuch geirrt,", "tokens": ["Wenn", "sie", "sich", "in", "ih\u00b7rem", "Haupt\u00b7buch", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wars ein Verwechseln von Bl\u00e4ttern und B\u00f6gen:", "tokens": ["Wars", "ein", "Ver\u00b7wech\u00b7seln", "von", "Bl\u00e4t\u00b7tern", "und", "B\u00f6\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Sie haben Soll und Haben zusammenaddiert", "tokens": ["Sie", "ha\u00b7ben", "Soll", "und", "Ha\u00b7ben", "zu\u00b7sam\u00b7me\u00b7nad\u00b7diert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAINF", "VMFIN", "KON", "NN", "VVPP"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und geglaubt, das sei ihr Verm\u00f6gen.", "tokens": ["Und", "ge\u00b7glaubt", ",", "das", "sei", "ihr", "Ver\u00b7m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}