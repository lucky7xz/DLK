{"textgrid.poem.54328": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erbrich dies schmerzensvolle Blat,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erbrich dies schmerzensvolle Blat,", "tokens": ["Er\u00b7brich", "dies", "schmer\u00b7zens\u00b7vol\u00b7le", "Blat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ich zu deinem Trost geschrieben.", "tokens": ["Das", "ich", "zu", "dei\u00b7nem", "Trost", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So lange hat kein Klagen statt", "tokens": ["So", "lan\u00b7ge", "hat", "kein", "Kla\u00b7gen", "statt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PIAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Halt ein mit Seufzen und Betr\u00fcben.", "tokens": ["Halt", "ein", "mit", "Seuf\u00b7zen", "und", "Be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch eh du alle Zeilen liest,", "tokens": ["Doch", "eh", "du", "al\u00b7le", "Zei\u00b7len", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So will ich Dir im voraus sagen:", "tokens": ["So", "will", "ich", "Dir", "im", "vo\u00b7raus", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPRART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df deine Freundin willig ist", "tokens": ["Da\u00df", "dei\u00b7ne", "Freun\u00b7din", "wil\u00b7lig", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die halbe Last mit Dir zu tragen.", "tokens": ["Die", "hal\u00b7be", "Last", "mit", "Dir", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm, leg dein Haupt in meinen Schoo\u00df.", "tokens": ["Komm", ",", "leg", "dein", "Haupt", "in", "mei\u00b7nen", "Schoo\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Zuspruch soll die Wunde heilen:", "tokens": ["Mein", "Zu\u00b7spruch", "soll", "die", "Wun\u00b7de", "hei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4r dein Jammer noch so gro\u00df,", "tokens": ["Und", "w\u00e4r", "dein", "Jam\u00b7mer", "noch", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird die Empfindung sich zertheilen.", "tokens": ["Wird", "die", "Emp\u00b7fin\u00b7dung", "sich", "zer\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich kenne Dich und deinen Sinn", "tokens": ["Ich", "ken\u00b7ne", "Dich", "und", "dei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein zartes Herz, die edle Seele;", "tokens": ["Dein", "zar\u00b7tes", "Herz", ",", "die", "ed\u00b7le", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vergib, wenn ich verwegen bin,", "tokens": ["Ver\u00b7gib", ",", "wenn", "ich", "ver\u00b7we\u00b7gen", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und deine Thr\u00e4nen einzeln z\u00e4hle.", "tokens": ["Und", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "ein\u00b7zeln", "z\u00e4h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo denk ich hin! was n\u00fctzte mir", "tokens": ["Wo", "denk", "ich", "hin", "!", "was", "n\u00fctz\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menge deiner herben Z\u00e4hren", "tokens": ["Die", "Men\u00b7ge", "dei\u00b7ner", "her\u00b7ben", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! meine M\u00fche w\u00fcrde Dir", "tokens": ["Ach", "!", "mei\u00b7ne", "M\u00fc\u00b7he", "w\u00fcr\u00b7de", "Dir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dadurch gar wenig Trost gewehren.", "tokens": ["Da\u00b7durch", "gar", "we\u00b7nig", "Trost", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich gehe in dein Klagehaus,", "tokens": ["Ich", "ge\u00b7he", "in", "dein", "Kla\u00b7ge\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Ehgemahl zugleich zu sprechen.", "tokens": ["Dein", "Eh\u00b7ge\u00b7mahl", "zu\u00b7gleich", "zu", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da sieht es wahrlich furchtbar aus:", "tokens": ["Da", "sieht", "es", "wahr\u00b7lich", "furcht\u00b7bar", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Herze mu\u00df vor Mitleid brechen.", "tokens": ["Das", "Her\u00b7ze", "mu\u00df", "vor", "Mit\u00b7leid", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr Liebsten, ich will Zeuge seyn", "tokens": ["Ihr", "Liebs\u00b7ten", ",", "ich", "will", "Zeu\u00b7ge", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "NN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seufzt, weinet beyde um die Wette;", "tokens": ["Seufzt", ",", "wei\u00b7net", "bey\u00b7de", "um", "die", "Wet\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Klagt, ich stimm mit Euch \u00fcberein;", "tokens": ["Klagt", ",", "ich", "stimm", "mit", "Euch", "\u00fc\u00b7be\u00b7re\u00b7in", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ach! wer noch eine Tochter h\u00e4tte!", "tokens": ["Ach", "!", "wer", "noch", "ei\u00b7ne", "Toch\u00b7ter", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch h\u00f6rt mich auch vor andern an:", "tokens": ["Doch", "h\u00f6rt", "mich", "auch", "vor", "an\u00b7dern", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr k\u00f6nnt Euch beyde v\u00f6llig fassen", "tokens": ["Ihr", "k\u00f6nnt", "Euch", "bey\u00b7de", "v\u00f6l\u00b7lig", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da Euch zur Tr\u00f6stung dienen kan", "tokens": ["Da", "Euch", "zur", "Tr\u00f6s\u00b7tung", "die\u00b7nen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df sie Euch Kinder hinterlassen.", "tokens": ["Da\u00df", "sie", "Euch", "Kin\u00b7der", "hin\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Mutter Geist und Trefflichkeit,", "tokens": ["Der", "Mut\u00b7ter", "Geist", "und", "Treff\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Anmuth, und so seltne Gaben,", "tokens": ["Und", "An\u00b7muth", ",", "und", "so", "selt\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verstand und wahre Redlichkeit,", "tokens": ["Ver\u00b7stand", "und", "wah\u00b7re", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ists was sie geerbet haben.", "tokens": ["Das", "ists", "was", "sie", "ge\u00b7er\u00b7bet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PWS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die zeigen Euch ihr Ebenbild", "tokens": ["Die", "zei\u00b7gen", "Euch", "ihr", "E\u00b7ben\u00b7bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Asch und Moder sie begehren,", "tokens": ["Wenn", "Asch", "und", "Mo\u00b7der", "sie", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hebt auf den Flor der Euch umh\u00fcllt;", "tokens": ["Hebt", "auf", "den", "Flor", "der", "Euch", "um\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr k\u00f6nnt die Todte anders ehren.", "tokens": ["Ihr", "k\u00f6nnt", "die", "Tod\u00b7te", "an\u00b7ders", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schreibt ihr noch auf den Leichenstein,", "tokens": ["Schreibt", "ihr", "noch", "auf", "den", "Lei\u00b7chen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lieb Euch dieses Kind gewesen.", "tokens": ["Wie", "lieb", "Euch", "die\u00b7ses", "Kind", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PDAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sollte dies zu wenig seyn,", "tokens": ["Und", "soll\u00b7te", "dies", "zu", "we\u00b7nig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PDS", "PTKA", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00dft die Welt noch dieses lesen:", "tokens": ["So", "la\u00dft", "die", "Welt", "noch", "die\u00b7ses", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier ruht ein Engel von Gestalt,", "tokens": ["Hier", "ruht", "ein", "En\u00b7gel", "von", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schmuck vom weiblichen Geschlechte.", "tokens": ["Der", "Schmuck", "vom", "weib\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Tod gebrauchte hier Gewalt,", "tokens": ["Der", "Tod", "ge\u00b7brauch\u00b7te", "hier", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum wichen Alter, Ahnen, Rechte.", "tokens": ["Drum", "wi\u00b7chen", "Al\u00b7ter", ",", "Ah\u00b7nen", ",", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}