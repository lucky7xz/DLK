{"textgrid.poem.48996": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "An Theone", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im schnellen Flug der Horen", "tokens": ["Im", "schnel\u00b7len", "Flug", "der", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwand deiner N\u00e4he Gl\u00fcck.", "tokens": ["Schwand", "dei\u00b7ner", "N\u00e4\u00b7he", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was wir in dir verloren,", "tokens": ["Was", "wir", "in", "dir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bringt uns kein Wunsch zur\u00fcck.", "tokens": ["Bringt", "uns", "kein", "Wunsch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie hofften wir im Lenze", "tokens": ["Wie", "hoff\u00b7ten", "wir", "im", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Bruders uns zu freu'n,", "tokens": ["Des", "Bru\u00b7ders", "uns", "zu", "freu'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und heitre Blumenkr\u00e4nze", "tokens": ["Und", "heit\u00b7re", "Blu\u00b7men\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf deinen Pfad zu streu'n!", "tokens": ["Auf", "dei\u00b7nen", "Pfad", "zu", "streu'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wie des Traums Entz\u00fccken", "tokens": ["Doch", "wie", "des", "Traums", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beim Strahl des Morgens flieht,", "tokens": ["Beim", "Strahl", "des", "Mor\u00b7gens", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entschwebst du unsern Blicken,", "tokens": ["Ent\u00b7schwebst", "du", "un\u00b7sern", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da kaum der Fr\u00fchling gl\u00fcht.", "tokens": ["Da", "kaum", "der", "Fr\u00fch\u00b7ling", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dir bl\u00fcht, wo du auch weilest,", "tokens": ["Dir", "bl\u00fcht", ",", "wo", "du", "auch", "wei\u00b7lest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rings um dich Zauberflur;", "tokens": ["Rings", "um", "dich", "Zau\u00b7ber\u00b7flur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns wird, da du enteilest,", "tokens": ["Uns", "wird", ",", "da", "du", "ent\u00b7ei\u00b7lest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entzaubert die Natur.", "tokens": ["Ent\u00b7zau\u00b7bert", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zur Treue still verbunden", "tokens": ["Zur", "Treu\u00b7e", "still", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bleibt unser Kreis umschwebt", "tokens": ["Bleibt", "un\u00b7ser", "Kreis", "um\u00b7schwebt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Schatten sel'ger Stunden,", "tokens": ["Vom", "Schat\u00b7ten", "sel'\u00b7ger", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die einst dein Hauch belebt.", "tokens": ["Die", "einst", "dein", "Hauch", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Oft wird im trunknen W\u00e4hnen,", "tokens": ["Oft", "wird", "im", "trunk\u00b7nen", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn Trennungstage flieh'n,", "tokens": ["Wenn", "Tren\u00b7nungs\u00b7ta\u00b7ge", "flieh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Geist in leisen T\u00f6nen", "tokens": ["Der", "Geist", "in", "lei\u00b7sen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu dir hin\u00fcber zieh'n,", "tokens": ["Zu", "dir", "hin\u00b7\u00fc\u00b7ber", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Oft wird am Wasserfalle,", "tokens": ["Oft", "wird", "am", "Was\u00b7ser\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den du so gern besucht,", "tokens": ["Den", "du", "so", "gern", "be\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im spiegelnden Krystalle", "tokens": ["Im", "spie\u00b7geln\u00b7den", "Krys\u00b7tal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von uns dein Bild gesucht.", "tokens": ["Von", "uns", "dein", "Bild", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wo Buchen sanft umd\u00fcstern", "tokens": ["Wo", "Bu\u00b7chen", "sanft", "um\u00b7d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Felsen W\u00e4nde Moos,", "tokens": ["Der", "Fel\u00b7sen", "W\u00e4n\u00b7de", "Moos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entsteigt dann unter Fl\u00fcstern", "tokens": ["Ent\u00b7steigt", "dann", "un\u00b7ter", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Nam' der Wellen Scho\u00df.", "tokens": ["Dein", "Nam'", "der", "Wel\u00b7len", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}