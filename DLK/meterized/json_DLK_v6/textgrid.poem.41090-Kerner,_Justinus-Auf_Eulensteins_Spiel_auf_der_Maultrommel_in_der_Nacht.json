{"textgrid.poem.41090": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf Eulensteins Spiel auf der Maultrommel in der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt von Bienen, was ich h\u00f6re?", "tokens": ["Kommt", "von", "Bie\u00b7nen", ",", "was", "ich", "h\u00f6\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00e4chtlich schw\u00e4rmen Bienen nicht!", "tokens": ["N\u00e4cht\u00b7lich", "schw\u00e4r\u00b7men", "Bie\u00b7nen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha! nun t\u00f6nt's wie Geisterch\u00f6re", "tokens": ["Ha", "!", "nun", "t\u00f6nt's", "wie", "Geis\u00b7ter\u00b7ch\u00f6\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Harter Sylphen leis und licht;", "tokens": ["Har\u00b7ter", "Syl\u00b7phen", "leis", "und", "licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lauter jetzt, wie Harfen klingen,", "tokens": ["Lau\u00b7ter", "jetzt", ",", "wie", "Har\u00b7fen", "klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sanft ber\u00fchrt von Windes Schwingen.", "tokens": ["Sanft", "be\u00b7r\u00fchrt", "von", "Win\u00b7des", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und aus diesen T\u00f6nen heben", "tokens": ["Und", "aus", "die\u00b7sen", "T\u00f6\u00b7nen", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sich Gestalten zart und klar,", "tokens": ["Sich", "Ge\u00b7stal\u00b7ten", "zart", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterne, Blumen seh' ich schweben,", "tokens": ["Ster\u00b7ne", ",", "Blu\u00b7men", "seh'", "ich", "schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zauberzeichen wunderbar.", "tokens": ["Zaub\u00b7er\u00b7zei\u00b7chen", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaffet Licht, auf da\u00df wir finden,", "tokens": ["Schaf\u00b7fet", "Licht", ",", "auf", "da\u00df", "wir", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "APPR", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welch ein Zauber uns will binden.", "tokens": ["Welch", "ein", "Zau\u00b7ber", "uns", "will", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha! es ist mit seinem Eisen", "tokens": ["Ha", "!", "es", "ist", "mit", "sei\u00b7nem", "Ei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eulenstein, der gute Geist,", "tokens": ["Eu\u00b7len\u00b7stein", ",", "der", "gu\u00b7te", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der durch \u00fcberird'sche Weisen", "tokens": ["Der", "durch", "\u00fc\u00b7ber\u00b7ird'\u00b7sche", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns ins Land der Geister rei\u00dft.", "tokens": ["Uns", "ins", "Land", "der", "Geis\u00b7ter", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch er schweigt, und langsam wieder", "tokens": ["Doch", "er", "schweigt", ",", "und", "lang\u00b7sam", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sinken wir zur Erde nieder.", "tokens": ["Sin\u00b7ken", "wir", "zur", "Er\u00b7de", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}