{"dta.poem.6374": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zeigt die Erfahrung uns nicht ferner an,", "tokens": ["Zeigt", "die", "Er\u00b7fah\u00b7rung", "uns", "nicht", "fer\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, wenn ein C\u00f6rper lau, er einen andern doch,", "tokens": ["Da\u00df", ",", "wenn", "ein", "C\u00f6r\u00b7per", "lau", ",", "er", "ei\u00b7nen", "an\u00b7dern", "doch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "ADJD", "$,", "PPER", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mehr\u2019re Hitze hat, erk\u00e4lten kan.", "tokens": ["Der", "mehr'\u00b7re", "Hit\u00b7ze", "hat", ",", "er\u00b7k\u00e4l\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Regeln der Bewegung dauren noch.", "tokens": ["Die", "Re\u00b7geln", "der", "Be\u00b7we\u00b7gung", "dau\u00b7ren", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn zeiget uns die Sonn\u2019 in heisser Mittags-Zeit", "tokens": ["Denn", "zei\u00b7get", "uns", "die", "Sonn'", "in", "heis\u00b7ser", "Mit\u00b7tags\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der hellen Strahlen schw\u00fcle Blitze;", "tokens": ["Der", "hel\u00b7len", "Strah\u00b7len", "schw\u00fc\u00b7le", "Blit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So f\u00fchlet unsre Brust, die voller Hitze,", "tokens": ["So", "f\u00fch\u00b7let", "uns\u00b7re", "Brust", ",", "die", "vol\u00b7ler", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der K\u00e4lte Hefftigkeit,", "tokens": ["Der", "K\u00e4l\u00b7te", "Heff\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wenn sie ins Wasser sinckt; das doch die Hand", "tokens": ["Wenn", "sie", "ins", "Was\u00b7ser", "sinckt", ";", "das", "doch", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$.", "PDS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vorhin gantz laulich fand.", "tokens": ["Vor\u00b7hin", "gantz", "lau\u00b7lich", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}