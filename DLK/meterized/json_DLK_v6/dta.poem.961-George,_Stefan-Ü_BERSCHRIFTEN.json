{"dta.poem.961": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "\u00dc BERSCHRIFTEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Zu meinen tr&#228;umen floh ich vor dem volke               ", "tokens": ["Zu", "mei\u00b7nen", "tr", "&#228;", "u\u00b7men", "floh", "ich", "vor", "dem", "vol\u00b7ke"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "VVINF", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit heissen h&#228;nden tastend nach der weite               ", "tokens": ["Mit", "heis\u00b7sen", "h", "&#228;", "nden", "tas\u00b7tend", "nach", "der", "wei\u00b7te"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "$(", "VVFIN", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sprach allein und rein mit stern und wolke               ", "tokens": ["Und", "sprach", "al\u00b7lein", "und", "rein", "mit", "stern", "und", "wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADJD", "APPR", "VVINF", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von meinem ersten jugendlichen streite             ", "tokens": ["Von", "mei\u00b7nem", "ers\u00b7ten", "ju\u00b7gend\u00b7li\u00b7chen", "strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die blumen hergeholt aus reichem leben               ", "tokens": ["Die", "blu\u00b7men", "her\u00b7ge\u00b7holt", "aus", "rei\u00b7chem", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umflocht ich frei und stolz an goldnen kreisen               ", "tokens": ["Um\u00b7flocht", "ich", "frei", "und", "stolz", "an", "gold\u00b7nen", "krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem fern im licht geheiligten efeben               ", "tokens": ["Dem", "fern", "im", "licht", "ge\u00b7hei\u00b7lig\u00b7ten", "e\u00b7fe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Verklang sein schmerz in feierlichen weisen             ", "tokens": ["Ver\u00b7klang", "sein", "schmerz", "in", "fei\u00b7er\u00b7li\u00b7chen", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu g\u00f6tterthalen blinkenden M&#228;andern               ", "tokens": ["Zu", "g\u00f6t\u00b7ter\u00b7tha\u00b7len", "blin\u00b7ken\u00b7den", "M", "&#228;", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "NE", "$(", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich liess in st&#228;tten innig hoher sitten               ", "tokens": ["Ich", "liess", "in", "st", "&#228;", "tten", "in\u00b7nig", "ho\u00b7her", "sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "$(", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in den s&#252;den meine seele wandern               ", "tokens": ["Und", "in", "den", "s", "&#252;", "den", "mei\u00b7ne", "see\u00b7le", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$(", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo sie gekr\u00f6nt den martertod erlitten             ", "tokens": ["Wo", "sie", "ge\u00b7kr\u00f6nt", "den", "mar\u00b7ter\u00b7tod", "er\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und heut geschieht es nur aus Einem grunde               ", "tokens": ["Und", "heut", "ge\u00b7schieht", "es", "nur", "aus", "Ei\u00b7nem", "grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich zum sang das lange schweigen breche               ", "tokens": ["Wenn", "ich", "zum", "sang", "das", "lan\u00b7ge", "schwei\u00b7gen", "bre\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "VVFIN", "ART", "ADJA", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dass wir uns freuen auf die zwielichtstunde               ", "tokens": ["Dass", "wir", "uns", "freu\u00b7en", "auf", "die", "zwie\u00b7lichts\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine d&#252;stre schwester also spreche:             ", "tokens": ["Und", "mei\u00b7ne", "d", "&#252;", "stre", "schwes\u00b7ter", "al\u00b7so", "spre\u00b7che", ":"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "XY", "$(", "VVFIN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Soll ich noch leben darf ich nicht vermissen               ", "tokens": ["Soll", "ich", "noch", "le\u00b7ben", "darf", "ich", "nicht", "ver\u00b7mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den trank aus deinen klingenden pokalen               ", "tokens": ["Den", "trank", "aus", "dei\u00b7nen", "klin\u00b7gen\u00b7den", "po\u00b7ka\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und f&#252;hrer sind in meinen finsternissen               ", "tokens": ["Und", "f", "&#252;", "hrer", "sind", "in", "mei\u00b7nen", "fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "$(", "ADJD", "VAFIN", "APPR", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die lichter die aus deinen wunden strahlen.             ", "tokens": ["Die", "lich\u00b7ter", "die", "aus", "dei\u00b7nen", "wun\u00b7den", "strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}