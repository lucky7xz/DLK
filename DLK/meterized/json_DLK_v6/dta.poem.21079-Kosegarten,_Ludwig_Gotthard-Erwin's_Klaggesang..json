{"dta.poem.21079": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Erwin's Klaggesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehe, wie die Fluren trauern!               ", "tokens": ["Sie\u00b7he", ",", "wie", "die", "Flu\u00b7ren", "trau\u00b7ern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie der Sonne goldnes Licht", "tokens": ["Wie", "der", "Son\u00b7ne", "gold\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Flor von Regenschauern", "tokens": ["Durch", "den", "Flor", "von", "Re\u00b7gen\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fchsam nur und weinend bricht!", "tokens": ["M\u00fch\u00b7sam", "nur", "und", "wei\u00b7nend", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Florens holde Kinder neigen", "tokens": ["Flo\u00b7rens", "hol\u00b7de", "Kin\u00b7der", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre H\u00e4upter krank und matt.", "tokens": ["Ih\u00b7re", "H\u00e4up\u00b7ter", "krank", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den leisgeregten Zweigen", "tokens": ["Von", "den", "leis\u00b7ge\u00b7reg\u00b7ten", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Taumelt raschelnd Blatt auf Blatt.", "tokens": ["Tau\u00b7melt", "ra\u00b7schelnd", "Blatt", "auf", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ausgestorben sind die W\u00e4lder", "tokens": ["Aus\u00b7ge\u00b7stor\u00b7ben", "sind", "die", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Einem Wahlplatz gleicht die Flur,", "tokens": ["Ei\u00b7nem", "Wahl\u00b7platz", "gleicht", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Einer Maalstatt Wies' und Felder,", "tokens": ["Ei\u00b7ner", "Maal\u00b7statt", "Wies'", "und", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Einer Wittwe die Natur.", "tokens": ["Ei\u00b7ner", "Witt\u00b7we", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Trauert immer, holde Auen,", "tokens": ["Trau\u00b7ert", "im\u00b7mer", ",", "hol\u00b7de", "Au\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traute Fluren, trauert nur!", "tokens": ["Trau\u00b7te", "Flu\u00b7ren", ",", "trau\u00b7ert", "nur", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fcllet euch in Nacht und Grauen!", "tokens": ["H\u00fcl\u00b7let", "euch", "in", "Nacht", "und", "Grau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traure, traure nur, Natur.", "tokens": ["Trau\u00b7re", ",", "trau\u00b7re", "nur", ",", "Na\u00b7tur", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch dein Freund und Liebling trauert,", "tokens": ["Auch", "dein", "Freund", "und", "Lieb\u00b7ling", "trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seines Geistes Licht ist hin;", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "Licht", "ist", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schimmerlose Nacht umschauert", "tokens": ["Schim\u00b7mer\u00b7lo\u00b7se", "Nacht", "um\u00b7schau\u00b7ert"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen gramumw\u00f6lkten Sinn.", "tokens": ["Sei\u00b7nen", "gra\u00b7mum\u00b7w\u00f6lk\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Leer ist seiner Pfeile K\u00f6cher,", "tokens": ["Leer", "ist", "sei\u00b7ner", "Pfei\u00b7le", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seines Bogens Sehn' erschlafft,", "tokens": ["Sei\u00b7nes", "Bo\u00b7gens", "Sehn'", "er\u00b7schlafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ausgesch\u00f6pft der goldne Becher,", "tokens": ["Aus\u00b7ge\u00b7sch\u00f6pft", "der", "gold\u00b7ne", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ausgel\u00f6scht des Herzens Kraft.", "tokens": ["Aus\u00b7ge\u00b7l\u00f6scht", "des", "Her\u00b7zens", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mit Recht wohl mag ich trauern.", "tokens": ["Und", "mit", "Recht", "wohl", "mag", "ich", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fern von ihrer Mutterflur,", "tokens": ["Fern", "von", "ih\u00b7rer", "Mut\u00b7ter\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Bezirke dumpfer Mauern,", "tokens": ["Im", "Be\u00b7zir\u00b7ke", "dum\u00b7pfer", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Gebiet der Unnatur,", "tokens": ["Im", "Ge\u00b7biet", "der", "Un\u00b7na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wandelt, die ich einzig meine,", "tokens": ["Wan\u00b7delt", ",", "die", "ich", "ein\u00b7zig", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mir Sinn und Seele f\u00fcllt,", "tokens": ["Die", "mir", "Sinn", "und", "See\u00b7le", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie, die Klare, sie die Reine,", "tokens": ["Sie", ",", "die", "Kla\u00b7re", ",", "sie", "die", "Rei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deren glanzumstrahltes Bild", "tokens": ["De\u00b7ren", "glan\u00b7zums\u00b7trahl\u00b7tes", "Bild"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Jenes Sch\u00f6n mir widerspiegelt,", "tokens": ["Je\u00b7nes", "Sch\u00f6n", "mir", "wi\u00b7der\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Das aus h\u00f6hern Sph\u00e4ren stammt,", "tokens": ["Das", "aus", "h\u00f6\u00b7hern", "Sph\u00e4\u00b7ren", "stammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Die zum Heros mich befl\u00fcgelt,", "tokens": ["Die", "zum", "He\u00b7ros", "mich", "be\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und zum Halbgott mich entflammt.", "tokens": ["Und", "zum", "Halb\u00b7gott", "mich", "ent\u00b7flammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, und taumelnd mit der Menge,", "tokens": ["Ach", ",", "und", "tau\u00b7melnd", "mit", "der", "Men\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die des Herzens Gl\u00fcck nicht kennt,", "tokens": ["Die", "des", "Her\u00b7zens", "Gl\u00fcck", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fortgerissen vom Gedr\u00e4nge,", "tokens": ["Fort\u00b7ge\u00b7ris\u00b7sen", "vom", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das nach eiteln Freuden rennt,", "tokens": ["Das", "nach", "ei\u00b7teln", "Freu\u00b7den", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der Thorheit Ring umrungen,", "tokens": ["Von", "der", "Thor\u00b7heit", "Ring", "um\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Moden Fluth umrollt,", "tokens": ["Von", "der", "Mo\u00b7den", "Fluth", "um\u00b7rollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwindelnd vor den Huldigungen,", "tokens": ["Schwin\u00b7delnd", "vor", "den", "Hul\u00b7di\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die die Schmeicheley ihr zollt;", "tokens": ["Die", "die", "Schmei\u00b7che\u00b7ley", "ihr", "zollt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Von der Lust Sirenent\u00f6nen", "tokens": ["Von", "der", "Lust", "Si\u00b7re\u00b7nen\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Eingewiegt in Lethargie,", "tokens": ["Ein\u00b7ge\u00b7wiegt", "in", "Le\u00b7thar\u00b7gie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denkst du wohl, o Preis der Sch\u00f6nen,", "tokens": ["Denkst", "du", "wohl", ",", "o", "Preis", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Deines schlichten Freundes nie.", "tokens": ["Dei\u00b7nes", "schlich\u00b7ten", "Freun\u00b7des", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oder widern deinem Herzen", "tokens": ["O\u00b7der", "wi\u00b7dern", "dei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schellenklang und Flatterglanz?", "tokens": ["Schel\u00b7len\u00b7klang", "und", "Flat\u00b7ter\u00b7glanz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gn\u00fcgt dir nicht der Saal voll Kerzen,", "tokens": ["Gn\u00fcgt", "dir", "nicht", "der", "Saal", "voll", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Voll Gesang und Spiel und Tanz?", "tokens": ["Voll", "Ge\u00b7sang", "und", "Spiel", "und", "Tanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sehnst du wohl' mal aus der Schw\u00fcle,", "tokens": ["Sehnst", "du", "wohl'", "mal", "aus", "der", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Enge dich zur\u00fcck", "tokens": ["Aus", "der", "En\u00b7ge", "dich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In die freye weite K\u00fchle,", "tokens": ["In", "die", "frey\u00b7e", "wei\u00b7te", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des D\u00f6rfleins l\u00e4ndlich Gl\u00fcck?", "tokens": ["In", "des", "D\u00f6r\u00b7fleins", "l\u00e4nd\u00b7lich", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In die kleebebl\u00fcmten Matten,", "tokens": ["In", "die", "kle\u00b7e\u00b7be\u00b7bl\u00fcm\u00b7ten", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "An den kalmusreichen Bach,", "tokens": ["An", "den", "kal\u00b7mus\u00b7rei\u00b7chen", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "In des Gartens Bl\u00fcthenschatten", "tokens": ["In", "des", "Gar\u00b7tens", "Bl\u00fc\u00b7then\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Unter deiner Lauben Dach?", "tokens": ["Un\u00b7ter", "dei\u