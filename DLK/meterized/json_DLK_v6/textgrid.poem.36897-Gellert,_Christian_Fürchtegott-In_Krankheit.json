{"textgrid.poem.36897": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "In Krankheit", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab in guten Stunden", "tokens": ["Ich", "hab", "in", "gu\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Lebens Gl\u00fcck empfunden;", "tokens": ["Des", "Le\u00b7bens", "Gl\u00fcck", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Freuden ohne Zahl:", "tokens": ["Und", "Freu\u00b7den", "oh\u00b7ne", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So will ich denn gelassen", "tokens": ["So", "will", "ich", "denn", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich auch in Leiden fassen;", "tokens": ["Mich", "auch", "in", "Lei\u00b7den", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Welch Leben hat nicht seine Qual?", "tokens": ["Welch", "Le\u00b7ben", "hat", "nicht", "sei\u00b7ne", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, Herr, ich bin ein S\u00fcnder,", "tokens": ["Ja", ",", "Herr", ",", "ich", "bin", "ein", "S\u00fcn\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stets strafst du gelinder,", "tokens": ["Und", "stets", "strafst", "du", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als es der Mensch verdient.", "tokens": ["Als", "es", "der", "Mensch", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Will ich, beschwert mit Schulden,", "tokens": ["Will", "ich", ",", "be\u00b7schwert", "mit", "Schul\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein zeitlich Weh erdulden,", "tokens": ["Kein", "zeit\u00b7lich", "Weh", "er\u00b7dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was doch zu meinem Besten dient?", "tokens": ["Was", "doch", "zu", "mei\u00b7nem", "Bes\u00b7ten", "dient", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir will ich mich ergeben,", "tokens": ["Dir", "will", "ich", "mich", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht meine Ruh, mein Leben,", "tokens": ["Nicht", "mei\u00b7ne", "Ruh", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr lieben, als den Herrn.", "tokens": ["Mehr", "lie\u00b7ben", ",", "als", "den", "Herrn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dir, Gott, will ich vertrauen,", "tokens": ["Dir", ",", "Gott", ",", "will", "ich", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und nicht auf Menschen bauen;", "tokens": ["Und", "nicht", "auf", "Men\u00b7schen", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du hilfst, und du errettest gern.", "tokens": ["Du", "hilfst", ",", "und", "du", "er\u00b7ret\u00b7test", "gern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df du mich Gnade finden,", "tokens": ["La\u00df", "du", "mich", "Gna\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich alle meine S\u00fcnden", "tokens": ["Mich", "al\u00b7le", "mei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erkennen und bereun.", "tokens": ["Er\u00b7ken\u00b7nen", "und", "be\u00b7re\u00b7un", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Itzt hat mein Geist noch Kr\u00e4fte;", "tokens": ["Itzt", "hat", "mein", "Geist", "noch", "Kr\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Heil la\u00df mein Gesch\u00e4fte,", "tokens": ["Sein", "Heil", "la\u00df", "mein", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Wort mir Trost und Leben sein.", "tokens": ["Dein", "Wort", "mir", "Trost", "und", "Le\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich in Christo sterbe:", "tokens": ["Wenn", "ich", "in", "Chris\u00b7to", "ster\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich des Himmels Erbe.", "tokens": ["Bin", "ich", "des", "Him\u00b7mels", "Er\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was schreckt mich Grab und Tod?", "tokens": ["Was", "schreckt", "mich", "Grab", "und", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auch auf des Todes Pfade", "tokens": ["Auch", "auf", "des", "To\u00b7des", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vertrau ich deiner Gnade;", "tokens": ["Ver\u00b7trau", "ich", "dei\u00b7ner", "Gna\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du, Herr, bist bei mir in der Not.", "tokens": ["Du", ",", "Herr", ",", "bist", "bei", "mir", "in", "der", "Not", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich will dem Kummer wehren,", "tokens": ["Ich", "will", "dem", "Kum\u00b7mer", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott durch Geduld verehren,", "tokens": ["Gott", "durch", "Ge\u00b7duld", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im Glauben zu ihm flehn.", "tokens": ["Im", "Glau\u00b7ben", "zu", "ihm", "flehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich will den Tod bedenken.", "tokens": ["Ich", "will", "den", "Tod", "be\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Herr wird alles lenken;", "tokens": ["Der", "Herr", "wird", "al\u00b7les", "len\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und was mir gut ist, wird geschehn.", "tokens": ["Und", "was", "mir", "gut", "ist", ",", "wird", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}