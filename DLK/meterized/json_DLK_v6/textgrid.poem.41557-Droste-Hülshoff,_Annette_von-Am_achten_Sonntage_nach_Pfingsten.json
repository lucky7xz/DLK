{"textgrid.poem.41557": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am achten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl sehr ersch\u00f6pft die Menge war", "tokens": ["Wohl", "sehr", "er\u00b7sch\u00f6pft", "die", "Men\u00b7ge", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wohl der Hunger nagte sehr,", "tokens": ["Und", "wohl", "der", "Hun\u00b7ger", "nag\u00b7te", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da nahmst du treulich ihrer wahr.", "tokens": ["Da", "nahmst", "du", "treu\u00b7lich", "ih\u00b7rer", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, f\u00fcr die Seele matt und leer,", "tokens": ["Ach", ",", "f\u00fcr", "die", "See\u00b7le", "matt", "und", "leer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach jahrelanger D\u00fcrr' und Schw\u00fcle,", "tokens": ["Nach", "jah\u00b7re\u00b7lan\u00b7ger", "D\u00fcrr'", "und", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du nicht einen Bissen auch,", "tokens": ["Hast", "du", "nicht", "ei\u00b7nen", "Bis\u00b7sen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht einen Labetrunk f\u00fcr sie,", "tokens": ["Nicht", "ei\u00b7nen", "La\u00b7be\u00b7trunk", "f\u00fcr", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht einen frischen Gnadenhauch,", "tokens": ["Nicht", "ei\u00b7nen", "fri\u00b7schen", "Gna\u00b7den\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der in der W\u00fcste Brand und M\u00fch'", "tokens": ["Der", "in", "der", "W\u00fcs\u00b7te", "Brand", "und", "M\u00fch'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das siedende Gehirne k\u00fchle?", "tokens": ["Das", "sie\u00b7den\u00b7de", "Ge\u00b7hir\u00b7ne", "k\u00fch\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Denn sieh, von ferne kam ich ja,", "tokens": ["Denn", "sieh", ",", "von", "fer\u00b7ne", "kam", "ich", "ja", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "APPR", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ob ich selber mich verbannt:", "tokens": ["Und", "ob", "ich", "sel\u00b7ber", "mich", "ver\u00b7bannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du stehst mir drum nicht minder nah.", "tokens": ["Du", "stehst", "mir", "drum", "nicht", "min\u00b7der", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer einmal sich zu dir gewandt", "tokens": ["Wer", "ein\u00b7mal", "sich", "zu", "dir", "ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PRF", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit neu erwachendem Gef\u00fchle,", "tokens": ["Mit", "neu", "er\u00b7wa\u00b7chen\u00b7dem", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer einmal aus des Treibers Joch", "tokens": ["Wer", "ein\u00b7mal", "aus", "des", "Trei\u00b7bers", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich fl\u00fcchtete zu deinem Ohr,", "tokens": ["Sich", "fl\u00fcch\u00b7te\u00b7te", "zu", "dei\u00b7nem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sei er so verk\u00fcmmert noch,", "tokens": ["Und", "sei", "er", "so", "ver\u00b7k\u00fcm\u00b7mert", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist so mild, h\u00e4ltst ihm nicht vor", "tokens": ["Du", "bist", "so", "mild", ",", "h\u00e4ltst", "ihm", "nicht", "vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sklavenpeitsche harte Schwiele.", "tokens": ["Der", "Skla\u00b7ven\u00b7peit\u00b7sche", "har\u00b7te", "Schwie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O rette mich, da\u00df nicht der Trug", "tokens": ["O", "ret\u00b7te", "mich", ",", "da\u00df", "nicht", "der", "Trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Hungers mich bezwingen kann,", "tokens": ["Des", "Hun\u00b7gers", "mich", "be\u00b7zwin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich nicht unter Wahnsinns Fluch", "tokens": ["Da\u00df", "ich", "nicht", "un\u00b7ter", "Wahn\u00b7sinns", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00e4nde strecke, greife an", "tokens": ["Die", "H\u00e4n\u00b7de", "stre\u00b7cke", ",", "grei\u00b7fe", "an"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die gift'ge Frucht am welken Stiele,", "tokens": ["Die", "gift'\u00b7ge", "Frucht", "am", "wel\u00b7ken", "Stie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So aus dem Paradiese trieb", "tokens": ["So", "aus", "dem", "Pa\u00b7ra\u00b7die\u00b7se", "trieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die Erkenntnis wird genannt!", "tokens": ["Und", "die", "Er\u00b7kennt\u00b7nis", "wird", "ge\u00b7nannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stiehlt sie das Leben wie ein Dieb:", "tokens": ["Stiehlt", "sie", "das", "Le\u00b7ben", "wie", "ein", "Dieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lockt sie doch des Gaumens Brand", "tokens": ["So", "lockt", "sie", "doch", "des", "Gau\u00b7mens", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit scheinbar frischen Saftes Spiele.", "tokens": ["Mit", "schein\u00b7bar", "fri\u00b7schen", "Saf\u00b7tes", "Spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach, nicht die W\u00fcste neben mir,", "tokens": ["Ach", ",", "nicht", "die", "W\u00fcs\u00b7te", "ne\u00b7ben", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die W\u00fcste mir im Busen liegt!", "tokens": ["Die", "W\u00fcs\u00b7te", "mir", "im", "Bu\u00b7sen", "liegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo find' ich denn, wo find' ich hier", "tokens": ["Wo", "find'", "ich", "denn", ",", "wo", "find'", "ich", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was meinen Hunger nicht betr\u00fcgt,", "tokens": ["Was", "mei\u00b7nen", "Hun\u00b7ger", "nicht", "be\u00b7tr\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was meine d\u00fcrre Kehle sp\u00fcle?", "tokens": ["Was", "mei\u00b7ne", "d\u00fcr\u00b7re", "Keh\u00b7le", "sp\u00fc\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So sprachen deine J\u00fcnger auch;", "tokens": ["So", "spra\u00b7chen", "dei\u00b7ne", "J\u00fcn\u00b7ger", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Gn\u00e4d'ger fandest doch ein Brod,", "tokens": ["Du", "Gn\u00e4d'\u00b7ger", "fan\u00b7dest", "doch", "ein", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sengenden Samumes Hauch", "tokens": ["Wo", "sen\u00b7gen\u00b7den", "Sa\u00b7mu\u00b7mes", "Hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Dir keine fromme \u00c4hre bot,", "tokens": ["Dir", "kei\u00b7ne", "from\u00b7me", "\u00c4h\u00b7re", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Sand und st\u00e4ubendes Gew\u00fchle.", "tokens": ["Nur", "Sand", "und", "st\u00e4u\u00b7ben\u00b7des", "Ge\u00b7w\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbda a\u00dfen sie und wurden satt,", "tokens": ["\u00bb", "da", "a\u00b7\u00dfen", "sie", "und", "wur\u00b7den", "satt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sammelten was \u00fcbrigblieb\u00ab;", "tokens": ["Und", "sam\u00b7mel\u00b7ten", "was", "\u00fcb\u00b7rig\u00b7blieb", "\u00ab", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PIS", "VVFIN", "$(", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "War keiner krank mehr, keiner matt", "tokens": ["War", "kei\u00b7ner", "krank", "mehr", ",", "kei\u00b7ner", "matt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIS", "ADJD", "ADV", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Genesne ward dir lieb,", "tokens": ["Und", "der", "Ge\u00b7nes\u00b7ne", "ward", "dir", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lieb als der Gesunden viele.", "tokens": ["So", "lieb", "als", "der", "Ge\u00b7sun\u00b7den", "vie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}