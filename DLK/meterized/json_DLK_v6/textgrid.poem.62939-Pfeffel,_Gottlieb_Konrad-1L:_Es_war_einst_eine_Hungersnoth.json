{"textgrid.poem.62939": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war einst eine Hungersnoth", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einst eine Hungersnoth", "tokens": ["Es", "war", "einst", "ei\u00b7ne", "Hun\u00b7gers\u00b7noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Thierreich, alles schrie nach Brod,", "tokens": ["Im", "Thier\u00b7reich", ",", "al\u00b7les", "schrie", "nach", "Brod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die V\u00f6gel fielen aus der Luft,", "tokens": ["Die", "V\u00f6\u00b7gel", "fie\u00b7len", "aus", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie M\u00fccken in die weite Gruft.", "tokens": ["Wie", "M\u00fc\u00b7cken", "in", "die", "wei\u00b7te", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Goldfasan schlich matt und schwer", "tokens": ["Ein", "Gold\u00b7fa\u00b7san", "schlich", "matt", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00e4chzend durch den Hain umher;", "tokens": ["Und", "\u00e4ch\u00b7zend", "durch", "den", "Hain", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm sah ein Specht von ferne zu", "tokens": ["Ihm", "sah", "ein", "Specht", "von", "fer\u00b7ne", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagte: Freund, was \u00e4chzest du?", "tokens": ["Und", "sag\u00b7te", ":", "Freund", ",", "was", "\u00e4ch\u00b7zest", "du", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "An deiner Stelle h\u00e4tt ich bald", "tokens": ["An", "dei\u00b7ner", "Stel\u00b7le", "h\u00e4tt", "ich", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den fettsten Tisch im ganzen Wald;", "tokens": ["Den", "fetts\u00b7ten", "Tisch", "im", "gan\u00b7zen", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verkaufe nur dein reiches Kleid,", "tokens": ["Ver\u00b7kau\u00b7fe", "nur", "dein", "rei\u00b7ches", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hast du Brod auf lange Zeit.", "tokens": ["So", "hast", "du", "Brod", "auf", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem Goldfasan gefiel der Rath,", "tokens": ["Dem", "Gold\u00b7fa\u00b7san", "ge\u00b7fiel", "der", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er setzte seinen ganzen Staat", "tokens": ["Er", "setz\u00b7te", "sei\u00b7nen", "gan\u00b7zen", "Staat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey einem alten Hamster ab,", "tokens": ["Bey", "ei\u00b7nem", "al\u00b7ten", "Hams\u00b7ter", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihm zwo Metzen Korn drum gab.", "tokens": ["Der", "ihm", "zwo", "Met\u00b7zen", "Korn", "drum", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "CARD", "NN", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun pflegt er sich bey F\u00fcrstenkost;", "tokens": ["Nun", "pflegt", "er", "sich", "bey", "F\u00fcrs\u00b7ten\u00b7kost", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch pl\u00f6tzlich fiel ein Winterfrost,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "fiel", "ein", "Win\u00b7ter\u00b7frost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und pl\u00f6tzlich war der arme Narr", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "war", "der", "ar\u00b7me", "Narr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am nakten Leibe blau und starr.", "tokens": ["Am", "nak\u00b7ten", "Lei\u00b7be", "blau", "und", "starr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O weh mir! sprach er nun zum Specht,", "tokens": ["O", "weh", "mir", "!", "sprach", "er", "nun", "zum", "Specht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein guter Freund, dein Rath war schlecht;", "tokens": ["Mein", "gu\u00b7ter", "Freund", ",", "dein", "Rath", "war", "schlecht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, man stirbt aus Hungersnoth,", "tokens": ["Ich", "wei\u00df", ",", "man", "stirbt", "aus", "Hun\u00b7gers\u00b7noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wer erfriert, ist gleichfalls todt.", "tokens": ["Doch", "wer", "er\u00b7friert", ",", "ist", "gleich\u00b7falls", "todt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}