{"textgrid.poem.37288": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es hatt' ein M\u00fcller eine M\u00fchl]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hatt' ein M\u00fcller eine M\u00fchl", "tokens": ["Es", "hatt'", "ein", "M\u00fcl\u00b7ler", "ei\u00b7ne", "M\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NE", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einem Wasser k\u00fchle;", "tokens": ["An", "ei\u00b7nem", "Was\u00b7ser", "k\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kamen h\u00fcbscher M\u00e4dchen viel", "tokens": ["Da", "ka\u00b7men", "h\u00fcb\u00b7scher", "M\u00e4d\u00b7chen", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu mahlen in der M\u00fchle.", "tokens": ["Zu", "mah\u00b7len", "in", "der", "M\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein armes M\u00e4del war darunt,", "tokens": ["Ein", "ar\u00b7mes", "M\u00e4\u00b7del", "war", "da\u00b7runt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Z\u00e4hlt sechzehn Jahre eben;", "tokens": ["Z\u00e4hlt", "sech\u00b7zehn", "Jah\u00b7re", "e\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allwo es ging, allwo es stund,", "tokens": ["All\u00b7wo", "es", "ging", ",", "all\u00b7wo", "es", "stund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der M\u00fcller stund daneben.", "tokens": ["Der", "M\u00fcl\u00b7ler", "stund", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er schenkt ein Ringlein ihr von Gold,", "tokens": ["Er", "schenkt", "ein", "Rin\u00b7glein", "ihr", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er in allen Ehren", "tokens": ["Da\u00df", "er", "in", "al\u00b7len", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie ewig immer lieben wollt;", "tokens": ["Sie", "e\u00b7wig", "im\u00b7mer", "lie\u00b7ben", "wollt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da lie\u00df sie sich bet\u00f6ren.", "tokens": ["Da", "lie\u00df", "sie", "sich", "be\u00b7t\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der M\u00fcller, der war falsch von Sinn:", "tokens": ["Der", "M\u00fcl\u00b7ler", ",", "der", "war", "falsch", "von", "Sinn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwenn ich mich tu verm\u00e4hlen,", "tokens": ["\u00bb", "wenn", "ich", "mich", "tu", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So will ich mir als M\u00fcllerin", "tokens": ["So", "will", "ich", "mir", "als", "M\u00fcl\u00b7le\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl eine Reiche w\u00e4hlen.\u00ab", "tokens": ["Wohl", "ei\u00b7ne", "Rei\u00b7che", "w\u00e4h\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NE", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da 's arme M\u00e4del das vernahm,", "tokens": ["Da", "'s", "ar\u00b7me", "M\u00e4\u00b7del", "das", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wird's bla\u00df und immer blasser", "tokens": ["Wird's", "bla\u00df", "und", "im\u00b7mer", "blas\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und red't nit mehr und ging und kam", "tokens": ["Und", "red't", "nit", "mehr", "und", "ging", "und", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprang ins tiefe Wasser.", "tokens": ["Und", "sprang", "ins", "tie\u00b7fe", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der M\u00fcller k\u00fcmmert sich nicht viel,", "tokens": ["Der", "M\u00fcl\u00b7ler", "k\u00fcm\u00b7mert", "sich", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "T\u00e4t Hochzeitleut bestellen", "tokens": ["T\u00e4t", "Hoch\u00b7zeit\u00b7leut", "be\u00b7stel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00e4hrt mit Sang und Saitenspiel", "tokens": ["Und", "f\u00e4hrt", "mit", "Sang", "und", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "'ne andre zur Kapellen.", "tokens": ["'ne", "and\u00b7re", "zur", "Ka\u00b7pel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch als man auf die Br\u00fccke kam,", "tokens": ["Doch", "als", "man", "auf", "die", "Br\u00fc\u00b7cke", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4ngt 's Wasser an zu wogen", "tokens": ["F\u00e4ngt", "'s", "Was\u00b7ser", "an", "zu", "wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zischt und rauscht verwundersam", "tokens": ["Und", "zischt", "und", "rauscht", "ver\u00b7wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herauf bis an den Bogen.", "tokens": ["Her\u00b7auf", "bis", "an", "den", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die wei\u00dfe Wassernixe stand", "tokens": ["Die", "wei\u00b7\u00dfe", "Was\u00b7ser\u00b7ni\u00b7xe", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf schaumgekr\u00f6nter Welle;", "tokens": ["Auf", "schaum\u00b7ge\u00b7kr\u00f6n\u00b7ter", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie h\u00e4lt in ihrer wei\u00dfen Hand", "tokens": ["Sie", "h\u00e4lt", "in", "ih\u00b7rer", "wei\u00b7\u00dfen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Gold ein Ringlein helle.", "tokens": ["Von", "Gold", "ein", "Rin\u00b7glein", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du Falscher, deine Zeit ist aus!", "tokens": ["Du", "Fal\u00b7scher", ",", "dei\u00b7ne", "Zeit", "ist", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bereite dich geschwinde!", "tokens": ["Be\u00b7rei\u00b7te", "dich", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich ruft hinab ins kalte Haus", "tokens": ["Dich", "ruft", "hin\u00b7ab", "ins", "kal\u00b7te", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter mit dem Kinde.", "tokens": ["Die", "Mut\u00b7ter", "mit", "dem", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}