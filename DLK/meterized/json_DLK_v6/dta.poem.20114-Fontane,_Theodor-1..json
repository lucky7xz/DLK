{"dta.poem.20114": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.71", "nl:0.14", "da:0.14"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Darnley reitet in den Wald, Lord Ruthven", "tokens": ["Herr", "Darn\u00b7ley", "rei\u00b7tet", "in", "den", "Wald", ",", "Lord", "Ruth\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ihm zur Seite;", "tokens": ["ihm", "zur", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Herr Darnley spricht: \u201ewas frommt es mir, da\u00df", "tokens": ["Herr", "Darn\u00b7ley", "spricht", ":", "\u201e", "was", "frommt", "es", "mir", ",", "da\u00df"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "NE", "VVFIN", "$.", "$(", "PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "in den Lenz ich reite?", "tokens": ["in", "den", "Lenz", "ich", "rei\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ich ritt hinaus ein Schreckgespenst mir aus dem", "tokens": ["Ich", "ritt", "hin\u00b7aus", "ein", "Schreck\u00b7ge\u00b7spenst", "mir", "aus", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APZR", "ART", "NN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sinn zu schlagen,", "tokens": ["Sinn", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ihr aber Ruthven hastet Euch in\u2019s Feuer Oel", "tokens": ["Ihr", "a\u00b7ber", "Ruth\u00b7ven", "has\u00b7tet", "Euch", "in's", "Feu\u00b7er", "O\u00b7el"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NE", "VVFIN", "PPER", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "zu tragen.\u201c", "tokens": ["zu", "tra\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Lord Ruthven streicht den rothen Bart, als sei", "tokens": ["Lord", "Ruth\u00b7ven", "streicht", "den", "ro\u00b7then", "Bart", ",", "als", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "$,", "KOKOM", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "er des zufrieden,", "tokens": ["er", "des", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Er schweigt, und denkt nur: \u201ewenn es hei\u00df, soll", "tokens": ["Er", "schweigt", ",", "und", "denkt", "nur", ":", "\u201e", "wenn", "es", "hei\u00df", ",", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$.", "$(", "KOUS", "PPER", "ADJD", "$,", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "man das Eisen schmieden\u201c;", "tokens": ["man", "das", "Ei\u00b7sen", "schmie\u00b7den", "\u201c", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "ART", "NN", "VVINF", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seit an Maria\u2019s Ohr er frech ein Liebeswort", "tokens": ["Seit", "an", "Ma\u00b7ria's", "Ohr", "er", "frech", "ein", "Lie\u00b7bes\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "verloren,", "tokens": ["ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Hat er der sch\u00f6nen K\u00f6nigin im Herzen Ha\u00df", "tokens": ["Hat", "er", "der", "sch\u00f6\u00b7nen", "K\u00f6\u00b7ni\u00b7gin", "im", "Her\u00b7zen", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "geschworen.", "tokens": ["ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Er spricht kein Wort, beredter spricht sein L\u00e4cheln", "tokens": ["Er", "spricht", "kein", "Wort", ",", "be\u00b7red\u00b7ter", "spricht", "sein", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "jetzt und Schweigen,", "tokens": ["jetzt", "und", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Er sieht, von Schritt zu Schritt, das Blut in", "tokens": ["Er", "sieht", ",", "von", "Schritt", "zu", "Schritt", ",", "das", "Blut", "in"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darnley\u2019s Wange steigen,", "tokens": ["Darn\u00b7ley's", "Wan\u00b7ge", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der ruft: \u201dsing aus Dein Rabenlied, und spricht's", "tokens": ["Der", "ruft", ":", "\"", "sing", "aus", "Dein", "Ra\u00b7ben\u00b7lied", ",", "und", "spricht's"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "wie Deine Blicke,", "tokens": ["wie", "Dei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Verdamm mich Gott, wenn ich den Fant nicht", "tokens": ["Ver\u00b7damm", "mich", "Gott", ",", "wenn", "ich", "den", "Fant", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "in die H\u00f6lle schicke!\u201c", "tokens": ["in", "die", "H\u00f6l\u00b7le", "schi\u00b7cke", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Lord Ruthven streicht den rothen Bart, und spricht:", "tokens": ["Lord", "Ruth\u00b7ven", "streicht", "den", "ro\u00b7then", "Bart", ",", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u201eso soll ich\u2019s glauben", "tokens": ["\u201e", "so", "soll", "ich's", "glau\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mein Herr und K\u00f6nig zweifle noch am Spiel", "tokens": ["Mein", "Herr", "und", "K\u00f6\u00b7nig", "zweif\u00b7le", "noch", "am", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der frommen Tauben?", "tokens": ["der", "from\u00b7men", "Tau\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Er wisse nicht, was Jeder wei\u00df vom schottschen", "tokens": ["Er", "wis\u00b7se", "nicht", ",", "was", "Je\u00b7der", "wei\u00df", "vom", "schott\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PIS", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "K\u00f6nigsstuhle,", "tokens": ["K\u00f6\u00b7nigs\u00b7stuh\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Da\u00df Heinrich Darnley\u2019s ehlich Weib des David", "tokens": ["Da\u00df", "Hein\u00b7rich", "Darn\u00b7ley's", "eh\u00b7lich", "Weib", "des", "Da\u00b7vid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "ADJD", "NN", "ART", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Rizzio Buhle!\u201c", "tokens": ["Riz\u00b7zio", "Buh\u00b7le", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Herr Darnley kehrt gen Edinburg, er h\u00e4lt vor", "tokens": ["Herr", "Darn\u00b7ley", "kehrt", "gen", "E\u00b7din\u00b7burg", ",", "er", "h\u00e4lt", "vor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPR", "NE", "$,", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "seinem Schlosse:", "tokens": ["sei\u00b7nem", "Schlos\u00b7se", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "\u201elord Ruthven \u2014 spricht er \u2014 so\u2019s beliebt, ", "tokens": ["\u201e", "lord", "Ruth\u00b7ven", "spricht", "er", "so's", "be\u00b7liebt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$(", "VVFIN", "PPER", "$(", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "ihr mein Jagdgenosse;", "tokens": ["ihr", "mein", "Jagd\u00b7ge\u00b7nos\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der Fuchs ist schlau, doch b\u00e4rg er sich in ", "tokens": ["Der", "Fuchs", "ist", "schlau", ",", "doch", "b\u00e4rg", "er", "sich", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", 