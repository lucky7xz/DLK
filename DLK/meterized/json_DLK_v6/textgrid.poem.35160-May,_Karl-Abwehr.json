{"textgrid.poem.35160": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Abwehr", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir glauben! L\u00e4chle nicht; es ist uns Ernst!", "tokens": ["Wir", "glau\u00b7ben", "!", "L\u00e4ch\u00b7le", "nicht", ";", "es", "ist", "uns", "Ernst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "PTKNEG", "$.", "PPER", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du kennst den Glauben nicht, und ich kann dir nicht zeigen,", "tokens": ["Du", "kennst", "den", "Glau\u00b7ben", "nicht", ",", "und", "ich", "kann", "dir", "nicht", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df wir mit ihm hinauf in alle Himmel reichen,", "tokens": ["Da\u00df", "wir", "mit", "ihm", "hin\u00b7auf", "in", "al\u00b7le", "Him\u00b7mel", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von denen du dich mehr und mehr entfernst.", "tokens": ["Von", "de\u00b7nen", "du", "dich", "mehr", "und", "mehr", "ent\u00b7fernst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist so leicht, den Himmel Himmel sein zu lassen;", "tokens": ["Es", "ist", "so", "leicht", ",", "den", "Him\u00b7mel", "Him\u00b7mel", "sein", "zu", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "NN", "VAINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist so schwer, vom Himmel aus die Erde zu erfassen;", "tokens": ["Es", "ist", "so", "schwer", ",", "vom", "Him\u00b7mel", "aus", "die", "Er\u00b7de", "zu", "er\u00b7fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "APPRART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Das ist der Grund, da\u00df du nicht glauben lernst.", "tokens": ["Das", "ist", "der", "Grund", ",", "da\u00df", "du", "nicht", "glau\u00b7ben", "lernst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir glauben! W\u00fc\u00dftest du doch, was das hei\u00dft!", "tokens": ["Wir", "glau\u00b7ben", "!", "W\u00fc\u00df\u00b7test", "du", "doch", ",", "was", "das", "hei\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "$,", "PWS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denk dir es nicht als ein pers\u00f6nliches Empfinden;", "tokens": ["Denk", "dir", "es", "nicht", "als", "ein", "per\u00b7s\u00f6n\u00b7li\u00b7ches", "Emp\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKNEG", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denk dirs als Meer, in dem wir Nahrung finden,", "tokens": ["Denk", "dirs", "als", "Meer", ",", "in", "dem", "wir", "Nah\u00b7rung", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "KOKOM", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denk aber nicht, da\u00df du es dann schon wei\u00dft.", "tokens": ["Denk", "a\u00b7ber", "nicht", ",", "da\u00df", "du", "es", "dann", "schon", "wei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Glaube bildet eine Welt, in der wir leben,", "tokens": ["Der", "Glau\u00b7be", "bil\u00b7det", "ei\u00b7ne", "Welt", ",", "in", "der", "wir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dieser Welt allein ist Seligkeit gegeben;", "tokens": ["Und", "die\u00b7ser", "Welt", "al\u00b7lein", "ist", "Se\u00b7lig\u00b7keit", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er ist der Raum, in dem die Hoffnung kreist.", "tokens": ["Er", "ist", "der", "Raum", ",", "in", "dem", "die", "Hoff\u00b7nung", "kreist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir glauben! Gro\u00dfes ist damit gesagt!", "tokens": ["Wir", "glau\u00b7ben", "!", "Gro\u00b7\u00dfes", "ist", "da\u00b7mit", "ge\u00b7sagt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das, was ihr wi\u00dft, verdankt ihr nur den \u00e4u\u00dfern Sinnen,", "tokens": ["Das", ",", "was", "ihr", "wi\u00dft", ",", "ver\u00b7dankt", "ihr", "nur", "den", "\u00e4u\u00b7\u00dfern", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch giebts nicht blos ein Au\u00dfen, sondern auch ein Innen,", "tokens": ["Doch", "giebts", "nicht", "blos", "ein", "Au\u00b7\u00dfen", ",", "son\u00b7dern", "auch", "ein", "In\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem eine Sonne um die andre tagt.", "tokens": ["Dem", "ei\u00b7ne", "Son\u00b7ne", "um", "die", "and\u00b7re", "tagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und \u00f6ffnet dieses Innen muthig seine Augen,", "tokens": ["Und", "\u00f6ff\u00b7net", "die\u00b7ses", "In\u00b7nen", "mut\u00b7hig", "sei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So d\u00fcrfen sie den Blick in Herrlichkeiten tauchen,", "tokens": ["So", "d\u00fcr\u00b7fen", "sie", "den", "Blick", "in", "Herr\u00b7lich\u00b7kei\u00b7ten", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An welche eure Brille nie sich wagt.", "tokens": ["An", "wel\u00b7che", "eu\u00b7re", "Bril\u00b7le", "nie", "sich", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir glauben! Dessen sch\u00e4men wir uns nicht!", "tokens": ["Wir", "glau\u00b7ben", "!", "Des\u00b7sen", "sch\u00e4\u00b7men", "wir", "uns", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "VVFIN", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es ist der Mensch verpflichtet, diesem Erdenleben", "tokens": ["Es", "ist", "der", "Mensch", "ver\u00b7pflich\u00b7tet", ",", "die\u00b7sem", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr sich und seine Br\u00fcder, was ihm fehlt, zu geben:", "tokens": ["F\u00fcr", "sich", "und", "sei\u00b7ne", "Br\u00fc\u00b7der", ",", "was", "ihm", "fehlt", ",", "zu", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRF", "KON", "PPOSAT", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Herzen Liebe, dem Verstande Licht.", "tokens": ["Dem", "Her\u00b7zen", "Lie\u00b7be", ",", "dem", "Ver\u00b7stan\u00b7de", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir fragen nicht: Wird diese Gabe angenommen?", "tokens": ["Wir", "fra\u00b7gen", "nicht", ":", "Wird", "die\u00b7se", "Ga\u00b7be", "an\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir wissen nur: Es ist die Zeit dazu gekommen,", "tokens": ["Wir", "wis\u00b7sen", "nur", ":", "Es", "ist", "die", "Zeit", "da\u00b7zu", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und darum sind wir voller Zuversicht.", "tokens": ["Und", "da\u00b7rum", "sind", "wir", "vol\u00b7ler", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir glauben! Aber wer sind diese \u00bbWir\u00ab?", "tokens": ["Wir", "glau\u00b7ben", "!", "A\u00b7ber", "wer", "sind", "die\u00b7se", "\u00bb", "Wir", "\u00ab", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PWS", "VAFIN", "PDAT", "$(", "PPER", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gieb dir nicht M\u00fche, unsre Ziffer zu bestimmen.", "tokens": ["Gieb", "dir", "nicht", "M\u00fc\u00b7he", ",", "uns\u00b7re", "Zif\u00b7fer", "zu", "be\u00b7stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wolltst du uns sehn, so m\u00fc\u00dftest Berge du erklimmen,", "tokens": ["Wolltst", "du", "uns", "sehn", ",", "so", "m\u00fc\u00df\u00b7test", "Ber\u00b7ge", "du", "er\u00b7klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "ADV", "VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und diese Berge stehen nicht nur hier.", "tokens": ["Und", "die\u00b7se", "Ber\u00b7ge", "ste\u00b7hen", "nicht", "nur", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch, wo nach Licht, nach Liebe sich ein Sehnen findet,", "tokens": ["Doch", ",", "wo", "nach", "Licht", ",", "nach", "Lie\u00b7be", "sich", "ein", "Seh\u00b7nen", "fin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "$,", "APPR", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da offenbart sich dir das Band, das uns verbindet;", "tokens": ["Da", "of\u00b7fen\u00b7bart", "sich", "dir", "das", "Band", ",", "das", "uns", "ver\u00b7bin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir f\u00fchren \u00bbLicht und Liebe\u00ab im Panier.", "tokens": ["Wir", "f\u00fch\u00b7ren", "\u00bb", "Licht", "und", "Lie\u00b7be", "\u00ab", "im", "Pa\u00b7nier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "KON", "NN", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir glauben! Das ist h\u00f6chste Th\u00e4tigkeit!", "tokens": ["Wir", "glau\u00b7ben", "!", "Das", "ist", "h\u00f6chs\u00b7te", "Th\u00e4\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du meinst, der Glaube sei uns nur ein Ruhekissen,", "tokens": ["Du", "meinst", ",", "der", "Glau\u00b7be", "sei", "uns", "nur", "ein", "Ru\u00b7he\u00b7kis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf dem wir unsre Psyche wohl zu pflegen wissen;", "tokens": ["Auf", "dem", "wir", "uns\u00b7re", "Psy\u00b7che", "wohl", "zu", "pfle\u00b7gen", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich gebe dir ganz anderen Bescheid:", "tokens": ["Ich", "ge\u00b7be", "dir", "ganz", "an\u00b7de\u00b7ren", "Be\u00b7scheid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir baun im Stillen, rastlos, uns und euch zum Gl\u00fccke,", "tokens": ["Wir", "baun", "im", "Stil\u00b7len", ",", "rast\u00b7los", ",", "uns", "und", "euch", "zum", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ADJD", "$,", "PPER", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Tag zu Tage neue Pfeiler, eine Br\u00fccke", "tokens": ["Von", "Tag", "zu", "Ta\u00b7ge", "neu\u00b7e", "Pfei\u00b7ler", ",", "ei\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hoch \u00fcbers Grab hinweg zur Ewigkeit.", "tokens": ["Hoch", "\u00fc\u00b7bers", "Grab", "hin\u00b7weg", "zur", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APZR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wir glauben! Welche Wonne, welche Lust!", "tokens": ["Wir", "glau\u00b7ben", "!", "Wel\u00b7che", "Won\u00b7ne", ",", "wel\u00b7che", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAT", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie freun wir uns darauf, den Vorhang zu entfernen;", "tokens": ["Wie", "freun", "wir", "uns", "da\u00b7rauf", ",", "den", "Vor\u00b7hang", "zu", "ent\u00b7fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PAV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wirst du da des Glaubens Walten kennen lernen,", "tokens": ["Wie", "wirst", "du", "da", "des", "Glau\u00b7bens", "Wal\u00b7ten", "ken\u00b7nen", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Br\u00fccke sehn, von der du nichts gewu\u00dft!", "tokens": ["Die", "Br\u00fc\u00b7cke", "sehn", ",", "von", "der", "du", "nichts", "ge\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPR", "PRELS", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du wirst dann schaun wie wir, nimmst Theil an unsern Gaben,", "tokens": ["Du", "wirst", "dann", "schaun", "wie", "wir", ",", "nimmst", "Theil", "an", "un\u00b7sern", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "KOKOM", "PPER", "$,", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch ohne so wie wir, vorher geglaubt zu haben,", "tokens": ["Doch", "oh\u00b7ne", "so", "wie", "wir", ",", "vor\u00b7her", "ge\u00b7glaubt", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "KOKOM", "PPER", "$,", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00fcr dich ein unersetzlicher Verlust!", "tokens": ["F\u00fcr", "dich", "ein", "un\u00b7er\u00b7setz\u00b7li\u00b7cher", "Ver\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wir glauben! Erstes, doch nicht letztes Wort!", "tokens": ["Wir", "glau\u00b7ben", "!", "Ers\u00b7tes", ",", "doch", "nicht", "letz\u00b7tes", "Wort", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "$,", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dies erste Wort, ich hab es heut zu dir gesprochen,", "tokens": ["Dies", "ers\u00b7te", "Wort", ",", "ich", "hab", "es", "heut", "zu", "dir", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das zweite hat schon l\u00e4ngst die Gr\u00e4ber aufgebrochen,", "tokens": ["Das", "zwei\u00b7te", "hat", "schon", "l\u00e4ngst", "die", "Gr\u00e4\u00b7ber", "auf\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch leider warf der Unverstand es fort.", "tokens": ["Doch", "lei\u00b7der", "warf", "der", "Un\u00b7ver\u00b7stand", "es", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das letzte hat sich unser Vater vorbehalten,", "tokens": ["Das", "letz\u00b7te", "hat", "sich", "un\u00b7ser", "Va\u00b7ter", "vor\u00b7be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und lie\u00dfest du nur ihn und seine Gnade walten,", "tokens": ["Und", "lie\u00b7\u00dfest", "du", "nur", "ihn", "und", "sei\u00b7ne", "Gna\u00b7de", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So h\u00f6rtest du's schon hier und nicht erst dort.", "tokens": ["So", "h\u00f6r\u00b7test", "du's", "schon", "hier", "und", "nicht", "erst", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "KON", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}