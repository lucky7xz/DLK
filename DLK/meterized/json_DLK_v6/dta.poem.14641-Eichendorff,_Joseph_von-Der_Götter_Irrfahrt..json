{"dta.poem.14641": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Irrfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie 'ne liebliche Syrene", "tokens": ["Wie", "'ne", "lieb\u00b7li\u00b7che", "Sy\u00b7re\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Halb nun \u00fcber'm Wellenglanz,", "tokens": ["Halb", "nun", "\u00fc\u00b7ber'm", "Wel\u00b7len\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Staunend ob der eig'nen Sch\u00f6ne,", "tokens": ["Stau\u00b7nend", "ob", "der", "eig'\u00b7nen", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt es mit dem Bl\u00fctenkranz,", "tokens": ["Schwebt", "es", "mit", "dem", "Bl\u00fc\u00b7ten\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei der L\u00fcfte lindem F\u00e4cheln", "tokens": ["Bei", "der", "L\u00fcf\u00b7te", "lin\u00b7dem", "F\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich im Meer, das rosig brennt,", "tokens": ["Sich", "im", "Meer", ",", "das", "ro\u00b7sig", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Spiegelnd mit versch\u00e4mtem L\u00e4cheln \u2014", "tokens": ["Spie\u00b7gelnd", "mit", "ver\u00b7sch\u00e4m\u00b7tem", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Erde sie der Vater nennt.", "tokens": ["Er\u00b7de", "sie", "der", "Va\u00b7ter", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}