{"textgrid.poem.67466": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die B\u00fcrde des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbw\u00e4get das Schicksal Leben und Tod? Wie oder ereilet", "tokens": ["\u00bb", "w\u00e4\u00b7get", "das", "Schick\u00b7sal", "Le\u00b7ben", "und", "Tod", "?", "Wie", "o\u00b7der", "er\u00b7ei\u00b7let"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$.", "PWAV", "KON", "VVFIN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Jeden ein blindes Loos, wie es die Urne gebeut?\u00ab", "tokens": ["Je\u00b7den", "ein", "blin\u00b7des", "Loos", ",", "wie", "es", "die", "Ur\u00b7ne", "ge\u00b7beut", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Also fragt' ich und sah im Gesicht die goldene Wage", "tokens": ["Al\u00b7so", "fragt'", "ich", "und", "sah", "im", "Ge\u00b7sicht", "die", "gol\u00b7de\u00b7ne", "Wa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Un\u00fcberschaubar hoch sinken und steigen im Kampf.", "tokens": ["Un\u00b7\u00fc\u00b7bersc\u00b7hau\u00b7bar", "hoch", "sin\u00b7ken", "und", "stei\u00b7gen", "im", "Kampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVINF", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Zitternd trat ich zur Urne. Da rief die Stimme des Schicksals:", "tokens": ["Zit\u00b7ternd", "trat", "ich", "zur", "Ur\u00b7ne", ".", "Da", "rief", "die", "Stim\u00b7me", "des", "Schick\u00b7sals", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPRART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "\u00bbziehe das Loos!\u00ab Ich zog bebend \u2013 mein eigenes selbst.", "tokens": ["\u00bb", "zie\u00b7he", "das", "Loos", "!", "\u00ab", "Ich", "zog", "be\u00b7bend", "\u2013", "mein", "ei\u00b7ge\u00b7nes", "selbst", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADJD", "$(", "PPOSAT", "ADJA", "ADV", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "B\u00fcrden lagen vor mir; ich pr\u00fcfte die leichteste B\u00fcrde,", "tokens": ["B\u00fcr\u00b7den", "la\u00b7gen", "vor", "mir", ";", "ich", "pr\u00fcf\u00b7te", "die", "leich\u00b7tes\u00b7te", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Und, o Wunder! ich sah, da\u00df es die meinige war.", "tokens": ["Und", ",", "o", "Wun\u00b7der", "!", "ich", "sah", ",", "da\u00df", "es", "die", "mei\u00b7ni\u00b7ge", "war", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "PPOSS", "VAFIN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}}}}