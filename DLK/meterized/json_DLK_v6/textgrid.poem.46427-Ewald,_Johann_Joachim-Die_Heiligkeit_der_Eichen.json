{"textgrid.poem.46427": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Die Heiligkeit der Eichen", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sprach ich mit den Eichen,", "tokens": ["J\u00fcngst", "sprach", "ich", "mit", "den", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und frug: Warum ihr Eichen,", "tokens": ["Und", "frug", ":", "Wa\u00b7rum", "ihr", "Ei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seyd ihr geheiligt worden?", "tokens": ["Seyd", "ihr", "ge\u00b7hei\u00b7ligt", "wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drauf zitterten die Wipfel,", "tokens": ["Drauf", "zit\u00b7ter\u00b7ten", "die", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sieh ein braunes M\u00e4dchen,", "tokens": ["Und", "sieh", "ein", "brau\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sprang aus der n\u00e4chsten Eiche,", "tokens": ["Sprang", "aus", "der", "n\u00e4chs\u00b7ten", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und rief: Der Nymphen wegen", "tokens": ["Und", "rief", ":", "Der", "Nym\u00b7phen", "we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die in den Eichen wohnen,", "tokens": ["Die", "in", "den", "Ei\u00b7chen", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sind sie geheiligt worden:", "tokens": ["Sind", "sie", "ge\u00b7hei\u00b7ligt", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nun ehr ich alle Eichen!", "tokens": ["Nun", "ehr", "ich", "al\u00b7le", "Ei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}