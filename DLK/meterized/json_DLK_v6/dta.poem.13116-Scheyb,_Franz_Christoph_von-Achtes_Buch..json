{"dta.poem.13116": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eist der Beweis zu schwach, mich allen vorzuziehen,", "tokens": ["\u201e", "ist", "der", "Be\u00b7weis", "zu", "schwach", ",", "mich", "al\u00b7len", "vor\u00b7zu\u00b7zie\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "PTKZU", "VVFIN", "$,", "PPER", "PIS", "VVIZU", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "\u201eso sey mir die Geduld noch zu dem Wort verliehen:", "tokens": ["\u201e", "so", "sey", "mir", "die", "Ge\u00b7duld", "noch", "zu", "dem", "Wort", "ver\u00b7lie\u00b7hen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edas Werck soll ewig stehn, das man errichten will;", "tokens": ["\u201e", "das", "Werck", "soll", "e\u00b7wig", "stehn", ",", "das", "man", "er\u00b7rich\u00b7ten", "will", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "ADJD", "VVINF", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "490\u201dWei\u00df jemand so zu baun, so leg ich mich zum Ziel.", "tokens": ["\"", "Wei\u00df", "je\u00b7mand", "so", "zu", "baun", ",", "so", "leg", "ich", "mich", "zum", "Ziel", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,", "ADV", "NE", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eallein ich zweifle sehr, da\u00df meine Kunst nicht siege,", "tokens": ["\u201e", "al\u00b7lein", "ich", "zweif\u00b7le", "sehr", ",", "da\u00df", "mei\u00b7ne", "Kunst", "nicht", "sie\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "\u201ewann ich, was meine Kraft vermag, zusammen f\u00fcge.", "tokens": ["\u201e", "wann", "ich", ",", "was", "mei\u00b7ne", "Kraft", "ver\u00b7mag", ",", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7ge", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201efreundinnen! bauet, mahlt, schnizt, macht was euch beliebt;", "tokens": ["\u201e", "freun\u00b7din\u00b7nen", "!", "bau\u00b7et", ",", "mahlt", ",", "schnizt", ",", "macht", "was", "euch", "be\u00b7liebt", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$.", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PWS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eergreifft was die Natur der Kunst zur Hilffe gibt!", "tokens": ["\u201e", "er\u00b7greifft", "was", "die", "Na\u00b7tur", "der", "Kunst", "zur", "Hilf\u00b7fe", "gibt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PWS", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "495\u201dBringt ein Geb\u00e4u von Stein, von Erz und Stahl zusammen,", "tokens": ["\"", "Bringt", "ein", "Ge\u00b7b\u00e4u", "von", "Stein", ",", "von", "Erz", "und", "Stahl", "zu\u00b7sam\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eerw\u00e4hlt den Grund dazu, von dem die Berge stammen.", "tokens": ["\u201e", "er\u00b7w\u00e4hlt", "den", "Grund", "da\u00b7zu", ",", "von", "dem", "die", "Ber\u00b7ge", "stam\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "PAV", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201egie\u00dft, schmiedet und erh\u00f6ht Colossen von Metall;", "tokens": ["\u201e", "gie\u00dft", ",", "schmie\u00b7det", "und", "er\u00b7h\u00f6ht", "Co\u00b7los\u00b7sen", "von", "Me\u00b7tall", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ebeschirmt sie f\u00fcr Gewalt, Ersch\u00fcttrung, Sturz und Fall:", "tokens": ["\u201e", "be\u00b7schirmt", "sie", "f\u00fcr", "Ge\u00b7walt", ",", "Er\u00b7sch\u00fct\u00b7trung", ",", "Sturz", "und", "Fall", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewird alles die\u00df so lang, als was ich baue, stehen,", "tokens": ["\u201e", "wird", "al\u00b7les", "die\u00df", "so", "lang", ",", "als", "was", "ich", "bau\u00b7e", ",", "ste\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "PDS", "ADV", "ADJD", "$,", "KOUS", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "500\u201dSo mu\u00df es mit der Welt und eher nicht vergehen.", "tokens": ["\"", "So", "mu\u00df", "es", "mit", "der", "Welt", "und", "e\u00b7her", "nicht", "ver\u00b7ge\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "KON", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eund bey dem Untergang bey dem gemeinen Fall", "tokens": ["\u201e", "und", "bey", "dem", "Un\u00b7ter\u00b7gang", "bey", "dem", "ge\u00b7mei\u00b7nen", "Fall"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201evernehmt ihr noch im Sturm der edeln Fl\u00f6te Schall,", "tokens": ["\u201e", "ver\u00b7nehmt", "ihr", "noch", "im", "Sturm", "der", "e\u00b7deln", "Fl\u00f6\u00b7te", "Schall", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eder dauert immer fort. Wofern ihr ihn nicht h\u00f6ret,", "tokens": ["\u201e", "der", "dau\u00b7ert", "im\u00b7mer", "fort", ".", "Wo\u00b7fern", "ihr", "ihn", "nicht", "h\u00f6\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VVFIN", "ADV", "PTKVZ", "$.", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eso ists, weil das Get\u00f6\u00df euch Ohr und Sinnen st\u00f6ret.", "tokens": ["\u201e", "so", "ists", ",", "weil", "das", "Ge\u00b7t\u00f6\u00df", "euch", "Ohr", "und", "Sin\u00b7nen", "st\u00f6\u00b7ret", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "KOUS", "ART", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "505\u201dDer Nahme, dessen Ruhm der Cither Klang besingt,", "tokens": ["\"", "Der", "Nah\u00b7me", ",", "des\u00b7sen", "Ruhm", "der", "Cit\u00b7her", "Klang", "be\u00b7singt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201egr\u00fcnt, w\u00e4chst und steigt so lang bi\u00df er zur Sonne dringt,", "tokens": ["\u201e", "gr\u00fcnt", ",", "w\u00e4chst", "und", "steigt", "so", "lang", "bi\u00df", "er", "zur", "Son\u00b7ne", "dringt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201eer trozt den Zahn der Zeit, das Feur, den Stahl, das Eisen,", "tokens": ["\u201e", "er", "trozt", "den", "Zahn", "der", "Zeit", ",", "das", "Feur", ",", "den", "Stahl", ",", "das", "Ei\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201eso gar dem Neid kann er die heitre Stirne weisen.", "tokens": ["\u201e", "so", "gar", "dem", "Neid", "kann", "er", "die", "heit\u00b7re", "Stir\u00b7ne", "wei\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}