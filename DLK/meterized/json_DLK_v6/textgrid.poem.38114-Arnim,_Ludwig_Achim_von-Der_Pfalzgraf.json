{"textgrid.poem.38114": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pfalzgraf", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es reitet die Gr\u00e4fin weit \u00fcber das Feld,", "tokens": ["Es", "rei\u00b7tet", "die", "Gr\u00e4\u00b7fin", "weit", "\u00fc\u00b7ber", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit ihrem gelbhaarigen T\u00f6chterlein fein,", "tokens": ["Mit", "ih\u00b7rem", "gelb\u00b7ha\u00b7a\u00b7ri\u00b7gen", "T\u00f6ch\u00b7ter\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie reiten wohl in des Pfalzgrafen sein Zelt,", "tokens": ["Sie", "rei\u00b7ten", "wohl", "in", "des", "Pfalz\u00b7gra\u00b7fen", "sein", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und wollen fein fr\u00f6lich und lustig sein.", "tokens": ["Und", "wol\u00b7len", "fein", "fr\u00f6\u00b7lich", "und", "lus\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Frau Gr\u00e4fin, was jagt ihr so fr\u00fch schon hinaus?", "tokens": ["Frau", "Gr\u00e4\u00b7fin", ",", "was", "jagt", "ihr", "so", "fr\u00fch", "schon", "hin\u00b7aus", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "O reitet mit eurem fein Liebchen nach Haus,", "tokens": ["O", "rei\u00b7tet", "mit", "eu\u00b7rem", "fein", "Lieb\u00b7chen", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Pfalzgraf kommt selber gleich zu euch hinab,", "tokens": ["Der", "Pfalz\u00b7graf", "kommt", "sel\u00b7ber", "gleich", "zu", "euch", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie tragen ihn morgen hinunter ins Grab:", "tokens": ["Sie", "tra\u00b7gen", "ihn", "mor\u00b7gen", "hin\u00b7un\u00b7ter", "ins", "Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Es hat ihn eine Kugel so t\u00f6dtlich verwundt,", "tokens": ["Es", "hat", "ihn", "ei\u00b7ne", "Ku\u00b7gel", "so", "t\u00f6dt\u00b7lich", "ver\u00b7wundt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da starb er sogleich in der n\u00e4mlichen Stund,", "tokens": ["Da", "starb", "er", "sog\u00b7leich", "in", "der", "n\u00e4m\u00b7li\u00b7chen", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da schickt er dem Fr\u00e4ulein ein Ringelein fein,", "tokens": ["Da", "schickt", "er", "dem", "Fr\u00e4u\u00b7lein", "ein", "Rin\u00b7ge\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Soll seiner beim Scheiden noch eingedenk sein.", "tokens": ["Soll", "sei\u00b7ner", "beim", "Schei\u00b7den", "noch", "ein\u00b7ge\u00b7denk", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "APPRART", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Hat dich o Pfalzgraf, die Kugel getroffen,", "tokens": ["Hat", "dich", "o", "Pfalz\u00b7graf", ",", "die", "Ku\u00b7gel", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "FM", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "W\u00e4r ich viel lieber im Neckar ersoffen;", "tokens": ["W\u00e4r", "ich", "viel", "lie\u00b7ber", "im", "Ne\u00b7ckar", "er\u00b7sof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tr\u00e4gt man den Liebsten zum Kirchhof herein,", "tokens": ["Tr\u00e4gt", "man", "den", "Liebs\u00b7ten", "zum", "Kirch\u00b7hof", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Steig ich wohl mit ihm ins Brautbett hinein.", "tokens": ["Steig", "ich", "wohl", "mit", "ihm", "ins", "Braut\u00b7bett", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Will reichen ihm meinen jungfr\u00e4ulichen Kranz,", "tokens": ["Will", "rei\u00b7chen", "ihm", "mei\u00b7nen", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Will sterben und scheiden von G\u00fcter und Glanz;", "tokens": ["Will", "ster\u00b7ben", "und", "schei\u00b7den", "von", "G\u00fc\u00b7ter", "und", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Lieb Mutter, sez du mir den Kranz in das Haar,", "tokens": ["Lieb", "Mut\u00b7ter", ",", "sez", "du", "mir", "den", "Kranz", "in", "das", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Auf da\u00df ich sch\u00f6n ruhen kann auf der Bahr.", "tokens": ["Auf", "da\u00df", "ich", "sch\u00f6n", "ru\u00b7hen", "kann", "auf", "der", "Bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Steck mir an den Finger das Ringlein fein,", "tokens": ["Steck", "mir", "an", "den", "Fin\u00b7ger", "das", "Rin\u00b7glein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Es mit mir soll liegen ins Grab hinein,", "tokens": ["Es", "mit", "mir", "soll", "lie\u00b7gen", "ins", "Grab", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VMFIN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein schneeweisses Hemdelein zieh du mir an,", "tokens": ["Ein", "schnee\u00b7weis\u00b7ses", "Hem\u00b7del\u00b7ein", "zieh", "du", "mir", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "++--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Auf da\u00df ich kann schlafen bei meinem Mann.", "tokens": ["Auf", "da\u00df", "ich", "kann", "schla\u00b7fen", "bei", "mei\u00b7nem", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.7": {"line.1": {"text": "Auf T\u00f6chterleins Grab sollst legen ein Stein,", "tokens": ["Auf", "T\u00f6ch\u00b7ter\u00b7leins", "Grab", "sollst", "le\u00b7gen", "ein", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VMFIN", "VVINF", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Drauf sollen die Worte geschrieben seyn:", "tokens": ["Drauf", "sol\u00b7len", "die", "Wor\u00b7te", "ge\u00b7schrie\u00b7ben", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hier ruhet der Pfalzgraf und seine Braut;", "tokens": ["Hier", "ru\u00b7het", "der", "Pfalz\u00b7graf", "und", "sei\u00b7ne", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da hat man den beiden das Brautbett gebaut.", "tokens": ["Da", "hat", "man", "den", "bei\u00b7den", "das", "Braut\u00b7bett", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ART", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}