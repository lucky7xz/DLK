{"textgrid.poem.34002": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die menschliche Gesellschaft", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du verk\u00f6rpert w\u00e4rst zu Einem Leibe,", "tokens": ["Wenn", "du", "ver\u00b7k\u00f6r\u00b7pert", "w\u00e4rst", "zu", "Ei\u00b7nem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit allen deinen Satzungen und Rechten,", "tokens": ["Mit", "al\u00b7len", "dei\u00b7nen", "Sat\u00b7zun\u00b7gen", "und", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die das Lebendig-Freie schaamlos knechten,", "tokens": ["Die", "das", "Le\u00b7ben\u00b7dig\u00b7Freie", "schaam\u00b7los", "knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Damit dem Totden diese Welt verbleibe;", "tokens": ["Da\u00b7mit", "dem", "Tot\u00b7den", "die\u00b7se", "Welt", "ver\u00b7blei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die gottverflucht in h\u00f6llischem Getreibe,", "tokens": ["Die", "gott\u00b7ver\u00b7flucht", "in", "h\u00f6l\u00b7li\u00b7schem", "Ge\u00b7trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00fcnden selbst erzeugen, die sie \u00e4chten,", "tokens": ["Die", "S\u00fcn\u00b7den", "selbst", "er\u00b7zeu\u00b7gen", ",", "die", "sie", "\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auf das Rad den Reformator flechten,", "tokens": ["Und", "auf", "das", "Rad", "den", "Re\u00b7for\u00b7ma\u00b7tor", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er die alten Ketten nicht zerreibe:", "tokens": ["Da\u00df", "er", "die", "al\u00b7ten", "Ket\u00b7ten", "nicht", "zer\u00b7rei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da d\u00fcrfte dir das schlimmste deiner Glieder,", "tokens": ["Da", "d\u00fcrf\u00b7te", "dir", "das", "schlimms\u00b7te", "dei\u00b7ner", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Keck, wie es wollte, in die Augen schauen,", "tokens": ["Keck", ",", "wie", "es", "woll\u00b7te", ",", "in", "die", "Au\u00b7gen", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du m\u00fc\u00dftest ganz gewi\u00df vor ihm err\u00f6then!", "tokens": ["Du", "m\u00fc\u00df\u00b7test", "ganz", "ge\u00b7wi\u00df", "vor", "ihm", "er\u00b7r\u00f6\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der R\u00e4uber braucht die Faust nur hin und wieder,", "tokens": ["Der", "R\u00e4u\u00b7ber", "braucht", "die", "Faust", "nur", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der M\u00f6rder treibt sein Werk nicht ohne Grauen,", "tokens": ["Der", "M\u00f6r\u00b7der", "treibt", "sein", "Werk", "nicht", "oh\u00b7ne", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast das Amt, zu rauben und zu t\u00f6dten.", "tokens": ["Du", "hast", "das", "Amt", ",", "zu", "rau\u00b7ben", "und", "zu", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}