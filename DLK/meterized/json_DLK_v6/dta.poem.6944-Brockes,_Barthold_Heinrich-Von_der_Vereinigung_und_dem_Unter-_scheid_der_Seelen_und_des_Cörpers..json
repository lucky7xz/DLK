{"dta.poem.6944": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist jemand, welcher es noch nicht begreiffen kan?", "tokens": ["Ist", "je\u00b7mand", ",", "wel\u00b7cher", "es", "noch", "nicht", "be\u00b7greif\u00b7fen", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Blos durch die Regsamkeit, die wir ihr schencken,", "tokens": ["Blos", "durch", "die", "Reg\u00b7sam\u00b7keit", ",", "die", "wir", "ihr", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will man sie k\u00f6nnen sehn, verstehen und gedencken!", "tokens": ["Will", "man", "sie", "k\u00f6n\u00b7nen", "sehn", ",", "ver\u00b7ste\u00b7hen", "und", "ge\u00b7den\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "VMFIN", "VVINF", "$,", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es sollen Sonnen St\u00e4ubelein", "tokens": ["Es", "sol\u00b7len", "Son\u00b7nen", "St\u00e4u\u00b7be\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erleuchtet, fromm, beredt, klug, wei\u00df und sinnreich seyn.", "tokens": ["Er\u00b7leuch\u00b7tet", ",", "fromm", ",", "be\u00b7redt", ",", "klug", ",", "wei\u00df", "und", "sinn\u00b7reich", "seyn", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "VVFIN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}