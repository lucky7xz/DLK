{"dta.poem.13314": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ebetrachtet jedes Leibs Einstimmigkeit und Kraft!", "tokens": ["\u201e", "be\u00b7trach\u00b7tet", "je\u00b7des", "Leibs", "Ein\u00b7stim\u00b7mig\u00b7keit", "und", "Kraft", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso seht ihr, da\u00df kein Theil nicht andern Vortheil schafft,", "tokens": ["\u201e", "so", "seht", "ihr", ",", "da\u00df", "kein", "Theil", "nicht", "an\u00b7dern", "Vor\u00b7theil", "schafft", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "PTKNEG", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eund dannoch kann er nicht der andern Hilff entb\u00e4hren;", "tokens": ["\u201e", "und", "dan\u00b7noch", "kann", "er", "nicht", "der", "an\u00b7dern", "Hilff", "ent\u00b7b\u00e4h\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201esteht der nicht jenem bey? mu\u00df der nicht jenen n\u00e4hren?", "tokens": ["\u201e", "steht", "der", "nicht", "je\u00b7nem", "bey", "?", "mu\u00df", "der", "nicht", "je\u00b7nen", "n\u00e4h\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "PTKNEG", "PDAT", "PTKVZ", "$.", "VMFIN", "ART", "PTKNEG", "PDS", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "375\u201dDurchgeht das Meer, die Luft, der Erden Grund und Fl\u00e4che!", "tokens": ["\"", "Durch\u00b7geht", "das", "Meer", ",", "die", "Luft", ",", "der", "Er\u00b7den", "Grund", "und", "Fl\u00e4\u00b7che", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ejhr findet kein Gesch\u00f6pf, das meinen Vortrag schw\u00e4che.", "tokens": ["\u201e", "jhr", "fin\u00b7det", "kein", "Ge\u00b7sch\u00f6pf", ",", "das", "mei\u00b7nen", "Vor\u00b7trag", "schw\u00e4\u00b7che", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edas Herz erh\u00e4lt sich nicht als durch des Hirnes Geist;", "tokens": ["\u201e", "das", "Herz", "er\u00b7h\u00e4lt", "sich", "nicht", "als", "durch", "des", "Hir\u00b7nes", "Geist", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "KOKOM", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewogegen dieser sich nicht als durch jenes spei\u00dft.", "tokens": ["\u201e", "wo\u00b7ge\u00b7gen", "die\u00b7ser", "sich", "nicht", "als", "durch", "je\u00b7nes", "spei\u00dft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PDAT", "PRF", "PTKNEG", "KOKOM", "APPR", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewas treibt sie beyderseits in diese Gegenregung?", "tokens": ["\u201e", "was", "treibt", "sie", "bey\u00b7der\u00b7seits", "in", "die\u00b7se", "Ge\u00b7gen\u00b7re\u00b7gung", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "380\u201dDie Luft und ihrer Last stets druckende Bewegung.", "tokens": ["\"", "Die", "Luft", "und", "ih\u00b7rer", "Last", "stets", "dru\u00b7cken\u00b7de", "Be\u00b7we\u00b7gung", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eseht hin, wohin ihr wollt! es dienet jeder Krei\u00df", "tokens": ["\u201e", "seht", "hin", ",", "wo\u00b7hin", "ihr", "wollt", "!", "es", "die\u00b7net", "je\u00b7der", "Krei\u00df"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VMFIN", "$.", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ezu meiner Rede Grund, Behauptung, und Beweis;", "tokens": ["\u201e", "zu", "mei\u00b7ner", "Re\u00b7de", "Grund", ",", "Be\u00b7haup\u00b7tung", ",", "und", "Be\u00b7weis", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ees schwinget alles sich in Ordnung auf und nieder;", "tokens": ["\u201e", "es", "schwin\u00b7get", "al\u00b7les", "sich", "in", "Ord\u00b7nung", "auf", "und", "nie\u00b7der", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "PRF", "APPR", "NN", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ewas GOtt erschuff, geht nur zum Helffen hin und wieder.", "tokens": ["\u201e", "was", "Gott", "er\u00b7schuff", ",", "geht", "nur", "zum", "Helf\u00b7fen", "hin", "und", "wie\u00b7der", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "PTKVZ", "$,", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "385\u201dGewicht und Ma\u00df und Zahl, End, Anfang, Ziel und Zeit;", "tokens": ["\"", "Ge\u00b7wicht", "und", "Ma\u00df", "und", "Zahl", ",", "End", ",", "An\u00b7fang", ",", "Ziel", "und", "Zeit", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "KON", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201ekurz: alle Welt besteht in der Einstimmigkeit.", "tokens": ["\u201e", "kurz", ":", "al\u00b7le", "Welt", "be\u00b7steht", "in", "der", "Ein\u00b7stim\u00b7mig\u00b7keit", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edie Wercke seynd verkn\u00fcpft; allein kann keines n\u00fczen;", "tokens": ["\u201e", "die", "Wer\u00b7cke", "seynd", "ver\u00b7kn\u00fcpft", ";", "al\u00b7lein", "kann", "kei\u00b7nes", "n\u00fc\u00b7zen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$.", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201edes einen Wirckung mu\u00df das Werck des andern st\u00fczen.", "tokens": ["\u201e", "des", "ei\u00b7nen", "Wir\u00b7ckung", "mu\u00df", "das", "Werck", "des", "an\u00b7dern", "st\u00fc\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ART", "NN", "VMFIN", "ART", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201ek\u00f6nnt eine Sach allein und sonderbar bestehn,", "tokens": ["\u201e", "k\u00f6nnt", "ei\u00b7ne", "Sach", "al\u00b7lein", "und", "son\u00b7der\u00b7bar", "be\u00b7stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ADV", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "390\u201dSo m\u00fc\u00dfte sie der Macht der Allmacht nahe gehn.", "tokens": ["\"", "So", "m\u00fc\u00df\u00b7te", "sie", "der", "Macht", "der", "All\u00b7macht", "na\u00b7he", "gehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}