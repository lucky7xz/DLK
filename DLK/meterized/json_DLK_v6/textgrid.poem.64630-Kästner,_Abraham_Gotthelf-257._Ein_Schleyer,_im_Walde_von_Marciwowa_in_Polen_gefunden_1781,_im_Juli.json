{"textgrid.poem.64630": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "257. Ein Schleyer, im Walde von Marciwowa in Polen gefunden 1781, im Juli", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gefunden hab' ich ihn, doch wo meinst du gefunden?", "tokens": ["Ge\u00b7fun\u00b7den", "hab'", "ich", "ihn", ",", "doch", "wo", "meinst", "du", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "KON", "PWAV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In mondhellen mittern\u00e4chtlichen Stunden?", "tokens": ["In", "mond\u00b7hel\u00b7len", "mit\u00b7ter\u00b7n\u00e4cht\u00b7li\u00b7chen", "Stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von einer L\u00f6winn Mund in Sand gew\u00fchlt,", "tokens": ["Von", "ei\u00b7ner", "L\u00f6\u00b7winn", "Mund", "in", "Sand", "ge\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die ihren Durst am Brunnen gek\u00fchlt?", "tokens": ["Die", "ih\u00b7ren", "Durst", "am", "Brun\u00b7nen", "ge\u00b7k\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Von grimmen W\u00f6lfen, von Zeidelb\u00e4ren?", "tokens": ["Von", "grim\u00b7men", "W\u00f6l\u00b7fen", ",", "von", "Zei\u00b7del\u00b7b\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nein! - im Brevier eines Trinit\u00e4ren", "tokens": ["Nein", "!", "im", "Bre\u00b7vier", "ei\u00b7nes", "Tri\u00b7ni\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "$(", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}