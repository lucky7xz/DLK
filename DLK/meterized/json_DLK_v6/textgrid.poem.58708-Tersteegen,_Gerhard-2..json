{"textgrid.poem.58708": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Pfeile eines Starken sind scharf. Psalm 120, 4", "tokens": ["Die", "Pfei\u00b7le", "ei\u00b7nes", "Star\u00b7ken", "sind", "scharf", ".", "Psalm", "120", ",", "4"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$.", "NN", "CARD", "$,", "CARD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du heil'ge Gotteslieb', wie ist mir's so vergn\u00fcgt,", "tokens": ["Du", "heil'\u00b7ge", "Got\u00b7tes\u00b7lieb'", ",", "wie", "ist", "mir's", "so", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWAV", "VAFIN", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich seh', wie dein Pfeil abfliegt!", "tokens": ["Wenn", "ich", "seh'", ",", "wie", "dein", "Pfeil", "ab\u00b7fliegt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ach m\u00f6cht' es meinem Wunsch gelingen,", "tokens": ["Ach", "m\u00f6cht'", "es", "mei\u00b7nem", "Wunsch", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6cht' er mich treffen und mein Herz nur tief durchdringen!", "tokens": ["M\u00f6cht'", "er", "mich", "tref\u00b7fen", "und", "mein", "Herz", "nur", "tief", "durch\u00b7drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "KON", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist geschehn, o ja, mein Herz ist schon erlegt,", "tokens": ["Es", "ist", "ge\u00b7schehn", ",", "o", "ja", ",", "mein", "Herz", "ist", "schon", "er\u00b7legt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "FM", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des liebsten Meisters Pfeil trifft's recht;", "tokens": ["Des", "liebs\u00b7ten", "Meis\u00b7ters", "Pfeil", "trifft's", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Solang ich hoch stund auf der Stange,", "tokens": ["So\u00b7lang", "ich", "hoch", "stund", "auf", "der", "Stan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War ich vor fremden Pfeilen bange.", "tokens": ["War", "ich", "vor", "frem\u00b7den", "Pfei\u00b7len", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Siegreiche Allmachtshand, du wirst schon vor Gefahren,", "tokens": ["Sieg\u00b7rei\u00b7che", "All\u00b7machts\u00b7hand", ",", "du", "wirst", "schon", "vor", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du erobert hast, bewahren;", "tokens": ["Was", "du", "er\u00b7o\u00b7bert", "hast", ",", "be\u00b7wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die blinde Weltlieb' mit verbundenem Gesicht", "tokens": ["Die", "blin\u00b7de", "Welt\u00b7lieb'", "mit", "ver\u00b7bun\u00b7de\u00b7nem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kann in die H\u00f6he sehen nicht.", "tokens": ["Kann", "in", "die", "H\u00f6\u00b7he", "se\u00b7hen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weg eitle Lieb', dein falsch Vergn\u00fcgen", "tokens": ["Weg", "eit\u00b7le", "Lieb'", ",", "dein", "falsch", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Soll nimmermehr mein Herz betr\u00fcgen!", "tokens": ["Soll", "nim\u00b7mer\u00b7mehr", "mein", "Herz", "be\u00b7tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}