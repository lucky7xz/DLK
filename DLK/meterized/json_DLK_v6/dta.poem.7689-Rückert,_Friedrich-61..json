{"dta.poem.7689": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nie such' ich in der Nacht den Schlummer auf den Pf\u00fchlen,", "tokens": ["Nie", "such'", "ich", "in", "der", "Nacht", "den", "Schlum\u00b7mer", "auf", "den", "Pf\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' erst mein liebstes Kind mit H\u00e4nden anzuf\u00fchlen.", "tokens": ["Ohn'", "erst", "mein", "liebs\u00b7tes", "Kind", "mit", "H\u00e4n\u00b7den", "an\u00b7zu\u00b7f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn ich ihm bef\u00fchlt die Hand und das Gesicht", "tokens": ["Und", "wenn", "ich", "ihm", "be\u00b7f\u00fchlt", "die", "Hand", "und", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Dunkeln, ists genug, zu sehen brauch' ichs nicht.", "tokens": ["Im", "Dun\u00b7keln", ",", "ists", "ge\u00b7nug", ",", "zu", "se\u00b7hen", "brauch'", "ichs", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "VAFIN", "ADV", "$,", "PTKZU", "VVINF", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwar wei\u00df ich wohl, nicht wird ihm die Ber\u00fchrung n\u00fctzen,", "tokens": ["Zwar", "wei\u00df", "ich", "wohl", ",", "nicht", "wird", "ihm", "die", "Be\u00b7r\u00fch\u00b7rung", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PTKNEG", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn bessre M\u00e4chte nicht die Nacht-durch es besch\u00fctzen.", "tokens": ["Wenn", "bess\u00b7re", "M\u00e4ch\u00b7te", "nicht", "die", "Nacht\u00b7durch", "es", "be\u00b7sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch bildet' ich mir ein, h\u00e4tt' ich es je vers\u00e4umt,", "tokens": ["Doch", "bil\u00b7det'", "ich", "mir", "ein", ",", "h\u00e4tt'", "ich", "es", "je", "ver\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich h\u00e4tte b\u00f6ser Macht den Spielraum einger\u00e4umt.", "tokens": ["Ich", "h\u00e4t\u00b7te", "b\u00f6\u00b7ser", "Macht", "den", "Spiel\u00b7raum", "ein\u00b7ge\u00b7r\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und h\u00e4tt' es deshalb auch nicht minder wohl geruht,", "tokens": ["Und", "h\u00e4tt'", "es", "des\u00b7halb", "auch", "nicht", "min\u00b7der", "wohl", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "ADV", "PTKNEG", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschlafen h\u00e4tt' ich selbst darum doch minder gut.", "tokens": ["Ge\u00b7schla\u00b7fen", "h\u00e4tt'", "ich", "selbst", "da\u00b7rum", "doch", "min\u00b7der", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "PAV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}