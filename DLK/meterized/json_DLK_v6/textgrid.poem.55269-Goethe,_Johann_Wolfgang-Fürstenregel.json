{"textgrid.poem.55269": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcrstenregel", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollen die Menschen nicht denken und dichten,", "tokens": ["Sol\u00b7len", "die", "Men\u00b7schen", "nicht", "den\u00b7ken", "und", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "KON", "ADJA", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "M\u00fc\u00dft ihr ihnen ein lustig Leben errichten;", "tokens": ["M\u00fc\u00dft", "ihr", "ih\u00b7nen", "ein", "lus\u00b7tig", "Le\u00b7ben", "er\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wollt ihr ihnen aber wahrhaft n\u00fctzen,", "tokens": ["Wollt", "ihr", "ih\u00b7nen", "a\u00b7ber", "wahr\u00b7haft", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "So m\u00fc\u00dft ihr sie scheren und sie besch\u00fctzen.", "tokens": ["So", "m\u00fc\u00dft", "ihr", "sie", "sche\u00b7ren", "und", "sie", "be\u00b7sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "KON", "PPER", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}