{"dta.poem.219": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie w\u00fcnschet allezeit in ihres Liebsten  \n Armen zu ruhen.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Kan ich immer mit Vergn\u00fcgen", "tokens": ["Kan", "ich", "im\u00b7mer", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In so sch\u00f6nen Armen liegen", "tokens": ["In", "so", "sch\u00f6\u00b7nen", "Ar\u00b7men", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich \u00fcberaus begl\u00fcckt.", "tokens": ["Bin", "ich", "\u00fc\u00b7be\u00b7raus", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwar ein angenehmes K\u00fcssen", "tokens": ["Zwar", "ein", "an\u00b7ge\u00b7neh\u00b7mes", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4st viel S\u00fcssigkeiten wissen;", "tokens": ["L\u00e4st", "viel", "S\u00fcs\u00b7sig\u00b7kei\u00b7ten", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn man aber gantz entz\u00fcckt/", "tokens": ["Wenn", "man", "a\u00b7ber", "gantz", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Haut auf Haut zusammen dr\u00fcckt/", "tokens": ["Haut", "auf", "Haut", "zu\u00b7sam\u00b7men", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kan ich wol die Lippen missen.", "tokens": ["Kan", "ich", "wol", "die", "Lip\u00b7pen", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kan ich immer mit Vergn\u00fcgen", "tokens": ["Kan", "ich", "im\u00b7mer", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In so sch\u00f6nen Armen liegen", "tokens": ["In", "so", "sch\u00f6\u00b7nen", "Ar\u00b7men", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Bin ich \u00fcberaus begl\u00fcckt.", "tokens": ["Bin", "ich", "\u00fc\u00b7be\u00b7raus", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Sich umarmen", "tokens": ["Sich", "um\u00b7ar\u00b7men"], "token_info": ["word", "word"], "pos": ["PRF", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Und erwarmen", "tokens": ["Und", "er\u00b7war\u00b7men"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Aneinander eingeschrenckt/", "tokens": ["An\u00b7ein\u00b7an\u00b7der", "ein\u00b7ge\u00b7schrenckt", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ist/ und bleibt die h\u00f6chste Lust/", "tokens": ["Ist", "/", "und", "bleibt", "die", "h\u00f6chs\u00b7te", "Lust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "So die Brust", "tokens": ["So", "die", "Brust"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.17": {"text": "Mit Anmuth tr\u00e4nckt.", "tokens": ["Mit", "An\u00b7muth", "tr\u00e4nckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "In dem Schertzen/ in dem Lachen/", "tokens": ["In", "dem", "Schert\u00b7zen", "/", "in", "dem", "La\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn zwey Seelen Hochzeit machen", "tokens": ["Wenn", "zwey", "See\u00b7len", "Hoch\u00b7zeit", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Liebe selbst versteckt.", "tokens": ["Ist", "die", "Lie\u00b7be", "selbst", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was man sonst so sehr verborgen/", "tokens": ["Was", "man", "sonst", "so", "sehr", "ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird alsdenn ohn alle Sorgen", "tokens": ["Wird", "als\u00b7denn", "ohn", "al\u00b7le", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen Liebsten aufgedeckt.", "tokens": ["Ei\u00b7nen", "Liebs\u00b7ten", "auf\u00b7ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem Schertzen in dem Lachen/", "tokens": ["In", "dem", "Schert\u00b7zen", "in", "dem", "La\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist die Liebe selbst versteckt.", "tokens": ["Ist", "die", "Lie\u00b7be", "selbst", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}