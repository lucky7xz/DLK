{"dta.poem.3850": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf 6 liebe Kinder.  \n  H. Michlenderleins.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Hier decket dieses Kirchhofs Sand", "tokens": ["Hier", "de\u00b7cket", "die\u00b7ses", "Kirch\u00b7hofs", "Sand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sechs Kinder redlich frommer Eltern,", "tokens": ["Sechs", "Kin\u00b7der", "red\u00b7lich", "from\u00b7mer", "El\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Angst- und Wehmuths-Thr\u00e4nen keltern,", "tokens": ["Die", "Angst", "und", "Weh\u00b7muths\u00b7Thr\u00e4\u00b7nen", "kel\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sind im frohen Vaterland;", "tokens": ["Sie", "sind", "im", "fro\u00b7hen", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort in den sch\u00f6nsten Himmels-Auen", "tokens": ["Dort", "in", "den", "sch\u00f6ns\u00b7ten", "Him\u00b7mels\u00b7Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Solt ihr sie einsten wieder schauen,", "tokens": ["Solt", "ihr", "sie", "eins\u00b7ten", "wie\u00b7der", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie ruffen aus der Himmels-Pracht:", "tokens": ["Sie", "ruf\u00b7fen", "aus", "der", "Him\u00b7mels\u00b7Pracht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Betr\u00fcbte Eltern, gute Nacht!", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "El\u00b7tern", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}