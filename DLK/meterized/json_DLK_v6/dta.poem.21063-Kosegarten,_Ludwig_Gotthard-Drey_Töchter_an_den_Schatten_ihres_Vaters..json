{"dta.poem.21063": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Drey T\u00f6chter  \n an den Schatten ihres Vaters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vater, ach, wo schwebst du itzt               ", "tokens": ["Va\u00b7ter", ",", "ach", ",", "wo", "schwebst", "du", "itzt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem weiten Raum der Welten?", "tokens": ["In", "dem", "wei\u00b7ten", "Raum", "der", "Wel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der Sonne Strahl erblitzt?", "tokens": ["Wo", "der", "Son\u00b7ne", "Strahl", "er\u00b7blitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder, wo in fernen Belten", "tokens": ["O\u00b7der", ",", "wo", "in", "fer\u00b7nen", "Bel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie ihr flammend Antlitz w\u00e4scht,", "tokens": ["Sie", "ihr", "flam\u00b7mend", "Ant\u00b7litz", "w\u00e4scht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Brand der Wangen l\u00f6scht?", "tokens": ["Und", "den", "Brand", "der", "Wan\u00b7gen", "l\u00f6scht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwimmst du in dem Ocean", "tokens": ["Schwimmst", "du", "in", "dem", "O\u00b7cean"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jener klaren Himmelferne?", "tokens": ["Je\u00b7ner", "kla\u00b7ren", "Him\u00b7mel\u00b7fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reisest du die hohe Bahn", "tokens": ["Rei\u00b7sest", "du", "die", "ho\u00b7he", "Bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stiller Monden, milder Sterne?", "tokens": ["Stil\u00b7ler", "Mon\u00b7den", ",", "mil\u00b7der", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem bleichen Uranus", "tokens": ["Von", "dem", "blei\u00b7chen", "U\u00b7ra\u00b7nus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis zum funkelnden Sirius?", "tokens": ["Bis", "zum", "fun\u00b7keln\u00b7den", "Si\u00b7rius", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder ruhst du sanft und sch\u00f6n", "tokens": ["O\u00b7der", "ruhst", "du", "sanft", "und", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Paradieses Auen,", "tokens": ["In", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Weste w\u00fcrzig wehn,", "tokens": ["Wo", "die", "Wes\u00b7te", "w\u00fcr\u00b7zig", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Fluren Nektar thauen,", "tokens": ["Wo", "die", "Flu\u00b7ren", "Nek\u00b7tar", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der Bach melodisch quillt,", "tokens": ["Wo", "der", "Bach", "me\u00b7lo\u00b7disch", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Harmonie die L\u00fcfte f\u00fcllt?", "tokens": ["Har\u00b7mo\u00b7nie", "die", "L\u00fcf\u00b7te", "f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hast du etwa \u2014 o der Lust! \u2014", "tokens": ["Hast", "du", "et\u00b7wa", "o", "der", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$(", "FM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort die Traute wiederfunden,", "tokens": ["Dort", "die", "Trau\u00b7te", "wie\u00b7der\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und geschmiegt an ihre Brust,", "tokens": ["Und", "ge\u00b7schmiegt", "an", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom getreuen Arm umwunden,", "tokens": ["Vom", "ge\u00b7treu\u00b7en", "Arm", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Horchst du nun in s\u00fcsser Ruh", "tokens": ["Horchst", "du", "nun", "in", "s\u00fcs\u00b7ser", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem Gesang der Sph\u00e4ren zu?", "tokens": ["Dem", "Ge\u00b7sang", "der", "Sph\u00e4\u00b7ren", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Theurer Vater, gern, ach gern,", "tokens": ["Theu\u00b7rer", "Va\u00b7ter", ",", "gern", ",", "ach", "gern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADV", "$,", "ITJ", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6nnen wir dir deine Freuden.", "tokens": ["G\u00f6n\u00b7nen", "wir", "dir", "dei\u00b7ne", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch d\u00fcnkst du uns so fern;", "tokens": ["Den\u00b7noch", "d\u00fcnkst", "du", "uns", "so", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch schmerzet uns dein Scheiden.", "tokens": ["Den\u00b7noch", "schmer\u00b7zet", "uns", "dein", "Schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rastlos schauet unser Blick", "tokens": ["Rast\u00b7los", "schau\u00b7et", "un\u00b7ser", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sehnsuchtvoll nach dir zur\u00fcck.", "tokens": ["Sehn\u00b7sucht\u00b7voll", "nach", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wann der goldne Tag uns weckt,", "tokens": ["Wann", "der", "gold\u00b7ne", "Tag", "uns", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00c4chzen wir: \u201eWo bist du, Vater?\u201c", "tokens": ["\u00c4ch\u00b7zen", "wir", ":", "\u201e", "Wo", "bist", "du", ",", "Va\u00b7ter", "?", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "PPER", "$.", "$(", "PWAV", "VAFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann die heilige Nacht uns deckt,", "tokens": ["Wann", "die", "hei\u00b7li\u00b7ge", "Nacht", "uns", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Seufzen wir: \u201eSchlaf ruhig, Vater!\u201c", "tokens": ["Seuf\u00b7zen", "wir", ":", "\u201e", "Schlaf", "ru\u00b7hig", ",", "Va\u00b7ter", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "PPER", "$.", "$(", "NN", "ADJD", "$,", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "In dem sanften Mondenschein", "tokens": ["In", "dem", "sanf\u00b7ten", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denken wir mit Wehmuth \u2014 dein.", "tokens": ["Den\u00b7ken", "wir", "mit", "Weh\u00b7muth", "dein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$(", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jedes W\u00f6lkchen, licht und sch\u00f6n,", "tokens": ["Je\u00b7des", "W\u00f6lk\u00b7chen", ",", "licht", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4ucht uns deiner Locken Kr\u00e4useln;", "tokens": ["D\u00e4ucht", "uns", "dei\u00b7ner", "Lo\u00b7cken", "Kr\u00e4u\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes L\u00fcftchens leises Wehn", "tokens": ["Je\u00b7des", "L\u00fcft\u00b7chens", "lei\u00b7ses", "Wehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcnkt uns deiner Stimme S\u00e4useln;", "tokens": ["D\u00fcnkt", "uns", "dei\u00b7ner", "Stim\u00b7me", "S\u00e4u\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Glanz, der uns umwallt,", "tokens": ["Je\u00b7der", "Glanz", ",", "der", "uns", "um\u00b7wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00fcnkt uns deine Lichtgestalt.", "tokens": ["D\u00fcnkt", "uns", "dei\u00b7ne", "Licht\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O, von deinem Stern herab", "tokens": ["O", ",", "von", "dei\u00b7nem", "Stern", "her\u00b7ab"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau auf die verwaisten T\u00f6chter!", "tokens": ["Schau", "auf", "die", "ver\u00b7wais\u00b7ten", "T\u00f6ch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Freundlich schau' auf uns herab,", "tokens": ["Freund\u00b7lich", "schau'", "auf", "uns", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey uns Schutzgeist, sey uns W\u00e4chter,", "tokens": ["Sey", "uns", "Schutz\u00b7geist", ",", "sey", "uns", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf der Reise durch diess Land", "tokens": ["Auf", "der", "Rei\u00b7se", "durch", "diess", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Voller Thr\u00e4nen, Traum und Tand!", "tokens": ["Vol\u00b7ler", "Thr\u00e4\u00b7nen", ",", "Traum", "und", "Tand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}