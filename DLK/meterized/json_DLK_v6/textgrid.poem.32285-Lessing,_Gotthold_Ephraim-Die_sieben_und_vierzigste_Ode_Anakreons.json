{"textgrid.poem.32285": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die sieben und vierzigste Ode Anakreons", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alter tanze! Wenn du tanzest,", "tokens": ["Al\u00b7ter", "tan\u00b7ze", "!", "Wenn", "du", "tan\u00b7zest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alter, so gef\u00e4llst du mir!", "tokens": ["Al\u00b7ter", ",", "so", "ge\u00b7f\u00e4llst", "du", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "J\u00fcngling, tanze! Wenn du tanzest,", "tokens": ["J\u00fcng\u00b7ling", ",", "tan\u00b7ze", "!", "Wenn", "du", "tan\u00b7zest", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00fcngling, so gef\u00e4llst du mir.", "tokens": ["J\u00fcng\u00b7ling", ",", "so", "ge\u00b7f\u00e4llst", "du", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alter, tanze, trotz den Jahren!", "tokens": ["Al\u00b7ter", ",", "tan\u00b7ze", ",", "trotz", "den", "Jah\u00b7ren", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche Freude, wenn es hei\u00dft:", "tokens": ["Wel\u00b7che", "Freu\u00b7de", ",", "wenn", "es", "hei\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alter, du bist alt an Haaren,", "tokens": ["Al\u00b7ter", ",", "du", "bist", "alt", "an", "Haa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchend aber ist dein Geist!", "tokens": ["Bl\u00fc\u00b7hend", "a\u00b7ber", "ist", "dein", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}