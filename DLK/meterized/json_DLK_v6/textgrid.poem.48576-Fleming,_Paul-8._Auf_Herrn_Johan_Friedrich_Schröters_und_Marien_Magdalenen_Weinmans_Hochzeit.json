{"textgrid.poem.48576": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Auf Herrn Johan Friedrich Schr\u00f6ters und Marien Magdalenen Weinmans Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute sind der G\u00f6tter Schaaren", "tokens": ["Heu\u00b7te", "sind", "der", "G\u00f6t\u00b7ter", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ausspazieret allzumal,", "tokens": ["aus\u00b7spa\u00b7zie\u00b7ret", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "haben sich verf\u00fcgt bei Paaren", "tokens": ["ha\u00b7ben", "sich", "ver\u00b7f\u00fcgt", "bei", "Paa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "in den weiten Sternensal,", "tokens": ["in", "den", "wei\u00b7ten", "Ster\u00b7nen\u00b7sal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "pflocken Blumen, winden Kr\u00e4nze,", "tokens": ["pflo\u00b7cken", "Blu\u00b7men", ",", "win\u00b7den", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fchren liebe Lobet\u00e4nze.", "tokens": ["f\u00fch\u00b7ren", "lie\u00b7be", "Lo\u00b7be\u00b7t\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Venus herzet ihren Bulen,", "tokens": ["Ve\u00b7nus", "her\u00b7zet", "ih\u00b7ren", "Bu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mars vertauscht den roten Streit,", "tokens": ["Mars", "ver\u00b7tauscht", "den", "ro\u00b7ten", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zynthius die blassen Schulen", "tokens": ["Zynt\u00b7hi\u00b7us", "die", "blas\u00b7sen", "Schu\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mit der s\u00fc\u00dfen M\u00fc\u00dfigkeit,", "tokens": ["mit", "der", "s\u00fc\u00b7\u00dfen", "M\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "pflocken Blumen, winden Kr\u00e4nze,", "tokens": ["pflo\u00b7cken", "Blu\u00b7men", ",", "win\u00b7den", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fchren liebe Lobet\u00e4nze.", "tokens": ["f\u00fch\u00b7ren", "lie\u00b7be", "Lo\u00b7be\u00b7t\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zeres springt auf allen Rainen", "tokens": ["Ze\u00b7res", "springt", "auf", "al\u00b7len", "Rai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mit der frohen Bauerwelt,", "tokens": ["mit", "der", "fro\u00b7hen", "Bau\u00b7er\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "um die Tennen, um die Scheunen,", "tokens": ["um", "die", "Ten\u00b7nen", ",", "um", "die", "Scheu\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "um das abgetane Feld,", "tokens": ["um", "das", "ab\u00b7ge\u00b7ta\u00b7ne", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "pflocket Blumen, windet Kr\u00e4nze,", "tokens": ["pflo\u00b7cket", "Blu\u00b7men", ",", "win\u00b7det", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fchret liebe Lobet\u00e4nze.", "tokens": ["f\u00fch\u00b7ret", "lie\u00b7be", "Lo\u00b7be\u00b7t\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr auch, G\u00f6tter und G\u00f6ttinnen,", "tokens": ["Ihr", "auch", ",", "G\u00f6t\u00b7ter", "und", "G\u00f6t\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nur nicht ohne Sterbligkeit,", "tokens": ["nur", "nicht", "oh\u00b7ne", "Ster\u00b7blig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "lebet nach des Himmels Sinnen,", "tokens": ["le\u00b7bet", "nach", "des", "Him\u00b7mels", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dem ihr sonst ganz \u00e4hnlich seid,", "tokens": ["dem", "ihr", "sonst", "ganz", "\u00e4hn\u00b7lich", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "pflocket Blumen, windet Kr\u00e4nze,", "tokens": ["pflo\u00b7cket", "Blu\u00b7men", ",", "win\u00b7det", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fchret liebe Lobet\u00e4nze!", "tokens": ["f\u00fch\u00b7ret", "lie\u00b7be", "Lo\u00b7be\u00b7t\u00e4n\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier steht riechender Lavendel,", "tokens": ["Hier", "steht", "rie\u00b7chen\u00b7der", "La\u00b7ven\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "da gesunde Saturei,", "tokens": ["da", "ge\u00b7sun\u00b7de", "Sa\u00b7tu\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eiswig, Polei, Narde, Quendel,", "tokens": ["Eis\u00b7wig", ",", "Po\u00b7lei", ",", "Nar\u00b7de", ",", "Quen\u00b7del", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausentsch\u00f6n und Allerlei.", "tokens": ["Tau\u00b7sent\u00b7sch\u00f6n", "und", "Al\u00b7ler\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pflocket Blumen, windet Kr\u00e4nze,", "tokens": ["Pflo\u00b7cket", "Blu\u00b7men", ",", "win\u00b7det", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fchret liebe Lobet\u00e4nze!", "tokens": ["f\u00fch\u00b7ret", "lie\u00b7be", "Lo\u00b7be\u00b7t\u00e4n\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "M\u00fcnze, Ro\u00dfmarin, Zypressen,", "tokens": ["M\u00fcn\u00b7ze", ",", "Ro\u00df\u00b7ma\u00b7rin", ",", "Zyp\u00b7res\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Nelken, Scharlach, Amaranth", "tokens": ["Nel\u00b7ken", ",", "Schar\u00b7lach", ",", "A\u00b7ma\u00b7ran\u00b7th"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "bleiben gleichsfals unvergessen,", "tokens": ["blei\u00b7ben", "gleichs\u00b7fals", "un\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und was noch nicht ist genannt.", "tokens": ["und", "was", "noch", "nicht", "ist", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pflocket Blumen, windet Kr\u00e4nze,", "tokens": ["Pflo\u00b7cket", "Blu\u00b7men", ",", "win\u00b7det", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fchret liebe Lobet\u00e4nze!", "tokens": ["f\u00fch\u00b7ret", "lie\u00b7be", "Lo\u00b7be\u00b7t\u00e4n\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Pflocket, windet um die Wette!", "tokens": ["Pflo\u00b7cket", ",", "win\u00b7det", "um", "die", "Wet\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles soll von Farben sein.", "tokens": ["Al\u00b7les", "soll", "von", "Far\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchret auf ein Blumen-Bette,", "tokens": ["F\u00fch\u00b7ret", "auf", "ein", "Blu\u00b7men\u00b7Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "legt die zwei Verliebten drein!", "tokens": ["legt", "die", "zwei", "Ver\u00b7lieb\u00b7ten", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pflocket Blumen, windet Kr\u00e4nze,", "tokens": ["Pflo\u00b7cket", "Blu\u00b7men", ",", "win\u00b7det", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fchret liebe Lobet\u00e4nze!", "tokens": ["f\u00fch\u00b7ret", "lie\u00b7be", "Lo\u00b7be\u00b7t\u00e4n\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Legt sie drein! Pflockt, windet immer,", "tokens": ["Legt", "sie", "drein", "!", "Pflockt", ",", "win\u00b7det", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "streuet auf das liebe Paar,", "tokens": ["streu\u00b7et", "auf", "das", "lie\u00b7be", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "tanzet um ihr buntes Zimmer", "tokens": ["tan\u00b7zet", "um", "ihr", "bun\u00b7tes", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und umschr\u00e4nkt sie ganz und gar!", "tokens": ["und", "um\u00b7schr\u00e4nkt", "sie", "ganz", "und", "gar", "!"], "token_info": ["word", "word", "word", "word", "w