{"textgrid.poem.40146": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Allgegenwart", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ich auch fern dir,", "tokens": ["Bin", "ich", "auch", "fern", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "bin ich doch ", "tokens": ["bin", "ich", "doch"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "denn ", "tokens": ["denn"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "In jedem Herzschlag,", "tokens": ["In", "je\u00b7dem", "Herz\u00b7schlag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "der mich durchwebt,", "tokens": ["der", "mich", "durch\u00b7webt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "bist Du's, die bebet,", "tokens": ["bist", "Du's", ",", "die", "be\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "bist du's, die lebt.", "tokens": ["bist", "du's", ",", "die", "lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Aus jedem Atemzug", "tokens": ["Aus", "je\u00b7dem", "A\u00b7tem\u00b7zug"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ringsum der Allnatur", "tokens": ["ring\u00b7sum", "der", "All\u00b7na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "weht mir entgegen", "tokens": ["weht", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dein Odem nur, \u2013", "tokens": ["Dein", "O\u00b7dem", "nur", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "die wir belauscht, beahnt,", "tokens": ["die", "wir", "be\u00b7lauscht", ",", "be\u00b7ahnt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "die wir erf\u00fchlten:", "tokens": ["die", "wir", "er\u00b7f\u00fchl\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Alles in Allem mahnt,", "tokens": ["Al\u00b7les", "in", "Al\u00b7lem", "mahnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIS", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Seele, an Dich.", "tokens": ["See\u00b7le", ",", "an", "Dich", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "dich in dem Sausen,", "tokens": ["dich", "in", "dem", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "dich in dem Brausen", "tokens": ["dich", "in", "dem", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "des Winds im Wald, \u2013", "tokens": ["des", "Winds", "im", "Wald", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "f\u00fchl' in dem Wisperlaut", "tokens": ["f\u00fchl'", "in", "dem", "Wis\u00b7per\u00b7laut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "alles des Lebens Dich,", "tokens": ["al\u00b7les", "des", "Le\u00b7bens", "Dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "in dem Gefl\u00fcster traut", "tokens": ["in", "dem", "Ge\u00b7fl\u00fcs\u00b7ter", "traut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "der Wesen Dich, \u2013", "tokens": ["der", "We\u00b7sen", "Dich", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "dich in der Wiesenluft,", "tokens": ["dich", "in", "der", "Wie\u00b7sen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "dich in dem Wasserduft,", "tokens": ["dich", "in", "dem", "Was\u00b7ser\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "die mich umh\u00fcllen", "tokens": ["die", "mich", "um\u00b7h\u00fcl\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "wie Du so still, \u2013", "tokens": ["wie", "Du", "so", "still", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Dich in dem Sonnenstrahl,", "tokens": ["Dich", "in", "dem", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "der mir ins Auge bricht,", "tokens": ["der", "mir", "ins", "Au\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich, meine Sonne,", "tokens": ["Dich", ",", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "mein Lebenslicht, \u2013", "tokens": ["mein", "Le\u00b7bens\u00b7licht", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Dich in der Thr\u00e4ne jetzt,", "tokens": ["Dich", "in", "der", "Thr\u00e4\u00b7ne", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "die mir vom Herzen schleicht,", "tokens": ["die", "mir", "vom", "Her\u00b7zen", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sehnend zitternd", "tokens": ["seh\u00b7nend", "zit\u00b7ternd"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "zum Auge steigt.", "tokens": ["zum", "Au\u00b7ge", "steigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}