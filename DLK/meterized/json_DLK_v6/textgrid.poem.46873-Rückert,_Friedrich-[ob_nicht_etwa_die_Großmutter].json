{"textgrid.poem.46873": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ob nicht etwa die Gro\u00dfmutter]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob nicht etwa die Gro\u00dfmutter", "tokens": ["Ob", "nicht", "et\u00b7wa", "die", "Gro\u00df\u00b7mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00fchlt zwiefaltig ihr Herz brechen,", "tokens": ["F\u00fchlt", "zwie\u00b7fal\u00b7tig", "ihr", "Herz", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "F\u00fchlt zwiespaltig den Schmerz stechen?", "tokens": ["F\u00fchlt", "zwies\u00b7pal\u00b7tig", "den", "Schmerz", "ste\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die zum Enkel, dem Gro\u00dfkinde,", "tokens": ["Die", "zum", "En\u00b7kel", ",", "dem", "Gro\u00df\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich nat\u00fcrlichen Trieb hatte,", "tokens": ["Gleich", "na\u00b7t\u00fcr\u00b7li\u00b7chen", "Trieb", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Gleich der Mutter ihn lieb hatte,", "tokens": ["Gleich", "der", "Mut\u00b7ter", "ihn", "lieb", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und dazu noch ihr Kind liebet,", "tokens": ["Und", "da\u00b7zu", "noch", "ihr", "Kind", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und nun siehet ihr Kind leiden,", "tokens": ["Und", "nun", "sie\u00b7het", "ihr", "Kind", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und den Enkel dahinscheiden!", "tokens": ["Und", "den", "En\u00b7kel", "da\u00b7hin\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Soll noch gr\u00f6\u00dfer der Gro\u00dfmutter", "tokens": ["Soll", "noch", "gr\u00f6\u00b7\u00dfer", "der", "Gro\u00df\u00b7mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Als der Mutter Verlust hei\u00dfen?", "tokens": ["Als", "der", "Mut\u00b7ter", "Ver\u00b7lust", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wer kann ihn aus der Brust rei\u00dfen?", "tokens": ["Wer", "kann", "ihn", "aus", "der", "Brust", "rei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Der die Mutter f\u00fcr's Kind tr\u00f6stet,", "tokens": ["Der", "die", "Mut\u00b7ter", "f\u00fcr's", "Kind", "tr\u00f6s\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Tr\u00f6ste dich f\u00fcr das Gro\u00dfkindchen,", "tokens": ["Tr\u00f6s\u00b7te", "dich", "f\u00fcr", "das", "Gro\u00df\u00b7kind\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Deinen Liebling, das Scho\u00dfkindchen.", "tokens": ["Dei\u00b7nen", "Lieb\u00b7ling", ",", "das", "Scho\u00df\u00b7kind\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}}}}}