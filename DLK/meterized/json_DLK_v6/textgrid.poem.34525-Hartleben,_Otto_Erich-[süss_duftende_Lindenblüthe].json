{"textgrid.poem.34525": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[s\u00fcss duftende Lindenbl\u00fcthe]", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcss duftende Lindenbl\u00fcthe", "tokens": ["S\u00fcss", "duf\u00b7ten\u00b7de", "Lin\u00b7den\u00b7bl\u00fct\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "in quellender Juninacht.", "tokens": ["in", "quel\u00b7len\u00b7der", "Ju\u00b7ni\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eine Wonne aus meinem Gem\u00fcthe", "tokens": ["Ei\u00b7ne", "Won\u00b7ne", "aus", "mei\u00b7nem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "ist mir in Sinnen erwacht.", "tokens": ["ist", "mir", "in", "Sin\u00b7nen", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Als kl\u00e4nge vor meinen Ohren", "tokens": ["Als", "kl\u00e4n\u00b7ge", "vor", "mei\u00b7nen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "leise das Lied vom Gl\u00fcck,", "tokens": ["lei\u00b7se", "das", "Lied", "vom", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "als t\u00f6ne, die lange verloren,", "tokens": ["als", "t\u00f6\u00b7ne", ",", "die", "lan\u00b7ge", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "die Jugend leise zur\u00fcck.", "tokens": ["die", "Ju\u00b7gend", "lei\u00b7se", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "S\u00fcss duftende Lindenbl\u00fcthe", "tokens": ["S\u00fcss", "duf\u00b7ten\u00b7de", "Lin\u00b7den\u00b7bl\u00fct\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "in quellender Juninacht.", "tokens": ["in", "quel\u00b7len\u00b7der", "Ju\u00b7ni\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eine Wonne aus meinem Gem\u00fcthe", "tokens": ["Ei\u00b7ne", "Won\u00b7ne", "aus", "mei\u00b7nem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "ist mir zu Schmerzen erwacht.", "tokens": ["ist", "mir", "zu", "Schmer\u00b7zen", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}