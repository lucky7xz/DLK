{"textgrid.poem.27294": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Pegasus, von ebendemselben", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine zarte Natur schockiert das grelle Gem\u00e4lde,", "tokens": ["Mei\u00b7ne", "zar\u00b7te", "Na\u00b7tur", "scho\u00b7ckiert", "das", "grel\u00b7le", "Ge\u00b7m\u00e4l\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber, von Langbein gemalt, mag ich den Teufel recht gern.", "tokens": ["A\u00b7ber", ",", "von", "Lang\u00b7bein", "ge\u00b7malt", ",", "mag", "ich", "den", "Teu\u00b7fel", "recht", "gern", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "VVPP", "$,", "VMFIN", "PPER", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}}}}}