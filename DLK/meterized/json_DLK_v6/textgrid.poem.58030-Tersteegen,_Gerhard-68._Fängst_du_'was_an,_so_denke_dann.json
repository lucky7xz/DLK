{"textgrid.poem.58030": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "68. F\u00e4ngst du 'was an, so denke dann", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herr, dies wollt' ich tun, ich glaub', es sei dein Wille;", "tokens": ["Mein", "Herr", ",", "dies", "wollt'", "ich", "tun", ",", "ich", "glaub'", ",", "es", "sei", "dein", "Wil\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willst du es haben nicht, sieh da, so lass' ich's stehn,", "tokens": ["Willst", "du", "es", "ha\u00b7ben", "nicht", ",", "sieh", "da", ",", "so", "lass'", "ich's", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VAFIN", "PTKNEG", "$,", "VVIMP", "ADV", "$,", "ADV", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelingt es oder nicht, ich bleibe immer stille,", "tokens": ["Ge\u00b7lingt", "es", "o\u00b7der", "nicht", ",", "ich", "blei\u00b7be", "im\u00b7mer", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bist du nur kontentiert, so ist mein Werk geschehn.", "tokens": ["Bist", "du", "nur", "kon\u00b7ten\u00b7tiert", ",", "so", "ist", "mein", "Werk", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}