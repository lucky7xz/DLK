{"textgrid.poem.35310": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einer albernen Fabel", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einer albernen Fabel", "tokens": ["Ei\u00b7ner", "al\u00b7ber\u00b7nen", "Fa\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Opferte dich, den Helden", "tokens": ["Op\u00b7fer\u00b7te", "dich", ",", "den", "Hel\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Spanischer Minne,", "tokens": ["Spa\u00b7ni\u00b7scher", "Min\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Deutsche Klatschbaserei.", "tokens": ["Deut\u00b7sche", "Klatschba\u00b7se\u00b7rei", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "\u00bbtausendunddrei\u00ab,", "tokens": ["\u00bb", "tau\u00b7sen\u00b7dund\u00b7drei", "\u00ab", ","], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "CARD", "$(", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Sagen die Frommen achselzuckend,", "tokens": ["Sa\u00b7gen", "die", "From\u00b7men", "ach\u00b7sel\u00b7zu\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und seit Jahrhunderten", "tokens": ["Und", "seit", "Jahr\u00b7hun\u00b7der\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Spukst du in engen Gem\u00fctern", "tokens": ["Spukst", "du", "in", "en\u00b7gen", "Ge\u00b7m\u00fc\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Als zierlich geputztes Monstrum,", "tokens": ["Als", "zier\u00b7lich", "ge\u00b7putz\u00b7tes", "Monst\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Das mit blutbefleckten Lippen", "tokens": ["Das", "mit", "blut\u00b7be\u00b7fleck\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Armen Tauben Liebe heuchelt.", "tokens": ["Ar\u00b7men", "Tau\u00b7ben", "Lie\u00b7be", "heu\u00b7chelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nheit, Weiblichkeit,", "tokens": ["Sch\u00f6n\u00b7heit", ",", "Weib\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Knospende Frauenanmut", "tokens": ["Knos\u00b7pen\u00b7de", "Frau\u00b7en\u00b7an\u00b7mut"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Oder reiferer Formen", "tokens": ["O\u00b7der", "rei\u00b7fe\u00b7rer", "For\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Blendende Reizesf\u00fclle", "tokens": ["Blen\u00b7den\u00b7de", "Rei\u00b7zes\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Herrschten \u00fcber dein ganzes Sein.", "tokens": ["Herrschten", "\u00fc\u00b7ber", "dein", "gan\u00b7zes", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ja, mit gewaltigen Z\u00fcgen", "tokens": ["Ja", ",", "mit", "ge\u00b7wal\u00b7ti\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sch\u00f6pftest du aus dem Borne", "tokens": ["Sch\u00f6pf\u00b7test", "du", "aus", "dem", "Bor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Unaussprechlicher Wonnen;", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Won\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Doch nicht Sinnestaumel,", "tokens": ["Doch", "nicht", "Sin\u00b7nes\u00b7tau\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Lebensdurst, siedende Sehnsucht", "tokens": ["Le\u00b7bens\u00b7durst", ",", "sie\u00b7den\u00b7de", "Sehn\u00b7sucht"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.11": {"text": "Zeigten dir jene Gefilde,", "tokens": ["Zeig\u00b7ten", "dir", "je\u00b7ne", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Wo sich an h\u00e4ngende Himmelsg\u00e4rten", "tokens": ["Wo", "sich", "an", "h\u00e4n\u00b7gen\u00b7de", "Him\u00b7mels\u00b7g\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Irdische Liebe klammern m\u00f6chte;", "tokens": ["Ir\u00b7di\u00b7sche", "Lie\u00b7be", "klam\u00b7mern", "m\u00f6ch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Unter s\u00e4uselnden Palmen", "tokens": ["Un\u00b7ter", "s\u00e4u\u00b7seln\u00b7den", "Pal\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wolltest du, Staubgeborner,", "tokens": ["Woll\u00b7test", "du", ",", "Staub\u00b7ge\u00b7bor\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "L\u00e4chelnde Engel umschlingend,", "tokens": ["L\u00e4\u00b7cheln\u00b7de", "En\u00b7gel", "um\u00b7schlin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "In der Wollust veratmenden Ohnmacht", "tokens": ["In", "der", "Wol\u00b7lust", "ver\u00b7at\u00b7men\u00b7den", "Ohn\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+---+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Mit offnen Augen tr\u00e4umen,", "tokens": ["Mit", "off\u00b7nen", "Au\u00b7gen", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um deiner Seele Einsamkeit", "tokens": ["Um", "dei\u00b7ner", "See\u00b7le", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit immer neuen Gef\u00fchlen", "tokens": ["Mit", "im\u00b7mer", "neu\u00b7en", "Ge\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und die angestammte Trauer", "tokens": ["Und", "die", "an\u00b7ge\u00b7stamm\u00b7te", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit Dithyramben zu t\u00e4uschen.", "tokens": ["Mit", "Dit\u00b7hy\u00b7ram\u00b7ben", "zu", "t\u00e4u\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Fr\u00f6hlich, zufrieden sein,", "tokens": ["Fr\u00f6h\u00b7lich", ",", "zu\u00b7frie\u00b7den", "sein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ist das Selbsterkenntnis", "tokens": ["Ist", "das", "Selbs\u00b7ter\u00b7kennt\u00b7nis"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oder tierische Stumpfheit?", "tokens": ["O\u00b7der", "tie\u00b7ri\u00b7sche", "Stumpf\u00b7heit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ist es Selbstvergessen", "tokens": ["Ist", "es", "Selbst\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Oder Geistesarmut?", "tokens": ["O\u00b7der", "Geis\u00b7te\u00b7sar\u00b7mut", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Kanntest du der Beschr\u00e4nkung,", "tokens": ["Kann\u00b7test", "du", "der", "Be\u00b7schr\u00e4n\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Der Gewohnheit sch\u00fcchterne Freuden?", "tokens": ["Der", "Ge\u00b7wohn\u00b7heit", "sch\u00fcch\u00b7ter\u00b7ne", "Freu\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ewig wechselnde Bilder,", "tokens": ["E\u00b7wig", "wech\u00b7seln\u00b7de", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Ob teure Erinnrung", "tokens": ["Ob", "teu\u00b7re", "E\u00b7rinn\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Oder des schaffenden Genius", "tokens": ["O\u00b7der", "des", "schaf\u00b7fen\u00b7den", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Nimmerm\u00fcde Gestaltungskraft", "tokens": ["Nim\u00b7mer\u00b7m\u00fc\u00b7de", "Ge\u00b7stal\u00b7tungs\u00b7kraft"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Aus dem Nichts sie riefen,", "tokens": ["Aus", "dem", "Nichts", "sie", "rie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Lie\u00dfen nie dein Blut erkalten", "tokens": ["Lie\u00b7\u00dfen", "nie", "dein", "Blut", "er\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "In behaglicher Sonntagsruhe;", "tokens": ["In", "be\u00b7hag\u00b7li\u00b7cher", "Sonn\u00b7tags\u00b7ru\u00b7he", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Und berauscht von dem Gifte,", "tokens": ["Und", "be\u00b7rauscht", "von", "dem", "Gif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Das in schmeichelnden Liebespsalmen", "tokens": ["Das", "in", "schmei\u00b7cheln\u00b7den", "Lie\u00b7be\u00b7spsal\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Deinen Lippen entstr\u00f6mte,", "tokens": ["Dei\u00b7nen", "Lip\u00b7pen", "ent\u00b7str\u00f6m\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Konnte von Evas T\u00f6chtern", "tokens": ["Konn\u00b7te", "von", "E\u00b7vas", "T\u00f6ch\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NE", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.19": {"text": "Keine dem Zauber entrinnen.", "tokens": ["Kei\u00b7ne", "dem", "Zau\u00b7ber", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Schale, d\u00fcrftige Welt,", "tokens": ["Scha\u00b7le", ",", "d\u00fcrf\u00b7ti\u00b7ge", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "W\u00e4re sie nicht erleuchtet", "tokens": ["W\u00e4\u00b7re", "sie", "nicht", "er\u00b7leuch\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Durch holder Frauenaugen", "tokens": ["Durch", "hol\u00b7der", "Frau\u00b7en\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Z\u00fcndende Strahlen!", "tokens": ["Z\u00fcn\u00b7den\u00b7de", "Strah\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Lieben, Geliebtsein \u2013", "tokens": ["Lie\u00b7ben", ",", "Ge\u00b7liebt\u00b7sein", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Unvollkommnes, kurzes,", "tokens": ["Un\u00b7voll\u00b7komm\u00b7nes", ",", "kur\u00b7zes", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "S\u00fc\u00dfes, schmerzenvolles,", "tokens": ["S\u00fc\u00b7\u00dfes", ",", "schmer\u00b7zen\u00b7vol\u00b