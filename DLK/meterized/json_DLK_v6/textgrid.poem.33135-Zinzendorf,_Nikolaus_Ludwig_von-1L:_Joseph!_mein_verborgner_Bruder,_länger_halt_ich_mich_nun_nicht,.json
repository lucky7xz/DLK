{"textgrid.poem.33135": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Joseph! mein verborgner Bruder, l\u00e4nger halt ich mich nun nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Joseph! mein verborgner Bruder, l\u00e4nger halt ich mich nun nicht,", "tokens": ["Jo\u00b7se\u00b7ph", "!", "mein", "ver\u00b7borg\u00b7ner", "Bru\u00b7der", ",", "l\u00e4n\u00b7ger", "halt", "ich", "mich", "nun", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Dich vor allen \u00f6ffentlich einen Knecht des Herrn zu nennen,", "tokens": ["Dich", "vor", "al\u00b7len", "\u00f6f\u00b7fent\u00b7lich", "ei\u00b7nen", "Knecht", "des", "Herrn", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "ADJD", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Deine Liebe gegen Den, der die Lieb ist, zu bekennen,", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", "ge\u00b7gen", "Den", ",", "der", "die", "Lieb", "ist", ",", "zu", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$,", "PRELS", "ART", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Joseph, du geschm\u00fckter Priester mit dem Recht und mit dem Licht!", "tokens": ["Jo\u00b7se\u00b7ph", ",", "du", "ge\u00b7schm\u00fck\u00b7ter", "Pries\u00b7ter", "mit", "dem", "Recht", "und", "mit", "dem", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.5": {"text": "Mu\u00df ich dann der Seligkeit, mich mit Br\u00fcdern zu erquikken,", "tokens": ["Mu\u00df", "ich", "dann", "der", "Se\u00b7lig\u00b7keit", ",", "mich", "mit", "Br\u00fc\u00b7dern", "zu", "er\u00b7quik\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "$,", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und mit ihnen aus dem Glauben zu verst\u00e4rken, m\u00fc\u00dfig gehn;", "tokens": ["Und", "mit", "ih\u00b7nen", "aus", "dem", "Glau\u00b7ben", "zu", "ver\u00b7st\u00e4r\u00b7ken", ",", "m\u00fc\u00b7\u00dfig", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Will ich endlich meine Fl\u00fcgel bis zu Gottes Stuhl erh\u00f6hn,", "tokens": ["Will", "ich", "end\u00b7lich", "mei\u00b7ne", "Fl\u00fc\u00b7gel", "bis", "zu", "Got\u00b7tes", "Stuhl", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Wo sie allerseits im Geist nach der Streiter L\u00e4gern blikken.", "tokens": ["Wo", "sie", "al\u00b7ler\u00b7seits", "im", "Geist", "nach", "der", "Strei\u00b7ter", "L\u00e4\u00b7gern", "blik\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Joseph! diesen letzten Ausdruk meiner Liebe gegen dich,", "tokens": ["Jo\u00b7se\u00b7ph", "!", "die\u00b7sen", "letz\u00b7ten", "Aus\u00b7druk", "mei\u00b7ner", "Lie\u00b7be", "ge\u00b7gen", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDAT", "ADJA", "NN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "+----+-+-+-+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "Die du lange schon gef\u00fchlt, la\u00df ich alle sehn und h\u00f6ren,", "tokens": ["Die", "du", "lan\u00b7ge", "schon", "ge\u00b7f\u00fchlt", ",", "la\u00df", "ich", "al\u00b7le", "sehn", "und", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,", "VVIMP", "PPER", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.11": {"text": "Deren Ueberlegungs-Kraft nicht die Vorurtheile st\u00f6ren.", "tokens": ["De\u00b7ren", "Ue\u00b7ber\u00b7le\u00b7gungs\u00b7Kraft", "nicht", "die", "Vor\u00b7urt\u00b7hei\u00b7le", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+--++--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Joseph! deine stille F\u00fchrung reitzte mich oft inniglich.", "tokens": ["Jo\u00b7se\u00b7ph", "!", "dei\u00b7ne", "stil\u00b7le", "F\u00fch\u00b7rung", "reitz\u00b7te", "mich", "oft", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.13": {"text": "Ich vergesse nimmermehr, was du an des Herren Tage", "tokens": ["Ich", "ver\u00b7ges\u00b7se", "nim\u00b7mer\u00b7mehr", ",", "was", "du", "an", "des", "Her\u00b7ren", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Ganz geheim mit mir gesprochen, wie du deinen Wandel f\u00fchrt'st,", "tokens": ["Ganz", "ge\u00b7heim", "mit", "mir", "ge\u00b7spro\u00b7chen", ",", "wie", "du", "dei\u00b7nen", "Wan\u00b7del", "f\u00fchrt'st", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Wie du deiner Br\u00fcder Herz gerne in einander r\u00fchrt'st:", "tokens": ["Wie", "du", "dei\u00b7ner", "Br\u00fc\u00b7der", "Herz", "ger\u00b7ne", "in", "ein\u00b7an\u00b7der", "r\u00fchrt'st", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "NN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Wo du aber inne hielt'st: Merke, hie\u00df es, was ich sage,", "tokens": ["Wo", "du", "a\u00b7ber", "in\u00b7ne", "hielt'st", ":", "Mer\u00b7ke", ",", "hie\u00df", "es", ",", "was", "ich", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKVZ", "VVFIN", "$.", "NE", "$,", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Joseph! deinen Hirten-Stekken kanst du mit getrostem Sinn", "tokens": ["Jo\u00b7se\u00b7ph", "!", "dei\u00b7nen", "Hir\u00b7ten\u00b7S\u00b7tek\u00b7ken", "kanst", "du", "mit", "ge\u00b7tros\u00b7tem", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.18": {"text": "Jesu, deinem Ober-Herrn, der dich einhohlt, \u00fcberreichen.", "tokens": ["Je\u00b7su", ",", "dei\u00b7nem", "O\u00b7ber\u00b7Herrn", ",", "der", "dich", "ein\u00b7hohlt", ",", "\u00fc\u00b7berr\u00b7ei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Sorau! unser Erz-Bischof gebe dir bald einen gleichen,", "tokens": ["So\u00b7rau", "!", "un\u00b7ser", "Er\u00b7zBi\u00b7schof", "ge\u00b7be", "dir", "bald", "ei\u00b7nen", "glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "-+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.20": {"text": "Da\u00df es hier nicht heissen m\u00f6ge, Josephs Geist ist mit dahin.", "tokens": ["Da\u00df", "es", "hier", "nicht", "heis\u00b7sen", "m\u00f6\u00b7ge", ",", "Jo\u00b7se\u00b7phs", "Geist", "ist", "mit", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,", "NE", "NN", "VAFIN", "APPR", "PAV", "$."], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Werther Graf! ich bitte dich, von der Wunden Christi wegen,", "tokens": ["Wert\u00b7her", "Graf", "!", "ich", "bit\u00b7te", "dich", ",", "von", "der", "Wun\u00b7den", "Chris\u00b7ti", "we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NE", "APPR", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Liebe Seine rauhen Dornen, la\u00df dem Fleische keine Ruh;", "tokens": ["Lie\u00b7be", "Sei\u00b7ne", "rau\u00b7hen", "Dor\u00b7nen", ",", "la\u00df", "dem", "Flei\u00b7sche", "kei\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "VVIMP", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Will Gott Seinen Sohn verkl\u00e4ren, fahre augenbliklich zu.", "tokens": ["Will", "Gott", "Sei\u00b7nen", "Sohn", "ver\u00b7kl\u00e4\u00b7ren", ",", "fah\u00b7re", "au\u00b7gen\u00b7blik\u00b7lich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPOSAT", "NN", "VVINF", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wer den Harnisch nimt, mu\u00df ihn ohne Sieg nicht von sich legen.", "tokens": ["Wer", "den", "Har\u00b7nisch", "nimt", ",", "mu\u00df", "ihn", "oh\u00b7ne", "Sieg", "nicht", "von", "sich", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VMFIN", "PPER", "APPR", "NN", "PTKNEG", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Hast du viel Verhindernisse, lieget dir die Perle tief,", "tokens": ["Hast", "du", "viel", "Ver\u00b7hin\u00b7der\u00b7nis\u00b7se", ",", "lie\u00b7get", "dir", "die", "Per\u00b7le", "tief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Weil du hochgeboren bist, neige dich zu Christo nieder,", "tokens": ["Weil", "du", "hoch\u00b7ge\u00b7bo\u00b7ren", "bist", ",", "nei\u00b7ge", "dich", "zu", "Chris\u00b7to", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VAFIN", "$,", "VVFIN", "PRF", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Der stieg eine H\u00f6h herab, und fand doch die H\u00f6he wieder.", "tokens": ["Der", "stieg", "ei\u00b7ne", "H\u00f6h", "her\u00b7ab", ",", "und", "fand", "doch", "die", "H\u00f6\u00b7he", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "O wie wohl ist mir geworden, da Er mich ans Creutze rief.", "tokens": ["O", "wie", "wohl", "ist", "mir", "ge\u00b7wor\u00b7den", ",", "da", "Er", "mich", "ans", "Creut\u00b7ze", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "PPER", "VAPP", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "F\u00fcrstin! deren guten Sinn unser Joseph stets gepriesen,", "tokens": ["F\u00fcrs\u00b7tin", "!", "de\u00b7ren", "gu\u00b7ten", "Sinn", "un\u00b7ser", "Jo\u00b7se\u00b7ph", "stets", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELAT", "ADJA", "NN", "PPOSAT", "NE", "ADV", "VVPP", "$,"], "meter": "+---+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Und gewi\u00df von ihr geglaubt, Jesus Christus w\u00fcrd ihr doch:", "tokens": ["Und", "ge\u00b7wi\u00df", "von", "ihr", "ge\u00b7glaubt", ",", "Je\u00b7sus", "Chris\u00b7tus", "w\u00fcrd", "ihr", "doch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "$,", "NE", "NE", "VAFIN", "PPER", "ADV", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Dieser ist nicht mehr vorhanden, aber sonst ein Joseph noch,", "tokens": ["Die\u00b7ser", "ist", "nicht", "mehr", "vor\u00b7han\u00b7den", ",", "a\u00b7ber", "sonst", "ein", "Jo\u00b7se\u00b7ph", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "ADV", "ART", "NE", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Der des Landes Vater ist, und zu dem er sie gewiesen.", "tokens": ["Der", "des", "Lan\u00b7des", "Va\u00b7ter", "ist", ",", "und", "zu", "dem", "er", "sie", "ge\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAFIN", "$,", "KON", "APPR", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "T\u00f6chter! die ihr unserm Heiland nicht mehr fremd und unbekant,", "tokens": ["T\u00f6ch\u00b7ter", "!", "die", "ihr", "un\u00b7serm", "Hei\u00b7land", "nicht", "mehr", "fremd", "und", "un\u00b7be\u00b7kant", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Und nur F\u00fcrsten-Kinder wart, eh ihr Kinder Gottes worden,", "tokens": ["Und", "nur", "F\u00fcrs\u00b7ten\u00b7Kin\u00b7der", "wart", ",", "eh", "ihr", "Kin\u00b7der", "Got\u00b7tes", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "NN", "VAPP", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Denkt an Joseph, weil ihr lebt, und verbleibt beym Creutzes-Orden.", "tokens": ["Denkt", "an", "Jo\u00b7se\u00b7ph", ",", "weil", "ihr", "lebt", ",", "und", "ver\u00b7bleibt", "beym", "Creut\u00b7zes\u00b7Or\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Alle Schafe seiner Pflege nehm' der Hirt in Seine Hand.", "tokens": ["Al\u00b7le", "Sch