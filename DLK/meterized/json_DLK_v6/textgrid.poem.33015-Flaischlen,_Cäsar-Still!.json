{"textgrid.poem.33015": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "Still!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tiefstiller dunkler Schlaf", "tokens": ["Tief\u00b7stil\u00b7ler", "dunk\u00b7ler", "Schlaf"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "sinkt \u00fcber meinen Tag,", "tokens": ["sinkt", "\u00fc\u00b7ber", "mei\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df ich nichts hoffen mehr,", "tokens": ["da\u00df", "ich", "nichts", "hof\u00b7fen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "nichts f\u00fcrchten mag!", "tokens": ["nichts", "f\u00fcrch\u00b7ten", "mag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das ganze Leben ...", "tokens": ["Das", "gan\u00b7ze", "Le\u00b7ben", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "ich entsinne mich kaum,", "tokens": ["ich", "ent\u00b7sin\u00b7ne", "mich", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "war es froh, war es traurig?!", "tokens": ["war", "es", "froh", ",", "war", "es", "trau\u00b7rig", "?!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Alles wird Traum ...", "tokens": ["Al\u00b7les", "wird", "Traum", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Es ist eine Wiege,", "tokens": ["Es", "ist", "ei\u00b7ne", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "von heimlicher Hand", "tokens": ["von", "heim\u00b7li\u00b7cher", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "leise geschaukelt", "tokens": ["lei\u00b7se", "ge\u00b7schau\u00b7kelt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "an Grabesrand!", "tokens": ["an", "Gra\u00b7bes\u00b7rand", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Still! ... Still!", "tokens": ["Still", "!", "...", "Still", "!"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["ADJD", "$.", "$(", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}