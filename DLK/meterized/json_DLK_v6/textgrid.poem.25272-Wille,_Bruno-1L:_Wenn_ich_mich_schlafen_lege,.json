{"textgrid.poem.25272": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich mich schlafen lege,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mich schlafen lege,", "tokens": ["Wenn", "ich", "mich", "schla\u00b7fen", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So f\u00e4llt mir manchmal ein,", "tokens": ["So", "f\u00e4llt", "mir", "manch\u00b7mal", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wieviel ich dir verdanke,", "tokens": ["Wie\u00b7viel", "ich", "dir", "ver\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du treuer Ruheschrein.", "tokens": ["Du", "treu\u00b7er", "Ru\u00b7he\u00b7schrein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du schaust mich an so gr\u00fcbelnd", "tokens": ["Du", "schaust", "mich", "an", "so", "gr\u00fc\u00b7belnd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit deiner Bretterstirn,", "tokens": ["Mit", "dei\u00b7ner", "Bret\u00b7ters\u00b7tirn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als m\u00f6chtest du erz\u00e4hlen,", "tokens": ["Als", "m\u00f6ch\u00b7test", "du", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4re Holz wie Hirn.", "tokens": ["Als", "w\u00e4\u00b7re", "Holz", "wie", "Hirn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf deinem breiten R\u00fccken", "tokens": ["Auf", "dei\u00b7nem", "brei\u00b7ten", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trugst du mich manche Nacht.", "tokens": ["Trugst", "du", "mich", "man\u00b7che", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein halbes Leben hab ich", "tokens": ["Ein", "hal\u00b7bes", "Le\u00b7ben", "hab", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deiner Hut verbracht.", "tokens": ["In", "dei\u00b7ner", "Hut", "ver\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich kam aus dunklem Scho\u00dfe", "tokens": ["Ich", "kam", "aus", "dunk\u00b7lem", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Lichte, zart und klein;", "tokens": ["Zum", "Lich\u00b7te", ",", "zart", "und", "klein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie legten mich vertraulich", "tokens": ["Sie", "leg\u00b7ten", "mich", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deine Pflege ein.", "tokens": ["In", "dei\u00b7ne", "Pfle\u00b7ge", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was ich mit schwerer Zunge", "tokens": ["Was", "ich", "mit", "schwe\u00b7rer", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Schlafe ausgeplauscht", "tokens": ["Im", "Schla\u00b7fe", "aus\u00b7ge\u00b7plauscht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von bunter Tr\u00e4ume Wispern,", "tokens": ["Von", "bun\u00b7ter", "Tr\u00e4u\u00b7me", "Wis\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hast duldsam du belauscht.", "tokens": ["Hast", "duld\u00b7sam", "du", "be\u00b7lauscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn mich Gespenster w\u00fcrgten,", "tokens": ["Wenn", "mich", "Ge\u00b7spens\u00b7ter", "w\u00fcrg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ich in Kl\u00fcfte fiel/", "tokens": ["Wenn", "ich", "in", "Kl\u00fcf\u00b7te", "fiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In deinem sanften Pf\u00fchle", "tokens": ["In", "dei\u00b7nem", "sanf\u00b7ten", "Pf\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fand ich mein tr\u00f6stlich Ziel.", "tokens": ["Fand", "ich", "mein", "tr\u00f6st\u00b7lich", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Seufzer, wenn des Sturmes", "tokens": ["Die", "Seuf\u00b7zer", ",", "wenn", "des", "Stur\u00b7mes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewimmer mich gequ\u00e4lt/", "tokens": ["Ge\u00b7wim\u00b7mer", "mich", "ge\u00b7qu\u00e4lt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, wei\u00dft du noch? die K\u00fcsse/", "tokens": ["Und", ",", "wei\u00dft", "du", "noch", "?", "die", "K\u00fcs\u00b7se", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast sie all gez\u00e4hlt.", "tokens": ["Du", "hast", "sie", "all", "ge\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du Zeuge voller Andacht,", "tokens": ["Du", "Zeu\u00b7ge", "vol\u00b7ler", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn schaffend ich gewacht,", "tokens": ["Wenn", "schaf\u00b7fend", "ich", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich, vom Geiste trunken,", "tokens": ["Wenn", "ich", ",", "vom", "Geis\u00b7te", "trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Liede nachgedacht.", "tokens": ["Dem", "Lie\u00b7de", "nach\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn schlotternd ich getaumelt,", "tokens": ["Wenn", "schlot\u00b7ternd", "ich", "ge\u00b7tau\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schl\u00e4fe fieberschw\u00fcl,", "tokens": ["Die", "Schl\u00e4\u00b7fe", "fie\u00b7ber\u00b7schw\u00fcl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast du mein Haupt gebettet", "tokens": ["Hast", "du", "mein", "Haupt", "ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deine Kissen k\u00fchl.", "tokens": ["In", "dei\u00b7ne", "Kis\u00b7sen", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dereinst, wenn ich so liege,", "tokens": ["De\u00b7reinst", ",", "wenn", "ich", "so", "lie\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann kommt der Rudermann,", "tokens": ["Dann", "kommt", "der", "Ru\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der deine treuen Planken", "tokens": ["Der", "dei\u00b7ne", "treu\u00b7en", "Plan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Barke w\u00f6lben kann.", "tokens": ["Zur", "Bar\u00b7ke", "w\u00f6l\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du tr\u00e4gst mich leise schaukelnd/", "tokens": ["Du", "tr\u00e4gst", "mich", "lei\u00b7se", "schau\u00b7kelnd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fahr wohl/ durch schwarze Flut", "tokens": ["Fahr", "wohl", "/", "durch", "schwar\u00b7ze", "Flut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Eiland der Zypressen,", "tokens": ["Zum", "Ei\u00b7land", "der", "Zyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Wo sich so selig ruht.", "tokens": ["Wo", "sich", "so", "se\u00b7lig", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}