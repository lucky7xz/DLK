{"dta.poem.12045": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Weisheitslehren, die dir Weisheitslehrer spenden,", "tokens": ["Die", "Weis\u00b7heits\u00b7leh\u00b7ren", ",", "die", "dir", "Weis\u00b7heits\u00b7leh\u00b7rer", "spen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O k\u00f6nntest du sie stets zur Weisheit nur verwenden!", "tokens": ["O", "k\u00f6nn\u00b7test", "du", "sie", "stets", "zur", "Weis\u00b7heit", "nur", "ver\u00b7wen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch du gewahrest bald, ein Lehrer widerspricht", "tokens": ["Doch", "du", "ge\u00b7wah\u00b7rest", "bald", ",", "ein", "Leh\u00b7rer", "wi\u00b7der\u00b7spricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem anderen, und wer im Recht sei, wei\u00dft du nicht.", "tokens": ["Dem", "an\u00b7de\u00b7ren", ",", "und", "wer", "im", "Recht", "sei", ",", "wei\u00dft", "du", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "PWS", "APPRART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du kannst nicht beiden, wem von beiden willst du glauben?", "tokens": ["Du", "kannst", "nicht", "bei\u00b7den", ",", "wem", "von", "bei\u00b7den", "willst", "du", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWS", "APPR", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll gar Glaubw\u00fcrdigkeit jedweder jedem rauben?", "tokens": ["Soll", "gar", "Glaub\u00b7w\u00fcr\u00b7dig\u00b7keit", "jed\u00b7we\u00b7der", "je\u00b7dem", "rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "PIAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und schlie\u00dfest du, da\u00df Recht von beiden keiner hat,", "tokens": ["Und", "schlie\u00b7\u00dfest", "du", ",", "da\u00df", "Recht", "von", "bei\u00b7den", "kei\u00b7ner", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "NN", "APPR", "PIAT", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hast du selber dir entzogen jeden Rath.", "tokens": ["So", "hast", "du", "sel\u00b7ber", "dir", "ent\u00b7zo\u00b7gen", "je\u00b7den", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denk lieber: Jeder hat nur Recht auf seine Weise;", "tokens": ["Denk", "lie\u00b7ber", ":", "Je\u00b7der", "hat", "nur", "Recht", "auf", "sei\u00b7ne", "Wei\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PIS", "VAFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das stell' auf deine dir zurecht in deinem Kreise.", "tokens": ["Das", "stell'", "auf", "dei\u00b7ne", "dir", "zu\u00b7recht", "in", "dei\u00b7nem", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Verschiedne F\u00e4lle gibts auf einer Lebensfahrt,", "tokens": ["Ver\u00b7schied\u00b7ne", "F\u00e4l\u00b7le", "gibts", "auf", "ei\u00b7ner", "Le\u00b7bens\u00b7fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo man wol brauchen kann Rath von verschiedner Art.", "tokens": ["Wo", "man", "wol", "brau\u00b7chen", "kann", "Rath", "von", "ver\u00b7schied\u00b7ner", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVINF", "VMFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Gl\u00fcckselig bist du, wenn f\u00fcr Auf- und Niedersteig", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "bist", "du", ",", "wenn", "f\u00fcr", "Auf", "und", "Nie\u00b7ders\u00b7teig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUS", "APPR", "TRUNC", "KON", "NE"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du immer recht verstehst den rechten Fingerzeig.", "tokens": ["Du", "im\u00b7mer", "recht", "ver\u00b7stehst", "den", "rech\u00b7ten", "Fin\u00b7ger\u00b7zeig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}