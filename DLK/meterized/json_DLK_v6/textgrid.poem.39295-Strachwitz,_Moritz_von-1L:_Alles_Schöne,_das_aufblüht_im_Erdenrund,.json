{"textgrid.poem.39295": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles Sch\u00f6ne, das aufbl\u00fcht im Erdenrund,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles Sch\u00f6ne, das aufbl\u00fcht im Erdenrund,", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7ne", ",", "das", "auf\u00b7bl\u00fcht", "im", "Er\u00b7den\u00b7rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Stirbt heut' oder morgen geschwind;", "tokens": ["Stirbt", "heut'", "o\u00b7der", "mor\u00b7gen", "ge\u00b7schwind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wo die Rose sich r\u00f6tet, in selber Stund',", "tokens": ["Wo", "die", "Ro\u00b7se", "sich", "r\u00f6\u00b7tet", ",", "in", "sel\u00b7ber", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Da tummelt im Schnee sich der Wind.", "tokens": ["Da", "tum\u00b7melt", "im", "Schnee", "sich", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Was in Lieb' an die Brust ich geschlossen hatt'", "tokens": ["Was", "in", "Lieb'", "an", "die", "Brust", "ich", "ge\u00b7schlos\u00b7sen", "hatt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "APPR", "ART", "NN", "PPER", "VVPP", "VAFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Floh hin wie der Woge Schaum,", "tokens": ["Floh", "hin", "wie", "der", "Wo\u00b7ge", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie im st\u00fcrmischen Herbste Blatt um Blatt", "tokens": ["Wie", "im", "st\u00fcr\u00b7mi\u00b7schen", "Herbs\u00b7te", "Blatt", "um", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "ADJA", "NN", "APPR", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "In gelblichen Wirbeln vom Baum.", "tokens": ["In", "gelb\u00b7li\u00b7chen", "Wir\u00b7beln", "vom", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Einen Freund ich hatt', ich vergo\u00df mein Blut,", "tokens": ["Ei\u00b7nen", "Freund", "ich", "hatt'", ",", "ich", "ver\u00b7go\u00df", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn sein Blick es begehrt von mir,", "tokens": ["Wenn", "sein", "Blick", "es", "be\u00b7gehrt", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "APPR", "PPER", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Doch ertrug er nicht meines Herzens Glut,", "tokens": ["Doch", "er\u00b7trug", "er", "nicht", "mei\u00b7nes", "Her\u00b7zens", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich zu k\u00fchlen mu\u00dft' er von hier.", "tokens": ["Sich", "zu", "k\u00fch\u00b7len", "mu\u00dft'", "er", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "VMFIN", "PPER", "APPR", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Laut weint' ich und rief den Namen sein,", "tokens": ["Laut", "weint'", "ich", "und", "rief", "den", "Na\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ja! Der Meinige war er einmal \u2013", "tokens": ["Ja", "!", "Der", "Mei\u00b7ni\u00b7ge", "war", "er", "ein\u00b7mal", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "PPOSS", "VAFIN", "PPER", "ADV", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die Erinn'rung an ihn umschlo\u00df ich allein,", "tokens": ["Die", "Er\u00b7inn'\u00b7rung", "an", "ihn", "um\u00b7schlo\u00df", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da ward eng mir der Weltensaal.", "tokens": ["Da", "ward", "eng", "mir", "der", "Wel\u00b7ten\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Am Sirenengestade, da hab' ich gekost", "tokens": ["Am", "Si\u00b7re\u00b7nen\u00b7ge\u00b7sta\u00b7de", ",", "da", "hab'", "ich", "ge\u00b7kost"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Auf der Liebe Rosengrund;", "tokens": ["Auf", "der", "Lie\u00b7be", "Ro\u00b7sen\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort war eine Blume mein Augentrost,", "tokens": ["Dort", "war", "ei\u00b7ne", "Blu\u00b7me", "mein", "Au\u00b7gen\u00b7trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Doch nur eine kurze Stund'.", "tokens": ["Doch", "nur", "ei\u00b7ne", "kur\u00b7ze", "Stund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So herrlich erbl\u00fchte meine Ros',", "tokens": ["So", "herr\u00b7lich", "er\u00b7bl\u00fch\u00b7te", "mei\u00b7ne", "Ros'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und der Duft lauter Liebe war;", "tokens": ["Und", "der", "Duft", "lau\u00b7ter", "Lie\u00b7be", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Doch ein R\u00e4uber ri\u00df die Bl\u00e4tter los", "tokens": ["Doch", "ein", "R\u00e4u\u00b7ber", "ri\u00df", "die", "Bl\u00e4t\u00b7ter", "los"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und lie\u00df mir die Dornenschar.", "tokens": ["Und", "lie\u00df", "mir", "die", "Dor\u00b7nen\u00b7schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und mein Wesen, es ward so stumm und kalt,", "tokens": ["Und", "mein", "We\u00b7sen", ",", "es", "ward", "so", "stumm", "und", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie wenn Tod mit dem Finger es schlug,", "tokens": ["Wie", "wenn", "Tod", "mit", "dem", "Fin\u00b7ger", "es", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Doch verschm\u00e4h' ich den Trost, der aus Tr\u00e4nen wallt,", "tokens": ["Doch", "ver\u00b7schm\u00e4h'", "ich", "den", "Trost", ",", "der", "aus", "Tr\u00e4\u00b7nen", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Man weint ja auf Erden genug.", "tokens": ["Man", "weint", "ja", "auf", "Er\u00b7den", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Matt l\u00e4chelt mein Aug', wie die Herbstessonn'", "tokens": ["Matt", "l\u00e4\u00b7chelt", "mein", "Aug'", ",", "wie", "die", "Herbs\u00b7tes\u00b7sonn'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00dcber K\u00fcsten einsam und leer,", "tokens": ["\u00dc\u00b7ber", "K\u00fcs\u00b7ten", "ein\u00b7sam", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Erde sieht oft neue Lenzeswonn',", "tokens": ["Die", "Er\u00b7de", "sieht", "oft", "neu\u00b7e", "Len\u00b7zes\u00b7wonn'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz wei\u00df vom Lenze nichts mehr.", "tokens": ["Mein", "Herz", "wei\u00df", "vom", "Len\u00b7ze", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PIS", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Doch wenn Himmel und Erde der Sturm aufst\u00f6rt,", "tokens": ["Doch", "wenn", "Him\u00b7mel", "und", "Er\u00b7de", "der", "Sturm", "auf\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Hab' ich noch am Sein meine Lust,", "tokens": ["Hab'", "ich", "noch", "am", "Sein", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, dem nichts auf der Erde angeh\u00f6rt,", "tokens": ["Der", ",", "dem", "nichts", "auf", "der", "Er\u00b7de", "an\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Er leidet auch keinen Verlust.", "tokens": ["Er", "lei\u00b7det", "auch", "kei\u00b7nen", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Nun geh' ich \u00fcber den Markt der Welt,", "tokens": ["Nun", "geh'", "ich", "\u00fc\u00b7ber", "den", "Markt", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo ein Spielzeug ist jede War',", "tokens": ["Wo", "ein", "Spiel\u00b7zeug", "ist", "je\u00b7de", "Wa\u00b7r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mein wird nicht die Sonne am Himmelszelt,", "tokens": ["Mein", "wird", "nicht", "die", "Son\u00b7ne", "am", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Drum begehr' ich nichts mehr immerdar.", "tokens": ["Drum", "be\u00b7gehr'", "ich", "nichts", "mehr", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}