{"dta.poem.19443": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "Der Gesang.  \n An Sch\u00f6nborn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fcsset, Gesang! so oft du vom", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fcs\u00b7set", ",", "Ge\u00b7sang", "!", "so", "oft", "du", "vom"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NN", "$.", "ADV", "ADV", "PPER", "APPRART"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "hohen Olympos", "tokens": ["ho\u00b7hen", "O\u00b7lym\u00b7pos"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+----", "measure": "dactylic.init"}, "line.3": {"text": "Zu mir k\u00f6mst! willkommen in jeder wechselnden", "tokens": ["Zu", "mir", "k\u00f6mst", "!", "will\u00b7kom\u00b7men", "in", "je\u00b7der", "wech\u00b7seln\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$.", "ADJD", "APPR", "PIAT", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sch\u00f6nheit!", "tokens": ["Sch\u00f6n\u00b7heit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wenn du auf leise bebenden Wallungen sanfter", "tokens": ["Wenn", "du", "auf", "lei\u00b7se", "be\u00b7ben\u00b7den", "Wal\u00b7lun\u00b7gen", "sanf\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "ADJA", "NN", "ADJD"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gedanken", "tokens": ["Ge\u00b7dan\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Meine gleitende Seel\u2019 in vertrauten Str\u00f6men", "tokens": ["Mei\u00b7ne", "glei\u00b7ten\u00b7de", "Seel'", "in", "ver\u00b7trau\u00b7ten", "Str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "einherf\u00fchrst,", "tokens": ["ein\u00b7her\u00b7f\u00fchrst", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.9": {"text": "Wo mir Freuden bl\u00fchen am Ufer, und Ruhe", "tokens": ["Wo", "mir", "Freu\u00b7den", "bl\u00fc\u00b7hen", "am", "U\u00b7fer", ",", "und", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "APPRART", "NN", "$,", "KON", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "mir schattet,", "tokens": ["mir", "schat\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Oder, wenn du, m\u00e4chtig mich f\u00fchrend, in st\u00fcr-", "tokens": ["O\u00b7der", ",", "wenn", "du", ",", "m\u00e4ch\u00b7tig", "mich", "f\u00fch\u00b7rend", ",", "in", "st\u00fcr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "$,", "ADJD", "PPER", "VVPP", "$,", "APPR", "TRUNC"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "mender Eile,", "tokens": ["men\u00b7der", "Ei\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.13": {"text": "Ueber Meere starker Gef\u00fchle, sonder Gestade,", "tokens": ["Ue\u00b7ber", "Mee\u00b7re", "star\u00b7ker", "Ge\u00b7f\u00fch\u00b7le", ",", "son\u00b7der", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Meinen staunenden Geist den kreisenden Strudeln", "tokens": ["Mei\u00b7nen", "stau\u00b7nen\u00b7den", "Geist", "den", "krei\u00b7sen\u00b7den", "Stru\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "entreissest,", "tokens": ["en\u00b7treis\u00b7sest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Jzt mit flammenden Blizen die \u00fcberhangende", "tokens": ["Jzt", "mit", "flam\u00b7men\u00b7den", "Bli\u00b7zen", "die", "\u00fc\u00b7berh\u00b7an\u00b7gen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Dr\u00e4uung", "tokens": ["Dr\u00e4u\u00b7ung"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "N\u00e4chtlicher Wogen, und izt des Abgrunds Tie-", "tokens": ["N\u00e4cht\u00b7li\u00b7cher", "Wo\u00b7gen", ",", "und", "izt", "des", "Ab\u00b7grunds", "Tie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADV", "ART", "NN", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "fen erhellend,", "tokens": ["fen", "er\u00b7hel\u00b7lend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.20": {"text": "Sei mir immer gegr\u00fc\u00dft mit \u00fcberwallender Seele,", "tokens": ["Sei", "mir", "im\u00b7mer", "ge\u00b7gr\u00fc\u00dft", "mit", "\u00fc\u00b7berw\u00b7al\u00b7len\u00b7der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.21": {"text": "Heil dir, G\u00f6ttlicher, Heil! Dir dank\u2019 ich die", "tokens": ["Heil", "dir", ",", "G\u00f6tt\u00b7li\u00b7cher", ",", "Heil", "!", "Dir", "dank'", "ich", "die"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "NN", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "ART"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "bessern Minuten,", "tokens": ["bes\u00b7sern", "Mi\u00b7nu\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Wenn mein ewiger Geist, in seinen Kr\u00e4ften sich", "tokens": ["Wenn", "mein", "e\u00b7wi\u00b7ger", "Geist", ",", "in", "sei\u00b7nen", "Kr\u00e4f\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "PRF"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "wiegend,", "tokens": ["wie\u00b7gend", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Schaffend winket, und schnell die neuen Sch\u00f6p-", "tokens": ["Schaf\u00b7fend", "win\u00b7ket", ",", "und", "schnell", "die", "neu\u00b7en", "Sch\u00f6p"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KON", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "fungen t\u00f6nen!", "tokens": ["fun\u00b7gen", "t\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Heil dir, G\u00f6ttlicher, Heil! Du f\u00fchrtest stra-", "tokens": ["Heil", "dir", ",", "G\u00f6tt\u00b7li\u00b7cher", ",", "Heil", "!", "Du", "f\u00fchr\u00b7test", "stra"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "NN", "$,", "NN", "$.", "PPER", "VVFIN", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "lenden Fluges,", "tokens": ["len\u00b7den", "Flu\u00b7ges", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.29": {"text": "Und auf Silbert\u00f6nenden Schwanenfl\u00fcgeln, die", "tokens": ["Und", "auf", "Sil\u00b7ber\u00b7t\u00f6\u00b7nen\u00b7den", "Schwa\u00b7nen\u00b7fl\u00fc\u00b7geln", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "PRELS"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Seele", "tokens": ["See\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Meines trauten Sch\u00f6nborn zu mir von der hor-", "tokens": ["Mei\u00b7nes", "trau\u00b7ten", "Sch\u00f6n\u00b7bo\u00b7rn", "zu", "mir", "von", "der", "hor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "APPR", "ART", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "chenden Themse!", "tokens": ["chen\u00b7den", "Them\u00b7se", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.33": {"text": "Heil dir, G\u00f6ttlicher, Heil! Du f\u00fchrest, stra-", "tokens": ["Heil", "dir", ",", "G\u00f6tt\u00b7li\u00b7cher", ",", "Heil", "!", "Du", "f\u00fch\u00b7rest", ",", "stra"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "NN", "$,", "NN", "$.", "PPER", "VVFIN", "$,", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "lenden Fluges,", "tokens": ["len\u00b7den", "Flu\u00b7ges", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.35": {"text": "Und auf Silbert\u00f6nenden Schwanenfl\u00fcgeln, die", "tokens": ["Und", "auf", "Sil\u00b7ber\u00b7t\u00f6\u00b7nen\u00b7den", "Schwa\u00b7nen\u00b7fl\u00fc\u00b7geln", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "PRELS"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Seele", "tokens": ["See\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Seines trauten Stolberg zu ihm vom Gestade", "tokens": ["Sei\u00b7nes", "trau\u00b7ten", "Stol\u00b7berg", "zu", "ihm", "vom", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.38": {"text": "des Nordmeers!", "tokens": ["des", "Nord\u00b7meers", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}