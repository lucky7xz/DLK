{"textgrid.poem.61934": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, o klingender Frost, du bringst uns die Sonne", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "o", "klin\u00b7gen\u00b7der", "Frost", ",", "du", "bringst", "uns", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wieder zur\u00fcck; tiefklar w\u00f6lbt sich das schimmernde Blau;", "tokens": ["Wie\u00b7der", "zu\u00b7r\u00fcck", ";", "tief\u00b7klar", "w\u00f6lbt", "sich", "das", "schim\u00b7mern\u00b7de", "Blau", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Siehe, da dr\u00e4ngt sich die Jugend hinab zur spiegelnden Eisbahn,", "tokens": ["Sie\u00b7he", ",", "da", "dr\u00e4ngt", "sich", "die", "Ju\u00b7gend", "hin\u00b7ab", "zur", "spie\u00b7geln\u00b7den", "Eis\u00b7bahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Welche des Nordwinds Hauch \u00fcber der Tiefe gebaut.", "tokens": ["Wel\u00b7che", "des", "Nord\u00b7winds", "Hauch", "\u00fc\u00b7ber", "der", "Tie\u00b7fe", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Auf der gediegenen Flut welch buntes Gewimmel! Es wiegt sich", "tokens": ["Auf", "der", "ge\u00b7die\u00b7ge\u00b7nen", "Flut", "welch", "bun\u00b7tes", "Ge\u00b7wim\u00b7mel", "!", "Es", "wiegt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PWAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "PRF"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Weithin kreisend die Schar auf dem befl\u00fcgelten Stahl.", "tokens": ["Weit\u00b7hin", "krei\u00b7send", "die", "Schar", "auf", "dem", "be\u00b7fl\u00fc\u00b7gel\u00b7ten", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Wie sie sich suchen und fliehn! Hell flattern die Schleier der M\u00e4dchen,", "tokens": ["Wie", "sie", "sich", "su\u00b7chen", "und", "fliehn", "!", "Hell", "flat\u00b7tern", "die", "Schlei\u00b7er", "der", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "KON", "VVINF", "$.", "NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wo sich die Lieblichste zeigt, st\u00fcrmen die J\u00fcnglinge nach.", "tokens": ["Wo", "sich", "die", "Lieb\u00b7lichs\u00b7te", "zeigt", ",", "st\u00fcr\u00b7men", "die", "J\u00fcng\u00b7lin\u00b7ge", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--++-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Zaghaft, nahe dem Ufer versucht sich der Minderge\u00fcbte,", "tokens": ["Zag\u00b7haft", ",", "na\u00b7he", "dem", "U\u00b7fer", "ver\u00b7sucht", "sich", "der", "Min\u00b7der\u00b7ge\u00b7\u00fcb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Doch in die Weite des Sees lockt es den Meister hinaus.", "tokens": ["Doch", "in", "die", "Wei\u00b7te", "des", "Sees", "lockt", "es", "den", "Meis\u00b7ter", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00dcber dem Spiegel von Eis am Hang lehnt sitzend ein schlankes", "tokens": ["\u00dc\u00b7ber", "dem", "Spie\u00b7gel", "von", "Eis", "am", "Hang", "lehnt", "sit\u00b7zend", "ein", "schlan\u00b7kes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "M\u00e4dchen, sie hat das Gewand eben zum Laufe gesch\u00fcrzt.", "tokens": ["M\u00e4d\u00b7chen", ",", "sie", "hat", "das", "Ge\u00b7wand", "e\u00b7ben", "zum", "Lau\u00b7fe", "ge\u00b7sch\u00fcrzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Vor ihr kniet dienstfertig ein Knab', und mit gl\u00fccklichem L\u00e4cheln", "tokens": ["Vor", "ihr", "kniet", "dienst\u00b7fer\u00b7tig", "ein", "Knab'", ",", "und", "mit", "gl\u00fcck\u00b7li\u00b7chem", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Schn\u00fcrt er den blanken Kothurn ihr an den zierlichen Fu\u00df.", "tokens": ["Schn\u00fcrt", "er", "den", "blan\u00b7ken", "Ko\u00b7thurn", "ihr", "an", "den", "zier\u00b7li\u00b7chen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Welch anmutiges Bild, wie sie freundlich zu ihm sich herabneigt,", "tokens": ["Welch", "an\u00b7mu\u00b7ti\u00b7ges", "Bild", ",", "wie", "sie", "freund\u00b7lich", "zu", "ihm", "sich", "her\u00b7ab\u00b7neigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PWAV", "PPER", "ADJD", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Da\u00df ihr Odem das Haar sanft ihm, das lockige, streift,", "tokens": ["Da\u00df", "ihr", "O\u00b7dem", "das", "Haar", "sanft", "ihm", ",", "das", "lo\u00b7cki\u00b7ge", ",", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "W\u00e4hrend er treu sich bem\u00fcht, kunstm\u00e4\u00dfig die Riemen zu schlingen,", "tokens": ["W\u00e4h\u00b7rend", "er", "treu", "sich", "be\u00b7m\u00fcht", ",", "kunst\u00b7m\u00e4\u00b7\u00dfig", "die", "Rie\u00b7men", "zu", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PRF", "VVPP", "$,", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Und den gehobenen Fu\u00df fast mit den Lippen ber\u00fchrt.", "tokens": ["Und", "den", "ge\u00b7ho\u00b7be\u00b7nen", "Fu\u00df", "fast", "mit", "den", "Lip\u00b7pen", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Z\u00f6gernd wend' ich mich ab und gedenk' im erinnernden Herzen,", "tokens": ["Z\u00f6\u00b7gernd", "wend'", "ich", "mich", "ab", "und", "ge\u00b7denk'", "im", "e\u00b7rin\u00b7nern\u00b7den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Wie ich den reizenden Dienst einst Melusinen getan.", "tokens": ["Wie", "ich", "den", "rei\u00b7zen\u00b7den", "Dienst", "einst", "Me\u00b7lu\u00b7si\u00b7nen", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "In das verschneite Gefild' mit stattlich befiederten Rappen", "tokens": ["In", "das", "ver\u00b7schnei\u00b7te", "Ge\u00b7fild'", "mit", "statt\u00b7lich", "be\u00b7fie\u00b7der\u00b7ten", "Rap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Fliegt, von Schellengel\u00e4ut klingend, ein Schlitten hinaus.", "tokens": ["Fliegt", ",", "von", "Schel\u00b7len\u00b7ge\u00b7l\u00e4ut", "klin\u00b7gend", ",", "ein", "Schlit\u00b7ten", "hin\u00b7aus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "VVPP", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Weithin blitzt das Metall des Geschirrs, und die Vliese der Pardel,", "tokens": ["Weit\u00b7hin", "blitzt", "das", "Me\u00b7tall", "des", "Ge\u00b7schirrs", ",", "und", "die", "Vlie\u00b7se", "der", "Par\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Pr\u00e4chtig mit Purpur ges\u00e4umt, bl\u00e4hn sich gehoben im Wind.", "tokens": ["Pr\u00e4ch\u00b7tig", "mit", "Pur\u00b7pur", "ge\u00b7s\u00e4umt", ",", "bl\u00e4hn", "sich", "ge\u00b7ho\u00b7ben", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,", "VVFIN", "PRF", "VVPP", "APPRART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Aber die Jungfrau schmiegt an den Freund sich mit brennenden Wangen,", "tokens": ["A\u00b7ber", "die", "Jung\u00b7frau", "schmiegt", "an", "den", "Freund", "sich", "mit", "bren\u00b7nen\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Der das erlesne Gespann kr\u00e4ftig und sicher beherrscht.", "tokens": ["Der", "das", "er\u00b7les\u00b7ne", "Ge\u00b7spann", "kr\u00e4f\u00b7tig", "und", "si\u00b7cher", "be\u00b7herrscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Eros flattert den Rossen voraus, und im gastlichen Forsthaus", "tokens": ["E\u00b7ros", "flat\u00b7tert", "den", "Ros\u00b7sen", "vo\u00b7raus", ",", "und", "im", "gast\u00b7li\u00b7chen", "Forst\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "F\u00fcr das beg\u00fcnstigte Paar deckt er den Tisch am Kamin.", "tokens": ["F\u00fcr", "das", "be\u00b7g\u00fcns\u00b7tig\u00b7te", "Paar", "deckt", "er", "den", "Tisch", "am", "Ka\u00b7min", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Kahl steht jeglicher Strauch, doch l\u00e4\u00dft uns der Winter die Rosen,", "tokens": ["Kahl", "steht", "jeg\u00b7li\u00b7cher", "Strauch", ",", "doch", "l\u00e4\u00dft", "uns", "der", "Win\u00b7ter", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Die er der Erde geraubt, feurig am Himmel erbl\u00fchn.", "tokens": ["Die", "er", "der", "Er\u00b7de", "ge\u00b7raubt", ",", "feu\u00b7rig", "am", "Him\u00b7mel", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sieh, welch seliger Glanz aus den lodernden G\u00e4rten herabstr\u00f6mt!", "tokens": ["Sieh", ",", "welch", "se\u00b7li\u00b7ger", "Glanz", "aus", "den", "lo\u00b7dern\u00b7den", "G\u00e4r\u00b7ten", "her\u00b7ab\u00b7str\u00f6mt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "\u00dcber das silberne Feld flutet ein purpurner Duft,", "tokens": ["\u00dc\u00b7ber", "das", "sil\u00b7ber\u00b7ne", "Feld", "flu\u00b7tet", "ein", "pur\u00b7pur\u00b7ner", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--++-+++-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und der entbl\u00e4tterte Wald, vom Rauhreif zierlich umfiedert,", "tokens": ["Und", "der", "ent\u00b7bl\u00e4t\u00b7ter\u00b7te", "Wald", ",", "vom", "Rauh\u00b7reif", "zier\u00b7lich", "um\u00b7fie\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gl\u00fcht, in den Schimmer getaucht, rot wie Korallenge\u00e4st.", "tokens": ["Gl\u00fcht", ",", "in", "den", "Schim\u00b7mer", "ge\u00b7taucht", ",", "rot", "wie", "Ko\u00b7ral\u00b7len\u00b7ge\u00b7\u00e4st", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}