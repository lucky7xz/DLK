{"textgrid.poem.45910": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[sieh dort auf dem Wiesengrunde]", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh dort auf dem Wiesengrunde", "tokens": ["Sieh", "dort", "auf", "dem", "Wie\u00b7sen\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzen jetzt ein Elfchen munter", "tokens": ["Tan\u00b7zen", "jetzt", "ein", "Elf\u00b7chen", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterm Rosenbusch hinunter,", "tokens": ["Un\u00b7term", "Ro\u00b7sen\u00b7busch", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die Bl\u00e4tter niederstreut.", "tokens": ["Der", "die", "Bl\u00e4t\u00b7ter", "nie\u00b7der\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Elfchen spielen Lotto heut,", "tokens": ["Elf\u00b7chen", "spie\u00b7len", "Lot\u00b7to", "heut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreiben auf die Bl\u00e4tter Nummern,", "tokens": ["Schrei\u00b7ben", "auf", "die", "Bl\u00e4t\u00b7ter", "Num\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja du darfst nur k\u00fchnlich schlummern,", "tokens": ["Ja", "du", "darfst", "nur", "k\u00fchn\u00b7lich", "schlum\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn dein Gl\u00fcck kommt dir im Schlummer.", "tokens": ["Denn", "dein", "Gl\u00fcck", "kommt", "dir", "im", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du gewinnst die beste Nummer:", "tokens": ["Du", "ge\u00b7winnst", "die", "bes\u00b7te", "Num\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Braut wirst du im Schlummer,", "tokens": ["Ei\u00b7ne", "Braut", "wirst", "du", "im", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum erwachst du ohne Kummer,", "tokens": ["Drum", "er\u00b7wachst", "du", "oh\u00b7ne", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hochzeit Hochzeit, hohe Zeit. \u2013", "tokens": ["Hoch\u00b7zeit", "Hoch\u00b7zeit", ",", "ho\u00b7he", "Zeit", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh wie scheint der Mond so weit,", "tokens": ["Sieh", "wie", "scheint", "der", "Mond", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Fr\u00f6sche und die Unken", "tokens": ["Und", "die", "Fr\u00f6\u00b7sche", "und", "die", "Un\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singen bei Johannisfunken", "tokens": ["Sin\u00b7gen", "bei", "Jo\u00b7han\u00b7nis\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Metten ganz betrunken.", "tokens": ["Ih\u00b7re", "Met\u00b7ten", "ganz", "be\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Br\u00fcnstig gl\u00fchn Johannisfunken,", "tokens": ["Br\u00fcns\u00b7tig", "gl\u00fchn", "Jo\u00b7han\u00b7nis\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sternlein k\u00fchl am Himmel prunken,", "tokens": ["Stern\u00b7lein", "k\u00fchl", "am", "Him\u00b7mel", "prun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Irrlicht h\u00fcpft betrunken,", "tokens": ["Und", "das", "Irr\u00b7licht", "h\u00fcpft", "be\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo Du gingst ein Jungfr\u00e4ulein.", "tokens": ["Wo", "Du", "gingst", "ein", "Jung\u00b7fr\u00e4u\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf dem Acker gl\u00fcht ein Schein,", "tokens": ["Auf", "dem", "A\u00b7cker", "gl\u00fcht", "ein", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo beim Drachen eingetruhet,", "tokens": ["Wo", "beim", "Dra\u00b7chen", "ein\u00b7ge\u00b7tru\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaltes Gold das rot erglutet,", "tokens": ["Kal\u00b7tes", "Gold", "das", "rot", "er\u00b7glu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fiel dein Kr\u00e4nzlein unvermutet", "tokens": ["Fiel", "dein", "Kr\u00e4nz\u00b7lein", "un\u00b7ver\u00b7mu\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In des Drachen Gruft hinunter", "tokens": ["In", "des", "Dra\u00b7chen", "Gruft", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APPO"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Drache ist gebunden,", "tokens": ["Und", "der", "Dra\u00b7che", "ist", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Schatz ist dir gefunden:", "tokens": ["Und", "der", "Schatz", "ist", "dir", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gold und Silber, Edelstein,", "tokens": ["Gold", "und", "Sil\u00b7ber", ",", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und drei Rosen die sind dein.", "tokens": ["Und", "drei", "Ro\u00b7sen", "die", "sind", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ART", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}