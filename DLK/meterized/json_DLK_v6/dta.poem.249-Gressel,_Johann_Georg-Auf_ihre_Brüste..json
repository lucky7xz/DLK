{"dta.poem.249": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre Br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Englische Br\u00fcste", "tokens": ["Eng\u00b7li\u00b7sche", "Br\u00fcs\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nichts reichet mehr L\u00fcste", "tokens": ["Nichts", "rei\u00b7chet", "mehr", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Als ihr/", "tokens": ["Als", "ihr", "/"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Den feurgen Rubinen/", "tokens": ["Den", "feur\u00b7gen", "Ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den weissen ", "tokens": ["Den", "weis\u00b7sen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Den wallenden Ballen", "tokens": ["Den", "wal\u00b7len\u00b7den", "Bal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Den s\u00fcssen Corallen", "tokens": ["Den", "s\u00fcs\u00b7sen", "Co\u00b7ral\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Milcherne Auen", "tokens": ["Mil\u00b7cher\u00b7ne", "Au\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Jhr lasset mir schauen", "tokens": ["Ihr", "las\u00b7set", "mir", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Frucht:", "tokens": ["Die", "Frucht", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Darinnen verstecket", "tokens": ["Da\u00b7rin\u00b7nen", "ver\u00b7ste\u00b7cket"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Was Anmuht erwecket.", "tokens": ["Was", "An\u00b7muht", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Da man den Leim findet/", "tokens": ["Da", "man", "den", "Leim", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Der Seelen verbindet.", "tokens": ["Der", "See\u00b7len", "ver\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und da man die Speise der Lieblichkeit sucht.", "tokens": ["Und", "da", "man", "die", "Spei\u00b7se", "der", "Lieb\u00b7lich\u00b7keit", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Auf eure H\u00fcgel", "tokens": ["Auf", "eu\u00b7re", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Tr\u00e4gt ", "tokens": ["Tr\u00e4gt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die Saat:", "tokens": ["Die", "Saat", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Daraus denn entspringet", "tokens": ["Da\u00b7raus", "denn", "ent\u00b7sprin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Was Lustbarkeit bringet/", "tokens": ["Was", "Lust\u00b7bar\u00b7keit", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was jedem beliebet/", "tokens": ["Was", "je\u00b7dem", "be\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Was Lieblichkeit giebet/", "tokens": ["Was", "Lieb\u00b7lich\u00b7keit", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und s\u00fcsse Vergn\u00fcgung im Uberflu\u00df hat.", "tokens": ["Und", "s\u00fcs\u00b7se", "Ver\u00b7gn\u00fc\u00b7gung", "im", "U\u00b7ber\u00b7flu\u00df", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}