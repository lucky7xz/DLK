{"dta.poem.23700": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Reue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doris, sieh, die falben Bl&#228;tter,", "tokens": ["Do\u00b7ris", ",", "sieh", ",", "die", "fal\u00b7ben", "Bl", "&#228;", "tter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "$(", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sieh, hier werden sie zu Leichen!", "tokens": ["Sieh", ",", "hier", "wer\u00b7den", "sie", "zu", "Lei\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wilst du nicht den Herbst verachten?", "tokens": ["Wilst", "du", "nicht", "den", "Herbst", "ver\u00b7ach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh, er raubt uns Laub und Schatten", "tokens": ["Sieh", ",", "er", "raubt", "uns", "Laub", "und", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die S\u00e4nger, auf den Zweigen,", "tokens": ["Und", "die", "S\u00e4n\u00b7ger", ",", "auf", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jagt er aus den gr\u00fcnen Zellen", "tokens": ["Jagt", "er", "aus", "den", "gr\u00fc\u00b7nen", "Zel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In die Ritzen hohler Klippen!", "tokens": ["In", "die", "Rit\u00b7zen", "hoh\u00b7ler", "Klip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Werden sie nun noch wol singen?", "tokens": ["Wer\u00b7den", "sie", "nun", "noch", "wol", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Doris, nein, sie werden schweigen,", "tokens": ["Do\u00b7ris", ",", "nein", ",", "sie", "wer\u00b7den", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKANT", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und sie haben schon geschwiegen,", "tokens": ["Und", "sie", "ha\u00b7ben", "schon", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Als du gestern fr\u00fch, im Garten,", "tokens": ["Als", "du", "ge\u00b7stern", "fr\u00fch", ",", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Mich mit tausend K\u00fcssen labtest.", "tokens": ["Mich", "mit", "tau\u00b7send", "K\u00fcs\u00b7sen", "lab\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "O wir werden ihre Lieder", "tokens": ["O", "wir", "wer\u00b7den", "ih\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "K\u00fcssend w\u00fcnschen und nicht h\u00f6ren.", "tokens": ["K\u00fcs\u00b7send", "w\u00fcn\u00b7schen", "und", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "O wie lange wird es w\u00e4hren,", "tokens": ["O", "wie", "lan\u00b7ge", "wird", "es", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da\u00df sie froh zu deinen K\u00fcssen", "tokens": ["Da\u00df", "sie", "froh", "zu", "dei\u00b7nen", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ihre Lieder wieder singen?", "tokens": ["Ih\u00b7re", "Lie\u00b7der", "wie\u00b7der", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Engel, ietzt empfinde Reue;", "tokens": ["En\u00b7gel", ",", "ietzt", "emp\u00b7fin\u00b7de", "Reu\u00b7e", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Denn, am zwanzigsten des Maien,", "tokens": ["Denn", ",", "am", "zwan\u00b7zigs\u00b7ten", "des", "Mai\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Als dich Nachtigallen lokkten,", "tokens": ["Als", "dich", "Nach\u00b7ti\u00b7gal\u00b7len", "lokk\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Woltest du nicht immer k\u00fcssen!", "tokens": ["Wol\u00b7test", "du", "nicht", "im\u00b7mer", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wenn sie k\u00fcnftig wieder lokken,", "tokens": ["Wenn", "sie", "k\u00fcnf\u00b7tig", "wie\u00b7der", "lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wilst du dann nicht immer k\u00fcssen?", "tokens": ["Wilst", "du", "dann", "nicht", "im\u00b7mer", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}