{"textgrid.poem.47546": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wein, o Schenke, das reine, das himmlische Gut bring' her!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wein, o Schenke, das reine, das himmlische Gut bring' her!", "tokens": ["Wein", ",", "o", "Schen\u00b7ke", ",", "das", "rei\u00b7ne", ",", "das", "himm\u00b7li\u00b7sche", "Gut", "bring'", "her", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Fl\u00fcssige Flammen und flammenenthaltende Fluth bring' her!", "tokens": ["Fl\u00fcs\u00b7si\u00b7ge", "Flam\u00b7men", "und", "flam\u00b7me\u00b7nen\u00b7thal\u00b7ten\u00b7de", "Fluth", "bring'", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+--+-+", "measure": "diphilius"}}, "stanza.2": {"line.1": {"text": "Wein, der jeglichen irdischen Harms Medicin und Trost,", "tokens": ["Wein", ",", "der", "jeg\u00b7li\u00b7chen", "ir\u00b7di\u00b7schen", "Harms", "Me\u00b7di\u00b7cin", "und", "Trost", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "ADJA", "NN", "NE", "KON", "NN", "$,"], "meter": "+-+--+---+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der messianische Wunder, unendliche, thut, bring' her;", "tokens": ["Der", "mes\u00b7si\u00b7a\u00b7ni\u00b7sche", "Wun\u00b7der", ",", "un\u00b7end\u00b7li\u00b7che", ",", "thut", ",", "bring'", "her", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "F\u00fcr den st\u00f6rrigen, grilligen Kopf die gewaltigen", "tokens": ["F\u00fcr", "den", "st\u00f6r\u00b7ri\u00b7gen", ",", "gril\u00b7li\u00b7gen", "Kopf", "die", "ge\u00b7wal\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "ART", "ADJA"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Stricke des Weins, bis er rastet in ihnen und ruht, bring' her!", "tokens": ["Stri\u00b7cke", "des", "Weins", ",", "bis", "er", "ras\u00b7tet", "in", "ih\u00b7nen", "und", "ruht", ",", "bring'", "her", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+--+-+", "measure": "diphilius"}}, "stanza.4": {"line.1": {"text": "Schweigt B\u00fclb\u00fclengesang, das erbebende Saitenspiel,", "tokens": ["Schweigt", "B\u00fcl\u00b7b\u00fc\u00b7len\u00b7ge\u00b7sang", ",", "das", "er\u00b7be\u00b7ben\u00b7de", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Gl\u00e4sergeklirr und der Reben erregendes Blut bring' her!", "tokens": ["Gl\u00e4\u00b7ser\u00b7ge\u00b7klirr", "und", "der", "Re\u00b7ben", "er\u00b7re\u00b7gen\u00b7des", "Blut", "bring'", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+--+-+", "measure": "diphilius"}}, "stanza.5": {"line.1": {"text": "Schein' ich ein Trunkener schon, o in Eile noch mehr des Weins,", "tokens": ["Schein'", "ich", "ein", "Trun\u00b7ke\u00b7ner", "schon", ",", "o", "in", "Ei\u00b7le", "noch", "mehr", "des", "Weins", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "$,", "FM", "APPR", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.2": {"text": "Da\u00df sich in Eile vollende mein trunkener Muth, bring' her!", "tokens": ["Da\u00df", "sich", "in", "Ei\u00b7le", "voll\u00b7en\u00b7de", "mein", "trun\u00b7ke\u00b7ner", "Muth", ",", "bring'", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "NN", "ADJA", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+--+-+", "measure": "diphilius"}}, "stanza.6": {"line.1": {"text": "Ein, zwei k\u00f6stliche Flaschen annoch f\u00fcr Hafis, den doch", "tokens": ["Ein", ",", "zwei", "k\u00f6st\u00b7li\u00b7che", "Fla\u00b7schen", "an\u00b7noch", "f\u00fcr", "Ha\u00b7fis", ",", "den", "doch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "CARD", "ADJA", "NN", "ADV", "APPR", "NE", "$,", "PRELS", "ADV"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nie vers\u00f6hnten mit giftiger, m\u00f6nchischer Brut bring' her!", "tokens": ["Nie", "ver\u00b7s\u00f6hn\u00b7ten", "mit", "gif\u00b7ti\u00b7ger", ",", "m\u00f6n\u00b7chi\u00b7scher", "Brut", "bring'", "her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}