{"dta.poem.7995": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbenden Helden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl wieget Eines viele Thaten auf, \u2014", "tokens": ["Wohl", "wie\u00b7get", "Ei\u00b7nes", "vie\u00b7le", "Tha\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie achten drauf \u2014", "tokens": ["Sie", "ach\u00b7ten", "drauf"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das ist um deines Vaterlandes Noth", "tokens": ["Das", "ist", "um", "dei\u00b7nes", "Va\u00b7ter\u00b7lan\u00b7des", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Heldentod.", "tokens": ["Der", "Hel\u00b7den\u00b7tod", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sieh hin! die Feinde fliehen; blick hinan!", "tokens": ["Sieh", "hin", "!", "die", "Fein\u00b7de", "flie\u00b7hen", ";", "blick", "hi\u00b7nan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "VVINF", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Himmel gl\u00e4nzt, dahin ist unsre Bahn!", "tokens": ["Der", "Him\u00b7mel", "gl\u00e4nzt", ",", "da\u00b7hin", "ist", "uns\u00b7re", "Bahn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}