{"textgrid.poem.48799": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "25. An Amorn, wie er wolle von ihm abgebildet sein", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lauf, Amor, suche bald dein Rei\u00dfzeug zu der Hand,", "tokens": ["Lauf", ",", "A\u00b7mor", ",", "su\u00b7che", "bald", "dein", "Rei\u00df\u00b7zeug", "zu", "der", "Hand", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bild' Augen, welche stets mit blassen Tr\u00e4nen quellen,", "tokens": ["bild'", "Au\u00b7gen", ",", "wel\u00b7che", "stets", "mit", "blas\u00b7sen", "Tr\u00e4\u00b7nen", "quel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mal' Wangen, die der Tod hei\u00dft seine Mitgesellen,", "tokens": ["mal'", "Wan\u00b7gen", ",", "die", "der", "Tod", "hei\u00dft", "sei\u00b7ne", "Mit\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mach' einen bleichen Mund und truckner noch als Sand,", "tokens": ["mach'", "ei\u00b7nen", "blei\u00b7chen", "Mund", "und", "truck\u00b7ner", "noch", "als", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "ADJD", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "ein Herze, welches steckt die lichte Loh' in Brand,", "tokens": ["ein", "Her\u00b7ze", ",", "wel\u00b7ches", "steckt", "die", "lich\u00b7te", "Loh'", "in", "Brand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die H\u00e4nde lasse sich den Schmerzen gleiche stellen,", "tokens": ["die", "H\u00e4n\u00b7de", "las\u00b7se", "sich", "den", "Schmer\u00b7zen", "glei\u00b7che", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die F\u00fc\u00dfe matt und krank und die sich selbsten f\u00e4llen,", "tokens": ["die", "F\u00fc\u00b7\u00dfe", "matt", "und", "krank", "und", "die", "sich", "selbs\u00b7ten", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "KON", "PRELS", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "di\u00df menschlich werde Nichts als die Gestalt erkannt.", "tokens": ["di\u00df", "menschlich", "wer\u00b7de", "Nichts", "als", "die", "Ge\u00b7stalt", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "PIS", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "++--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Di\u00df nim und bring' es bald zu meiner ", "tokens": ["Di\u00df", "nim", "und", "bring'", "es", "bald", "zu", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von welcher Sch\u00f6nheit ich so ungestalt bin worden,", "tokens": ["von", "wel\u00b7cher", "Sch\u00f6n\u00b7heit", "ich", "so", "un\u00b7ge\u00b7stalt", "bin", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ADV", "ADJD", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ich wei\u00df, sie kennt das Bild, so balde sie's erblickt.", "tokens": ["ich", "wei\u00df", ",", "sie", "kennt", "das", "Bild", ",", "so", "bal\u00b7de", "sie's", "er\u00b7blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und da\u00df sie desto eh' zur G\u00fcte sich mag kehren,", "tokens": ["Und", "da\u00df", "sie", "des\u00b7to", "eh'", "zur", "G\u00fc\u00b7te", "sich", "mag", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "KOUS", "APPRART", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so zeichne straks hierbei und neben diesen Z\u00e4ren", "tokens": ["so", "zeich\u00b7ne", "straks", "hier\u00b7bei", "und", "ne\u00b7ben", "die\u00b7sen", "Z\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "such meine Seufzer an, die ich nach ihr geschickt!", "tokens": ["such", "mei\u00b7ne", "Seuf\u00b7zer", "an", ",", "die", "ich", "nach", "ihr", "ge\u00b7schickt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}}}}}