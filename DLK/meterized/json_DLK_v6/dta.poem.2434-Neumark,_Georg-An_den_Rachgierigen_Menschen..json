{"dta.poem.2434": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An den Rachgierigen Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Schau deinen Heyland an wie er da steht zerschlagen/", "tokens": ["Schau", "dei\u00b7nen", "Hey\u00b7land", "an", "wie", "er", "da", "steht", "zer\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "KOKOM", "PPER", "ADV", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Peitschen gantz durchritzt beschimpffet und", "tokens": ["Mit", "Peit\u00b7schen", "gantz", "durc\u00b7hritzt", "be\u00b7schimpf\u00b7fet", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "VVFIN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "bespeit/", "tokens": ["be\u00b7speit", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Schau b\u00f6ser S\u00fcndenmensch/ wie manches Hertze", "tokens": ["Schau", "b\u00f6\u00b7ser", "S\u00fcn\u00b7den\u00b7mensch", "/", "wie", "man\u00b7ches", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$(", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "leid", "tokens": ["leid"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Mu\u00df dein Erl\u00f6ser doch \u00fcm deinetwillen tragen/", "tokens": ["Mu\u00df", "dein", "Er\u00b7l\u00f6\u00b7ser", "doch", "\u00fcm", "dei\u00b7net\u00b7wil\u00b7len", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und alles mit Geduld. Und du schwilst bald von Gifft/", "tokens": ["Und", "al\u00b7les", "mit", "Ge\u00b7duld", ".", "Und", "du", "schwilst", "bald", "von", "Gifft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$.", "KON", "PPER", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn dich ob schon mit Recht/ ein kleines Schimpffchen", "tokens": ["Wenn", "dich", "ob", "schon", "mit", "Recht", "/", "ein", "klei\u00b7nes", "Schimpf\u00b7fchen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "ADV", "APPR", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "trifft.", "tokens": ["trifft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}