{"textgrid.poem.59958": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Feige Sinnen weiche sind,", "genre": "verse", "period": "N.A.", "pub_year": 1653, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feige Sinnen weiche sind,", "tokens": ["Fei\u00b7ge", "Sin\u00b7nen", "wei\u00b7che", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weichen, wie von jedem Wind", "tokens": ["Wei\u00b7chen", ",", "wie", "von", "je\u00b7dem", "Wind"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird ein Wetterhahn gedrehet.", "tokens": ["Wird", "ein", "Wet\u00b7ter\u00b7hahn", "ge\u00b7dre\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00dfer Muth steht unverwandt,", "tokens": ["Gro\u00b7\u00dfer", "Muth", "steht", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fest gegr\u00fcndet auf Bestand,", "tokens": ["Fest", "ge\u00b7gr\u00fcn\u00b7det", "auf", "Be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den kein Nordensturm umwehet.", "tokens": ["Den", "kein", "Nor\u00b7dens\u00b7turm", "um\u00b7we\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df die Wellen brausen her,", "tokens": ["La\u00df", "die", "Wel\u00b7len", "brau\u00b7sen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die Winde sausen sehr,", "tokens": ["La\u00df", "die", "Win\u00b7de", "sau\u00b7sen", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df den hei\u00dfen Mittag stechen:", "tokens": ["La\u00df", "den", "hei\u00b7\u00dfen", "Mit\u00b7tag", "ste\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ungl\u00fcck, Noth und Ungemach,", "tokens": ["Un\u00b7gl\u00fcck", ",", "Noth", "und", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst der Tod ist viel zu schwach,", "tokens": ["Selbst", "der", "Tod", "ist", "viel", "zu", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen festen Muth zu brechen.", "tokens": ["Ei\u00b7nen", "fes\u00b7ten", "Muth", "zu", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer den Dank erlaufen will,", "tokens": ["Wer", "den", "Dank", "er\u00b7lau\u00b7fen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df durchaus nicht halten still,", "tokens": ["Mu\u00df", "durc\u00b7haus", "nicht", "hal\u00b7ten", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis er hat das Ziel erreichet.", "tokens": ["Bis", "er", "hat", "das", "Ziel", "er\u00b7rei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00e4mpfen bringet keine Kron',", "tokens": ["K\u00e4mp\u00b7fen", "brin\u00b7get", "kei\u00b7ne", "Kron'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn man eher l\u00e4\u00dft davon,", "tokens": ["Wenn", "man", "e\u00b7her", "l\u00e4\u00dft", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als der Feind bezwungen weichet.", "tokens": ["Als", "der", "Feind", "be\u00b7zwun\u00b7gen", "wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch Bestand die Tugend wird", "tokens": ["Durch", "Be\u00b7stand", "die", "Tu\u00b7gend", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In Vollkommenheit gef\u00fchrt,", "tokens": ["In", "Voll\u00b7kom\u00b7men\u00b7heit", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eingepflanzt in das Gem\u00fcthe,", "tokens": ["Ein\u00b7ge\u00b7pflanzt", "in", "das", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Wille nichts mehr will,", "tokens": ["Da\u00df", "der", "Wil\u00b7le", "nichts", "mehr", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als was dem Verstand gefiel,", "tokens": ["Als", "was", "dem", "Ver\u00b7stand", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Tugend wallet im Gebl\u00fcte.", "tokens": ["Tu\u00b7gend", "wal\u00b7let", "im", "Ge\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Endlich doch beh\u00e4lt Bestand", "tokens": ["End\u00b7lich", "doch", "be\u00b7h\u00e4lt", "Be\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl vergn\u00fcgt die Oberhand,", "tokens": ["Wohl", "ver\u00b7gn\u00fcgt", "die", "O\u00b7ber\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehet seine Sorgenwende.", "tokens": ["Sie\u00b7het", "sei\u00b7ne", "Sor\u00b7gen\u00b7wen\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df denn nichts dich f\u00fchren ab,", "tokens": ["La\u00df", "denn", "nichts", "dich", "f\u00fch\u00b7ren", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PIS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denk', da\u00df nur Belohnung hab',", "tokens": ["Denk'", ",", "da\u00df", "nur", "Be\u00b7loh\u00b7nung", "hab'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer beharret bis an's Ende.", "tokens": ["Wer", "be\u00b7har\u00b7ret", "bis", "an's", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}