{"dta.poem.3583": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Knabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.71", "cy:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mys Lieb' isch gar wyt inne,               ", "tokens": ["Mys", "Lieb'", "isch", "gar", "wyt", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort inne uf der steinige Fluh;", "tokens": ["Dort", "in\u00b7ne", "uf", "der", "stei\u00b7ni\u00b7ge", "Fluh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wenn i scho zun ihm wetti,", "tokens": ["Wenn", "i", "scho", "zun", "ihm", "wet\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "FM", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "O so reute mi di Schuh!", "tokens": ["O", "so", "reu\u00b7te", "mi", "di", "Schuh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "NE", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La du di dSchuh nit reuen,", "tokens": ["La", "du", "di", "dSchuh", "nit", "reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leg du dine Bant\u00f6ffeli a;", "tokens": ["Leg", "du", "di\u00b7ne", "Ban\u00b7t\u00f6f\u00b7fe\u00b7li", "a", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "We du si de hest broche,", "tokens": ["We", "du", "si", "de", "hest", "bro\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "So chast ja de angeri ha.", "tokens": ["So", "chast", "ja", "de", "an\u00b7ge\u00b7ri", "ha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "NE", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}