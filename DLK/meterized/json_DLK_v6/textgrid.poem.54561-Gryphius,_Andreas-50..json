{"textgrid.poem.54561": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der standhafft reine Geist/ der f\u00fcr das Recht der Welt", "tokens": ["Der", "stand\u00b7hafft", "rei\u00b7ne", "Geist", "/", "der", "f\u00fcr", "das", "Recht", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ehr/ Amt/ und Ruhm und Gold/ Hof/ Kind und Leib auffsetzet/", "tokens": ["Ehr", "/", "Amt", "/", "und", "Ruhm", "und", "Gold", "/", "Hof", "/", "Kind", "und", "Leib", "auff\u00b7set\u00b7zet", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "KON", "NN", "KON", "NN", "$(", "NE", "$(", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lebt/ ob des K\u00e4ysers Beil schon Fleisch und Hals verletzet.", "tokens": ["Lebt", "/", "ob", "des", "K\u00e4y\u00b7sers", "Beil", "schon", "Fleisch", "und", "Hals", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "ART", "NN", "NN", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und pocht was Erd und Zeit in engen Gr\u00e4ntzen h\u00e4lt.", "tokens": ["Und", "pocht", "was", "Erd", "und", "Zeit", "in", "en\u00b7gen", "Gr\u00e4nt\u00b7zen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er h\u00f6hnt den trotzen Todt/ er steigt indem er f\u00e4llt/", "tokens": ["Er", "h\u00f6hnt", "den", "trot\u00b7zen", "Todt", "/", "er", "steigt", "in\u00b7dem", "er", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und schr\u00f6ckt/ und tritt/ und bricht/ was ihm sich widersetzet", "tokens": ["Und", "schr\u00f6ckt", "/", "und", "tritt", "/", "und", "bricht", "/", "was", "ihm", "sich", "wi\u00b7der\u00b7set\u00b7zet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KON", "VVFIN", "$(", "KON", "VVFIN", "$(", "PWS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In Purpur/ die er selbst mit heilgen Blut genetzet/", "tokens": ["In", "Pur\u00b7pur", "/", "die", "er", "selbst", "mit", "heil\u00b7gen", "Blut", "ge\u00b7net\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als er der ", "tokens": ["Als", "er", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.9": {"text": "Ich wei\u00df ihn Held zu dir/ so/ wie er mir erschienen/", "tokens": ["Ich", "wei\u00df", "ihn", "Held", "zu", "dir", "/", "so", "/", "wie", "er", "mir", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "PPER", "$(", "ADV", "$(", "PWAV", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als er zu seinem Dienst die schwache Faust erkohr;", "tokens": ["Als", "er", "zu", "sei\u00b7nem", "Dienst", "die", "schwa\u00b7che", "Faust", "er\u00b7kohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "H\u00f6r an/ wofern er sich zu reden darff erk\u00fchnen/", "tokens": ["H\u00f6r", "an", "/", "wo\u00b7fern", "er", "sich", "zu", "re\u00b7den", "darff", "er\u00b7k\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$(", "KOUS", "PPER", "PRF", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein \u00fcberwunden Angst/ mit gn\u00e4dig-offnem Ohr/", "tokens": ["Sein", "\u00fc\u00b7berw\u00b7un\u00b7den", "Angst", "/", "mit", "gn\u00e4\u00b7dig\u00b7off\u00b7nem", "Ohr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wem m\u00f6cht er lieber wol sein herrlich Leid erzehlen/", "tokens": ["Wem", "m\u00f6cht", "er", "lie\u00b7ber", "wol", "sein", "herr\u00b7lich", "Leid", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als dem der Ungl\u00fcck hier wolt ihm zum Ruhm erwehlen.", "tokens": ["Als", "dem", "der", "Un\u00b7gl\u00fcck", "hier", "wolt", "ihm", "zum", "Ruhm", "er\u00b7weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}