{"dta.poem.15969": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mond rollt um die Erd', und um die Sonne sie,", "tokens": ["Der", "Mond", "rollt", "um", "die", "Erd'", ",", "und", "um", "die", "Son\u00b7ne", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die um h\u00f6here Sonn', und um noch h\u00f6here die!", "tokens": ["Und", "die", "um", "h\u00f6\u00b7he\u00b7re", "Sonn'", ",", "und", "um", "noch", "h\u00f6\u00b7he\u00b7re", "die", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "$,", "KON", "APPR", "ADV", "VVFIN", "ART", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und immer weiter so, und immer weiter nur?", "tokens": ["Und", "im\u00b7mer", "wei\u00b7ter", "so", ",", "und", "im\u00b7mer", "wei\u00b7ter", "nur", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "KON", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der Unendlichkeit verliert der Geist die Spur.", "tokens": ["In", "der", "Un\u00b7end\u00b7lich\u00b7keit", "ver\u00b7liert", "der", "Geist", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Unendlich sei die Kraft, unendlich sei das Leben,", "tokens": ["Un\u00b7end\u00b7lich", "sei", "die", "Kraft", ",", "un\u00b7end\u00b7lich", "sei", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nicht unendlich sei der Raum deswegen eben.", "tokens": ["Doch", "nicht", "un\u00b7end\u00b7lich", "sei", "der", "Raum", "des\u00b7we\u00b7gen", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VAFIN", "ART", "NN", "PAV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was w\u00e4r' Unendlichkeit die \u00e4u\u00dferliche so?", "tokens": ["Was", "w\u00e4r'", "Un\u00b7end\u00b7lich\u00b7keit", "die", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7che", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der innerlichen nur des Geistes bin ich froh.", "tokens": ["Der", "in\u00b7ner\u00b7li\u00b7chen", "nur", "des", "Geis\u00b7tes", "bin", "ich", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jenseit der K\u00f6rperwelt mu\u00df eine Lichtwelt stehn,", "tokens": ["Jen\u00b7seit", "der", "K\u00f6r\u00b7per\u00b7welt", "mu\u00df", "ei\u00b7ne", "Licht\u00b7welt", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus der sie niedersank, in die sie auf will gehn.", "tokens": ["Aus", "der", "sie", "nie\u00b7der\u00b7sank", ",", "in", "die", "sie", "auf", "will", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Sonnen leuchten nicht von ihrem eignen Lichte,", "tokens": ["Die", "Son\u00b7nen", "leuch\u00b7ten", "nicht", "von", "ih\u00b7rem", "eig\u00b7nen", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie leuchten von dem Licht auf Gottes Angesichte.", "tokens": ["Sie", "leuch\u00b7ten", "von", "dem", "Licht", "auf", "Got\u00b7tes", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Licht ist das geistige Kleid, das diese Welt umflie\u00dft,", "tokens": ["Licht", "ist", "das", "geis\u00b7ti\u00b7ge", "Kleid", ",", "das", "die\u00b7se", "Welt", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Das sich an jedes Glied des gro\u00dfen Leibes schlie\u00dft.", "tokens": ["Das", "sich", "an", "je\u00b7des", "Glied", "des", "gro\u00b7\u00dfen", "Lei\u00b7bes", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dis geistige Netz, gewebt aus Gottes Liebesblicken,", "tokens": ["Dis", "geis\u00b7ti\u00b7ge", "Netz", ",", "ge\u00b7webt", "aus", "Got\u00b7tes", "Lie\u00b7bes\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Will immer br\u00fcnstiger die K\u00f6rperwelt umstricken.", "tokens": ["Will", "im\u00b7mer", "br\u00fcns\u00b7ti\u00b7ger", "die", "K\u00f6r\u00b7per\u00b7welt", "um\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und jedes Glied schlie\u00dft an ein h\u00f6heres sich an,", "tokens": ["Und", "je\u00b7des", "Glied", "schlie\u00dft", "an", "ein", "h\u00f6\u00b7he\u00b7res", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch dessen Zug es will gezogen seyn hinan.", "tokens": ["Durch", "des\u00b7sen", "Zug", "es", "will", "ge\u00b7zo\u00b7gen", "seyn", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VMFIN", "VVPP", "VAINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Zu Sonnen werden, die sich stark im Licht verkl\u00e4ren,", "tokens": ["Zu", "Son\u00b7nen", "wer\u00b7den", ",", "die", "sich", "stark", "im", "Licht", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "$,", "PRELS", "PRF", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von deren Ausflu\u00df dann die schw\u00e4cheren sich n\u00e4hren.", "tokens": ["Von", "de\u00b7ren", "Aus\u00b7flu\u00df", "dann", "die", "schw\u00e4\u00b7che\u00b7ren", "sich", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "ADJA", "PRF", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Doch wie sie nach dem Saum des Lichtes ewig greifen,", "tokens": ["Doch", "wie", "sie", "nach", "dem", "Saum", "des", "Lich\u00b7tes", "e\u00b7wig", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Sonnen werden auch die letzten endlich reifen.", "tokens": ["Zu", "Son\u00b7nen", "wer\u00b7den", "auch", "die", "letz\u00b7ten", "end\u00b7lich", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Und was auf ihnen ist, reift durch der Sonnen Kraft,", "tokens": ["Und", "was", "auf", "ih\u00b7nen", "ist", ",", "reift", "durch", "der", "Son\u00b7nen", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "VAFIN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Welt wird durch und durch mehr und mehr sonnenhaft.", "tokens": ["Die", "Welt", "wird", "durch", "und", "durch", "mehr", "und", "mehr", "son\u00b7nen\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "KON", "APPR", "PIAT", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "O Geist, mit diesem Thau mu\u00dft du dich auch befeuchten,", "tokens": ["O", "Geist", ",", "mit", "die\u00b7sem", "Thau", "mu\u00dft", "du", "dich", "auch", "be\u00b7feuch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PDAT", "NN", "VMFIN", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du in diesem Bau mit willst als Sonne leuchten.", "tokens": ["Wenn", "du", "in", "die\u00b7sem", "Bau", "mit", "willst", "als", "Son\u00b7ne", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "APPR", "ADJD", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}