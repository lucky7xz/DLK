{"textgrid.poem.52114": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pamphilia ist Frau, du suchst Sie auszutragen,", "tokens": ["Pam\u00b7phi\u00b7lia", "ist", "Frau", ",", "du", "suchst", "Sie", "aus\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Stadt wei\u00df nichts von ihr als Liebs' und Guts zu sagen:", "tokens": ["Die", "Stadt", "wei\u00df", "nichts", "von", "ihr", "als", "Liebs'", "und", "Guts", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "PPER", "KOUS", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Athem stinckt dir selbst: Nun, das ist allen kund;", "tokens": ["Der", "A\u00b7them", "stinckt", "dir", "selbst", ":", "Nun", ",", "das", "ist", "al\u00b7len", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "ADV", "$,", "PDS", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Hintere bey ihr ist reiner, als dein Mund.", "tokens": ["Der", "Hin\u00b7te\u00b7re", "bey", "ihr", "ist", "rei\u00b7ner", ",", "als", "dein", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "A