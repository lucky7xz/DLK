{"textgrid.poem.28608": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df was gehet auff,", "tokens": ["Da\u00df", "was", "ge\u00b7het", "auff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sauffet tapffer drauff!", "tokens": ["Sauf\u00b7fet", "tapf\u00b7fer", "drauff", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Leicht ist zu gedencken,", "tokens": ["Leicht", "ist", "zu", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Magen mu\u00df man tr\u00e4ncken;", "tokens": ["Ma\u00b7gen", "mu\u00df", "man", "tr\u00e4n\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wo da ist ein F\u00fcrst,", "tokens": ["Wo", "da", "ist", "ein", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ist auch wer, den d\u00fcrst.", "tokens": ["Ist", "auch", "wer", ",", "den", "d\u00fcrst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "$,", "PRELS", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}