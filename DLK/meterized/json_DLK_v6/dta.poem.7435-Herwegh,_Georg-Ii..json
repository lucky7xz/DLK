{"dta.poem.7435": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte hingehn wie das Abendrot", "tokens": ["Ich", "m\u00f6ch\u00b7te", "hin\u00b7gehn", "wie", "das", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie der Tag mit seinen letzten Gluten \u2014", "tokens": ["Und", "wie", "der", "Tag", "mit", "sei\u00b7nen", "letz\u00b7ten", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O leichter, sanfter, ungef\u00fchlter Tod! \u2014", "tokens": ["O", "leich\u00b7ter", ",", "sanf\u00b7ter", ",", "un\u00b7ge\u00b7f\u00fchl\u00b7ter", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich in den Schoos des Ewigen verbluten.", "tokens": ["Mich", "in", "den", "Schoos", "des", "E\u00b7wi\u00b7gen", "ver\u00b7blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6chte hingehn wie der heitre Stern,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "hin\u00b7gehn", "wie", "der", "heit\u00b7re", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im vollsten Glanz, in ungeschw\u00e4chtem Blinken;", "tokens": ["Im", "volls\u00b7ten", "Glanz", ",", "in", "un\u00b7ge\u00b7schw\u00e4ch\u00b7tem", "Blin\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So stille und so schmerzlos m\u00f6chte gern", "tokens": ["So", "stil\u00b7le", "und", "so", "schmerz\u00b7los", "m\u00f6ch\u00b7te", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADV", "ADJD", "VMFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich in des Himmels blaue Tiefen sinken.", "tokens": ["Ich", "in", "des", "Him\u00b7mels", "blau\u00b7e", "Tie\u00b7fen", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6chte hingehn wie der Blume Duft,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "hin\u00b7gehn", "wie", "der", "Blu\u00b7me", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der freudig sich dem sch\u00f6nen Kelch entringet", "tokens": ["Der", "freu\u00b7dig", "sich", "dem", "sch\u00f6\u00b7nen", "Kelch", "ent\u00b7rin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auf dem Fittig bl\u00fctenschwangrer Luft", "tokens": ["Und", "auf", "dem", "Fit\u00b7tig", "bl\u00fc\u00b7ten\u00b7schwang\u00b7rer", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Weihrauch auf des Herren Altar schwinget.", "tokens": ["Als", "Weih\u00b7rauch", "auf", "des", "Her\u00b7ren", "Al\u00b7tar", "schwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich m\u00f6chte hingehn wie der Thau im Thal,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "hin\u00b7gehn", "wie", "der", "Thau", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn durstig ihm des Morgens Feuer winken;", "tokens": ["Wenn", "durs\u00b7tig", "ihm", "des", "Mor\u00b7gens", "Feu\u00b7er", "win\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O wollte Gott, wie ihn der Sonnenstrahl,", "tokens": ["O", "woll\u00b7te", "Gott", ",", "wie", "ihn", "der", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NN", "$,", "PWAV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch meine lebensm\u00fcde Seele trinken!", "tokens": ["Auch", "mei\u00b7ne", "le\u00b7bens\u00b7m\u00fc\u00b7de", "See\u00b7le", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich m\u00f6chte hingehn wie der bange Ton,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "hin\u00b7gehn", "wie", "der", "ban\u00b7ge", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der aus den Saiten einer Harfe dringet,", "tokens": ["Der", "aus", "den", "Sai\u00b7ten", "ei\u00b7ner", "Har\u00b7fe", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, kaum dem irdischen Metall entflohn,", "tokens": ["Und", ",", "kaum", "dem", "ir\u00b7di\u00b7schen", "Me\u00b7tall", "ent\u00b7flohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wohllaut in des Sch\u00f6pfers Brust verklinget.", "tokens": ["Ein", "Wohl\u00b7laut", "in", "des", "Sch\u00f6p\u00b7fers", "Brust", "ver\u00b7klin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du wirst nicht hingehn wie das Abendrot,", "tokens": ["Du", "wirst", "nicht", "hin\u00b7gehn", "wie", "das", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wirst nicht stille wie der Stern versinken,", "tokens": ["Du", "wirst", "nicht", "stil\u00b7le", "wie", "der", "Stern", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVFIN", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du stirbst nicht einer Blume leichten Tod,", "tokens": ["Du", "stirbst", "nicht", "ei\u00b7ner", "Blu\u00b7me", "leich\u00b7ten", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Morgenstrahl wird Deine Seele trinken.", "tokens": ["Kein", "Mor\u00b7gen\u00b7strahl", "wird", "Dei\u00b7ne", "See\u00b7le", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wohl wirst Du hingehn, hingehn ohne Spur,", "tokens": ["Wohl", "wirst", "Du", "hin\u00b7gehn", ",", "hin\u00b7gehn", "oh\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch wird das Elend Deine Kraft erst schw\u00e4chen,", "tokens": ["Doch", "wird", "das", "E\u00b7lend", "Dei\u00b7ne", "Kraft", "erst", "schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sanft stirbt es einzig sich in der Natur,", "tokens": ["Sanft", "stirbt", "es", "ein\u00b7zig", "sich", "in", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das arme Menschenherz mu\u00df st\u00fcckweis brechen.", "tokens": ["Das", "ar\u00b7me", "Men\u00b7schen\u00b7herz", "mu\u00df", "st\u00fcck\u00b7weis", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}