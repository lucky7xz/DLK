{"dta.poem.17931": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "174.  Jm Meer seind viel eins.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Viel K\u00f6rnlein seind ein Brodt/ ein Meer viel tr\u00f6pffelein", "tokens": ["Viel", "K\u00f6rn\u00b7lein", "seind", "ein", "Brodt", "/", "ein", "Meer", "viel", "tr\u00f6pf\u00b7fe\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So seind auch unser viel in GOtt ein einges ein.", "tokens": ["So", "seind", "auch", "un\u00b7ser", "viel", "in", "Gott", "ein", "ein\u00b7ges", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ADV", "APPR", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}