{"textgrid.poem.40655": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: O tr\u00e4ume, was dein Herz einst mocht' erregen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O tr\u00e4ume, was dein Herz einst mocht' erregen,", "tokens": ["O", "tr\u00e4u\u00b7me", ",", "was", "dein", "Herz", "einst", "mocht'", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schau' in Ekstasen, was versagt dem Wachen,", "tokens": ["Schau'", "in", "Eks\u00b7ta\u00b7sen", ",", "was", "ver\u00b7sagt", "dem", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Besieg' im Traum den alten S\u00fcndendrachen;", "tokens": ["Be\u00b7sieg'", "im", "Traum", "den", "al\u00b7ten", "S\u00fcn\u00b7den\u00b7dra\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sch\u00fctt' aus den edlen Zorn in Wetterschl\u00e4gen;", "tokens": ["Sch\u00fctt'", "aus", "den", "ed\u00b7len", "Zorn", "in", "Wet\u00b7ter\u00b7schl\u00e4\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch sieh auch deines, unsres Zornes Segen:", "tokens": ["Doch", "sieh", "auch", "dei\u00b7nes", ",", "uns\u00b7res", "Zor\u00b7nes", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PPOSAT", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wort, entknechtet, gro\u00dfe That entfachen,", "tokens": ["Das", "Wort", ",", "ent\u00b7knech\u00b7tet", ",", "gro\u00b7\u00dfe", "That", "ent\u00b7fa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O sieh des Vaterlands glorreich Erwachen,", "tokens": ["O", "sieh", "des", "Va\u00b7ter\u00b7lands", "glor\u00b7reich", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Saatenjubel nach Gewitterregen!", "tokens": ["Den", "Saa\u00b7ten\u00b7ju\u00b7bel", "nach", "Ge\u00b7wit\u00b7ter\u00b7re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das sch\u00f6ne Deutschland einig, frei und m\u00e4chtig;", "tokens": ["Das", "sch\u00f6\u00b7ne", "Deutschland", "ei\u00b7nig", ",", "frei", "und", "m\u00e4ch\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Weisheit h\u00e4lt das Buch, das Recht den Degen,", "tokens": ["Die", "Weis\u00b7heit", "h\u00e4lt", "das", "Buch", ",", "das", "Recht", "den", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Hader nur lie\u00df sie in Ketten legen.", "tokens": ["Den", "Ha\u00b7der", "nur", "lie\u00df", "sie", "in", "Ket\u00b7ten", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "O schwelgerisches Wahnbild, stolz und pr\u00e4chtig!", "tokens": ["O", "schwel\u00b7ge\u00b7ri\u00b7sches", "Wahn\u00b7bild", ",", "stolz", "und", "pr\u00e4ch\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Fieber nur darf dran die Augen weiden \u2013", "tokens": ["Das", "Fie\u00b7ber", "nur", "darf", "dran", "die", "Au\u00b7gen", "wei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PAV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weh, der Gesunde mu\u00df den Kranken neiden.", "tokens": ["Weh", ",", "der", "Ge\u00b7sun\u00b7de", "mu\u00df", "den", "Kran\u00b7ken", "nei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}