{"dta.poem.19714": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Maria Geburt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie die lieb Waldv\u00f6gelein,               ", "tokens": ["Gleich", "wie", "die", "lieb", "Wald\u00b7v\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihren Stimmen gro\u00df und klein", "tokens": ["Mit", "ih\u00b7ren", "Stim\u00b7men", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fr\u00fch morgens lieblich singen,", "tokens": ["Fr\u00fch", "mor\u00b7gens", "lieb\u00b7lich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sobald anbricht die Morgenr\u00f6th,", "tokens": ["So\u00b7bald", "an\u00b7bricht", "die", "Mor\u00b7gen\u00b7r\u00f6th", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenns purpurfarb am Himmel steht,", "tokens": ["Wenns", "pur\u00b7purf\u00b7arb", "am", "Him\u00b7mel", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Berg und Thal sie klingen.", "tokens": ["In", "Berg", "und", "Thal", "sie", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Also ihr Menschen kommt herbey,", "tokens": ["Al\u00b7so", "ihr", "Men\u00b7schen", "kommt", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "La\u00dft h\u00f6ren eure Melodey,", "tokens": ["La\u00dft", "h\u00f6\u00b7ren", "eu\u00b7re", "Me\u00b7lo\u00b7dey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kindelein zu gr\u00fcssen.", "tokens": ["Das", "Kin\u00b7de\u00b7lein", "zu", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heut fr\u00f6hlig sein Geburtstag f\u00e4llt,", "tokens": ["Heut", "fr\u00f6h\u00b7lig", "sein", "Ge\u00b7burts\u00b7tag", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sankt Anna bringt es auf die Welt,", "tokens": ["Sankt", "An\u00b7na", "bringt", "es", "auf", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es lasset euch geniessen.", "tokens": ["Es", "las\u00b7set", "euch", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Morgenr\u00f6th so k\u00fchl und na\u00df,", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6th", "so", "k\u00fchl", "und", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6nen Blumen, Laub und Gras", "tokens": ["Die", "sch\u00f6\u00b7nen", "Blu\u00b7men", ",", "Laub", "und", "Gras"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich alle freundlich neigen,", "tokens": ["Sich", "al\u00b7le", "freund\u00b7lich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil dieses Kind mit G\u00fctigkeit", "tokens": ["Weil", "die\u00b7ses", "Kind", "mit", "G\u00fc\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erquicket ihre Mattigkeit,", "tokens": ["Er\u00b7quic\u00b7ket", "ih\u00b7re", "Mat\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie ihren Dank so zeigen.", "tokens": ["Sie", "ih\u00b7ren", "Dank", "so", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Also, weil wie der Morgenthau,", "tokens": ["Al\u00b7so", ",", "weil", "wie", "der", "Mor\u00b7gent\u00b7hau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Heut aufgeht unsre liebe Frau", "tokens": ["Heut", "auf\u00b7geht", "uns\u00b7re", "lie\u00b7be", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Trost der armen Seelen,", "tokens": ["Zum", "Trost", "der", "ar\u00b7men", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Demuth gr\u00fc\u00df sie jedermann,", "tokens": ["In", "De\u00b7muth", "gr\u00fc\u00df", "sie", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn sie ists, die uns tr\u00f6sten kann", "tokens": ["Denn", "sie", "ists", ",", "die", "uns", "tr\u00f6s\u00b7ten", "kann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In aller Trauer Qu\u00e4len.", "tokens": ["In", "al\u00b7ler", "Trau\u00b7er", "Qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}