{"dta.poem.7296": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ein milder Wintertag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "An jenes Waldes Enden,", "tokens": ["An", "je\u00b7nes", "Wal\u00b7des", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo still der Weiher liegt", "tokens": ["Wo", "still", "der", "Wei\u00b7her", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4ngs den Fichtenw\u00e4nden", "tokens": ["Und", "l\u00e4ngs", "den", "Fich\u00b7ten\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich lind Gemurmel wiegt:", "tokens": ["Sich", "lind", "Ge\u00b7mur\u00b7mel", "wiegt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo in der Sonnenhelle,", "tokens": ["Wo", "in", "der", "Son\u00b7nen\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So matt und kalt sie ist,", "tokens": ["So", "matt", "und", "kalt", "sie", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch immerfort die Welle", "tokens": ["Doch", "im\u00b7mer\u00b7fort", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Ufer flimmernd k\u00fc\u00dft:", "tokens": ["Das", "U\u00b7fer", "flim\u00b7mernd", "k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da wei\u00df ich, sch\u00f6n zum Malen,", "tokens": ["Da", "wei\u00df", "ich", ",", "sch\u00f6n", "zum", "Ma\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch eine schmale Schlucht,", "tokens": ["Noch", "ei\u00b7ne", "schma\u00b7le", "Schlucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo all' die kleinen Strahlen", "tokens": ["Wo", "all'", "die", "klei\u00b7nen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich fangen in der Bucht;", "tokens": ["Sich", "fan\u00b7gen", "in", "der", "Bucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein trocken, windstill Eckchen,", "tokens": ["Ein", "tro\u00b7cken", ",", "wind\u00b7still", "Eck\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und so an Gr\u00fcne reich,", "tokens": ["Und", "so", "an", "Gr\u00fc\u00b7ne", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df auf dem ganzen Fleckchen", "tokens": ["Da\u00df", "auf", "dem", "gan\u00b7zen", "Fleck\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich kr\u00e4nkt kein d\u00fcrrer Zweig.", "tokens": ["Mich", "kr\u00e4nkt", "kein", "d\u00fcr\u00b7rer", "Zweig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Will ich den Mantel dichte", "tokens": ["Will", "ich", "den", "Man\u00b7tel", "dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun legen \u00fcber's Moos,", "tokens": ["Nun", "le\u00b7gen", "\u00fc\u00b7ber's", "Moos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich lehnen an die Fichte,", "tokens": ["Mich", "leh\u00b7nen", "an", "die", "Fich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dann auf meinen Schoo\u00df", "tokens": ["Und", "dann", "auf", "mei\u00b7nen", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gezweig' und Kr\u00e4uter breiten,", "tokens": ["Ge\u00b7zweig'", "und", "Kr\u00e4u\u00b7ter", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So gut ich's finden mag:", "tokens": ["So", "gut", "ich's", "fin\u00b7den", "mag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer will mir's \u00fcbel deuten,", "tokens": ["Wer", "will", "mir's", "\u00fc\u00b7bel", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spiel' ich den Sommertag?", "tokens": ["Spiel'", "ich", "den", "Som\u00b7mer\u00b7tag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Will nicht die Grille hallen,", "tokens": ["Will", "nicht", "die", "Gril\u00b7le", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So s\u00e4uselt doch das Ried;", "tokens": ["So", "s\u00e4u\u00b7selt", "doch", "das", "Ried", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind stumm die Nachtigallen,", "tokens": ["Sind", "stumm", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sing' ich selbst ein Lied.", "tokens": ["So", "sing'", "ich", "selbst", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und hat Natur zum Feste", "tokens": ["Und", "hat", "Na\u00b7tur", "zum", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur wenig dargebracht:", "tokens": ["Nur", "we\u00b7nig", "dar\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lust ist stets die beste,", "tokens": ["Die", "Lust", "ist", "stets", "die", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die man sich selber macht.", "tokens": ["Die", "man", "sich", "sel\u00b7ber", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}