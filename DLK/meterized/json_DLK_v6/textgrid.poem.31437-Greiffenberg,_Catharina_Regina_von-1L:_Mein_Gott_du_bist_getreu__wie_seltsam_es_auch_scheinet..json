{"textgrid.poem.31437": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Gott du bist getreu / wie seltsam es auch scheinet.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott du bist getreu / wie seltsam es auch scheinet.", "tokens": ["Mein", "Gott", "du", "bist", "ge\u00b7treu", "/", "wie", "selt\u00b7sam", "es", "auch", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "ADJD", "$(", "PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann alles knackt und kracht / wann Blut und muht erliegt /", "tokens": ["Wann", "al\u00b7les", "knackt", "und", "kracht", "/", "wann", "Blut", "und", "muht", "er\u00b7liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$(", "PWAV", "NN", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wann selbst das Herz entherzt kein safft noch Krafft mehr kriegt;", "tokens": ["wann", "selbst", "das", "Herz", "en\u00b7therzt", "kein", "safft", "noch", "Krafft", "mehr", "kriegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "PIAT", "VVFIN", "ADV", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wann alles man verhaust und aus zu seyn vermeinet;", "tokens": ["wann", "al\u00b7les", "man", "ver\u00b7haust", "und", "aus", "zu", "seyn", "ver\u00b7mei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "VVFIN", "KON", "APPR", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ja wann uns auch gedunkt der Himmel ganz versteinet /", "tokens": ["ja", "wann", "uns", "auch", "ge\u00b7dunkt", "der", "Him\u00b7mel", "ganz", "ver\u00b7stei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "ADV", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df weder flehn noch bitt ihm etwas mehr ansiegt /", "tokens": ["da\u00df", "we\u00b7der", "flehn", "noch", "bitt", "ihm", "et\u00b7was", "mehr", "an\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "VVFIN", "ADV", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "hingegen alles sich uns zu betr\u00fcben f\u00fcgt /", "tokens": ["hin\u00b7ge\u00b7gen", "al\u00b7les", "sich", "uns", "zu", "be\u00b7tr\u00fc\u00b7ben", "f\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PRF", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "und sich mit ganzer macht zu unsern Creutz vereinet:", "tokens": ["und", "sich", "mit", "gan\u00b7zer", "macht", "zu", "un\u00b7sern", "Creutz", "ver\u00b7ei\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "so ist bereit die zeit der gnaden-labung hie /", "tokens": ["so", "ist", "be\u00b7reit", "die", "zeit", "der", "gna\u00b7den\u00b7la\u00b7bung", "hie", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "die kan so wunderbald das Leid in Freud verwandlen /", "tokens": ["die", "kan", "so", "wun\u00b7der\u00b7bald", "das", "Leid", "in", "Freud", "ver\u00b7wand\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "die pflegt so lieblich s\u00fc\u00df die schmerzen zu behandlen", "tokens": ["die", "pflegt", "so", "lieb\u00b7lich", "s\u00fc\u00df", "die", "schmer\u00b7zen", "zu", "be\u00b7hand\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "ADV", "ADJD", "VVFIN", "ART", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das man nicht w\u00fcnschen soll / das sie gewesen nie.", "tokens": ["das", "man", "nicht", "w\u00fcn\u00b7schen", "soll", "/", "das", "sie", "ge\u00b7we\u00b7sen", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVINF", "VMFIN", "$(", "PRELS", "PPER", "VAPP", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da siht man / da\u00df Gott / nur recht zu erfreuen / kr\u00e4nket.", "tokens": ["Da", "siht", "man", "/", "da\u00df", "Gott", "/", "nur", "recht", "zu", "er\u00b7freu\u00b7en", "/", "kr\u00e4n\u00b7ket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "KOUS", "NN", "$(", "ADV", "ADJD", "PTKZU", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "sein Liebessinn auf nichts / als unsre wolfart denket.", "tokens": ["sein", "Lie\u00b7bes\u00b7sinn", "auf", "nichts", "/", "als", "uns\u00b7re", "wol\u00b7fart", "den\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIS", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}