{"textgrid.poem.52322": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "41. De Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Criwitz was einmal en Kanter,", "tokens": ["In", "Cri\u00b7witz", "was", "ein\u00b7mal", "en", "Kan\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PWS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "De hellschen stark in't Singen wir,", "tokens": ["De", "hell\u00b7schen", "stark", "in't", "Sin\u00b7gen", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un ok tau glike Tid en Ratsverwandter,", "tokens": ["Un", "ok", "tau", "gli\u00b7ke", "Tid", "en", "Rats\u00b7ver\u00b7wand\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "De Mann was Kopmann un heit L\u00fchr.", "tokens": ["De", "Mann", "was", "Kop\u00b7mann", "un", "heit", "L\u00fchr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "De hadd 'ne Dochter, wat en smuckes M\u00e4ten", "tokens": ["De", "hadd", "'ne", "Doch\u00b7ter", ",", "wat", "en", "smu\u00b7ckes", "M\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Un ok noch jung bi Johren wir,", "tokens": ["Un", "ok", "noch", "jung", "bi", "Joh\u00b7ren", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJD", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In de hadd unser Papa L\u00fchr", "tokens": ["In", "de", "hadd", "un\u00b7ser", "Pa\u00b7pa", "L\u00fchr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So recht sick sinen Naren freten.", "tokens": ["So", "recht", "sick", "si\u00b7nen", "Na\u00b7ren", "fre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbje, \u00e4werst\u00ab, denkt hei mal un nimmt 'ne Pris',", "tokens": ["\u00bb", "je", ",", "\u00e4\u00b7werst", "\u00ab", ",", "denkt", "hei", "mal", "un", "nimmt", "'", "ne", "Pris", "'", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "$(", "$,", "VVFIN", "FM", "FM", "FM", "VVFIN", "$(", "FM", "FM", "$(", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "\u00bbdat's all recht gaud! Doch min Lowis'", "tokens": ["\u00bb", "dat's", "all", "recht", "gaud", "!", "Doch", "min", "Lo\u00b7wi\u00b7s'"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NE", "PIAT", "NN", "NE", "$.", "KON", "PPOSAT", "NN"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.11": {"text": "M\u00f6t'ck doch tauletzt in't Kloster spunn'n;", "tokens": ["M\u00f6t'ck", "doch", "tau\u00b7letzt", "in't", "Klos\u00b7ter", "spunn'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Mit gr\u00e4une Sep un Hiringstunn'n,", "tokens": ["Mit", "gr\u00e4u\u00b7ne", "Sep", "un", "Hi\u00b7ring\u00b7stunn'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dor laten sick kein Br\u00fcjams locken;", "tokens": ["Dor", "la\u00b7ten", "sick", "kein", "Br\u00fc\u00b7jams", "lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ick heww tau wenig intaubrocken.", "tokens": ["Ick", "heww", "tau", "we\u00b7nig", "in\u00b7tau\u00b7bro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dat einzigst, wat noch helpen kann,", "tokens": ["Dat", "ein\u00b7zigst", ",", "wat", "noch", "hel\u00b7pen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dat is, ick lat ehr Bildung lihr'n,", "tokens": ["Dat", "is", ",", "ick", "lat", "ehr", "Bil\u00b7dung", "lih\u00b7r'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Denn f\u00fcnn f\u00f6r mine smucke Dirn", "tokens": ["Denn", "f\u00fcnn", "f\u00f6r", "mi\u00b7ne", "smu\u00b7cke", "Dirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sick endlich ok woll noch en Mann.", "tokens": ["Sick", "end\u00b7lich", "ok", "woll", "noch", "en", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wenn sei kann Klawezimbel spelen,", "tokens": ["Wenn", "sei", "kann", "Kla\u00b7we\u00b7zim\u00b7bel", "spe\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn sei en beten singen kann,", "tokens": ["Wenn", "sei", "en", "be\u00b7ten", "sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Bitt ok bi ", "tokens": ["Bitt", "ok", "bi"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Denn kann de Br\u00fcjam gor nich fehlen.", "tokens": ["Denn", "kann", "de", "Br\u00fc\u00b7jam", "gor", "nich", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "NE", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ick will man glik nah'n Kanter gahn.\u00ab", "tokens": ["Ick", "will", "man", "glik", "nah'n", "Kan\u00b7ter", "gahn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PIS", "NN", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "De Kanter was mit inverstahn,", "tokens": ["De", "Kan\u00b7ter", "was", "mit", "in\u00b7ver\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRELS", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Un nu w\u00fcrd up de St\u00e4d anfungen,", "tokens": ["Un", "nu", "w\u00fcrd", "up", "de", "St\u00e4d", "an\u00b7fun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "NE", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Un nah de M\u00e4glichkeit w\u00fcrd sungen,", "tokens": ["Un", "nah", "de", "M\u00e4g\u00b7lich\u00b7keit", "w\u00fcrd", "sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NE", "NN", "VAFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Un de Lowis', de Dirn, de hadd 'ne Kehl!", "tokens": ["Un", "de", "Lo\u00b7wi\u00b7s'", ",", "de", "Dirn", ",", "de", "hadd", "'ne", "Kehl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "NE", "NN", "$,", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.28": {"text": "Ne, so'ne Kehlen giwwt nich vel,", "tokens": ["Ne", ",", "so'\u00b7ne", "Keh\u00b7len", "giwwt", "nich", "vel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "VVFIN", "PTKNEG", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Sei sung jug schir so fin un d\u00fcnn,", "tokens": ["Sei", "sung", "jug", "schir", "so", "fin", "un", "d\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Dat ein't tauletzt gor nich mihr h\u00fcren k\u00fcnn.", "tokens": ["Dat", "ein't", "tau\u00b7letzt", "gor", "nich", "mihr", "h\u00fc\u00b7ren", "k\u00fcnn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Denn stunn de Oll mit sinen Ladenswengel", "tokens": ["Denn", "stunn", "de", "Oll", "mit", "si\u00b7nen", "La\u00b7dens\u00b7wen\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "So \u00e4wergl\u00fccklich in de Baud,", "tokens": ["So", "\u00e4\u00b7wer\u00b7gl\u00fcck\u00b7lich", "in", "de", "Baud", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Denn was em justement tau Maud,", "tokens": ["Denn", "was", "em", "jus\u00b7te\u00b7ment", "tau", "Maud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "As wenn von'n Himmel all de Engel", "tokens": ["As", "wenn", "von'n", "Him\u00b7mel", "all", "de", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "APPR", "NN", "PIAT", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Wir'n runner kamen v\u00f6r sin D\u00f6r", "tokens": ["Wir'n", "run\u00b7ner", "ka\u00b7men", "v\u00f6r", "sin", "D\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.36": {"text": "Un s\u00fcngen em en St\u00fcckschen v\u00f6r;", "tokens": ["Un", "s\u00fcn\u00b7gen", "em", "en", "St\u00fcck\u00b7schen", "v\u00f6r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Denn rew hei sick so froh de H\u00e4n'n", "tokens": ["Denn", "rew", "hei", "sick", "so", "froh", "de", "H\u00e4n'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "ADV", "ADJD", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Un reckt sick mal so lang in En'n.", "tokens": ["Un", "reckt", "sick", "mal", "so", "lang", "in", "En'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NE", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Na, einstens g\u00fcng hei ok mal wedder", "tokens": ["Na", ",", "eins\u00b7tens", "g\u00fcng", "hei", "ok", "mal", "wed\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "In sinen Laden up un nedder,", "tokens": ["In", "si\u00b7nen", "La\u00b7den", "up", "un", "ned\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.41": {"text": "So fr\u00f6hlich as 'ne Fil