 "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "De Kanter sung just ut den besten Dur,", "tokens": ["De", "Kan\u00b7ter", "sung", "just", "ut", "den", "bes\u00b7ten", "Dur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Un Wise L\u00fchr'n, de quinkeliert mit mang.", "tokens": ["Un", "Wi\u00b7se", "L\u00fchr'n", ",", "de", "quin\u00b7ke\u00b7liert", "mit", "mang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Herr L\u00fchr, de stunn, as stunn hei up de Lur,", "tokens": ["Herr", "L\u00fchr", ",", "de", "stunn", ",", "as", "stunn", "hei", "up", "de", "Lur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "FM.fr", "FM.fr", "$,", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Un horchte up den pr\u00e4chtigen Gesang,", "tokens": ["Un", "horch\u00b7te", "up", "den", "pr\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Dunn stellt de Ollsch woll up den Disch den Pott,", "tokens": ["Dunn", "stellt", "de", "Ollsch", "woll", "up", "den", "Disch", "den", "Pott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "