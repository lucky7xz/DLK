{"textgrid.poem.64035": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Triumph, nach m\u00fchevollen Tagen,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Triumph, nach m\u00fchevollen Tagen,", "tokens": ["Im", "Tri\u00b7umph", ",", "nach", "m\u00fc\u00b7he\u00b7vol\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Kehrt Sesostris heim aus blut'ger Schlacht.", "tokens": ["Kehrt", "Se\u00b7sost\u00b7ris", "heim", "aus", "blut'\u00b7ger", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "K\u00f6nige, die seine Fesseln tragen,", "tokens": ["K\u00f6\u00b7ni\u00b7ge", ",", "die", "sei\u00b7ne", "Fes\u00b7seln", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mehren seines Zuges stolze Pracht;", "tokens": ["Meh\u00b7ren", "sei\u00b7nes", "Zu\u00b7ges", "stol\u00b7ze", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn gespannt an hohen Siegeswagen,", "tokens": ["Denn", "ge\u00b7spannt", "an", "ho\u00b7hen", "Sie\u00b7ges\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hat sie ihres \u00dcberwinders Macht.", "tokens": ["Hat", "sie", "ih\u00b7res", "\u00dc\u00b7berw\u00b7in\u00b7ders", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Unverm\u00f6gend, seinem Trotz zu fliehen,", "tokens": ["Un\u00b7ver\u00b7m\u00f6\u00b7gend", ",", "sei\u00b7nem", "Trotz", "zu", "flie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "M\u00fcssen sie ihn statt der Rosse ziehen.", "tokens": ["M\u00fcs\u00b7sen", "sie", "ihn", "statt", "der", "Ros\u00b7se", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Tiefgebeugt, und in sich selbst gekehret,", "tokens": ["Tief\u00b7ge\u00b7beugt", ",", "und", "in", "sich", "selbst", "ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wanken sie dahin im finstern Gram.", "tokens": ["Wan\u00b7ken", "sie", "da\u00b7hin", "im", "fins\u00b7tern", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, dass er ihr Leben, so entehret,", "tokens": ["Ach", ",", "dass", "er", "ihr", "Le\u00b7ben", ",", "so", "en\u00b7teh\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht zugleich mit Thron und Scepter nahm!", "tokens": ["Nicht", "zu\u00b7gleich", "mit", "Thron", "und", "Scep\u00b7ter", "nahm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Seiner Diener Hohn, den niemand wehret,", "tokens": ["Sei\u00b7ner", "Die\u00b7ner", "Hohn", ",", "den", "nie\u00b7mand", "weh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sch\u00e4rfet noch der Unterjochten Scham,", "tokens": ["Sch\u00e4r\u00b7fet", "noch", "der", "Un\u00b7ter\u00b7joch\u00b7ten", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und sie senken die ergrimmten Blicke,", "tokens": ["Und", "sie", "sen\u00b7ken", "die", "er\u00b7grimm\u00b7ten", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Fluchend ihrem feindlichen Geschicke.", "tokens": ["Flu\u00b7chend", "ih\u00b7rem", "feind\u00b7li\u00b7chen", "Ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Einer wendet nur sein Haupt zur Seite,", "tokens": ["Ei\u00b7ner", "wen\u00b7det", "nur", "sein", "Haupt", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4chelt still mit wehmuthsvoller Lust.", "tokens": ["L\u00e4\u00b7chelt", "still", "mit", "weh\u00b7muths\u00b7vol\u00b7ler", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihm verletzt das schm\u00e4hliche Geleite", "tokens": ["Ihm", "ver\u00b7letzt", "das", "schm\u00e4h\u00b7li\u00b7che", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Frecher Sp\u00f6tter nicht die Felsenbrust.", "tokens": ["Fre\u00b7cher", "Sp\u00f6t\u00b7ter", "nicht", "die", "Fel\u00b7sen\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nimmer mit sich selbst im bangen Streite,", "tokens": ["Nim\u00b7mer", "mit", "sich", "selbst", "im", "ban\u00b7gen", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bleibt er seiner W\u00fcrde sich bewusst,", "tokens": ["Bleibt", "er", "sei\u00b7ner", "W\u00fcr\u00b7de", "sich", "be\u00b7wusst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und des Rades ewig gleicher Schwung", "tokens": ["Und", "des", "Ra\u00b7des", "e\u00b7wig", "glei\u00b7cher", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "F\u00fcllt sein Auge mit Beruhigung.", "tokens": ["F\u00fcllt", "sein", "Au\u00b7ge", "mit", "Be\u00b7ru\u00b7hi\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+----", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Und man h\u00f6rt Sesostris stolz ihn fragen:", "tokens": ["Und", "man", "h\u00f6rt", "Se\u00b7sost\u00b7ris", "stolz", "ihn", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "NE", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warum bleibt Dein Muth stets gleich und gross?", "tokens": ["Wa\u00b7rum", "bleibt", "Dein", "Muth", "stets", "gleich", "und", "gross", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u2013 Schmachvoll zieh ich Deinen Siegeswagen,", "tokens": ["\u2013", "Schmach\u00b7voll", "zieh", "ich", "Dei\u00b7nen", "Sie\u00b7ges\u00b7wa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Spricht der K\u00f6nig \u2013 Schande ist mein Loos.", "tokens": ["Spricht", "der", "K\u00f6\u00b7nig", "\u2013", "Schan\u00b7de", "ist", "mein", "Loos", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Doch dies Rad hilft mir mein Elend tragen", "tokens": ["Doch", "dies", "Rad", "hilft", "mir", "mein", "E\u00b7lend", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und erh\u00e4lt mich in der Hoffnung Schoos.", "tokens": ["Und", "er\u00b7h\u00e4lt", "mich", "in", "der", "Hoff\u00b7nung", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Gleich dem Gl\u00fcck hat mich sein Gang belehret,", "tokens": ["Gleich", "dem", "Gl\u00fcck", "hat", "mich", "sein", "Gang", "be\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Dass sich ", "tokens": ["Dass", "sich"], "token_info": ["word", "word"], "pos": ["KOUS", "PRF"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Da ergriffen schreckende Gewalten", "tokens": ["Da", "er\u00b7grif\u00b7fen", "schre\u00b7cken\u00b7de", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ADJA", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Rauh den Sieger, der so trotzig war;", "tokens": ["Rauh", "den", "Sie\u00b7ger", ",", "der", "so", "trot\u00b7zig", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und er l\u00e4sst den Zug des Wagens halten", "tokens": ["Und", "er", "l\u00e4sst", "den", "Zug", "des", "Wa\u00b7gens", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und steigt ab. \u2013 Vor seiner V\u00f6lkerschaar", "tokens": ["Und", "steigt", "ab", ".", "\u2013", "Vor", "sei\u00b7ner", "V\u00f6l\u00b7ker\u00b7schaar"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "$(", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Reichet er dem tiefgekr\u00e4nkten Alten", "tokens": ["Rei\u00b7chet", "er", "dem", "tief\u00b7ge\u00b7kr\u00e4nk\u00b7ten", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seine Rechte zur Vers\u00f6hnung dar.", "tokens": ["Sei\u00b7ne", "Rech\u00b7te", "zur", "Ver\u00b7s\u00f6h\u00b7nung", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wohl verg\u00e4nglich, spricht er, ist das Gl\u00fcck \u2013", "tokens": ["Wohl", "ver\u00b7g\u00e4ng\u00b7lich", ",", "spricht", "er", ",", "ist", "das", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Darum nimm die Krone nun zur\u00fcck!", "tokens": ["Da\u00b7rum", "nimm", "die", "Kro\u00b7ne", "nun", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}