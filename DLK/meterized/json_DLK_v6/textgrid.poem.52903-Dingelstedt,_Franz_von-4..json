{"textgrid.poem.52903": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fragt mich einer, wie's der Brust ergangen,", "tokens": ["Es", "fragt", "mich", "ei\u00b7ner", ",", "wie's", "der", "Brust", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die jenes Wort im K\u00f6nigsgarten rief,", "tokens": ["Die", "je\u00b7nes", "Wort", "im", "K\u00f6\u00b7nigs\u00b7gar\u00b7ten", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ob sie nicht schon hinter Schlo\u00df und Stangen", "tokens": ["Und", "ob", "sie", "nicht", "schon", "hin\u00b7ter", "Schlo\u00df", "und", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Fr\u00fchlingsr\u00e4uschlein abgek\u00fchlt verschlief?", "tokens": ["Ihr", "Fr\u00fch\u00b7lings\u00b7r\u00e4usc\u00b7hlein", "ab\u00b7ge\u00b7k\u00fchlt", "ver\u00b7schlief", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach nein, mein Freund! Das Abenteuer lief", "tokens": ["Ach", "nein", ",", "mein", "Freund", "!", "Das", "A\u00b7bent\u00b7eu\u00b7er", "lief"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKANT", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Friedfertig ab und ohne gro\u00df Belangen,", "tokens": ["Fried\u00b7fer\u00b7tig", "ab", "und", "oh\u00b7ne", "gro\u00df", "Be\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der S\u00e4nger schwieg, nachdem er, hoch und tief,", "tokens": ["Der", "S\u00e4n\u00b7ger", "schwieg", ",", "nach\u00b7dem", "er", ",", "hoch", "und", "tief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zehnmal sein Lied vergeblich angefangen.", "tokens": ["Zehn\u00b7mal", "sein", "Lied", "ver\u00b7geb\u00b7lich", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dicht unter ihm und seiner schweren Klage", "tokens": ["Dicht", "un\u00b7ter", "ihm", "und", "sei\u00b7ner", "schwe\u00b7ren", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ging achtlos das geputzte Volk vorbei,", "tokens": ["Ging", "acht\u00b7los", "das", "ge\u00b7putz\u00b7te", "Volk", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Orient schwatzend und der Zuckerfrage.", "tokens": ["Vom", "O\u00b7rient", "schwat\u00b7zend", "und", "der", "Zu\u00b7cker\u00b7fra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihn \u00fcberschrie die Liebesdudelei", "tokens": ["Ihn", "\u00fc\u00b7bersc\u00b7hrie", "die", "Lie\u00b7bes\u00b7du\u00b7de\u00b7lei"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus einem nahen Spatzen-Lustgelage", "tokens": ["Aus", "ei\u00b7nem", "na\u00b7hen", "Spat\u00b7zen\u00b7Lust\u00b7ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und zahmer Papagein Lobhudelei ...", "tokens": ["Und", "zah\u00b7mer", "Pa\u00b7pa\u00b7gein", "Lob\u00b7hu\u00b7de\u00b7lei", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}