{"textgrid.poem.66303": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An einem Communiontage", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcnden, eure H\u00f6llenschmerzen", "tokens": ["S\u00fcn\u00b7den", ",", "eu\u00b7re", "H\u00f6l\u00b7len\u00b7schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treiben mich zum Gnadenstuhl;", "tokens": ["Trei\u00b7ben", "mich", "zum", "Gna\u00b7den\u00b7stuhl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihr brennt in meinem Herzen", "tokens": ["Denn", "ihr", "brennt", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie in einem Schwefelpfuhl!", "tokens": ["Wie", "in", "ei\u00b7nem", "Schwe\u00b7fel\u00b7pfuhl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es schlagen Feuerflammen", "tokens": ["Und", "es", "schla\u00b7gen", "Feu\u00b7er\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber meinem Haupt zusammen.", "tokens": ["Ue\u00b7ber", "mei\u00b7nem", "Haupt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, wo bist du, Freund der Seele,", "tokens": ["Ach", ",", "wo", "bist", "du", ",", "Freund", "der", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arzt der Kranken, wo bist du?", "tokens": ["Arzt", "der", "Kran\u00b7ken", ",", "wo", "bist", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tropfen nur von deinem Oele", "tokens": ["Trop\u00b7fen", "nur", "von", "dei\u00b7nem", "O\u00b7e\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Heilen gro\u00dfe Wunden zu.", "tokens": ["Hei\u00b7len", "gro\u00b7\u00dfe", "Wun\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, so tr\u00e4ufle in das Herze", "tokens": ["Ach", ",", "so", "tr\u00e4uf\u00b7le", "in", "das", "Her\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lindrung nach dem H\u00f6llenschmerze.", "tokens": ["Lind\u00b7rung", "nach", "dem", "H\u00f6l\u00b7len\u00b7schmer\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Krank, m\u00fchselig und beladen", "tokens": ["Krank", ",", "m\u00fch\u00b7se\u00b7lig", "und", "be\u00b7la\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KON", "ADJA"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Lieg' ich auf den Knieen hier,", "tokens": ["Lieg'", "ich", "auf", "den", "Kni\u00b7e\u00b7en", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Rede doch dein Wort der Gnaden:", "tokens": ["Re\u00b7de", "doch", "dein", "Wort", "der", "Gna\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt, ihr S\u00fcnder, her zu mir,", "tokens": ["Kommt", ",", "ihr", "S\u00fcn\u00b7der", ",", "her", "zu", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr die Last auf eurem R\u00fccken", "tokens": ["F\u00fcr", "die", "Last", "auf", "eu\u00b7rem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch mit Labsal zu erquicken.", "tokens": ["Euch", "mit", "Lab\u00b7sal", "zu", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch, o Freude! schon erschallet", "tokens": ["Doch", ",", "o", "Freu\u00b7de", "!", "schon", "er\u00b7schal\u00b7let"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Trostesstimme dort,", "tokens": ["Die\u00b7se", "Tros\u00b7tes\u00b7stim\u00b7me", "dort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Tempel wiederhallet", "tokens": ["Und", "der", "Tem\u00b7pel", "wie\u00b7der\u00b7hal\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenes gnadenvolle Wort:", "tokens": ["Je\u00b7nes", "gna\u00b7den\u00b7vol\u00b7le", "Wort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesus tr\u00f6stet! rufet! winket!", "tokens": ["Je\u00b7sus", "tr\u00f6s\u00b7tet", "!", "ru\u00b7fet", "!", "win\u00b7ket", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt ihr M\u00fcden, ess't und trinket;", "tokens": ["Kommt", "ihr", "M\u00fc\u00b7den", ",", "ess't", "und", "trin\u00b7ket", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gehst du? O ihr F\u00fc\u00dfe sinket", "tokens": ["Gehst", "du", "?", "O", "ihr", "F\u00fc\u00b7\u00dfe", "sin\u00b7ket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "NE", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem Donner, der euch droht:", "tokens": ["Vor", "dem", "Don\u00b7ner", ",", "der", "euch", "droht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer unw\u00fcrdig i\u00dft und trinket,", "tokens": ["Wer", "un\u00b7w\u00fcr\u00b7dig", "i\u00dft", "und", "trin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "I\u00dft Gericht und trinket Tod.", "tokens": ["I\u00dft", "Ge\u00b7richt", "und", "trin\u00b7ket", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprich, verklagendes Gewissen,", "tokens": ["Sprich", ",", "ver\u00b7kla\u00b7gen\u00b7des", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ich f\u00e4hig zum Genie\u00dfen?", "tokens": ["Bin", "ich", "f\u00e4\u00b7hig", "zum", "Ge\u00b7nie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nein! so donnert mein Gewissen,", "tokens": ["Nein", "!", "so", "don\u00b7nert", "mein", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fcnder, nein, das bist du nicht!", "tokens": ["S\u00fcn\u00b7der", ",", "nein", ",", "das", "bist", "du", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du eilst mit schnellen F\u00fc\u00dfen", "tokens": ["Und", "du", "eilst", "mit", "schnel\u00b7len", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Verderben, zum Gericht.", "tokens": ["Zum", "Ver\u00b7der\u00b7ben", ",", "zum", "Ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau! von diesem Kelche flammen", "tokens": ["Schau", "!", "von", "die\u00b7sem", "Kel\u00b7che", "flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blitze, die von fern verdammen.", "tokens": ["Blit\u00b7ze", ",", "die", "von", "fern", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch ich eile, weil mein Herze", "tokens": ["Doch", "ich", "ei\u00b7le", ",", "weil", "mein", "Her\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Gedanken stammeln kann:", "tokens": ["Den", "Ge\u00b7dan\u00b7ken", "stam\u00b7meln", "kann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "(diesen Himmel nach dem Schmerze!)", "tokens": ["(", "die\u00b7sen", "Him\u00b7mel", "nach", "dem", "Schmer\u00b7ze", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDAT", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jesus nimmt die S\u00fcnder an.", "tokens": ["Je\u00b7sus", "nimmt", "die", "S\u00fcn\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Bu\u00dfe Thr\u00e4nen rollen,", "tokens": ["Und", "der", "Bu\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die Donner t\u00f6dten wollen.", "tokens": ["Wenn", "die", "Don\u00b7ner", "t\u00f6d\u00b7ten", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mit Marien Magdalenen", "tokens": ["Mit", "Ma\u00b7ri\u00b7en", "Mag\u00b7da\u00b7le\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fall' ich auf mein Angesicht,", "tokens": ["Fall'", "ich", "auf", "mein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es sprechen tausend Thr\u00e4nen:", "tokens": ["Und", "es", "spre\u00b7chen", "tau\u00b7send", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Richter, ach, versto\u00df mich nicht!", "tokens": ["Rich\u00b7ter", ",", "ach", ",", "ver\u00b7sto\u00df", "mich", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh auf Z\u00e4hren, Bu\u00df' und Reue,", "tokens": ["Sieh", "auf", "Z\u00e4h\u00b7ren", ",", "Bu\u00df'", "und", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei ein Vater und verzeihe.", "tokens": ["Sei", "ein", "Va\u00b7ter", "und", "ver\u00b7zei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun ich wandle zum Altare", "tokens": ["Nun", "ich", "wand\u00b7le", "zum", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein armer S\u00fcnder hin,", "tokens": ["Als", "ein", "ar\u00b7mer", "S\u00fcn\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Esse, trinke und erfahre,", "tokens": ["Es\u00b7se", ",", "trin\u00b7ke", "und", "er\u00b7fah\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich ausges\u00f6hnet bin.", "tokens": ["Da\u00df", "ich", "aus\u00b7ge\u00b7s\u00f6h\u00b7net", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ganze Freudenstr\u00f6me flie\u00dfen", "tokens": ["Gan\u00b7ze", "Freu\u00b7den\u00b7str\u00f6\u00b7me", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf mein durstendes Gewissen.", "tokens": ["Auf", "mein", "durs\u00b7ten\u00b7des", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Fromme Seele, zittre weiter", "tokens": ["From\u00b7me", "See\u00b7le", ",", "zitt\u00b7re", "wei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht vor Krankheit, H\u00f6lle, Tod!", "tokens": ["Nicht", "vor", "Krank\u00b7heit", ",", "H\u00f6l\u00b7le", ",", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn nun sind sie, deine Kleider,", "tokens": ["Denn", "nun", "sind", "sie", ",", "dei\u00b7ne", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Blut