{"textgrid.poem.52169": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebes Kind, du bist gemagert, bist verbrannt von Mittagssonnen,", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebes Kind, du bist gemagert, bist verbrannt von Mittagssonnen,", "tokens": ["Lie\u00b7bes", "Kind", ",", "du", "bist", "ge\u00b7ma\u00b7gert", ",", "bist", "ver\u00b7brannt", "von", "Mit\u00b7tags\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Deine Wangen bl\u00fchen frischer, wuschest dich an k\u00fchlen Bronnen,", "tokens": ["Dei\u00b7ne", "Wan\u00b7gen", "bl\u00fc\u00b7hen", "fri\u00b7scher", ",", "wu\u00b7schest", "dich", "an", "k\u00fch\u00b7len", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie du schreitest, schlank und kr\u00e4ftig, \u00fcber deines G\u00e4rtchens Stufen!", "tokens": ["Wie", "du", "schrei\u00b7test", ",", "schlank", "und", "kr\u00e4f\u00b7tig", ",", "\u00fc\u00b7ber", "dei\u00b7nes", "G\u00e4rt\u00b7chens", "Stu\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Deine Stimme wurde voller, die das Echo wachgerufen,", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", "wur\u00b7de", "vol\u00b7ler", ",", "die", "das", "E\u00b7cho", "wach\u00b7ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "In dem klaren Herdgel\u00e4ute wurde deine Stimme heller,", "tokens": ["In", "dem", "kla\u00b7ren", "Herd\u00b7ge\u00b7l\u00e4u\u00b7te", "wur\u00b7de", "dei\u00b7ne", "Stim\u00b7me", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Deine wegeskund'gen Blicke kreisen rascher, streifen schneller,", "tokens": ["Dei\u00b7ne", "weges\u00b7kun\u00b7d'\u00b7gen", "Bli\u00b7cke", "krei\u00b7sen", "ra\u00b7scher", ",", "strei\u00b7fen", "schnel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Deine Lippen wurden stiller, edler wurde deine Stirne,", "tokens": ["Dei\u00b7ne", "Lip\u00b7pen", "wur\u00b7den", "stil\u00b7ler", ",", "ed\u00b7ler", "wur\u00b7de", "dei\u00b7ne", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJA", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und dein Auge, gro\u00dfge\u00f6ffnet, es betrachtet noch die Firne.", "tokens": ["Und", "dein", "Au\u00b7ge", ",", "gro\u00df\u00b7ge\u00f6ff\u00b7net", ",", "es", "be\u00b7trach\u00b7tet", "noch", "die", "Fir\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}