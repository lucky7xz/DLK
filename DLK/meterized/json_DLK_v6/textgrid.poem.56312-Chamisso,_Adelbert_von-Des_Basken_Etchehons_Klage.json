{"textgrid.poem.56312": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Des Basken Etchehons Klage", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gensdarmen, ausgesendet", "tokens": ["Gens\u00b7dar\u00b7men", ",", "aus\u00b7ge\u00b7sen\u00b7det"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu fahen den Etchehon,", "tokens": ["Zu", "fa\u00b7hen", "den", "Et\u00b7che\u00b7hon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr sucht ihn vergeblich zu Barcus,", "tokens": ["Ihr", "sucht", "ihn", "ver\u00b7geb\u00b7lich", "zu", "Bar\u00b7cus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er ist zu den Bergen entflohn.", "tokens": ["Er", "ist", "zu", "den", "Ber\u00b7gen", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die Pyren\u00e4en verbergen", "tokens": ["Die", "Py\u00b7re\u00b7n\u00e4\u00b7en", "ver\u00b7ber\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihn gastlich in ihrem Scho\u00df,", "tokens": ["Ihn", "gast\u00b7lich", "in", "ih\u00b7rem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da teilt er, in bitterem Elend,", "tokens": ["Da", "teilt", "er", ",", "in", "bit\u00b7te\u00b7rem", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Des fl\u00fcchtigen Wildes Los.", "tokens": ["Des", "fl\u00fcch\u00b7ti\u00b7gen", "Wil\u00b7des", "Los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es staunen La Soules Hirten", "tokens": ["Es", "stau\u00b7nen", "La", "Sou\u00b7les", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu Eguiton ihn an,", "tokens": ["Zu", "E\u00b7gui\u00b7ton", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und reichen das Brot des Mitleids", "tokens": ["Und", "rei\u00b7chen", "das", "Brot", "des", "Mit\u00b7leids"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem blutigen S\u00e4ngersmann.", "tokens": ["Dem", "blu\u00b7ti\u00b7gen", "S\u00e4n\u00b7gers\u00b7mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr staunt, mitleidige Hirten,", "tokens": ["Ihr", "staunt", ",", "mit\u00b7lei\u00b7di\u00b7ge", "Hir\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie blutig die Hand mir sei? \u2013", "tokens": ["Wie", "blu\u00b7tig", "die", "Hand", "mir", "sei", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zehn Jahre hab ich geschmachtet", "tokens": ["Zehn", "Jah\u00b7re", "hab", "ich", "ge\u00b7schmach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Ketten und Sklaverei.", "tokens": ["In", "Ket\u00b7ten", "und", "Skla\u00b7ve\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich hab ein Weib mir gefreiet", "tokens": ["Ich", "hab", "ein", "Weib", "mir", "ge\u00b7frei\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In meiner Jugend Kraft,", "tokens": ["In", "mei\u00b7ner", "Ju\u00b7gend", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat mich umstricket in Liebe,", "tokens": ["Sie", "hat", "mich", "um\u00b7stri\u00b7cket", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mir Gift in das Haus nur geschafft.", "tokens": ["Mir", "Gift", "in", "das", "Haus", "nur", "ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "F\u00fcnf Jahre lag ich in Ketten,", "tokens": ["F\u00fcnf", "Jah\u00b7re", "lag", "ich", "in", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War kaum noch meiner bewu\u00dft;", "tokens": ["War", "kaum", "noch", "mei\u00b7ner", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In Eifersucht zehn Jahre.", "tokens": ["In", "Ei\u00b7fer\u00b7sucht", "zehn", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die rei\u00dft erst scharf in die Brust.", "tokens": ["Die", "rei\u00dft", "erst", "scharf", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Ich trug wohl, Eguiapal,", "tokens": ["Ich", "trug", "wohl", ",", "E\u00b7gui\u00b7a\u00b7pal", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um dich der Ketten Last; \u2013", "tokens": ["Um", "dich", "der", "Ket\u00b7ten", "Last", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was trieb dich, mein Weib zu verf\u00fchren,", "tokens": ["Was", "trieb", "dich", ",", "mein", "Weib", "zu", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der selbst du ein Weib doch hast?", "tokens": ["Der", "selbst", "du", "ein", "Weib", "doch", "hast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Du wu\u00dftest R\u00e4nke zu schmieden,", "tokens": ["Du", "wu\u00df\u00b7test", "R\u00e4n\u00b7ke", "zu", "schmie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du spanntest um mich den Verdacht;", "tokens": ["Du", "spann\u00b7test", "um", "mich", "den", "Ver\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Derweil in S\u00fcnde du schwelgtest,", "tokens": ["Der\u00b7weil", "in", "S\u00fcn\u00b7de", "du", "schwelg\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verkam ich in Kerkersnacht.", "tokens": ["Ver\u00b7kam", "ich", "in", "Ker\u00b7ker\u00b7snacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Ich lag in Ketten, im Kerker,", "tokens": ["Ich", "lag", "in", "Ket\u00b7ten", ",", "im", "Ker\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf Stroh, in Elend und Not,", "tokens": ["Auf", "Stroh", ",", "in", "E\u00b7lend", "und", "Not", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Erweichte mit meinen Tr\u00e4nen", "tokens": ["Er\u00b7weich\u00b7te", "mit", "mei\u00b7nen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein hartes, mein trockenes Brot.", "tokens": ["Mein", "har\u00b7tes", ",", "mein", "tro\u00b7cke\u00b7nes", "Brot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Du \u00fcberm\u00fct'ger Geselle,", "tokens": ["Du", "\u00fc\u00b7ber\u00b7m\u00fct'\u00b7ger", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Warst Herr in dem Hause mein,", "tokens": ["Warst", "Herr", "in", "dem", "Hau\u00b7se", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und schliefest auf meinen Pf\u00fchlen,", "tokens": ["Und", "schlie\u00b7fest", "auf", "mei\u00b7nen", "Pf\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und trankest von meinem Wein.", "tokens": ["Und", "tran\u00b7kest", "von", "mei\u00b7nem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Und als den Tag der Freiheit", "tokens": ["Und", "als", "den", "Tag", "der", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich endlich, endlich geschaut,", "tokens": ["Ich", "end\u00b7lich", ",", "end\u00b7lich", "ge\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da d\u00fcnkte reif uns die Rache,", "tokens": ["Da", "d\u00fcnk\u00b7te", "reif", "uns", "die", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da hat es vor mir dir gegraut.", "tokens": ["Da", "hat", "es", "vor", "mir", "dir", "ge\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ja! zittre, t\u00fcckischer Bube!", "tokens": ["Ja", "!", "zitt\u00b7re", ",", "t\u00fc\u00b7cki\u00b7scher", "Bu\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich lade verh\u00e4ngnisvoll", "tokens": ["Ich", "la\u00b7de", "ver\u00b7h\u00e4ng\u00b7nis\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ins Feuerrohr die Kugel,", "tokens": ["Ins", "Feu\u00b7er\u00b7rohr", "die", "Ku\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nieder dich strecken soll.", "tokens": ["Die", "nie\u00b7der", "dich", "stre\u00b7cken", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "So harrt ich zu Nacht bei der Br\u00fccke", "tokens": ["So", "harrt", "ich", "zu", "Nacht", "bei", "der", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von Barcus auf dich, mein Ziel; \u2013", "tokens": ["Von", "Bar\u00b7cus", "auf", "dich", ",", "mein", "Ziel", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "APPR", "PPER", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es trieben die Geister der H\u00f6lle", "tokens": ["Es", "trie\u00b7ben", "die", "Geis\u00b7ter", "der", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit mir ihr grausiges Spiel.", "tokens": ["Mit", "mir", "ihr", "grau\u00b7si\u00b7ges", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Ich sah dich, du kamst gegangen,", "tokens": ["Ich", "sah", "dich", ",", "du", "kamst", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich zielte sicher und gut,", "tokens": ["Ich", "ziel\u00b7te", "si\u00b7cher", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Druck \u2013 und \u2013 Etchegoyen", "tokens": ["Ein", "Druck", "\u2013", "und", "\u2013", "Et\u00b7che\u00b7go\u00b7yen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$(", "KON", "$(", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Lag r\u00f6chelnd in seinem Blut.", "tokens": ["Lag", "r\u00f6\u00b7chelnd", "in", "sei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Mein Etchegoyen, der liebend", "tokens": ["Mein", "Et\u00b7che\u00b7go\u00b7yen", ",", "der", "lie\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich stets zu erfreuen gestrebt! \u2013", "token