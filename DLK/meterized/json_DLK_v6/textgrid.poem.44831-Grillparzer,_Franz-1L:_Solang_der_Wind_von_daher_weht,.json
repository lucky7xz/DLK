{"textgrid.poem.44831": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Solang der Wind von daher weht,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solang der Wind von daher weht,", "tokens": ["So\u00b7lang", "der", "Wind", "von", "da\u00b7her", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh immerhin spazieren,", "tokens": ["Geh", "im\u00b7mer\u00b7hin", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur freilich, hat er sich gedreht,", "tokens": ["Nur", "frei\u00b7lich", ",", "hat", "er", "sich", "ge\u00b7dreht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VAFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann hei\u00dft es wohl marschieren.", "tokens": ["Dann", "hei\u00dft", "es", "wohl", "mar\u00b7schie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und fragst du aber um die Zeit,", "tokens": ["Und", "fragst", "du", "a\u00b7ber", "um", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du dich machest wegbereit,", "tokens": ["Da\u00df", "du", "dich", "ma\u00b7chest", "weg\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schau nur auf Wolfgang Menzel!", "tokens": ["Schau", "nur", "auf", "Wolf\u00b7gang", "Men\u00b7zel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NE", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Geht er, schn\u00fcr auch den R\u00e4nzel.", "tokens": ["Geht", "er", ",", "schn\u00fcr", "auch", "den", "R\u00e4n\u00b7zel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Du forschest weiter um den Tag,", "tokens": ["Du", "for\u00b7schest", "wei\u00b7ter", "um", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da jener andre gehen mag?", "tokens": ["Da", "je\u00b7ner", "and\u00b7re", "ge\u00b7hen", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn nicht der Deutsche seinen Durst", "tokens": ["Wenn", "nicht", "der", "Deut\u00b7sche", "sei\u00b7nen", "Durst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr stillen will mit Pfeffer und Wurst.", "tokens": ["Mehr", "stil\u00b7len", "will", "mit", "Pfef\u00b7fer", "und", "Wurst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}