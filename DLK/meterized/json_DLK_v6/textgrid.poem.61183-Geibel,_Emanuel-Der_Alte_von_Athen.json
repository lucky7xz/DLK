"meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Kreuz auf Sankt Sophiens Dom!", "tokens": ["Das", "Kreuz", "auf", "Sankt", "So\u00b7phi\u00b7ens", "Dom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "H\u00f6rt ihr's in hoher Luft wie ziehnde Schw\u00e4ne singen?", "tokens": ["H\u00f6rt", "ih\u00b7r's", "in", "ho\u00b7her", "Luft", "wie", "ziehn\u00b7de", "Schw\u00e4\u00b7ne", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Der Engel Scharen sind's, die Flammenschwerter schwingen,", "tokens": ["Der", "En\u00b7gel", "Scha\u00b7ren", "sin\u00b7d's", ",", "die", "Flam\u00b7men\u00b7schwer\u00b7ter", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vor ihnen wird der Feind zum Spott;", "tokens": ["Vor", "ih\u00b7nen", "wird", "der", "Feind", "zum", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wem sie zu H\u00e4upten ziehn, mag Not und Tod verachten,", "tokens": ["Wem", "sie", "zu", "H\u00e4up\u00b7ten", "ziehn", ",", "mag", "Not", "und", "Tod", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVINF", "$,", "VMFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Darum frisch auf, mein Volk! Es rufen dich die Schlachten,", "tokens": ["Da\u00b7rum", "frisch", "auf", ",", "mein", "Volk", "!", "Es", "ru\u00b7fen", "dich", "die", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vorw\u00e4rts! Vorw\u00e4rts! Mit uns ist Gott.\u00ab", "tokens": ["Vor\u00b7w\u00e4rts", "!", "Vor\u00b7w\u00e4rts", "!", "Mit", "uns", "ist", "Gott", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "APPR", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "So sprach der hohe Greis und schwand im Volksgedr\u00e4nge,", "tokens": ["So", "sprach", "der", "ho\u00b7he", "Greis", "und", "schwand", "im", "Volks\u00b7ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hoch schlug das Feuer auf - ersch\u00fcttert stand die Menge,", "tokens": ["Hoch", "schlug", "das", "Feu\u00b7er", "auf", "er\u00b7sch\u00fct\u00b7tert", "stand", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "$(", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Sie bebten; jeder Mund sprach murmelnd ein Gebet.", "tokens": ["Sie", "beb\u00b7ten", ";", "je\u00b7der", "Mund", "sprach", "mur\u00b7melnd", "ein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PIAT", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wohl forscht' ich, aber wo der Alte hergekommen,", "tokens": ["Wohl", "forscht'", "ich", ",", "a\u00b7ber", "wo", "der", "Al\u00b7te", "her\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob er ein Schw\u00e4rmer war, ich hab' es nicht vernommen;", "tokens": ["Ob", "er", "ein", "Schw\u00e4r\u00b7mer", "war", ",", "ich", "hab'", "es", "nicht", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch, traun, mir d\u00fcnkt' er ein Prophet.", "tokens": ["Doch", ",", "traun", ",", "mir", "d\u00fcnkt'", "er", "ein", "Pro\u00b7phet", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVINF", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}