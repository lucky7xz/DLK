{"textgrid.poem.61183": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Alte von Athen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wehte k\u00fchl vom Meer, der Tag war l\u00e4ngst gesunken,", "tokens": ["Es", "weh\u00b7te", "k\u00fchl", "vom", "Meer", ",", "der", "Tag", "war", "l\u00e4ngst", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Feuer am Ili\u00df verspr\u00fchte rote Funken,", "tokens": ["Das", "Feu\u00b7er", "am", "I\u00b7li\u00df", "ver\u00b7spr\u00fch\u00b7te", "ro\u00b7te", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Im Kreise lag die Schar, das Banner aufgepflanzt,", "tokens": ["Im", "Krei\u00b7se", "lag", "die", "Schar", ",", "das", "Ban\u00b7ner", "auf\u00b7ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Pfeifen glommen hell, der Becher ging im Kreise,", "tokens": ["Die", "Pfei\u00b7fen", "glom\u00b7men", "hell", ",", "der", "Be\u00b7cher", "ging", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und zu der Trommel Schlag und der Hoboen Weise", "tokens": ["Und", "zu", "der", "Trom\u00b7mel", "Schlag", "und", "der", "Ho\u00b7boen", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ward die Romaika getanzt.", "tokens": ["Ward", "die", "Ro\u00b7mai\u00b7ka", "ge\u00b7tanzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie klirrten da im Takt die S\u00e4bel der Gesellen!", "tokens": ["Wie", "klirr\u00b7ten", "da", "im", "Takt", "die", "S\u00e4\u00b7bel", "der", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie flatterten im Wind die wei\u00dfen Fustanellen!", "tokens": ["Wie", "flat\u00b7ter\u00b7ten", "im", "Wind", "die", "wei\u00b7\u00dfen", "Fus\u00b7ta\u00b7nel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Flamme Strahl beschien manch b\u00e4rtig Angesicht,", "tokens": ["Der", "Flam\u00b7me", "Strahl", "be\u00b7schien", "manch", "b\u00e4r\u00b7tig", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gefurcht und sonnverbrannt, und pl\u00f6tzlich dann dazwischen", "tokens": ["Ge\u00b7furcht", "und", "sonn\u00b7ver\u00b7brannt", ",", "und", "pl\u00f6tz\u00b7lich", "dann", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "KON", "ADJD", "ADV", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein lockig Knabenhaupt; so schaut aus dunkeln B\u00fcschen", "tokens": ["Ein", "lo\u00b7ckig", "Kna\u00b7ben\u00b7haupt", ";", "so", "schaut", "aus", "dun\u00b7keln", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im Lenz der Rose junges Licht.", "tokens": ["Im", "Lenz", "der", "Ro\u00b7se", "jun\u00b7ges", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da trat ein alter Mann ins tosende Gedr\u00e4nge,", "tokens": ["Da", "trat", "ein", "al\u00b7ter", "Mann", "ins", "to\u00b7sen\u00b7de", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohl ragt' er aus der Schar um eines Hauptes L\u00e4nge,", "tokens": ["Wohl", "ragt'", "er", "aus", "der", "Schar", "um", "ei\u00b7nes", "Haup\u00b7tes", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hinab zum G\u00fcrtel flo\u00df der Bart ihm silberwei\u00df,", "tokens": ["Hin\u00b7ab", "zum", "G\u00fcr\u00b7tel", "flo\u00df", "der", "Bart", "ihm", "sil\u00b7ber\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "K\u00fchn war die Stirn, darum die Locken flatternd wehten,", "tokens": ["K\u00fchn", "war", "die", "Stirn", ",", "da\u00b7rum", "die", "Lo\u00b7cken", "flat\u00b7ternd", "weh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In seinem Auge glomm das Feuer des Propheten,", "tokens": ["In", "sei\u00b7nem", "Au\u00b7ge", "glomm", "das", "Feu\u00b7er", "des", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und also rief der hohe Greis:", "tokens": ["Und", "al\u00b7so", "rief", "der", "ho\u00b7he", "Greis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbhinweg, Verblendete, mit Trinkgelag und Reigen!", "tokens": ["\u00bb", "hin\u00b7weg", ",", "Ver\u00b7blen\u00b7de\u00b7te", ",", "mit", "Trink\u00b7ge\u00b7lag", "und", "Rei\u00b7gen", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Setzt ab den Weinpokal, la\u00dft die Hoboen schweigen,", "tokens": ["Setzt", "ab", "den", "Wein\u00b7po\u00b7kal", ",", "la\u00dft", "die", "Ho\u00b7boen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den lust'gen Schall der Trommel d\u00e4mpft!", "tokens": ["Den", "lust'\u00b7gen", "Schall", "der", "Trom\u00b7mel", "d\u00e4mpft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verge\u00dft ihr, da\u00df, indes ihr schwelgt in m\u00fc\u00df'ger Feier,", "tokens": ["Ver\u00b7ge\u00dft", "ihr", ",", "da\u00df", ",", "in\u00b7des", "ihr", "schwelgt", "in", "m\u00fc\u00df'\u00b7ger", "Fei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf Kretas blut'gem Strand der Adler mit dem Geier", "tokens": ["Auf", "Kre\u00b7tas", "blut'\u00b7gem", "Strand", "der", "Ad\u00b7ler", "mit", "dem", "Gei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Um eurer Br\u00fcder Leichen k\u00e4mpft?", "tokens": ["Um", "eu\u00b7rer", "Br\u00fc\u00b7der", "Lei\u00b7chen", "k\u00e4mpft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O w\u00e4r' ich noch ein Knab', ich k\u00f6nnte Tr\u00e4nen weinen!", "tokens": ["O", "w\u00e4r'", "ich", "noch", "ein", "Knab'", ",", "ich", "k\u00f6nn\u00b7te", "Tr\u00e4\u00b7nen", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Mut! Wie unheilvoll f\u00fcr uns die Sterne scheinen,", "tokens": ["Doch", "Mut", "!", "Wie", "un\u00b7heil\u00b7voll", "f\u00fcr", "uns", "die", "Ster\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PWAV", "ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch ward die Hoffnung nicht zum Trug;", "tokens": ["Noch", "ward", "die", "Hoff\u00b7nung", "nicht", "zum", "Trug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Leonidas erlag einst an den Thermopylen,", "tokens": ["Leo\u00b7ni\u00b7das", "er\u00b7lag", "einst", "an", "den", "Ther\u00b7mo\u00b7py\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "In Flammen stand Athen, und seine Tempel fielen,", "tokens": ["In", "Flam\u00b7men", "stand", "A\u00b7then", ",", "und", "sei\u00b7ne", "Tem\u00b7pel", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Eh' Salamis die Perser schlug.", "tokens": ["Eh'", "Sa\u00b7la\u00b7mis", "die", "Per\u00b7ser", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum auf! Nicht l\u00e4nger h\u00f6rt, was euch die Fremden raten;", "tokens": ["Drum", "auf", "!", "Nicht", "l\u00e4n\u00b7ger", "h\u00f6rt", ",", "was", "euch", "die", "Frem\u00b7den", "ra\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$.", "PTKNEG", "ADJD", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Schwerte nur ist Heil, und mit des Schwertes Taten", "tokens": ["Im", "Schwer\u00b7te", "nur", "ist", "Heil", ",", "und", "mit", "des", "Schwer\u00b7tes", "Ta\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "R\u00e4cht Kretas Schmach und Griechenlands;", "tokens": ["R\u00e4cht", "Kre\u00b7tas", "Schmach", "und", "Grie\u00b7chen\u00b7lands", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeit ist reif, den Grund, drin unsre Heil'gen modern,", "tokens": ["Die", "Zeit", "ist", "reif", ",", "den", "Grund", ",", "drin", "uns\u00b7re", "Heil'\u00b7gen", "mo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den frechgeraubten Grund im Kampf zur\u00fcckzufodern;", "tokens": ["Den", "frech\u00b7ge\u00b7raub\u00b7ten", "Grund", "im", "Kampf", "zu\u00b7r\u00fcck\u00b7zu\u00b7fo\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gen Norden geht es nach Byzanz!", "tokens": ["Gen", "Nor\u00b7den", "geht", "es", "nach", "By\u00b7zanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So steigt denn vom Gebirg', ihr braunen Klephten, nieder,", "tokens": ["So", "steigt", "denn", "vom", "Ge\u00b7bir\u00b7g'", ",", "ihr", "brau\u00b7nen", "Kleph\u00b7ten", ",", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ergreift das lange Rohr, den krummen S\u00e4bel wieder,", "tokens": ["Er\u00b7greift", "das", "lan\u00b7ge", "Rohr", ",", "den", "krum\u00b7men", "S\u00e4\u00b7bel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erwacht, ihr M\u00e4nner von Athen!", "tokens": ["Er\u00b7wacht", ",", "ihr", "M\u00e4n\u00b7ner", "von", "A\u00b7then", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr Adler Sulis, auf und zeigt den Weg den andern,", "tokens": ["Ihr", "Ad\u00b7ler", "Su\u00b7lis", ",", "auf", "und", "zeigt", "den", "Weg", "den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,", "APPR", "KON", "VVFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kanaris, f\u00fclle du den Hellespont mit Brandern,", "tokens": ["Ka\u00b7na\u00b7ris", ",", "f\u00fcl\u00b7le", "du", "den", "Hel\u00b7les\u00b7pont", "mit", "Bran\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00df, Hydra, deine Wimpel wehn!", "tokens": ["La\u00df", ",", "Hyd\u00b7ra", ",", "dei\u00b7ne", "Wim\u00b7pel", "wehn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und du, o junger F\u00fcrst von blondem Heldenstamme,", "tokens": ["Und", "du", ",", "o", "jun\u00b7ger", "F\u00fcrst", "von", "blon\u00b7dem", "Hel\u00b7den\u00b7stam\u00b7me", ","], "token_info": ["word", "word", "