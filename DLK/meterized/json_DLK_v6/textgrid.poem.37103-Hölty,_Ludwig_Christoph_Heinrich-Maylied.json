{"textgrid.poem.37103": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Maylied", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil dir l\u00e4chelnder May,", "tokens": ["Heil", "dir", "l\u00e4\u00b7cheln\u00b7der", "May", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Blumensch\u00f6pfer,", "tokens": ["Blu\u00b7men\u00b7sch\u00f6p\u00b7fer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Herzenfe\u00dfler,", "tokens": ["Her\u00b7zen\u00b7fe\u00df\u00b7ler", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wecker des Vergn\u00fcgens,", "tokens": ["We\u00b7cker", "des", "Ver\u00b7gn\u00fc\u00b7gens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Heil dir l\u00e4chelnder Bl\u00fcthenmond!", "tokens": ["Heil", "dir", "l\u00e4\u00b7cheln\u00b7der", "Bl\u00fc\u00b7then\u00b7mond", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Er beschwebet die Flur,", "tokens": ["Er", "be\u00b7schwe\u00b7bet", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Streuet Veilchen,", "tokens": ["Streu\u00b7et", "Veil\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schl\u00fc\u00dfelblumen,", "tokens": ["Schl\u00fc\u00b7\u00dfel\u00b7blu\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wei\u00dfe Mayenglocken,", "tokens": ["Wei\u00b7\u00dfe", "Ma\u00b7yeng\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Streut sein goldenes F\u00fcllhorn leer.", "tokens": ["Streut", "sein", "gol\u00b7de\u00b7nes", "F\u00fcll\u00b7horn", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "L\u00f6st die Haare des Hains,", "tokens": ["L\u00f6st", "die", "Haa\u00b7re", "des", "Hains", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "H\u00fcllt den Schlehstrauch,", "tokens": ["H\u00fcllt", "den", "Schleh\u00b7strauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "H\u00fcllt den Hagdorn,", "tokens": ["H\u00fcllt", "den", "Hag\u00b7dorn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Der den Garten z\u00e4unet,", "tokens": ["Der", "den", "Gar\u00b7ten", "z\u00e4u\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "H\u00fcllt den Kirschbaum in Bl\u00fcthenschnee.", "tokens": ["H\u00fcllt", "den", "Kirschbaum", "in", "Bl\u00fc\u00b7then\u00b7schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schaut, er tanzet heran,", "tokens": ["Schaut", ",", "er", "tan\u00b7zet", "he\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schaut, des Kirschbaums", "tokens": ["Schaut", ",", "des", "Kirschbaums"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wipfel s\u00e4useln", "tokens": ["Wip\u00b7fel", "s\u00e4u\u00b7seln"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ein Gew\u00f6lk von Silber", "tokens": ["Ein", "Ge\u00b7w\u00f6lk", "von", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Um sein wehendes Lockenhaar.", "tokens": ["Um", "sein", "we\u00b7hen\u00b7des", "Lo\u00b7cken\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Wie der Apfelbaum nickt!", "tokens": ["Wie", "der", "Ap\u00b7fel\u00b7baum", "nickt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Roth und wei\u00dfe", "tokens": ["Roth", "und", "wei\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bl\u00fcthen purpern", "tokens": ["Bl\u00fc\u00b7then", "pur\u00b7pern"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Seinen gr\u00fcnen Wipfel,", "tokens": ["Sei\u00b7nen", "gr\u00fc\u00b7nen", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Purpern alles Gezweig' umher.", "tokens": ["Pur\u00b7pern", "al\u00b7les", "Ge\u00b7zweig'", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Bien' auf Biene durchsummt", "tokens": ["Bi\u00b7en'", "auf", "Bie\u00b7ne", "durch\u00b7summt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bald die Bl\u00fcmchen", "tokens": ["Bald", "die", "Bl\u00fcm\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Unterm Baume,", "tokens": ["Un\u00b7term", "Bau\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Bald die Wipfelbl\u00fcthen,", "tokens": ["Bald", "die", "Wip\u00b7fel\u00b7bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die der Morgen mit Gold bemalt.", "tokens": ["Die", "der", "Mor\u00b7gen", "mit", "Gold", "be\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Tief im bunten Gew\u00f6lk,", "tokens": ["Tief", "im", "bun\u00b7ten", "Ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Das die rothen", "tokens": ["Das", "die", "ro\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ART", "ADJA"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Apfelbl\u00fcthen", "tokens": ["Ap\u00b7fel\u00b7bl\u00fc\u00b7then"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Um die Wipfel w\u00f6lken,", "tokens": ["Um", "die", "Wip\u00b7fel", "w\u00f6l\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "T\u00f6nt die Kehle der Nachtigall.", "tokens": ["T\u00f6nt", "die", "Keh\u00b7le", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Str\u00f6mt in Liedern dahin,", "tokens": ["Str\u00f6mt", "in", "Lie\u00b7dern", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PAV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "T\u00f6nt den J\u00fcngling,", "tokens": ["T\u00f6nt", "den", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der am Busen", "tokens": ["Der", "am", "Bu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Seiner Gattin schlummert,", "tokens": ["Sei\u00b7ner", "Gat\u00b7tin", "schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aus den Armen des Morgenschlafs.", "tokens": ["Aus", "den", "Ar\u00b7men", "des", "Mor\u00b7gen\u00b7schlafs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Seht, er wandelt mit ihr", "tokens": ["Seht", ",", "er", "wan\u00b7delt", "mit", "ihr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch den Garten,", "tokens": ["Durch", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wo die Sonne,", "tokens": ["Wo", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wo der blaue Himmel", "tokens": ["Wo", "der", "blau\u00b7e", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Durch die r\u00f6thlichten Bl\u00fcthen bebt.", "tokens": ["Durch", "die", "r\u00f6th\u00b7lich\u00b7ten", "Bl\u00fc\u00b7then", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Helle Morgenmusik", "tokens": ["Hel\u00b7le", "Mor\u00b7gen\u00b7mu\u00b7sik"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Str\u00f6mt vom Wipfel.", "tokens": ["Str\u00f6mt", "vom", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ihre Herzen", "tokens": ["Ih\u00b7re", "Her\u00b7zen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Tanzen nach den Fugen,", "tokens": ["Tan\u00b7zen", "nach", "den", "Fu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die der schmelzende Vogel t\u00f6nt.", "tokens": ["Die", "der", "schmel\u00b7zen\u00b7de", "Vo\u00b7gel", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Nachtigallenmusik", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len\u00b7mu\u00b7sik"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wirbelt Schlummer,", "tokens": ["Wir\u00b7belt", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "S\u00fc\u00dfen Schlummer", "tokens": ["S\u00fc\u00b7\u00dfen", "Schlum\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "\u00dcber ihre H\u00e4upter,", "tokens": ["\u00dc\u00b7ber", "ih\u00b7re", "H\u00e4up\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn die Stunde der Ruhe kommt.", "tokens": ["Wenn", "die", "Stun\u00b7de", "der", "Ru\u00b7he", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Hespers l\u00e4chelndes Aug", "tokens": ["Hes\u00b7pers", "l\u00e4\u00b7cheln\u00b7des", "Aug"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Blicket neidisch", "tokens": ["Bli\u00b7cket", "nei\u00b7disch"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Durch die Fenster,", "tokens": ["Durch", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und die Nachtigallen", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "T\u00f6nen fr\u00f6hlichen Brautgesang.", "tokens": ["T\u00f6\u00b7nen", "fr\u00f6h\u00b7li\u00b7chen", "Braut\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}