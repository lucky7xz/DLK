{"textgrid.poem.66145": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Am Geburtstage meiner Gattin", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als du geboren warst, als Gottes Licht", "tokens": ["Als", "du", "ge\u00b7bo\u00b7ren", "warst", ",", "als", "Got\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum ersten Mal dein keusches Aug' gek\u00fc\u00dft,", "tokens": ["Zum", "ers\u00b7ten", "Mal", "dein", "keu\u00b7sches", "Aug'", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da mischtest du an deiner Mutter Brust", "tokens": ["Da", "mischtest", "du", "an", "dei\u00b7ner", "Mut\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die s\u00fc\u00dfe Milch mit Thr\u00e4nen, die dir mehr,", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Milch", "mit", "Thr\u00e4\u00b7nen", ",", "die", "dir", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als andern S\u00e4uglingen entquollen;", "tokens": ["Als", "an\u00b7dern", "S\u00e4ug\u00b7lin\u00b7gen", "ent\u00b7quol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Denn ach! dein Engel stand am Eingang", "tokens": ["Denn", "ach", "!", "dein", "En\u00b7gel", "stand", "am", "Ein\u00b7gang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In deines Lebens dornbes\u00e4te G\u00e4nge", "tokens": ["In", "dei\u00b7nes", "Le\u00b7bens", "dorn\u00b7be\u00b7s\u00e4\u00b7te", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sprach prophetisch diese Worte:", "tokens": ["Und", "sprach", "pro\u00b7phe\u00b7tisch", "die\u00b7se", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Helena! in der Stunde deines Werdens", "tokens": ["He\u00b7le\u00b7na", "!", "in", "der", "Stun\u00b7de", "dei\u00b7nes", "Wer\u00b7dens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Sah ich im Heiligthum, von Wolkendunkel", "tokens": ["Sah", "ich", "im", "Hei\u00b7lig\u00b7thum", ",", "von", "Wol\u00b7ken\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dichtumflossen einen goldnen Becher,", "tokens": ["Dich\u00b7tum\u00b7flos\u00b7sen", "ei\u00b7nen", "gold\u00b7nen", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Gef\u00fcllt mit starkem Wein, durchbittert", "tokens": ["Ge\u00b7f\u00fcllt", "mit", "star\u00b7kem", "Wein", ",", "durch\u00b7bit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mit Wermuth. \u2013 \u00bbJa, sie soll ihn trinken,", "tokens": ["Mit", "Wer\u00b7muth", ".", "\u2013", "\u00bb", "Ja", ",", "sie", "soll", "ihn", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "$(", "$(", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Sprach weggewandt der Menschenvater,", "tokens": ["Sprach", "weg\u00b7ge\u00b7wandt", "der", "Men\u00b7schen\u00b7va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und ist er ausgeschl\u00fcrft bis auf die Hefe,", "tokens": ["Und", "ist", "er", "aus\u00b7ge\u00b7schl\u00fcrft", "bis", "auf", "die", "He\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die tr\u00fcb' und schlammig an des Bechers", "tokens": ["Die", "tr\u00fcb'", "und", "schlam\u00b7mig", "an", "des", "Be\u00b7chers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Goldnem Boden g\u00e4hrt: so hole sie!", "tokens": ["Gold\u00b7nem", "Bo\u00b7den", "g\u00e4hrt", ":", "so", "ho\u00b7le", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "(dich, Eliel, w\u00e4hl' ich zu ihrem Engel)", "tokens": ["(", "dich", ",", "E\u00b7liel", ",", "w\u00e4hl'", "ich", "zu", "ih\u00b7rem", "En\u00b7gel", ")"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NE", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "So hole sie in Wolken s\u00fc\u00dfes Schlummers", "tokens": ["So", "ho\u00b7le", "sie", "in", "Wol\u00b7ken", "s\u00fc\u00b7\u00dfes", "Schlum\u00b7mers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Herauf zu mir. Hier, diese Krone,", "tokens": ["Her\u00b7auf", "zu", "mir", ".", "Hier", ",", "die\u00b7se", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$.", "ADV", "$,", "PDAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Mit meines Himmels hellsten Steinen", "tokens": ["Mit", "mei\u00b7nes", "Him\u00b7mels", "hells\u00b7ten", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Bes\u00e4t \u2013 und dort dies Schneegewand,", "tokens": ["Be\u00b7s\u00e4t", "\u2013", "und", "dort", "dies", "Schnee\u00b7ge\u00b7wand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "ADV", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "So wei\u00df im Lammesblut gewaschen,", "tokens": ["So", "wei\u00df", "im", "Lam\u00b7mes\u00b7blut", "ge\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sei dann ihr Lohn! Auch spro\u00dft dort eine Palme", "tokens": ["Sei", "dann", "ihr", "Lohn", "!", "Auch", "spro\u00dft", "dort", "ei\u00b7ne", "Pal\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "F\u00fcr ihre Rechte, sie zu schwingen", "tokens": ["F\u00fcr", "ih\u00b7re", "Rech\u00b7te", ",", "sie", "zu", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Am gl\u00e4sernen Meer.\u00ab Der Menschenvater schwieg.", "tokens": ["Am", "gl\u00e4\u00b7ser\u00b7nen", "Meer", ".", "\u00ab", "Der", "Men\u00b7schen\u00b7va\u00b7ter", "schwieg", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ich flog herab und k\u00fchle dir die Wange,", "tokens": ["Ich", "flog", "her\u00b7ab", "und", "k\u00fch\u00b7le", "dir", "die", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Noch gl\u00fchend von den Schmerzen der Geburt.", "tokens": ["Noch", "gl\u00fc\u00b7hend", "von", "den", "Schmer\u00b7zen", "der", "Ge\u00b7burt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "O Dulderin, was hier der Engel sprach,", "tokens": ["O", "Dul\u00b7de\u00b7rin", ",", "was", "hier", "der", "En\u00b7gel", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ist bald erf\u00fcllt. Bald ist der letzte Tropfen", "tokens": ["Ist", "bald", "er\u00b7f\u00fcllt", ".", "Bald", "ist", "der", "letz\u00b7te", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Hinabgeschl\u00fcrft in deinem Leidenskelche.", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7schl\u00fcrft", "in", "dei\u00b7nem", "Lei\u00b7dens\u00b7kel\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, mancher Tropfen fiel wie Feuer", "tokens": ["Ach", ",", "man\u00b7cher", "Trop\u00b7fen", "fiel", "wie", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "NN", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Helena, aufs Herz. Doch keiner hei\u00dfer,", "tokens": ["Dir", ",", "He\u00b7le\u00b7na", ",", "aufs", "Herz", ".", "Doch", "kei\u00b7ner", "hei\u00b7\u00dfer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "APPRART", "NN", "$.", "KON", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als da ein Todesengel mich mit eiserm Arm", "tokens": ["Als", "da", "ein", "To\u00b7de\u00b7sen\u00b7gel", "mich", "mit", "ei\u00b7serm", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von deinem Busen ri\u00df, und mich", "tokens": ["Von", "dei\u00b7nem", "Bu\u00b7sen", "ri\u00df", ",", "und", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ins Felsengrab verschlo\u00df \u2013 lebendigtodt!", "tokens": ["Ins", "Fel\u00b7sen\u00b7grab", "ver\u00b7schlo\u00df", "\u2013", "le\u00b7ben\u00b7dig\u00b7todt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du eine Witwe \u2013 ich lebendigtodt!", "tokens": ["Du", "ei\u00b7ne", "Wit\u00b7we", "\u2013", "ich", "le\u00b7ben\u00b7dig\u00b7todt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$(", "PPER", "ADJD", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Die vollen Halme meiner Mannesjahre", "tokens": ["Die", "vol\u00b7len", "Hal\u00b7me", "mei\u00b7ner", "Man\u00b7nes\u00b7jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zerknickt, im Hagelsturm zerknickt!", "tokens": ["Zer\u00b7knickt", ",", "im", "Ha\u00b7gel\u00b7sturm", "zer\u00b7knickt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da starrtest du \u2013 ein Denkmal des Entsetzens!", "tokens": ["Da", "starr\u00b7test", "du", "\u2013", "ein", "Denk\u00b7mal", "des", "Ent\u00b7set\u00b7zens", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und deine Kinder heulend um dich her.", "tokens": ["Und", "dei\u00b7ne", "Kin\u00b7der", "heu\u00b7lend", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So liegen abgeri\u00dfne Zweige um den Baum,", "tokens": ["So", "lie\u00b7gen", "ab\u00b7ge\u00b7ri\u00df\u00b7ne", "Zwei\u00b7ge", "um", "den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Gottes Wetterstrahl gefl\u00fcgelt traf;", "tokens": ["Den", "Got\u00b7tes", "Wet\u00b7ter\u00b7strahl", "ge\u00b7fl\u00fc\u00b7gelt", "traf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Aufdampft der Stamm und Zweig' und Wipfel dorren.", "tokens": ["Auf\u00b7dampft", "der", "Stamm", "und", "Zweig'", "und", "Wip\u00b7fel", "dor\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ich aber lag in grauser Kerkernacht", "tokens": ["Ich", "a\u00b7ber", "lag", "in", "grau\u00b7ser", "Ker\u00b7ker\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und meine Ketten klirrten f\u00fcrchterlich.", "tokens": ["Und", "mei\u00b7ne", "Ket\u00b7ten", "klirr\u00b7ten", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Doch f\u00fcrchterlicher war das Angstgebr\u00fcll", "tokens": ["Doch", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "war", "das", "Angst\u00b7ge\u00b7br\u00fcll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Nach Freiheit! und nach dir! und meinen Kindern!", "tokens": ["Nach", "Frei\u00b7heit", "!", "und", "nach", "dir", "!", "und", "mei\u00b7nen", "Kin\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "KON", "APPR", "PPER", "$.", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Von Thr\u00e4nenblut und Angstschwei\u00df faulte", "tokens": ["Von", "Thr\u00e4\u00b7nen\u00b7blut", "und", "A\u00b7ngstschwei\u00df", "faul\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.1