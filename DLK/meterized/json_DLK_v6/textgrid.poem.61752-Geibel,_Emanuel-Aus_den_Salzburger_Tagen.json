{"textgrid.poem.61752": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Aus den Salzburger Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutsches Volk, was s\u00e4umst du l\u00e4nger?", "tokens": ["Deut\u00b7sches", "Volk", ",", "was", "s\u00e4umst", "du", "l\u00e4n\u00b7ger", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau, wie deinem alten Dr\u00e4nger", "tokens": ["Schau", ",", "wie", "dei\u00b7nem", "al\u00b7ten", "Dr\u00e4n\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon vor deiner Eintracht graust,", "tokens": ["Schon", "vor", "dei\u00b7ner", "Ein\u00b7tracht", "graust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er mit beklemmten Sinnen", "tokens": ["Wie", "er", "mit", "be\u00b7klemm\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese Zinnen", "tokens": ["Die\u00b7se", "Zin\u00b7nen"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Steigen sieht, die du erbaust.", "tokens": ["Stei\u00b7gen", "sieht", ",", "die", "du", "er\u00b7baust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und du wolltest von dem Werke", "tokens": ["Und", "du", "woll\u00b7test", "von", "dem", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Wachstums, deiner St\u00e4rke", "tokens": ["Dei\u00b7nes", "Wachs\u00b7tums", ",", "dei\u00b7ner", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lassen, nun es halb gereift,", "tokens": ["Las\u00b7sen", ",", "nun", "es", "halb", "ge\u00b7reift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil mit eingezogner Klaue", "tokens": ["Weil", "mit", "ein\u00b7ge\u00b7zog\u00b7ner", "Klau\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir der Schlaue", "tokens": ["Dir", "der", "Schlau\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Seinen alten Lockruf pfeift?", "tokens": ["Sei\u00b7nen", "al\u00b7ten", "Lock\u00b7ruf", "pfeift", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freilich m\u00f6cht' er dich zerspalten;", "tokens": ["Frei\u00b7lich", "m\u00f6cht'", "er", "dich", "zer\u00b7spal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennt er doch den Spruch der Alten:", "tokens": ["Kennt", "er", "doch", "den", "Spruch", "der", "Al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leicht gebietet, wer entzweit.", "tokens": ["Leicht", "ge\u00b7bie\u00b7tet", ",", "wer", "ent\u00b7zweit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freilich drum in die Gem\u00fcter", "tokens": ["Frei\u00b7lich", "drum", "in", "die", "Ge\u00b7m\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner H\u00fcter", "tokens": ["Dei\u00b7ner", "H\u00fc\u00b7ter"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "S\u00e4t er Argwohn, Ha\u00df und Neid.", "tokens": ["S\u00e4t", "er", "Arg\u00b7wohn", ",", "Ha\u00df", "und", "Neid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber la\u00df dich nicht verwirren!", "tokens": ["A\u00b7ber", "la\u00df", "dich", "nicht", "ver\u00b7wir\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Achte seinen Rat dem Girren", "tokens": ["Ach\u00b7te", "sei\u00b7nen", "Rat", "dem", "Gir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener ersten Schlange gleich!", "tokens": ["Je\u00b7ner", "ers\u00b7ten", "Schlan\u00b7ge", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Baue weiter unverdrossen!", "tokens": ["Bau\u00b7e", "wei\u00b7ter", "un\u00b7ver\u00b7dros\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihm zum Possen", "tokens": ["Ihm", "zum", "Pos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Bau' es aus, das deutsche Reich!", "tokens": ["Bau'", "es", "aus", ",", "das", "deut\u00b7sche", "Reich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "St\u00e4mme w\u00e4lz' und Quaderst\u00fccke", "tokens": ["St\u00e4m\u00b7me", "w\u00e4lz'", "und", "Qua\u00b7der\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den Main und wirf die Br\u00fccke", "tokens": ["An", "den", "Main", "und", "wirf", "die", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber den ents\u00fchnten Strom", "tokens": ["\u00dc\u00b7ber", "den", "ent\u00b7s\u00fchn\u00b7ten", "Strom"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, den dort die Fluten waschen,", "tokens": ["Und", ",", "den", "dort", "die", "Flu\u00b7ten", "wa\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Aus den Aschen", "tokens": ["Aus", "den", "A\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Richt' empor den Kaiserdom!", "tokens": ["Richt'", "em\u00b7por", "den", "Kai\u00b7ser\u00b7dom", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und zur Antwort auf die leise", "tokens": ["Und", "zur", "Ant\u00b7wort", "auf", "die", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Buhlende Sirenenweise,", "tokens": ["Buh\u00b7len\u00b7de", "Si\u00b7re\u00b7nen\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die so lind sich wiegt im West,", "tokens": ["Die", "so", "lind", "sich", "wiegt", "im", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df verk\u00fcnden seine Glocken", "tokens": ["La\u00df", "ver\u00b7k\u00fcn\u00b7den", "sei\u00b7ne", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Frohlocken", "tokens": ["Mit", "Froh\u00b7lo\u00b7cken"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Deines Schirmvogts Kr\u00f6nungsfest!", "tokens": ["Dei\u00b7nes", "Schirm\u00b7vogts", "Kr\u00f6\u00b7nungs\u00b7fest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}