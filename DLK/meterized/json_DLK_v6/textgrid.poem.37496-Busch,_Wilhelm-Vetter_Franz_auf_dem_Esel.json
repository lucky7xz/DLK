{"textgrid.poem.37496": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vetter Franz auf dem Esel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die h\u00fcbschen B\u00e4schen bitten sehr:", "tokens": ["Die", "h\u00fcb\u00b7schen", "B\u00e4\u00b7schen", "bit\u00b7ten", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbach, Vetter Franz! reit' mal umher!\u00ab", "tokens": ["\u00bb", "ach", ",", "Vet\u00b7ter", "Franz", "!", "reit'", "mal", "um\u00b7her", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "NE", "$.", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und Franz, nat\u00fcrlich gleich bereit,", "tokens": ["Und", "Franz", ",", "na\u00b7t\u00fcr\u00b7lich", "gleich", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewinnt das Tier durch Freundlichkeit.", "tokens": ["Ge\u00b7winnt", "das", "Tier", "durch", "Freund\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon sitzt er drauf und kommt nicht weiter,", "tokens": ["Schon", "sitzt", "er", "drauf", "und", "kommt", "nicht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Worob die Basen \u00e4u\u00dferst heiter.", "tokens": ["Wo\u00b7rob", "die", "Ba\u00b7sen", "\u00e4u\u00b7\u00dferst", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er denkt: \u00bbJa wart'! du wirst schon gehn!", "tokens": ["Er", "denkt", ":", "\u00bb", "Ja", "wart'", "!", "du", "wirst", "schon", "gehn", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PTKANT", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mu\u00df dich 'mal beim Schwanze drehn!\u00ab", "tokens": ["Ich", "mu\u00df", "dich", "'mal", "beim", "Schwan\u00b7ze", "drehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jetzt brennt er ihm am Kreuz herum,", "tokens": ["Jetzt", "brennt", "er", "ihm", "am", "Kreuz", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Esel ziehn die Schmerzen krumm.", "tokens": ["Den", "E\u00b7sel", "ziehn", "die", "Schmer\u00b7zen", "krumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und er, der eben noch verstockt,", "tokens": ["Und", "er", ",", "der", "e\u00b7ben", "noch", "ver\u00b7stockt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ermuntert sich und springt und bockt.", "tokens": ["Er\u00b7mun\u00b7tert", "sich", "und", "springt", "und", "bockt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Im Eck, wo die Ger\u00e4te h\u00e4ngen,", "tokens": ["Im", "Eck", ",", "wo", "die", "Ge\u00b7r\u00e4\u00b7te", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht er den Vetter zu bedr\u00e4ngen.", "tokens": ["Sucht", "er", "den", "Vet\u00b7ter", "zu", "be\u00b7dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun druckt er gar nach hint' hinaus", "tokens": ["Nun", "druckt", "er", "gar", "nach", "hint'", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins glasbedeckte Blumenhaus.", "tokens": ["Ins", "glas\u00b7be\u00b7deck\u00b7te", "Blu\u00b7men\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da steht die bittre Aloe,", "tokens": ["Da", "steht", "die", "bitt\u00b7re", "A\u00b7loe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Setzt man sich drauf, so tut es weh.", "tokens": ["Setzt", "man", "sich", "drauf", ",", "so", "tut", "es", "weh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "PAV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die treibt durch ihre Dorne", "tokens": ["Die", "treibt", "durch", "ih\u00b7re", "Dor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Esel schnell nach vorne.", "tokens": ["Den", "E\u00b7sel", "schnell", "nach", "vor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und \u2013 schwupp! \u2013 kommt Vetter Franz im Bogen", "tokens": ["Und", "\u2013", "schwupp", "!", "\u2013", "kommt", "Vet\u00b7ter", "Franz", "im", "Bo\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "ADJD", "$.", "$(", "VVFIN", "NE", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf die Kusinen zugeflogen.", "tokens": ["Auf", "die", "Ku\u00b7si\u00b7nen", "zu\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "Und \u2013 plauz! perdautz! \u2013 geht's \u00fcber Kopf", "tokens": ["Und", "\u2013", "plauz", "!", "per\u00b7dautz", "!", "\u2013", "geht's", "\u00fc\u00b7ber", "Kopf"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "$(", "VVIMP", "$.", "NN", "$.", "$(", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Butter und durch Millichtopf.", "tokens": ["Durch", "But\u00b7ter", "und", "durch", "Mil\u00b7lich\u00b7topf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Am Schlu\u00df bem\u00fcht ein jeder sich,", "tokens": ["Am", "Schlu\u00df", "be\u00b7m\u00fcht", "ein", "je\u00b7der", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "PIAT", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinwegzutun, was hinderlich.", "tokens": ["Hin\u00b7weg\u00b7zu\u00b7tun", ",", "was", "hin\u00b7der\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}