{"textgrid.poem.41420": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Jupiter, die Thiere und der Mensch", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Jupiter der unbewohnten Erde", "tokens": ["Als", "Ju\u00b7pi\u00b7ter", "der", "un\u00b7be\u00b7wohn\u00b7ten", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Menschen und die Thiere schuf,", "tokens": ["Die", "Men\u00b7schen", "und", "die", "Thie\u00b7re", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bestimmt er jeglichem den k\u00fcnftigen Beruf,", "tokens": ["Be\u00b7stimmt", "er", "jeg\u00b7li\u00b7chem", "den", "k\u00fcnf\u00b7ti\u00b7gen", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Lebens Art und Zeit und Arbeit und Beschwerde.", "tokens": ["Des", "Le\u00b7bens", "Art", "und", "Zeit", "und", "Ar\u00b7beit", "und", "Be\u00b7schwer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zum Esel sagte Zeus: Dein Schicksal legt dir Last", "tokens": ["Zum", "E\u00b7sel", "sag\u00b7te", "Zeus", ":", "Dein", "Schick\u00b7sal", "legt", "dir", "Last"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und harte Knechtschaft auf; nur Disteln, keine Mast.", "tokens": ["Und", "har\u00b7te", "Knecht\u00b7schaft", "auf", ";", "nur", "Dis\u00b7teln", ",", "kei\u00b7ne", "Mast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$.", "ADV", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ist dein Loos. Wohlan! so dien', und lebe", "tokens": ["Das", "ist", "dein", "Loos", ".", "Wo\u00b7hlan", "!", "so", "dien'", ",", "und", "le\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "$.", "ADV", "PDS", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So viele Jahr', als ich dem Monat Tage gebe.", "tokens": ["So", "vie\u00b7le", "Jahr'", ",", "als", "ich", "dem", "Mo\u00b7nat", "Ta\u00b7ge", "ge\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Esel Erstling schreit: Zu viel legst du mir bei.", "tokens": ["Der", "E\u00b7sel", "Erst\u00b7ling", "schreit", ":", "Zu", "viel", "legst", "du", "mir", "bei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$.", "APPR", "PIS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie? drei\u00dfig Jahre! Zeus! ach nimm mir zwanzig Jahre.", "tokens": ["Wie", "?", "drei\u00b7\u00dfig", "Jah\u00b7re", "!", "Zeus", "!", "ach", "nimm", "mir", "zwan\u00b7zig", "Jah\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "CARD", "NN", "$.", "NE", "$.", "XY", "VVIMP", "PPER", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sonst qu\u00e4l' ich mich zu lang: es graun mir schon die Haare.", "tokens": ["Sonst", "qu\u00e4l'", "ich", "mich", "zu", "lang", ":", "es", "graun", "mir", "schon", "die", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKA", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der gro\u00dfe Zeus erh\u00f6rt sein flehendes Geschrei.", "tokens": ["Der", "gro\u00b7\u00dfe", "Zeus", "er\u00b7h\u00f6rt", "sein", "fle\u00b7hen\u00b7des", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum Hunde spricht er: Wache flei\u00dfig!", "tokens": ["Zum", "Hun\u00b7de", "spricht", "er", ":", "Wa\u00b7che", "flei\u00b7\u00dfig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00fct' eifrig Trift und Haus! du \u00fcberkamst von mir", "tokens": ["H\u00fct'", "eif\u00b7rig", "Trift", "und", "Haus", "!", "du", "\u00fc\u00b7ber\u00b7kamst", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Muth, Treue, Fertigkeit, und du erreichst daf\u00fcr", "tokens": ["Muth", ",", "Treu\u00b7e", ",", "Fer\u00b7tig\u00b7keit", ",", "und", "du", "er\u00b7reichst", "da\u00b7f\u00fcr"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "PPER", "VVFIN", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An edlen Jahren f\u00fcnfunddrei\u00dfig ...", "tokens": ["An", "ed\u00b7len", "Jah\u00b7ren", "f\u00fcn\u00b7fund\u00b7drei\u00b7\u00dfig", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "CARD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das W\u00e4chteramt ist schwer: ich bitte, Herr, von dir,", "tokens": ["Das", "W\u00e4ch\u00b7ter\u00b7amt", "ist", "schwer", ":", "ich", "bit\u00b7te", ",", "Herr", ",", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "ADV", "$,", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Dauer meiner Pflicht aus Mitleid einzuschr\u00e4nken,", "tokens": ["Die", "Dau\u00b7er", "mei\u00b7ner", "Pflicht", "aus", "Mit\u00b7leid", "ein\u00b7zu\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und f\u00fcnfundzwanzig mir zu schenken.", "tokens": ["Und", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zig", "mir", "zu", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Gunst gew\u00e4hret ihm der Gott.", "tokens": ["Die", "Gunst", "ge\u00b7w\u00e4h\u00b7ret", "ihm", "der", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zum Affen sagt er drauf: Du Halbmensch, deine Mienen,", "tokens": ["Zum", "Af\u00b7fen", "sagt", "er", "drauf", ":", "Du", "Halb\u00b7mensch", ",", "dei\u00b7ne", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein ganzes Wesen kann zu nichts als Kurzweil dienen.", "tokens": ["Dein", "gan\u00b7zes", "We\u00b7sen", "kann", "zu", "nichts", "als", "Kurz\u00b7weil", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sei nackt, gefesselt, arm, der Kinder Lust und Spott,", "tokens": ["Sei", "nackt", ",", "ge\u00b7fes\u00b7selt", ",", "arm", ",", "der", "Kin\u00b7der", "Lust", "und", "Spott", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VVPP", "$,", "ADJD", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der Bedienten Spiel, auf sechs Olympiaden.", "tokens": ["Und", "der", "Be\u00b7dien\u00b7ten", "Spiel", ",", "auf", "sechs", "O\u00b7lym\u00b7pia\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,", "APPR", "CARD", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sechs! spricht der Aff', o gib mir doch aus Gnaden", "tokens": ["Sechs", "!", "spricht", "der", "Aff'", ",", "o", "gib", "mir", "doch", "aus", "Gna\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "$.", "VVFIN", "ART", "NN", "$,", "FM", "VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur vier. Die sind genug. Nur l\u00e4cherlich zu sein,", "tokens": ["Nur", "vier", ".", "Die", "sind", "ge\u00b7nug", ".", "Nur", "l\u00e4\u00b7cher\u00b7lich", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "$.", "ART", "VAFIN", "ADV", "$.", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bedarf ich wenig Zeit. Zeus r\u00e4umt die Zeit ihm ein.", "tokens": ["Be\u00b7darf", "ich", "we\u00b7nig", "Zeit", ".", "Zeus", "r\u00e4umt", "die", "Zeit", "ihm", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$.", "NE", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es n\u00e4hert sich der Mensch. Zeus spricht: Du, meine Freude,", "tokens": ["Es", "n\u00e4\u00b7hert", "sich", "der", "Mensch", ".", "Zeus", "spricht", ":", "Du", ",", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$.", "NE", "VVFIN", "$.", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-++--+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du zierst mein neues Weltgeb\u00e4ude.", "tokens": ["Du", "zierst", "mein", "neu\u00b7es", "Welt\u00b7ge\u00b7b\u00e4u\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist mein Meisterst\u00fcck. Es sei die Erde dein!", "tokens": ["Du", "bist", "mein", "Meis\u00b7ter\u00b7st\u00fcck", ".", "Es", "sei", "die", "Er\u00b7de", "dein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr dich sei sie so sch\u00f6n, so fruchtbar, so voll Sch\u00e4tze.", "tokens": ["F\u00fcr", "dich", "sei", "sie", "so", "sch\u00f6n", ",", "so", "frucht\u00b7bar", ",", "so", "voll", "Sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vers\u00e4ume nicht, dich zu erfreun,", "tokens": ["Ver\u00b7s\u00e4u\u00b7me", "nicht", ",", "dich", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ich zum Leben dir nur drei\u00dfig Sommer setze.", "tokens": ["Weil", "ich", "zum", "Le\u00b7ben", "dir", "nur", "drei\u00b7\u00dfig", "Som\u00b7mer", "set\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Fast wie beim ersten Blitz, beim ersten Donnerschlag", "tokens": ["Fast", "wie", "beim", "ers\u00b7ten", "Blitz", ",", "beim", "ers\u00b7ten", "Don\u00b7ner\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erschrak der Mensch, und sagt: O Zeus, dein Sch\u00f6pfungstag", "tokens": ["Er\u00b7schrak", "der", "Mensch", ",", "und", "sagt", ":", "O", "Zeus", ",", "dein", "Sch\u00f6p\u00b7fungs\u00b7tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$.", "NE", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bereichert mich mit deinen besten Gaben;", "tokens": ["Be\u00b7rei\u00b7chert", "mich", "mit", "dei\u00b7nen", "bes\u00b7ten", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch, soll mein Dasein nur so wenig Jahre haben?", "tokens": ["Doch", ",", "soll", "mein", "Da\u00b7sein", "nur", "so", "we\u00b7nig", "Jah\u00b7re", "ha\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das ist bejammernswerth! Dafern ich w\u00e4hlen mag,", "tokens": ["Das", "ist", "be\u00b7jam\u00b7merns\u00b7werth", "!", "Da\u00b7fern", "ich", "w\u00e4h\u00b7len", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$.", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So w\u00e4hl' ich mir zu meinem l\u00e4ngern Leben,", "tokens": ["So", "w\u00e4hl'", "ich", "mir", "zu", "mei\u00b7nem", "l\u00e4n\u00b7gern", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was Esel, Hund und Aff' an ihrem aufgegeben.", "tokens": ["Was", "E\u00b7sel", ",", "Hund", "und", "Aff'", "an", "ih\u00b7rem", "auf\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es sei! spricht Jupiter: doch dies bleibt festgestellt:", "tokens": ["Es", "sei", "!", "spricht", "Ju\u00b7pi\u00b7ter", ":", "doch", "dies", "bleibt", "fest\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "VVFIN", "NN", "$.", "ADV", "PDS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dein l\u00e4ngres Alter soll, nach jenen drei\u00dfig Jahren,", "tokens": ["Dein", "l\u00e4ng\u00b7res", "Al\u00b7ter", "soll", ",", "nach", "je\u00b7nen", "drei\u00b7\u00dfig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "$,", "APPR", "PDAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auch jedes Thieres Stand erfahren,", "tokens": ["Auch", "je\u00b7des", "Thie\u00b7res", "Stand", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dem ich die Zeit erlie\u00df, die jetzt der Mensch erh\u00e4lt.", "tokens": ["Dem", "ich", "die", "Zeit", "er\u00b7lie\u00df", ",", "die", "jetzt", "der", "Mensch", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ganz unver\u00e4nderlich ist dieser G\u00f6tterschlu\u00df.", "tokens": ["Ganz", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "ist", "die\u00b7ser", "G\u00f6t\u00b7ter\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur unsre Jugend ist der Sitz der Fr\u00f6hlichkeiten.", "tokens": ["Nur", "uns\u00b7re", "Ju\u00b7gend", "ist", "der", "Sitz", "der", "Fr\u00f6h\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir spielen drei\u00dfig Jahr', ohn' Ernst und Ueberdru\u00df,", "tokens": ["Wir", "spie\u00b7len", "drei\u00b7\u00dfig", "Jahr'", ",", "ohn'", "Ernst", "und", "Ue\u00b7berd\u00b7ru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir kennen nicht den Zwang der strengern Folgezeiten,", "tokens": ["Wir", "ken\u00b7nen", "nicht", "den", "Zwang", "der", "stren\u00b7gern", "Fol\u00b7ge\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und unser Leben ist Genu\u00df.", "tokens": ["Und", "un\u00b7ser", "Le\u00b7ben", "ist", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns wollte Jupiter nur dieses Alter geben.", "tokens": ["Uns", "woll\u00b7te", "Ju\u00b7pi\u00b7ter", "nur", "die\u00b7ses", "Al\u00b7ter", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach h\u00e4tte doch die\u00df Flehen nichts erreicht,", "tokens": ["Ach", "h\u00e4t\u00b7te", "doch", "die\u00df", "Fle\u00b7hen", "nichts", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "ADV", "PDS", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und uns kein Wahn verf\u00fchrt, nach fernerm Ziel zu streben!", "tokens": ["Und", "uns", "kein", "Wahn", "ver\u00b7f\u00fchrt", ",", "nach", "fer\u00b7nerm", "Ziel", "zu", "stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kaum, da\u00df der Menschen Lenz, die Zeit der Lust, verstreicht,", "tokens": ["Kaum", ",", "da\u00df", "der", "Men\u00b7schen", "Lenz", ",", "die", "Zeit", "der", "Lust", ",", "ver\u00b7streicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So \u00fcberladen uns mit ungewohnten B\u00fcrden", "tokens": ["So", "\u00fc\u00b7berl\u00b7a\u00b7den", "uns", "mit", "un\u00b7ge\u00b7wohn\u00b7ten", "B\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Haus- und Ehestand, Gesch\u00e4fte, Pflichten, W\u00fcrden,", "tokens": ["Der", "Haus", "und", "E\u00b7hes\u00b7tand", ",", "Ge\u00b7sch\u00e4f\u00b7te", ",", "Pflich\u00b7ten", ",", "W\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bis da\u00df der Thiere Herr dem tr\u00e4gsten Lastthier gleicht.", "tokens": ["Bis", "da\u00df", "der", "Thie\u00b7re", "Herr", "dem", "tr\u00e4gs\u00b7ten", "Last\u00b7thier", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der F\u00fcnfzigj\u00e4hrige besitzt nur seine G\u00fcter,", "tokens": ["Der", "F\u00fcnf\u00b7zig\u00b7j\u00e4h\u00b7ri\u00b7ge", "be\u00b7sitzt", "nur", "sei\u00b7ne", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vermeidet den Gebrauch, entbehret, was er hat,", "tokens": ["Ver\u00b7mei\u00b7det", "den", "Ge\u00b7brauch", ",", "ent\u00b7beh\u00b7ret", ",", "was", "er", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "H\u00e4uft, rechnet, z\u00e4hlt, verschlie\u00dft, scheut Diebstahl und Verrath,", "tokens": ["H\u00e4uft", ",", "rech\u00b7net", ",", "z\u00e4hlt", ",", "ver\u00b7schlie\u00dft", ",", "scheut", "Dieb\u00b7stahl", "und", "Ver\u00b7rath", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist schlaflos, wie sein Hund, auch ein so scharfer H\u00fcter.", "tokens": ["Ist", "schlaf\u00b7los", ",", "wie", "sein", "Hund", ",", "auch", "ein", "so", "schar\u00b7fer", "H\u00fc\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,", "ADV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der ganz verl\u00e4hmte Greis, der k\u00fcmmerlich sich regt,", "tokens": ["Der", "ganz", "ver\u00b7l\u00e4hm\u00b7te", "Greis", ",", "der", "k\u00fcm\u00b7mer\u00b7lich", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sitzt, wie der Halbmensch an der Kette.", "tokens": ["Sitzt", ",", "wie", "der", "Halb\u00b7mensch", "an", "der", "Ket\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Noch gl\u00fccklich, wenn er nicht auch dessen Schicksal h\u00e4tte,", "tokens": ["Noch", "gl\u00fcck\u00b7lich", ",", "wenn", "er", "nicht", "auch", "des\u00b7sen", "Schick\u00b7sal", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df Kind und Knecht und Magd ihn zu belachen pflegt.", "tokens": ["Da\u00df", "Kind", "und", "Knecht", "und", "Magd", "ihn", "zu", "be\u00b7la\u00b7chen", "pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+