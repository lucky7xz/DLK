{"dta.poem.1922": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Offt wo ein v\u00f6llig\nvolles Korn der Furche ward vertraut/", "tokens": ["Offt", "wo", "ein", "v\u00f6l\u00b7lig", "vol\u00b7les", "Korn", "der", "Fur\u00b7che", "ward", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJD", "ADJA", "NN", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "W\u00e4chst ungl\u00fcckselge Tresp\u2019 und fliegend Haber-Kraut.", "tokens": ["W\u00e4chst", "un\u00b7gl\u00fcck\u00b7sel\u00b7ge", "Tre\u00b7sp'", "und", "flie\u00b7gend", "Ha\u00b7ber\u00b7Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJD", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ist nichts als rauche R\u00e4hm und Distel zu erholen.", "tokens": ["Ist", "nichts", "als", "rau\u00b7che", "R\u00e4hm", "und", "Dis\u00b7tel", "zu", "er\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}