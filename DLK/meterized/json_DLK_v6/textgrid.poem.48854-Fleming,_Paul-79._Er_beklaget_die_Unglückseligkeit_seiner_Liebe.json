{"textgrid.poem.48854": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "79. Er beklaget die Ungl\u00fcckseligkeit seiner Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "die sch\u00f6ne ", "tokens": ["die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Was ich vor liebte so, das mu\u00df ich gleichsam hassen,", "tokens": ["Was", "ich", "vor", "lieb\u00b7te", "so", ",", "das", "mu\u00df", "ich", "gleich\u00b7sam", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "VVFIN", "ADV", "$,", "PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als ob mir niemals wol von ihr gewesen w\u00e4r'.", "tokens": ["als", "ob", "mir", "nie\u00b7mals", "wol", "von", "ihr", "ge\u00b7we\u00b7sen", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist, Amor, di\u00df der Lohn, da\u00df ich dich also ehr'?", "tokens": ["Ist", ",", "A\u00b7mor", ",", "di\u00df", "der", "Lohn", ",", "da\u00df", "ich", "dich", "al\u00b7so", "ehr'", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$,", "PDS", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Grausamer! Was Trost, was Herze soll ich fassen?", "tokens": ["O", "Grau\u00b7sa\u00b7mer", "!", "Was", "Trost", ",", "was", "Her\u00b7ze", "soll", "ich", "fas\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "NN", "$,", "PWS", "VVFIN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weh' euch, ihr Augen, weh', ihr traurigen, ihr nassen,", "tokens": ["Weh'", "euch", ",", "ihr", "Au\u00b7gen", ",", "weh'", ",", "ihr", "trau\u00b7ri\u00b7gen", ",", "ihr", "nas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihr weinet doch nicht gnung, und weint ihr noch so sehr.", "tokens": ["ihr", "wei\u00b7net", "doch", "nicht", "gnung", ",", "und", "weint", "ihr", "noch", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Leander, Pyramus und wer ihr andern seid,", "tokens": ["Le\u00b7an\u00b7der", ",", "Py\u00b7ra\u00b7mus", "und", "wer", "ihr", "an\u00b7dern", "seid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "PWS", "PPER", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die ihr noch itzt beklagt der Liebe Grausamkeit,", "tokens": ["die", "ihr", "noch", "itzt", "be\u00b7klagt", "der", "Lie\u00b7be", "Grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "was ist doch eure Pein f\u00fcr meiner Angst gewesen?", "tokens": ["was", "ist", "doch", "eu\u00b7re", "Pein", "f\u00fcr", "mei\u00b7ner", "Angst", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein milder Augenblick entfreit' euch aller Not,", "tokens": ["Ein", "mil\u00b7der", "Au\u00b7gen\u00b7blick", "ent\u00b7freit'", "euch", "al\u00b7ler", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "half allen Schmerzen ab. Vor mir fleugt auch mein Tod,", "tokens": ["half", "al\u00b7len", "Schmer\u00b7zen", "ab", ".", "Vor", "mir", "fleugt", "auch", "mein", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$.", "APPR", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "darmit ich dermaleins ja m\u00f6ge nicht genesen.", "tokens": ["dar\u00b7mit", "ich", "der\u00b7ma\u00b7leins", "ja", "m\u00f6\u00b7ge", "nicht", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VMFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}