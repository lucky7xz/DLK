{"textgrid.poem.48168": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Das Ende", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und also war's zum letztenmal,", "tokens": ["Und", "al\u00b7so", "wa\u00b7r's", "zum", "letz\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPRART", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "da\u00df unsre H\u00e4nde sich umfangen, \u2013", "tokens": ["da\u00df", "uns\u00b7re", "H\u00e4n\u00b7de", "sich", "um\u00b7fan\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "dann bin ich stumm, wie Hagar einst,", "tokens": ["dann", "bin", "ich", "stumm", ",", "wie", "Ha\u00b7gar", "einst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PWAV", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in Nacht und Not hinausgegangen.", "tokens": ["in", "Nacht", "und", "Not", "hin\u00b7aus\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "An meines Lebens Himmel war", "tokens": ["An", "mei\u00b7nes", "Le\u00b7bens", "Him\u00b7mel", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der letzte lichte Stern gesunken,", "tokens": ["der", "letz\u00b7te", "lich\u00b7te", "Stern", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die heilge Glut in meiner Brust", "tokens": ["die", "heil\u00b7ge", "Glut", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "erloschen bis zum Aschenfunken.", "tokens": ["er\u00b7lo\u00b7schen", "bis", "zum", "A\u00b7schen\u00b7fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was jetzt noch kommt ist Schmerz und Schmach,", "tokens": ["Was", "jetzt", "noch", "kommt", "ist", "Schmerz", "und", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ist todesruhiges Entsagen:", "tokens": ["ist", "to\u00b7des\u00b7ru\u00b7hi\u00b7ges", "Ent\u00b7sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "ich werde meines Daseins Last", "tokens": ["ich", "wer\u00b7de", "mei\u00b7nes", "Da\u00b7seins", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit ungebeugtem Haupte tragen.", "tokens": ["mit", "un\u00b7ge\u00b7beug\u00b7tem", "Haup\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und sagt ich's euch, ihr glaubtet's nicht,", "tokens": ["Und", "sagt", "ich's", "euch", ",", "ihr", "glaub\u00b7tet's", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "selbst nicht den fr\u00fch gebleichten Haaren:", "tokens": ["selbst", "nicht", "den", "fr\u00fch", "ge\u00b7bleich\u00b7ten", "Haa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wie riesenstark das Menschenherz,", "tokens": ["wie", "rie\u00b7sen\u00b7stark", "das", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mu\u00df jeder an sich selbst erfahren.", "tokens": ["mu\u00df", "je\u00b7der", "an", "sich", "selbst", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}