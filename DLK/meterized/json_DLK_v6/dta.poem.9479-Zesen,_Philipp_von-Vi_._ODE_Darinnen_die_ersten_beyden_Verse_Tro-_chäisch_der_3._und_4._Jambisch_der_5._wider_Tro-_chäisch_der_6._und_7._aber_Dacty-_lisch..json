{"dta.poem.9479": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n ODE/  \n  Darinnen die ersten beyden Verse Tro-  \n ch\u00e4isch/ der 3. und 4. Jambisch/ der 5. wider Tro-  \n ch\u00e4isch/ der 6. und 7. aber Dacty-  \n lisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! mein Licht/", "tokens": ["Ach", "!", "mein", "Licht", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Es kann nicht", "tokens": ["Es", "kann", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Verdrie\u00dfen Jhr/", "tokens": ["Ver\u00b7drie\u00b7\u00dfen", "Ihr", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ach! bleib bey mir.", "tokens": ["Ach", "!", "bleib", "bey", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}