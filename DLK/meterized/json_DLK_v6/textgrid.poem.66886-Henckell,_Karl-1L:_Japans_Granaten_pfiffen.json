{"textgrid.poem.66886": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Japans Granaten pfiffen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Japans Granaten pfiffen", "tokens": ["Ja\u00b7pans", "Gra\u00b7na\u00b7ten", "pfif\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zischten berstend auf Deck,", "tokens": ["Und", "zischten", "bers\u00b7tend", "auf", "Deck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kommandeur auf den russischen Schiffen", "tokens": ["Kom\u00b7man\u00b7deur", "auf", "den", "rus\u00b7si\u00b7schen", "Schif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "War der Tod und der Mord und der Schreck.", "tokens": ["War", "der", "Tod", "und", "der", "Mord", "und", "der", "Schreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ein Fluchen, Murren, Grollen", "tokens": ["Ein", "Flu\u00b7chen", ",", "Mur\u00b7ren", ",", "Grol\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Backbord zum Vorderkiel:", "tokens": ["Vom", "Back\u00b7bord", "zum", "Vor\u00b7der\u00b7kiel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbund wir wollen nicht mehr! Wir wollen", "tokens": ["\u00bb", "und", "wir", "wol\u00b7len", "nicht", "mehr", "!", "Wir", "wol\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KON", "PPER", "VMFIN", "PTKNEG", "ADV", "$.", "PPER", "VMFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ein Ende dem teuflischen Spiel!\u00ab", "tokens": ["Ein", "En\u00b7de", "dem", "teuf\u00b7li\u00b7schen", "Spiel", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Den Herren Offizieren", "tokens": ["Den", "Her\u00b7ren", "Of\u00b7fi\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward sehr um die Haltung bang:", "tokens": ["Ward", "sehr", "um", "die", "Hal\u00b7tung", "bang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbdies R\u00f6cheln und Krepieren", "tokens": ["\u00bb", "dies", "R\u00f6\u00b7cheln", "und", "Kre\u00b7pie\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PDS", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Reizt auf und dauert lang.", "tokens": ["Reizt", "auf", "und", "dau\u00b7ert", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Marsch! Was verreckt, ins Feuer!", "tokens": ["Marsch", "!", "Was", "ver\u00b7reckt", ",", "ins", "Feu\u00b7er", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kr\u00fcppel bord\u00fcber! ins Meer!\u00ab", "tokens": ["Die", "Kr\u00fcp\u00b7pel", "bord\u00b7\u00fc\u00b7ber", "!", "ins", "Meer", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u2013 Es kriecht ein Ungeheuer", "tokens": ["\u2013", "Es", "kriecht", "ein", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00dcber die Kessel her ...", "tokens": ["\u00dc\u00b7ber", "die", "Kes\u00b7sel", "her", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Japan erbeutet die Schiffe", "tokens": ["Ja\u00b7pan", "er\u00b7beu\u00b7tet", "die", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und r\u00e4umt die Leichen vom Rost.", "tokens": ["Und", "r\u00e4umt", "die", "Lei\u00b7chen", "vom", "Rost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Haie verschlingen am Riffe", "tokens": ["Die", "Hai\u00b7e", "ver\u00b7schlin\u00b7gen", "am", "Rif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gierig die russische Kost.", "tokens": ["Gie\u00b7rig", "die", "rus\u00b7si\u00b7sche", "Kost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}