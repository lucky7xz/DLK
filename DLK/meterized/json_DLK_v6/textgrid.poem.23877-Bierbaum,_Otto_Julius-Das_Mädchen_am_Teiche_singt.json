{"textgrid.poem.23877": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen am Teiche singt", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fische, Fische feine", "tokens": ["Fi\u00b7sche", ",", "Fi\u00b7sche", "fei\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schwimmen in meinem Teiche;", "tokens": ["Schwim\u00b7men", "in", "mei\u00b7nem", "Tei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ueber dem Wasser im Sonnenscheine", "tokens": ["Ue\u00b7ber", "dem", "Was\u00b7ser", "im", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "H\u00e4ngt eine gr\u00fcne Weide.", "tokens": ["H\u00e4ngt", "ei\u00b7ne", "gr\u00fc\u00b7ne", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Leise, leise, leise", "tokens": ["Lei\u00b7se", ",", "lei\u00b7se", ",", "lei\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "R\u00fcnden sich Wellenkreise,", "tokens": ["R\u00fcn\u00b7den", "sich", "Wel\u00b7len\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wenn ein Fischlein sprang.", "tokens": ["Wenn", "ein", "Fisc\u00b7hlein", "sprang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bald ist mir lustig, bald ist mir bang.", "tokens": ["Bald", "ist", "mir", "lus\u00b7tig", ",", "bald", "ist", "mir", "bang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Ein Ringlein la\u00df ich sinken", "tokens": ["Ein", "Rin\u00b7glein", "la\u00df", "ich", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief auf den Grund.", "tokens": ["Tief", "auf", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Thut aus der Tiefe blinken", "tokens": ["Thut", "aus", "der", "Tie\u00b7fe", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ringlein rot und rund.", "tokens": ["Rin\u00b7glein", "rot", "und", "rund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wie wenn ein Fischlein sprang,", "tokens": ["Wie", "wenn", "ein", "Fisc\u00b7hlein", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gehn von dem Ring die Kreise;", "tokens": ["Gehn", "von", "dem", "Ring", "die", "Krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ach Gott, wie ist mir bang.", "tokens": ["Ach", "Gott", ",", "wie", "ist", "mir", "bang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}