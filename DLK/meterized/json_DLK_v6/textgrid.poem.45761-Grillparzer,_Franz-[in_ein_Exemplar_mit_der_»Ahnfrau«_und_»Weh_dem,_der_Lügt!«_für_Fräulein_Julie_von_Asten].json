{"textgrid.poem.45761": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in ein Exemplar mit der \u00bbAhnfrau\u00ab und \u00bbWeh dem, der L\u00fcgt!\u00ab f\u00fcr Fr\u00e4ulein Julie von Asten]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft ich gefehlt,", "tokens": ["Wie", "oft", "ich", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Es sei nicht gez\u00e4hlt;", "tokens": ["Es", "sei", "nicht", "ge\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch was ich getroffen,", "tokens": ["Doch", "was", "ich", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "L\u00e4\u00dft mich eine Zukunft hoffen.", "tokens": ["L\u00e4\u00dft", "mich", "ei\u00b7ne", "Zu\u00b7kunft", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}