{"textgrid.poem.53767": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorgestern nacht habe ich von zwei M\u00e4dchen getr\u00e4umt,", "tokens": ["Vor\u00b7ges\u00b7tern", "nacht", "ha\u00b7be", "ich", "von", "zwei", "M\u00e4d\u00b7chen", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "APPR", "CARD", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.2": {"text": "die waren furchtbar kregel und aufger\u00e4umt.", "tokens": ["die", "wa\u00b7ren", "furcht\u00b7bar", "kre\u00b7gel", "und", "auf\u00b7ge\u00b7r\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die eine hatte einen schwarzen Bubikopf und die andre einen braunen,", "tokens": ["Die", "ei\u00b7ne", "hat\u00b7te", "ei\u00b7nen", "schwar\u00b7zen", "Bu\u00b7bi\u00b7kopf", "und", "die", "and\u00b7re", "ei\u00b7nen", "brau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "ADJA", "NN", "KON", "ART", "PIS", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "und sie hatten einander so lieb, das war einfach zum Staunen.", "tokens": ["und", "sie", "hat\u00b7ten", "ein\u00b7an\u00b7der", "so", "lieb", ",", "das", "war", "ein\u00b7fach", "zum", "Stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PRF", "ADV", "ADJD", "$,", "PDS", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Sie waren leicht gekleidet \u2013 glatt zum Erk\u00e4lten,", "tokens": ["Sie", "wa\u00b7ren", "leicht", "ge\u00b7klei\u00b7det", "\u2013", "glatt", "zum", "Er\u00b7k\u00e4l\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$(", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "und sie taten einander immer Gleiches mit Gleichem vergelten.", "tokens": ["und", "sie", "ta\u00b7ten", "ein\u00b7an\u00b7der", "im\u00b7mer", "Glei\u00b7ches", "mit", "Glei\u00b7chem", "ver\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+--+-+-+--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ich erwachte. Was war das gewesen?", "tokens": ["Ich", "er\u00b7wach\u00b7te", ".", "Was", "war", "das", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "ART", "VAPP", "$."], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.8": {"text": "In meinem gro\u00dfen \u00e4gyptischen Traumbuch steht zu lesen:", "tokens": ["In", "mei\u00b7nem", "gro\u00b7\u00dfen", "\u00e4\u00b7gypt\u00b7i\u00b7schen", "Traum\u00b7buch", "steht", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "\u00bbgl\u00fcckliches Familienleben.\u00ab", "tokens": ["\u00bb", "gl\u00fcck\u00b7li\u00b7ches", "Fa\u00b7mi\u00b7li\u00b7en\u00b7le\u00b7ben", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$("], "meter": "+-+----+-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Gestern habe ich von lauter Umh\u00e4ngeb\u00e4rten getr\u00e4umt.", "tokens": ["Ge\u00b7stern", "ha\u00b7be", "ich", "von", "lau\u00b7ter", "Um\u00b7h\u00e4n\u00b7ge\u00b7b\u00e4r\u00b7ten", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die hatten alle ein Glas, mit etwas, das sch\u00e4umt.", "tokens": ["Die", "hat\u00b7ten", "al\u00b7le", "ein", "Glas", ",", "mit", "et\u00b7was", ",", "das", "sch\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "ART", "NN", "$,", "APPR", "PIS", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Darauf stand: \u00bbKochende Volksseele\u00ab \u2013 aber sie machten niemand na\u00df,", "tokens": ["Da\u00b7rauf", "stand", ":", "\u00bb", "Ko\u00b7chen\u00b7de", "Volks\u00b7see\u00b7le", "\u00ab", "\u2013", "a\u00b7ber", "sie", "mach\u00b7ten", "nie\u00b7mand", "na\u00df", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$.", "$(", "ADJA", "NN", "$(", "$(", "KON", "PPER", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "und der Sturm blieb im Wasserglas.", "tokens": ["und", "der", "Sturm", "blieb", "im", "Was\u00b7ser\u00b7glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Darauf kam ein Reichswehrgeneral mit einem Wehrpflichtprogramm;", "tokens": ["Da\u00b7rauf", "kam", "ein", "Reichs\u00b7wehr\u00b7ge\u00b7ne\u00b7ral", "mit", "ei\u00b7nem", "Wehr\u00b7pflicht\u00b7pro\u00b7gramm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "da rissen sie alle die Knochen vor ihm zusamm'.", "tokens": ["da", "ris\u00b7sen", "sie", "al\u00b7le", "die", "Kno\u00b7chen", "vor", "ihm", "zu\u00b7samm'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ich erwachte. Was war das gewesen?", "tokens": ["Ich", "er\u00b7wach\u00b7te", ".", "Was", "war", "das", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "ART", "VAPP", "$."], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.8": {"text": "In meinem gro\u00dfen \u00e4gyptischen Traumbuch ist zu lesen:", "tokens": ["In", "mei\u00b7nem", "gro\u00b7\u00dfen", "\u00e4\u00b7gypt\u00b7i\u00b7schen", "Traum\u00b7buch", "ist", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "\u00bbihnen steht eine Republik ins Haus.\u00ab", "tokens": ["\u00bb", "ih\u00b7nen", "steht", "ei\u00b7ne", "Re\u00b7pub\u00b7lik", "ins", "Haus", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Heute nacht habe ich von einem Mann getr\u00e4umt,", "tokens": ["Heu\u00b7te", "nacht", "ha\u00b7be", "ich", "von", "ei\u00b7nem", "Mann", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "der hatte sich seinen Talar schwarzwei\u00dfrot ums\u00e4umt.", "tokens": ["der", "hat\u00b7te", "sich", "sei\u00b7nen", "Ta\u00b7lar", "schwarz\u00b7wei\u00df\u00b7rot", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er rollte seine kleinen Kalm\u00fcckenaugen und hackte auf mir herum \u2013", "tokens": ["Er", "roll\u00b7te", "sei\u00b7ne", "klei\u00b7nen", "Kal\u00b7m\u00fc\u00b7cken\u00b7au\u00b7gen", "und", "hack\u00b7te", "auf", "mir", "he\u00b7rum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-++-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "ich stand hinter einer Schranke, und er redete laut und dumm.", "tokens": ["ich", "stand", "hin\u00b7ter", "ei\u00b7ner", "Schran\u00b7ke", ",", "und", "er", "re\u00b7de\u00b7te", "laut", "und", "dumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Er sagte: \u00bbWas? Sie wollen \u00fcber einen Generalfeldmarschall etwas dichten?", "tokens": ["Er", "sag\u00b7te", ":", "\u00bb", "Was", "?", "Sie", "wol\u00b7len", "\u00fc\u00b7ber", "ei\u00b7nen", "Ge\u00b7ne\u00b7ral\u00b7feld\u00b7mar\u00b7schall", "et\u00b7was", "dich\u00b7ten", "?"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWS", "$.", "PPER", "VMFIN", "APPR", "ART", "NN", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "\u00dcber diesen gro\u00dfen Mann hat nur die Geschichte zu richten!", "tokens": ["\u00dc\u00b7ber", "die\u00b7sen", "gro\u00b7\u00dfen", "Mann", "hat", "nur", "die", "Ge\u00b7schich\u00b7te", "zu", "rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Ich lasse den Saal r\u00e4umen! Ruhe! Sind Sie Kommunist? Jetzt rede ich!", "tokens": ["Ich", "las\u00b7se", "den", "Saal", "r\u00e4u\u00b7men", "!", "Ru\u00b7he", "!", "Sind", "Sie", "Kom\u00b7mu\u00b7nist", "?", "Jetzt", "re\u00b7de", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$.", "NN", "$.", "VAFIN", "PPER", "NN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+--++-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Ich nehme Sie in eine Ordnungsstrafe! Was denken Sie sich eigentlich! \u2013\u00ab", "tokens": ["Ich", "neh\u00b7me", "Sie", "in", "ei\u00b7ne", "Ord\u00b7nungs\u00b7stra\u00b7fe", "!", "Was", "den\u00b7ken", "Sie", "sich", "ei\u00b7gent\u00b7lich", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "PRF", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Und da wollte ich meine Meinung nicht l\u00e4nger verstecken.", "tokens": ["Und", "da", "woll\u00b7te", "ich", "mei\u00b7ne", "Mei\u00b7nung", "nicht", "l\u00e4n\u00b7ger", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ich sage:", "tokens": ["Ich", "sa\u00b7ge", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "\u00bbherr\u00ab, sage ich, \u00bb... ...... .... .. ...... ......!\u00ab", "tokens": ["\u00bb", "herr", "\u00ab", ",", "sa\u00b7ge", "ich", ",", "\u00bb", "...", "......", "....", "..", "......", "......", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "$(", "$(", "$.", "$(", "$(", "$(", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Aber wie das so ist in der Welten Lauf \u2013:", "tokens": ["A\u00b7ber", "wie", "das", "so", "ist", "in", "der", "Wel\u00b7ten", "Lauf", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "PDS", "ADV", "VAFIN", "APPR", "ART", "NN", "NN", "$(", "$."], "meter": "+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.13": {"text": "grade, wenns am sch\u00f6nsten wird, dann wacht man auf.", "tokens": ["gra\u00b7de", ",", "wenns", "am", "sch\u00f6ns\u00b7ten", "wird", ",", "dann", "wacht", "man", "auf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPRART", "ADJA", "VAFIN", "$,", "ADV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}