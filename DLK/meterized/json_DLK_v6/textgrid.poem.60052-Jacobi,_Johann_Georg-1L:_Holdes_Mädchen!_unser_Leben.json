{"textgrid.poem.60052": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Holdes M\u00e4dchen! unser Leben", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holdes M\u00e4dchen! unser Leben", "tokens": ["Hol\u00b7des", "M\u00e4d\u00b7chen", "!", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War ein frohes Hirtenspiel:", "tokens": ["War", "ein", "fro\u00b7hes", "Hir\u00b7ten\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00e4nze durften wir uns geben,", "tokens": ["Kr\u00e4n\u00b7ze", "durf\u00b7ten", "wir", "uns", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcsse, wenn es uns gefiel.", "tokens": ["K\u00fcs\u00b7se", ",", "wenn", "es", "uns", "ge\u00b7fiel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heerde, Stab und Fest und Freude,", "tokens": ["Heer\u00b7de", ",", "Stab", "und", "Fest", "und", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb und Kr\u00e4nze sind dahin!", "tokens": ["Lieb", "und", "Kr\u00e4n\u00b7ze", "sind", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch reden Flur und Weide", "tokens": ["Den\u00b7noch", "re\u00b7den", "Flur", "und", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir von meiner Sch\u00e4ferinn.", "tokens": ["Mir", "von", "mei\u00b7ner", "Sch\u00e4\u00b7fe\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Engel oder Liebesg\u00f6tter", "tokens": ["En\u00b7gel", "o\u00b7der", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahlen dein getreues Bild", "tokens": ["Mah\u00b7len", "dein", "ge\u00b7treu\u00b7es", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die kleinsten Rosenbl\u00e4tter:", "tokens": ["Auf", "die", "kleins\u00b7ten", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles ist von dir erf\u00fcllt.", "tokens": ["Al\u00b7les", "ist", "von", "dir", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deinen Athem haucht die Nelke,", "tokens": ["Dei\u00b7nen", "A\u00b7them", "haucht", "die", "Nel\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihr Balsamduft sich hebt:", "tokens": ["Wenn", "ihr", "Bal\u00b7sam\u00b7duft", "sich", "hebt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du erscheinst mir im Gew\u00f6lke,", "tokens": ["Du", "er\u00b7scheinst", "mir", "im", "Ge\u00b7w\u00f6l\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das am blauen Himmel schwebt.", "tokens": ["Das", "am", "blau\u00b7en", "Him\u00b7mel", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Welch ein Lispeln auf den H\u00f6hen!", "tokens": ["Welch", "ein", "Lis\u00b7peln", "auf", "den", "H\u00f6\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch ein S\u00e4useln um den Flu\u00df!", "tokens": ["Welch", "ein", "S\u00e4u\u00b7seln", "um", "den", "Flu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O ich f\u00fchl' im sanften Wehen,", "tokens": ["O", "ich", "f\u00fchl'", "im", "sanf\u00b7ten", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O ich f\u00fchle deinen Ku\u00df.", "tokens": ["O", "ich", "f\u00fch\u00b7le", "dei\u00b7nen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unter lockenden Schalmeyen,", "tokens": ["Un\u00b7ter", "lo\u00b7cken\u00b7den", "Schal\u00b7me\u00b7yen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In der Nachtigall Gesang,", "tokens": ["In", "der", "Nach\u00b7ti\u00b7gall", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Gefl\u00fcster junger Meyen", "tokens": ["Im", "Ge\u00b7fl\u00fcs\u00b7ter", "jun\u00b7ger", "Me\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "H\u00f6r' ich deiner Stimme Klang.", "tokens": ["H\u00f6r'", "ich", "dei\u00b7ner", "Stim\u00b7me", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja! du rufst mich aus der Ferne,", "tokens": ["Ja", "!", "du", "rufst", "mich", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauschest mir im finstern Hain,", "tokens": ["Rau\u00b7schest", "mir", "im", "fins\u00b7tern", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickst herab von jenem Sterne,", "tokens": ["Blickst", "her\u00b7ab", "von", "je\u00b7nem", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lachst mich an im Mondenschein;", "tokens": ["Lachst", "mich", "an", "im", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Kommst in nahenden Gewittern;", "tokens": ["Kommst", "in", "na\u00b7hen\u00b7den", "Ge\u00b7wit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es gleicht ihr banger Zug", "tokens": ["Denn", "es", "gleicht", "ihr", "ban\u00b7ger", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenem Schweigen, jenem Zittern,", "tokens": ["Je\u00b7nem", "Schwei\u00b7gen", ",", "je\u00b7nem", "Zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als mein Herz an deinem schlug.", "tokens": ["Als", "mein", "Herz", "an", "dei\u00b7nem", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}