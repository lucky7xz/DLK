{"dta.poem.19068": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XlIX.   Auf Herrn Graf Henckels Jahrs-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du ewiger Abgrund der seligen Liebe", "tokens": ["Du", "e\u00b7wi\u00b7ger", "Ab\u00b7grund", "der", "se\u00b7li\u00b7gen", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In JEsu Christo aufgethan,", "tokens": ["In", "Je\u00b7su", "Chris\u00b7to", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie brennen, wie flammen die freudigen Triebe,", "tokens": ["Wie", "bren\u00b7nen", ",", "wie", "flam\u00b7men", "die", "freu\u00b7di\u00b7gen", "Trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die kein Verstand begreiffen kan;", "tokens": ["Die", "kein", "Ver\u00b7stand", "be\u00b7greif\u00b7fen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was liebest du? S\u00fcnder, die schn\u00f6de Zucht,", "tokens": ["Was", "lie\u00b7best", "du", "?", "S\u00fcn\u00b7der", ",", "die", "schn\u00f6\u00b7de", "Zucht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wen segnest du? Kinder, die dir geflucht;", "tokens": ["Wen", "seg\u00b7nest", "du", "?", "Kin\u00b7der", ",", "die", "dir", "ge\u00b7flucht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "O grosses, ja gutes, ja freundliches Wesen!", "tokens": ["O", "gros\u00b7ses", ",", "ja", "gu\u00b7tes", ",", "ja", "freund\u00b7li\u00b7ches", "We\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADV", "ADJA", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Du hast dir was schlechtes zum Lust-Spiel erlesen.", "tokens": ["Du", "hast", "dir", "was", "schlech\u00b7tes", "zum", "Lust\u00b7Spiel", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADJA", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ist doch, HErr JEsu, deine Braut", "tokens": ["Ist", "doch", ",", "Herr", "Je\u00b7su", ",", "dei\u00b7ne", "Braut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "NN", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gantz arm", "tokens": ["gantz", "arm"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "noch hast du sie dir selbst", "tokens": ["noch", "hast", "du", "sie", "dir", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "am Creutz, mit Todes-Banden!", "tokens": ["am", "Creutz", ",", "mit", "To\u00b7des\u00b7Ban\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ist sie", "tokens": ["ist", "sie"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Fluch, Unflath, Tod", "tokens": ["Fluch", ",", "Un\u00b7flath", ",", "Tod"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "noch darfst du ihrentwegen", "tokens": ["noch", "darfst", "du", "ih\u00b7rent\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "den", "tokens": ["den"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Weils aber dein Liebes-Rath also beschlossen,", "tokens": ["Weils", "a\u00b7ber", "dein", "Lie\u00b7bes\u00b7Rath", "al\u00b7so", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der gerne freye W\u00fcrckung hat;", "tokens": ["Der", "ger\u00b7ne", "frey\u00b7e", "W\u00fcr\u00b7ckung", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So werde mit ewigem Dancke genossen", "tokens": ["So", "wer\u00b7de", "mit", "e\u00b7wi\u00b7gem", "Dan\u00b7cke", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein\u2019 jede Frucht von seiner Gnad,", "tokens": ["Ein'", "je\u00b7de", "Frucht", "von", "sei\u00b7ner", "Gnad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir geben die Seelen im Leibe hin,", "tokens": ["Wir", "ge\u00b7ben", "die", "See\u00b7len", "im", "Lei\u00b7be", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "In irrdischen H\u00f6len den Himmels-Sinn,", "tokens": ["In", "irr\u00b7di\u00b7schen", "H\u00f6\u00b7len", "den", "Him\u00b7mels\u00b7Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der ewigen, herrlichen, seligen Liebe,", "tokens": ["Der", "e\u00b7wi\u00b7gen", ",", "herr\u00b7li\u00b7chen", ",", "se\u00b7li\u00b7gen", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVIZU", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Zur Werckstatt der geistlich- und g\u00f6ttlichen Triebe.", "tokens": ["Zur", "Wercks\u00b7tatt", "der", "geist\u00b7lich", "und", "g\u00f6tt\u00b7li\u00b7chen", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Lebe dann, und lieb und labe", "tokens": ["Le\u00b7be", "dann", ",", "und", "lieb", "und", "la\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "KON", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in der neuen Crea-", "tokens": ["in", "der", "neu\u00b7en", "Crea"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lebens-F\u00fcrst, durch deine Gabe,", "tokens": ["Le\u00b7bens\u00b7F\u00fcrst", ",", "durch", "dei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die er-", "tokens": ["die", "er"], "token_info": ["word", "word"], "pos": ["ART", "TRUNC"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "erwecke dein Paradie\u00df", "tokens": ["er\u00b7we\u00b7cke", "dein", "Pa\u00b7ra\u00b7die\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "wieder", "tokens": ["wie\u00b7der"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "und bringe noch n\u00e4her", "tokens": ["und", "brin\u00b7ge", "noch", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "da du dich in allen den Gliedern", "tokens": ["da", "du", "dich", "in", "al\u00b7len", "den", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIAT", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "sie hier noch des ewigen Lebens ge-", "tokens": ["sie", "hier", "noch", "des", "e\u00b7wi\u00b7gen", "Le\u00b7bens", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Dagegen verspricht uns das pr\u00e4chtige Wesen,", "tokens": ["Da\u00b7ge\u00b7gen", "ver\u00b7spricht", "uns", "das", "pr\u00e4ch\u00b7ti\u00b7ge", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So sich als Vater kund gethan,", "tokens": ["So", "sich", "als", "Va\u00b7ter", "kund", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "KOUS", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In himmlischen Sch\u00e4tzen uns auszuerlesen,", "tokens": ["In", "himm\u00b7li\u00b7schen", "Sch\u00e4t\u00b7zen", "uns", "aus\u00b7zu\u00b7er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Was unsre Seelen zieren kan;", "tokens": ["Was", "uns\u00b7re", "See\u00b7len", "zie\u00b7ren", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcber die H\u00fctten, die bricht wie Gla\u00df,", "tokens": ["Und", "\u00fc\u00b7ber", "die", "H\u00fct\u00b7ten", ",", "die", "bricht", "wie", "Gla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Auch Segen zu sch\u00fctten mit vollem Maa\u00df,", "tokens": ["Auch", "Se\u00b7gen", "zu", "sch\u00fct\u00b7ten", "mit", "vol\u00b7lem", "Maa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKZU", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Wir sollen von unzuersch\u00f6pfenden Sch\u00e4tzen", "tokens": ["Wir", "sol\u00b7len", "von", "un\u00b7zu\u00b7er\u00b7sch\u00f6p\u00b7fen\u00b7den", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Uns selber, und neben uns andre, erg\u00f6tzen.", "tokens": ["Uns", "sel\u00b7ber", ",", "und", "ne\u00b7ben", "uns", "and\u00b7re", ",", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KON", "APPR", "PPER", "PIS", "$,", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Seht aber, wie selig wir haben erw\u00e4hlet,", "tokens": ["Seht", "a\u00b7ber", ",", "wie", "se\u00b7lig", "wir", "ha\u00b7ben", "er\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "die", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "wir", "tokens": ["wir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ach! lobet den", "tokens": ["Ach", "!", "lo\u00b7bet", "den"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "ART"], "meter": "++--", "measure": "unknown.measure.di"}, "line.5": {"text": "Singt ihm mit", "tokens": ["Singt", "ihm", "mit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "ohn Loben und", "tokens": ["ohn", "Lo\u00b7ben", "und"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "KON"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wir stehn vor dem", "tokens": ["Wir", "stehn", "vor", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Du K\u00f6nig der Herrlichkeit, unser Verlangen", "tokens": ["Du", "K\u00f6\u00b7nig", "der", "Herr\u00b7lich\u00b7keit", ",", "un\u00b7ser", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Geht nie so weit, als deine Huld;", "tokens": ["Geht", "nie", "so", "weit", ",", "als", "dei\u00b7ne", "Huld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir haben mehr Wohlthat und Segen empfangen,", "tokens": ["Wir", "ha\u00b7ben", "mehr", "Wohlt\u00b7hat", "und", "Se\u00b7gen", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als Strafe wir bey dir verschuldt.", "tokens": ["Als", "Stra\u00b7fe", "wir", "bey", "dir", "ver\u00b7schuldt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum lehr uns vertrauen dem Vater-Sinn,", "tokens": ["Drum", "lehr", "uns", "ver\u00b7trau\u00b7en", "dem", "Va\u00b7ter\u00b7Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und sehnende schauen zum Sohne hin,", "tokens": ["Und", "seh\u00b7nen\u00b7de", "schau\u00b7en", "zum", "Soh\u00b7ne", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Dein Geist unterricht uns bey fr\u00f6lichen Tagen", "tokens": ["Dein", "Geist", "un\u00b7ter\u00b7richt", "uns", "bey", "fr\u00f6\u00b7li\u00b7chen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Dir etwas Erh\u00f6rlichs vom Bruder zu sagen.", "tokens": ["Dir", "et\u00b7was", "Er\u00b7h\u00f6r\u00b7lichs", "vom", "Bru\u00b7der", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Was mich dein Geist selbst bitten lehret,", "tokens": ["Was", "mich", "dein", "Geist", "selbst", "bit\u00b7ten", "leh\u00b7ret