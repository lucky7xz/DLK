{"textgrid.poem.60025": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ein M\u00e4dchen unter seinen Schwestern,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ein M\u00e4dchen unter seinen Schwestern,", "tokens": ["Wenn", "ein", "M\u00e4d\u00b7chen", "un\u00b7ter", "sei\u00b7nen", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als die Sch\u00f6nste geht, ihr Busen sanfter schl\u00e4gt;", "tokens": ["Als", "die", "Sch\u00f6ns\u00b7te", "geht", ",", "ihr", "Bu\u00b7sen", "sanf\u00b7ter", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Wenn sie hohe Freuden in dem Blicke tr\u00e4gt,", "tokens": ["Wenn", "sie", "ho\u00b7he", "Freu\u00b7den", "in", "dem", "Bli\u00b7cke", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und die Frevler, welche Tugend l\u00e4stern,", "tokens": ["Und", "die", "Frev\u00b7ler", ",", "wel\u00b7che", "Tu\u00b7gend", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Durch ein L\u00e4cheln widerlegt;", "tokens": ["Durch", "ein", "L\u00e4\u00b7cheln", "wi\u00b7der\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ein Dichter eine Welt bekehret,", "tokens": ["Wenn", "ein", "Dich\u00b7ter", "ei\u00b7ne", "Welt", "be\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und die Menschen s\u00fc\u00dfen Frieden lehret,", "tokens": ["Und", "die", "Men\u00b7schen", "s\u00fc\u00b7\u00dfen", "Frie\u00b7den", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "S\u00fc\u00df, wie seiner Leyer Ton;", "tokens": ["S\u00fc\u00df", ",", "wie", "sei\u00b7ner", "Le\u00b7yer", "Ton", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "O dann hat dem M\u00e4dchen und dem Dichter schon,", "tokens": ["O", "dann", "hat", "dem", "M\u00e4d\u00b7chen", "und", "dem", "Dich\u00b7ter", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "In des Lebens ersten D\u00e4mmerungen,", "tokens": ["In", "des", "Le\u00b7bens", "ers\u00b7ten", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Eine Grazie gesungen;", "tokens": ["Ei\u00b7ne", "Gra\u00b7zie", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Eine Grazie, die bey der Wiege stand,", "tokens": ["Ei\u00b7ne", "Gra\u00b7zie", ",", "die", "bey", "der", "Wie\u00b7ge", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Als die zarte Seele, kaum geboren,", "tokens": ["Als", "die", "zar\u00b7te", "See\u00b7le", ",", "kaum", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Sich zu suchen schien, und noch verloren", "tokens": ["Sich", "zu", "su\u00b7chen", "schien", ",", "und", "noch", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "VVFIN", "$,", "KON", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "In dem ersten, tiefen Traume sich nicht fand.", "tokens": ["In", "dem", "ers\u00b7ten", ",", "tie\u00b7fen", "Trau\u00b7me", "sich", "nicht", "fand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Da die zarte Seele schon zu bilden,", "tokens": ["Da", "die", "zar\u00b7te", "See\u00b7le", "schon", "zu", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Schuf die G\u00f6ttin, nach und nach,", "tokens": ["Schuf", "die", "G\u00f6t\u00b7tin", ",", "nach", "und", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Wo das holde Kind im Schlummer lag,", "tokens": ["Wo", "das", "hol\u00b7de", "Kind", "im", "Schlum\u00b7mer", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Eine kleine Welt von bl\u00fchenden Gefilden.", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "Welt", "von", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7fil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Durch die Silberwolken brach,", "tokens": ["Durch", "die", "Sil\u00b7ber\u00b7wol\u00b7ken", "brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Sanft gem\u00e4\u00dfiget, der Tag;", "tokens": ["Sanft", "ge\u00b7m\u00e4\u00b7\u00dfi\u00b7get", ",", "der", "Tag", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Sch\u00f6ne Tr\u00e4ume folgten sch\u00f6nen Tr\u00e4umen;", "tokens": ["Sch\u00f6\u00b7ne", "Tr\u00e4u\u00b7me", "folg\u00b7ten", "sch\u00f6\u00b7nen", "Tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "L\u00e4mmer spielten unter Myrthenb\u00e4umen;", "tokens": ["L\u00e4m\u00b7mer", "spiel\u00b7ten", "un\u00b7ter", "Myr\u00b7then\u00b7b\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Bey den L\u00e4mmern wohnte stille Ruh:", "tokens": ["Bey", "den", "L\u00e4m\u00b7mern", "wohn\u00b7te", "stil\u00b7le", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Fernher sang ihr Lied die Grazie dazu.", "tokens": ["Fern\u00b7her", "sang", "ihr", "Lied", "die", "Gra\u00b7zie", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Dunkel zwar dem M\u00e4dchen und dem Dichter", "tokens": ["Dun\u00b7kel", "zwar", "dem", "M\u00e4d\u00b7chen", "und", "dem", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sind der Kindheit erste Traumgesichter,", "tokens": ["Sind", "der", "Kind\u00b7heit", "ers\u00b7te", "Traum\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unverst\u00e4ndlich ist das Lied f\u00fcr sie;", "tokens": ["Un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "ist", "das", "Lied", "f\u00fcr", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aber dennoch ihre Seele bilden,", "tokens": ["A\u00b7ber", "den\u00b7noch", "ih\u00b7re", "See\u00b7le", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In der kleinen Welt von bl\u00fchenden Gefilden,", "tokens": ["In", "der", "klei\u00b7nen", "Welt", "von", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Mu\u00df des Liedes Harmonie.", "tokens": ["Mu\u00df", "des", "Lie\u00b7des", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einst auf Blumen wird das M\u00e4dchen liegen,", "tokens": ["Einst", "auf", "Blu\u00b7men", "wird", "das", "M\u00e4d\u00b7chen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einst auf Blumen, wo im jungen May", "tokens": ["Einst", "auf", "Blu\u00b7men", ",", "wo", "im", "jun\u00b7gen", "May"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nachtigallen \u00fcber ihr sich wiegen;", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "\u00fc\u00b7ber", "ihr", "sich", "wie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und des Hirten Feldschalmey", "tokens": ["Und", "des", "Hir\u00b7ten", "Feld\u00b7schal\u00b7mey"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruft der Freuden be\u00dfres Chor herbey;", "tokens": ["Ruft", "der", "Freu\u00b7den", "be\u00df\u00b7res", "Chor", "her\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Jedes Bl\u00e4ttchen sagt im jungen May", "tokens": ["Je\u00b7des", "Bl\u00e4tt\u00b7chen", "sagt", "im", "jun\u00b7gen", "May"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Lispelnd ihr, wie sch\u00f6n die Unschuld sey.", "tokens": ["Lis\u00b7pelnd", "ihr", ",", "wie", "sch\u00f6n", "die", "Un\u00b7schuld", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Zwischen Hirten, welche Kr\u00e4nze winden,", "tokens": ["Zwi\u00b7schen", "Hir\u00b7ten", ",", "wel\u00b7che", "Kr\u00e4n\u00b7ze", "win\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wird der Dichter einst die Weisheit finden;", "tokens": ["Wird", "der", "Dich\u00b7ter", "einst", "die", "Weis\u00b7heit", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Voller Einfalt, so wie die Natur,", "tokens": ["Vol\u00b7ler", "Ein\u00b7falt", ",", "so", "wie", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie der Himmel, rein, und lachend, wie die Flur.", "tokens": ["Wie", "der", "Him\u00b7mel", ",", "rein", ",", "und", "la\u00b7chend", ",", "wie", "die", "Flur", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "$,", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Deines Lebens erste D\u00e4mmerungen", "tokens": ["Dei\u00b7nes", "Le\u00b7bens", "ers\u00b7te", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sahen auch, die Grazien, o Freund!", "tokens": ["Sa\u00b7hen", "auch", ",", "die", "Gra\u00b7zi\u00b7en", ",", "o", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und dir haben sie, vereint,", "tokens": ["Und", "dir", "ha\u00b7ben", "sie", ",", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Voll Unsterblichkeit ein Lied gesungen.", "tokens": ["Voll", "U\u00b7nsterb\u00b7lich\u00b7keit", "ein", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbnymphen in den Hainen, in den Fl\u00fcssen!", "tokens": ["\u00bb", "nym\u00b7phen", "in", "den", "Hai\u00b7nen", ",", "in", "den", "Fl\u00fcs\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kleine Nymphen, wachset auf mit ihm;", "tokens": ["Klei\u00b7ne", "Nym\u00b7phen", ",", "wach\u00b7set", "auf", "mit", "ihm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Spielet um ihn her, und lehrt ihn k\u00fcssen:", "tokens": ["Spie\u00b7let", "um", "ihn", "her", ",", "und", "lehrt", "ihn", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn es wird der Bosheit Ungest\u00fcm", "tokens": ["Denn", "es", "wird", "der", "Bos\u00b7heit", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sich, sobald er singt, zu seinen F\u00fc\u00dfen", "tokens": ["Sich", ",", "so\