{"dta.poem.21126": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vesuv .  \n An den Freyherrn von G \u2012 \u2012 \u2012.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich die schrecklichste Nacht mit ihren gef\u00fcrchte-\nten Fl\u00fcgeln", "tokens": ["Wenn", "sich", "die", "schreck\u00b7lichs\u00b7te", "Nacht", "mit", "ih\u00b7ren", "ge\u00b7f\u00fcrch\u00b7te", "ten", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "TRUNC", "ADJA", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ueber ein schlafendes Thal am dunklem Vesuve gebreitet:", "tokens": ["Ue\u00b7ber", "ein", "schla\u00b7fen\u00b7des", "Thal", "am", "dunk\u00b7lem", "Ve\u00b7su\u00b7ve", "ge\u00b7brei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schaudert der bangen Natur, und eherne Wolken voll", "tokens": ["Schau\u00b7dert", "der", "ban\u00b7gen", "Na\u00b7tur", ",", "und", "e\u00b7her\u00b7ne", "Wol\u00b7ken", "voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "ADJD"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "H\u00e4ngen herab auf das wartende Thal.", "tokens": ["H\u00e4n\u00b7gen", "her\u00b7ab", "auf", "das", "war\u00b7ten\u00b7de", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber auf einmal ert\u00f6nt, tief in den Gew\u00f6lben des", "tokens": ["A\u00b7ber", "auf", "ein\u00b7mal", "er\u00b7t\u00f6nt", ",", "tief", "in", "den", "Ge\u00b7w\u00f6l\u00b7ben", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "ART"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Br\u00fcllen verschlossener Gluth, und dunkles Gemurmel", "tokens": ["Br\u00fcl\u00b7len", "ver\u00b7schlos\u00b7se\u00b7ner", "Gluth", ",", "und", "dunk\u00b7les", "Ge\u00b7mur\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Pl\u00f6tzlich ergie\u00dfen sich Dampf und Gluth und fliegende", "tokens": ["Pl\u00f6tz\u00b7lich", "er\u00b7gie\u00b7\u00dfen", "sich", "Dampf", "und", "Gluth", "und", "flie\u00b7gen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "NN", "KON", "NN", "KON", "ADJA"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ueber das Thal, das mit Schrecken erwacht.", "tokens": ["Ue\u00b7ber", "das", "Thal", ",", "das", "mit", "Schre\u00b7cken", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Weinend ergreifet alsdann in voller Verzweiflung", "tokens": ["Wei\u00b7nend", "er\u00b7grei\u00b7fet", "als\u00b7dann", "in", "vol\u00b7ler", "Ver\u00b7zwei\u00b7flung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-++-+--+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Bey der erkalteten Hand sein halbohnm\u00e4chtiges M\u00e4d-", "tokens": ["Bey", "der", "er\u00b7kal\u00b7te\u00b7ten", "Hand", "sein", "hal\u00b7bohn\u00b7m\u00e4ch\u00b7ti\u00b7ges", "M\u00e4d"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "F\u00fchrt sie mit Todesangst fort von w\u00fcsten dampfenden", "tokens": ["F\u00fchrt", "sie", "mit", "To\u00b7de\u00b7sangst", "fort", "von", "w\u00fcs\u00b7ten", "damp\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "APPR", "VVFIN", "ADJA"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Welche das schreckliche Feuer verheert.", "tokens": ["Wel\u00b7che", "das", "schreck\u00b7li\u00b7che", "Feu\u00b7er", "ver\u00b7heert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Um sie fliegt Donner und Dampf und Schwefel und", "tokens": ["Um", "sie", "fliegt", "Don\u00b7ner", "und", "Dampf", "und", "Schwe\u00b7fel", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "NN", "KON", "NN", "KON", "NN", "KON"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und der erschrockene Fu\u00df f\u00fchlt schon den Abgrund er-", "tokens": ["Und", "der", "er\u00b7schro\u00b7cke\u00b7ne", "Fu\u00df", "f\u00fchlt", "schon", "den", "Ab\u00b7grund", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Beyden er\u00f6fnen vielleicht die sich entz\u00fcndenden Schl\u00fcnde", "tokens": ["Bey\u00b7den", "er\u00b7\u00f6f\u00b7nen", "viel\u00b7leicht", "die", "sich", "ent\u00b7z\u00fcn\u00b7den\u00b7den", "Schl\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "PRELS", "PRF", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Feurige Gr\u00e4ber unter dem Schritt.", "tokens": ["Feu\u00b7ri\u00b7ge", "Gr\u00e4\u00b7ber", "un\u00b7ter", "dem", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Aber durch Feuer und Dampf f\u00fchrt sie ein sch\u00fc-", "tokens": ["A\u00b7ber", "durch", "Feu\u00b7er", "und", "Dampf", "f\u00fchrt", "sie", "ein", "sch\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ehe der gl\u00fchende Flu\u00df noch seine zerschmelzenden Wel-", "tokens": ["E\u00b7he", "der", "gl\u00fc\u00b7hen\u00b7de", "Flu\u00df", "noch", "sei\u00b7ne", "zer\u00b7schmel\u00b7zen\u00b7den", "Wel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "ADJA", "TRUNC"], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ueber das rauchende Feld, gleich einem Bache der H\u00f6lle,", "tokens": ["Ue\u00b7ber", "das", "rau\u00b7chen\u00b7de", "Feld", ",", "gleich", "ei\u00b7nem", "Ba\u00b7che", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Aus den metallischen Schleusen ergie\u00dft.", "tokens": ["Aus", "den", "me\u00b7tal\u00b7li\u00b7schen", "Schleu\u00b7sen", "er\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Eine gesicherte H\u00f6h, gesichert vor Feuer und Asche,", "tokens": ["Ei\u00b7ne", "ge\u00b7si\u00b7cher\u00b7te", "H\u00f6h", ",", "ge\u00b7si\u00b7chert", "vor", "Feu\u00b7er", "und", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Th\u00fcrmet sich m\u00e4chtig vor sie; und frische balsamische", "tokens": ["Th\u00fcr\u00b7met", "sich", "m\u00e4ch\u00b7tig", "vor", "sie", ";", "und", "fri\u00b7sche", "bal\u00b7sa\u00b7mi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "PPER", "$.", "KON", "ADJA", "NN"], "meter": "+--+----+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nehmen sie freundlich auf in ihre wohlth\u00e4tigen Schat-", "tokens": ["Neh\u00b7men", "sie", "freund\u00b7lich", "auf", "in", "ih\u00b7re", "wohlt\u00b7h\u00e4\u00b7ti\u00b7gen", "Schat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Welche noch nie die Verw\u00fcstung gest\u00f6rt.", "tokens": ["Wel\u00b7che", "noch", "nie", "die", "Ver\u00b7w\u00fcs\u00b7tung", "ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Freund, wie der wilde Vesuv, wenn er die flam-", "tokens": ["Freund", ",", "wie", "der", "wil\u00b7de", "Ve\u00b7suv", ",", "wenn", "er", "die", "flam"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ueber Jtalien jagt, so donnert ietzt Ungl\u00fcck auf Ungl\u00fcck.", "tokens": ["Ue\u00b7ber", "Jta\u00b7li\u00b7en", "jagt", ",", "so", "don\u00b7nert", "ietzt", "Un\u00b7gl\u00fcck", "auf", "Un\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "K\u00f6ntest du doch aus der Noth ein zitterndes M\u00e4dchen", "tokens": ["K\u00f6n\u00b7test", "du", "doch", "aus", "der", "Noth", "ein", "zit\u00b7tern\u00b7des", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Welches das eiserne Schicksal verfolgt.", "tokens": ["Wel\u00b7ches", "das", "ei\u00b7ser\u00b7ne", "Schick\u00b7sal", "ver\u00b7folgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber ihr winket kein Wald mehr hinter verschon-", "tokens": ["A\u00b7ber", "ihr", "win\u00b7ket", "kein", "Wald", "mehr", "hin\u00b7ter", "ver\u00b7schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "ADV", "APPR", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Grimmiger br\u00fcllet um sie das dunkle schwere Gewitter.", "tokens": ["Grim\u00b7mi\u00b7ger", "br\u00fcl\u00b7let", "um", "sie", "das", "dunk\u00b7le", "schwe\u00b7re", "Ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Asche bedecket ihr Haupt, und ihren fliehenden Schritten", "tokens": ["A\u00b7sche", "be\u00b7de\u00b7cket", "ihr", "Haupt", ",", "und", "ih\u00b7ren", "flie\u00b7hen\u00b7den", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Folget die zischende flammende Fluth.", "tokens": ["Fol\u00b7get", "die", "zi\u00b7schen\u00b7de", "flam\u00b7men\u00b7de", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}