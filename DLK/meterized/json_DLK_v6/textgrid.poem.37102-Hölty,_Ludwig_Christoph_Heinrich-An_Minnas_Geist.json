{"textgrid.poem.37102": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Minnas Geist", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im leichten Tanz, mit Fl\u00fcgeln der Minute,", "tokens": ["Im", "leich\u00b7ten", "Tanz", ",", "mit", "Fl\u00fc\u00b7geln", "der", "Mi\u00b7nu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entfloh mir jeder Tag,", "tokens": ["Ent\u00b7floh", "mir", "je\u00b7der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Minna noch mit mir im Schatten ruhte,", "tokens": ["Als", "Min\u00b7na", "noch", "mit", "mir", "im", "Schat\u00b7ten", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit mir von Liebe sprach.", "tokens": ["Mit", "mir", "von", "Lie\u00b7be", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es folgeten, in lauten Harfench\u00f6ren,", "tokens": ["Es", "fol\u00b7ge\u00b7ten", ",", "in", "lau\u00b7ten", "Har\u00b7fenc\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir Engel durch den Hain,", "tokens": ["Mir", "En\u00b7gel", "durch", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6rte die Musik der Himmelsph\u00e4ren,", "tokens": ["Ich", "h\u00f6r\u00b7te", "die", "Mu\u00b7sik", "der", "Him\u00b7mel\u00b7sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sang ein Lied darein.", "tokens": ["Und", "sang", "ein", "Lied", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und f\u00fchlte das Koncert der Abendhaine,", "tokens": ["Und", "f\u00fchl\u00b7te", "das", "Kon\u00b7cert", "der", "A\u00b7bend\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ichs noch nie gef\u00fchlt,", "tokens": ["Wie", "ichs", "noch", "nie", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Minna mich, am Ufer meiner Leine,", "tokens": ["Wenn", "Min\u00b7na", "mich", ",", "am", "U\u00b7fer", "mei\u00b7ner", "Lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sanft in den Armen hielt.", "tokens": ["Sanft", "in", "den", "Ar\u00b7men", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sie starb: \u2013 Stets bleibt im Innern meiner Seele", "tokens": ["Sie", "starb", ":", "\u2013", "Stets", "bleibt", "im", "In\u00b7nern", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des M\u00e4dchens Bild zur\u00fcck! \u2013", "tokens": ["Des", "M\u00e4d\u00b7chens", "Bild", "zu\u00b7r\u00fcck", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun reizt mein Ohr kein Lied der Philomele,", "tokens": ["Nun", "reizt", "mein", "Ohr", "kein", "Lied", "der", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Bl\u00fcmchen meinen Blick.", "tokens": ["Kein", "Bl\u00fcm\u00b7chen", "mei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun irr' ich durch verschr\u00e4nkte Tannenhaine,", "tokens": ["Nun", "irr'", "ich", "durch", "ver\u00b7schr\u00e4nk\u00b7te", "Tan\u00b7nen\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sink' auf verdorrtes Moos,", "tokens": ["Sink'", "auf", "ver\u00b7dorr\u00b7tes", "Moos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und klage stets den Himmel an, und weine", "tokens": ["Und", "kla\u00b7ge", "stets", "den", "Him\u00b7mel", "an", ",", "und", "wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Leid in meinen Schoos.", "tokens": ["Mein", "Leid", "in", "mei\u00b7nen", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Stets seh ich noch die Rosen ihrer Wangen,", "tokens": ["Stets", "seh", "ich", "noch", "die", "Ro\u00b7sen", "ih\u00b7rer", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den zauberischen Gang,", "tokens": ["Den", "zau\u00b7be\u00b7ri\u00b7schen", "Gang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seh ihr Gelock', ein Spiel der L\u00fcftgen, hangen,", "tokens": ["Seh", "ihr", "Ge\u00b7lock'", ",", "ein", "Spiel", "der", "L\u00fcft\u00b7gen", ",", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "H\u00f6r' ihrer Stimme Klang.", "tokens": ["H\u00f6r'", "ih\u00b7rer", "Stim\u00b7me", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O sch\u00f6ner Geist! Durch Wiesen, durch Alleen,", "tokens": ["O", "sch\u00f6\u00b7ner", "Geist", "!", "Durch", "Wie\u00b7sen", ",", "durch", "Al\u00b7leen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seh ich dich, bald im Kranz", "tokens": ["Seh", "ich", "dich", ",", "bald", "im", "Kranz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADV", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Von Rosmarin und Tausendsch\u00f6nchen gehen,", "tokens": ["Von", "Ros\u00b7ma\u00b7rin", "und", "Tau\u00b7send\u00b7sch\u00f6n\u00b7chen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald tanzen Geistertanz.", "tokens": ["Bald", "tan\u00b7zen", "Geis\u00b7ter\u00b7tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du sitzest oft, erh\u00f6ht zum Engelrange,", "tokens": ["Du", "sit\u00b7zest", "oft", ",", "er\u00b7h\u00f6ht", "zum", "En\u00b7gel\u00b7ran\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An meines Lagers Rand,", "tokens": ["An", "mei\u00b7nes", "La\u00b7gers", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und streichelst mir die bleichgeh\u00e4rmte Wange", "tokens": ["Und", "strei\u00b7chelst", "mir", "die", "bleich\u00b7ge\u00b7h\u00e4rm\u00b7te", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit deiner wei\u00dfen Hand;", "tokens": ["Mit", "dei\u00b7ner", "wei\u00b7\u00dfen", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Enttrocknest mit dem Schleyer mir die Thr\u00e4ne,", "tokens": ["Ent\u00b7trock\u00b7nest", "mit", "dem", "Schle\u00b7yer", "mir", "die", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die meine Seele weint,", "tokens": ["Die", "mei\u00b7ne", "See\u00b7le", "weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn deines Todes trauervolle Scene", "tokens": ["Wenn", "dei\u00b7nes", "To\u00b7des", "trau\u00b7er\u00b7vol\u00b7le", "Sce\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Traume mir erscheint.", "tokens": ["Im", "Trau\u00b7me", "mir", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O warum wall ich noch im Erdenstaube?", "tokens": ["O", "wa\u00b7rum", "wall", "ich", "noch", "im", "Er\u00b7dens\u00b7tau\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O wohnt' ich schon mit dir,", "tokens": ["O", "wohnt'", "ich", "schon", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sch\u00f6ner Geist, in deiner Himmelslaube!", "tokens": ["Du", "sch\u00f6\u00b7ner", "Geist", ",", "in", "dei\u00b7ner", "Him\u00b7mels\u00b7lau\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was weil' ich l\u00e4nger hier?", "tokens": ["Was", "weil'", "ich", "l\u00e4n\u00b7ger", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}