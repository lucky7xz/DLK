{"textgrid.poem.65171": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erst waren wir nur Banden,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst waren wir nur Banden,", "tokens": ["Erst", "wa\u00b7ren", "wir", "nur", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sich zusammenfanden", "tokens": ["Die", "sich", "zu\u00b7sam\u00b7men\u00b7fan\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Wald und auf der H\u00f6h'.", "tokens": ["Im", "Wald", "und", "auf", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch \u00e4ndern sich die Lose!", "tokens": ["Doch", "\u00e4n\u00b7dern", "sich", "die", "Lo\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir sind nunmehr die gro\u00dfe", "tokens": ["Wir", "sind", "nun\u00b7mehr", "die", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Katholische Armee!", "tokens": ["Ka\u00b7tho\u00b7li\u00b7sche", "Ar\u00b7mee", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Erst fehlt' es uns an Waffen!", "tokens": ["Erst", "fehlt'", "es", "uns", "an", "Waf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch, wie die zu beschaffen,", "tokens": ["Doch", ",", "wie", "die", "zu", "be\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich keiner lang besann.", "tokens": ["Sich", "kei\u00b7ner", "lang", "be\u00b7sann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da mochte Ein's nur frommen:", "tokens": ["Da", "moch\u00b7te", "Ein's", "nur", "from\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Feind sie abgenommen", "tokens": ["Dem", "Feind", "sie", "ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Kampf Mann gegen Mann! \u2013", "tokens": ["Im", "Kampf", "Mann", "ge\u00b7gen", "Mann", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "NN", "APPR", "NN", "$.", "$("], "meter": "-++--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wenn drau\u00dfen Sch\u00fcsse knallen,", "tokens": ["Wenn", "drau\u00b7\u00dfen", "Sch\u00fcs\u00b7se", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird in den Kirchen allen", "tokens": ["Wird", "in", "den", "Kir\u00b7chen", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um Sieg f\u00fcr uns gefleht!", "tokens": ["Um", "Sieg", "f\u00fcr", "uns", "ge\u00b7fleht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es feiet uns're Leiber", "tokens": ["Es", "fei\u00b7et", "un\u00b7s'\u00b7re", "Lei\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Greise, Kinder, Weiber", "tokens": ["Der", "Grei\u00b7se", ",", "Kin\u00b7der", ",", "Wei\u00b7ber"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Inbr\u00fcnstiges Gebet!", "tokens": ["In\u00b7br\u00fcns\u00b7ti\u00b7ges", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Und F\u00fchrer, ohne gleichen", "tokens": ["Und", "F\u00fch\u00b7rer", ",", "oh\u00b7ne", "glei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "KOUI", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In aller Herren Reichen,", "tokens": ["In", "al\u00b7ler", "Her\u00b7ren", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die wurden uns zu teil!", "tokens": ["Die", "wur\u00b7den", "uns", "zu", "teil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sie voran uns schreiten,", "tokens": ["Wenn", "sie", "vo\u00b7ran", "uns", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur Lust wird dann das Streiten,", "tokens": ["Zur", "Lust", "wird", "dann", "das", "Strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Weg d\u00fcnkt uns zu steil!", "tokens": ["Kein", "Weg", "d\u00fcnkt", "uns", "zu", "steil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da steht in erster Reihe", "tokens": ["Da", "steht", "in", "ers\u00b7ter", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herr von Bonchamp! die Weihe", "tokens": ["Herr", "von", "Bon\u00b7champ", "!", "die", "Wei\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$.", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Des Ruhm's empfing er lang,", "tokens": ["Des", "Ruhm's", "emp\u00b7fing", "er", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als er, noch jung an Jahren,", "tokens": ["Als", "er", ",", "noch", "jung", "an", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit grimmig wilden Scharen", "tokens": ["Mit", "grim\u00b7mig", "wil\u00b7den", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Morgenlande rang!", "tokens": ["Im", "Mor\u00b7gen\u00b7lan\u00b7de", "rang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ungl\u00e4ub'gem Heidenvolke", "tokens": ["Un\u00b7gl\u00e4ub'\u00b7gem", "Hei\u00b7den\u00b7vol\u00b7ke"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "War er die Wetterwolke,", "tokens": ["War", "er", "die", "Wet\u00b7ter\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daraus der Blitzstrahl bricht!", "tokens": ["Da\u00b7raus", "der", "Blitz\u00b7strahl", "bricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt geht mit schlimmern Heiden,", "tokens": ["Jetzt", "geht", "mit", "schlim\u00b7mern", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die selbst vom Heil sich scheiden,", "tokens": ["Die", "selbst", "vom", "Heil", "sich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er strafend in's Gericht!", "tokens": ["Er", "stra\u00b7fend", "in's", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihm ist, der unsrem Kreise", "tokens": ["Ihm", "ist", ",", "der", "uns\u00b7rem", "Krei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entspro\u00df, der gute, weise", "tokens": ["Ent\u00b7spro\u00df", ",", "der", "gu\u00b7te", ",", "wei\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Cathelineau gesellt.", "tokens": ["Cat\u00b7he\u00b7li\u00b7ne\u00b7au", "ge\u00b7sellt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als Bauernkind geboren,", "tokens": ["Als", "Bau\u00b7ern\u00b7kind", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchrt er, von Gott erkoren,", "tokens": ["F\u00fchrt", "er", ",", "von", "Gott", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jetzt Heere in das Feld!", "tokens": ["Jetzt", "Hee\u00b7re", "in", "das", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Des Amt's wei\u00df er zu walten!", "tokens": ["Des", "Amt's", "wei\u00df", "er", "zu", "wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00df jedem Treu' zu halten,", "tokens": ["Wei\u00df", "je\u00b7dem", "Treu'", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Schutz von ihm erhofft!", "tokens": ["Der", "Schutz", "von", "ihm", "er\u00b7hofft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem Leibe deckte,", "tokens": ["Mit", "sei\u00b7nem", "Lei\u00b7be", "deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn unser Zorn sie schreckte,", "tokens": ["Wenn", "un\u00b7ser", "Zorn", "sie", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er die Gefang'nen oft! \u2013", "tokens": ["Er", "die", "Ge\u00b7fang'\u00b7nen", "oft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie Gold, erprobt im Feuer,", "tokens": ["Wie", "Gold", ",", "er\u00b7probt", "im", "Feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wert ist uns, so teuer,", "tokens": ["So", "wert", "ist", "uns", ",", "so", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lescure, der edle Graf!", "tokens": ["Le\u00b7scu\u00b7re", ",", "der", "ed\u00b7le", "Graf", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Es schmerzet keine Wunde,", "tokens": ["Es", "schmer\u00b7zet", "kei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6rt man aus seinem Munde", "tokens": ["H\u00f6rt", "man", "aus", "sei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Lob: \u00bbIhr k\u00e4mpftet brav!\u00ab", "tokens": ["Das", "Lob", ":", "\u00bb", "Ihr", "k\u00e4mpf\u00b7tet", "brav", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sein Herz ist ohne Listen,", "tokens": ["Sein", "Herz", "ist", "oh\u00b7ne", "Lis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Herz des echten Christen,", "tokens": ["Das", "Herz", "des", "ech\u00b7ten", "Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Furcht und S\u00fcnde fremd.", "tokens": ["Dem", "Furcht", "und", "S\u00fcn\u00b7de", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Scharf seines Degens Schneide!", "tokens": ["Scharf", "sei\u00b7nes", "De\u00b7gens", "Schnei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch unter seinem Kleide", "tokens": ["Doch", "un\u00b7ter", "sei\u00b7nem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tr\u00e4gt er ein h\u00e4r'nes Hemd. \u2013", "tokens": ["Tr\u00e4gt", "er", "ein", "h\u00e4r'\u00b7nes", "Hemd", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Wer aber ist's, da\u00df N\u00e4he", "tokens": ["Wer", "a\u00b7ber", "ist's", ",", "da\u00df", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Sieg verb\u00fcrgt, noch ehe", "tokens": ["Den", "Sieg", "ver\u00b7b\u00fcrgt", ",", "noch", "e\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Arm ihn uns verlieh?", "tokens": ["Sein", "Arm", "ihn", "uns", "ver\u00b7lieh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer ist's, f\u00fcr den das Leben", "tokens": ["Wer", "ist's", ",", "f\u00fcr", "den", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir alle freudig g\u00e4ben?", "tokens": ["Wir", "al\u00b7le", "freu\u00b7dig", "g\u00e4\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es ist Monsieur Henri!", "tokens": ["Es", "ist", "Mon\u00b7si\u00b7eur", "Hen\u00b7ri", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "FM", "FM", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.12": {"line.1": {"text": "Mit seinen blonden Haaren,", "tokens": ["Mit", "sei\u00b7nen", "blon\u00b7den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinen zwanzig Jahren,", "tokens": ["Mit", "sei\u00b7nen", "zwan\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Scheint er noch halb ein Kind.", "tokens": ["Scheint", "er", "noch", "halb", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch seht sein Antlitz gl\u00fchen,", "tokens": ["Doch", "seht", "sein", "Ant\u00b7litz", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Seht seine Augen spr\u00fchen,", "tokens": ["Seht", "sei\u00b7ne", "Au\u00b7gen", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn das Gefecht beginnt!", "tokens": ["Wenn", "das", "Ge\u00b7fecht", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Kein Kind, ein Mann und Ritter,", "tokens": ["Kein", "Kind", ",", "ein", "Mann", "und", "Rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schl\u00e4gt z\u00fcrnend er in Splitter", "tokens": ["Schl\u00e4gt", "z\u00fcr\u00b7nend", "er", "in", "Split\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die finstre H\u00f6llenmacht!", "tokens": ["Die", "finst\u00b7re", "H\u00f6l\u00b7len\u00b7macht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer, der im Schlachtentosen", "tokens": ["Wer", ",", "der", "im", "Schlach\u00b7ten\u00b7to\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWS", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihn sah, hat nicht des gro\u00dfen", "tokens": ["Ihn", "sah", ",", "hat", "nicht", "des", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sankt Michael gedacht?! \u2013", "tokens": ["Sankt", "Mic\u00b7hael", "ge\u00b7dacht", "?!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NE", "VVPP", "$.", "$("], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.14": {"line.1": {"text": "Der Graf Montrey, an Treue", "tokens": ["Der", "Graf", "Mon\u00b7trey", ",", "an", "Treu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und k\u00fchnem Mut ein Leue,", "tokens": ["Und", "k\u00fch\u00b7nem", "Mut", "ein", "Leu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der tapfre Herr d'Elb\u00e9e,", "tokens": ["Der", "tapf\u00b7re", "Herr", "d'\u00b7Elb\u00e9e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Starken, die Gerechten,", "tokens": ["Die", "Star\u00b7ken", ",", "die", "Ge\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sind in des Ungl\u00fccks N\u00e4chten", "tokens": ["Sind", "in", "des", "Un\u00b7gl\u00fccks", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Sterne der Vend\u00e9e!", "tokens": ["Die", "Ster\u00b7ne", "der", "Ven\u00b7d\u00e9e", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Von manchen andern Helden", "tokens": ["Von", "man\u00b7chen", "an\u00b7dern", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00fc\u00dft' ich wohl noch zu melden,", "tokens": ["W\u00fc\u00dft'", "ich", "wohl", "noch", "zu", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sei's f\u00fcr heut genug!", "tokens": ["Doch", "sei's", "f\u00fcr", "heut", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schon formen sich die Reihen,", "tokens": ["Schon", "for\u00b7men", "sich", "die", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbmacht fertig!\u00ab h\u00f6r' ich schreien, \u2013", "tokens": ["\u00bb", "macht", "fer\u00b7tig", "!", "\u00ab", "h\u00f6r'", "ich", "schrei\u00b7en", ",", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADJD", "$.", "$(", "VVFIN", "PPER", "VVFIN", "$,", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Nach Nantes geht unser Zug!", "tokens": ["Nach", "Nan\u00b7tes", "geht", "un\u00b7ser", "Zug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}