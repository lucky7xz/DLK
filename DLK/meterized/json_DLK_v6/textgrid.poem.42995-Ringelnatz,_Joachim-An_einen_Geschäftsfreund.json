{"textgrid.poem.42995": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An einen Gesch\u00e4ftsfreund", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlage nicht Freundschaften in den Wind,", "tokens": ["Schla\u00b7ge", "nicht", "Freund\u00b7schaf\u00b7ten", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die by and by ersprie\u00dflich,", "tokens": ["Die", "by", "and", "by", "er\u00b7sprie\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Au\u00dferdem aufrichtig sind!", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "auf\u00b7rich\u00b7tig", "sind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Was siegt denn schlie\u00dflich?", "tokens": ["Was", "siegt", "denn", "schlie\u00df\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Organischer Erwerb.", "tokens": ["Or\u00b7ga\u00b7ni\u00b7scher", "Er\u00b7werb", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Eilgier f\u00fchrt zum Verderb.", "tokens": ["Eil\u00b7gier", "f\u00fchrt", "zum", "Ver\u00b7derb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Jedwedes Experiment,", "tokens": ["Jed\u00b7we\u00b7des", "Ex\u00b7pe\u00b7ri\u00b7ment", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Das Werte \u00fcberspringen will", "tokens": ["Das", "Wer\u00b7te", "\u00fc\u00b7bers\u00b7prin\u00b7gen", "will"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oder Werte verkennt,", "tokens": ["O\u00b7der", "Wer\u00b7te", "ver\u00b7kennt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Entschl\u00e4ft mindestens auff\u00e4llig still.", "tokens": ["Ent\u00b7schl\u00e4ft", "min\u00b7des\u00b7tens", "auf\u00b7f\u00e4l\u00b7lig", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Boheme ist ein kurzes Bequem", "tokens": ["Bo\u00b7he\u00b7me", "ist", "ein", "kur\u00b7zes", "Be\u00b7quem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit langem Schwanz Reue.", "tokens": ["Mit", "lan\u00b7gem", "Schwanz", "Reu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer Anstand und Treue", "tokens": ["Wer", "An\u00b7stand", "und", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Aufgibt oder unterbricht,", "tokens": ["Auf\u00b7gibt", "o\u00b7der", "un\u00b7ter\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schei\u00dft sich selber ins Gesicht.", "tokens": ["Schei\u00dft", "sich", "sel\u00b7ber", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kurz, ich rufe dir herzlich zu:", "tokens": ["Kurz", ",", "ich", "ru\u00b7fe", "dir", "herz\u00b7lich", "zu", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "\u00bbdu?! \u2013 Du?!\u00ab", "tokens": ["\u00bb", "du", "?!", "\u2013", "Du", "?!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["$(", "PPER", "$.", "$(", "PPER", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}}}}