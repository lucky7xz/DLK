{"textgrid.poem.50672": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Scheinheilige", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thrax spricht, wenn ich ihn unbewacht", "tokens": ["Thrax", "spricht", ",", "wenn", "ich", "ihn", "un\u00b7be\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei einer sch\u00f6nen Thais finde,", "tokens": ["Bei", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Thais", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sein Amt hab' ihn hieher gebracht,", "tokens": ["Sein", "Amt", "hab'", "ihn", "hie\u00b7her", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um sie von ihrer schn\u00f6den S\u00fcnde", "tokens": ["Um", "sie", "von", "ih\u00b7rer", "schn\u00f6\u00b7den", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch seinen treuen Unterricht", "tokens": ["Durch", "sei\u00b7nen", "treu\u00b7en", "Un\u00b7ter\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gewissenhaftig abzuschrecken:", "tokens": ["Ge\u00b7wis\u00b7sen\u00b7haf\u00b7tig", "ab\u00b7zu\u00b7schre\u00b7cken", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er w\u00e4rmt sich an der Sonn' und spricht, \u2013", "tokens": ["Er", "w\u00e4rmt", "sich", "an", "der", "Sonn'", "und", "spricht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er schaue nur nach ihren Flecken.", "tokens": ["Er", "schau\u00b7e", "nur", "nach", "ih\u00b7ren", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}