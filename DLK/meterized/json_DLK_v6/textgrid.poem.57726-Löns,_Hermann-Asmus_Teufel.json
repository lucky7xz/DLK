{"textgrid.poem.57726": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Asmus Teufel", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Pfingstfestglocken klingen,", "tokens": ["Die", "Pfingst\u00b7fest\u00b7glo\u00b7cken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Maigr\u00fcn ist Feld und Wald,", "tokens": ["Mai\u00b7gr\u00fcn", "ist", "Feld", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der bunten Finken Singen", "tokens": ["Der", "bun\u00b7ten", "Fin\u00b7ken", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus allen Zweigen schallt;", "tokens": ["Aus", "al\u00b7len", "Zwei\u00b7gen", "schallt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O M\u00fcnden, du mu\u00dft vergehen,", "tokens": ["O", "M\u00fcn\u00b7den", ",", "du", "mu\u00dft", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "M\u00fcnden, du vielsch\u00f6ne Stadt,", "tokens": ["M\u00fcn\u00b7den", ",", "du", "viel\u00b7sch\u00f6\u00b7ne", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Des Kaisers Fahnen wehen,", "tokens": ["Des", "Kai\u00b7sers", "Fah\u00b7nen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Tilly s\u00e4uft in Blut sich satt.", "tokens": ["Der", "Til\u00b7ly", "s\u00e4uft", "in", "Blut", "sich", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es huben zw\u00f6lf Kartaunen", "tokens": ["Es", "hu\u00b7ben", "zw\u00f6lf", "Kar\u00b7tau\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl an ein schn\u00f6d Gebrumm,", "tokens": ["Wohl", "an", "ein", "schn\u00f6d", "Ge\u00b7brumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df von dem Posaunen", "tokens": ["Bis", "da\u00df", "von", "dem", "Po\u00b7sau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Mauern fielen um;", "tokens": ["Die", "Mau\u00b7ern", "fie\u00b7len", "um", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist mancher hingeworden", "tokens": ["Ist", "man\u00b7cher", "hin\u00b7ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In dieser Nacht der Not,", "tokens": ["In", "die\u00b7ser", "Nacht", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es ging das gro\u00dfe Morden", "tokens": ["Es", "ging", "das", "gro\u00b7\u00dfe", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom Abend bis zum Morgenrot.", "tokens": ["Vom", "A\u00b7bend", "bis", "zum", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da war ein M\u00fcndener B\u00fcrger,", "tokens": ["Da", "war", "ein", "M\u00fcn\u00b7de\u00b7ner", "B\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Asmus Teufel genannt,", "tokens": ["As\u00b7mus", "Teu\u00b7fel", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der schrie: \u00bbDu papistischer W\u00fcrger,", "tokens": ["Der", "schrie", ":", "\u00bb", "Du", "pa\u00b7pis\u00b7ti\u00b7scher", "W\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dir wird jetzt die Schnauze verbrannt!\u00ab", "tokens": ["Dir", "wird", "jetzt", "die", "Schnau\u00b7ze", "ver\u00b7brannt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Er stopfte mit Hackblei und N\u00e4geln", "tokens": ["Er", "stopf\u00b7te", "mit", "Hack\u00b7blei", "und", "N\u00e4\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ein gro\u00dfes Gest\u00fccke voll", "tokens": ["Ein", "gro\u00b7\u00dfes", "Ge\u00b7st\u00fc\u00b7cke", "voll"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und rief: \u00bbDen welschen Flegeln", "tokens": ["Und", "rief", ":", "\u00bb", "Den", "wel\u00b7schen", "Fle\u00b7geln"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Willkomm schlecht geraten soll.\u00ab", "tokens": ["Der", "Will\u00b7komm", "schlecht", "ge\u00b7ra\u00b7ten", "soll", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie kamen angelaufen", "tokens": ["Sie", "ka\u00b7men", "an\u00b7ge\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Mordiogeschrei,", "tokens": ["Mit", "Mor\u00b7dio\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er hielt in den dicksten Haufen,", "tokens": ["Er", "hielt", "in", "den", "dicks\u00b7ten", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es ging kein Br\u00f6cklein vorbei;", "tokens": ["Es", "ging", "kein", "Br\u00f6ck\u00b7lein", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da flogen Arme und Beine", "tokens": ["Da", "flo\u00b7gen", "Ar\u00b7me", "und", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In der Luft umher,", "tokens": ["In", "der", "Luft", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Das gab ein gro\u00dfes Gegreine,", "tokens": ["Das", "gab", "ein", "gro\u00b7\u00dfes", "Ge\u00b7grei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Doch Asmus Teufel lachte sehr.", "tokens": ["Doch", "As\u00b7mus", "Teu\u00b7fel", "lach\u00b7te", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie suchten in allen Gassen", "tokens": ["Sie", "such\u00b7ten", "in", "al\u00b7len", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo er zu finden sei,", "tokens": ["Wo", "er", "zu", "fin\u00b7den", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie mu\u00dften ihn leben lassen,", "tokens": ["Sie", "mu\u00df\u00b7ten", "ihn", "le\u00b7ben", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er kam gesund vorbei;", "tokens": ["Er", "kam", "ge\u00b7sund", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu G\u00f6ttingen sa\u00df er und zechte,", "tokens": ["Zu", "G\u00f6t\u00b7tin\u00b7gen", "sa\u00df", "er", "und", "zech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ein helles Lachen sich hub,", "tokens": ["Ein", "hel\u00b7les", "La\u00b7chen", "sich", "hub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Als kund ward, wie sich r\u00e4chte", "tokens": ["Als", "kund", "ward", ",", "wie", "sich", "r\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PTKVZ", "VAFIN", "$,", "PWAV", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Teufel an dem Beelzebub.", "tokens": ["Der", "Teu\u00b7fel", "an", "dem", "Beel\u00b7ze\u00b7bub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}