{"textgrid.poem.31266": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: auff deinem Athla\u00df-Tr\u00f6hngen/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "auff deinem Athla\u00df-Tr\u00f6hngen/", "tokens": ["auff", "dei\u00b7nem", "Ath\u00b7la\u00df\u00b7\u00b7T\u00b7r\u00f6hn\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "der Chloens Busen ist/", "tokens": ["der", "Chloens", "Bu\u00b7sen", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "versuch ich/ dich zu haschen/", "tokens": ["ver\u00b7such", "ich", "/", "dich", "zu", "ha\u00b7schen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "gleich wuttschstu durch die Maschen/", "tokens": ["gleich", "wutt\u00b7schstu", "durch", "die", "Ma\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ich wei\u00df schon/ wo du bist.", "tokens": ["ich", "wei\u00df", "schon", "/", "wo", "du", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In dihsen blauen Schleiffen", "tokens": ["In", "dih\u00b7sen", "blau\u00b7en", "Schleif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "bistu nicht mehr zu greiffen/", "tokens": ["bis\u00b7tu", "nicht", "mehr", "zu", "greif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "noch hindter dihsem Flohr;", "tokens": ["noch", "hind\u00b7ter", "dih\u00b7sem", "Flohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "du flohgst bedeutend tieffer/", "tokens": ["du", "flohgst", "be\u00b7deu\u00b7tend", "tief\u00b7fer", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "du g\u00f6ldnes Ungezieffer/", "tokens": ["du", "g\u00f6ld\u00b7nes", "Un\u00b7ge\u00b7zief\u00b7fer", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "du kleiner Matador!", "tokens": ["du", "klei\u00b7ner", "Ma\u00b7ta\u00b7dor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verstekkt in lautter Rohsen/", "tokens": ["Vers\u00b7tekkt", "in", "laut\u00b7ter", "Roh\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "auff nichts al\u00df albre Chosen", "tokens": ["auff", "nichts", "al\u00df", "alb\u00b7re", "Cho\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "sinnirstu itzt und d\u00e4nckst:", "tokens": ["sin\u00b7nirs\u00b7tu", "itzt", "und", "d\u00e4n\u00b7ckst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "So hokkt es sich geheuer", "tokens": ["So", "hokkt", "es", "sich", "ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nicht mahl im Fege-Feuer/", "tokens": ["nicht", "mahl", "im", "Fe\u00b7ge\u00b7Feu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "sih zu/ wie du mich f\u00e4ngkst!", "tokens": ["sih", "zu", "/", "wie", "du", "mich", "f\u00e4ngkst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "$(", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schab immer auff mich R\u00fcbgen/", "tokens": ["Schab", "im\u00b7mer", "auff", "mich", "R\u00fcb\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "du lohses Fl\u00fcgel-B\u00fcbgen/", "tokens": ["du", "loh\u00b7ses", "Fl\u00fc\u00b7gel\u00b7B\u00fcb\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ich zahl dir noch den Lohn!", "tokens": ["ich", "zahl", "dir", "noch", "den", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Brechen und mit Biegen/", "tokens": ["Mit", "Bre\u00b7chen", "und", "mit", "Bie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ich werde dich schon kriegen \u2013", "tokens": ["ich", "wer\u00b7de", "dich", "schon", "krie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "da/ sihstu? Hat ihm schon!", "tokens": ["da", "/", "sih\u00b7stu", "?", "Hat", "ihm", "schon", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "$.", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}