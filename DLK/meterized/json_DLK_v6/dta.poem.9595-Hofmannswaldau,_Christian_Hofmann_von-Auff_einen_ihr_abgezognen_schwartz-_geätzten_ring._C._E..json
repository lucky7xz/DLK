{"dta.poem.9595": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff einen ihr abgezognen schwartz-  \n ge\u00e4tzten ring.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schatz/ dem kein demant gleicht/ du bild der ewigkeit/", "tokens": ["Schatz", "/", "dem", "kein", "de\u00b7mant", "gleicht", "/", "du", "bild", "der", "e\u00b7wig\u00b7keit", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "PIAT", "ADJD", "VVFIN", "$(", "PPER", "NN", "ART", "NN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Das keinen anfang zeigt und auch kein ende kennet;", "tokens": ["Das", "kei\u00b7nen", "an\u00b7fang", "zeigt", "und", "auch", "kein", "en\u00b7de", "ken\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du abri\u00df des gestirns/ das um den welt-kr\u00e4y\u00df rennet/", "tokens": ["Du", "ab\u00b7ri\u00df", "des", "ge\u00b7stirns", "/", "das", "um", "den", "welt\u00b7kr\u00e4y\u00df", "ren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und uns bey stiller nacht die bleiche strahlen beut;", "tokens": ["Und", "uns", "bey", "stil\u00b7ler", "nacht", "die", "blei\u00b7che", "strah\u00b7len", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald stell ich mir durch dich und deines zirckels zier", "tokens": ["Bald", "stell", "ich", "mir", "durch", "dich", "und", "dei\u00b7nes", "zir\u00b7ckels", "zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das grosse rund der welt/ bald gar den himmel f\u00fcr;", "tokens": ["Das", "gros\u00b7se", "rund", "der", "welt", "/", "bald", "gar", "den", "him\u00b7mel", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$(", "ADV", "ADV", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald wieder auch die pracht und sch\u00f6nheit meiner sonnen/", "tokens": ["Bald", "wie\u00b7der", "auch", "die", "pracht", "und", "sch\u00f6n\u00b7heit", "mei\u00b7ner", "son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "KON", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die ihrem wesen nach ist eine kleine welt/", "tokens": ["Die", "ih\u00b7rem", "we\u00b7sen", "nach", "ist", "ei\u00b7ne", "klei\u00b7ne", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die allen \u00fcberflu\u00df von anmuth in sich h\u00e4lt/", "tokens": ["Die", "al\u00b7len", "\u00fc\u00b7berf\u00b7lu\u00df", "von", "an\u00b7muth", "in", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und durch verborgne krafft den sieg mir angewonnen.", "tokens": ["Und", "durch", "ver\u00b7borg\u00b7ne", "krafft", "den", "sieg", "mir", "an\u00b7ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Phillis liegt in dir/ und du in ihr gepr\u00e4gt/", "tokens": ["Die", "Phil\u00b7lis", "liegt", "in", "dir", "/", "und", "du", "in", "ihr", "ge\u00b7pr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPER", "$(", "KON", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ist/ wie du/ ein ring der reinsten liebligkeiten;", "tokens": ["Sie", "ist", "/", "wie", "du", "/", "ein", "ring", "der", "reins\u00b7ten", "lieb\u00b7lig\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWAV", "PPER", "$(", "ART", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr auge kan die krafft den diamanten streiten;", "tokens": ["Ihr", "au\u00b7ge", "kan", "die", "krafft", "den", "di\u00b7a\u00b7man\u00b7ten", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das haar/ das sich verwirrt in runde locken legt;", "tokens": ["Das", "haar", "/", "das", "sich", "ver\u00b7wirrt", "in", "run\u00b7de", "lo\u00b7cken", "legt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "ADJD", "APPR", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der mund/ der wie ein ring von purpur und rubien/", "tokens": ["Der", "mund", "/", "der", "wie", "ein", "ring", "von", "pur\u00b7pur", "und", "ru\u00b7bi\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "KOKOM", "ART", "VVFIN", "APPR", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Durch seine rothe glut besch\u00e4mt den corallien;", "tokens": ["Durch", "sei\u00b7ne", "ro\u00b7the", "glut", "be\u00b7sch\u00e4mt", "den", "co\u00b7ral\u00b7li\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Die brust/ die selbst umkr\u00e4ntzt mit wei\u00dfen marmol-ringen;", "tokens": ["Die", "brust", "/", "die", "selbst", "um\u00b7kr\u00e4ntzt", "mit", "wei\u00b7\u00dfen", "mar\u00b7mol\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df und ein mehrers/ das die mi\u00dfgunst uns verh\u00fcllt/", "tokens": ["Di\u00df", "und", "ein", "meh\u00b7rers", "/", "das", "die", "mi\u00df\u00b7gunst", "uns", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "ART", "PIAT", "$(", "PRELS", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Tr\u00e4gt in sich eingedr\u00fcckt dein s\u00fcsses ebenbild/", "tokens": ["Tr\u00e4gt", "in", "sich", "ein\u00b7ge\u00b7dr\u00fcckt", "dein", "s\u00fcs\u00b7ses", "e\u00b7ben\u00b7bild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und kan durch stilles feur die k\u00e4ltste hertzen zwingen.", "tokens": ["Und", "kan", "durch", "stil\u00b7les", "feur", "die", "k\u00e4lts\u00b7te", "hert\u00b7zen", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vor trug die Phillis dich an ihrer rechten hand/", "tokens": ["Vor", "trug", "die", "Phil\u00b7lis", "dich", "an", "ih\u00b7rer", "rech\u00b7ten", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NE", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum zeugni\u00df/ da\u00df sie dich in h\u00f6chsten w\u00fcrden hielte.", "tokens": ["Zum", "zeug\u00b7ni\u00df", "/", "da\u00df", "sie", "dich", "in", "h\u00f6chs\u00b7ten", "w\u00fcr\u00b7den", "hiel\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KOUS", "PPER", "PRF", "APPR", "ADJA", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach! aber/ da das gl\u00fcck so freundlich mit dir spielte/", "tokens": ["Ach", "!", "a\u00b7ber", "/", "da", "das", "gl\u00fcck", "so", "freund\u00b7lich", "mit", "dir", "spiel\u00b7te", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$(", "KOUS", "ART", "NN", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wurd\u2019stu ihr mit list/ ich weis nicht wie/ entwand.", "tokens": ["So", "wurd'\u00b7stu", "ihr", "mit", "list", "/", "ich", "weis", "nicht", "wie", "/", "ent\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "$(", "PPER", "PTKVZ", "PTKNEG", "KOKOM", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jtzt fordert Phillis dich mit ungest\u00fcm zur\u00fcck/", "tokens": ["Jtzt", "for\u00b7dert", "Phil\u00b7lis", "dich", "mit", "un\u00b7ge\u00b7st\u00fcm", "zu\u00b7r\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PRF", "APPR", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und g\u00f6nnt mir ferner nicht das l\u00e4ngst gew\u00fcnschte gl\u00fcck/", "tokens": ["Und", "g\u00f6nnt", "mir", "fer\u00b7ner", "nicht", "das", "l\u00e4ngst", "ge\u00b7w\u00fcnschte", "gl\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das ich dich soll/ wie sie/ an meinem finger tragen;", "tokens": ["Das", "ich", "dich", "soll", "/", "wie", "sie", "/", "an", "mei\u00b7nem", "fin\u00b7ger", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "VMFIN", "$(", "PWAV", "PPER", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie nimt kein reden an/ sie h\u00f6rt kein bitten nicht/", "tokens": ["Sie", "nimt", "kein", "re\u00b7den", "an", "/", "sie", "h\u00f6rt", "kein", "bit\u00b7ten", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "VVFIN", "PTKVZ", "$(", "PPER", "VVFIN", "PIAT", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du selber sehnst dich auch nach ihrem sonnen-licht/", "tokens": ["Du", "sel\u00b7ber", "sehnst", "dich", "auch", "nach", "ih\u00b7rem", "son\u00b7nen\u00b7licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und fliehst vor meiner noth/ wie sie vor meinem klagen.", "tokens": ["Und", "fliehst", "vor", "mei\u00b7ner", "noth", "/", "wie", "sie", "vor", "mei\u00b7nem", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PWAV", "PPER", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geh dann/ du leichtes bild der unbest\u00e4ndigkeit/", "tokens": ["Geh", "dann", "/", "du", "leich\u00b7tes", "bild", "der", "un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$(", "PPER", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df/ wie ein rad sich dreht/ bald auff-bald abwerts steiget/", "tokens": ["Da\u00df", "/", "wie", "ein", "rad", "sich", "dreht", "/", "bald", "auf\u00b7fbald", "ab\u00b7werts", "stei\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOKOM", "ART", "NN", "PRF", "VVFIN", "$(", "ADV", "NE", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt uns in himmel hebt/ und drauff die h\u00f6lle zeiget/", "tokens": ["Jtzt", "uns", "in", "him\u00b7mel", "hebt", "/", "und", "drauff", "die", "h\u00f6l\u00b7le", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "$(", "KON", "PAV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du spiegel meines gl\u00fccks/ das lauter ungl\u00fcck dr\u00e4ut.", "tokens": ["Du", "spie\u00b7gel", "mei\u00b7nes", "gl\u00fccks", "/", "das", "lau\u00b7ter", "un\u00b7gl\u00fcck", "dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein schwartzer anblick spricht von marter/ angst und noth/", "tokens": ["Dein", "schwart\u00b7zer", "an\u00b7blick", "spricht", "von", "mar\u00b7ter", "/", "angst", "und", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NE", "$(", "VVPP", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von finsternis und nacht/ gef\u00e4ngnis/ grab und todt;", "tokens": ["Von", "fins\u00b7ter\u00b7nis", "und", "nacht", "/", "ge\u00b7f\u00e4ng\u00b7nis", "/", "grab", "und", "todt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$(", "NE", "$(", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du stellst die fesseln vor/ die meinen geist umringen/", "tokens": ["Du", "stellst", "die", "fes\u00b7seln", "vor", "/", "die", "mei\u00b7nen", "geist", "um\u00b7rin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$(", "ART", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die banden/ die/ wie du/ ohn alles ende seyn/", "tokens": ["Die", "ban\u00b7den", "/", "die", "/", "wie", "du", "/", "ohn", "al\u00b7les", "en\u00b7de", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "$(", "PWAV", "PPER", "$(", "APPR", "PIAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die meine lust verkehrn in sorgen-volle pein/", "tokens": ["Die", "mei\u00b7ne", "lust", "ver\u00b7kehrn", "in", "sor\u00b7gen\u00b7vol\u00b7le", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und hoffnung und gedult letzt zur verzweiflung bringen.", "tokens": ["Und", "hoff\u00b7nung", "und", "ge\u00b7dult", "letzt", "zur", "ver\u00b7zwei\u00b7flung", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}