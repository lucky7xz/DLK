{"textgrid.poem.29437": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob der rechte Rechts-Verstand", "tokens": ["Ob", "der", "rech\u00b7te", "Rechts\u00b7Ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ie sey worden wem bekant,", "tokens": ["Ie", "sey", "wor\u00b7den", "wem", "be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VAPP", "PWS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist zu zweiffeln; allem meinen", "tokens": ["Ist", "zu", "zweif\u00b7feln", ";", "al\u00b7lem", "mei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PTKZU", "VVINF", "$.", "PIS", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wil stets was zu wider scheinen;", "tokens": ["Wil", "stets", "was", "zu", "wi\u00b7der", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "PTKZU", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist also, was zweiffelhafft,", "tokens": ["Ist", "al\u00b7so", ",", "was", "zweif\u00b7fel\u00b7hafft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWS", "VVFIN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Schwerlich eine Wissenschafft.", "tokens": ["Schwer\u00b7lich", "ei\u00b7ne", "Wis\u00b7sen\u00b7schafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}