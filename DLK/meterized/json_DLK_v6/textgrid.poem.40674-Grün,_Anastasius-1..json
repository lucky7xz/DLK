{"textgrid.poem.40674": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbpoet, geschmiedet an die Staatsgaleere", "tokens": ["\u00bb", "poet", ",", "ge\u00b7schmie\u00b7det", "an", "die", "Staats\u00b7ga\u00b7lee\u00b7re"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf Lebenszeit, wo bleibt dein helles Singen?", "tokens": ["Auf", "Le\u00b7bens\u00b7zeit", ",", "wo", "bleibt", "dein", "hel\u00b7les", "Sin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn m\u00fchsam nur die Ruder vorw\u00e4rts dringen,", "tokens": ["Wenn", "m\u00fch\u00b7sam", "nur", "die", "Ru\u00b7der", "vor\u00b7w\u00e4rts", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sprich, wird zur Strafe nicht dir solche Ehre?\u00ab \u2013", "tokens": ["Sprich", ",", "wird", "zur", "Stra\u00b7fe", "nicht", "dir", "sol\u00b7che", "Eh\u00b7re", "?", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "$,", "VAFIN", "APPRART", "NN", "PTKNEG", "PPER", "PIAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir ist, als ob ich einst auf Adlerschwingen", "tokens": ["Mir", "ist", ",", "als", "ob", "ich", "einst", "auf", "Ad\u00b7ler\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Nu zu Alpenh\u00f6hn geflogen w\u00e4re;", "tokens": ["Im", "Nu", "zu", "Al\u00b7pen\u00b7h\u00f6hn", "ge\u00b7flo\u00b7gen", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt mu\u00df ich, keuchend unter Lastenschwere,", "tokens": ["Jetzt", "mu\u00df", "ich", ",", "keu\u00b7chend", "un\u00b7ter", "Las\u00b7ten\u00b7schwe\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Stein die Stufen brechend, aufw\u00e4rts ringen!", "tokens": ["In", "Stein", "die", "Stu\u00b7fen", "bre\u00b7chend", ",", "auf\u00b7w\u00e4rts", "rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als Bergmann in die Tiefen einst gestiegen,", "tokens": ["Als", "Berg\u00b7mann", "in", "die", "Tie\u00b7fen", "einst", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Hausrath jetzt und Paragraphendr\u00e4hten", "tokens": ["Zu", "Haus\u00b7rath", "jetzt", "und", "Pa\u00b7ra\u00b7gra\u00b7phen\u00b7dr\u00e4h\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df des Gedankenschachtes Erz ich biegen!", "tokens": ["Mu\u00df", "des", "Ge\u00b7dan\u00b7ken\u00b7schach\u00b7tes", "Erz", "ich", "bie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein Tagwerk \u00fcb' ich treu, doch mu\u00df ich beten:", "tokens": ["Mein", "Tag\u00b7werk", "\u00fcb'", "ich", "treu", ",", "doch", "mu\u00df", "ich", "be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df jene Schwinge mir nicht ganz entsinke,", "tokens": ["Da\u00df", "je\u00b7ne", "Schwin\u00b7ge", "mir", "nicht", "ganz", "ent\u00b7sin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des alten Grubenlichts ein Strahl mir blinke!", "tokens": ["Des", "al\u00b7ten", "Gru\u00b7ben\u00b7lichts", "ein", "Strahl", "mir", "blin\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}