{"textgrid.poem.59834": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hne niemand sich den Weisen", "tokens": ["W\u00e4h\u00b7ne", "nie\u00b7mand", "sich", "den", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Genu\u00df des Weins vergleichbar;", "tokens": ["Im", "Ge\u00b7nu\u00df", "des", "Weins", "ver\u00b7gleich\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn was wir im Trunke preisen,", "tokens": ["Denn", "was", "wir", "im", "Trun\u00b7ke", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt den Toren unerreichbar!", "tokens": ["Bleibt", "den", "To\u00b7ren", "un\u00b7er\u00b7reich\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Durch den Wein zum Blumenbeet", "tokens": ["Durch", "den", "Wein", "zum", "Blu\u00b7men\u00b7beet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird die Phantasie verwandelt,", "tokens": ["Wird", "die", "Phan\u00b7ta\u00b7sie", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drin der Odem Gottes weht,", "tokens": ["Drin", "der", "O\u00b7dem", "Got\u00b7tes", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin der Geist der Sch\u00f6nheit wandelt.", "tokens": ["Drin", "der", "Geist", "der", "Sch\u00f6n\u00b7heit", "wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blumen bl\u00fchen uns zu F\u00fc\u00dfen,", "tokens": ["Blu\u00b7men", "bl\u00fc\u00b7hen", "uns", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns zu H\u00e4upten gl\u00fchen Sterne \u2013", "tokens": ["Uns", "zu", "H\u00e4up\u00b7ten", "gl\u00fc\u00b7hen", "Ster\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene aus der N\u00e4he gr\u00fc\u00dfen,", "tokens": ["Je\u00b7ne", "aus", "der", "N\u00e4\u00b7he", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese gr\u00fc\u00dfen aus der Ferne!", "tokens": ["Die\u00b7se", "gr\u00fc\u00b7\u00dfen", "aus", "der", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch ein liebliches Gewimmel!", "tokens": ["Welch", "ein", "lieb\u00b7li\u00b7ches", "Ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freude bl\u00fcht auf jedem Schritt mir \u2013", "tokens": ["Freu\u00b7de", "bl\u00fcht", "auf", "je\u00b7dem", "Schritt", "mir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "PPER", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den ganzen Sternenhimmel", "tokens": ["Und", "den", "gan\u00b7zen", "Ster\u00b7nen\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Samt den Blumen trag' ich mit mir!", "tokens": ["Samt", "den", "Blu\u00b7men", "trag'", "ich", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}