{"textgrid.poem.57715": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Prinzessin von Ahlden", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Vogel singt im hohen Holz,", "tokens": ["Ein", "Vo\u00b7gel", "singt", "im", "ho\u00b7hen", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00f6n ist doch der Mai,", "tokens": ["Wie", "sch\u00f6n", "ist", "doch", "der", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Vogel, der ist gelb und stolz,", "tokens": ["Der", "Vo\u00b7gel", ",", "der", "ist", "gelb", "und", "stolz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe, die ist frei;", "tokens": ["Die", "Lie\u00b7be", ",", "die", "ist", "frei", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die ganze Welt ist aufgebl\u00fcht,", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der gelbe Vogel singt sein Lied,", "tokens": ["Der", "gel\u00b7be", "Vo\u00b7gel", "singt", "sein", "Lied",