{"textgrid.poem.46538": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in meinen Blumengarten ist]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinen Blumengarten ist", "tokens": ["In", "mei\u00b7nen", "Blu\u00b7men\u00b7gar\u00b7ten", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Mehlthau gefallen,", "tokens": ["Ein", "Mehlt\u00b7hau", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Davon gewelkt du, Rose, bist,", "tokens": ["Da\u00b7von", "ge\u00b7welkt", "du", ",", "Ro\u00b7se", ",", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NE", "$,", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sch\u00f6nste von allen.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIAT", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Und von den Blumen welken nach,", "tokens": ["Und", "von", "den", "Blu\u00b7men", "wel\u00b7ken", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df nicht wie viele;", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "vie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie h\u00e4ngen, seit der Tod dich brach,", "tokens": ["Sie", "h\u00e4n\u00b7gen", ",", "seit", "der", "Tod", "dich", "brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An welkendem Stiele.", "tokens": ["An", "wel\u00b7ken\u00b7dem", "Stie\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}