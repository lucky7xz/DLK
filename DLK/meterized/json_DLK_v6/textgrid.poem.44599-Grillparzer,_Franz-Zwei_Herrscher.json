{"textgrid.poem.44599": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Herrscher", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorl\u00e4ngst Alexander der Gro\u00dfe,", "tokens": ["Vor\u00b7l\u00e4ngst", "A\u00b7lex\u00b7an\u00b7der", "der", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ART", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Aus Mi\u00dfbrauch so genannt,", "tokens": ["Aus", "Mi\u00df\u00b7brauch", "so", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Kam aus der Mutter Sch\u00f6\u00dfe", "tokens": ["Kam", "aus", "der", "Mut\u00b7ter", "Sch\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Herr von wenig Land.", "tokens": ["Als", "Herr", "von", "we\u00b7nig", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und da\u00df er ein Gro\u00dfer werde,", "tokens": ["Und", "da\u00df", "er", "ein", "Gro\u00b7\u00dfer", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Tat Krieg und Schlacht ihm not,", "tokens": ["Tat", "Krieg", "und", "Schlacht", "ihm", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00e4rbte mit Blut die Erde", "tokens": ["Er", "f\u00e4rb\u00b7te", "mit", "Blut", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und tauchte das Leben in Tod.", "tokens": ["Und", "tauch\u00b7te", "das", "Le\u00b7ben", "in", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Du wardst so gro\u00df geboren,", "tokens": ["Du", "wardst", "so", "gro\u00df", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df fast die B\u00fcrde zu schwer,", "tokens": ["Da\u00df", "fast", "die", "B\u00fcr\u00b7de", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dir hat Gehorsam geschworen", "tokens": ["Dir", "hat", "Ge\u00b7hor\u00b7sam", "ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ein Weltteil und wohl noch mehr.", "tokens": ["Ein", "Welt\u00b7teil", "und", "wohl", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ADV", "ADV", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Du brauchst nicht nach au\u00dfen zu schweifen,", "tokens": ["Du", "brauchst", "nicht", "nach", "au\u00b7\u00dfen", "zu", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADV", "PTKZU", "VVINF", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "F\u00fcr dich ist alles zu klein,", "tokens": ["F\u00fcr", "dich", "ist", "al\u00b7les", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "PTKA", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Um an die Wolken zu streifen,", "tokens": ["Um", "an", "die", "Wol\u00b7ken", "zu", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Brauchst du nur du selber zu sein.", "tokens": ["Brauchst", "du", "nur", "du", "sel\u00b7ber", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "PTKZU", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wie andre der Ehrgeiz qu\u00e4le,", "tokens": ["Wie", "and\u00b7re", "der", "Ehr\u00b7geiz", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du merkst seinen Stachel kaum,", "tokens": ["Du", "merkst", "sei\u00b7nen", "Sta\u00b7chel", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und fandest im Innern der Seele", "tokens": ["Und", "fan\u00b7dest", "im", "In\u00b7nern", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Allein noch zu wachsen Raum.", "tokens": ["Al\u00b7lein", "noch", "zu", "wach\u00b7sen", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wem Gott noch Sieg beschieden,", "tokens": ["Wem", "Gott", "noch", "Sieg", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist doch nur Gott bewu\u00dft,", "tokens": ["Ist", "doch", "nur", "Gott", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sch\u00f6pftest der Welt den Frieden", "tokens": ["Du", "sch\u00f6pf\u00b7test", "der", "Welt", "den", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus dem deiner eignen Brust.", "tokens": ["Aus", "dem", "dei\u00b7ner", "eig\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Von deinem Kaiserschlosse", "tokens": ["Von", "dei\u00b7nem", "Kai\u00b7ser\u00b7schlos\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "L\u00f6st sich die Taube los.", "tokens": ["L\u00f6st", "sich", "die", "Tau\u00b7be", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei nur Alexander der Gro\u00dfe!", "tokens": ["Sei", "nur", "A\u00b7lex\u00b7an\u00b7der", "der", "Gro\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "ART", "ADJA", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Denn Gro\u00dfmut auch ist gro\u00df.", "tokens": ["Denn", "Gro\u00df\u00b7mut", "auch", "ist", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}