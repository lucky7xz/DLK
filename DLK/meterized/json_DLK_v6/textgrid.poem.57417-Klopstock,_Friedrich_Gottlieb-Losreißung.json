{"textgrid.poem.57417": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Losrei\u00dfung", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weiche von mir, Gedanke des Kriegs, du belastest", "tokens": ["Wei\u00b7che", "von", "mir", ",", "Ge\u00b7dan\u00b7ke", "des", "Kriegs", ",", "du", "be\u00b7las\u00b7test"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "NN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schwer mir den Geist! du umziehst ihn, wie die Wolke,", "tokens": ["Schwer", "mir", "den", "Geist", "!", "du", "um\u00b7ziehst", "ihn", ",", "wie", "die", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die den weckenden Strahl einkerkert,", "tokens": ["Die", "den", "we\u00b7cken\u00b7den", "Strahl", "ein\u00b7ker\u00b7kert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Den uns die Fr\u00fche gebar;", "tokens": ["Den", "uns", "die", "Fr\u00fc\u00b7he", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Steckest ihn an mit Trauer, mit Gram, mit des Abscheus", "tokens": ["Ste\u00b7ckest", "ihn", "an", "mit", "Trau\u00b7er", ",", "mit", "Gram", ",", "mit", "des", "Ab\u00b7scheus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Pestigen Glut, da\u00df, verzweifelnd an der Menschheit,", "tokens": ["Pes\u00b7ti\u00b7gen", "Glut", ",", "da\u00df", ",", "ver\u00b7zwei\u00b7felnd", "an", "der", "Menschheit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Er erbebt, und ach nichts Edles", "tokens": ["Er", "er\u00b7bebt", ",", "und", "ach", "nichts", "Ed\u00b7les"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "XY", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr in den Sterblichen sieht!", "tokens": ["Mehr", "in", "den", "Sterb\u00b7li\u00b7chen", "sieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Kehre mir nie, Gedanke! zur\u00fcck, in den Stunden", "tokens": ["Keh\u00b7re", "mir", "nie", ",", "Ge\u00b7dan\u00b7ke", "!", "zu\u00b7r\u00fcck", ",", "in", "den", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$.", "PTKVZ", "$,", "APPR", "ART", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Selbst nicht zur\u00fcck, wenn am schnellsten du dich regest,", "tokens": ["Selbst", "nicht", "zu\u00b7r\u00fcck", ",", "wenn", "am", "schnells\u00b7ten", "du", "dich", "re\u00b7gest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PTKVZ", "$,", "KOUS", "APPRART", "VVFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und vom leisesten Hauch der Stimme", "tokens": ["Und", "vom", "lei\u00b7ses\u00b7ten", "Hauch", "der", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deiner Gef\u00e4hrten erwachst.", "tokens": ["Dei\u00b7ner", "Ge\u00b7f\u00e4hr\u00b7ten", "er\u00b7wachst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6ne Natur, Begeisterung sei mir dein Anschaun!", "tokens": ["Sch\u00f6\u00b7ne", "Na\u00b7tur", ",", "Be\u00b7geis\u00b7te\u00b7rung", "sei", "mir", "dein", "An\u00b7schaun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+---+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sch\u00f6nheit der Kunst, werd auch du mir zu Beseelung!", "tokens": ["Sch\u00f6n\u00b7heit", "der", "Kunst", ",", "werd", "auch", "du", "mir", "zu", "Be\u00b7see\u00b7lung", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VAFIN", "ADV", "PPER", "PPER", "APPR", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "V\u00f6lkerruhe, die war, einst wieder", "tokens": ["V\u00f6l\u00b7ker\u00b7ru\u00b7he", ",", "die", "war", ",", "einst", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "VAFIN", "$,", "ADV", "ADV"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Freuen wird, sei mir Genu\u00df!", "tokens": ["Freu\u00b7en", "wird", ",", "sei", "mir", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6ne Natur ... o bl\u00fchen vielleicht mir noch Blumen?", "tokens": ["Sch\u00f6\u00b7ne", "Na\u00b7tur", "...", "o", "bl\u00fc\u00b7hen", "viel\u00b7leicht", "mir", "noch", "Blu\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "FM", "VVFIN", "ADV", "PPER", "ADV", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ihr seid gewelkt; doch ist s\u00fc\u00df mir die Erinnrung.", "tokens": ["Ihr", "seid", "ge\u00b7welkt", ";", "doch", "ist", "s\u00fc\u00df", "mir", "die", "E\u00b7rinn\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ADV", "VAFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch des heiteren Tags Weissagung", "tokens": ["Auch", "des", "hei\u00b7te\u00b7ren", "Tags", "Weis\u00b7sa\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hellet den tr\u00fcben mir auf.", "tokens": ["Hel\u00b7let", "den", "tr\u00fc\u00b7ben", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Aber wenn ihr nun wieder mir bl\u00fcht, wenn er wirklich", "tokens": ["A\u00b7ber", "wenn", "ihr", "nun", "wie\u00b7der", "mir", "bl\u00fcht", ",", "wenn", "er", "wirk\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leuchtet, so str\u00f6mt mir Erquickung, so durchwall' er", "tokens": ["Leuch\u00b7tet", ",", "so", "str\u00f6mt", "mir", "Er\u00b7quic\u00b7kung", ",", "so", "durch\u00b7wall'", "er"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "VMFIN", "PPER"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mit Gef\u00fchl mich, das tiefre Labung", "tokens": ["Mit", "Ge\u00b7f\u00fchl", "mich", ",", "das", "tief\u00b7re", "La\u00b7bung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sei, wie der Fl\u00fcchtige kennt.", "tokens": ["Sei", ",", "wie", "der", "Fl\u00fcch\u00b7ti\u00b7ge", "kennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "H\u00f6ret! Wer t\u00f6nt vom Siege mir dort? vom Gemorde?", "tokens": ["H\u00f6\u00b7ret", "!", "Wer", "t\u00f6nt", "vom", "Sie\u00b7ge", "mir", "dort", "?", "vom", "Ge\u00b7mor\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWS", "VVFIN", "APPRART", "NN", "PPER", "ADV", "$.", "APPRART", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aber er ist, o der Unhold! schon entflohen;", "tokens": ["A\u00b7ber", "er", "ist", ",", "o", "der", "Un\u00b7hold", "!", "schon", "ent\u00b7flo\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "FM", "ART", "NN", "$.", "ADV", "VVPP", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Denn ich bannet' ihn in die \u00d6de,", "tokens": ["Denn", "ich", "ban\u00b7net'", "ihn", "in", "die", "\u00d6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Samt den Gespensten der Schlacht!", "tokens": ["Samt", "den", "Ge\u00b7spens\u00b7ten", "der", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Lebender Scherz sei unser Geno\u00df, und das sanfte", "tokens": ["Le\u00b7ben\u00b7der", "Scherz", "sei", "un\u00b7ser", "Ge\u00b7no\u00df", ",", "und", "das", "sanf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ART", "ADJA"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "L\u00e4cheln, dies geh' in dem Auge, wie der junge", "tokens": ["L\u00e4\u00b7cheln", ",", "dies", "geh'", "in", "dem", "Au\u00b7ge", ",", "wie", "der", "jun\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PDS", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Morgen auf; der Gesang erhebt; ihr", "tokens": ["Mor\u00b7gen", "auf", ";", "der", "Ge\u00b7sang", "er\u00b7hebt", ";", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$.", "PPOSAT"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Kr\u00e4nzet die Traub' im Kristall;", "tokens": ["Kr\u00e4n\u00b7zet", "die", "Traub'", "im", "Kris\u00b7tall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Weckt zu Gespr\u00e4ch, des Freude den Ernst nicht verscheuchet.", "tokens": ["Weckt", "zu", "Ge\u00b7spr\u00e4ch", ",", "des", "Freu\u00b7de", "den", "Ernst", "nicht", "ver\u00b7scheu\u00b7chet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Freundschaft und Pflicht, die nur handelt, und nicht redet,", "tokens": ["Freund\u00b7schaft", "und", "Pflicht", ",", "die", "nur", "han\u00b7delt", ",", "und", "nicht", "re\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sei von allem, was uns veredelt,", "tokens": ["Sei", "von", "al\u00b7lem", ",", "was", "uns", "ver\u00b7e\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Unser geliebteres Ziel!", "tokens": ["Un\u00b7ser", "ge\u00b7lieb\u00b7te\u00b7res", "Ziel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Forschung, die still in dem sich verliert, was schon lange", "tokens": ["For\u00b7schung", ",", "die", "still", "in", "dem", "sich", "ver\u00b7liert", ",", "was", "schon", "lan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "PRELS", "PRF", "VVFIN", "$,", "PRELS", "ADV", "ADV"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "War, und was wird, in der Sch\u00f6pfung Labyrinthe!", "tokens": ["War", ",", "und", "was", "wird", ",", "in", "der", "Sch\u00f6p\u00b7fung", "La\u00b7by\u00b7rin\u00b7the", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KON", "PWS", "VAFIN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Du bist Quelle mir auch, von der mir", "tokens": ["Du", "bist", "Quel\u00b7le", "mir", "auch", ",", "von", "der", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "PPER", "ADV", "$,", "APPR", "PRELS", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wonne der Einsamkeit rinnt.", "tokens": ["Won\u00b7ne", "der", "Ein\u00b7sam\u00b7keit", "rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "Hat sich mein Geist in der Wahrheit vertieft, die auch fern nur", "tokens": ["Hat", "sich", "mein", "Geist", "in", "der", "Wahr\u00b7heit", "ver\u00b7tieft", ",", "die", "auch", "fern", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$,", "PRELS", "ADV", "ADJD", "ADV"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Spuren mir zeigt vom Beherrscher der Erschaffnen:", "tokens": ["Spu\u00b7ren", "mir", "zeigt", "vom", "Be\u00b7herr\u00b7scher", "der", "Er\u00b7schaff\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "O so t\u00f6ne man rings vom Kriege,", "tokens": ["O", "so", "t\u00f6\u00b7ne", "man", "rings", "vom", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Kriege! ich h\u00f6re dann nicht.", "tokens": ["Krie\u00b7ge", "!", "ich", "h\u00f6\u00b7re", "dann", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}