{"textgrid.poem.50560": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "25. Cathegismus-Frage", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging an einem ", "tokens": ["Es", "ging", "an", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit seinen Geistlichen ein ", "tokens": ["Mit", "sei\u00b7nen", "Geist\u00b7li\u00b7chen", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als viele ", "tokens": ["Als", "vie\u00b7le"], "token_info": ["word", "word"], "pos": ["KOUS", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Die ungeachtet ihrer Plag',", "tokens": ["Die", "un\u00b7ge\u00b7ach\u00b7tet", "ih\u00b7rer", "Plag'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Felde woll bezecht mit mancher Kuhmagd sprungen,", "tokens": ["Im", "Fel\u00b7de", "woll", "be\u00b7zecht", "mit", "man\u00b7cher", "Kuh\u00b7magd", "sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Der Bischoff sprach: Ihr Herrn schaut diese ", "tokens": ["Der", "Bi\u00b7schoff", "sprach", ":", "Ihr", "Herrn", "schaut", "die\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PDAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wie jeder so gar leicht, ", "tokens": ["Wie", "je\u00b7der", "so", "gar", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Hergegen solte man sie um den ", "tokens": ["Her\u00b7ge\u00b7gen", "sol\u00b7te", "man", "sie", "um", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PIS", "PPER", "APPR", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "So w\u00fcste niemand, was zu sagen.", "tokens": ["So", "w\u00fcs\u00b7te", "nie\u00b7mand", ",", "was", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PRELS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Als zum Beweiss: Du grober Knoll,", "tokens": ["Als", "zum", "Be\u00b7weiss", ":", "Du", "gro\u00b7ber", "Knoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und dennoch ", "tokens": ["Und", "den\u00b7noch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}}}}