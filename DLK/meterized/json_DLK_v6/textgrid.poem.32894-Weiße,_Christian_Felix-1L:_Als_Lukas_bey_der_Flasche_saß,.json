{"textgrid.poem.32894": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Lukas bey der Flasche sa\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Lukas bey der Flasche sa\u00df,", "tokens": ["Als", "Lu\u00b7kas", "bey", "der", "Fla\u00b7sche", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da weint er laut bey jedem Gla\u00df,", "tokens": ["Da", "weint", "er", "laut", "bey", "je\u00b7dem", "Gla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das er sich eingeschenkt:", "tokens": ["Das", "er", "sich", "ein\u00b7ge\u00b7schenkt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Nachbar sah ihm lange zu,", "tokens": ["Sein", "Nach\u00b7bar", "sah", "ihm", "lan\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und rief zuletzt: was weinest du?", "tokens": ["Und", "rief", "zu\u00b7letzt", ":", "was", "wei\u00b7nest", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein, sag mir, Lukas, was dich kr\u00e4nkt?", "tokens": ["Mein", ",", "sag", "mir", ",", "Lu\u00b7kas", ",", "was", "dich", "kr\u00e4nkt", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "VVFIN", "PPER", "$,", "NE", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Die Flasche, seufzt er, kr\u00e4nket mich!", "tokens": ["Die", "Fla\u00b7sche", ",", "seufzt", "er", ",", "kr\u00e4n\u00b7ket", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trink ich einmal, wie gr\u00e4mt sie sich!", "tokens": ["Trink", "ich", "ein\u00b7mal", ",", "wie", "gr\u00e4mt", "sie", "sich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schrecklich nimmt sie ab!", "tokens": ["Wie", "schreck\u00b7lich", "nimmt", "sie", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach ja, rief Stax, itzt seh ichs ein!", "tokens": ["Ach", "ja", ",", "rief", "Stax", ",", "itzt", "seh", "ichs", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "VVFIN", "NE", "$,", "ADV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und half dem armen Lukas schreyn,", "tokens": ["Und", "half", "dem", "ar\u00b7men", "Lu\u00b7kas", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn dieser ihm zu trinken gab.", "tokens": ["Wenn", "die\u00b7ser", "ihm", "zu", "trin\u00b7ken", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}