{"textgrid.poem.46875": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nun will die Sonne so hell aufgehn]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun will die Sonne so hell aufgehn,", "tokens": ["Nun", "will", "die", "Son\u00b7ne", "so", "hell", "auf\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als sei kein Ungl\u00fcck die Nacht geschehn.", "tokens": ["Als", "sei", "kein", "Un\u00b7gl\u00fcck", "die", "Nacht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Ungl\u00fcck geschah auch mir allein,", "tokens": ["Das", "Un\u00b7gl\u00fcck", "ge\u00b7schah", "auch", "mir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sonne, sie scheinet allgemein.", "tokens": ["Die", "Son\u00b7ne", ",", "sie", "schei\u00b7net", "all\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du mu\u00dft die Nacht nicht in dir verschrenken,", "tokens": ["Du", "mu\u00dft", "die", "Nacht", "nicht", "in", "dir", "ver\u00b7schren\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mu\u00dft sie ins ewige Licht versenken.", "tokens": ["Mu\u00dft", "sie", "ins", "e\u00b7wi\u00b7ge", "Licht", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein L\u00e4mpchen verlosch in meinem Zelt,", "tokens": ["Ein", "L\u00e4mp\u00b7chen", "ver\u00b7losch", "in", "mei\u00b7nem", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heil sei dem Freudenlichte der Welt!", "tokens": ["Heil", "sei", "dem", "Freu\u00b7den\u00b7lich\u00b7te", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}