{"textgrid.poem.44858": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00fcn und Grimm", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie leicht bewegt man sich im Gro\u00dfen und im Fernen,", "tokens": ["Wie", "leicht", "be\u00b7wegt", "man", "sich", "im", "Gro\u00b7\u00dfen", "und", "im", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PIS", "PRF", "APPRART", "NN", "KON", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie schwer fa\u00dft sich, was nah und einzeln, an:", "tokens": ["Wie", "schwer", "fa\u00dft", "sich", ",", "was", "nah", "und", "ein\u00b7zeln", ",", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Statt vom Grammatiker fein still zu lernen,", "tokens": ["Statt", "vom", "Gram\u00b7ma\u00b7ti\u00b7ker", "fein", "still", "zu", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bewunderst du, halloh! den Freiheitsmann.", "tokens": ["Be\u00b7wun\u00b7derst", "du", ",", "hal\u00b7loh", "!", "den", "Frei\u00b7heits\u00b7mann", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}