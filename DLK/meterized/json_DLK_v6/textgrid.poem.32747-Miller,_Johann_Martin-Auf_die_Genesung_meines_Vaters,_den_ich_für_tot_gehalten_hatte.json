{"textgrid.poem.32747": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Genesung meines Vaters, den ich f\u00fcr tot gehalten hatte", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dr\u00e4nge dich, mein Lied, vor allen", "tokens": ["Dr\u00e4n\u00b7ge", "dich", ",", "mein", "Lied", ",", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu des Hochgelobten Thron!", "tokens": ["Zu", "des", "Hoch\u00b7ge\u00b7lob\u00b7ten", "Thron", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauter m\u00fcssest du erschallen,", "tokens": ["Lau\u00b7ter", "m\u00fcs\u00b7sest", "du", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als noch je ein Dankeston!", "tokens": ["Als", "noch", "je", "ein", "Dan\u00b7kes\u00b7ton", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der Freude frommem Beben,", "tokens": ["Mit", "der", "Freu\u00b7de", "from\u00b7mem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott! mit Thr\u00e4nen dank' ich dir;", "tokens": ["Gott", "!", "mit", "Thr\u00e4\u00b7nen", "dank'", "ich", "dir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meines Vaters zweites Leben,", "tokens": ["Mei\u00b7nes", "Va\u00b7ters", "zwei\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meinen Vater gabst du mir!", "tokens": ["Mei\u00b7nen", "Va\u00b7ter", "gabst", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! du Teurer, als du littest,", "tokens": ["Ach", "!", "du", "Teu\u00b7rer", ",", "als", "du", "lit\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freut' ich mich auf fremder Flur;", "tokens": ["Freut'", "ich", "mich", "auf", "frem\u00b7der", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als du mit dem Tode strittest,", "tokens": ["Als", "du", "mit", "dem", "To\u00b7de", "strit\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kannt' ich Fr\u00fchlingsfreuden nur;", "tokens": ["Kannt'", "ich", "Fr\u00fch\u00b7lings\u00b7freu\u00b7den", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Banger Ahndung Schauer schreckte", "tokens": ["Ban\u00b7ger", "Ahn\u00b7dung", "Schau\u00b7er", "schreck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich in ihren Armen nicht,", "tokens": ["Mich", "in", "ih\u00b7ren", "Ar\u00b7men", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und aus schweren Tr\u00e4umen weckte", "tokens": ["Und", "aus", "schwe\u00b7ren", "Tr\u00e4u\u00b7men", "weck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich kein deutendes Gesicht.", "tokens": ["Mich", "kein", "deu\u00b7ten\u00b7des", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber, Gott! wie Donner, hallten", "tokens": ["A\u00b7ber", ",", "Gott", "!", "wie", "Don\u00b7ner", ",", "hall\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "NN", "$.", "PWAV", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Todesstimmen schnell daher;", "tokens": ["To\u00b7dess\u00b7tim\u00b7men", "schnell", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleiche, sterbende Gestalten", "tokens": ["Blei\u00b7che", ",", "ster\u00b7ben\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Schwebten f\u00fcrchterlich umher.", "tokens": ["Schweb\u00b7ten", "f\u00fcrch\u00b7ter\u00b7lich", "um\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nirgends Fr\u00fchling! V\u00f6gel schweigen;", "tokens": ["Nir\u00b7gends", "Fr\u00fch\u00b7ling", "!", "V\u00f6\u00b7gel", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vater, ach, so schweigest du!", "tokens": ["Va\u00b7ter", ",", "ach", ",", "so", "schwei\u00b7gest", "du", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blumen sinken; ach, sie neigen,", "tokens": ["Blu\u00b7men", "sin\u00b7ken", ";", "ach", ",", "sie", "nei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "XY", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vater, sich dem Grabe zu!", "tokens": ["Va\u00b7ter", ",", "sich", "dem", "Gra\u00b7be", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Beten? \u2013 Gott! ich kann nicht beten,", "tokens": ["Be\u00b7ten", "?", "\u2013", "Gott", "!", "ich", "kann", "nicht", "be\u00b7ten", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rst du nicht die Seufzer an!", "tokens": ["H\u00f6rst", "du", "nicht", "die", "Seuf\u00b7zer", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach ich kann, ich kann nicht beten!", "tokens": ["Ach", "ich", "kann", ",", "ich", "kann", "nicht", "be\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er ist der beste Mann! \u2013", "tokens": ["Und", "er", "ist", "der", "bes\u00b7te", "Mann", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wochen schwinden! Immer tr\u00fcber,", "tokens": ["Wo\u00b7chen", "schwin\u00b7den", "!", "Im\u00b7mer", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer schw\u00e4rzer um mich her!", "tokens": ["Im\u00b7mer", "schw\u00e4r\u00b7zer", "um", "mich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vater! ach, es ist vor\u00fcber!", "tokens": ["Va\u00b7ter", "!", "ach", ",", "es", "ist", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "XY", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, du leidest wohl nicht mehr!", "tokens": ["Ach", ",", "du", "lei\u00b7dest", "wohl", "nicht", "mehr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fern von deinem fr\u00fchen Grabe", "tokens": ["Fern", "von", "dei\u00b7nem", "fr\u00fc\u00b7hen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wein' ich tausend Thr\u00e4nen dir!", "tokens": ["Wein'", "ich", "tau\u00b7send", "Thr\u00e4\u00b7nen", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "CARD", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vater! unsre beste Gabe,", "tokens": ["Va\u00b7ter", "!", "uns\u00b7re", "bes\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thr\u00e4nen gab der Himmel mir!", "tokens": ["Thr\u00e4\u00b7nen", "gab", "der", "Him\u00b7mel", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwester, weinest du? Vergie\u00dfe", "tokens": ["Schwes\u00b7ter", ",", "wei\u00b7nest", "du", "?", "Ver\u00b7gie\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie auf deines Vaters Grab,", "tokens": ["Sie", "auf", "dei\u00b7nes", "Va\u00b7ters", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Diese Thr\u00e4nen! ach, es flie\u00dfe", "tokens": ["Die\u00b7se", "Thr\u00e4\u00b7nen", "!", "ach", ",", "es", "flie\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$.", "XY", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine mit f\u00fcr mich herab! \u2013", "tokens": ["Ei\u00b7ne", "mit", "f\u00fcr", "mich", "her\u00b7ab", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "APPR", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott! Er lebt! Er lebt! da wallen", "tokens": ["Gott", "!", "Er", "lebt", "!", "Er", "lebt", "!", "da", "wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friedensboten her zu mir!", "tokens": ["Frie\u00b7dens\u00b7bo\u00b7ten", "her", "zu", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Laute Jubelt\u00f6ne schallen!", "tokens": ["Lau\u00b7te", "Ju\u00b7bel\u00b7t\u00f6\u00b7ne", "schal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott ich dank', ich dank' ihn dir!", "tokens": ["Gott", "ich", "dank'", ",", "ich", "dank'", "ihn", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sterben soll er nicht, soll leben!", "tokens": ["Ster\u00b7ben", "soll", "er", "nicht", ",", "soll", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leben! scholl's von dir herab.", "tokens": ["Le\u00b7ben", "!", "scholl's", "von", "dir", "her\u00b7ab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott, wie soll ich dich erheben!", "tokens": ["Gott", ",", "wie", "soll", "ich", "dich", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich, der ihn mir wiedergab!", "tokens": ["Dich", ",", "der", "ihn", "mir", "wie\u00b7der\u00b7gab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}