{"textgrid.poem.35947": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es lagen da zwei Leichen", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lagen da zwei Leichen", "tokens": ["Es", "la\u00b7gen", "da", "zwei", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Aachen auf der Bahr;", "tokens": ["Zu", "Aa\u00b7chen", "auf", "der", "Bahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl trennt' sie manches Jahr,", "tokens": ["Wohl", "trennt'", "sie", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch sind sie wunderbar", "tokens": ["Doch", "sind", "sie", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwei Leichen, die sich gleichen.", "tokens": ["Zwei", "Lei\u00b7chen", ",", "die", "sich", "glei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl sind sie zu vergleichen", "tokens": ["Wohl", "sind", "sie", "zu", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die beiden auserw\u00e4hlt \u2013", "tokens": ["Die", "bei\u00b7den", "au\u00b7ser\u00b7w\u00e4hlt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfrau, Gott verm\u00e4hlt,", "tokens": ["Die", "Jung\u00b7frau", ",", "Gott", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Greis, im Kampf gest\u00e4hlt, \u2013", "tokens": ["Der", "Greis", ",", "im", "Kampf", "ge\u00b7st\u00e4hlt", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Rose und der Eiche.", "tokens": ["Der", "Ro\u00b7se", "und", "der", "Ei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zwei Leichen und zwei Zeichen", "tokens": ["Zwei", "Lei\u00b7chen", "und", "zwei", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Zeit und unserm Land,", "tokens": ["Der", "Zeit", "und", "un\u00b7serm", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu uns vom Herrn gesandt", "tokens": ["Zu", "uns", "vom", "Herrn", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als stark' und milde Hand;", "tokens": ["Als", "stark'", "und", "mil\u00b7de", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwei Leichen sonder Gleichen!", "tokens": ["Zwei", "Lei\u00b7chen", "son\u00b7der", "Glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die gab der Herr uns eigen,", "tokens": ["Die", "gab", "der", "Herr", "uns", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu st\u00e4rken unsern Muth,", "tokens": ["Zu", "st\u00e4r\u00b7ken", "un\u00b7sern", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwei Seelen rein und gut,", "tokens": ["Zwei", "See\u00b7len", "rein", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Darauf sein Segen ruht,", "tokens": ["Da\u00b7rauf", "sein", "Se\u00b7gen", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den ihre Werke zeigen.", "tokens": ["Den", "ih\u00b7re", "Wer\u00b7ke", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwei Seelen, die sich gleichen:", "tokens": ["Zwei", "See\u00b7len", ",", "die", "sich", "glei\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Liebe war das Band,", "tokens": ["Die", "Lie\u00b7be", "war", "das", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Armen zugewandt", "tokens": ["Im", "Ar\u00b7men", "zu\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem Herren und Heiland", "tokens": ["Dem", "Her\u00b7ren", "und", "Hei\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Von beiden Segenreichen.", "tokens": ["Von", "bei\u00b7den", "Se\u00b7gen\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00dft uns auch ihnen gleichen,", "tokens": ["La\u00dft", "uns", "auch", "ih\u00b7nen", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im vollen Gottvertrau'n", "tokens": ["Im", "vol\u00b7len", "Gott\u00b7ver\u00b7trau'n"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An ihren Werken bau'n,", "tokens": ["An", "ih\u00b7ren", "Wer\u00b7ken", "bau'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bis wir sie wieder schau'n", "tokens": ["Bis", "wir", "sie", "wie\u00b7der", "schau'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Gottes ew'gem Reiche!", "tokens": ["In", "Got\u00b7tes", "ew'\u00b7gem", "Rei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}