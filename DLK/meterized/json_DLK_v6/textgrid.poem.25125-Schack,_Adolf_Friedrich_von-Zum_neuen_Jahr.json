{"textgrid.poem.25125": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Zum neuen Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Herrlichkeit, wie sie die Welt nicht sah", "tokens": ["In", "Herr\u00b7lich\u00b7keit", ",", "wie", "sie", "die", "Welt", "nicht", "sah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seit grauer Zeit des Altertumes,", "tokens": ["Seit", "grau\u00b7er", "Zeit", "des", "Al\u00b7ter\u00b7tu\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein deutsches Vaterland, stehst du nun da", "tokens": ["Mein", "deut\u00b7sches", "Va\u00b7ter\u00b7land", ",", "stehst", "du", "nun", "da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf Sonnenh\u00f6hen deines Ruhmes.", "tokens": ["Auf", "Son\u00b7nen\u00b7h\u00f6\u00b7hen", "dei\u00b7nes", "Ruh\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verderben schleudert auf den Feind und Tod", "tokens": ["Ver\u00b7der\u00b7ben", "schleu\u00b7dert", "auf", "den", "Feind", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Falten deiner m\u00e4cht'gen Stirne,", "tokens": ["Das", "Fal\u00b7ten", "dei\u00b7ner", "m\u00e4cht'\u00b7gen", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch spielt milder Glanz um sie, wie Rot", "tokens": ["Und", "doch", "spielt", "mil\u00b7der", "Glanz", "um", "sie", ",", "wie", "Rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJA", "NN", "APPR", "PPER", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Morgens um der Alpen Firne.", "tokens": ["Des", "Mor\u00b7gens", "um", "der", "Al\u00b7pen", "Fir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl! Um die Schl\u00e4fe, die der Siegesaar", "tokens": ["Wohl", "!", "Um", "die", "Schl\u00e4\u00b7fe", ",", "die", "der", "Sie\u00b7ge\u00b7saar"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "KOUI", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umkreist mit den gewalt'gen Schwingen,", "tokens": ["Um\u00b7kreist", "mit", "den", "ge\u00b7walt'\u00b7gen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Magst an des Friedens duftendem Altar", "tokens": ["Magst", "an", "des", "Frie\u00b7dens", "duf\u00b7ten\u00b7dem", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du dir der Kr\u00e4nze reichsten schlingen!", "tokens": ["Du", "dir", "der", "Kr\u00e4n\u00b7ze", "reichs\u00b7ten", "schlin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr, die als sch\u00f6nster Schatz der Menschheit gilt", "tokens": ["Ihr", ",", "die", "als", "sch\u00f6ns\u00b7ter", "Schatz", "der", "Menschheit", "gilt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "KOUS", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und sie der Geisterwelt verkettet,", "tokens": ["Und", "sie", "der", "Geis\u00b7ter\u00b7welt", "ver\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der heil'gen ", "tokens": ["Der", "heil'\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Sei H\u00fctrin, die sie sch\u00fctzt und rettet!", "tokens": ["Sei", "H\u00fct\u00b7rin", ",", "die", "sie", "sch\u00fctzt", "und", "ret\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schritt nicht die Dichtung durch den Schatten schon,", "tokens": ["Schritt", "nicht", "die", "Dich\u00b7tung", "durch", "den", "Schat\u00b7ten", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den deine Urwaldeichen warfen,", "tokens": ["Den", "dei\u00b7ne", "Ur\u00b7wal\u00b7dei\u00b7chen", "war\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und rauschten ihre Wipfel nicht beim Ton,", "tokens": ["Und", "rauschten", "ih\u00b7re", "Wip\u00b7fel", "nicht", "beim", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem ehernen, der Bardenharfen?", "tokens": ["Dem", "e\u00b7her\u00b7nen", ",", "der", "Bar\u00b7den\u00b7har\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Gedenk, wie dich von fr\u00fcher, nie versiegt,", "tokens": ["Ge\u00b7denk", ",", "wie", "dich", "von", "fr\u00fc\u00b7her", ",", "nie", "ver\u00b7siegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "APPR", "ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Melodieen Strom durchflutet,", "tokens": ["Der", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "Strom", "durch\u00b7flu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf dem Beethoven sich, der Schwan, gewiegt,", "tokens": ["Auf", "dem", "Beet\u00b7ho\u00b7ven", "sich", ",", "der", "Schwan", ",", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In dem sich Mozarts Herz verblutet!", "tokens": ["In", "dem", "sich", "Mo\u00b7zarts", "Herz", "ver\u00b7blu\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Strahlt nicht als heller Morgenstern der Kunst,", "tokens": ["Strahlt", "nicht", "als", "hel\u00b7ler", "Mor\u00b7gens\u00b7tern", "der", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOUS", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Der andern lichter Reigenf\u00fchrer,", "tokens": ["Der", "an\u00b7dern", "lich\u00b7ter", "Rei\u00b7gen\u00b7f\u00fch\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu uns aus finstrer Zeiten Nebeldunst", "tokens": ["Zu", "uns", "aus", "finst\u00b7rer", "Zei\u00b7ten", "Ne\u00b7bel\u00b7dunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Her\u00fcber der erhabne D\u00fcrer?", "tokens": ["Her\u00b7\u00fc\u00b7ber", "der", "er\u00b7hab\u00b7ne", "D\u00fc\u00b7rer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und l\u00e4nger k\u00f6nnte dich, die das besitzt,", "tokens": ["Und", "l\u00e4n\u00b7ger", "k\u00f6nn\u00b7te", "dich", ",", "die", "das", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "$,", "PRELS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beth\u00f6ren noch der Tand der Seine?", "tokens": ["Be\u00b7th\u00f6\u00b7ren", "noch", "der", "Tand", "der", "Sei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom eitlen Bildwerk, das der Franke schnitzt,", "tokens": ["Vom", "eit\u00b7len", "Bild\u00b7werk", ",", "das", "der", "Fran\u00b7ke", "schnitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auflesen m\u00f6chtest du die Sp\u00e4ne?", "tokens": ["Auf\u00b7le\u00b7sen", "m\u00f6ch\u00b7test", "du", "die", "Sp\u00e4\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nein! Aufw\u00e4rts schau, zu jener Riesenwelt,", "tokens": ["Nein", "!", "Auf\u00b7w\u00e4rts", "schau", ",", "zu", "je\u00b7ner", "Rie\u00b7sen\u00b7welt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "PTKVZ", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die sich, ein Werk der Feen und Gnomen,", "tokens": ["Die", "sich", ",", "ein", "Werk", "der", "Feen", "und", "Gno\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur durch ein ew'ges Wunder aufrecht h\u00e4lt,", "tokens": ["Nur", "durch", "ein", "ew'\u00b7ges", "Wun\u00b7der", "auf\u00b7recht", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu K\u00f6lns und Stra\u00dfburgs hohen Domen!", "tokens": ["Zu", "K\u00f6lns", "und", "Stra\u00df\u00b7burgs", "ho\u00b7hen", "Do\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So wie hochauf ihr Wald von Pfeilern steigt", "tokens": ["So", "wie", "hoch\u00b7auf", "ihr", "Wald", "von", "Pfei\u00b7lern", "steigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und mit den Aesten, Ranken, Reben", "tokens": ["Und", "mit", "den", "A\u00b7es\u00b7ten", ",", "Ran\u00b7ken", ",", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zur m\u00e4cht'gen S\u00e4ulenlaube sich verzweigt,", "tokens": ["Zur", "m\u00e4cht'\u00b7gen", "S\u00e4u\u00b7len\u00b7lau\u00b7be", "sich", "ver\u00b7zweigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll deine Kunst gen Himmel streben.", "tokens": ["Soll", "dei\u00b7ne", "Kunst", "gen", "Him\u00b7mel", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ein hoher Tempel sollst du selber sein,", "tokens": ["Ein", "ho\u00b7her", "Tem\u00b7pel", "sollst", "du", "sel\u00b7ber", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und, wenn ringsum der Sch\u00f6nheit Bl\u00fcten", "tokens": ["Und", ",", "wenn", "ring\u00b7sum", "der", "Sch\u00f6n\u00b7heit", "Bl\u00fc\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Im Sturm des Herbstes sinken, noch allein", "tokens": ["Im", "Sturm", "des", "Herbs\u00b7tes", "sin\u00b7ken", ",", "noch", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Geistes Heiligt\u00fcmer h\u00fcten.", "tokens": ["Des", "Geis\u00b7tes", "Hei\u00b7lig\u00b7t\u00fc\u00b7mer", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Und flieht an andre K\u00fcsten einst der Tag,", "tokens": ["Und", "flieht", "an", "and\u00b7re", "K\u00fcs\u00b7ten", "einst", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wechselnde, der Weltgeschichte:", "tokens": ["Der", "wech\u00b7seln\u00b7de", ",", "der", "Welt\u00b7ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergoldend lang auf deinen Zinnen mag", "tokens": ["Ver\u00b7gol\u00b7dend", "lang", "auf", "dei\u00b7nen", "Zin\u00b7nen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "ADJD", "APPR", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er ruhen noch mit letztem Lichte!", "tokens": ["Er", "ru\u00b7hen", "noch", "mit", "letz\u00b7tem", "Lich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So spielt um die Ruinen Griechenlands", "tokens": ["So", "spielt", "um", "die", "Ru\u00b7i\u00b7nen", "Grie\u00b7chen\u00b7lands"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch heut ein Abendrot, als k\u00fc\u00dfte", "tokens": ["Noch", "heut", "ein", "A\u00b7ben\u00b7drot", ",", "als", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der untergeh'nden Sonne Scheideglanz", "tokens": ["Der", "un\u00b7ter\u00b7geh'n\u00b7den", "Son\u00b7ne", "Schei\u00b7de\u00b7glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des M\u00e4oniden Marmorb\u00fcste.", "tokens": ["Des", "M\u00e4o\u00b7ni\u00b7den", "Mar\u00b7mor\u00b7b\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}