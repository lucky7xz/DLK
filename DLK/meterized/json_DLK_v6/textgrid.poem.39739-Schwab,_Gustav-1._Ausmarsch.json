{"textgrid.poem.39739": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1. Ausmarsch", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Angelegt den Sommerrock,", "tokens": ["An\u00b7ge\u00b7legt", "den", "Som\u00b7mer\u00b7rock", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf, ergriffen Hut und Stock,", "tokens": ["Auf", ",", "er\u00b7grif\u00b7fen", "Hut", "und", "Stock", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVPP", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmel steht im blausten Kleide,", "tokens": ["Him\u00b7mel", "steht", "im", "blaus\u00b7ten", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erd' in ihrer gr\u00fcnsten Seide.", "tokens": ["Erd'", "in", "ih\u00b7rer", "gr\u00fcns\u00b7ten", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ei wie lacht des Wandrers Herz", "tokens": ["Ei", "wie", "lacht", "des", "Wand\u00b7rers", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut' am letzten Tag im M\u00e4rz,", "tokens": ["Heut'", "am", "letz\u00b7ten", "Tag", "im", "M\u00e4rz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann ist wo ein Mai erschienen", "tokens": ["Wann", "ist", "wo", "ein", "Mai", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PWAV", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit so hellen, heitern Mienen?", "tokens": ["Mit", "so", "hel\u00b7len", ",", "hei\u00b7tern", "Mie\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Luft und Licht, und Farb' und Glut!", "tokens": ["Luft", "und", "Licht", ",", "und", "Fa\u00b7rb'", "und", "Glut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In den Adern schwillt das Blut,", "tokens": ["In", "den", "A\u00b7dern", "schwillt", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00dft uns ferne Reisen wagen", "tokens": ["Hei\u00dft", "uns", "fer\u00b7ne", "Rei\u00b7sen", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In so wunderbaren Tagen.", "tokens": ["In", "so", "wun\u00b7der\u00b7ba\u00b7ren", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Morgen gr\u00fc\u00dfet mich April,", "tokens": ["Mor\u00b7gen", "gr\u00fc\u00b7\u00dfet", "mich", "Ap\u00b7ril", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was doch der erst bringen will?", "tokens": ["Was", "doch", "der", "erst", "brin\u00b7gen", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ringsum tausend Knospen tr\u00e4umen,", "tokens": ["Ring\u00b7sum", "tau\u00b7send", "Knos\u00b7pen", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen bl\u00fchn sie von den B\u00e4umen!", "tokens": ["Mor\u00b7gen", "bl\u00fchn", "sie", "von", "den", "B\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}