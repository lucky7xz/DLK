{"dta.poem.1042": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Der 2. Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00f6tter zu vnserm frommen", "tokens": ["Die", "G\u00f6t\u00b7ter", "zu", "vn\u00b7serm", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wolten das jhr zarte scho\u00df/", "tokens": ["Wol\u00b7ten", "das", "jhr", "zar\u00b7te", "scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Verachtend Amors gescho\u00df/", "tokens": ["Ver\u00b7ach\u00b7tend", "A\u00b7mors", "ge\u00b7scho\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "NE", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Von Hymen wurd eingenommen/", "tokens": ["Von", "Hy\u00b7men", "wurd", "ein\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Damit sie in disem land", "tokens": ["Da\u00b7mit", "sie", "in", "di\u00b7sem", "land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhre wunder macht bekant.", "tokens": ["Ih\u00b7re", "wun\u00b7der", "macht", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wan vns dan die G\u00f6tter lieben/", "tokens": ["Wan", "vns", "dan", "die", "G\u00f6t\u00b7ter", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Solle man sich nicht enttr\u00fcben", "tokens": ["Sol\u00b7le", "man", "sich", "nicht", "ent\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PRF", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zu einer solchen ankunft?", "tokens": ["Zu", "ei\u00b7ner", "sol\u00b7chen", "an\u00b7kunft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So la\u00df vns zu jhren f\u00fcssen", "tokens": ["So", "la\u00df", "vns", "zu", "jhren", "f\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nu werfen in einer zunft/", "tokens": ["Nu", "wer\u00b7fen", "in", "ei\u00b7ner", "zunft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Vnd sie dem\u00fchtig begr\u00fcssen", "tokens": ["Vnd", "sie", "de\u00b7m\u00fch\u00b7tig", "be\u00b7gr\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Mit dem s\u00fcssen music klang/", "tokens": ["Mit", "dem", "s\u00fcs\u00b7sen", "mu\u00b7sic", "klang", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Vnd einem frewden gesang.", "tokens": ["Vnd", "ei\u00b7nem", "frew\u00b7den", "ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}