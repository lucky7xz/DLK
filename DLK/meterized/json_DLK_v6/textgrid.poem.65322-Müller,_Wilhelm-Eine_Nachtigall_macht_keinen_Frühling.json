{"textgrid.poem.65322": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Eine Nachtigall macht keinen Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' mir eine Nachtigall gezogen,", "tokens": ["Ich", "hab'", "mir", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die lie\u00df ich heut' an ihre Scheiben fliegen,", "tokens": ["Die", "lie\u00df", "ich", "heut'", "an", "ih\u00b7re", "Schei\u00b7ben", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit sie d\u00e4chte: Lenz sei vor dem Thore.", "tokens": ["Da\u00b7mit", "sie", "d\u00e4ch\u00b7te", ":", "Lenz", "sei", "vor", "dem", "Tho\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das V\u00f6glein that, so wie ich's ihm bestellte,", "tokens": ["Das", "V\u00f6\u00b7glein", "that", ",", "so", "wie", "ich's", "ihm", "be\u00b7stell\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "KOKOM", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sah sich um verwundert nach dem Lenze.", "tokens": ["Und", "sah", "sich", "um", "ver\u00b7wun\u00b7dert", "nach", "dem", "Len\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als sie mich erblickte auf der Gasse,", "tokens": ["Und", "als", "sie", "mich", "er\u00b7blick\u00b7te", "auf", "der", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da lachte sie und sprach: Mein Lieber, wisse,", "tokens": ["Da", "lach\u00b7te", "sie", "und", "sprach", ":", "Mein", "Lie\u00b7ber", ",", "wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$.", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}