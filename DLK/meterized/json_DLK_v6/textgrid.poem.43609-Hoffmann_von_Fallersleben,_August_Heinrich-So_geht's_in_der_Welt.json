{"textgrid.poem.43609": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "So geht's in der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sparb\u00fcchschen, nun ist es", "tokens": ["Spar\u00b7b\u00fcch\u00b7schen", ",", "nun", "ist", "es"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Mit dir auch vorbei:", "tokens": ["Mit", "dir", "auch", "vor\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich will dich zerschlagen,", "tokens": ["Ich", "will", "dich", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dann bist du entzwei.", "tokens": ["Dann", "bist", "du", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Und als ich das ird'ne", "tokens": ["Und", "als", "ich", "das", "ird'\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sparb\u00fcchschen zerschlug,", "tokens": ["Spar\u00b7b\u00fcch\u00b7schen", "zer\u00b7schlug", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "Da ging ich zum Kaufmann", "tokens": ["Da", "ging", "ich", "zum", "Kauf\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und kauft' einen Krug.", "tokens": ["Und", "kauft'", "ei\u00b7nen", "Krug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Hin fiel mir das Kr\u00fcglein,", "tokens": ["Hin", "fiel", "mir", "das", "Kr\u00fcg\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da war es entzwei:", "tokens": ["Da", "war", "es", "ent\u00b7zwei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nun ist's mit dem Spar'n", "tokens": ["Nun", "ist's", "mit", "dem", "Spa\u00b7r'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und dem Gelde vorbei.", "tokens": ["Und", "dem", "Gel\u00b7de", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}