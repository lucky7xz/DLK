{"textgrid.poem.66191": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Laudon", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, dem in grauer Locke", "tokens": ["Du", ",", "dem", "in", "grau\u00b7er", "Lo\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Lorbeer weht,", "tokens": ["Der", "Lor\u00b7beer", "weht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der nun auf Berbirs Walle", "tokens": ["Der", "nun", "auf", "Ber\u00b7birs", "Wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Donnerjubelhalle", "tokens": ["Im", "Don\u00b7ner\u00b7ju\u00b7bel\u00b7hal\u00b7le"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Wodan selber steht.", "tokens": ["Wie", "Wo\u00b7dan", "sel\u00b7ber", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, du Edler,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "Ed\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei mir gegr\u00fc\u00dft!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In der gefeirten Stunde,", "tokens": ["In", "der", "ge\u00b7feir\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Balsam auf die Wunde", "tokens": ["Die", "Bal\u00b7sam", "auf", "die", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des gro\u00dfen Josephs gie\u00dft.", "tokens": ["Des", "gro\u00b7\u00dfen", "Jo\u00b7se\u00b7phs", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Vor seinem Namen fliehet", "tokens": ["Vor", "sei\u00b7nem", "Na\u00b7men", "flie\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der T\u00fcrken Schwarm,", "tokens": ["Der", "T\u00fcr\u00b7ken", "Schwarm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie scheuen deine Blitze,", "tokens": ["Sie", "scheu\u00b7en", "dei\u00b7ne", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Donner der Gesch\u00fctze", "tokens": ["Den", "Don\u00b7ner", "der", "Ge\u00b7sch\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und den gehobnen Arm.", "tokens": ["Und", "den", "ge\u00b7hob\u00b7nen", "Arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ha, wie der stolze Bassa", "tokens": ["Ha", ",", "wie", "der", "stol\u00b7ze", "Bas\u00b7sa"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Aus Berbir weicht;", "tokens": ["Aus", "Ber\u00b7bir", "weicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es fliehen seine Schaaren", "tokens": ["Es", "flie\u00b7hen", "sei\u00b7ne", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie aufgeschreckte Staaren,", "tokens": ["Wie", "auf\u00b7ge\u00b7schreck\u00b7te", "Staa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn sie der Hagel scheucht.", "tokens": ["Wenn", "sie", "der", "Ha\u00b7gel", "scheucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Trag deine Schrecken weiter,", "tokens": ["Trag", "dei\u00b7ne", "Schre\u00b7cken", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du grauer Held!", "tokens": ["Du", "grau\u00b7er", "Held", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis all die stolzen Bassen", "tokens": ["Bis", "all", "die", "stol\u00b7zen", "Bas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor deinem Grimm erblassen", "tokens": ["Vor", "dei\u00b7nem", "Grimm", "er\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Flieg dann auf deinem Rosse", "tokens": ["Flieg", "dann", "auf", "dei\u00b7nem", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Kaiser zu.", "tokens": ["Dem", "Kai\u00b7ser", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O dann genest der Kranke,", "tokens": ["O", "dann", "ge\u00b7nest", "der", "Kran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und spricht mit hohem Danke:", "tokens": ["Und", "spricht", "mit", "ho\u00b7hem", "Dan\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein bester Arzt warst du!", "tokens": ["Mein", "bes\u00b7ter", "Arzt", "warst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}