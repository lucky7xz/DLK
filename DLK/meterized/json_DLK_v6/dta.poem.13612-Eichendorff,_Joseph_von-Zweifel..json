{"dta.poem.13612": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zweifel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' es jemals denn verbl\u00fchen,", "tokens": ["K\u00f6nnt'", "es", "je\u00b7mals", "denn", "ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Gl\u00e4nzen, dieses Licht,", "tokens": ["Die\u00b7ses", "Gl\u00e4n\u00b7zen", ",", "die\u00b7ses", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das durch Arbeit, Sorgen, M\u00fchen", "tokens": ["Das", "durch", "Ar\u00b7beit", ",", "Sor\u00b7gen", ",", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Tag durch Wolken bricht,", "tokens": ["Wie", "der", "Tag", "durch", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blumen, die so farbig gl\u00fchen,", "tokens": ["Blu\u00b7men", ",", "die", "so", "far\u00b7big", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um das \u00f6de Leben flicht?", "tokens": ["Um", "das", "\u00f6\u00b7de", "Le\u00b7ben", "flicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Golden sind des Himmels S\u00e4ume,", "tokens": ["Gol\u00b7den", "sind", "des", "Him\u00b7mels", "S\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abw\u00e4rts ziehen Furcht und Nacht,", "tokens": ["Ab\u00b7w\u00e4rts", "zie\u00b7hen", "Furcht", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fcstig rauschen Str\u00f6m' und B\u00e4ume", "tokens": ["R\u00fcs\u00b7tig", "rau\u00b7schen", "Str\u00f6m'", "und", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die heitre Runde lacht,", "tokens": ["Und", "die", "heit\u00b7re", "Run\u00b7de", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Ach, das sind nicht leere Tr\u00e4ume,", "tokens": ["Ach", ",", "das", "sind", "nicht", "lee\u00b7re", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was im Busen da erwacht!", "tokens": ["Was", "im", "Bu\u00b7sen", "da", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bunt verschlingen sich die G\u00e4nge,", "tokens": ["Bunt", "ver\u00b7schlin\u00b7gen", "sich", "die", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tost die Menge her und hin,", "tokens": ["Tost", "die", "Men\u00b7ge", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schallen zwischendrein Ges\u00e4nge,", "tokens": ["Schal\u00b7len", "zwi\u00b7schen\u00b7drein", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die durch's Ganze golden ziehn,", "tokens": ["Die", "durch's", "Gan\u00b7ze", "gol\u00b7den", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Still begegnet im Gedr\u00e4nge", "tokens": ["Still", "be\u00b7geg\u00b7net", "im", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir des Lebens ernster Sinn.", "tokens": ["Dir", "des", "Le\u00b7bens", "erns\u00b7ter", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und das Herz denkt sich verloren,", "tokens": ["Und", "das", "Herz", "denkt", "sich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Besser Andrer Thun und Wust,", "tokens": ["Bes\u00b7ser", "A\u00b7ndrer", "Thun", "und", "Wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlt sich wieder dann erkohren,", "tokens": ["F\u00fchlt", "sich", "wie\u00b7der", "dann", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig einsam doch die Brust.", "tokens": ["E\u00b7wig", "ein\u00b7sam", "doch", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O des Wechsels, o des Thoren,", "tokens": ["O", "des", "Wech\u00b7sels", ",", "o", "des", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "FM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O der Schmerzen, o der Lust!", "tokens": ["O", "der", "Schmer\u00b7zen", ",", "o", "der", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "FM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}