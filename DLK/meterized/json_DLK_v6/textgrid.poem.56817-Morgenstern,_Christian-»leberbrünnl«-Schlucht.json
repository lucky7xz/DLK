{"textgrid.poem.56817": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbleberbr\u00fcnnl\u00ab-Schlucht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeden Abend, den ich kehre", "tokens": ["Je\u00b7den", "A\u00b7bend", ",", "den", "ich", "keh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aus der T\u00e4ler weitem Lauf,", "tokens": ["aus", "der", "T\u00e4\u00b7ler", "wei\u00b7tem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "geht mein Herz in Dank und Ehre", "tokens": ["geht", "mein", "Herz", "in", "Dank", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "deiner stillen Sch\u00f6nheit auf.", "tokens": ["dei\u00b7ner", "stil\u00b7len", "Sch\u00f6n\u00b7heit", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freundin Phanta hat unzweiflich", "tokens": ["Freun\u00b7din", "Phan\u00b7ta", "hat", "un\u00b7zwei\u00b7flich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "VAFIN", "ADJD"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "mich hier oben schn\u00f6d verlassen,", "tokens": ["mich", "hier", "o\u00b7ben", "schn\u00f6d", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "doch Faulenzen, Schlafen, Prassen", "tokens": ["doch", "Fau\u00b7len\u00b7zen", ",", "Schla\u00b7fen", ",", "Pras\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "macht es unliebsam begreiflich.", "tokens": ["macht", "es", "un\u00b7lieb\u00b7sam", "be\u00b7greif\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}