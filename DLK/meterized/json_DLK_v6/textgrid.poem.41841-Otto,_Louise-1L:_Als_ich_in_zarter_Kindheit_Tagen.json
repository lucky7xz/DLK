{"textgrid.poem.41841": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich in zarter Kindheit Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich in zarter Kindheit Tagen", "tokens": ["Als", "ich", "in", "zar\u00b7ter", "Kind\u00b7heit", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versteckt mein erstes Lied gesungen,", "tokens": ["Ver\u00b7steckt", "mein", "ers\u00b7tes", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit tiefem Augenniederschlagen", "tokens": ["Mit", "tie\u00b7fem", "Au\u00b7gen\u00b7nie\u00b7der\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wort vernahm: es sei gelungen!", "tokens": ["Das", "Wort", "ver\u00b7nahm", ":", "es", "sei", "ge\u00b7lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da weint ich, da\u00df man es erlauscht,", "tokens": ["Da", "weint", "ich", ",", "da\u00df", "man", "es", "er\u00b7lauscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und war doch stolz und gl\u00fcckberauscht.", "tokens": ["Und", "war", "doch", "stolz", "und", "gl\u00fcck\u00b7be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht Lob noch Ruhm mocht' ich begehren,", "tokens": ["Nicht", "Lob", "noch", "Ruhm", "mocht'", "ich", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor niemand wollt ich eitel gl\u00e4nzen,", "tokens": ["Vor", "nie\u00b7mand", "wollt", "ich", "ei\u00b7tel", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dachte nie an \u00e4u\u00dfre Ehren,", "tokens": ["Ich", "dach\u00b7te", "nie", "an", "\u00e4u\u00df\u00b7re", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich tr\u00e4umte nie von Lorberkr\u00e4nzen;", "tokens": ["Ich", "tr\u00e4um\u00b7te", "nie", "von", "Lor\u00b7ber\u00b7kr\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Noch h\u00f6her Ziel mein Herz mir riet:", "tokens": ["Noch", "h\u00f6\u00b7her", "Ziel", "mein", "Herz", "mir", "riet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Lied, vor Gottes Thron gesungen", "tokens": ["Ein", "Lied", ",", "vor", "Got\u00b7tes", "Thron", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im h\u00f6h'ren Chor, in Himmelsn\u00e4he,", "tokens": ["Im", "h\u00f6h'\u00b7ren", "Chor", ",", "in", "Him\u00b7mels\u00b7n\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So von Begeisterung durchdrungen,", "tokens": ["So", "von", "Be\u00b7geis\u00b7te\u00b7rung", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df nur Begeistertes gesch\u00e4he,", "tokens": ["Da\u00df", "nur", "Be\u00b7geis\u00b7ter\u00b7tes", "ge\u00b7sch\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df alles Sein in Poesie", "tokens": ["Da\u00df", "al\u00b7les", "Sein", "in", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor mir ersteh' und anders nie!", "tokens": ["Vor", "mir", "er\u00b7steh'", "und", "an\u00b7ders", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und anders nie! \u2013 ein k\u00fchnes Sinnen;", "tokens": ["Und", "an\u00b7ders", "nie", "!", "\u2013", "ein", "k\u00fch\u00b7nes", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch was ich wollt', hab ich gehalten \u2013", "tokens": ["Doch", "was", "ich", "wollt'", ",", "hab", "ich", "ge\u00b7hal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VMFIN", "$,", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Prosa jagt ich stolz von hinnen,", "tokens": ["Die", "Pro\u00b7sa", "jagt", "ich", "stolz", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verg\u00f6nnt ihr nie ein stetig Walten;", "tokens": ["Ver\u00b7g\u00f6nnt", "ihr", "nie", "ein", "ste\u00b7tig", "Wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hoch ging mein Flug, und Himmelsschein", "tokens": ["Hoch", "ging", "mein", "Flug", ",", "und", "Him\u00b7mels\u00b7schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verbannte alles, was gemein.", "tokens": ["Ver\u00b7bann\u00b7te", "al\u00b7les", ",", "was", "ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wird einst mein letztes Lied ert\u00f6nen,", "tokens": ["Wird", "einst", "mein", "letz\u00b7tes", "Lied", "er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach allen K\u00e4mpfen schwerer Zeiten:", "tokens": ["Nach", "al\u00b7len", "K\u00e4mp\u00b7fen", "schwe\u00b7rer", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Leben war ein Dienst des Sch\u00f6nen,", "tokens": ["Mein", "Le\u00b7ben", "war", "ein", "Dienst", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich danke Gott, der mir beschied:", "tokens": ["Ich", "dan\u00b7ke", "Gott", ",", "der", "mir", "be\u00b7schied", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein ganzes Leben war ein Lied.", "tokens": ["Mein", "gan\u00b7zes", "Le\u00b7ben", "war", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}