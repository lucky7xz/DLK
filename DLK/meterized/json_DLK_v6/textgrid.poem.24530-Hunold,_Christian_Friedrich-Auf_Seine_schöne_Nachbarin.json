{"textgrid.poem.24530": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Seine sch\u00f6ne Nachbarin", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Sommer/ Winter/ Herbst/ hab' ich den Fr\u00fchlings-Schein;", "tokens": ["Im", "Som\u00b7mer", "/", "Win\u00b7ter", "/", "Herbst", "/", "hab'", "ich", "den", "Fr\u00fch\u00b7lings\u00b7Schein", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "$(", "NN", "$(", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo kan die Aussicht wohl beliebt und sch\u00f6ner seyn/", "tokens": ["Wo", "kan", "die", "Aus\u00b7sicht", "wohl", "be\u00b7liebt", "und", "sch\u00f6\u00b7ner", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als die mein Auge hat? Hier sieht es auf ein Feld/", "tokens": ["Als", "die", "mein", "Au\u00b7ge", "hat", "?", "Hier", "sieht", "es", "auf", "ein", "Feld", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Rosen hier und dar und Lilien bestellt.", "tokens": ["Mit", "Ro\u00b7sen", "hier", "und", "dar", "und", "Li\u00b7li\u00b7en", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "PTKVZ", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Aussicht ist beliebt/ doch tausendmahl mehr sch\u00f6n/", "tokens": ["Die", "Aus\u00b7sicht", "ist", "be\u00b7liebt", "/", "doch", "tau\u00b7send\u00b7mahl", "mehr", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die aus den Tugend-Strahl der Nachbarin kan sehn.", "tokens": ["Die", "aus", "den", "Tu\u00b7gen\u00b7dStrahl", "der", "Nach\u00b7ba\u00b7rin", "kan", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}