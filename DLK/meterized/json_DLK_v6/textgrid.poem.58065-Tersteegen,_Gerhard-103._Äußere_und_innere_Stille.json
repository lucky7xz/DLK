{"textgrid.poem.58065": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "103. \u00c4u\u00dfere und innere Stille", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00fc\u00df ist's, wenn Gedanken, Glieder, Sinnen,", "tokens": ["Wie", "s\u00fc\u00df", "ist's", ",", "wenn", "Ge\u00b7dan\u00b7ken", ",", "Glie\u00b7der", ",", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "KOUS", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Affekte, Wille und Begierden stille sind,", "tokens": ["Af\u00b7fek\u00b7te", ",", "Wil\u00b7le", "und", "Be\u00b7gier\u00b7den", "stil\u00b7le", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn alles schweigt von au\u00dfen und von innen", "tokens": ["Wenn", "al\u00b7les", "schweigt", "von", "au\u00b7\u00dfen", "und", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ADV", "KON", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und man im heitern Grund Gott gegenw\u00e4rtig find't!", "tokens": ["Und", "man", "im", "hei\u00b7tern", "Grund", "Gott", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "find't", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "ADJA", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}