{"textgrid.poem.41573": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Allerheiligentage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig sind im Geist die Armen,", "tokens": ["Se\u00b7lig", "sind", "im", "Geist", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die zu ihres N\u00e4chsten F\u00fc\u00dfen", "tokens": ["Die", "zu", "ih\u00b7res", "N\u00e4chs\u00b7ten", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gern an seinem Licht erwarmen", "tokens": ["Gern", "an", "sei\u00b7nem", "Licht", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Dienerwort ihn gr\u00fc\u00dfen,", "tokens": ["Und", "mit", "Die\u00b7ner\u00b7wort", "ihn", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fremden Fehles sich erbarmen,", "tokens": ["Frem\u00b7den", "Feh\u00b7les", "sich", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fremden Gl\u00fcckes \u00fcberflie\u00dfen:", "tokens": ["Frem\u00b7den", "Gl\u00fc\u00b7ckes", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, zu ihres N\u00e4chsten F\u00fc\u00dfen", "tokens": ["Ja", ",", "zu", "ih\u00b7res", "N\u00e4chs\u00b7ten", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selig, selig sind die Armen.", "tokens": ["Se\u00b7lig", ",", "se\u00b7lig", "sind", "die", "Ar\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Selig sind der Sanftmut Kinder,", "tokens": ["Se\u00b7lig", "sind", "der", "Sanft\u00b7mut", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denen Z\u00fcrnen wird zum L\u00e4cheln", "tokens": ["De\u00b7nen", "Z\u00fcr\u00b7nen", "wird", "zum", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Milde Saat nicht minder", "tokens": ["Und", "der", "Mil\u00b7de", "Saat", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprie\u00dft aus Dorn und scharfen Hecheln,", "tokens": ["Sprie\u00dft", "aus", "Dorn", "und", "schar\u00b7fen", "He\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deren letztes Wort ein linder", "tokens": ["De\u00b7ren", "letz\u00b7tes", "Wort", "ein", "lin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebeshauch in Todesr\u00f6cheln,", "tokens": ["Lie\u00b7bes\u00b7hauch", "in", "To\u00b7des\u00b7r\u00f6\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn das Zucken wird zum L\u00e4cheln:", "tokens": ["Wenn", "das", "Zu\u00b7cken", "wird", "zum", "L\u00e4\u00b7cheln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selig sind der Sanftmut Kinder!", "tokens": ["Se\u00b7lig", "sind", "der", "Sanft\u00b7mut", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Selig sind die Trauer tragen", "tokens": ["Se\u00b7lig", "sind", "die", "Trau\u00b7er", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihr Brod mit Tr\u00e4nen tr\u00e4nken,", "tokens": ["Und", "ihr", "Brod", "mit", "Tr\u00e4\u00b7nen", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die eigne S\u00fcnde klagen", "tokens": ["Nur", "die", "eig\u00b7ne", "S\u00fcn\u00b7de", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der fremden nicht gedenken,", "tokens": ["Und", "der", "frem\u00b7den", "nicht", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An den eignen Busen schlagen,", "tokens": ["An", "den", "eig\u00b7nen", "Bu\u00b7sen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fremder Schuld die Blicke senken:", "tokens": ["Frem\u00b7der", "Schuld", "die", "Bli\u00b7cke", "sen\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ihr Brod mit Tr\u00e4nen tr\u00e4nken,", "tokens": ["Die", "ihr", "Brod", "mit", "Tr\u00e4\u00b7nen", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selig sind die Trauer tragen!", "tokens": ["Se\u00b7lig", "sind", "die", "Trau\u00b7er", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Selig wen der Durst ergriffen", "tokens": ["Se\u00b7lig", "wen", "der", "Durst", "er\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PWS", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Rechten, nach dem Guten,", "tokens": ["Nach", "dem", "Rech\u00b7ten", ",", "nach", "dem", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mutig, ob auf morschen Schiffen,", "tokens": ["Mu\u00b7tig", ",", "ob", "auf", "mor\u00b7schen", "Schif\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mutig steuernd nach den Fluten,", "tokens": ["Mu\u00b7tig", "steu\u00b7ernd", "nach", "den", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollte unter Strand und Riffen", "tokens": ["Soll\u00b7te", "un\u00b7ter", "Strand", "und", "Rif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch das Leben sich verbluten:", "tokens": ["Auch", "das", "Le\u00b7ben", "sich", "ver\u00b7blu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach dem Rechten, nach dem Guten,", "tokens": ["Nach", "dem", "Rech\u00b7ten", ",", "nach", "dem", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selig, wen der Durst ergriffen!", "tokens": ["Se\u00b7lig", ",", "wen", "der", "Durst", "er\u00b7grif\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Barmherzigen sind selig,", "tokens": ["Die", "Barm\u00b7her\u00b7zi\u00b7gen", "sind", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So nur nach der Wunde sehen,", "tokens": ["So", "nur", "nach", "der", "Wun\u00b7de", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Nicht erpressend kalt und w\u00e4hlig,", "tokens": ["Nicht", "er\u00b7pres\u00b7send", "kalt", "und", "w\u00e4h\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Schaden mocht' entstehen,", "tokens": ["Wie", "der", "Scha\u00b7den", "mocht'", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leise, schonend und allm\u00e4hlich", "tokens": ["Lei\u00b7se", ",", "scho\u00b7nend", "und", "all\u00b7m\u00e4h\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lassen drin den Balsam gehen:", "tokens": ["Las\u00b7sen", "drin", "den", "Bal\u00b7sam", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So nur nach der Wunde sehen,", "tokens": ["So", "nur", "nach", "der", "Wun\u00b7de", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Die Barmherzigen sind selig.", "tokens": ["Die", "Barm\u00b7her\u00b7zi\u00b7gen", "sind", "se\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00dcberselig reine Herzen,", "tokens": ["\u00dc\u00b7ber\u00b7se\u00b7lig", "rei\u00b7ne", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unbefleckter Jungfraun Sinnen!", "tokens": ["Un\u00b7be\u00b7fleck\u00b7ter", "Jung\u00b7fraun", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denen Kindeslust das Scherzen,", "tokens": ["De\u00b7nen", "Kin\u00b7des\u00b7lust", "das", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denen Himmelshauch das Minnen,", "tokens": ["De\u00b7nen", "Him\u00b7mels\u00b7hauch", "das", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die wie an Altares Kerzen", "tokens": ["Die", "wie", "an", "Al\u00b7ta\u00b7res", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00fcndeten ihr klar Beginnen:", "tokens": ["Z\u00fcn\u00b7de\u00b7ten", "ihr", "klar", "Be\u00b7gin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "Unbefleckter Jungfraun Sinnen,", "tokens": ["Un\u00b7be\u00b7fleck\u00b7ter", "Jung\u00b7fraun", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcberselig reine Herzen!", "tokens": ["\u00dc\u00b7ber\u00b7se\u00b7lig", "rei\u00b7ne", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und des Friedens fromme W\u00e4chter", "tokens": ["Und", "des", "Frie\u00b7dens", "from\u00b7me", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selig, an den Schranken waltend,", "tokens": ["Se\u00b7lig", ",", "an", "den", "Schran\u00b7ken", "wal\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Einigkeit Verfechter", "tokens": ["Und", "der", "Ei\u00b7nig\u00b7keit", "Ver\u00b7fech\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch die wei\u00dfe Fahne haltend,", "tokens": ["Hoch", "die", "wei\u00b7\u00dfe", "Fah\u00b7ne", "hal\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mild und fest gen den Ver\u00e4chter", "tokens": ["Mild", "und", "fest", "gen", "den", "Ver\u00b7\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Daun die Klinge spaltend:", "tokens": ["Wie", "der", "Daun", "die", "Klin\u00b7ge", "spal\u00b7tend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Selig an den Schranken waltend,", "tokens": ["Se\u00b7lig", "an", "den", "Schran\u00b7ken", "wal\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selig sind des Friedens W\u00e4chter!", "tokens": ["Se\u00b7lig", "sind", "des", "Frie\u00b7dens", "W\u00e4ch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die um dich Verfolgung leiden,", "tokens": ["Die", "um", "dich", "Ver\u00b7fol\u00b7gung", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6chster Feldherr, deine Scharen", "tokens": ["H\u00f6chs\u00b7ter", "Feld\u00b7herr", ",", "dei\u00b7ne", "Scha\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selig, wenn sie alles meiden,", "tokens": ["Se\u00b7lig", ",", "wenn", "sie", "al\u00b7les", "mei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um dein Banner sich zu wahren!", "tokens": ["Um", "dein", "Ban\u00b7ner", "sich", "zu", "wah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag es nie von ihnen scheiden", "tokens": ["Mag", "es", "nie", "von", "ih\u00b7nen", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht in Lust noch in Gefahren:", "tokens": ["Nicht", "in", "Lust", "noch", "in", "Ge\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Selig, selig deine Scharen!", "tokens": ["Se\u00b7lig", ",", "se\u00b7lig", "dei\u00b7ne", "Scha\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selig, die Verfolgung leiden!", "tokens": ["Se\u00b7lig", ",", "die", "Ver\u00b7fol\u00b7gung", "lei\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und so mu\u00df ich selig nennen", "tokens": ["Und", "so", "mu\u00df", "ich", "se\u00b7lig", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle, denen fremd mein Treiben,", "tokens": ["Al\u00b7le", ",", "de\u00b7nen", "fremd", "mein", "Trei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df indes die Wunden brennen,", "tokens": ["Mu\u00df", "in\u00b7des", "die", "Wun\u00b7den", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fremden Gl\u00fcckes Herold bleiben.", "tokens": ["Frem\u00b7den", "Gl\u00fc\u00b7ckes", "He\u00b7rold", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird denn nichts von dir mich trennen,", "tokens": ["Wird", "denn", "nichts", "von", "dir", "mich", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wildes, saftlos morsches Treiben?", "tokens": ["Wil\u00b7des", ",", "saft\u00b7los", "mor\u00b7sches", "Trei\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df ich selber mich zerreiben?", "tokens": ["Mu\u00df", "ich", "sel\u00b7ber", "mich", "zer\u00b7rei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"te