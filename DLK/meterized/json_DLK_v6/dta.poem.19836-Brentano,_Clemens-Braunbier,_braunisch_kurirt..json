{"dta.poem.19836": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Braunbier, braunisch kurirt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bauer. Herr Doktor, ich will fragen,               ", "tokens": ["Bau\u00b7er", ".", "Herr", "Dok\u00b7tor", ",", "ich", "will", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Seht dieses Glas voll Bier,", "tokens": ["Seht", "die\u00b7ses", "Glas", "voll", "Bier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr k\u00f6nnet mir wohl sagen,", "tokens": ["Ihr", "k\u00f6n\u00b7net", "mir", "wohl", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was fehlt dem Bier allhier?", "tokens": ["Was", "fehlt", "dem", "Bier", "all\u00b7hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es thut sich ganz entf\u00e4rben,", "tokens": ["Es", "thut", "sich", "ganz", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Kr\u00e4fte sind auch hin,", "tokens": ["Die", "Kr\u00e4f\u00b7te", "sind", "auch", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn es vor mir sollt sterben,", "tokens": ["Wenn", "es", "vor", "mir", "sollt", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein arme Wais ich bin.", "tokens": ["Ein", "ar\u00b7me", "Wais", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doktor. H\u00f6rt Bauer, kanns nicht sagen,               ", "tokens": ["Dok\u00b7tor", ".", "H\u00f6rt", "Bau\u00b7er", ",", "kanns", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "NE", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab nicht auf das studirt,", "tokens": ["Hab", "nicht", "auf", "das", "stu\u00b7dirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihr m\u00fc\u00dft nur weiter fragen,", "tokens": ["Ihr", "m\u00fc\u00dft", "nur", "wei\u00b7ter", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht sagts euch der Wirth.", "tokens": ["Viel\u00b7leicht", "sagts", "euch", "der", "Wirth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will doch ein wenig sehen,", "tokens": ["Will", "doch", "ein", "we\u00b7nig", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ob ich nichts kennen kann;", "tokens": ["Ob", "ich", "nichts", "ken\u00b7nen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wei\u00df schon was ihm geschehen,", "tokens": ["Wei\u00df", "schon", "was", "ihm", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Wassersucht ist dran.", "tokens": ["Die", "Was\u00b7ser\u00b7sucht", "ist", "dran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bauer. Jezt merk ich schon den Possen,               ", "tokens": ["Bau\u00b7er", ".", "Jezt", "merk", "ich", "schon", "den", "Pos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es ist ein Fehler drin,", "tokens": ["Es", "ist", "ein", "Feh\u00b7ler", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man hat das Bier vergossen,", "tokens": ["Man", "hat", "das", "Bier", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst w\u00e4r es nicht so d\u00fcnn;", "tokens": ["Sonst", "w\u00e4r", "es", "nicht", "so", "d\u00fcnn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Malz ist ihm verronnen,", "tokens": ["Das", "Malz", "ist", "ihm", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Wasser hat's f\u00fcr drei,", "tokens": ["Und", "Was\u00b7ser", "hat's", "f\u00fcr", "drei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Wirth hat nah zum Bronnen,", "tokens": ["Der", "Wirth", "hat", "nah", "zum", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was rathet ihr dabey?", "tokens": ["Was", "ra\u00b7thet", "ihr", "da\u00b7bey", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doktor. Das ein will ich euch rathen,               ", "tokens": ["Dok\u00b7tor", ".", "Das", "ein", "will", "ich", "euch", "ra\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "PTKVZ", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ihr m\u00fc\u00dft halt folgen mir,", "tokens": ["Ihr", "m\u00fc\u00dft", "halt", "fol\u00b7gen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wett mit euch Dukaten,", "tokens": ["Ich", "wett", "mit", "euch", "Du\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr kriegt das beste Bier.", "tokens": ["Ihr", "kriegt", "das", "bes\u00b7te", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geschwind zum Wirth thut laufen,", "tokens": ["Ge\u00b7schwind", "zum", "Wirth", "thut", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er macht euch Bier nach Wunsch,", "tokens": ["Er", "macht", "euch", "Bier", "nach", "Wunsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Wasser gebt zu kaufen,", "tokens": ["Das", "Was\u00b7ser", "gebt", "zu", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Gerste gebt umsonst.", "tokens": ["Die", "Gers\u00b7te", "gebt", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bauer. Herr Doktor, ich mu\u00df lachen,               ", "tokens": ["Bau\u00b7er", ".", "Herr", "Dok\u00b7tor", ",", "ich", "mu\u00df", "la\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Handel geht nicht an,", "tokens": ["Der", "Han\u00b7del", "geht", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er nur Bier th\u00e4t machen,", "tokens": ["Wenn", "er", "nur", "Bier", "th\u00e4t", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mans blos trinken kann,", "tokens": ["Das", "mans", "blos", "trin\u00b7ken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das wollen wir halt hoffen,", "tokens": ["Das", "wol\u00b7len", "wir", "halt", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Wasser gehts nicht zu Grund,", "tokens": ["Im", "Was\u00b7ser", "gehts", "nicht", "zu", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Das Bier w\u00e4r l\u00e4ngst versoffen,", "tokens": ["Das", "Bier", "w\u00e4r", "l\u00e4ngst", "ver\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn es nicht schwimmen kunnt.", "tokens": ["Wenn", "es", "nicht", "schwim\u00b7men", "kunnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}