{"dta.poem.11910": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Affe hat geh\u00f6rt, da\u00df s\u00fc\u00df der Nu\u00dfkern schmecke,", "tokens": ["Der", "Af\u00b7fe", "hat", "ge\u00b7h\u00f6rt", ",", "da\u00df", "s\u00fc\u00df", "der", "Nu\u00df\u00b7kern", "schme\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und N\u00fcsse nahm er vor mitsamt der gr\u00fcnen Decke.", "tokens": ["Und", "N\u00fcs\u00b7se", "nahm", "er", "vor", "mit\u00b7samt", "der", "gr\u00fc\u00b7nen", "De\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die wegzur\u00e4umen sollt' er brauchen seine Pfote,", "tokens": ["Die", "weg\u00b7zu\u00b7r\u00e4u\u00b7men", "sollt'", "er", "brau\u00b7chen", "sei\u00b7ne", "Pfo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nimmt er sie ins Maul gleich einem s\u00fc\u00dfen Brote.", "tokens": ["Doch", "nimmt", "er", "sie", "ins", "Maul", "gleich", "ei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Bro\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ruft, wenn ihm den Mund verbitterte die Schale:", "tokens": ["Und", "ruft", ",", "wenn", "ihm", "den", "Mund", "ver\u00b7bit\u00b7ter\u00b7te", "die", "Scha\u00b7le", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betrogen hat mich, wer dich mir empfahl zum Male.", "tokens": ["Be\u00b7tro\u00b7gen", "hat", "mich", ",", "wer", "dich", "mir", "emp\u00b7fahl", "zum", "Ma\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PWS", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Noch eine Weile f\u00e4hrt er fort hineinzukaun,", "tokens": ["Noch", "ei\u00b7ne", "Wei\u00b7le", "f\u00e4hrt", "er", "fort", "hin\u00b7ein\u00b7zu\u00b7kaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer will es nicht dem Gaumen s\u00fc\u00dfer thaun.", "tokens": ["Und", "im\u00b7mer", "will", "es", "nicht", "dem", "Gau\u00b7men", "s\u00fc\u00b7\u00dfer", "thaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Eh er den Kern geschmeckt, hat er den Ueberdru\u00df", "tokens": ["Eh", "er", "den", "Kern", "ge\u00b7schmeckt", ",", "hat", "er", "den", "Ue\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Gefressen an der Schal', und wirft hinweg die Nu\u00df.", "tokens": ["Ge\u00b7fres\u00b7sen", "an", "der", "Schal'", ",", "und", "wirft", "hin\u00b7weg", "die", "Nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So sah ich manchen, den man keinen Affen hie\u00df,", "tokens": ["So", "sah", "ich", "man\u00b7chen", ",", "den", "man", "kei\u00b7nen", "Af\u00b7fen", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "PRELS", "PIS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, von der Schale satt, den Nu\u00dfkern fallen lie\u00df.", "tokens": ["Der", ",", "von", "der", "Scha\u00b7le", "satt", ",", "den", "Nu\u00df\u00b7kern", "fal\u00b7len", "lie\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ADJD", "$,", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}