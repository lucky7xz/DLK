{"textgrid.poem.52869": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Neue M\u00e4hr", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im ged\u00e4mpften Nebenzimmer", "tokens": ["Im", "ge\u00b7d\u00e4mpf\u00b7ten", "Ne\u00b7ben\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelt des Professors Tochter,", "tokens": ["Wan\u00b7delt", "des", "Pro\u00b7fes\u00b7sors", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pauken und Trompetenjubel", "tokens": ["Pau\u00b7ken", "und", "Trom\u00b7pe\u00b7ten\u00b7ju\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Klingt her\u00fcber aus dem Tanzsaal.", "tokens": ["Klingt", "her\u00b7\u00fc\u00b7ber", "aus", "dem", "Tanz\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4stig werden mir die T\u00e4nze", "tokens": ["L\u00e4s\u00b7tig", "wer\u00b7den", "mir", "die", "T\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die s\u00fc\u00dfen Schmeichelworte", "tokens": ["Und", "die", "s\u00fc\u00b7\u00dfen", "Schmei\u00b7chel\u00b7wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Studenten, die so zierlich", "tokens": ["Der", "Stu\u00b7den\u00b7ten", ",", "die", "so", "zier\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mich vergleichen mit den Blumen.", "tokens": ["Mich", "ver\u00b7glei\u00b7chen", "mit", "den", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ueberl\u00e4stig wird mir Alles,", "tokens": ["Ue\u00b7ber\u00b7l\u00e4s\u00b7tig", "wird", "mir", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit ich sah im Strahl des Mondes", "tokens": ["Seit", "ich", "sah", "im", "Strahl", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenen Turner, dessen Jodeln", "tokens": ["Je\u00b7nen", "Tur\u00b7ner", ",", "des\u00b7sen", "Jo\u00b7deln"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4chtens mich an's Fenster lockte.", "tokens": ["N\u00e4ch\u00b7tens", "mich", "an's", "Fens\u00b7ter", "lock\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie er stand so schlank und muthig,", "tokens": ["Wie", "er", "stand", "so", "schlank", "und", "mut\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Augenr\u00e4der rollten,", "tokens": ["Und", "die", "Au\u00b7gen\u00b7r\u00e4\u00b7der", "roll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feurig um die Adlernase,", "tokens": ["Feu\u00b7rig", "um", "die", "Ad\u00b7ler\u00b7na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glich er einem Tempelritter.", "tokens": ["Glich", "er", "ei\u00b7nem", "Tem\u00b7pel\u00b7rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dacht's des Theologen Tochter,", "tokens": ["Dacht's", "des", "Theo\u00b7lo\u00b7gen", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und sie schaute auf den Boden.", "tokens": ["Und", "sie", "schau\u00b7te", "auf", "den", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie aufblickt steht der K\u00fchne,", "tokens": ["Wie", "sie", "auf\u00b7blickt", "steht", "der", "K\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Unbekannte Turner vor ihr.", "tokens": ["Un\u00b7be\u00b7kann\u00b7te", "Tur\u00b7ner", "vor", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00e4ndedr\u00fcckend, liebeschmunzelnd,", "tokens": ["H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7ckend", ",", "lie\u00b7be\u00b7schmun\u00b7zelnd", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wandeln sie umher im Storchschritt,", "tokens": ["Wan\u00b7deln", "sie", "um\u00b7her", "im", "Storch\u00b7schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kuchen winken freundlich", "tokens": ["Und", "die", "Ku\u00b7chen", "win\u00b7ken", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Gl\u00fchwein duftet gottvoll.", "tokens": ["Und", "der", "Gl\u00fch\u00b7wein", "duf\u00b7tet", "gott\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Und der Gl\u00fchwein duftet gottvoll,", "tokens": ["Und", "der", "Gl\u00fch\u00b7wein", "duf\u00b7tet", "gott\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und vom Liebestrank ergl\u00fcht er.", "tokens": ["Und", "vom", "Lie\u00b7bes\u00b7trank", "er\u00b7gl\u00fcht", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Awer sag \u00e4 mal, Gel\u00fcbte,", "tokens": ["A\u00b7wer", "sag", "\u00e4", "mal", ",", "Ge\u00b7l\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Warum Du so pletzlich wei\u00df w\u00fcrst?", "tokens": ["Wa\u00b7rum", "Du", "so", "pletz\u00b7lich", "wei\u00df", "w\u00fcrst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Eine Spinne l\u00e4uft am Ohr mir", "tokens": ["Ei\u00b7ne", "Spin\u00b7ne", "l\u00e4uft", "am", "Ohr", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Spinnen sind, Geliebter,", "tokens": ["Und", "die", "Spin\u00b7nen", "sind", ",", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Mir so tief verha\u00dft, als w\u00e4ren's", "tokens": ["Mir", "so", "tief", "ver\u00b7ha\u00dft", ",", "als", "w\u00e4\u00b7ren's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$,", "KOUS", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Krummgebeinte Judenbuben.", "tokens": ["Krumm\u00b7ge\u00b7bein\u00b7te", "Ju\u00b7den\u00b7bu\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df d\u00f6 Spinne un d\u00f6 Judde!", "tokens": ["La\u00df", "d\u00f6", "Spin\u00b7ne", "un", "d\u00f6", "Jud\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht der Turner freundlich kosend,", "tokens": ["Spricht", "der", "Tur\u00b7ner", "freund\u00b7lich", "ko\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Saale dringen blendend", "tokens": ["Aus", "dem", "Saa\u00b7le", "drin\u00b7gen", "blen\u00b7dend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend goldene Lampenlichter.", "tokens": ["Tau\u00b7send", "gol\u00b7de\u00b7ne", "Lam\u00b7pen\u00b7lich\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Tausend goldene Lampenlichter", "tokens": ["Tau\u00b7send", "gol\u00b7de\u00b7ne", "Lam\u00b7pen\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Haben ihren Strahl ergossen.", "tokens": ["Ha\u00b7ben", "ih\u00b7ren", "Strahl", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Awer sag \u00e4 mal Gel\u00fcbte,", "tokens": ["A\u00b7wer", "sag", "\u00e4", "mal", "Ge\u00b7l\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ADV", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Bischt Du auch in mich verkeulet?", "tokens": ["Bischt", "Du", "auch", "in", "mich", "ver\u00b7keu\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ja ich liebe Dich, Geliebter,", "tokens": ["Ja", "ich", "lie\u00b7be", "Dich", ",", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem Heiland sei's geschworen,", "tokens": ["Bei", "dem", "Hei\u00b7land", "sei's", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den die Juden ", "tokens": ["Den", "die", "Ju\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wie mein Vater neu bewiesen.", "tokens": ["Wie", "mein", "Va\u00b7ter", "neu", "be\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "La\u00df d\u00f6 Heuland, la\u00df d\u00f6 Judde!", "tokens": ["La\u00df", "d\u00f6", "Heu\u00b7land", ",", "la\u00df", "d\u00f6", "Jud\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht der Turner, freundlich kosend,", "tokens": ["Spricht", "der", "Tur\u00b7ner", ",", "freund\u00b7lich", "ko\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Ferne schwanken traumhaft", "tokens": ["In", "der", "Fer\u00b7ne", "schwan\u00b7ken", "traum\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stengelgl\u00e4ser schaumumflossen.", "tokens": ["Sten\u00b7gel\u00b7gl\u00e4\u00b7ser", "schau\u00b7mum\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Stengelgl\u00e4ser schaumumflossen", "tokens": ["Sten\u00b7gel\u00b7gl\u00e4\u00b7ser", "schau\u00b7mum\u00b7flos\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwanken in der Ferne traumhaft.", "tokens": ["Schwan\u00b7ken", "in", "der", "Fer\u00b7ne", "traum\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Awer sag \u00e4 mal, Gel\u00fcbte,", "tokens": ["A\u00b7wer", "sag", "\u00e4", "mal", ",", "Ge\u00b7l\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hast Du mich n\u00f6t angeloge?", "tokens": ["Hast", "Du", "mich", "n\u00f6t", "an\u00b7ge\u00b7lo\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Falsch ist nicht in mir Geliebter,", "tokens": ["Falsch", "ist", "nicht", "in", "mir", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PTKNEG", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie in mir kein Tropfen Blutes", "tokens": ["Wie", "in", "mir", "kein", "Trop\u00b7fen", "Blu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPER", "PIAT", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ist von dem Verr\u00e4ther Judas,", "tokens": ["Ist", "von", "dem", "Ver\u00b7r\u00e4\u00b7ther", "Ju\u00b7das", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sich j\u00fcdisch sehr benommen.", "tokens": ["Der", "sich", "j\u00fc\u00b7disch", "sehr", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "La\u00df doch d\u00f6 Isra\u00f6l\u00fctte!", "tokens": ["La\u00df", "doch", "d\u00f6", "Is\u00b7ra\u00b7\u00f6\u00b7l\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Spricht der Turner, freundlich kosend,", "tokens": ["Spricht", "der", "Tur\u00b7ner", ",", "freund\u00b7lich", "ko\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,