{"dta.poem.12761": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das lob der priester-ehe,  \n Bey dem G. und P. hochzeit-feste.  \n P. P.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die priester-ehen sind die besten in der welt,", "tokens": ["Die", "pries\u00b7ter\u00b7e\u00b7hen", "sind", "die", "bes\u00b7ten", "in", "der", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil gleichsam GOtt hier selbst mit menschen hochzeit h\u00e4lt;", "tokens": ["Weil", "gleich\u00b7sam", "Gott", "hier", "selbst", "mit", "men\u00b7schen", "hoch\u00b7zeit", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich durch selbige mit sterblichen verbindet;", "tokens": ["Und", "sich", "durch", "sel\u00b7bi\u00b7ge", "mit", "sterb\u00b7li\u00b7chen", "ver\u00b7bin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn priester stellen ja in ihrer amts-geb\u00fchr,", "tokens": ["Denn", "pries\u00b7ter", "stel\u00b7len", "ja", "in", "ih\u00b7rer", "amts\u00b7ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was sie sonst auch thun, den Allerh\u00f6chsten f\u00fcr,", "tokens": ["Und", "was", "sie", "sonst", "auch", "thun", ",", "den", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "VVINF", "$,", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sich bey ihnen meist, wie sie bey uns, befindet.", "tokens": ["Der", "sich", "bey", "ih\u00b7nen", "meist", ",", "wie", "sie", "bey", "uns", ",", "be\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVFIN", "$,", "PWAV", "PPER", "APPR", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Jm lieben legen sie auch meist den rechten grund,", "tokens": ["Jm", "lie\u00b7ben", "le\u00b7gen", "sie", "auch", "meist", "den", "rech\u00b7ten", "grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und richten dessen zweck nach ihres himmels bund,", "tokens": ["Und", "rich\u00b7ten", "des\u00b7sen", "zweck", "nach", "ih\u00b7res", "him\u00b7mels", "bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der selbst den menschen hat befohlen sich zu lieben;", "tokens": ["Der", "selbst", "den", "men\u00b7schen", "hat", "be\u00b7foh\u00b7len", "sich", "zu", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "VVPP", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die s\u00e4ulen sind allhier, treu, tugend und verstand,", "tokens": ["Die", "s\u00e4u\u00b7len", "sind", "all\u00b7hier", ",", "treu", ",", "tu\u00b7gend", "und", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die der verliebeten h\u00f6chst-dauerhafftes band,", "tokens": ["Die", "der", "ver\u00b7lie\u00b7be\u00b7ten", "h\u00f6chst\u00b7dau\u00b7e\u00b7rhaff\u00b7tes", "band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df sie kein unbestand kan in der lust betr\u00fcben.", "tokens": ["Da\u00df", "sie", "kein", "un\u00b7be\u00b7stand", "kan", "in", "der", "lust", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die welt ist meistentheils nun heute so gesinnt,", "tokens": ["Die", "welt", "ist", "meis\u00b7ten\u00b7theils", "nun", "heu\u00b7te", "so", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df sie nur dahin zielt, wo sie was reichthum findt,", "tokens": ["Da\u00df", "sie", "nur", "da\u00b7hin", "zielt", ",", "wo", "sie", "was", "reicht\u00b7hum", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "VVFIN", "$,", "PWAV", "PPER", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die reichen m\u00e4gdgen sind bey vielen itzt die besten;", "tokens": ["Die", "rei\u00b7chen", "m\u00e4gd\u00b7gen", "sind", "bey", "vie\u00b7len", "itzt", "die", "bes\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn diesen dingrigen gleich alle tugend fehlt,", "tokens": ["Wenn", "die\u00b7sen", "ding\u00b7ri\u00b7gen", "gleich", "al\u00b7le", "tu\u00b7gend", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn aber ihre hand nur viel dueaten zehlt,", "tokens": ["Wenn", "a\u00b7ber", "ih\u00b7re", "hand", "nur", "viel", "du\u00b7ea\u00b7ten", "zehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So sehnet sie sich bald nach diesen reichen g\u00e4sten.", "tokens": ["So", "seh\u00b7net", "sie", "sich", "bald", "nach", "die\u00b7sen", "rei\u00b7chen", "g\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein den priestern wird dieselbe eh verlacht,", "tokens": ["Al\u00b7lein", "den", "pries\u00b7tern", "wird", "die\u00b7sel\u00b7be", "eh", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PDAT", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die man mit sch\u00e4tzgen nur, nicht mit der liebsten macht,", "tokens": ["Die", "man", "mit", "sch\u00e4tz\u00b7gen", "nur", ",", "nicht", "mit", "der", "liebs\u00b7ten", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "VVFIN", "ADV", "$,", "PTKNEG", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie wissen besser sich hierinnen aufzuf\u00fchren;", "tokens": ["Sie", "wis\u00b7sen", "bes\u00b7ser", "sich", "hie\u00b7rin\u00b7nen", "auf\u00b7zu\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "PAV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ist die nur tugend-reich, wohin ihr sinn sie tr\u00e4gt,", "tokens": ["Ist", "die", "nur", "tu\u00b7gen\u00b7dreich", ",", "wo\u00b7hin", "ihr", "sinn", "sie", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So ist sie reich genung und kostbar eingelegt,", "tokens": ["So", "ist", "sie", "reich", "ge\u00b7nung", "und", "kost\u00b7bar", "ein\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auch so ein sch\u00f6nes pfand, bey dem nichts zu verlieren.", "tokens": ["Auch", "so", "ein", "sch\u00f6\u00b7nes", "pfand", ",", "bey", "dem", "nichts", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "ART", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "So sind sie mehr begl\u00fcckt als einer, der aufs geld", "tokens": ["So", "sind", "sie", "mehr", "be\u00b7gl\u00fcckt", "als", "ei\u00b7ner", ",", "der", "aufs", "geld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "KOKOM", "ART", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nur seiner liebe grund und hoffnung hat gestellt,", "tokens": ["Nur", "sei\u00b7ner", "lie\u00b7be", "grund", "und", "hoff\u00b7nung", "hat", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So k\u00f6nnen sie sich auch im geiste recht vergn\u00fcgen;", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", "sich", "auch", "im", "geis\u00b7te", "recht", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da jener, wenn das geld zu zeitlich f\u00fc\u00dfe kriegt,", "tokens": ["Da", "je\u00b7ner", ",", "wenn", "das", "geld", "zu", "zeit\u00b7lich", "f\u00fc\u00b7\u00dfe", "kriegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "KOUS", "ART", "NN", "PTKA", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und seiner wege geht, alsbald wird mi\u00dfvergn\u00fcgt,", "tokens": ["Und", "sei\u00b7ner", "we\u00b7ge", "geht", ",", "als\u00b7bald", "wird", "mi\u00df\u00b7ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wider seine frau mu\u00df stets im felde liegen.", "tokens": ["Und", "wi\u00b7der", "sei\u00b7ne", "frau", "mu\u00df", "stets", "im", "fel\u00b7de", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So bleibets endlich doch und gantz gewi\u00df dabey,", "tokens": ["So", "blei\u00b7bets", "end\u00b7lich", "doch", "und", "gantz", "ge\u00b7wi\u00df", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "KON", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df eine priester-eh die allerbeste sey,", "tokens": ["Da\u00df", "ei\u00b7ne", "pries\u00b7ter\u00b7eh", "die", "al\u00b7ler\u00b7bes\u00b7te", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Weil GOtt und tugend hier den grund-stein selber legen;", "tokens": ["Weil", "Gott", "und", "tu\u00b7gend", "hier", "den", "grun\u00b7dstein", "sel\u00b7ber", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Weil sich die eitelkeit und auch ihr goldner schlu\u00df", "tokens": ["Weil", "sich", "die", "ei\u00b7tel\u00b7keit", "und", "auch", "ihr", "gold\u00b7ner", "schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Von dieser liebes-glut gar weit entfernen mu\u00df,", "tokens": ["Von", "die\u00b7ser", "lie\u00b7bes\u00b7glut", "gar", "weit", "ent\u00b7fer\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und weder hand noch fu\u00df noch sonsten was darff regen.", "tokens": ["Und", "we\u00b7der", "hand", "noch", "fu\u00df", "noch", "sons\u00b7ten", "was", "darff", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "ADV", "PTKVZ", "ADV", "ADV", "PWS", "VMFIN", "ADJA", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Hochwerthgesch\u00e4tztes Paar! so bin ich recht erfreut,", "tokens": ["Hoch\u00b7werth\u00b7ge\u00b7sch\u00e4tz\u00b7tes", "Paar", "!", "so", "bin", "ich", "recht", "er\u00b7freut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df deine lieb auch hat der himmel eingeweiht,", "tokens": ["Da\u00df", "dei\u00b7ne", "lieb", "auch", "hat", "der", "him\u00b7mel", "ein\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Als welcher selbst ist hier im spiele mit gewesen,", "tokens": ["Als", "wel\u00b7cher", "selbst", "ist", "hier", "im", "spie\u00b7le", "mit", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "ADV", "VAFIN", "ADV", "APPRART", "VVFIN", "APPR", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dir, Liebste Schwester! wirfft die liebe rosen zu;", "tokens": ["Dir", ",", "Liebs\u00b7te", "Schwes\u00b7ter", "!", "wirfft", "die", "lie\u00b7be", "ro\u00b7sen", "zu", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das gl\u00fccke sagt dir auch ins ohr, wie wohl die thu,", "tokens": ["Das", "gl\u00fc\u00b7cke", "sagt", "dir", "auch", "ins", "ohr", ",", "wie", "wohl", "die", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PWAV", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die einen priester ihr zum liebsten auserlesen.", "tokens": ["Die", "ei\u00b7nen", "pries\u00b7ter", "ihr", "zum", "liebs\u00b7ten", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich selbst bin h\u00f6chst vergn\u00fcgt, weil du so bist vergn\u00fcgt,", "tokens": ["Ich", "selbst", "bin", "h\u00f6chst", "ver\u00b7gn\u00fcgt", ",", "weil", "du", "so", "bist", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da dieser schon bereits in deinem hertzen liegt,", "tokens": ["Da", "die\u00b7ser", "schon", "be\u00b7reits", "in", "dei\u00b7nem", "hert\u00b7zen", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Der auch in kurtzem wird in deinen armen liegen;", "tokens": ["Der", "auch", "in", "kurt\u00b7zem", "wird", "in", "dei\u00b7nen", "ar\u00b7men", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "VAFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Du must dem himmel lieb, und er dir gn\u00e4dig seyn,", "tokens": ["Du", "must", "dem", "him\u00b7mel", "lieb", ",", "und", "er", "dir", "gn\u00e4\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "$,", "KON", "PPER", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Weil er zum liebsten dir pflegt so ein haupt zu weihn,", "tokens": ["Weil", "er", "zum", "liebs\u00b7ten", "dir", "pflegt", "so", "ein", "haupt", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "VVFIN", "PPER", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Da\u00df sich beym himmel kan, und der in dem vergn\u00fcgen.", "tokens": ["Da\u00df", "sich", "beym", "him\u00b7mel", "kan", ",", "und", "der", "in", "dem", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "NN", "VMFIN", "$,", "KON", "ART", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Es macht die folge-zeit bereits schon diesen schlu\u00df:", "tokens": ["Es", "macht", "die", "fol\u00b7ge\u00b7zeit", "be\u00b7reits", "schon", "die\u00b7sen", "schlu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Da\u00df eine priester-frau sehr gl\u00fccklich leben mu\u00df.", "tokens": ["Da\u00df", "ei\u00b7ne", "pries\u00b7ter\u00b7frau", "sehr", "gl\u00fcck\u00b7lich", "le\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Di\u00df solt du k\u00fcnfftig auch mit deiner lust erfahren,", "tokens": ["Di\u00df", "solt", "du", "k\u00fcnff\u00b7tig", "auch", "mit", "dei\u00b7ner", "lust", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Wenn er wird beym altar in heisser andacht stehn,", "tokens": ["Wenn", "er", "wird", "beym", "al\u00b7tar", "in", "heis\u00b7ser", "an\u00b7dacht", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wenn du als wirthin wirst herum im hause gehn,", "tokens": ["Wenn", "du", "als", "wirt\u00b7hin", "wirst", "he\u00b7rum", "im", "hau\u00b7se", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "ADV", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "So wird der seegen sich mit eurer liebe paaren.", "tokens": ["So", "wird", "der", "see\u00b7gen", "sich", "mit", "eu\u00b7rer", "lie\u00b7be", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "PRF", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "So liebet h\u00f6chst-begl\u00fcckt! der himmel segne euch!", "tokens": ["So", "lie\u00b7bet", "h\u00f6chst\u00b7be\u00b7gl\u00fcckt", "!", "der", "him\u00b7mel", "seg\u00b7ne", "euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Sein wille mache euch an leib und seele reich!", "tokens": ["Sein", "wil\u00b7le", "ma\u00b7che", "euch", "an", "leib", "und", "see\u00b7le", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Euch sey der priester-stand ein himmel auf der erden,", "tokens": ["Euch", "sey", "der", "pries\u00b7ter\u00b7stand", "ein", "him\u00b7mel", "auf", "der", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "So mu\u00df, was andre sonst am allermeisten plagt,", "tokens": ["So", "mu\u00df", ",", "was", "and\u00b7re", "sonst", "am", "al\u00b7ler\u00b7meis\u00b7ten", "plagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PRELS", "PIS", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und ihre hertzen wie ein b\u00f6ser wurm zernagt,", "tokens": ["Und", "ih\u00b7re", "hert\