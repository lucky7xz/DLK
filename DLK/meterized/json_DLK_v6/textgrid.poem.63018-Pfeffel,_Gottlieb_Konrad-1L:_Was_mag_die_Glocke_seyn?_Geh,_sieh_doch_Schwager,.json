{"textgrid.poem.63018": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was mag die Glocke seyn? Geh, sieh doch Schwager,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mag die Glocke seyn? Geh, sieh doch Schwager,", "tokens": ["Was", "mag", "die", "Glo\u00b7cke", "seyn", "?", "Geh", ",", "sieh", "doch", "Schwa\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VAINF", "$.", "NE", "$,", "VVIMP", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Garten auf die Sonnenuhr!", "tokens": ["Im", "Gar\u00b7ten", "auf", "die", "Son\u00b7ne\u00b7nuhr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach Junker Hans auf seinem Krankenlager", "tokens": ["Sprach", "Jun\u00b7ker", "Hans", "auf", "sei\u00b7nem", "Kran\u00b7ken\u00b7la\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu seinem l\u00e4ndlichen Merkur.", "tokens": ["Zu", "sei\u00b7nem", "l\u00e4nd\u00b7li\u00b7chen", "Mer\u00b7kur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lips geht, und bringt nach langem Weilen", "tokens": ["Lips", "geht", ",", "und", "bringt", "nach", "lan\u00b7gem", "Wei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonnenuhr vors Kanape:", "tokens": ["Die", "Son\u00b7ne\u00b7nuhr", "vors", "Ka\u00b7na\u00b7pe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da, Herr, seht selber zu! sprach er mit Heulen,", "tokens": ["Da", ",", "Herr", ",", "seht", "sel\u00b7ber", "zu", "!", "sprach", "er", "mit", "Heu\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ADV", "PTKVZ", "$.", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gott wei\u00df, da\u00df ich vom Dinge nichts versteh.", "tokens": ["Gott", "wei\u00df", ",", "da\u00df", "ich", "vom", "Din\u00b7ge", "nichts", "ver\u00b7steh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "J\u00fcngst las mir Stauzius aus einer alten Fiebel", "tokens": ["J\u00fcngst", "las", "mir", "Stau\u00b7zius", "aus", "ei\u00b7ner", "al\u00b7ten", "Fie\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "++-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Den Schwank, und jauchzte wie ein Kind,", "tokens": ["Den", "Schwank", ",", "und", "jauchz\u00b7te", "wie", "ein", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gute Mann wei\u00df nicht, da\u00df er sich seiner Bibel", "tokens": ["Der", "gu\u00b7te", "Mann", "wei\u00df", "nicht", ",", "da\u00df", "er", "sich", "sei\u00b7ner", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie Lips der Sonnenuhr bedient.", "tokens": ["Wie", "Lips", "der", "Son\u00b7ne\u00b7nuhr", "be\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}