{"textgrid.poem.49450": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Dinge, die in uns singen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Dinge, die in uns singen,", "tokens": ["Die", "Din\u00b7ge", ",", "die", "in", "uns", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wann unser Bewusstsein ruhte,", "tokens": ["Wann", "un\u00b7ser", "Be\u00b7wusst\u00b7sein", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie t\u00f6nen in unserem Blute,", "tokens": ["Sie", "t\u00f6\u00b7nen", "in", "un\u00b7se\u00b7rem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O fernes, verschwiegenes Klingen!", "tokens": ["O", "fer\u00b7nes", ",", "ver\u00b7schwie\u00b7ge\u00b7nes", "Klin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Horcht! Unser Blut ist's, das leidet,", "tokens": ["Horcht", "!", "Un\u00b7ser", "Blut", "ist's", ",", "das", "lei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wann unsere Seele entflohn ist,", "tokens": ["Wann", "un\u00b7se\u00b7re", "See\u00b7le", "ent\u00b7flohn", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie so fremd und seltsam sein Ton ist,", "tokens": ["Wie", "so", "fremd", "und", "selt\u00b7sam", "sein", "Ton", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der bald im Schweigen verscheidet.", "tokens": ["Der", "bald", "im", "Schwei\u00b7gen", "ver\u00b7schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "O Blut der rosigen Traube,", "tokens": ["O", "Blut", "der", "ro\u00b7si\u00b7gen", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O Wein der schw\u00e4rzlichen Venen,", "tokens": ["O", "Wein", "der", "schw\u00e4rz\u00b7li\u00b7chen", "Ve\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wein und Blut, verkl\u00e4render Glaube.", "tokens": ["Wein", "und", "Blut", ",", "ver\u00b7kl\u00e4\u00b7ren\u00b7der", "Glau\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Singt! L\u00f6st unsre Seele in Tr\u00e4nen,", "tokens": ["Singt", "!", "L\u00f6st", "uns\u00b7re", "See\u00b7le", "in", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und bis in die Tiefen hernieder", "tokens": ["Und", "bis", "in", "die", "Tie\u00b7fen", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durchbebt unsre armen Glieder.", "tokens": ["Durch\u00b7bebt", "uns\u00b7re", "ar\u00b7men", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}