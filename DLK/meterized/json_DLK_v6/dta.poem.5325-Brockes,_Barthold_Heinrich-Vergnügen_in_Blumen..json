{"dta.poem.5325": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgen in Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df andre, mit geschwollnen Trieben,", "tokens": ["La\u00df", "and\u00b7re", ",", "mit", "ge\u00b7schwoll\u00b7nen", "Trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Hofes schimmernd Elend lieben", "tokens": ["Des", "Ho\u00b7fes", "schim\u00b7mernd", "E\u00b7lend", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer, um sich zu erh\u00f6hn,", "tokens": ["Und", "im\u00b7mer", ",", "um", "sich", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einem glatten Fall-Brett stehn;", "tokens": ["Auf", "ei\u00b7nem", "glat\u00b7ten", "Fall\u00b7Brett", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df andre Lust im Wucher finden", "tokens": ["La\u00df", "and\u00b7re", "Lust", "im", "Wu\u00b7cher", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJA", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Gold und Geld zusammen schinden,", "tokens": ["Und", "Gold", "und", "Geld", "zu\u00b7sam\u00b7men", "schin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zum nie zu brauchenden Genu\u00df,", "tokens": ["Zum", "nie", "zu", "brau\u00b7chen\u00b7den", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und d\u00fcrftig seyn im Ueberflu\u00df;", "tokens": ["Und", "d\u00fcrf\u00b7tig", "seyn", "im", "Ue\u00b7berf\u00b7lu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "La\u00df sie, zum besten froher Erben,", "tokens": ["La\u00df", "sie", ",", "zum", "bes\u00b7ten", "fro\u00b7her", "Er\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Arm leben, blos um reich zu sterben;", "tokens": ["Arm", "le\u00b7ben", ",", "blos", "um", "reich", "zu", "ster\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Es suchen ander\u2019 ihr Vergn\u00fcgen,", "tokens": ["Es", "su\u00b7chen", "an\u00b7der'", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Vom Helden-Wurm genagt, im Kriegen;", "tokens": ["Vom", "Hel\u00b7den\u00b7Wurm", "ge\u00b7nagt", ",", "im", "Krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "La\u00df sie im Sturm durch Bomb- und Klingen,", "tokens": ["La\u00df", "sie", "im", "Sturm", "durch", "Bom\u00b7b", "und", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Zerschmettert und gel\u00e4hmet, dringen,", "tokens": ["Zer\u00b7schmet\u00b7tert", "und", "ge\u00b7l\u00e4h\u00b7met", ",", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Um ihren Nahmen in Gazetten", "tokens": ["Um", "ih\u00b7ren", "Nah\u00b7men", "in", "Ga\u00b7zet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Von der Vergessenheit zu retten;", "tokens": ["Von", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "zu", "ret\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wer will, mag aus Dorinden Augen", "tokens": ["Wer", "will", ",", "mag", "aus", "Do\u00b7rin\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "$,", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Den bittern Nectar br\u00fcnstig saugen,", "tokens": ["Den", "bit\u00b7tern", "Nec\u00b7tar", "br\u00fcns\u00b7tig", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Zu ihren F\u00fcssen sclavisch knien,", "tokens": ["Zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", "scla\u00b7visch", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Aus ihrer Br\u00fcste weichen Klippen", "tokens": ["Aus", "ih\u00b7rer", "Br\u00fcs\u00b7te", "wei\u00b7chen", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Gift, und aus ihren falschen Lippen", "tokens": ["Gift", ",", "und", "aus", "ih\u00b7ren", "fal\u00b7schen", "Lip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Die s\u00fcssen Coloqvinten ziehn;", "tokens": ["Die", "s\u00fcs\u00b7sen", "Co\u00b7lo\u00b7qvin\u00b7ten", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "La\u00df Madidum ein Gut verschlemmen", "tokens": ["La\u00df", "Ma\u00b7di\u00b7dum", "ein", "Gut", "ver\u00b7schlem\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und Hals und Magen \u00fcberschwemmen", "tokens": ["Und", "Hals", "und", "Ma\u00b7gen", "\u00fc\u00b7bersc\u00b7hwem\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Mit Ausbruch vo", "tokens": ["Mit", "Aus\u00b7bruch", "vo"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "La\u00df ihn bey seinen nassen Br\u00fcdern", "tokens": ["La\u00df", "ihn", "bey", "sei\u00b7nen", "nas\u00b7sen", "Br\u00fc\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und, zwischen kaum verstandnen Liedern,", "tokens": ["Und", ",", "zwi\u00b7schen", "kaum", "ver\u00b7stand\u00b7nen", "Lie\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Auf seine Weise fr\u00f6lich seyn:", "tokens": ["Auf", "sei\u00b7ne", "Wei\u00b7se", "fr\u00f6\u00b7lich", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}