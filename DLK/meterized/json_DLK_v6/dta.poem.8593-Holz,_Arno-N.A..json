{"dta.poem.8593": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In meine Dachkammer,", "tokens": ["In", "mei\u00b7ne", "Dach\u00b7kam\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "eine Etage h\u00f6her als der Himmel,", "tokens": ["ei\u00b7ne", "E\u00b7ta\u00b7ge", "h\u00f6\u00b7her", "als", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "kommen sie alle.", "tokens": ["kom\u00b7men", "sie", "al\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Menschen, die Goya und Utamaro lieben,", "tokens": ["Men\u00b7schen", ",", "die", "Go\u00b7ya", "und", "Ut\u00b7a\u00b7ma\u00b7ro", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "NE", "KON", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "seltne, ganz ausgefallne, verdrehte Exemplare und H\u00fchner,", "tokens": ["selt\u00b7ne", ",", "ganz", "aus\u00b7ge\u00b7fall\u00b7ne", ",", "ver\u00b7dreh\u00b7te", "Ex\u00b7emp\u00b7la\u00b7re", "und", "H\u00fch\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "ADJA", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "die Palestrina \u00fcber Pietro Mascagni stellen,", "tokens": ["die", "Pa\u00b7le\u00b7stri\u00b7na", "\u00fc\u00b7ber", "Pie\u00b7tro", "Mas\u00b7cag\u00b7ni", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "alte Herren, die heimlich, wenn im M\u00e4rz die Veilchen wieder bl\u00fchn,", "tokens": ["al\u00b7te", "Her\u00b7ren", ",", "die", "heim\u00b7lich", ",", "wenn", "im", "M\u00e4rz", "die", "Veil\u00b7chen", "wie\u00b7der", "bl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "$,", "KOUS", "APPRART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "auf den Strassen kleinen Rotznasen Bonbons zustecken,", "tokens": ["auf", "den", "Stras\u00b7sen", "klei\u00b7nen", "Rotz\u00b7na\u00b7sen", "Bon\u00b7bons", "zu\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "und junge Leute, die B\u00fccher verkaufen", "tokens": ["und", "jun\u00b7ge", "Leu\u00b7te", ",", "die", "B\u00fc\u00b7cher", "ver\u00b7kau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und Sonntags, in ihren Mussestunden, den lieben Gott totschlagen.", "tokens": ["und", "Sonn\u00b7tags", ",", "in", "ih\u00b7ren", "Mus\u00b7ses\u00b7tun\u00b7den", ",", "den", "lie\u00b7ben", "Gott", "tot\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Meister, der Meester, der Ma\u00ebstro, der Ma\u00ebstrino und der", "tokens": ["Der", "Meis\u00b7ter", ",", "der", "Mees\u00b7ter", ",", "der", "Ma\u00ebstro", ",", "der", "Ma\u00ebstri\u00b7no", "und", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NE", "$,", "ART", "NN", "KON", "ART"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ma\u00ebstrillo.", "tokens": ["Ma\u00ebstril\u00b7lo", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Der Ma\u00ebstrillo, wie immer, ist der Erste.", "tokens": ["Der", "Ma\u00ebstril\u00b7lo", ",", "wie", "im\u00b7mer", ",", "ist", "der", "Ers\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PWAV", "ADV", "$,", "VAFIN", "ART", "ADJA", "$."], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Er sch\u00fcttelt den Schnee von den Schultern,", "tokens": ["Er", "sch\u00fct\u00b7telt", "den", "Schnee", "von", "den", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "zieht die Handschuhe aus, kn\u00fcpft das Halstuch ab,", "tokens": ["zieht", "die", "Hand\u00b7schu\u00b7he", "aus", ",", "kn\u00fcpft", "das", "Hal\u00b7stuch", "ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "die nassen Galoschen stellt er draussen neben den Rauchfang auf die", "tokens": ["die", "nas\u00b7sen", "Ga\u00b7lo\u00b7schen", "stellt", "er", "draus\u00b7sen", "ne\u00b7ben", "den", "Rauch\u00b7fang", "auf", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "ART"], "meter": "-+----+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bodentreppe.", "tokens": ["Bo\u00b7den\u00b7trep\u00b7pe", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Um unser rotes, irisches Oefchen, auf Feldst\u00fchlchen,", "tokens": ["Um", "un\u00b7ser", "ro\u00b7tes", ",", "i\u00b7risc\u00b7hes", "O\u00b7e\u00b7fchen", ",", "auf", "Feld\u00b7st\u00fchl\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+---+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sitzen wir dann,", "tokens": ["sit\u00b7zen", "wir", "dann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "horchen, wie ab und zu, unsichtbar, durch die Stille auf den Rost der Coaks nachrutscht", "tokens": ["hor\u00b7chen", ",", "wie", "ab", "und", "zu", ",", "un\u00b7sicht\u00b7bar", ",", "durch", "die", "Stil\u00b7le", "auf", "den", "Rost", "der", "Coaks", "nach\u00b7rutscht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PTKVZ", "KON", "PTKVZ", "$,", "ADJD", "$,", "APPR", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "+--+--+--+-+-+-+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "und freuen uns, wie durchs Dunkel unsre Cigarren gl\u00fchn!", "tokens": ["und", "freu\u00b7en", "uns", ",", "wie", "durchs", "Dun\u00b7kel", "uns\u00b7re", "Ci\u00b7gar\u00b7ren", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}