{"dta.poem.12755": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Frie\u00df- und Seligmannischen  \n Verbindung.  \n D. M. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich solte, Werthes Paar! auch auf dein hochzeit-fest", "tokens": ["Ich", "sol\u00b7te", ",", "Wert\u00b7hes", "Paar", "!", "auch", "auf", "dein", "hoch\u00b7zeit\u00b7fest"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "ADJA", "NN", "$.", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Etwas mit netter hand von deinem wohl entwerffen;", "tokens": ["Et\u00b7was", "mit", "net\u00b7ter", "hand", "von", "dei\u00b7nem", "wohl", "ent\u00b7werf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach aber! wer wird mir die stumpffe feder sch\u00e4rffen?", "tokens": ["Ach", "a\u00b7ber", "!", "wer", "wird", "mir", "die", "stumpf\u00b7fe", "fe\u00b7der", "sch\u00e4rf\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PWS", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die zeit und einfalt noch gar wenig schreiben l\u00e4st.", "tokens": ["Die", "zeit", "und", "ein\u00b7falt", "noch", "gar", "we\u00b7nig", "schrei\u00b7ben", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "ADV", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du weist es selber wohl, wie meine laute klinget,", "tokens": ["Du", "weist", "es", "sel\u00b7ber", "wohl", ",", "wie", "mei\u00b7ne", "lau\u00b7te", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und was der schwache mund vor schlechte lieder singet.", "tokens": ["Und", "was", "der", "schwa\u00b7che", "mund", "vor", "schlech\u00b7te", "lie\u00b7der", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch weil mauch kluger kopff auch offt im schlafe denckt,", "tokens": ["Doch", "weil", "mauch", "klu\u00b7ger", "kopff", "auch", "offt", "im", "schla\u00b7fe", "denckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADJA", "NN", "ADV", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er, ich wei\u00df nicht wie, mit s\u00fc\u00dfen tr\u00e4umen spielet,", "tokens": ["Wenn", "er", ",", "ich", "wei\u00df", "nicht", "wie", ",", "mit", "s\u00fc\u00b7\u00dfen", "tr\u00e4u\u00b7men", "spie\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "APPR", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sein entz\u00fcckter geist die beste regung f\u00fchlet,", "tokens": ["Da\u00df", "sein", "ent\u00b7z\u00fcck\u00b7ter", "geist", "die", "bes\u00b7te", "re\u00b7gung", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ihm die Castalis aus ihren quellen schenckt,", "tokens": ["Die", "ihm", "die", "Cas\u00b7ta\u00b7lis", "aus", "ih\u00b7ren", "quel\u00b7len", "schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So will ich es getrost mit meinen versen wagen,", "tokens": ["So", "will", "ich", "es", "ge\u00b7trost", "mit", "mei\u00b7nen", "ver\u00b7sen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVPP", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und di\u00df, was aug und ohr gemercket hat, vortragen.", "tokens": ["Und", "di\u00df", ",", "was", "aug", "und", "ohr", "ge\u00b7mer\u00b7cket", "hat", ",", "vor\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "ADJD", "KON", "NN", "VVPP", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich ward durch einen freund ins cabinet gebracht,", "tokens": ["Ich", "ward", "durch", "ei\u00b7nen", "freund", "ins", "ca\u00b7bi\u00b7net", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo die Eusebie bey ihren b\u00fcchern sa\u00dfe,", "tokens": ["Wo", "die", "Eu\u00b7se\u00b7bie", "bey", "ih\u00b7ren", "b\u00fc\u00b7chern", "sa\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und gleich mit allem flei\u00df in dem Menzero lase,", "tokens": ["Und", "gleich", "mit", "al\u00b7lem", "flei\u00df", "in", "dem", "Men\u00b7ze\u00b7ro", "la\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was er von gl\u00fcck und heyl der heyrath hat erdacht:", "tokens": ["Was", "er", "von", "gl\u00fcck", "und", "heyl", "der", "hey\u00b7rath", "hat", "er\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie man das erstere gar leichte k\u00f6nt\u2019 erlangen,", "tokens": ["Wie", "man", "das", "ers\u00b7te\u00b7re", "gar", "leich\u00b7te", "k\u00f6nt'", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "ADV", "ADJA", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn nur das letztere mit GOtt w\u00e4r angefangen.", "tokens": ["Wenn", "nur", "das", "letz\u00b7te\u00b7re", "mit", "Gott", "w\u00e4r", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "PIS", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So viel als mir davon noch in die augen fiel,", "tokens": ["So", "viel", "als", "mir", "da\u00b7von", "noch", "in", "die", "au\u00b7gen", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "PAV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War, da\u00df man auch den rath der menschen m\u00fcste wissen,", "tokens": ["War", ",", "da\u00df", "man", "auch", "den", "rath", "der", "men\u00b7schen", "m\u00fcs\u00b7te", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn man das gantze werck bed\u00e4chtig wolte schl\u00fcssen,", "tokens": ["Wenn", "man", "das", "gant\u00b7ze", "werck", "be\u00b7d\u00e4ch\u00b7tig", "wol\u00b7te", "schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der tr\u00e4ff allererst, dem sch\u00fctzen gleich, das ziel,", "tokens": ["Und", "der", "tr\u00e4ff", "al\u00b7le\u00b7rerst", ",", "dem", "sch\u00fct\u00b7zen", "gleich", ",", "das", "ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "$,", "PRELS", "VVFIN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer ihre meynung noch auf eine wage br\u00e4chte,", "tokens": ["Wer", "ih\u00b7re", "mey\u00b7nung", "noch", "auf", "ei\u00b7ne", "wa\u00b7ge", "br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und denn durch kluge wahl den ausschlag recht bed\u00e4chte.", "tokens": ["Und", "denn", "durch", "klu\u00b7ge", "wahl", "den", "aus\u00b7schlag", "recht", "be\u00b7d\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich h\u00e4tte gern noch mehr in dieser schrifft ersehn;", "tokens": ["Ich", "h\u00e4t\u00b7te", "gern", "noch", "mehr", "in", "die\u00b7ser", "schrifft", "er\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein, die g\u00f6ttin gab das buch aus ihren h\u00e4nden,", "tokens": ["Al\u00b7lein", ",", "die", "g\u00f6t\u00b7tin", "gab", "das", "buch", "aus", "ih\u00b7ren", "h\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "NE", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sprach: wo soll ich mich bey dieser zeit hinwenden?", "tokens": ["Und", "sprach", ":", "wo", "soll", "ich", "mich", "bey", "die\u00b7ser", "zeit", "hin\u00b7wen\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VMFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie wird mir zweiffelnder im kummer doch geschehn?", "tokens": ["Wie", "wird", "mir", "zweif\u00b7feln\u00b7der", "im", "kum\u00b7mer", "doch", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die klugheit pfleget offt in ihrer kunst zu fehlen,", "tokens": ["Die", "klug\u00b7heit", "pfle\u00b7get", "offt", "in", "ih\u00b7rer", "kunst", "zu", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn sie aus zweyen will das allerbeste wehlen.", "tokens": ["Wenn", "sie", "aus", "zwe\u00b7yen", "will", "das", "al\u00b7ler\u00b7bes\u00b7te", "weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VVINF", "VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hilff GOtt! was soll ich thun? JA ist zwar kurtz und klein,", "tokens": ["Hilff", "Gott", "!", "was", "soll", "ich", "thun", "?", "Ja", "ist", "zwar", "kurtz", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$.", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach aber! allerdings mit schwierigkeit verbunden;", "tokens": ["Ach", "a\u00b7ber", "!", "al\u00b7ler\u00b7dings", "mit", "schwie\u00b7rig\u00b7keit", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und NEJN schlitzt manchesmal gar vielen tiefe wunden,", "tokens": ["Und", "NeJN", "schlitzt", "man\u00b7ches\u00b7mal", "gar", "vie\u00b7len", "tie\u00b7fe", "wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADV", "PIAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn das gel\u00fccke nicht wie vor, will g\u00fcnstig seyn.", "tokens": ["Wenn", "das", "ge\u00b7l\u00fc\u00b7cke", "nicht", "wie", "vor", ",", "will", "g\u00fcns\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PTKNEG", "KOKOM", "APPR", "$,", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie! soll ich wol mein kind, die Margaris, mein leben,", "tokens": ["Wie", "!", "soll", "ich", "wol", "mein", "kind", ",", "die", "Mar\u00b7ga\u00b7ris", ",", "mein", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "PRELS", "NE", "$,", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem, den man mir genennt, zu seiner braut hingeben?", "tokens": ["Dem", ",", "den", "man", "mir", "ge\u00b7nennt", ",", "zu", "sei\u00b7ner", "braut", "hin\u00b7ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIS", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ein ieder lobet ihn von seiner sittsamkeit:", "tokens": ["Ein", "ie\u00b7der", "lo\u00b7bet", "ihn", "von", "sei\u00b7ner", "sitt\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Gott-geweihten sinn sp\u00fcrt man in allen thaten;", "tokens": ["Den", "Got\u00b7tge\u00b7weih\u00b7ten", "sinn", "sp\u00fcrt", "man", "in", "al\u00b7len", "tha\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIS", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In rechten wei\u00df er ihm und andern wohl zu rathen;", "tokens": ["In", "rech\u00b7ten", "wei\u00df", "er", "ihm", "und", "an\u00b7dern", "wohl", "zu", "ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "PPER", "KON", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die feder zeigt verstand, der mund bescheidenheit;", "tokens": ["Die", "fe\u00b7der", "zeigt", "ver\u00b7stand", ",", "der", "mund", "be\u00b7schei\u00b7den\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die treu und redligkeit r\u00fchmt ihn als einen Christen,", "tokens": ["Die", "treu", "und", "red\u00b7lig\u00b7keit", "r\u00fchmt", "ihn", "als", "ei\u00b7nen", "Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "In dem die falschheit nicht gewohnet ist zu nisten.", "tokens": ["In", "dem", "die", "falschheit", "nicht", "ge\u00b7woh\u00b7net", "ist", "zu", "nis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PTKNEG", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "---+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie war nach dem gantz still, und dr\u00fcckte hand auf hand,", "tokens": ["Sie", "war", "nach", "dem", "gantz", "still", ",", "und", "dr\u00fcck\u00b7te", "hand", "auf", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADV", "ADJD", "$,", "KON", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die seuffzer fiengen an in ihrer brust zu beten,", "tokens": ["Die", "seuff\u00b7zer", "fi\u00b7en\u00b7gen", "an", "in", "ih\u00b7rer", "brust", "zu", "be\u00