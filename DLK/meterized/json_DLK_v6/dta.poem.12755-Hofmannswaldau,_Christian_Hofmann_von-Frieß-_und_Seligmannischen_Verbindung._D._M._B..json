, "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn gleichheit und verstand das hochzeit-bette drechseln,", "tokens": ["Wenn", "gleich\u00b7heit", "und", "ver\u00b7stand", "das", "hoch\u00b7zeit\u00b7bet\u00b7te", "drech\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das gegen neid und sturm auf marmor-seulen steht.", "tokens": ["Das", "ge\u00b7gen", "neid", "und", "sturm", "auf", "mar\u00b7mor\u00b7seu\u00b7len", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "KON", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum lasset mir mein kind und auch den Doctor holen,", "tokens": ["Drum", "las\u00b7set", "mir", "mein", "kind", "und", "auch", "den", "Doc\u00b7tor", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die gottes-furcht reicht ihm der liebe hei\u00dfe kohlen.", "tokens": ["Die", "got\u00b7tes\u00b7furcht", "reicht", "ihm", "der", "lie\u00b7be", "hei\u00b7\u00dfe", "koh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Ich sahe dich erfreut, du Hochbegl\u00fccktes Paar!", "tokens": ["Ich", "