gen.", "tokens": ["Auf", "dich", ",", "mein", "lie\u00b7bes", "Kind", "!", "und", "dei\u00b7nen", "Liebs\u00b7ten", "le\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}