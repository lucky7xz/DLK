{"textgrid.poem.34600": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was fluchst du/ Sylvia/ wenn meine schwartze hand", "tokens": ["Was", "fluchst", "du", "/", "Syl\u00b7via", "/", "wenn", "mei\u00b7ne", "schwart\u00b7ze", "hand"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$(", "NE", "$(", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um deinen busen spielet?", "tokens": ["Um", "dei\u00b7nen", "bu\u00b7sen", "spie\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie war so wei\u00df als du/ eh' sie der liebe brand/", "tokens": ["Sie", "war", "so", "wei\u00df", "als", "du", "/", "eh'", "sie", "der", "lie\u00b7be", "brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "KOUS", "PPER", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deine macht gef\u00fchlet.", "tokens": ["Und", "dei\u00b7ne", "macht", "ge\u00b7f\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Fl\u00f6stu das feuer nun in meine glieder ein/", "tokens": ["Fl\u00f6s\u00b7tu", "das", "feu\u00b7er", "nun", "in", "mei\u00b7ne", "glie\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan ja meine hand nicht schnee und marmel seyn.", "tokens": ["So", "kan", "ja", "mei\u00b7ne", "hand", "nicht", "schnee", "und", "mar\u00b7mel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du sprichst: Sie hat hier nichts zu suchen und zu thun.", "tokens": ["Du", "sprichst", ":", "Sie", "hat", "hier", "nichts", "zu", "su\u00b7chen", "und", "zu", "thun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gar recht; Es soll auch bleiben.", "tokens": ["Gar", "recht", ";", "Es", "soll", "auch", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sie suchet nichts als dich/ sie w\u00fcnschet blo\u00df zu ruhn", "tokens": ["Sie", "su\u00b7chet", "nichts", "als", "dich", "/", "sie", "w\u00fcn\u00b7schet", "blo\u00df", "zu", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "PPER", "$(", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ihren schertz zu treiben.", "tokens": ["Und", "ih\u00b7ren", "schertz", "zu", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Was ursach hast du dann, da\u00df du dich so beklagst?", "tokens": ["Was", "ur\u00b7sach", "hast", "du", "dann", ",", "da\u00df", "du", "dich", "so", "be\u00b7klagst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da du doch diese gunst den fl\u00f6hen nicht versagst.", "tokens": ["Da", "du", "doch", "die\u00b7se", "gunst", "den", "fl\u00f6\u00b7hen", "nicht", "ver\u00b7sagst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "NN", "ART", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}