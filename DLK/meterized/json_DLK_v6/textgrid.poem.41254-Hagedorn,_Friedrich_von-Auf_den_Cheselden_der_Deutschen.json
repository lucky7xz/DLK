{"textgrid.poem.41254": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Cheselden der Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lebe Carpser lang! er zieret unsre Zeiten.", "tokens": ["Es", "le\u00b7be", "Carp\u00b7ser", "lang", "!", "er", "zie\u00b7ret", "uns\u00b7re", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00fcnscht Aerzten seine Kunst, und K\u00f6nigen sein Herz!", "tokens": ["W\u00fcnscht", "A\u00b7erz\u00b7ten", "sei\u00b7ne", "Kunst", ",", "und", "K\u00f6\u00b7ni\u00b7gen", "sein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "$,", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Sein Anblick selbst erquickt, die Schwermut hemmt sein Scherz,", "tokens": ["Sein", "An\u00b7blick", "selbst", "er\u00b7quickt", ",", "die", "Schwer\u00b7mut", "hemmt", "sein", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und er vergi\u00dft sonst nichts, als seine G\u00fctigkeiten.", "tokens": ["Und", "er", "ver\u00b7gi\u00dft", "sonst", "nichts", ",", "als", "sei\u00b7ne", "G\u00fc\u00b7tig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}