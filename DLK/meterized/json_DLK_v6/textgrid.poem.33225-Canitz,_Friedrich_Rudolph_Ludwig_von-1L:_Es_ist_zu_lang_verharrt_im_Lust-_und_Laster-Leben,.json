nct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "PRELS", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Thor st\u00fcrtzt sich hinein:", "tokens": ["Ein", "Thor", "st\u00fcrtzt", "sich", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Weise suchet sich durch Vorsicht zu entziehen,", "tokens": ["Der", "Wei\u00b7se", "su\u00b7chet", "sich", "durch", "Vor\u00b7sicht", "zu", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und f\u00e4llt doch auch darein.", "tokens": ["Und", "f\u00e4llt", "doch", "auch", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "In dieser Zeitlichkeit kan es nicht anderst werden,", "tokens": ["In", "die\u00b7ser", "Zeit\u00b7lich\u00b7keit", "kan", "es", "nicht", "an\u00b7derst", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drum, Seele, sey bem\u00fcht,", "tokens": ["Drum", ",", "See\u00b7le", ",", "sey", "be\u00b7m\u00fcht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\