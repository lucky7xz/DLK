{"textgrid.poem.33225": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist zu lang verharrt im Lust- und Laster-Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1676, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist zu lang verharrt im Lust- und Laster-Leben,", "tokens": ["Es", "ist", "zu", "lang", "ver\u00b7harrt", "im", "Lust", "und", "Las\u00b7ter\u00b7Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVFIN", "APPRART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mir nun selbst mi\u00dff\u00e4llt;", "tokens": ["Das", "mir", "nun", "selbst", "mi\u00df\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich rei\u00df das Band entzwey, und will itzt Abschied geben", "tokens": ["Ich", "rei\u00df", "das", "Band", "ent\u00b7zwey", ",", "und", "will", "itzt", "Ab\u00b7schied", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VMFIN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Fleisch und auch der Welt.", "tokens": ["Dem", "Fleisch", "und", "auch", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Pracht ist eitler Dunst, und alles ihr Vergn\u00fcgen", "tokens": ["Ihr", "Pracht", "ist", "eit\u00b7ler", "Dunst", ",", "und", "al\u00b7les", "ihr", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,", "KON", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Schatten, Rauch und Schein;", "tokens": ["Nur", "Schat\u00b7ten", ",", "Rauch", "und", "Schein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil unter ihrer Lust verborgne Straffen liegen,", "tokens": ["Weil", "un\u00b7ter", "ih\u00b7rer", "Lust", "ver\u00b7borg\u00b7ne", "Straf\u00b7fen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die unvermeidlich seyn.", "tokens": ["Die", "un\u00b7ver\u00b7meid\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gantz einem andern Herrn will ich zu Dienste leben,", "tokens": ["Gantz", "ei\u00b7nem", "an\u00b7dern", "Herrn", "will", "ich", "zu", "Diens\u00b7te", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Leib, Hertz, Seel und Muth,", "tokens": ["Mit", "Leib", ",", "Hertz", ",", "Seel", "und", "Muth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der mir zum Gnaden-Lohn verspricht daf\u00fcr zu geben", "tokens": ["Der", "mir", "zum", "Gna\u00b7den\u00b7Lohn", "ver\u00b7spricht", "da\u00b7f\u00fcr", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das ewig-h\u00f6chste Gut.", "tokens": ["Das", "e\u00b7wig\u00b7h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hier ist doch kein Bestand, die Menschen m\u00fcssen sterben,", "tokens": ["Hier", "ist", "doch", "kein", "Be\u00b7stand", ",", "die", "Men\u00b7schen", "m\u00fcs\u00b7sen", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Welt-Bau selbst vergeht;", "tokens": ["Der", "Welt\u00b7Bau", "selbst", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Was heute kaum erzeugt, kan morgen schon verderben,", "tokens": ["Was", "heu\u00b7te", "kaum", "er\u00b7zeugt", ",", "kan", "mor\u00b7gen", "schon", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "$,", "VMFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nichts Zeitliches besteht.", "tokens": ["Nichts", "Zeit\u00b7li\u00b7ches", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich thu die Augen auf, und fliehe nun die Bande,", "tokens": ["Ich", "thu", "die", "Au\u00b7gen", "auf", ",", "und", "flie\u00b7he", "nun", "die", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mich so lang bestrickt.", "tokens": ["Die", "mich", "so", "lang", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df, da\u00df mich der Tod aus diesem Jammer-Lande,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "mich", "der", "Tod", "aus", "die\u00b7sem", "Jam\u00b7mer\u00b7Lan\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ins Freuden-Leben r\u00fcckt.", "tokens": ["Ins", "Freu\u00b7den\u00b7Le\u00b7ben", "r\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es ist ein kurtzer Schritt zum Grabe von der Wiegen,", "tokens": ["Es", "ist", "ein", "kurt\u00b7zer", "Schritt", "zum", "Gra\u00b7be", "von", "der", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tod schleicht gleich mit ein;", "tokens": ["Der", "Tod", "schleicht", "gleich", "mit", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der erste Tag, da wir in Mutter-Armen liegen,", "tokens": ["Der", "ers\u00b7te", "Tag", ",", "da", "wir", "in", "Mut\u00b7ter\u00b7Ar\u00b7men", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan auch der letzte seyn.", "tokens": ["Kan", "auch", "der", "letz\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Tod ehrt keine Zeit, ihm kan nichts wiederstehen,", "tokens": ["Der", "Tod", "ehrt", "kei\u00b7ne", "Zeit", ",", "ihm", "kan", "nichts", "wie\u00b7ders\u00b7te\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er achtet alles gleich.", "tokens": ["Er", "ach\u00b7tet", "al\u00b7les", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Klopft er, so mu\u00df der Herr, als wie der Diener, gehen", "tokens": ["Klopft", "er", ",", "so", "mu\u00df", "der", "Herr", ",", "als", "wie", "der", "Die\u00b7ner", ",", "ge\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VMFIN", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ins schwartze Schatten-Reich.", "tokens": ["Ins", "schwart\u00b7ze", "Schat\u00b7ten\u00b7Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er l\u00e4sset sich sehr offt an solchen Orten finden,", "tokens": ["Er", "l\u00e4s\u00b7set", "sich", "sehr", "offt", "an", "sol\u00b7chen", "Or\u00b7ten", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo man ihn sucht zu fliehn;", "tokens": ["Wo", "man", "ihn", "sucht", "zu", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schont dich in der Schlacht, und rei\u00dft dich wohl in S\u00fcnden", "tokens": ["Er", "schont", "dich", "in", "der", "Schlacht", ",", "und", "rei\u00dft", "dich", "wohl", "in", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Tisch und Bette hin.", "tokens": ["Von", "Tisch", "und", "Bet\u00b7te", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dein eigen Hau\u00df, worinn du dich gem\u00e4chlich pflegest,", "tokens": ["Dein", "ei\u00b7gen", "Hau\u00df", ",", "wo\u00b7rinn", "du", "dich", "ge\u00b7m\u00e4ch\u00b7lich", "pfle\u00b7gest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sey gro\u00df oder klein,", "tokens": ["Es", "sey", "gro\u00df", "o\u00b7der", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kan, wie dein Schwerdt, das du zu deinem Schutze tr\u00e4gest,", "tokens": ["Kan", ",", "wie", "dein", "Schwerdt", ",", "das", "du", "zu", "dei\u00b7nem", "Schut\u00b7ze", "tr\u00e4\u00b7gest", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWAV", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Sarg, dein M\u00f6rder seyn.", "tokens": ["Dein", "Sarg", ",", "dein", "M\u00f6r\u00b7der", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wo man die h\u00f6chste Lust allhier zu finden meinet,", "tokens": ["Wo", "man", "die", "h\u00f6chs\u00b7te", "Lust", "all\u00b7hier", "zu", "fin\u00b7den", "mei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da steckt die gr\u00f6ste Noht.", "tokens": ["Da", "steckt", "die", "gr\u00f6s\u00b7te", "Noht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja selbst die Artzeney, die dir so heilsam scheinet,", "tokens": ["Ja", "selbst", "die", "Art\u00b7ze\u00b7ney", ",", "die", "dir", "so", "heil\u00b7sam", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verursacht deinen Tod.", "tokens": ["Ver\u00b7ur\u00b7sacht", "dei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Himmel selbst, der fr\u00fch mit Seegen dich bethauet,", "tokens": ["Der", "Him\u00b7mel", "selbst", ",", "der", "fr\u00fch", "mit", "See\u00b7gen", "dich", "be\u00b7thau\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zieht Abends Wolcken an,", "tokens": ["Zieht", "A\u00b7bends", "Wol\u00b7cken", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und richtet Donner zu, der dir von ferne drauet,", "tokens": ["Und", "rich\u00b7tet", "Don\u00b7ner", "zu", ",", "der", "dir", "von", "fer\u00b7ne", "drau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dich leicht treffen kan.", "tokens": ["Und", "dich", "leicht", "tref\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Nichts ist in der Natur, so nicht dein Grab kan werden;", "tokens": ["Nichts", "ist", "in", "der", "Na\u00b7tur", ",", "so", "nicht", "dein", "Grab", "kan", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "$,", "ADV", "PTKNEG", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein jedes Element", "tokens": ["Ein", "je\u00b7des", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das dich erhalten soll, Lufft, Wasser, Feuer, Erden,", "tokens": ["Das", "dich", "er\u00b7hal\u00b7ten", "soll", ",", "Lufft", ",", "Was\u00b7ser", ",", "Feu\u00b7er", ",", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "PRF", "VVINF", "VMFIN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beschleunigt auch dein End.", "tokens": ["Be\u0