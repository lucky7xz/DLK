{"textgrid.poem.58492": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "530. Hinein und stille sein!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, kehr dich doch hinein, da scheint die Gnadensonne,", "tokens": ["Mensch", ",", "kehr", "dich", "doch", "hin\u00b7ein", ",", "da", "scheint", "die", "Gna\u00b7den\u00b7son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da beut dir Gott sein Herz, sein Leben, Licht und Wonne!", "tokens": ["Da", "beut", "dir", "Gott", "sein", "Herz", ",", "sein", "Le\u00b7ben", ",", "Licht", "und", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du stehst der Sonn' im Weg durch hundert sch\u00f6ne Sachen,", "tokens": ["Du", "stehst", "der", "Sonn'", "im", "Weg", "durch", "hun\u00b7dert", "sch\u00f6\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du sinnest, willst und machst; schweig Gotte, la\u00df ihn machen,", "tokens": ["Du", "sin\u00b7nest", ",", "willst", "und", "machst", ";", "schweig", "Got\u00b7te", ",", "la\u00df", "ihn", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "KON", "VVFIN", "$.", "ADJD", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Halt still dein tr\u00fcbes W\u00e4sserlein,", "tokens": ["Halt", "still", "dein", "tr\u00fc\u00b7bes", "W\u00e4s\u00b7ser\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sonne strahlt sonst nicht hinein!", "tokens": ["Die", "Son\u00b7ne", "strahlt", "sonst", "nicht", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}