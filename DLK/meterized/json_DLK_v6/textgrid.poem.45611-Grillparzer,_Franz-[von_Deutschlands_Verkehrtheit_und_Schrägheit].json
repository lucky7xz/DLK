{"textgrid.poem.45611": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[von Deutschlands Verkehrtheit und Schr\u00e4gheit]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Deutschlands Verkehrtheit und Schr\u00e4gheit", "tokens": ["Von", "Deutschlands", "Ver\u00b7kehrt\u00b7heit", "und", "Schr\u00e4g\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist der Grund das Gesetz der Tr\u00e4gheit.", "tokens": ["Ist", "der", "Grund", "das", "Ge\u00b7setz", "der", "Tr\u00e4g\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Jahrhunderte unbeweglich,", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te", "un\u00b7be\u00b7weg\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geschah kaum der erste Sto\u00df,", "tokens": ["Ge\u00b7schah", "kaum", "der", "ers\u00b7te", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So schwingen sie st\u00fcndlich und t\u00e4glich", "tokens": ["So", "schwin\u00b7gen", "sie", "st\u00fcnd\u00b7lich", "und", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und werden das Wechseln nicht los.", "tokens": ["Und", "wer\u00b7den", "das", "Wech\u00b7seln", "nicht", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}