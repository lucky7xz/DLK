{"dta.poem.22774": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "80. Mit Lieb\u2019 erlangt man Gnad.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wan\u0303 dich der S\u00fcnder fragt wie er sol Gnad erlange\u0303.", "tokens": ["Wa\u00f1", "dich", "der", "S\u00fcn\u00b7der", "fragt", "wie", "er", "sol", "Gnad", "er\u00b7lang\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "KOKOM", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sage da\u00df er GOtt zulieben an sol fangen.", "tokens": ["So", "sa\u00b7ge", "da\u00df", "er", "Gott", "zu\u00b7lie\u00b7ben", "an", "sol", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "NN", "ADV", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}