{"textgrid.poem.49473": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Juni", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Monat der Liebe, Monat Jesu, gold und rot,", "tokens": ["Mo\u00b7nat", "der", "Lie\u00b7be", ",", "Mo\u00b7nat", "Je\u00b7su", ",", "gold", "und", "rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NE", "NE", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Juni, dir entfaltet sich in lichtem Scheine", "tokens": ["O", "Ju\u00b7ni", ",", "dir", "ent\u00b7fal\u00b7tet", "sich", "in", "lich\u00b7tem", "Schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Seele Blume und das Herz, das flammend loht", "tokens": ["Der", "See\u00b7le", "Blu\u00b7me", "und", "das", "Herz", ",", "das", "flam\u00b7mend", "loht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie br\u00e4utlicher Gesang und D\u00fcfte s\u00fcsser Reine.", "tokens": ["Wie", "br\u00e4ut\u00b7li\u00b7cher", "Ge\u00b7sang", "und", "D\u00fcf\u00b7te", "s\u00fcs\u00b7ser", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du Fest des heil'gen Herzens, o Fronleichnamstag,", "tokens": ["Du", "Fest", "des", "heil'\u00b7gen", "Her\u00b7zens", ",", "o", "Fron\u00b7leich\u00b7nams\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch g\u00f6ttlich echtes Blut und Fleisch geweihte Zeiten!", "tokens": ["Durch", "g\u00f6tt\u00b7lich", "ech\u00b7tes", "Blut", "und", "Fleisch", "ge\u00b7weih\u00b7te", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Sieg des Sommers lacht der \u00fcpp'ge gr\u00fcne Hag,", "tokens": ["Im", "Sieg", "des", "Som\u00b7mers", "lacht", "der", "\u00fcpp'\u00b7ge", "gr\u00fc\u00b7ne", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und es erstickt der Lolch in korndurchwogten Weiten.", "tokens": ["Und", "es", "er\u00b7stickt", "der", "Lolch", "in", "korn\u00b7durch\u00b7wog\u00b7ten", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und uns die S\u00fcnder, uns die ganz Verlornen weiht", "tokens": ["Und", "uns", "die", "S\u00fcn\u00b7der", ",", "uns", "die", "ganz", "Ver\u00b7lor\u00b7nen", "weiht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "$,", "PPER", "ART", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von neuem die Allgegenwart, die g\u00f6ttlich hehre.", "tokens": ["Von", "neu\u00b7em", "die", "All\u00b7ge\u00b7gen\u00b7wart", ",", "die", "g\u00f6tt\u00b7lich", "heh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wir f\u00fchlen uns gest\u00e4rkt zu neuem, hartem Streit", "tokens": ["Wir", "f\u00fch\u00b7len", "uns", "ge\u00b7st\u00e4rkt", "zu", "neu\u00b7em", ",", "har\u00b7tem", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Satan und zu neuer, sieggekr\u00f6nter Ehre.", "tokens": ["Mit", "Sa\u00b7tan", "und", "zu", "neu\u00b7er", ",", "sieg\u00b7ge\u00b7kr\u00f6n\u00b7ter", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und uns bewacht vom Himmel her und vom Altar", "tokens": ["Und", "uns", "be\u00b7wacht", "vom", "Him\u00b7mel", "her", "und", "vom", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "APPRART", "NN", "PTKVZ", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die angebetete, die reine, blut'ge Liebe.", "tokens": ["Die", "an\u00b7ge\u00b7be\u00b7te\u00b7te", ",", "die", "rei\u00b7ne", ",", "blut'\u00b7ge", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In schmerzensvoller Brust f\u00fchlt Hoffnung zart und klar", "tokens": ["In", "schmer\u00b7zens\u00b7vol\u00b7ler", "Brust", "f\u00fchlt", "Hoff\u00b7nung", "zart", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Herz, das gl\u00fch'nde Herz, durchbebt vom heil'gen Triebe,", "tokens": ["Das", "Herz", ",", "das", "gl\u00fch'n\u00b7de", "Herz", ",", "durch\u00b7bebt", "vom", "heil'\u00b7gen", "Trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Unseren zu retten, gnadenreiche Macht", "tokens": ["Die", "Un\u00b7se\u00b7ren", "zu", "ret\u00b7ten", ",", "gna\u00b7den\u00b7rei\u00b7che", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der G\u00fcte, die f\u00fcr uns den Sieg errang im Kampfe.", "tokens": ["Der", "G\u00fc\u00b7te", ",", "die", "f\u00fcr", "uns", "den", "Sieg", "er\u00b7rang", "im", "Kamp\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mystisch steigt der Weihrauch tiefer Sommerpracht", "tokens": ["Und", "mys\u00b7tisch", "steigt", "der", "Weih\u00b7rauch", "tie\u00b7fer", "Som\u00b7mer\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Glorreich zum Himmel auf in stillem Opferdampfe.", "tokens": ["Glor\u00b7reich", "zum", "Him\u00b7mel", "auf", "in", "stil\u00b7lem", "Op\u00b7fer\u00b7damp\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}