{"textgrid.poem.65018": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kennst mein tiefstes Wesen nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kennst mein tiefstes Wesen nicht,", "tokens": ["Du", "kennst", "mein", "tiefs\u00b7tes", "We\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kennst den Zweck nicht meiner Sendung,", "tokens": ["Und", "kennst", "den", "Zweck", "nicht", "mei\u00b7ner", "Sen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verlangest du, da\u00df mein Gedicht", "tokens": ["Ver\u00b7lan\u00b7gest", "du", ",", "da\u00df", "mein", "Ge\u00b7dicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Anstrebe freudige Vollendung.", "tokens": ["An\u00b7stre\u00b7be", "freu\u00b7di\u00b7ge", "Vol\u00b7len\u00b7dung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+--+--", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Nein! Gott hat mich nicht ausgesandt", "tokens": ["Nein", "!", "Gott", "hat", "mich", "nicht", "aus\u00b7ge\u00b7sandt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat die Kraft mir nicht gegeben,", "tokens": ["Und", "hat", "die", "Kraft", "mir", "nicht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um glorreich, mit geweihter Hand", "tokens": ["Um", "glor\u00b7reich", ",", "mit", "ge\u00b7weih\u00b7ter", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sieges Palmen zu erstreben.", "tokens": ["Des", "Sie\u00b7ges", "Pal\u00b7men", "zu", "er\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Marmor prange und in Erz,", "tokens": ["In", "Mar\u00b7mor", "pran\u00b7ge", "und", "in", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Name de\u00df, der sie erstritten.", "tokens": ["Der", "Na\u00b7me", "de\u00df", ",", "der", "sie", "er\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin nichts weiter als ein Herz,", "tokens": ["Ich", "bin", "nichts", "wei\u00b7ter", "als", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das viel geliebt und viel gelitten.", "tokens": ["Das", "viel", "ge\u00b7liebt", "und", "viel", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und meine ganze Poesie", "tokens": ["Und", "mei\u00b7ne", "gan\u00b7ze", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist nur ein lautes Offenbaren", "tokens": ["Ist", "nur", "ein", "lau\u00b7tes", "Of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von all den stillen Schmerzen, die", "tokens": ["Von", "all", "den", "stil\u00b7len", "Schmer\u00b7zen", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Weibes Seele kann erfahren.", "tokens": ["Des", "Wei\u00b7bes", "See\u00b7le", "kann", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl w\u00e4r' es doppelt sch\u00f6n und gro\u00df", "tokens": ["Wohl", "w\u00e4r'", "es", "dop\u00b7pelt", "sch\u00f6n", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als starker Tr\u00f6ster zu erscheinen,", "tokens": ["Als", "star\u00b7ker", "Tr\u00f6s\u00b7ter", "zu", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ich, ach! ich verstehe blo\u00df", "tokens": ["Doch", "ich", ",", "ach", "!", "ich", "ver\u00b7ste\u00b7he", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ITJ", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit der bedr\u00e4ngten Schaar zu weinen. \u2013", "tokens": ["Mit", "der", "be\u00b7dr\u00e4ng\u00b7ten", "Schaar", "zu", "wei\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was einst Johannes sprach, das spricht", "tokens": ["Was", "einst", "Jo\u00b7han\u00b7nes", "sprach", ",", "das", "spricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "NE", "VVFIN", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz ihm nach mit leisem Beben:", "tokens": ["Mein", "Herz", "ihm", "nach", "mit", "lei\u00b7sem", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich selbst bin der Messias nicht", "tokens": ["Ich", "selbst", "bin", "der", "Mes\u00b7si\u00b7as", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "PTKNEG"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und soll von ihm nur Zeugni\u00df geben.", "tokens": ["Und", "soll", "von", "ihm", "nur", "Zeug\u00b7ni\u00df", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Licht, das l\u00e4uternd und verkl\u00e4rt,", "tokens": ["Das", "Licht", ",", "das", "l\u00e4u\u00b7ternd", "und", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Strahl der Weihe soll entz\u00fcnden,", "tokens": ["Den", "Strahl", "der", "Wei\u00b7he", "soll", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin es nicht und bin kaum werth", "tokens": ["Ich", "bin", "es", "nicht", "und", "bin", "kaum", "werth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "KON", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch nur sein Nahen zu verk\u00fcnden.", "tokens": ["Euch", "nur", "sein", "Na\u00b7hen", "zu", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Amt, das mir der Herr beschied,", "tokens": ["Das", "Amt", ",", "das", "mir", "der", "Herr", "be\u00b7schied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu er Kraft verlieh der Schwachen,", "tokens": ["Wo\u00b7zu", "er", "Kraft", "ver\u00b7lieh", "der", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein andres ist's als durch das Lied", "tokens": ["Kein", "and\u00b7res", "ist's", "als", "durch", "das", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PIS", "VAFIN", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sehnsucht br\u00fcnst'ger anzufachen.", "tokens": ["Die", "Sehn\u00b7sucht", "br\u00fcnst'\u00b7ger", "an\u00b7zu\u00b7fa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wenn euch klar, was ihr vermi\u00dft,", "tokens": ["Und", "wenn", "euch", "klar", ",", "was", "ihr", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn euer Geist verst\u00f6rt, beklommen", "tokens": ["Wenn", "eu\u00b7er", "Geist", "ver\u00b7st\u00f6rt", ",", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Abgrunds Tiefe ganz ermi\u00dft,", "tokens": ["Des", "Ab\u00b7grunds", "Tie\u00b7fe", "ganz", "er\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann wird vielleicht der Tr\u00f6ster kommen!", "tokens": ["Dann", "wird", "viel\u00b7leicht", "der", "Tr\u00f6s\u00b7ter", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}