{"textgrid.poem.39668": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja Quitos Hand, hat meine Hand ber\u00fchret", "tokens": ["Ja", "Qui\u00b7tos", "Hand", ",", "hat", "mei\u00b7ne", "Hand", "be\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "NN", "NN", "$,", "VAFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und freundlich zu den Lippen sie gef\u00fchret,", "tokens": ["Und", "freund\u00b7lich", "zu", "den", "Lip\u00b7pen", "sie", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An meinem Busen hat sein Haupt geruht.", "tokens": ["An", "mei\u00b7nem", "Bu\u00b7sen", "hat", "sein", "Haupt", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da f\u00fchlt ich tief ein liebend fromm Ergeben.", "tokens": ["Da", "f\u00fchlt", "ich", "tief", "ein", "lie\u00b7bend", "fromm", "Er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00dft ich dich \u00fcberleben, sch\u00f6nes Leben?", "tokens": ["Mu\u00dft", "ich", "dich", "\u00fc\u00b7berl\u00b7e\u00b7ben", ",", "sch\u00f6\u00b7nes", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch Zukunft haben, da du keine hast?", "tokens": ["Noch", "Zu\u00b7kunft", "ha\u00b7ben", ",", "da", "du", "kei\u00b7ne", "hast", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$,", "KOUS", "PPER", "PIAT", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Zeitenstrome wirst du mir erbleichen,", "tokens": ["Im", "Zei\u00b7ten\u00b7stro\u00b7me", "wirst", "du", "mir", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "St\u00fcrb ich mit dir, wie bei der Sonne Neigen", "tokens": ["St\u00fcrb", "ich", "mit", "dir", ",", "wie", "bei", "der", "Son\u00b7ne", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Farben all' in dunkler Nacht vergehn.", "tokens": ["Die", "Far\u00b7ben", "all'", "in", "dunk\u00b7ler", "Nacht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}