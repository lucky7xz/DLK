{"dta.poem.10897": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat man jemals die Natur, mit der Kunst vereint, gesehn:", "tokens": ["Hat", "man", "je\u00b7mals", "die", "Na\u00b7tur", ",", "mit", "der", "Kunst", "ver\u00b7eint", ",", "ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "So geschichts auf diesen Bl\u00e4ttern. Da\u00df, mit etwas schwar-", "tokens": ["So", "ge\u00b7schichts", "auf", "die\u00b7sen", "Bl\u00e4t\u00b7tern", ".", "Da\u00df", ",", "mit", "et\u00b7was", "schwa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$.", "KOUS", "$,", "APPR", "PIAT", "TRUNC"], "meter": "+-+-+-+---+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Alles, was auf dieser Welt wundersch\u00f6n, so wundersch\u00f6n", "tokens": ["Al\u00b7les", ",", "was", "auf", "die\u00b7ser", "Welt", "wun\u00b7der\u00b7sch\u00f6n", ",", "so", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "$,", "PRELS", "APPR", "PDAT", "NN", "ADJD", "$,", "ADV", "ADJD"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Vorgestellt, gef\u00e4rbt, gebildet, ja fast gar belebet werde;", "tokens": ["Vor\u00b7ge\u00b7stellt", ",", "ge\u00b7f\u00e4rbt", ",", "ge\u00b7bil\u00b7det", ",", "ja", "fast", "gar", "be\u00b7le\u00b7bet", "wer\u00b7de", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVPP", "$,", "ADV", "ADV", "ADV", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Ist so k\u00fcnst-als unbegreiflich. Schaut den nah-und fernen Wald!", "tokens": ["Ist", "so", "k\u00fcnst\u00b7als", "un\u00b7be\u00b7greif\u00b7lich", ".", "Schaut", "den", "nah\u00b7\u00b7und", "fer\u00b7nen", "Wald", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$.", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Es vertieft der Blick sich hier, in den weit entlegnen B\u00fcschen,", "tokens": ["Es", "ver\u00b7tieft", "der", "Blick", "sich", "hier", ",", "in", "den", "weit", "ent\u00b7leg\u00b7nen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADV", "$,", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wo sich, mit gebrochnen Lichtern, Schlag - und andre Schatten", "tokens": ["Wo", "sich", ",", "mit", "ge\u00b7broch\u00b7nen", "Lich\u00b7tern", ",", "Schlag", "und", "and\u00b7re", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "$,", "APPR", "ADJA", "NN", "$,", "NN", "$(", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "An des bunten Birkenstammes Riesen-f\u00f6rmige Gestalt,", "tokens": ["An", "des", "bun\u00b7ten", "Bir\u00b7kens\u00b7tam\u00b7mes", "Rie\u00b7sen\u00b7f\u00f6r\u00b7mi\u00b7ge", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Freut sich mein erstauntes Aug. In geruhigem Vergn\u00fcgen,", "tokens": ["Freut", "sich", "mein", "er\u00b7staun\u00b7tes", "Aug.", "In", "ge\u00b7ru\u00b7hi\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Sieher man, in weichem Gra\u00df, ein, dem Schein nach, lebend Bild;", "tokens": ["Sie\u00b7her", "man", ",", "in", "wei\u00b7chem", "Gra\u00df", ",", "ein", ",", "dem", "Schein", "nach", ",", "le\u00b7bend", "Bild", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$,", "ADJD", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ein, mit seinem zarten K\u00e4lbchen, wiederkauendes St\u00fcck Wild,", "tokens": ["Ein", ",", "mit", "sei\u00b7nem", "zar\u00b7ten", "K\u00e4lb\u00b7chen", ",", "wie\u00b7der\u00b7kau\u00b7en\u00b7des", "St\u00fcck", "Wild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Das, in seiner jungen Einfalt, die\u00df, mit ernster Vorsicht, liegen.", "tokens": ["Das", ",", "in", "sei\u00b7ner", "jun\u00b7gen", "Ein\u00b7falt", ",", "die\u00df", ",", "mit", "erns\u00b7ter", "Vor\u00b7sicht", ",", "lie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PDS", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Des behaarten feisten Spiesserts rasch und muntre Fl\u00fcchtigkeit", "tokens": ["Des", "be\u00b7haar\u00b7ten", "feis\u00b7ten", "Spies\u00b7serts", "rasch", "und", "mun\u00b7tre", "Fl\u00fcch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADJD", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Zeigen die benervten Schenkel. Wer bewundert nicht, in ihnen,", "tokens": ["Zei\u00b7gen", "die", "be\u00b7nerv\u00b7ten", "Schen\u00b7kel", ".", "Wer", "be\u00b7wun\u00b7dert", "nicht", ",", "in", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "PWS", "VVFIN", "PTKNEG", "$,", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Da das Urbild und der Abdruck uns zur Lust und Nahrung", "tokens": ["Da", "das", "Ur\u00b7bild", "und", "der", "Ab\u00b7druck", "uns", "zur", "Lust", "und", "Nah\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "PPER", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "Unsers Sch\u00f6pfers sch\u00f6ne Werke, und der Kunst Vollkommenheit?", "tokens": ["Un\u00b7sers", "Sch\u00f6p\u00b7fers", "sch\u00f6\u00b7ne", "Wer\u00b7ke", ",", "und", "der", "Kunst", "Voll\u00b7kom\u00b7men\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}