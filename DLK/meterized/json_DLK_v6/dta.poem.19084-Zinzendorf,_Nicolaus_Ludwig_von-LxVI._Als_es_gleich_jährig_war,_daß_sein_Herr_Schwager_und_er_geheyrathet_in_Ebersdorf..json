{"dta.poem.19084": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxVI.  Als es gleich j\u00e4hrig war, da\u00df sein  \n Herr Schwager und er geheyrathet  \n in Ebersdorf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.57", "da:0.42"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Gottes F\u00fchrung fodert Stille:", "tokens": ["Got\u00b7tes", "F\u00fch\u00b7rung", "fo\u00b7dert", "Stil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Fu\u00df noch selber rauscht,", "tokens": ["Wo", "der", "Fu\u00df", "noch", "sel\u00b7ber", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird des ewgen Vaters Wille,", "tokens": ["Wird", "des", "ew\u00b7gen", "Va\u00b7ters", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der eignen Wahl vertauscht.", "tokens": ["Mit", "der", "eig\u00b7nen", "Wahl", "ver\u00b7tauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer da leben will, der sterbe:", "tokens": ["Wer", "da", "le\u00b7ben", "will", ",", "der", "ster\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer nicht stirbt, der lebet nicht:", "tokens": ["Wer", "nicht", "stirbt", ",", "der", "le\u00b7bet", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehe denn das Fleisch verderbe,", "tokens": ["E\u00b7he", "denn", "das", "Fleisch", "ver\u00b7der\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheinet uns kein wahres Licht.", "tokens": ["Schei\u00b7net", "uns", "kein", "wah\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was die andern Menschen wollen,", "tokens": ["Was", "die", "an\u00b7dern", "Men\u00b7schen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft der Sch\u00f6pfer noch geschehn:", "tokens": ["L\u00e4\u00dft", "der", "Sch\u00f6p\u00b7fer", "noch", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, wenn die Kinder schmollen,", "tokens": ["A\u00b7ber", ",", "wenn", "die", "Kin\u00b7der", "schmol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft er sie die Ruthe sehn.", "tokens": ["L\u00e4\u00dft", "er", "sie", "die", "Ru\u00b7the", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle menschliche Gesch\u00e4fte", "tokens": ["Al\u00b7le", "menschli\u00b7che", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gehen \u00fcberhaupt nicht gut,", "tokens": ["Ge\u00b7hen", "\u00fc\u00b7ber\u00b7haupt", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo man sie durch eigne Kr\u00e4fte", "tokens": ["Wo", "man", "sie", "durch", "eig\u00b7ne", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht aus der Gnade thut.", "tokens": ["Und", "nicht", "aus", "der", "Gna\u00b7de", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "G\u00f6ttliche und innre Dinge", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "und", "inn\u00b7re", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lassen vollends gar nicht zu,", "tokens": ["Las\u00b7sen", "vol\u00b7lends", "gar", "nicht", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man sie mit Sturm erzwinge,", "tokens": ["Da\u00df", "man", "sie", "mit", "Sturm", "er\u00b7zwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern weisen uns zur Ruh.", "tokens": ["Son\u00b7dern", "wei\u00b7sen", "uns", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zeitlich, ewig, geist- und leiblich,", "tokens": ["Zeit\u00b7lich", ",", "e\u00b7wig", ",", "geist", "und", "leib\u00b7lich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "TRUNC", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beut sich oftermals die Hand:", "tokens": ["Beut", "sich", "of\u00b7ter\u00b7mals", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, wie so unbeschreiblich", "tokens": ["A\u00b7ber", ",", "wie", "so", "un\u00b7be\u00b7schreib\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlie\u00dft sichs an das Ehe-Band.", "tokens": ["Schlie\u00dft", "sichs", "an", "das", "E\u00b7he\u00b7Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Darum ist es unumg\u00e4nglich,", "tokens": ["Da\u00b7rum", "ist", "es", "un\u00b7um\u00b7g\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesus f\u00fchr uns erst hinein;", "tokens": ["Je\u00b7sus", "f\u00fchr", "uns", "erst", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will man hoffen \u00fcberschwenglich", "tokens": ["Will", "man", "hof\u00b7fen", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drinnen unterst\u00fctzt zu seyn.", "tokens": ["Drin\u00b7nen", "un\u00b7ter\u00b7st\u00fctzt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn wir uns nur richtig w\u00fcsten,", "tokens": ["Wenn", "wir", "uns", "nur", "rich\u00b7tig", "w\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Was die Regel anbelangt,", "tokens": ["Was", "die", "Re\u00b7gel", "an\u00b7be\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da der Br\u00e4utigam der Christen,", "tokens": ["Da", "der", "Br\u00e4u\u00b7ti\u00b7gam", "der", "Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vormals drinnen hergeprangt.", "tokens": ["Vor\u00b7mals", "drin\u00b7nen", "her\u00b7ge\u00b7prangt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nein! bey unserm Ehe-Bande,", "tokens": ["Nein", "!", "bey", "un\u00b7serm", "E\u00b7he\u00b7Ban\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich diesen Tag verneut,", "tokens": ["Das", "sich", "die\u00b7sen", "Tag", "ver\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist zu wenig Schmach nnd Schande,", "tokens": ["Ist", "zu", "we\u00b7nig", "Schmach", "nnd", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zu viel Gem\u00e4chlichkeit.", "tokens": ["Und", "zu", "viel", "Ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "H\u00f6chstes Vorbild aller Ehe,", "tokens": ["H\u00f6chs\u00b7tes", "Vor\u00b7bild", "al\u00b7ler", "E\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche heilig ist und rein:", "tokens": ["Wel\u00b7che", "hei\u00b7lig", "ist", "und", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine St\u00e4be, ", "tokens": ["Dei\u00b7ne", "St\u00e4\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Richten unsre Ehen ein.", "tokens": ["Rich\u00b7ten", "uns\u00b7re", "E\u00b7hen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Deine blutige Gestalten", "tokens": ["Dei\u00b7ne", "blu\u00b7ti\u00b7ge", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcssen unsern Ehestand", "tokens": ["M\u00fcs\u00b7sen", "un\u00b7sern", "E\u00b7hes\u00b7tand"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jmmer in den Schrancken halten:", "tokens": ["Jm\u00b7mer", "in", "den", "Schran\u00b7cken", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn wir sind dir nah verwand.", "tokens": ["Denn", "wir", "sind", "dir", "nah", "ver\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "Das bi\u00dfherige Versehen", "tokens": ["Das", "bi\u00df\u00b7he\u00b7ri\u00b7ge", "Ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uberfahre mit dem Blut,", "tokens": ["U\u00b7ber\u00b7fah\u00b7re", "mit", "dem", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das vor aller Welt Vergehen", "tokens": ["Das", "vor", "al\u00b7ler", "Welt", "Ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gnug und \u00fcberschwenglich thut.", "tokens": ["Gnug", "und", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "thut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "La", "tokens": ["La"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Was noch hinterstellig ist,", "tokens": ["Was", "noch", "hin\u00b7ter\u00b7stel\u00b7lig", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir in dem Lichte wandeln,", "tokens": ["Da\u00df", "wir", "in", "dem", "Lich\u00b7te", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr! wie du im Lichte bist.", "tokens": ["Herr", "!", "wie", "du", "im", "Lich\u00b7te", "bist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Unsern Stand la\u00df mit dem Glantze", "tokens": ["Un\u00b7sern", "Stand", "la\u00df", "mit", "dem", "Glant\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Deiner Kraft umfangen seyn,", "tokens": ["Dei\u00b7ner", "Kraft", "um\u00b7fan\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein iedes Kind zur Pflantze", "tokens": ["Und", "ein", "ie\u00b7des", "Kind", "zur", "Pflant\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Gerechtigkeit gedeyhn.", "tokens": ["Der", "Ge\u00b7rech\u00b7tig\u00b7keit", "ge\u00b7deyhn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "La\u00df uns nicht besch\u00e4met stehen,", "tokens": ["La\u00df", "uns", "nicht", "be\u00b7sch\u00e4\u00b7met", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann du Eh\u2019-Gerichte hegst;", "tokens": ["Wann", "du", "Eh'\u00b7Ge\u00b7rich\u00b7te", "hegst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern mit zur Hochzeit gehen,", "tokens": ["Son\u00b7dern", "mit", "zur", "Hoch\u00b7zeit", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo du zu bewirthen pflegst.", "tokens": ["Wo", "