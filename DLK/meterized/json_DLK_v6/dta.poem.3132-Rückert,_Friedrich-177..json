{"dta.poem.3132": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "177.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht immer noth, (der Meister hats gesprochen)", "tokens": ["Es", "ist", "nicht", "im\u00b7mer", "noth", ",", "(", "der", "Meis\u00b7ter", "hats", "ge\u00b7spro\u00b7chen", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Wahres werd' ein Leib, ein Leib mit Fleisch und Knochen.", "tokens": ["Da\u00df", "Wah\u00b7res", "werd'", "ein", "Leib", ",", "ein", "Leib", "mit", "Fleisch", "und", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VAFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn geistig in der Luft es schwebt, gen\u00fcgt es schon,", "tokens": ["Wenn", "geis\u00b7tig", "in", "der", "Luft", "es", "schwebt", ",", "ge\u00b7n\u00fcgt", "es", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie, Herzen stimmend, sanft und ernst, ein Glockenton.", "tokens": ["Wie", ",", "Her\u00b7zen", "stim\u00b7mend", ",", "sanft", "und", "ernst", ",", "ein", "Glo\u00b7cken\u00b7ton", "."], "token_info