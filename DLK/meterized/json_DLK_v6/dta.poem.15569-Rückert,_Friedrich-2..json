{"dta.poem.15569": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus der Vollkommenheit der Welt willst du beweisen", "tokens": ["Aus", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "der", "Welt", "willst", "du", "be\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Daseyn Eines, der sie h\u00e4lt in ihren Kreisen.", "tokens": ["Das", "Da\u00b7seyn", "Ei\u00b7nes", ",", "der", "sie", "h\u00e4lt", "in", "ih\u00b7ren", "Krei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und die Vollkommenheit der Welt in jeder Spur", "tokens": ["Und", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "der", "Welt", "in", "je\u00b7der", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beweisest du woraus? aus Jenes Daseyn nur.", "tokens": ["Be\u00b7wei\u00b7sest", "du", "wo\u00b7raus", "?", "aus", "Je\u00b7nes", "Da\u00b7seyn", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht schelt' ich den Beweis, da\u00df er sich dreht im Kreis;", "tokens": ["Nicht", "schelt'", "ich", "den", "Be\u00b7weis", ",", "da\u00df", "er", "sich", "dreht", "im", "Kreis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielmehr des Denkens Kreis dreht sich um den Beweis.", "tokens": ["Viel\u00b7mehr", "des", "Den\u00b7kens", "Kreis", "dreht", "sich", "um", "den", "Be\u00b7weis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie sch\u00f6n, da\u00df so voraus sich diese beiden setzen,", "tokens": ["Wie", "sch\u00f6n", ",", "da\u00df", "so", "vo\u00b7raus", "sich", "die\u00b7se", "bei\u00b7den", "set\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "ADV", "ADV", "PRF", "PDAT", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und du der dritte bist, daran dich zu ergetzen.", "tokens": ["Und", "du", "der", "drit\u00b7te", "bist", ",", "da\u00b7ran", "dich", "zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "VAFIN", "$,", "PAV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}