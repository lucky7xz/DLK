{"dta.poem.5604": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Ursprunge der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus den zwo Meinungen ist eine zu erlesen:", "tokens": ["Aus", "den", "zwo", "Mei\u00b7nun\u00b7gen", "ist", "ei\u00b7ne", "zu", "er\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "VAFIN", "ART", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Entweder zog ein w\u00fcrckend weises Wesen", "tokens": ["Ent\u00b7we\u00b7der", "zog", "ein", "w\u00fcr\u00b7ckend", "wei\u00b7ses", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt aus nichts, und gab ihr die Gestalt,", "tokens": ["Die", "Welt", "aus", "nichts", ",", "und", "gab", "ihr", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch ein\u2019 allm\u00e4chtige Gewalt.", "tokens": ["Durch", "ein'", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wo nicht; so hat ein Klump, der ungeheuer schwer,", "tokens": ["Wo", "nicht", ";", "so", "hat", "ein", "Klump", ",", "der", "un\u00b7ge\u00b7heu\u00b7er", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Chaos voller Dunckelheit,", "tokens": ["Ein", "Chaos", "vol\u00b7ler", "Dun\u00b7ckel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Vermischet und verwirrt von Ewigkeit,", "tokens": ["Ver\u00b7mi\u00b7schet", "und", "ver\u00b7wirrt", "von", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So mancher Sch\u00f6nheit Pracht, von ohngefehr", "tokens": ["So", "man\u00b7cher", "Sch\u00f6n\u00b7heit", "Pracht", ",", "von", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "NN", "$,", "APPR", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Empfangen und erlangt: Allein,", "tokens": ["Emp\u00b7fan\u00b7gen", "und", "er\u00b7langt", ":", "Al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wer hat den Klumpen doch, wer hat des Chaos Nacht,", "tokens": ["Wer", "hat", "den", "Klum\u00b7pen", "doch", ",", "wer", "hat", "des", "Chaos", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "$,", "PWS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "So von der g\u00e4nzlichen Natur der Quell soll seyn,", "tokens": ["So", "von", "der", "g\u00e4nz\u00b7li\u00b7chen", "Na\u00b7tur", "der", "Quell", "soll", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Aus dem verwirrten Stand hervorgebracht?", "tokens": ["Aus", "dem", "ver\u00b7wirr\u00b7ten", "Stand", "her\u00b7vor\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zog er sich selbst heraus? hat er sich selbst geschaffen?", "tokens": ["Zog", "er", "sich", "selbst", "he\u00b7raus", "?", "hat", "er", "sich", "selbst", "ge\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$.", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Soll er von uns mit einer ew\u2019gen Macht", "tokens": ["Soll", "er", "von", "uns", "mit", "ei\u00b7ner", "ew'\u00b7gen", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Beschencket und verherrlicht seyn?", "tokens": ["Be\u00b7schen\u00b7cket", "und", "ver\u00b7herr\u00b7licht", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nein, die Vernunfft geht dieses nimmer ein.", "tokens": ["Nein", ",", "die", "Ver\u00b7nunfft", "geht", "die\u00b7ses", "nim\u00b7mer", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "PDAT", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}