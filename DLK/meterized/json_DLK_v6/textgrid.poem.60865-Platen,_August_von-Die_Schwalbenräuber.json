{"textgrid.poem.60865": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwalbenr\u00e4uber", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwalben, unz\u00e4hlige, hatten sich rings um die H\u00fctte des Landmanns,", "tokens": ["Schwal\u00b7ben", ",", "un\u00b7z\u00e4h\u00b7li\u00b7ge", ",", "hat\u00b7ten", "sich", "rings", "um", "die", "H\u00fct\u00b7te", "des", "Land\u00b7manns", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "VAFIN", "PRF", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ob der erquicklichen Luft, Nester an Nester gebaut:", "tokens": ["Ob", "der", "er\u00b7quick\u00b7li\u00b7chen", "Luft", ",", "Nes\u00b7ter", "an", "Nes\u00b7ter", "ge\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Fromm zwar hegte die Guten der Greis; doch, als er entfernt war,", "tokens": ["Fromm", "zwar", "heg\u00b7te", "die", "Gu\u00b7ten", "der", "Greis", ";", "doch", ",", "als", "er", "ent\u00b7fernt", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "R\u00fcckte die Leiter der Sohn, pl\u00fcnderte s\u00e4mtliche Brut.", "tokens": ["R\u00fcck\u00b7te", "die", "Lei\u00b7ter", "der", "Sohn", ",", "pl\u00fcn\u00b7der\u00b7te", "s\u00e4mt\u00b7li\u00b7che", "Brut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Wehe dem ruchlos F\u00fchlenden, der den vertraulichen Vogel,", "tokens": ["We\u00b7he", "dem", "ruch\u00b7los", "F\u00fch\u00b7len\u00b7den", ",", "der", "den", "ver\u00b7trau\u00b7li\u00b7chen", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Welcher an Gastfreundschaft Glaubte, zu t\u00f6ten gewagt!", "tokens": ["Wel\u00b7cher", "an", "Gast\u00b7freund\u00b7schaft", "Glaub\u00b7te", ",", "zu", "t\u00f6\u00b7ten", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "VVPP", "$."], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}