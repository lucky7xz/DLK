{"dta.poem.921": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "In DER HEIMAT", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Resedenduft durchs kranke Fenster irrt;", "tokens": ["Re\u00b7se\u00b7den\u00b7duft", "durchs", "kran\u00b7ke", "Fens\u00b7ter", "irrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein alter Platz, Kastanien schwarz und w\u00fcst.", "tokens": ["Ein", "al\u00b7ter", "Platz", ",", "Kas\u00b7ta\u00b7ni\u00b7en", "schwarz", "und", "w\u00fcst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Dach durchbricht ein goldener Strahl und flie\u00dft", "tokens": ["Das", "Dach", "durch\u00b7bricht", "ein", "gol\u00b7de\u00b7ner", "Strahl", "und", "flie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auf die Geschwister traumhaft und verwirrt.", "tokens": ["Auf", "die", "Ge\u00b7schwis\u00b7ter", "traum\u00b7haft", "und", "ver\u00b7wirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Sp\u00fclicht treibt Verfallnes, leise girrt", "tokens": ["Im", "Sp\u00fc\u00b7licht", "treibt", "Ver\u00b7fall\u00b7nes", ",", "lei\u00b7se", "girrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der F\u00f6hn im braunen G\u00e4rtchen; sehr still genie\u00dft", "tokens": ["Der", "F\u00f6hn", "im", "brau\u00b7nen", "G\u00e4rt\u00b7chen", ";", "sehr", "still", "ge\u00b7nie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$.", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr Gold die Sonnenblume und zerflie\u00dft.", "tokens": ["Ihr", "Gold", "die", "Son\u00b7nen\u00b7blu\u00b7me", "und", "zer\u00b7flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch blaue Luft der Ruf der Wache klirrt.", "tokens": ["Durch", "blau\u00b7e", "Luft", "der", "Ruf", "der", "Wa\u00b7che", "klirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Resedenduft. Die Mauern d\u00e4mmern kahl.", "tokens": ["Re\u00b7se\u00b7den\u00b7duft", ".", "Die", "Mau\u00b7ern", "d\u00e4m\u00b7mern", "kahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Schwester Schlaf ist schwer. Der Nachtwind w\u00fchlt", "tokens": ["Der", "Schwes\u00b7ter", "Schlaf", "ist", "schwer", ".", "Der", "Nacht\u00b7wind", "w\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In ihrem Haar, das mondner Glanz umsp\u00fclt.", "tokens": ["In", "ih\u00b7rem", "Haar", ",", "das", "mond\u00b7ner", "Glanz", "um\u00b7sp\u00fclt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Katze Schatten gleitet blau und schmal", "tokens": ["Der", "Kat\u00b7ze", "Schat\u00b7ten", "glei\u00b7tet", "blau", "und", "schmal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom morschen Dach, das nahes Unheil s\u00e4umt,", "tokens": ["Vom", "mor\u00b7schen", "Dach", ",", "das", "na\u00b7hes", "Un\u00b7heil", "s\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Kerzenflamme, die sich purpurn b\u00e4umt.", "tokens": ["Die", "Ker\u00b7zen\u00b7flam\u00b7me", ",", "die", "sich", "pur\u00b7purn", "b\u00e4umt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}