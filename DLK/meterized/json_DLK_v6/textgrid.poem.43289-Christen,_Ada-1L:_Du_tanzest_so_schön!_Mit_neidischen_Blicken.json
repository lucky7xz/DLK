{"textgrid.poem.43289": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du tanzest so sch\u00f6n! Mit neidischen Blicken", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du tanzest so sch\u00f6n! Mit neidischen Blicken", "tokens": ["Du", "tan\u00b7zest", "so", "sch\u00f6n", "!", "Mit", "nei\u00b7di\u00b7schen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verfolgen dich alle, mein zierliches Weib,", "tokens": ["Ver\u00b7fol\u00b7gen", "dich", "al\u00b7le", ",", "mein", "zier\u00b7li\u00b7ches", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Frauen, sie zischeln, fragen und nicken,", "tokens": ["Die", "Frau\u00b7en", ",", "sie", "zi\u00b7scheln", ",", "fra\u00b7gen", "und", "ni\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "$,", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich aber umspanne den bl\u00fchenden Leib.", "tokens": ["Ich", "a\u00b7ber", "um\u00b7span\u00b7ne", "den", "bl\u00fc\u00b7hen\u00b7den", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Geliebte, nur ", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "nur"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Es darf dich kein Andrer zum Tanze erk\u00fcren,", "tokens": ["Es", "darf", "dich", "kein", "A\u00b7ndrer", "zum", "Tan\u00b7ze", "er\u00b7k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}