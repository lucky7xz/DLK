{"dta.poem.21725": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Ihre Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Blumen, welche Sie geweiht,", "tokens": ["Ihr", "Blu\u00b7men", ",", "wel\u00b7che", "Sie", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die ich sorgsam aufgespart,", "tokens": ["Und", "die", "ich", "sorg\u00b7sam", "auf\u00b7ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr zaubert die Vergangenheit", "tokens": ["Ihr", "zau\u00b7bert", "die", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir um zu lichter Gegenwart.", "tokens": ["Mir", "um", "zu", "lich\u00b7ter", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PTKA", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, Traute, diesen ", "tokens": ["Sieh", ",", "Trau\u00b7te", ",", "die\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Einst deiner Locken schlichte Zier.", "tokens": ["Einst", "dei\u00b7ner", "Lo\u00b7cken", "schlich\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du flogst dahin in leichtem Tanz,", "tokens": ["Du", "flogst", "da\u00b7hin", "in", "leich\u00b7tem", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sahst tr\u00fcb mich stehn und gabst ihn mir.", "tokens": ["Sahst", "tr\u00fcb", "mich", "stehn", "und", "gabst", "ihn", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "VVINF", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kennst du noch die ", "tokens": ["Kennst", "du", "noch", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hoch schwellte sie dein schlagend Herz.", "tokens": ["Hoch", "schwell\u00b7te", "sie", "dein", "schla\u00b7gend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schien der Mond. Der Abschied scholl.", "tokens": ["Es", "schien", "der", "Mond", ".", "Der", "Ab\u00b7schied", "scholl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Diess Bl\u00fcmchen brach des Abschieds Schmerz.", "tokens": ["Diess", "Bl\u00fcm\u00b7chen", "brach", "des", "Ab\u00b7schieds", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh dieses Rosenzwillingpaar.", "tokens": ["Sieh", "die\u00b7ses", "Ro\u00b7senz\u00b7wil\u00b7ling\u00b7paar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst str\u00f6mt' es s\u00fcsse D\u00fcfte dir.", "tokens": ["Einst", "str\u00f6mt'", "es", "s\u00fcs\u00b7se", "D\u00fcf\u00b7te", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es welkt', es starb; und undankbar", "tokens": ["Es", "welkt'", ",", "es", "starb", ";", "und", "un\u00b7dank\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warfst du es hin. Ich barg es mir.", "tokens": ["Warfst", "du", "es", "hin", ".", "Ich", "barg", "es", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Diess g\u00fcldne ", "tokens": ["Diess", "g\u00fcld\u00b7ne"], "token_info": ["word", "word"], "pos": ["PDAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Auf meiner ", "tokens": ["Auf", "mei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Du pfl\u00fccktest es, Holdselige,", "tokens": ["Du", "pfl\u00fcck\u00b7test", "es", ",", "Hold\u00b7se\u00b7li\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Und gabst es mir im Abendstrahl.", "tokens": ["Und", "gabst", "es", "mir", "im", "A\u00b7bend\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dort, wo am Strand die Fluth sich bricht,", "tokens": ["Dort", ",", "wo", "am", "Strand", "die", "Fluth", "sich", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(du ruhtest auf dem m\u00e4cht'gen Stein)", "tokens": ["(", "du", "ruh\u00b7test", "auf", "dem", "m\u00e4cht'\u00b7gen", "Stein", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gabst du mir diess ", "tokens": ["Gabst", "du", "mir", "diess"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PDS"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Nein, Traute, nie vergess ich dein.", "tokens": ["Nein", ",", "Trau\u00b7te", ",", "nie", "ver\u00b7gess", "ich", "dein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und als ich auf ", "tokens": ["Und", "als", "ich", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dir flehend in das Auge sah,", "tokens": ["Dir", "fle\u00b7hend", "in", "das", "Au\u00b7ge", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erh\u00f6rtest du das stumme Flehn,", "tokens": ["Er\u00b7h\u00f6r\u00b7test", "du", "das", "stum\u00b7me", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicktest ein erbarmend Ja.", "tokens": ["Und", "nick\u00b7test", "ein", "er\u00b7bar\u00b7mend", "Ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVPP", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch wenn ich euch, Cyanen, seh,", "tokens": ["Doch", "wenn", "ich", "euch", ",", "Cy\u00b7a\u00b7nen", ",", "seh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr schm\u00fccktet j\u00fcngst noch Ihre Brust,", "tokens": ["Ihr", "schm\u00fcck\u00b7tet", "j\u00fcngst", "noch", "Ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So \u00fcberschwemmt mich selig Weh,", "tokens": ["So", "\u00fc\u00b7bersc\u00b7hwemmt", "mich", "se\u00b7lig", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich \u00fcberdr\u00e4ngt qualvolle Lust \u2014 \u2014", "tokens": ["Mich", "\u00fc\u00b7berd\u00b7r\u00e4ngt", "qual\u00b7vol\u00b7le", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$(", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "O Bl\u00fcmchen, eurer jedes ruft", "tokens": ["O", "Bl\u00fcm\u00b7chen", ",", "eu\u00b7rer", "je\u00b7des", "ruft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erinnerungen mir ins Herz.", "tokens": ["E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "mir", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwar farbelos und arm an Duft,", "tokens": ["Zwar", "far\u00b7be\u00b7los", "und", "arm", "an", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4nkt ihr mich doch mit Wonn' und Schmerz.", "tokens": ["Tr\u00e4nkt", "ihr", "mich", "doch", "mit", "Wonn'", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ach, treuer Lieb' ist nichts gering,", "tokens": ["Ach", ",", "treu\u00b7er", "Lieb'", "ist", "nichts", "ge\u00b7ring", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sie durch treue Lieb' erwand.", "tokens": ["Was", "sie", "durch", "treu\u00b7e", "Lieb'", "er\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht feil ist ihr um Stern und Ring", "tokens": ["Nicht", "feil", "ist", "ihr", "um", "Stern", "und", "Ring"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bl\u00fcmlein aus geliebter Hand.", "tokens": ["Ein", "Bl\u00fcm\u00b7lein", "aus", "ge\u00b7lieb\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}