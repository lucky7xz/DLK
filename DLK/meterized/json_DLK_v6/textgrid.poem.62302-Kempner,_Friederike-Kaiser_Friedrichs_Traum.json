{"textgrid.poem.62302": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Friedrichs Traum", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es tr\u00e4umte einst ein K\u00f6nigskind", "tokens": ["Es", "tr\u00e4um\u00b7te", "einst", "ein", "K\u00f6\u00b7nigs\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Purpur und in Seide,", "tokens": ["In", "Pur\u00b7pur", "und", "in", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df alle K\u00f6nigskinder sind", "tokens": ["Da\u00df", "al\u00b7le", "K\u00f6\u00b7nigs\u00b7kin\u00b7der", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schlo\u00df wie auf der Heide.", "tokens": ["Im", "Schlo\u00df", "wie", "auf", "der", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch einsam blieb das K\u00f6nigskind", "tokens": ["Doch", "ein\u00b7sam", "blieb", "das", "K\u00f6\u00b7nigs\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und barfu\u00df blieb die Heide,", "tokens": ["Und", "bar\u00b7fu\u00df", "blieb", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es pfiff und heulte rauh der Wind", "tokens": ["Es", "pfiff", "und", "heul\u00b7te", "rauh", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch eine Trauerweide.", "tokens": ["Durch", "ei\u00b7ne", "Trau\u00b7er\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ob Kaiser Friedrich, er es war,", "tokens": ["Ob", "Kai\u00b7ser", "Fried\u00b7rich", ",", "er", "es", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "$,", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herzblut \u00fcbersch\u00e4umte;", "tokens": ["Des", "Herz\u00b7blut", "\u00fc\u00b7bersc\u00b7h\u00e4um\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als das Herz gebrochen war,", "tokens": ["Und", "als", "das", "Herz", "ge\u00b7bro\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch liebend weiter tr\u00e4umte?", "tokens": ["Noch", "lie\u00b7bend", "wei\u00b7ter", "tr\u00e4um\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}