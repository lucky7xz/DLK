{"textgrid.poem.39232": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war ein D\u00e4nenadmiral,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein D\u00e4nenadmiral,", "tokens": ["Es", "war", "ein", "D\u00e4\u00b7ne\u00b7nad\u00b7mi\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Donnerschild hie\u00df der Mann,", "tokens": ["Herr", "Don\u00b7ner\u00b7schild", "hie\u00df", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der war in Treffen und Wasserschlacht", "tokens": ["Der", "war", "in", "Tref\u00b7fen", "und", "Was\u00b7ser\u00b7schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beim Entern stets voran.", "tokens": ["Beim", "En\u00b7tern", "stets", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier liegt ein Schiff, hei\u00dft: Donnerschild,", "tokens": ["Hier", "liegt", "ein", "Schiff", ",", "hei\u00dft", ":", "Don\u00b7ner\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "$.", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt still und ruhig itzt,", "tokens": ["Liegt", "still", "und", "ru\u00b7hig", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und vornen steht des Helden Bild,", "tokens": ["Und", "vor\u00b7nen", "steht", "des", "Hel\u00b7den", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar sauber in Holz geschnitzt.", "tokens": ["Gar", "sau\u00b7ber", "in", "Holz", "ge\u00b7schnitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es haben sich halt in D\u00e4nemark", "tokens": ["Es", "ha\u00b7ben", "sich", "halt", "in", "D\u00e4\u00b7ne\u00b7mark"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVFIN", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Dinge ver\u00e4ndert sehr:", "tokens": ["Die", "Din\u00b7ge", "ver\u00b7\u00e4n\u00b7dert", "sehr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch immer vorn steht Tordenskiold,", "tokens": ["Noch", "im\u00b7mer", "vorn", "steht", "Tor\u00b7dens\u00b7ki\u00b7old", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Allein er entert nicht mehr.", "tokens": ["Al\u00b7lein", "er", "en\u00b7tert", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}