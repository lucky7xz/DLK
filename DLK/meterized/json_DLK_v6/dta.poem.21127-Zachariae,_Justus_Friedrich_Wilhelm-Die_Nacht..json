{"dta.poem.21127": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Ende vieler dunklen Tage", "tokens": ["Das", "En\u00b7de", "vie\u00b7ler", "dunk\u00b7len", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die treue Nacht bricht schon herein.", "tokens": ["Die", "treu\u00b7e", "Nacht", "bricht", "schon", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verh\u00fclle dich, mein Geist, und klage,", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "dich", ",", "mein", "Geist", ",", "und", "kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht ist diese Stunde dein.", "tokens": ["Viel\u00b7leicht", "ist", "die\u00b7se", "Stun\u00b7de", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Leiden, das man unterdr\u00fccket,", "tokens": ["Ein", "Lei\u00b7den", ",", "das", "man", "un\u00b7ter\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vermehret den geheimen Schmerz;", "tokens": ["Ver\u00b7meh\u00b7ret", "den", "ge\u00b7hei\u00b7men", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jede Thr\u00e4ne, die ersticket,", "tokens": ["Und", "je\u00b7de", "Thr\u00e4\u00b7ne", ",", "die", "er\u00b7sti\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00e4bt blutig sich in unser Herz.", "tokens": ["Gr\u00e4bt", "blu\u00b7tig", "sich", "in", "un\u00b7ser", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt, da die Thoren mich verlassen,", "tokens": ["Jetzt", ",", "da", "die", "Tho\u00b7ren", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die diesen tr\u00fcben Tag umschw\u00e4rmt;", "tokens": ["Die", "die\u00b7sen", "tr\u00fc\u00b7ben", "Tag", "um\u00b7schw\u00e4rmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich dem Schmerz mich \u00fcberlassen,", "tokens": ["Will", "ich", "dem", "Schmerz", "mich", "\u00fc\u00b7ber\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der minder wird, wenn er sich h\u00e4rmt.", "tokens": ["Der", "min\u00b7der", "wird", ",", "wenn", "er", "sich", "h\u00e4rmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Schlaf wird mich vor\u00fcber gehen,", "tokens": ["Der", "Schlaf", "wird", "mich", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der oft den R\u00fccken mir gewandt,", "tokens": ["Der", "oft", "den", "R\u00fc\u00b7cken", "mir", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn noch von aufgehellten H\u00f6hen", "tokens": ["Wenn", "noch", "von", "auf\u00b7ge\u00b7hell\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Morgenroth mich weinend fand.", "tokens": ["Das", "Mor\u00b7gen\u00b7roth", "mich", "wei\u00b7nend", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich fleh ihn an, mir zu erscheinen,", "tokens": ["Ich", "fleh", "ihn", "an", ",", "mir", "zu", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch er ist wie ein falscher Freund;", "tokens": ["Doch", "er", "ist", "wie", "ein", "fal\u00b7scher", "Freund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er k\u00f6mmt im Gl\u00fcck nur zu den Seinen,", "tokens": ["Er", "k\u00f6mmt", "im", "Gl\u00fcck", "nur", "zu", "den", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "APPR", "ART", "PPOSS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und flieht ein Auge, welches weint.", "tokens": ["Und", "flieht", "ein", "Au\u00b7ge", ",", "wel\u00b7ches", "weint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schon siegt der Tag mit hellem Strale,", "tokens": ["Schon", "siegt", "der", "Tag", "mit", "hel\u00b7lem", "Stra\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo bist du, holder Gott der Ruh?", "tokens": ["Wo", "bist", "du", ",", "hol\u00b7der", "Gott", "der", "Ruh", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er k\u00f6mmt, und dr\u00fcckt zum erstenmale", "tokens": ["Er", "k\u00f6mmt", ",", "und", "dr\u00fcckt", "zum", "ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Auge voller Thr\u00e4nen zu.", "tokens": ["Ein", "Au\u00b7ge", "vol\u00b7ler", "Thr\u00e4\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}