{"textgrid.poem.41559": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am zehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum den eitlen Mammon mir", "tokens": ["Wa\u00b7rum", "den", "eit\u00b7len", "Mam\u00b7mon", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du gesellt nach deinem Willen?", "tokens": ["Hast", "du", "ge\u00b7sellt", "nach", "dei\u00b7nem", "Wil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht da\u00df er, eine blanke Zier,", "tokens": ["Nicht", "da\u00df", "er", ",", "ei\u00b7ne", "blan\u00b7ke", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll eingefre\u00dfne Sch\u00e4den h\u00fcllen;", "tokens": ["Soll", "ein\u00b7ge\u00b7fre\u00df\u00b7ne", "Sch\u00e4\u00b7den", "h\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch nicht die fl\u00fccht'gen Stunden hier", "tokens": ["Auch", "nicht", "die", "fl\u00fccht'\u00b7gen", "Stun\u00b7den", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit frischem Erdenreiz zu f\u00fcllen,", "tokens": ["Mit", "fri\u00b7schem", "Er\u00b7den\u00b7reiz", "zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, anders wohl;", "tokens": ["Nein", ",", "an\u00b7ders", "wohl", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "O was du gibst ist nicht so leer und hohl!", "tokens": ["O", "was", "du", "gibst", "ist", "nicht", "so", "leer", "und", "hohl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "PPER", "VVFIN", "VAFIN", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich soll mit seinem bunten Strahl", "tokens": ["Ich", "soll", "mit", "sei\u00b7nem", "bun\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deinem Segen Wucher treiben;", "tokens": ["In", "dei\u00b7nem", "Se\u00b7gen", "Wu\u00b7cher", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr meinen Hunger soll ein Mahl", "tokens": ["F\u00fcr", "mei\u00b7nen", "Hun\u00b7ger", "soll", "ein", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich in die ew'ge Rechnung schreiben;", "tokens": ["Ich", "in", "die", "ew'\u00b7ge", "Rech\u00b7nung", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und meiner Bl\u00f6\u00dfe, matt und fahl,", "tokens": ["Und", "mei\u00b7ner", "Bl\u00f6\u00b7\u00dfe", ",", "matt", "und", "fahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll er ein warmer Mantel bleiben,", "tokens": ["Soll", "er", "ein", "war\u00b7mer", "Man\u00b7tel", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn bricht herein", "tokens": ["Wenn", "bricht", "her\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die Zeit, wo st\u00e4ubt und rostet, was nicht mein.", "tokens": ["Die", "Zeit", ",", "wo", "st\u00e4ubt", "und", "ros\u00b7tet", ",", "was", "nicht", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann bin ich krank und ganz verarmt,", "tokens": ["Dann", "bin", "ich", "krank", "und", "ganz", "ver\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann wird der bittre Mangel kommen,", "tokens": ["Dann", "wird", "der", "bitt\u00b7re", "Man\u00b7gel", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo starrt, woran mein Herz erwarmt,", "tokens": ["Wo", "starrt", ",", "wo\u00b7ran", "mein", "Herz", "er\u00b7warmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerst\u00e4ubt, woher ich Trost genommen;", "tokens": ["Zer\u00b7st\u00e4ubt", ",", "wo\u00b7her", "ich", "Trost", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn deine Hand sich nicht erbarmt", "tokens": ["Wenn", "dei\u00b7ne", "Hand", "sich", "nicht", "er\u00b7barmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zeichnet noch zu meinem Frommen", "tokens": ["Und", "zeich\u00b7net", "noch", "zu", "mei\u00b7nem", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In Mildigkeit", "tokens": ["In", "Mil\u00b7dig\u00b7keit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Den Heller heimgelegt f\u00fcr jene Zeit.", "tokens": ["Den", "Hel\u00b7ler", "heim\u00b7ge\u00b7legt", "f\u00fcr", "je\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00df, Herr, in jener Stunde Macht", "tokens": ["La\u00df", ",", "Herr", ",", "in", "je\u00b7ner", "Stun\u00b7de", "Macht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich nicht so h\u00fclfeweinend fallen!", "tokens": ["Mich", "nicht", "so", "h\u00fcl\u00b7fe\u00b7wei\u00b7nend", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die vor mir steht wie Chaos' Nacht,", "tokens": ["Die", "vor", "mir", "steht", "wie", "Cha\u00b7os'", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KOKOM", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Dunkel \u00fcber Dunkel wallen.", "tokens": ["Wie", "Dun\u00b7kel", "\u00fc\u00b7ber", "Dun\u00b7kel", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weh mir, ich hab' es nicht bedacht!", "tokens": ["Weh", "mir", ",", "ich", "hab'", "es", "nicht", "be\u00b7dacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So la\u00df es mir fortan vor allen", "tokens": ["So", "la\u00df", "es", "mir", "for\u00b7tan", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gew\u00e4rtig sein;", "tokens": ["Ge\u00b7w\u00e4r\u00b7tig", "sein", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "O rege mich durch Milde oder Pein!", "tokens": ["O", "re\u00b7ge", "mich", "durch", "Mil\u00b7de", "o\u00b7der", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "La\u00df mich hinfort der Worte Gold", "tokens": ["La\u00df", "mich", "hin\u00b7fort", "der", "Wor\u00b7te", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ausgeben mit des Wuchrers Sorgen,", "tokens": ["Aus\u00b7ge\u00b7ben", "mit", "des", "Wuc\u00b7hrers", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, wenn das Heute nun entrollt,", "tokens": ["Da\u00df", ",", "wenn", "das", "Heu\u00b7te", "nun", "ent\u00b7rollt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PDS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir nicht verloren ist das Morgen;", "tokens": ["Mir", "nicht", "ver\u00b7lo\u00b7ren", "ist", "das", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df mich bedenken, da\u00df der Sold,", "tokens": ["La\u00df", "mich", "be\u00b7den\u00b7ken", ",", "da\u00df", "der", "Sold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den eitlem Ruhm ich mu\u00dfte borgen,", "tokens": ["Den", "eit\u00b7lem", "Ruhm", "ich", "mu\u00df\u00b7te", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Genommen ward", "tokens": ["Ge\u00b7nom\u00b7men", "ward"], "token_info": ["word", "word"], "pos": ["NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dem goldnen Hort f\u00fcr einst und Gegenwart!", "tokens": ["Dem", "gold\u00b7nen", "Hort", "f\u00fcr", "einst", "und", "Ge\u00b7gen\u00b7wart", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und eine Feder la\u00df mich nur", "tokens": ["Und", "ei\u00b7ne", "Fe\u00b7der", "la\u00df", "mich", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betrachten mit geheimem Beben,", "tokens": ["Be\u00b7trach\u00b7ten", "mit", "ge\u00b7hei\u00b7mem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bedenkend, da\u00df der schwarzen Spur", "tokens": ["Be\u00b7den\u00b7kend", ",", "da\u00df", "der", "schwar\u00b7zen", "Spur"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Folgt leise schleichend Tod und Leben.", "tokens": ["Folgt", "lei\u00b7se", "schlei\u00b7chend", "Tod", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Pfunden, so mir gab Natur,", "tokens": ["Den", "Pfun\u00b7den", ",", "so", "mir", "gab", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Herr la\u00df Zinsen mich entheben;", "tokens": ["O", "Herr", "la\u00df", "Zin\u00b7sen", "mich", "ent\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVIMP", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bin so arm,", "tokens": ["Ich", "bin", "so", "arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So nur in dem geborgten Pelze warm!", "tokens": ["So", "nur", "in", "dem", "ge\u00b7borg\u00b7ten", "Pel\u00b7ze", "warm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ach Gott, wie wird mein Herz so schwer,", "tokens": ["Ach", "Gott", ",", "wie", "wird", "mein", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gepre\u00dft vom d\u00e4mmernden Verstande!", "tokens": ["Ge\u00b7pre\u00dft", "vom", "d\u00e4m\u00b7mern\u00b7den", "Ver\u00b7stan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob es gelingt die Gaben hehr", "tokens": ["Ob", "es", "ge\u00b7lingt", "die", "Ga\u00b7ben", "hehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu legen mir auf edle Pfande?", "tokens": ["Zu", "le\u00b7gen", "mir", "auf", "ed\u00b7le", "Pfan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O nur aus deiner Weisheit Meer", "tokens": ["O", "nur", "aus", "dei\u00b7ner", "Weis\u00b7heit", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein einzig Tr\u00f6pflein mir vom Rande!", "tokens": ["Ein", "ein\u00b7zig", "Tr\u00f6pf\u00b7lein", "mir", "vom", "Ran\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch des Genu\u00df", "tokens": ["Durch", "des", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Galle selbst zu Honig werden mu\u00df!", "tokens": ["Die", "Gal\u00b7le", "selbst", "zu", "Ho\u00b7nig", "wer\u00b7den", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}