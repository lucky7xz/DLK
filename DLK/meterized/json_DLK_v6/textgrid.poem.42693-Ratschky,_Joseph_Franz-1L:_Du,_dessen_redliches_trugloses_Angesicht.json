{"textgrid.poem.42693": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, dessen redliches trugloses Angesicht", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, dessen redliches trugloses Angesicht", "tokens": ["Du", ",", "des\u00b7sen", "red\u00b7li\u00b7ches", "trug\u00b7lo\u00b7ses", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Seele Lauterkeit beym ersten Blick verk\u00fcndet,", "tokens": ["Der", "See\u00b7le", "Lau\u00b7ter\u00b7keit", "beym", "ers\u00b7ten", "Blick", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Freund, mit dem das Band der br\u00fcderlichen Pflicht,", "tokens": ["O", "Freund", ",", "mit", "dem", "das", "Band", "der", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch mehr noch eigne Wahl des Herzens mich verbindet,", "tokens": ["Doch", "mehr", "noch", "eig\u00b7ne", "Wahl", "des", "Her\u00b7zens", "mich", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Diess Blatt, mit welchem dich dein ferner Freund begr\u00fcsst,", "tokens": ["Diess", "Blatt", ",", "mit", "wel\u00b7chem", "dich", "dein", "fer\u00b7ner", "Freund", "be\u00b7gr\u00fcs\u00b7st", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.6": {"text": "Sey dir ein Unterpfand, wie theuer du mir bist", "tokens": ["Sey", "dir", "ein", "Un\u00b7ter\u00b7pfand", ",", "wie", "theu\u00b7er", "du", "mir", "bist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du warst mir gut auf festem Lande:", "tokens": ["Du", "warst", "mir", "gut", "auf", "fes\u00b7tem", "Lan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sey mir's nicht minder hier am feuchten Meeresstrande,", "tokens": ["Sey", "mir's", "nicht", "min\u00b7der", "hier", "am", "feuch\u00b7ten", "Mee\u00b7res\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn gleich itzt zwischen uns so manche Wolke schwebt,", "tokens": ["Wenn", "gleich", "itzt", "zwi\u00b7schen", "uns", "so", "man\u00b7che", "Wol\u00b7ke", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und mancher steile Berg sein Riesenhaupt erhebt!", "tokens": ["Und", "man\u00b7cher", "stei\u00b7le", "Berg", "sein", "Rie\u00b7sen\u00b7haupt", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geliebter! dreymal hat nun Titans goldner Wagen", "tokens": ["Ge\u00b7lieb\u00b7ter", "!", "drey\u00b7mal", "hat", "nun", "Ti\u00b7tans", "gold\u00b7ner", "Wa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "ADV", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Amphitritens flimmernd Gr\u00fcn", "tokens": ["In", "Am\u00b7phit\u00b7ri\u00b7tens", "flim\u00b7mernd", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJD", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hinunter sich getaucht, seitdem ich (so zu sagen)", "tokens": ["Hin\u00b7un\u00b7ter", "sich", "ge\u00b7taucht", ",", "seit\u00b7dem", "ich", "(", "so", "zu", "sa\u00b7gen", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVPP", "$,", "KOUS", "PPER", "$(", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Einem Element hier mit den Fischen bin.", "tokens": ["In", "Ei\u00b7nem", "E\u00b7le\u00b7ment", "hier", "mit", "den", "Fi\u00b7schen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Anblick dieser Stadt, die auf dem weichen R\u00fccken", "tokens": ["Der", "An\u00b7blick", "die\u00b7ser", "Stadt", ",", "die", "auf", "dem", "wei\u00b7chen", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des m\u00e4chtigen Neptuns, von der besch\u00e4umten Flut", "tokens": ["Des", "m\u00e4ch\u00b7ti\u00b7gen", "Nep\u00b7tuns", ",", "von", "der", "be\u00b7sch\u00e4um\u00b7ten", "Flut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Meeres rings besp\u00fclt, fest, wie auf Felsen, ruht,", "tokens": ["Des", "Mee\u00b7res", "rings", "be\u00b7sp\u00fclt", ",", "fest", ",", "wie", "auf", "Fel\u00b7sen", ",", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "PTKVZ", "$,", "PWAV", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist in den ersten Augenblicken", "tokens": ["Ist", "in", "den", "ers\u00b7ten", "Au\u00b7gen\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "F\u00fcr eines Neulings Aug' ein magisch Ph\u00e4nomen.", "tokens": ["F\u00fcr", "ei\u00b7nes", "Neu\u00b7lings", "Aug'", "ein", "ma\u00b7gisch", "Ph\u00e4\u00b7no\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.10": {"text": "Statt Menschen w\u00fcrdest du Amphibien hier sehn,", "tokens": ["Statt", "Men\u00b7schen", "w\u00fcr\u00b7dest", "du", "Am\u00b7phi\u00b7bi\u00b7en", "hier", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die t\u00e4glich halb auf trocknem Boden leben,", "tokens": ["Die", "t\u00e4g\u00b7lich", "halb", "auf", "trock\u00b7nem", "Bo\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Halb auf der See in schwarzen Gondeln schweben.", "tokens": ["Halb", "auf", "der", "See", "in", "schwar\u00b7zen", "Gon\u00b7deln", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Du w\u00e4hntest, durch die Allgewalt", "tokens": ["Du", "w\u00e4hn\u00b7test", ",", "durch", "die", "All\u00b7ge\u00b7walt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Des Zaubers unsichtbarer Feen", "tokens": ["Des", "Zau\u00b7bers", "un\u00b7sicht\u00b7ba\u00b7rer", "Feen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In eine neue Welt dich hingebannt zu sehen.", "tokens": ["In", "ei\u00b7ne", "neu\u00b7e", "Welt", "dich", "hin\u00b7ge\u00b7bannt", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch, Theuerster, nur allzubald", "tokens": ["Doch", ",", "Theu\u00b7ers\u00b7ter", ",", "nur", "all\u00b7zu\u00b7bald"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "ADV", "ADV"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Hat man aus hundert tollen Streichen,", "tokens": ["Hat", "man", "aus", "hun\u00b7dert", "tol\u00b7len", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wodurch die Menschen sich hiernieden alle gleichen,", "tokens": ["Wo\u00b7durch", "die", "Men\u00b7schen", "sich", "hier\u00b7nie\u00b7den", "al\u00b7le", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "PAV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bey diesem V\u00f6lkchen sich belehrt,", "tokens": ["Bey", "die\u00b7sem", "V\u00f6lk\u00b7chen", "sich", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dass es nur zu gewiss zum Narrenrund geh\u00f6rt.", "tokens": ["Dass", "es", "nur", "zu", "ge\u00b7wiss", "zum", "Nar\u00b7ren\u00b7rund", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Freund! w\u00e4r' ich Heraklit, so weint' ich nun wohl freylich,", "tokens": ["Freund", "!", "w\u00e4r'", "ich", "Her\u00b7a\u00b7klit", ",", "so", "weint'", "ich", "nun", "wohl", "frey\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "NE", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dass ich, wie \u00fcberall, hier wieder Narren fand:", "tokens": ["Dass", "ich", ",", "wie", "\u00fc\u00b7be\u00b7rall", ",", "hier", "wie\u00b7der", "Nar\u00b7ren", "fand", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "ADV", "$,", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch ein gewisses Mass von Tollsinn ist verzeihlich;", "tokens": ["Doch", "ein", "ge\u00b7wis\u00b7ses", "Mass", "von", "Toll\u00b7sinn", "ist", "ver\u00b7zeih\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Welt ist ja der Thorheit Vaterland.", "tokens": ["Die", "Welt", "ist", "ja", "der", "Thor\u00b7heit", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Man mag sie weit und breit von einem Pol zum andern,", "tokens": ["Man", "mag", "sie", "weit", "und", "breit", "von", "ei\u00b7nem", "Pol", "zum", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu Wasser und zu Land durchwandern,", "tokens": ["Zu", "Was\u00b7ser", "und", "zu", "Land", "durch\u00b7wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So trifft man allerw\u00e4rts der Menschheit Schw\u00e4chen an.", "tokens": ["So", "trifft", "man", "al\u00b7ler\u00b7w\u00e4rts", "der", "Menschheit", "Schw\u00e4\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wer drob sich h\u00e4rmen will, ist wahrlich schlimm daran,", "tokens": ["Wer", "drob", "sich", "h\u00e4r\u00b7men", "will", ",", "ist", "wahr\u00b7lich", "schlimm", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "VVINF", "VMFIN", "$,", "VAFIN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Besonders hier; denn traun! bey allen V\u00f6lkerschaaren", "tokens": ["Be\u00b7son\u00b7ders", "hier", ";", "denn", "traun", "!", "bey", "al\u00b7len", "V\u00f6l\u00b7ker\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "KON", "VVINF", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kann Aberglaube sich mit Sittenlosigkeit,", "tokens": ["Kann", "A\u00b7berg\u00b7lau\u00b7be", "sich", "mit", "Sit\u00b7ten\u00b7lo\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schamlose Betteley mit Aufgeblasenheit", "tokens": ["Scham\u00b7lo\u00b7se", "Bet\u00b7te\u00b7ley", "mit", "Auf\u00b7ge\u00b7bla\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Wohl nirgendwo, als hier, in solcher Bl\u00f6sse paaren.", "tokens": ["Wohl", "nir\u00b7gend\u00b7wo", ",", "als", "hier", ",", "in", "sol\u00b7cher", "Bl\u00f6s\u00b7se", "paa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "$,", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von einem Baldachin umschanzt,", "tokens": ["Von", "ei\u00b7nem", "Bal\u00b7da\u00b7ch\u00b7in", "um\u00b7schanzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und mit Akazien und Rosen rings bepflanzt,", "tokens": ["Und", "mit", "A\u00b7ka\u00b7zi\u00b7en", "und", "Ro\u00b7sen", "rings", "be\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zeigt sich, von Fackeln hell umschimmert, deinem Blicke", "tokens": ["Zeigt", "sich", ",", "von", "Fa\u00b7ckeln", "hell", "um\