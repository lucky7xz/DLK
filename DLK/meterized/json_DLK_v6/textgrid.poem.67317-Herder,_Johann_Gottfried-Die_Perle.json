{"textgrid.poem.67317": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Perle", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm, o Freundin, dieser Perlen,", "tokens": ["Nimm", ",", "o", "Freun\u00b7din", ",", "die\u00b7ser", "Per\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Silbertropfen Band!", "tokens": ["Die\u00b7ser", "Sil\u00b7ber\u00b7trop\u00b7fen", "Band", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die G\u00f6ttin stiller Anmuth", "tokens": ["Denn", "die", "G\u00f6t\u00b7tin", "stil\u00b7ler", "An\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat Dir selbst sie zuerkannt.", "tokens": ["Hat", "Dir", "selbst", "sie", "zu\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als sie aus des Meeres Wellen", "tokens": ["Als", "sie", "aus", "des", "Mee\u00b7res", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Traum der Liebe stieg,", "tokens": ["Wie", "ein", "Traum", "der", "Lie\u00b7be", "stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam dem\u00fcthig eine Muschel,", "tokens": ["Kam", "de\u00b7m\u00fct\u00b7hig", "ei\u00b7ne", "Mu\u00b7schel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die sie trug und sittsam schwieg.", "tokens": ["Die", "sie", "trug", "und", "sitt\u00b7sam", "schwieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wellen h\u00fcpften um die G\u00f6ttin,", "tokens": ["Wel\u00b7len", "h\u00fcpf\u00b7ten", "um", "die", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weste buhlten um sie her;", "tokens": ["Wes\u00b7te", "buhl\u00b7ten", "um", "sie", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber die gef\u00e4llig-gute", "tokens": ["A\u00b7ber", "die", "ge\u00b7f\u00e4l\u00b7lig\u00b7gu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dienerin gefiel ihr mehr.", "tokens": ["Die\u00b7ne\u00b7rin", "ge\u00b7fiel", "ihr", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbwomit soll ich Dich belohnen?\u00ab", "tokens": ["\u00bb", "wo\u00b7mit", "soll", "ich", "Dich", "be\u00b7loh\u00b7nen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach sie, und vom Silberglanz", "tokens": ["Sprach", "sie", ",", "und", "vom", "Sil\u00b7ber\u00b7glanz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrer Glieder schwamm die Muschel", "tokens": ["Ih\u00b7rer", "Glie\u00b7der", "schwamm", "die", "Mu\u00b7schel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Silbern schon im Wellentanz.", "tokens": ["Sil\u00b7bern", "schon", "im", "Wel\u00b7len\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbnimm den Tropfen meines Haares,", "tokens": ["\u00bb", "nimm", "den", "Trop\u00b7fen", "mei\u00b7nes", "Haa\u00b7res", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcnftig nur der Unschuld Schmuck!\u00ab", "tokens": ["K\u00fcnf\u00b7tig", "nur", "der", "Un\u00b7schuld", "Schmuck", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Tropfen ward zur Perle", "tokens": ["Und", "der", "Trop\u00b7fen", "ward", "zur", "Per\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Muschel, die sie trug.", "tokens": ["In", "der", "Mu\u00b7schel", ",", "die", "sie", "trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ewig jetzt ein Schmuck der Unschuld,", "tokens": ["E\u00b7wig", "jetzt", "ein", "Schmuck", "der", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiller Anmuth selbst ein Bild,", "tokens": ["Stil\u00b7ler", "An\u00b7muth", "selbst", "ein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Gaukelei der Farben", "tokens": ["Oh\u00b7ne", "Gau\u00b7ke\u00b7lei", "der", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In bescheidnen Reiz geh\u00fcllt,", "tokens": ["In", "be\u00b7scheid\u00b7nen", "Reiz", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sehnet sie sich aus der Krone", "tokens": ["Seh\u00b7net", "sie", "sich", "aus", "der", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Monarchen in das Band,", "tokens": ["Des", "Mon\u00b7ar\u00b7chen", "in", "das", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das der Unschuld Haar umschlinget,", "tokens": ["Das", "der", "Un\u00b7schuld", "Haar", "um\u00b7schlin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer G\u00f6ttin Haar entwandt.", "tokens": ["Ei\u00b7ner", "G\u00f6t\u00b7tin", "Haar", "ent\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}