{"textgrid.poem.61648": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich w\u00fcnschte vor manchem Jahr,", "tokens": ["Was", "ich", "w\u00fcnschte", "vor", "man\u00b7chem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat das Leben mir nicht beschert,", "tokens": ["Hat", "das", "Le\u00b7ben", "mir", "nicht", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Aber es hat mich daf\u00fcr gelehrt,", "tokens": ["A\u00b7ber", "es", "hat", "mich", "da\u00b7f\u00fcr", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PRF", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df mein Wunsch ein t\u00f6richter war.", "tokens": ["Da\u00df", "mein", "Wunsch", "ein", "t\u00f6\u00b7rich\u00b7ter", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}}}}