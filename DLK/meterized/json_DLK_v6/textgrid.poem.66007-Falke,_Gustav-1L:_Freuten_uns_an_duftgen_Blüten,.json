{"textgrid.poem.66007": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freuten uns an duftgen Bl\u00fcten,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freuten uns an duftgen Bl\u00fcten,", "tokens": ["Freu\u00b7ten", "uns", "an", "duft\u00b7gen", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die f\u00fcr uns im Laube gl\u00fchten.", "tokens": ["Die", "f\u00fcr", "uns", "im", "Lau\u00b7be", "gl\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun, da sich auch Fr\u00fcchte zeigen,", "tokens": ["Nun", ",", "da", "sich", "auch", "Fr\u00fcch\u00b7te", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PRF", "ADV", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Pfl\u00fccken wir aus vollen Zweigen.", "tokens": ["Pfl\u00fc\u00b7cken", "wir", "aus", "vol\u00b7len", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kommt der Winter, n\u00fctzt aufs beste,", "tokens": ["Kommt", "der", "Win\u00b7ter", ",", "n\u00fctzt", "aufs", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rmend uns, ein d\u00fcrr Ge\u00e4ste.", "tokens": ["W\u00e4r\u00b7mend", "uns", ",", "ein", "d\u00fcrr", "Ge\u00b7\u00e4s\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJD", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wenn die Flammen aufw\u00e4rts schlagen,", "tokens": ["Wenn", "die", "Flam\u00b7men", "auf\u00b7w\u00e4rts", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4umen wir von Fr\u00fchlingstagen.", "tokens": ["Tr\u00e4u\u00b7men", "wir", "von", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}