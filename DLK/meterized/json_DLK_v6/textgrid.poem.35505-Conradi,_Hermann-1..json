{"textgrid.poem.35505": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich mich auf den Fr\u00fchling freue!", "tokens": ["Wie", "ich", "mich", "auf", "den", "Fr\u00fch\u00b7ling", "freu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie mir das Alte und doch so Neue", "tokens": ["Wie", "mir", "das", "Al\u00b7te", "und", "doch", "so", "Neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "KON", "ADV", "ADV", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schon im tiefsten Winter die Seele bewegt!", "tokens": ["Schon", "im", "tiefs\u00b7ten", "Win\u00b7ter", "die", "See\u00b7le", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch ist's erst Weihnacht! Noch atmet der Winter", "tokens": ["Noch", "ist's", "erst", "Weih\u00b7nacht", "!", "Noch", "at\u00b7met", "der", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aus vollen Lungen!", "tokens": ["Aus", "vol\u00b7len", "Lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und doch ist's mir, als ob schon dahinter", "tokens": ["Und", "doch", "ist's", "mir", ",", "als", "ob", "schon", "da\u00b7hin\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "KOKOM", "KOUS", "ADV", "PAV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sehnsuchtsbezwungen", "tokens": ["Sehn\u00b7suchts\u00b7be\u00b7zwun\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Leise, ganz leise der Lenz sich regt ...", "tokens": ["Lei\u00b7se", ",", "ganz", "lei\u00b7se", "der", "Lenz", "sich", "regt", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}