{"textgrid.poem.55143": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Morgenklagen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du loses, leidigliebes M\u00e4dchen,", "tokens": ["O", "du", "lo\u00b7ses", ",", "lei\u00b7di\u00b7glie\u00b7bes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sag mir an, womit hab ich's verschuldet,", "tokens": ["Sag", "mir", "an", ",", "wo\u00b7mit", "hab", "ich's", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PWAV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da\u00df du mich auf diese Folter spannest,", "tokens": ["Da\u00df", "du", "mich", "auf", "die\u00b7se", "Fol\u00b7ter", "span\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df du dein gegeben Wort gebrochen?", "tokens": ["Da\u00df", "du", "dein", "ge\u00b7ge\u00b7ben", "Wort", "ge\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Drucktest doch so freundlich gestern abend", "tokens": ["Druck\u00b7test", "doch", "so", "freund\u00b7lich", "ge\u00b7stern", "a\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir die H\u00e4nde, lispeltest so lieblich:", "tokens": ["Mir", "die", "H\u00e4n\u00b7de", ",", "lis\u00b7pel\u00b7test", "so", "lieb\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbja, ich komme, komme gegen Morgen", "tokens": ["\u00bb", "ja", ",", "ich", "kom\u00b7me", ",", "kom\u00b7me", "ge\u00b7gen", "Mor\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ganz gewi\u00df, mein Freund, auf deine Stube.\u00ab", "tokens": ["Ganz", "ge\u00b7wi\u00df", ",", "mein", "Freund", ",", "auf", "dei\u00b7ne", "Stu\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Angelehnet lie\u00df ich meine T\u00fcre,", "tokens": ["An\u00b7ge\u00b7leh\u00b7net", "lie\u00df", "ich", "mei\u00b7ne", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hatte wohl die Angeln erst gepr\u00fcfet", "tokens": ["Hat\u00b7te", "wohl", "die", "An\u00b7geln", "erst", "ge\u00b7pr\u00fc\u00b7fet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und mich recht gefreut, da\u00df sie nicht knarrten.", "tokens": ["Und", "mich", "recht", "ge\u00b7freut", ",", "da\u00df", "sie", "nicht", "knarr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Welche Nacht des Wartens ist vergangen!", "tokens": ["Wel\u00b7che", "Nacht", "des", "War\u00b7tens", "ist", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wacht ich doch und z\u00e4hlte jedes Viertel:", "tokens": ["Wacht", "ich", "doch", "und", "z\u00e4hl\u00b7te", "je\u00b7des", "Vier\u00b7tel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schlief ich ein auf wenig Augenblicke,", "tokens": ["Schlief", "ich", "ein", "auf", "we\u00b7nig", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "War mein Herz best\u00e4ndig wach geblieben,", "tokens": ["War", "mein", "Herz", "be\u00b7st\u00e4n\u00b7dig", "wach", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Weckte mich von meinem leisen Schlummer.", "tokens": ["Weck\u00b7te", "mich", "von", "mei\u00b7nem", "lei\u00b7sen", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ja, da segnet ich die Finsternisse,", "tokens": ["Ja", ",", "da", "seg\u00b7net", "ich", "die", "Fins\u00b7ter\u00b7nis\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die so ruhig alles \u00fcberdeckten,", "tokens": ["Die", "so", "ru\u00b7hig", "al\u00b7les", "\u00fc\u00b7berd\u00b7eck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Freute mich der allgemeinen Stille,", "tokens": ["Freu\u00b7te", "mich", "der", "all\u00b7ge\u00b7mei\u00b7nen", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Horchte lauschend immer in die Stille,", "tokens": ["Horch\u00b7te", "lau\u00b7schend", "im\u00b7mer", "in", "die", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ob sich nicht ein Laut bewegen m\u00f6chte.", "tokens": ["Ob", "sich", "nicht", "ein", "Laut", "be\u00b7we\u00b7gen", "m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PTKNEG", "ART", "APPR", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbh\u00e4tte sie Gedanken, wie ich denke,", "tokens": ["\u00bb", "h\u00e4t\u00b7te", "sie", "Ge\u00b7dan\u00b7ken", ",", "wie", "ich", "den\u00b7ke", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4tte sie Gef\u00fchl, wie ich empfinde,", "tokens": ["H\u00e4t\u00b7te", "sie", "Ge\u00b7f\u00fchl", ",", "wie", "ich", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00fcrde sie den Morgen nicht erwarten,", "tokens": ["W\u00fcr\u00b7de", "sie", "den", "Mor\u00b7gen", "nicht", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00fcrde schon in dieser Stunde kommen.\u00ab", "tokens": ["W\u00fcr\u00b7de", "schon", "in", "die\u00b7ser", "Stun\u00b7de", "kom\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "H\u00fcpft' ein K\u00e4tzchen oben \u00fcbern Boden,", "tokens": ["H\u00fcpft'", "ein", "K\u00e4tz\u00b7chen", "o\u00b7ben", "\u00fc\u00b7bern", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Knisterte das M\u00e4uschen in der Ecke,", "tokens": ["Knis\u00b7ter\u00b7te", "das", "M\u00e4u\u00b7schen", "in", "der", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Regte sich, ich wei\u00df nicht was, im Hause,", "tokens": ["Reg\u00b7te", "sich", ",", "ich", "wei\u00df", "nicht", "was", ",", "im", "Hau\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Immer hofft ich, deinen Schritt zu h\u00f6ren,", "tokens": ["Im\u00b7mer", "hofft", "ich", ",", "dei\u00b7nen", "Schritt", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Immer glaubt ich, deinen Tritt zu h\u00f6ren.", "tokens": ["Im\u00b7mer", "glaubt", "ich", ",", "dei\u00b7nen", "Tritt", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Und so lag ich lang' und immer l\u00e4nger,", "tokens": ["Und", "so", "lag", "ich", "lang'", "und", "im\u00b7mer", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und es fing der Tag schon an zu grauen,", "tokens": ["Und", "es", "fing", "der", "Tag", "schon", "an", "zu", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und es rauschte hier und rauschte dorten.", "tokens": ["Und", "es", "rauschte", "hier", "und", "rauschte", "dor\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbist es ihre T\u00fcre? W\u00e4r's die meine!\u00ab", "tokens": ["\u00bb", "ist", "es", "ih\u00b7re", "T\u00fc\u00b7re", "?", "W\u00e4r's", "die", "mei\u00b7ne", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "PPOSAT", "NN", "$.", "VAFIN", "ART", "PPOSAT", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sa\u00df ich aufgestemmt in meinem Bette,", "tokens": ["Sa\u00df", "ich", "auf\u00b7ge\u00b7stemmt", "in", "mei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schaute nach der halb erhellten T\u00fcre,", "tokens": ["Schau\u00b7te", "nach", "der", "halb", "er\u00b7hell\u00b7ten", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ob sie nicht sich wohl bewegen m\u00f6chte.", "tokens": ["Ob", "sie", "nicht", "sich", "wohl", "be\u00b7we\u00b7gen", "m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Angelehnet blieben beide Fl\u00fcgel", "tokens": ["An\u00b7ge\u00b7leh\u00b7net", "blie\u00b7ben", "bei\u00b7de", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auf den leisen Angeln ruhig hangen.", "tokens": ["Auf", "den", "lei\u00b7sen", "An\u00b7geln", "ru\u00b7hig", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Und der Tag ward immer hell' und heller;", "tokens": ["Und", "der", "Tag", "ward", "im\u00b7mer", "hell'", "und", "hel\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00f6rt ich schon des Nachbars T\u00fcre gehen,", "tokens": ["H\u00f6rt", "ich", "schon", "des", "Nach\u00b7bars", "T\u00fc\u00b7re", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der das Taglohn zu gewinnen eilet,", "tokens": ["Der", "das", "Tag\u00b7lohn", "zu", "ge\u00b7win\u00b7nen", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6rt ich bald darauf die Wagen rasseln,", "tokens": ["H\u00f6rt", "ich", "bald", "da\u00b7rauf", "die", "Wa\u00b7gen", "ras\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "War das Tor der Stadt nun auch er\u00f6ffnet,", "tokens": ["War", "das", "Tor", "der", "Stadt", "nun", "auch", "er\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und es regte sich der ganze Plunder", "tokens": ["Und", "es", "reg\u00b7te", "sich", "der", "gan\u00b7ze", "Plun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Des bewegten Marktes durcheinander.", "tokens": ["Des", "be\u00b7weg\u00b7ten", "Mark\u00b7tes", "durch\u00b7ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Ward nun in dem Haus ein Gehn und Kommen", "tokens": ["Ward", "nun", "in", "dem", "Haus", "ein", "Gehn", "und", "Kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf und ab die Stiegen, hin und wieder", "tokens": ["Auf", "und", "ab", "die", "Stie\u00b7gen", ",", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "KON", "APPR", "ART", "NN", "$,", "ADV", "KON", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Knarrte