{"textgrid.poem.67417": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die goldne Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor manchen, manchen Jahren,", "tokens": ["Vor", "man\u00b7chen", ",", "man\u00b7chen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ich zuerst Dich sah,", "tokens": ["Als", "ich", "zu\u00b7erst", "Dich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War Deine Locke rabenschwarz,", "tokens": ["War", "Dei\u00b7ne", "Lo\u00b7cke", "ra\u00b7ben\u00b7schwarz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Braun Deine Wange da.", "tokens": ["Braun", "Dei\u00b7ne", "Wan\u00b7ge", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt ist die Wange bl\u00e4sser,", "tokens": ["Jetzt", "ist", "die", "Wan\u00b7ge", "bl\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Silber gl\u00e4nzt Dein Haar,", "tokens": ["Wie", "Sil\u00b7ber", "gl\u00e4nzt", "Dein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dennoch bist Du lieber mir,", "tokens": ["Und", "den\u00b7noch", "bist", "Du", "lie\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ja lieber,", "tokens": ["Ja", "lie\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Als mir der J\u00fcngling war.", "tokens": ["Als", "mir", "der", "J\u00fcng\u00b7ling", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Lebens schroffen H\u00fcgel", "tokens": ["Des", "Le\u00b7bens", "schrof\u00b7fen", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erstiegen Hand in Hand", "tokens": ["Er\u00b7stie\u00b7gen", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir, wie es Wind und Wetter gab,", "tokens": ["Wir", ",", "wie", "es", "Wind", "und", "Wet\u00b7ter", "gab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hin \u00fcber Fels und Sand.", "tokens": ["Hin", "\u00fc\u00b7ber", "Fels", "und", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt ist der Abend milder,", "tokens": ["Jetzt", "ist", "der", "A\u00b7bend", "mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir stiegen sanft hinab,", "tokens": ["Wir", "stie\u00b7gen", "sanft", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dort am Fu\u00df erwartet uns", "tokens": ["Und", "dort", "am", "Fu\u00df", "er\u00b7war\u00b7tet", "uns"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zusammen", "tokens": ["Zu\u00b7sam\u00b7men"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Ein Brautgemach, das Grab.", "tokens": ["Ein", "Braut\u00b7ge\u00b7mach", ",", "das", "Grab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wolauf, Ihr S\u00f6hn' und T\u00f6chter,", "tokens": ["Wo\u00b7lauf", ",", "Ihr", "S\u00f6hn'", "und", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Singt unsern Hochgesang", "tokens": ["Singt", "un\u00b7sern", "Hoch\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und streuet Myrten vor uns her", "tokens": ["Und", "streu\u00b7et", "Myr\u00b7ten", "vor", "uns", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den kurzen Weg entlang!", "tokens": ["Den", "kur\u00b7zen", "Weg", "ent\u00b7lang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und preiset jede Stunde,", "tokens": ["Und", "prei\u00b7set", "je\u00b7de", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die uns der Himmel gab!", "tokens": ["Die", "uns", "der", "Him\u00b7mel", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umschatt' einst unser Grab!", "tokens": ["Um\u00b7schatt'", "einst", "un\u00b7ser", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}