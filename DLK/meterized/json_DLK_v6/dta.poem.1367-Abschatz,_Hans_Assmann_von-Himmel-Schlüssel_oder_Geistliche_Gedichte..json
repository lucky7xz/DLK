{"dta.poem.1367": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "JeSU/ deine Taffel fliehn/", "tokens": ["Je\u00b7SU", "/", "dei\u00b7ne", "Taf\u00b7fel", "fliehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Lebens-Quelle meiden/", "tokens": ["Ist", "die", "Le\u00b7bens\u00b7Quel\u00b7le", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solche nicht recht unterscheiden/", "tokens": ["Sol\u00b7che", "nicht", "recht", "un\u00b7ter\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist sich ins Gerichte ziehn/", "tokens": ["Ist", "sich", "ins", "Ge\u00b7rich\u00b7te", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohn dich mu\u00df die Seele schmachten", "tokens": ["Ohn", "dich", "mu\u00df", "die", "See\u00b7le", "schmach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VMFIN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und vergehn/ ohn Recht betrachten.", "tokens": ["Und", "ver\u00b7gehn", "/", "ohn", "Recht", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schau ich deine Hoheit an/", "tokens": ["Schau", "ich", "dei\u00b7ne", "Ho\u00b7heit", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie niedrig ich zu sch\u00e4tzen/", "tokens": ["Und", "wie", "nied\u00b7rig", "ich", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So umgiebt mich ein Entsetzen/", "tokens": ["So", "um\u00b7giebt", "mich", "ein", "Ent\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ich kaum begreiffen kan/", "tokens": ["Das", "ich", "kaum", "be\u00b7greif\u00b7fen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mitten stehn die schwachen Sinnen", "tokens": ["Mit\u00b7ten", "stehn", "die", "schwa\u00b7chen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter Furcht und Hoffnung innen.", "tokens": ["Un\u00b7ter", "Furcht", "und", "Hoff\u00b7nung", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "HeRR erleuchte meinen Geist", "tokens": ["HeRR", "er\u00b7leuch\u00b7te", "mei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Strahlen deiner Gnade/", "tokens": ["Durch", "die", "Strah\u00b7len", "dei\u00b7ner", "Gna\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich nicht auffs Hertze lade", "tokens": ["Da\u00df", "ich", "nicht", "auffs", "Hert\u00b7ze", "la\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "VVFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mich ins Verderben reist/", "tokens": ["Was", "mich", "ins", "Ver\u00b7der\u00b7ben", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch mich des nicht mag verzeihen/", "tokens": ["Auch", "mich", "des", "nicht", "mag", "ver\u00b7zei\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was zum Heyl mir soll gedeyen.", "tokens": ["Was", "zum", "Heyl", "mir", "soll", "ge\u00b7de\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Nimmer werd\u2019 ich w\u00fcrdig seyn/", "tokens": ["Nim\u00b7mer", "werd'", "ich", "w\u00fcr\u00b7dig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "(wenn du mich nicht selbst bereitest/", "tokens": ["(", "wenn", "du", "mich", "nicht", "selbst", "be\u00b7rei\u00b7test", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Furcht und b\u00f6se Lust bestreitest/)", "tokens": ["Furcht", "und", "b\u00f6\u00b7se", "Lust", "be\u00b7strei\u00b7test", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich zu mir zu nehmen ein;", "tokens": ["Dich", "zu", "mir", "zu", "neh\u00b7men", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich mu\u00df in des Todes Schatten", "tokens": ["Ich", "mu\u00df", "in", "des", "To\u00b7des", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dein ohn deine H\u00fclff\u2019 entrathen.", "tokens": ["Dein", "ohn", "dei\u00b7ne", "H\u00fclff'", "ent\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch rufft mich dein Wort zu dir/", "tokens": ["Doch", "rufft", "mich", "dein", "Wort", "zu", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du heist alle zu dir kommen/", "tokens": ["Du", "heist", "al\u00b7le", "zu", "dir", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mit Schuld sich \u00fcbernommen/", "tokens": ["Die", "mit", "Schuld", "sich", "\u00fc\u00b7ber\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses sagst du auch zu mir.", "tokens": ["Die\u00b7ses", "sagst", "du", "auch", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleiche Schuld und gleiches Ruffen", "tokens": ["Glei\u00b7che", "Schuld", "und", "glei\u00b7ches", "Ruf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4st mich gleich Erbarmnis hoffen.", "tokens": ["L\u00e4st", "mich", "gleich", "Er\u00b7barm\u00b7nis", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schick mich aber selber zu/", "tokens": ["Schick", "mich", "a\u00b7ber", "sel\u00b7ber", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie du mich bereit wilt haben/", "tokens": ["Wie", "du", "mich", "be\u00b7reit", "wilt", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVPP", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schencke mir die gutten Gaben/", "tokens": ["Schen\u00b7cke", "mir", "die", "gut\u00b7ten", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich selbst nichts Guttes thu/", "tokens": ["Weil", "ich", "selbst", "nichts", "Gut\u00b7tes", "thu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reu und Glauben/ Krafft und Willen/", "tokens": ["Reu", "und", "Glau\u00b7ben", "/", "Krafft", "und", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein Wort besser zu erf\u00fcllen.", "tokens": ["Dein", "Wort", "bes\u00b7ser", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00df mich einen Spiegel seyn/", "tokens": ["La\u00df", "mich", "ei\u00b7nen", "Spie\u00b7gel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dein reines Bildnis fassen/", "tokens": ["Der", "dein", "rei\u00b7nes", "Bild\u00b7nis", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in sich mag w\u00fcrcken lassen", "tokens": ["Und", "in", "sich", "mag", "w\u00fcr\u00b7cken", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "VMFIN", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Sonne Gnaden-Schein/", "tokens": ["Dei\u00b7ner", "Son\u00b7ne", "Gna\u00b7den\u00b7Schein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich deine Glutt empfinde/", "tokens": ["Da\u00df", "ich", "dei\u00b7ne", "Glutt", "emp\u00b7fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich zu neuer Lieb\u2019 entz\u00fcnde.", "tokens": ["Mich", "zu", "neu\u00b7er", "Lieb'", "ent\u00b7z\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}