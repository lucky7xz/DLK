{"textgrid.poem.31239": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Augen/ ihr verbuhlte Sonnen/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Augen/ ihr verbuhlte Sonnen/", "tokens": ["Au\u00b7gen", "/", "ihr", "ver\u00b7buhl\u00b7te", "Son\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zwingt noch immer euer Glantz", "tokens": ["zwingt", "noch", "im\u00b7mer", "eu\u00b7er", "Glantz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "mit durchau\u00df ambrirten Wonnen", "tokens": ["mit", "durch\u00b7au\u00df", "am\u00b7br\u00b7ir\u00b7ten", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "mich in seinen Zirckel-Dantz?", "tokens": ["mich", "in", "sei\u00b7nen", "Zir\u00b7ckel\u00b7Dantz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwischen B\u00fcchern und Bostillen", "tokens": ["Zwi\u00b7schen", "B\u00fc\u00b7chern", "und", "Bos\u00b7til\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sa\u00df ich da und fing mir Grillen/", "tokens": ["sa\u00df", "ich", "da", "und", "fing", "mir", "Gril\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "nein/ wa\u00df war ich meiner Seel", "tokens": ["nein", "/", "wa\u00df", "war", "ich", "mei\u00b7ner", "Seel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "PWAV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "bloh\u00df for graues M\u00fchl-Kameel!", "tokens": ["bloh\u00df", "for", "grau\u00b7es", "M\u00fchl\u00b7Ka\u00b7meel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stell ich mir ihr holdes Wesen", "tokens": ["Stell", "ich", "mir", "ihr", "hol\u00b7des", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "auch nur in Gedancken f\u00fcr/", "tokens": ["auch", "nur", "in", "Ge\u00b7dan\u00b7cken", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "gleich mit ihrem Fehder-Besen", "tokens": ["gleich", "mit", "ih\u00b7rem", "Feh\u00b7der\u00b7Be\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "steht sie dan in meiner Th\u00fcr.", "tokens": ["steht", "sie", "dan", "in", "mei\u00b7ner", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meine h\u00f6fflich-zahrte Sitten", "tokens": ["Mei\u00b7ne", "h\u00f6ff\u00b7lich\u00b7zahr\u00b7te", "Sit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "machen mich bey ihr gelitten/", "tokens": ["ma\u00b7chen", "mich", "bey", "ihr", "ge\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "VVPP", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "gleich so schleusst sie mir darauff", "tokens": ["gleich", "so", "schleusst", "sie", "mir", "dar\u00b7auff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "ihren Anmuhts-Garten auff!", "tokens": ["ih\u00b7ren", "Anm\u00b7uhts\u00b7Gar\u00b7ten", "auff", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "War\u00fcmb schn\u00e4beln sich die Tauben/", "tokens": ["Wa\u00b7r\u00fcmb", "schn\u00e4\u00b7beln", "sich", "die", "Tau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "war\u00fcmb dritt der Hahn das Huhn/", "tokens": ["wa\u00b7r\u00fcmb", "dritt", "der", "Hahn", "das", "Huhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "welches/ wie doch wohl zu glauben/", "tokens": ["wel\u00b7ches", "/", "wie", "doch", "wohl", "zu", "glau\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "KOKOM", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sie gewi\u00df \u00fcmbsonst nicht dhun?", "tokens": ["sie", "ge\u00b7wi\u00df", "\u00fcm\u00b7bsonst", "nicht", "dhun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "B\u00fcn ich gleich ein Klos au\u00df Erden/", "tokens": ["B\u00fcn", "ich", "gleich", "ein", "Klos", "au\u00df", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "m\u00f6cht ich dennoch selig werden/", "tokens": ["m\u00f6cht", "ich", "den\u00b7noch", "se\u00b7lig", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "ach/ noch bi\u00df in meinem Traum", "tokens": ["ach", "/", "noch", "bi\u00df", "in", "mei\u00b7nem", "Traum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["XY", "$(", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "r\u00e4ucht sie wie ein Weyhrauch-Baum!", "tokens": ["r\u00e4ucht", "sie", "wie", "ein", "Weyhrauch\u00b7Baum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nein/ du kanst mich nicht mehr lokken/", "tokens": ["Nein", "/", "du", "kanst", "mich", "nicht", "mehr", "lok\u00b7ken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nein/ du dhust mir nicht mehr weh/", "tokens": ["nein", "/", "du", "dhust", "mir", "nicht", "mehr", "weh", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wonnigen Narzissen-Armen", "tokens": ["won\u00b7ni\u00b7gen", "Na\u00b7rzis\u00b7sen\u00b7Ar\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mich so Zokker-s\u00fc\u00df \u00fcmbstrikkt/", "tokens": ["mich", "so", "Zok\u00b7ker\u00b7s\u00fc\u00df", "\u00fcmbs\u00b7trikkt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df mir kaum das Hertz noch tikkt!", "tokens": ["da\u00df", "mir", "kaum", "das", "Hertz", "noch", "tikkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}