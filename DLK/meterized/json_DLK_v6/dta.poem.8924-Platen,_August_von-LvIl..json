{"dta.poem.8924": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LvIl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Gestirne schnell und unbewu\u00dft erbleichen,", "tokens": ["Ge\u00b7stir\u00b7ne", "schnell", "und", "un\u00b7be\u00b7wu\u00dft", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erliegen m\u00f6cht' ich einst des Todes Streichen,", "tokens": ["Er\u00b7lie\u00b7gen", "m\u00f6cht'", "ich", "einst", "des", "To\u00b7des", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Sagen uns vom Pindaros berichten.", "tokens": ["Wie", "Sa\u00b7gen", "uns", "vom", "Pin\u00b7da\u00b7ros", "be\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich will ja nicht im Leben oder Dichten", "tokens": ["Ich", "will", "ja", "nicht", "im", "Le\u00b7ben", "o\u00b7der", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den gro\u00dfen Unerreichlichen erreichen,", "tokens": ["Den", "gro\u00b7\u00dfen", "Un\u00b7er\u00b7reich\u00b7li\u00b7chen", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6cht', o Freund, ihm nur im Tode gleichen;", "tokens": ["Ich", "m\u00f6cht'", ",", "o", "Freund", ",", "ihm", "nur", "im", "To\u00b7de", "glei\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "FM", "NN", "$,", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch h\u00f6re nun die sch\u00f6nste der Geschichten!", "tokens": ["Doch", "h\u00f6\u00b7re", "nun", "die", "sch\u00f6ns\u00b7te", "der", "Ge\u00b7schich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er sa\u00df im Schauspiel, vom Gesang beweget,", "tokens": ["Er", "sa\u00df", "im", "Schau\u00b7spiel", ",", "vom", "Ge\u00b7sang", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hatte, der Erm\u00fcdete, die Wangen", "tokens": ["Und", "hat\u00b7te", ",", "der", "Er\u00b7m\u00fc\u00b7de\u00b7te", ",", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf seines Lieblings sch\u00f6nes Knie geleget:", "tokens": ["Auf", "sei\u00b7nes", "Lieb\u00b7lings", "sch\u00f6\u00b7nes", "Knie", "ge\u00b7le\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als nun der Ch\u00f6re Melodien verklangen,", "tokens": ["Als", "nun", "der", "Ch\u00f6\u00b7re", "Me\u00b7lo\u00b7dien", "ver\u00b7klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will wecken ihn, der ihn so sanft geheget,", "tokens": ["Will", "we\u00b7cken", "ihn", ",", "der", "ihn", "so", "sanft", "ge\u00b7he\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch zu den G\u00f6ttern war er heimgegangen.", "tokens": ["Doch", "zu", "den", "G\u00f6t\u00b7tern", "war", "er", "heim\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}