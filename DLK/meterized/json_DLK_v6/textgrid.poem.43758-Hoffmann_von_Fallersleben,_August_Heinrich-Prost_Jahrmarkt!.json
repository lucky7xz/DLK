{"textgrid.poem.43758": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Prost Jahrmarkt!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prost Jahrmarkt! Kommt und kauft mir was!", "tokens": ["Prost", "Jahr\u00b7markt", "!", "Kommt", "und", "kauft", "mir", "was", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVFIN", "KON", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das und dies, dies und das!", "tokens": ["Das", "und", "dies", ",", "dies", "und", "das", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "$,", "PDS", "KON", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trommeln und Pfeifen, Trompeten und Geigen,", "tokens": ["Trom\u00b7meln", "und", "Pfei\u00b7fen", ",", "Trom\u00b7pe\u00b7ten", "und", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Kuchen und Torten, Datteln und Feigen,", "tokens": ["Ku\u00b7chen", "und", "Tor\u00b7ten", ",", "Dat\u00b7teln", "und", "Fei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Hammer und Glocke, Degen und Flinten,", "tokens": ["Ham\u00b7mer", "und", "Glo\u00b7cke", ",", "De\u00b7gen", "und", "Flin\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mandeln, Rosinen, N\u00fcss' und Korinthen!", "tokens": ["Man\u00b7deln", ",", "Ro\u00b7si\u00b7nen", ",", "N\u00fcss'", "und", "Ko\u00b7rin\u00b7then", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Kauft mir was!", "tokens": ["Kauft", "mir", "was", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Dies und das!", "tokens": ["Dies", "und", "das", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Prost Jahrmarkt! Kommt und kauft mir was!", "tokens": ["Prost", "Jahr\u00b7markt", "!", "Kommt", "und", "kauft", "mir", "was", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVFIN", "KON", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das und dies, dies und das!", "tokens": ["Das", "und", "dies", ",", "dies", "und", "das", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "$,", "PDS", "KON", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Puppen, Soldaten und Bilderb\u00fccher,", "tokens": ["Pup\u00b7pen", ",", "Sol\u00b7da\u00b7ten", "und", "Bil\u00b7der\u00b7b\u00fc\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "M\u00fctzen und H\u00fcte, Sch\u00fcrzen und T\u00fccher,", "tokens": ["M\u00fct\u00b7zen", "und", "H\u00fc\u00b7te", ",", "Sch\u00fcr\u00b7zen", "und", "T\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Hampelm\u00e4nner und Gummib\u00e4llchen,", "tokens": ["Ham\u00b7pel\u00b7m\u00e4n\u00b7ner", "und", "Gum\u00b7mi\u00b7b\u00e4ll\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Kegel und Kugeln, Gl\u00f6ckchen und Schellchen!", "tokens": ["Ke\u00b7gel", "und", "Ku\u00b7geln", ",", "Gl\u00f6ck\u00b7chen", "und", "Schell\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Kauft mir was!", "tokens": ["Kauft", "mir", "was", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Dies und das!", "tokens": ["Dies", "und", "das", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}