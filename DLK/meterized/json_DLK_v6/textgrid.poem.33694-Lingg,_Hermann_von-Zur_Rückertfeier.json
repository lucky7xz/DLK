{"textgrid.poem.33694": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Zur R\u00fcckertfeier", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu \u00bbSchwert und Leier\u00ab in dem Sturme,", "tokens": ["Zu", "\u00bb", "Schwert", "und", "Lei\u00b7er", "\u00ab", "in", "dem", "Stur\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "NN", "KON", "NN", "$(", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem sich Deutschland aufgerafft,", "tokens": ["In", "dem", "sich", "Deutschland", "auf\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NE", "VVFIN", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Klang wie des W\u00e4chters Ruf vom Turme", "tokens": ["Klang", "wie", "des", "W\u00e4ch\u00b7ters", "Ruf", "vom", "Tur\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "NN", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein Taglied, k\u00fchn und reckenhaft.", "tokens": ["Ein", "Ta\u00b7glied", ",", "k\u00fchn", "und", "re\u00b7cken\u00b7haft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das war ein R\u00fctteln an der Kette,", "tokens": ["Das", "war", "ein", "R\u00fct\u00b7teln", "an", "der", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als dr\u00f6hnend mit zum Waffengang", "tokens": ["Als", "dr\u00f6h\u00b7nend", "mit", "zum", "Waf\u00b7fen\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Schar geharnischter Sonette", "tokens": ["Die", "Schar", "ge\u00b7har\u00b7nischter", "So\u00b7net\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Des Geistes helle Schwerter schwang.", "tokens": ["Des", "Geis\u00b7tes", "hel\u00b7le", "Schwer\u00b7ter", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es war, als stiegen aus der Erde", "tokens": ["Es", "war", ",", "als", "stie\u00b7gen", "aus", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die eingesargten H\u00fcnen auf,", "tokens": ["Die", "ein\u00b7ge\u00b7sarg\u00b7ten", "H\u00fc\u00b7nen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als sprengten auf dem Musenpferde", "tokens": ["Als", "spreng\u00b7ten", "auf", "dem", "Mu\u00b7sen\u00b7pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Walk\u00fcren her im Sturmeslauf.", "tokens": ["Wal\u00b7k\u00fc\u00b7ren", "her", "im", "Stur\u00b7mes\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da h\u00f6rte man von edlem Zorne", "tokens": ["Da", "h\u00f6r\u00b7te", "man", "von", "ed\u00b7lem", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gedanken knirschen, mit Gestampf", "tokens": ["Ge\u00b7dan\u00b7ken", "knir\u00b7schen", ",", "mit", "Ge\u00b7stampf"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Versfu\u00df klirren und die Norne", "tokens": ["Den", "Ver\u00b7sfu\u00df", "klir\u00b7ren", "und", "die", "Nor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Das Schicksal weben um den Kampf.", "tokens": ["Das", "Schick\u00b7sal", "we\u00b7ben", "um", "den", "Kampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl glich ihr S\u00e4nger auch der Eiche,", "tokens": ["Wohl", "glich", "ihr", "S\u00e4n\u00b7ger", "auch", "der", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So dauernd und so m\u00e4cht'ger Kraft,", "tokens": ["So", "dau\u00b7ernd", "und", "so", "m\u00e4cht'\u00b7ger", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sprache weitverzweigte Reiche", "tokens": ["Der", "Spra\u00b7che", "weit\u00b7ver\u00b7zweig\u00b7te", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gestaltend voller Meisterschaft.", "tokens": ["Ge\u00b7stal\u00b7tend", "vol\u00b7ler", "Meis\u00b7ter\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er immer selbst, die freie Seele;", "tokens": ["Er", "im\u00b7mer", "selbst", ",", "die", "frei\u00b7e", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein deutsches Herz gab stets sich kund,", "tokens": ["Sein", "deut\u00b7sches", "Herz", "gab", "stets", "sich", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob Madrigal, ob Spruch, Ghasele,", "tokens": ["Ob", "Mad\u00b7ri\u00b7gal", ",", "ob", "Spruch", ",", "Gha\u00b7se\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ob Lieder sang sein reicher Mund.", "tokens": ["Ob", "Lie\u00b7der", "sang", "sein", "rei\u00b7cher", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie k\u00fcnstlich auch die Formen gipfeln,", "tokens": ["Wie", "k\u00fcnst\u00b7lich", "auch", "die", "For\u00b7men", "gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Grund wie streng und schlicht und stark!", "tokens": ["Der", "Grund", "wie", "streng", "und", "schlicht", "und", "stark", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJD", "KON", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An ihm war alles von den Wipfeln", "tokens": ["An", "ihm", "war", "al\u00b7les", "von", "den", "Wip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis zu der Wurzel gleiches Mark.", "tokens": ["Bis", "zu", "der", "Wur\u00b7zel", "glei\u00b7ches", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, unter diesem Eichenbaume", "tokens": ["Ja", ",", "un\u00b7ter", "die\u00b7sem", "Ei\u00b7chen\u00b7bau\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erbl\u00fchten Rosen, wunderzart;", "tokens": ["Er\u00b7bl\u00fch\u00b7ten", "Ro\u00b7sen", ",", "wun\u00b7derz\u00b7art", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Weisheit lag vertieft im Traume,", "tokens": ["Die", "Weis\u00b7heit", "lag", "ver\u00b7tieft", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beim Eisen der Juwel verwahrt.", "tokens": ["Beim", "Ei\u00b7sen", "der", "Ju\u00b7wel", "ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Dem Dichter, der die s\u00fc\u00dfen Lieder,", "tokens": ["Dem", "Dich\u00b7ter", ",", "der", "die", "s\u00fc\u00b7\u00dfen", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den \u00bbLiebesfr\u00fchling\u00ab sang, ein All", "tokens": ["Den", "\u00bb", "Lie\u00b7bes\u00b7fr\u00fch\u00b7ling", "\u00ab", "sang", ",", "ein", "All"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "$(", "NN", "$(", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gl\u00fcck und Treue, bl\u00fch' der Flieder", "tokens": ["Von", "Gl\u00fcck", "und", "Treu\u00b7e", ",", "bl\u00fch'", "der", "Flie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und singe stets die Nachtigall.", "tokens": ["Und", "sin\u00b7ge", "stets", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Dichters Denkmal wird gesungen,", "tokens": ["Des", "Dich\u00b7ters", "Denk\u00b7mal", "wird", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht nur gebaut aus Erz und Stein:", "tokens": ["Nicht", "nur", "ge\u00b7baut", "aus", "Erz", "und", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stets wird es in Begeisterungen", "tokens": ["Stets", "wird", "es", "in", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und im lebend'gen Worte sein.", "tokens": ["Und", "im", "le\u00b7ben\u00b7d'\u00b7gen", "Wor\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}}}}