{"dta.poem.21156": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Linde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du majest\u00e4tsche Linde,", "tokens": ["Du", "ma\u00b7jes\u00b7t\u00e4t\u00b7sche", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Worunter oft Lucinde", "tokens": ["Wo\u00b7run\u00b7ter", "oft", "Lu\u00b7cin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit ruhigem Gem\u00fcth", "tokens": ["Mit", "ru\u00b7hi\u00b7gem", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der Nacht entgegen sieht;", "tokens": ["Der", "Nacht", "ent\u00b7ge\u00b7gen", "sieht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O sch\u00fctte von den Aesten,", "tokens": ["O", "sch\u00fct\u00b7te", "von", "den", "A\u00b7es\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Bewegt von sanften Westen,", "tokens": ["Be\u00b7wegt", "von", "sanf\u00b7ten", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Bl\u00fcthen s\u00fc\u00dfen Duft", "tokens": ["Der", "Bl\u00fc\u00b7then", "s\u00fc\u00b7\u00dfen", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In die gek\u00fchlte Luft.", "tokens": ["In", "die", "ge\u00b7k\u00fchl\u00b7te", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die einsame Lucinde", "tokens": ["Die", "ein\u00b7sa\u00b7me", "Lu\u00b7cin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Genie\u00dft dich nur, o Linde,", "tokens": ["Ge\u00b7nie\u00dft", "dich", "nur", ",", "o", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00f6mmt, als Nachbarin,", "tokens": ["Und", "k\u00f6mmt", ",", "als", "Nach\u00b7ba\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In deinen Schatten hin.", "tokens": ["In", "dei\u00b7nen", "Schat\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Bl\u00fcthen \u00fcberdecket", "tokens": ["Von", "Bl\u00fc\u00b7then", "\u00fc\u00b7berd\u00b7e\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hast du ihr Herz erwecket;", "tokens": ["Hast", "du", "ihr", "Herz", "er\u00b7we\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie oft hat deine Pracht", "tokens": ["Wie", "oft", "hat", "dei\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sie nicht entz\u00fcckt gemacht!", "tokens": ["Sie", "nicht", "ent\u00b7z\u00fcckt", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So bald die ersten Stralen", "tokens": ["So", "bald", "die", "ers\u00b7ten", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wilden H\u00fcgel malen,", "tokens": ["Die", "wil\u00b7den", "H\u00fc\u00b7gel", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gr\u00fc\u00dft dich der V\u00f6gel Ton,", "tokens": ["Gr\u00fc\u00dft", "dich", "der", "V\u00f6\u00b7gel", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und auch Lucinde schon.", "tokens": ["Und", "auch", "Lu\u00b7cin\u00b7de", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn, mit tr\u00e4gen Rossen,", "tokens": ["Und", "wenn", ",", "mit", "tr\u00e4\u00b7gen", "Ros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Ackersmann verdrossen", "tokens": ["Der", "A\u00b7ckers\u00b7mann", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nach seinen H\u00fctten zieht,", "tokens": ["Nach", "sei\u00b7nen", "H\u00fct\u00b7ten", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gr\u00fc\u00dft dich ihr muntres Lied.", "tokens": ["Gr\u00fc\u00dft", "dich", "ihr", "mun\u00b7tres", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O bl\u00fche f\u00fcr Lucinden!", "tokens": ["O", "bl\u00fc\u00b7he", "f\u00fcr", "Lu\u00b7cin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jhr Herz nur kan empfinden,", "tokens": ["Ihr", "Herz", "nur", "kan", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch wessen starke Macht", "tokens": ["Durch", "wes\u00b7sen", "star\u00b7ke", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein Haupt in Wolken lacht.", "tokens": ["Dein", "Haupt", "in", "Wol\u00b7ken", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mehr kan ein Kleist nicht f\u00fchlen,", "tokens": ["Mehr", "kan", "ein", "Kleist", "nicht", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn er, am Bach im K\u00fchlen,", "tokens": ["Wenn", "er", ",", "am", "Bach", "im", "K\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf Thomsons Laute spielt,", "tokens": ["Auf", "Thom\u00b7sons", "Lau\u00b7te", "spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als hier Lucinde f\u00fchlt.", "tokens": ["Als", "hier", "Lu\u00b7cin\u00b7de", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es schleicht mit stillen Schritten", "tokens": ["Es", "schleicht", "mit", "stil\u00b7len", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Abend um die H\u00fctten,", "tokens": ["Der", "A\u00b7bend", "um", "die", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der hohe Wald wird grau,", "tokens": ["Der", "ho\u00b7he", "Wald", "wird", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Wiesen tr\u00e4nkt der Thau;", "tokens": ["Und", "Wie\u00b7sen", "tr\u00e4nkt", "der", "Thau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O schicke durch die L\u00fcfte", "tokens": ["O", "schi\u00b7cke", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Viel tausend s\u00fc\u00dfe D\u00fcfte,", "tokens": ["Viel", "tau\u00b7send", "s\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zum Anwunsch sanfter Ruh,", "tokens": ["Zum", "An\u00b7wunsch", "sanf\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Lucindens Fenster zu!", "tokens": ["Lu\u00b7cin\u00b7dens", "Fens\u00b7ter", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}