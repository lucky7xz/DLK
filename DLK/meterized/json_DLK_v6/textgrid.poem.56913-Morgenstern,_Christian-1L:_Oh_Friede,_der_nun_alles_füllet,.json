{"textgrid.poem.56913": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oh Friede, der nun alles f\u00fcllet,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh Friede, der nun alles f\u00fcllet,", "tokens": ["Oh", "Frie\u00b7de", ",", "der", "nun", "al\u00b7les", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "erf\u00fcll auch uns mit s\u00fc\u00dfer Ruh,", "tokens": ["er\u00b7f\u00fcll", "auch", "uns", "mit", "s\u00fc\u00b7\u00dfer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und bis ein Tag sich neu enth\u00fcllet,", "tokens": ["und", "bis", "ein", "Tag", "sich", "neu", "ent\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "deck uns mit trauten Tr\u00e4umen zu.", "tokens": ["deck", "uns", "mit", "trau\u00b7ten", "Tr\u00e4u\u00b7men", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wie manches, was des Tages Wille", "tokens": ["Wie", "man\u00b7ches", ",", "was", "des", "Ta\u00b7ges", "Wil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mit rechter Klarheit nicht ergreift,", "tokens": ["mit", "rech\u00b7ter", "Klar\u00b7heit", "nicht", "er\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dem hilf, da\u00df es in deiner Stille", "tokens": ["dem", "hilf", ",", "da\u00df", "es", "in", "dei\u00b7ner", "Stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zu freundlicher Vollendung reift!", "tokens": ["zu", "freund\u00b7li\u00b7cher", "Vol\u00b7len\u00b7dung", "reift", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wen Schicksalsschl\u00e4ge grausam trafen,", "tokens": ["Wen", "Schick\u00b7sals\u00b7schl\u00e4\u00b7ge", "grau\u00b7sam", "tra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "den la\u00df vergessen, was geschehn;", "tokens": ["den", "la\u00df", "ver\u00b7ges\u00b7sen", ",", "was", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wer neid\u2013 und ha\u00dferf\u00fcllt entschlafen,", "tokens": ["wer", "neid", "\u2013", "und", "ha\u00b7\u00dfer\u00b7f\u00fcllt", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KON", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den la\u00df vers\u00f6hnt den Morgen sehn!", "tokens": ["den", "la\u00df", "ver\u00b7s\u00f6hnt", "den", "Mor\u00b7gen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So allem, dem gleich uns auf Erden", "tokens": ["So", "al\u00b7lem", ",", "dem", "gleich", "uns", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zu Teil des Lebens schwankes Los,", "tokens": ["zu", "Teil", "des", "Le\u00b7bens", "schwan\u00b7kes", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "la\u00df deines Segens Tiefe werden,", "tokens": ["la\u00df", "dei\u00b7nes", "Se\u00b7gens", "Tie\u00b7fe", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "gib Kraft aus deinem heiligen Scho\u00df!", "tokens": ["gib", "Kraft", "aus", "dei\u00b7nem", "hei\u00b7li\u00b7gen", "Scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}