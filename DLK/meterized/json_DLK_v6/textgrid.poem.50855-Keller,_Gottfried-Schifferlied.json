{"textgrid.poem.50855": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Schifferlied", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon hat die Nacht den Silberschrein", "tokens": ["Schon", "hat", "die", "Nacht", "den", "Sil\u00b7ber\u00b7schrein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels aufgetan:", "tokens": ["Des", "Him\u00b7mels", "auf\u00b7ge\u00b7tan", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun sp\u00fclt der See den Widerschein", "tokens": ["Nun", "sp\u00fclt", "der", "See", "den", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dir, zu dir hinan!", "tokens": ["Zu", "dir", ",", "zu", "dir", "hi\u00b7nan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wach auf, Marian!", "tokens": ["Wach", "auf", ",", "Ma\u00b7ri\u00b7an", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und in dem Glanze schaukelt sich", "tokens": ["Und", "in", "dem", "Glan\u00b7ze", "schau\u00b7kelt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leichter dunkler Kahn,", "tokens": ["Ein", "leich\u00b7ter", "dunk\u00b7ler", "Kahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der aber tr\u00e4gt und schaukelt mich", "tokens": ["Der", "a\u00b7ber", "tr\u00e4gt", "und", "schau\u00b7kelt", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dir, zu dir hinan!", "tokens": ["Zu", "dir", ",", "zu", "dir", "hi\u00b7nan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wach auf, Marian!", "tokens": ["Wach", "auf", ",", "Ma\u00b7ri\u00b7an", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6re schon den Brunnen gehn", "tokens": ["Ich", "h\u00f6\u00b7re", "schon", "den", "Brun\u00b7nen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Pf\u00f6rtlein nebenan,", "tokens": ["Dem", "Pf\u00f6rt\u00b7lein", "ne\u00b7be\u00b7nan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Und dieses hat ein frisches Wehn", "tokens": ["Und", "die\u00b7ses", "hat", "ein", "fri\u00b7sches", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soeben aufgetan.", "tokens": ["Soe\u00b7ben", "auf\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wach auf, Marian!", "tokens": ["Wach", "auf", ",", "Ma\u00b7ri\u00b7an", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchle, wie die Erde schwellt", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "wie", "die", "Er\u00b7de", "schwellt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Himmel leis hinan;", "tokens": ["Zum", "Him\u00b7mel", "leis", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach Liebe d\u00fcrstet alle Welt \u2013", "tokens": ["Nach", "Lie\u00b7be", "d\u00fcrs\u00b7tet", "al\u00b7le", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schifflein, leg dich an!", "tokens": ["Mein", "Schif\u00b7flein", ",", "leg", "dich", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wach auf, Marian!", "tokens": ["Wach", "auf", ",", "Ma\u00b7ri\u00b7an", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Dies Lied hat mir ein Bursch gemacht,", "tokens": ["Dies", "Lied", "hat", "mir", "ein", "Bursch", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der fuhr in meinem Kahn;", "tokens": ["Der", "fuhr", "in", "mei\u00b7nem", "Kahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat's f\u00fcr dich und mich erdacht,", "tokens": ["Er", "hat's", "f\u00fcr", "dich", "und", "mich", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bet f\u00fcr ihn, Marian!", "tokens": ["Bet", "f\u00fcr", "ihn", ",", "Ma\u00b7ri\u00b7an", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wach auf, Marian!", "tokens": ["Wach", "auf", ",", "Ma\u00b7ri\u00b7an", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}