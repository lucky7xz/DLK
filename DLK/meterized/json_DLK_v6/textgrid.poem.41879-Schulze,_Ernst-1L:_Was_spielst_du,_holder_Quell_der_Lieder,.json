{"textgrid.poem.41879": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was spielst du, holder Quell der Lieder,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was spielst du, holder Quell der Lieder,", "tokens": ["Was", "spielst", "du", ",", "hol\u00b7der", "Quell", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sehnsuchtsvoll in meiner Brust?", "tokens": ["So", "sehn\u00b7suchts\u00b7voll", "in", "mei\u00b7ner", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie spiegelt sich der Lenz der Lust", "tokens": ["Nie", "spie\u00b7gelt", "sich", "der", "Lenz", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner hellen Tiefe wieder!", "tokens": ["In", "dei\u00b7ner", "hel\u00b7len", "Tie\u00b7fe", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du Zauberglanz der Liebesstunden,", "tokens": ["Du", "Zau\u00b7ber\u00b7glanz", "der", "Lie\u00b7bess\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du D\u00e4mmrung zarter Tr\u00e4umerey,", "tokens": ["Du", "D\u00e4mm\u00b7rung", "zar\u00b7ter", "Tr\u00e4u\u00b7me\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bl\u00fchender, du gold'ner May,", "tokens": ["Du", "bl\u00fc\u00b7hen\u00b7der", ",", "du", "gold'\u00b7ner", "May", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie bist du schnell dahin geschwunden!", "tokens": ["Wie", "bist", "du", "schnell", "da\u00b7hin", "ge\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du Born, worin das Bild der Liebe", "tokens": ["Du", "Born", ",", "wo\u00b7rin", "das", "Bild", "der", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wie schwimmt auf deinem feuchten Gr\u00fcn", "tokens": ["Wie", "schwimmt", "auf", "dei\u00b7nem", "feuch\u00b7ten", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel jetzt so schwer und tr\u00fcbe;", "tokens": ["Der", "Him\u00b7mel", "jetzt", "so", "schwer", "und", "tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verschwiegnes Thal, wo mir am Morgen", "tokens": ["Ver\u00b7schwieg\u00b7nes", "Thal", ",", "wo", "mir", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nste Traum der Liebe kam,", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Traum", "der", "Lie\u00b7be", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo mir der Abend Alles nahm,", "tokens": ["Wo", "mir", "der", "A\u00b7bend", "Al\u00b7les", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du liegst wohl tief im Schnee verborgen!", "tokens": ["Du", "liegst", "wohl", "tief", "im", "Schnee", "ver\u00b7bor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du Kranz aus frischem Eichenlaube,", "tokens": ["Du", "Kranz", "aus", "fri\u00b7schem", "Ei\u00b7chen\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den tr\u00f6stend mir das Mitleid wand,", "tokens": ["Den", "tr\u00f6s\u00b7tend", "mir", "das", "Mit\u00b7leid", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schmerzliches, du s\u00fc\u00dfes Pfand,", "tokens": ["Du", "schmerz\u00b7li\u00b7ches", ",", "du", "s\u00fc\u00b7\u00dfes", "Pfand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du ruhst nun l\u00e4ngst verwelkt im Staube!", "tokens": ["Du", "ruhst", "nun", "l\u00e4ngst", "ver\u00b7welkt", "im", "Stau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr, die mein heimliches Verlangen", "tokens": ["Ihr", ",", "die", "mein", "heim\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als zarte Boten oft enth\u00fcllt,", "tokens": ["Als", "zar\u00b7te", "Bo\u00b7ten", "oft", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Mayenbl\u00fcmlein klar und mild,", "tokens": ["Ihr", "Ma\u00b7yen\u00b7bl\u00fcm\u00b7lein", "klar", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr seyd wohl l\u00e4ngst dahingegangen!", "tokens": ["Ihr", "seyd", "wohl", "l\u00e4ngst", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch hat euch Liebesthau befeuchtet,", "tokens": ["Doch", "hat", "euch", "Lie\u00b7best\u00b7hau", "be\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Liebe hat euch abgepfl\u00fcckt,", "tokens": ["Und", "Lie\u00b7be", "hat", "euch", "ab\u00b7ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Lieb' euch traurend angeblickt,", "tokens": ["Und", "Lieb'", "euch", "trau\u00b7rend", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ihr den Kelch zum Tode neigtet!", "tokens": ["Als", "ihr", "den", "Kelch", "zum", "To\u00b7de", "neig\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nach meiner Lust wird Keiner fragen,", "tokens": ["Nach", "mei\u00b7ner", "Lust", "wird", "Kei\u00b7ner", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Keiner ahnet meine Noth,", "tokens": ["Und", "Kei\u00b7ner", "ah\u00b7net", "mei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Keiner weint um meinen Tod! \u2013", "tokens": ["Und", "Kei\u00b7ner", "weint", "um", "mei\u00b7nen", "Tod", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Brich, armes Herz, du darfst nicht klagen!", "tokens": ["Brich", ",", "ar\u00b7mes", "Herz", ",", "du", "darfst", "nicht", "kla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}