{"dta.poem.9690": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber einen ihr genommenen ku\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was klagt Belise viel mich eines diebstals an/", "tokens": ["Was", "klagt", "Be\u00b7li\u00b7se", "viel", "mich", "ei\u00b7nes", "diebs\u00b7tals", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchrt um einen ku\u00df proce\u00df auf leib und leben?", "tokens": ["Und", "f\u00fchrt", "um", "ei\u00b7nen", "ku\u00df", "pro\u00b7ce\u00df", "auf", "leib", "und", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich leide/ was das recht f\u00fcr straffe drauf gethan/", "tokens": ["Ich", "lei\u00b7de", "/", "was", "das", "recht", "f\u00fcr", "straf\u00b7fe", "drauf", "ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "ART", "ADJD", "APPR", "VVFIN", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und will/ was ich entwandt/ ihr f\u00fcnffach wiedergeben.", "tokens": ["Und", "will", "/", "was", "ich", "ent\u00b7wandt", "/", "ihr", "f\u00fcnf\u00b7fach", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "wo