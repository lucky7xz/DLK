{"textgrid.poem.25025": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon rauscht der Herbst durchs Waldgezweig,", "tokens": ["Schon", "rauscht", "der", "Herbst", "durchs", "Wald\u00b7ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Eiche, Buche, Linde", "tokens": ["Und", "Ei\u00b7che", ",", "Bu\u00b7che", ",", "Lin\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Streun ihre Bl\u00e4tter, gelb und bleich,", "tokens": ["Streun", "ih\u00b7re", "Bl\u00e4t\u00b7ter", ",", "gelb", "und", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In die Oktoberwinde.", "tokens": ["In", "die", "Ok\u00b7to\u00b7ber\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Doch eine Buche, die sich k\u00fchn", "tokens": ["Doch", "ei\u00b7ne", "Bu\u00b7che", ",", "die", "sich", "k\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hebt aus der andern Kreise,", "tokens": ["Hebt", "aus", "der", "an\u00b7dern", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bleibt seit dem ersten Lenzhauch gr\u00fcn", "tokens": ["Bleibt", "seit", "dem", "ers\u00b7ten", "Lenz\u00b7hauch", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis zu des Winters Eise.", "tokens": ["Bis", "zu", "des", "Win\u00b7ters", "Ei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als Margaretens Namenszug", "tokens": ["Als", "Mar\u00b7ga\u00b7re\u00b7tens", "Na\u00b7mens\u00b7zug"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich eingrub ihrem Stamme,", "tokens": ["Ich", "ein\u00b7grub", "ih\u00b7rem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So stolz aus ihr zum Himmel schlug", "tokens": ["So", "stolz", "aus", "ihr", "zum", "Him\u00b7mel", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Wipfels gr\u00fcne Flamme.", "tokens": ["Des", "Wip\u00b7fels", "gr\u00fc\u00b7ne", "Flam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch lange, wenn, des Herbstes Raub,", "tokens": ["Noch", "lan\u00b7ge", ",", "wenn", ",", "des", "Herbs\u00b7tes", "Raub", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der andern Bl\u00e4tter fallen,", "tokens": ["Der", "an\u00b7dern", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bebt von der Elfen Tanz ihr Laub,", "tokens": ["Bebt", "von", "der", "El\u00b7fen", "Tanz", "ihr", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Lied der Nachtigallen.", "tokens": ["Dem", "Lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und bei der V\u00f6gel Melodie,", "tokens": ["Und", "bei", "der", "V\u00f6\u00b7gel", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Geister frohem Reigen", "tokens": ["Der", "Geis\u00b7ter", "fro\u00b7hem", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Webt s\u00fc\u00dfe Liebestr\u00e4ume sie", "tokens": ["Webt", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be\u00b7str\u00e4u\u00b7me", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In immer gr\u00fcnen Zweigen.", "tokens": ["In", "im\u00b7mer", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}