{"textgrid.poem.44552": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Fortschritt-M\u00e4nner", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch kann mein Lied, ich f\u00fchls, nicht mehr gefallen,", "tokens": ["Euch", "kann", "mein", "Lied", ",", "ich", "f\u00fchls", ",", "nicht", "mehr", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "PPER", "ADV", "$,", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist zu karg, zu d\u00fcrftig und zu klein,", "tokens": ["Es", "ist", "zu", "karg", ",", "zu", "d\u00fcrf\u00b7tig", "und", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die ihr so weit in jedem und in allen,", "tokens": ["Die", "ihr", "so", "weit", "in", "je\u00b7dem", "und", "in", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "PIAT", "KON", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fa\u00dft euch nicht gern in enge Schranken ein.", "tokens": ["Fa\u00dft", "euch", "nicht", "gern", "in", "en\u00b7ge", "Schran\u00b7ken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die Au\u00dfenwelt verf\u00fchrte meine Blicke,", "tokens": ["Die", "Au\u00b7\u00dfen\u00b7welt", "ver\u00b7f\u00fchr\u00b7te", "mei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In der sich alles rundet und erg\u00e4nzt,", "tokens": ["In", "der", "sich", "al\u00b7les", "run\u00b7det", "und", "er\u00b7g\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PIS", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Leeres irgend, nirgends eine L\u00fccke,", "tokens": ["Kein", "Lee\u00b7res", "ir\u00b7gend", ",", "nir\u00b7gends", "ei\u00b7ne", "L\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jede Bildung voll und scharf begrenzt.", "tokens": ["Und", "je\u00b7de", "Bil\u00b7dung", "voll", "und", "scharf", "be\u00b7grenzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das sucht ich nun im Geiste nachzuahmen,", "tokens": ["Das", "sucht", "ich", "nun", "im", "Geis\u00b7te", "nach\u00b7zu\u00b7ah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und da die Kraft mir nicht so reichlich quillt,", "tokens": ["Und", "da", "die", "Kraft", "mir", "nicht", "so", "reich\u00b7lich", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4hlt ich bescheidne, strenggeschlo\u00dfne Rahmen", "tokens": ["W\u00e4hlt", "ich", "be\u00b7scheid\u00b7ne", ",", "streng\u00b7ge\u00b7schlo\u00df\u00b7ne", "Rah\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr mein dem Leben nachgeschaffnes Bild.", "tokens": ["F\u00fcr", "mein", "dem", "Le\u00b7ben", "nach\u00b7ge\u00b7schaff\u00b7nes", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr aber habt der Wesen Grund ergr\u00fcndet,", "tokens": ["Ihr", "a\u00b7ber", "habt", "der", "We\u00b7sen", "Grund", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gottheit selber liegt euch auf der Hand,", "tokens": ["Die", "Got\u00b7theit", "sel\u00b7ber", "liegt", "euch", "auf", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ja ihr etwas unbegreiflich findet,", "tokens": ["Wenn", "ja", "ihr", "et\u00b7was", "un\u00b7be\u00b7greif\u00b7lich", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ists, da\u00df so lang mans unbegreiflich fand.", "tokens": ["Ists", ",", "da\u00df", "so", "lang", "mans", "un\u00b7be\u00b7greif\u00b7lich", "fand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "ADJD", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Sch\u00f6ne, das ein R\u00e4tsel uns, den Schwachen,", "tokens": ["Das", "Sch\u00f6\u00b7ne", ",", "das", "ein", "R\u00e4t\u00b7sel", "uns", ",", "den", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr habts gel\u00f6st durch Vordersatz und Schlu\u00df,", "tokens": ["Ihr", "habts", "ge\u00b7l\u00f6st", "durch", "Vor\u00b7der\u00b7satz", "und", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwar k\u00f6nnt ihrs vorderhand nicht wirklich machen,", "tokens": ["Zwar", "k\u00f6nnt", "ihrs", "vor\u00b7der\u00b7hand", "nicht", "wirk\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wi\u00dft ihr, wie mans machen soll und mu\u00df.", "tokens": ["Doch", "wi\u00dft", "ihr", ",", "wie", "mans", "ma\u00b7chen", "soll", "und", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So schreitet ihr denn vor mit Riesenschritte,", "tokens": ["So", "schrei\u00b7tet", "ihr", "denn", "vor", "mit", "Rie\u00b7sen\u00b7schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tat selbst, die sonst Denkern schwer gelingt,", "tokens": ["Die", "Tat", "selbst", ",", "die", "sonst", "Den\u00b7kern", "schwer", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Habt ihr erfa\u00dft, ob zwar nach Dichter-Sitte,", "tokens": ["Habt", "ihr", "er\u00b7fa\u00dft", ",", "ob", "zwar", "nach", "Dich\u00b7ter\u00b7Sit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "KOUS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Handlung nennt, auch Fabel, was er singt.", "tokens": ["Der", "Hand\u00b7lung", "nennt", ",", "auch", "Fa\u00b7bel", ",", "was", "er", "singt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Baum der Selbstmacht ward durch euch ger\u00fcttelt,", "tokens": ["Der", "Baum", "der", "Selbst\u00b7macht", "ward", "durch", "euch", "ge\u00b7r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur ist er knorrig und bewahrt die Frucht,", "tokens": ["Nur", "ist", "er", "knor\u00b7rig", "und", "be\u00b7wahrt", "die", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn sie je der Sturm vom Aste sch\u00fcttelt,", "tokens": ["Doch", "wenn", "sie", "je", "der", "Sturm", "vom", "As\u00b7te", "sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr lest sie auf und habt dann, was gesucht.", "tokens": ["Ihr", "lest", "sie", "auf", "und", "habt", "dann", ",", "was", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "ADV", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "F\u00fcr euch nun, die dem \u00dcberflu\u00df im Scho\u00dfe,", "tokens": ["F\u00fcr", "euch", "nun", ",", "die", "dem", "\u00dc\u00b7berf\u00b7lu\u00df", "im", "Scho\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr versteht der Sch\u00f6pfung Allmachtruf,", "tokens": ["Die", "ihr", "ver\u00b7steht", "der", "Sch\u00f6p\u00b7fung", "All\u00b7macht\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor denen klar das Kleine und das Gro\u00dfe,", "tokens": ["Vor", "de\u00b7nen", "klar", "das", "Klei\u00b7ne", "und", "das", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "ART", "ADJA", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist freilich arm, was ich bescheiden schuf.", "tokens": ["Ist", "frei\u00b7lich", "arm", ",", "was", "ich", "be\u00b7schei\u00b7den", "schuf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Allein bedenkt doch! die Natur ist sparsam,", "tokens": ["Al\u00b7lein", "be\u00b7denkt", "doch", "!", "die", "Na\u00b7tur", "ist", "spar\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Gleichem, seit dem Anfang h\u00e4lt sie Haus,", "tokens": ["Mit", "Glei\u00b7chem", ",", "seit", "dem", "An\u00b7fang", "h\u00e4lt", "sie", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was allzuviel, nimmt r\u00fcck sie in Gewahrsam", "tokens": ["Was", "all\u00b7zu\u00b7viel", ",", "nimmt", "r\u00fcck", "sie", "in", "Ge\u00b7wahr\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "$,", "VVFIN", "PTKVZ", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gleicht durch Kargheit die Verschwendung aus.", "tokens": ["Und", "gleicht", "durch", "Karg\u00b7heit", "die", "Ver\u00b7schwen\u00b7dung", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Auf jede Zeit von Recken und Heroen", "tokens": ["Auf", "je\u00b7de", "Zeit", "von", "Re\u00b7cken", "und", "He\u00b7roen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Folgt eine andre, die wie andre klein,", "tokens": ["Folgt", "ei\u00b7ne", "and\u00b7re", ",", "die", "wie", "and\u00b7re", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "PRELS", "KOKOM", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und die Giganten, die dem Himmel drohen,", "tokens": ["Und", "die", "Gi\u00b7gan\u00b7ten", ",", "die", "dem", "Him\u00b7mel", "dro\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie schrumpfen auf das Ma\u00df von Menschen ein.", "tokens": ["Sie", "schrump\u00b7fen", "auf", "das", "Ma\u00df", "von", "Men\u00b7schen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So folgt \u2013 die Form, die euch erzeugt, gebrochen \u2013", "tokens": ["So", "folgt", "\u2013", "die", "Form", ",", "die", "euch", "er\u00b7zeugt", ",", "ge\u00b7bro\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Enkelvolk, das sich um euch bewegt,", "tokens": ["Ein", "En\u00b7kel\u00b7volk", ",", "das", "sich", "um", "euch", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie um fossile, m\u00e4chtge Mammutknochen", "tokens": ["Wie", "um", "fos\u00b7si\u00b7le", ",", "m\u00e4cht\u00b7ge", "Mam\u00b7mut\u00b7kno\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "NE", "$,", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Von Tieren, wie die Welt sie nicht mehr tr\u00e4gt.", "tokens": ["Von", "Tie\u00b7ren", ",", "wie", "die", "Welt", "sie", "nicht", "mehr", "tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Das, von den Worten fl\u00fcchtend zu den Sachen", "tokens": ["Das", ",", "von", "den", "Wor\u00b7ten", "fl\u00fcch\u00b7tend", "zu", "den", "Sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "