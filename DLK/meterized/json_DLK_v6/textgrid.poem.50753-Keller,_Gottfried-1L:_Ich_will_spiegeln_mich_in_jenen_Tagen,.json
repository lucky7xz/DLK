{"textgrid.poem.50753": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will spiegeln mich in jenen Tagen,", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will spiegeln mich in jenen Tagen,", "tokens": ["Ich", "will", "spie\u00b7geln", "mich", "in", "je\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die wie Lindenwipfelwehn entflohn,", "tokens": ["Die", "wie", "Lin\u00b7den\u00b7wip\u00b7fel\u00b7wehn", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo die Silbersaite, angeschlagen,", "tokens": ["Wo", "die", "Sil\u00b7ber\u00b7sai\u00b7te", ",", "an\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Klar, doch bebend, gab den ersten Ton,", "tokens": ["Klar", ",", "doch", "be\u00b7bend", ",", "gab", "den", "ers\u00b7ten", "Ton", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Der mein Leben lang,", "tokens": ["Der", "mein", "Le\u00b7ben", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Erst heut noch, widerklang,", "tokens": ["Erst", "heut", "noch", ",", "wi\u00b7der\u00b7klang", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob die Saite l\u00e4ngst zerrissen schon!", "tokens": ["Ob", "die", "Sai\u00b7te", "l\u00e4ngst", "zer\u00b7ris\u00b7sen", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wo ich ohne Tugend, ohne S\u00fcnde,", "tokens": ["Wo", "ich", "oh\u00b7ne", "Tu\u00b7gend", ",", "oh\u00b7ne", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blank wie Schnee, rein vor der Sonne lag;", "tokens": ["Blank", "wie", "Schnee", ",", "rein", "vor", "der", "Son\u00b7ne", "lag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo dem Kinderauge noch die Binde", "tokens": ["Wo", "dem", "Kin\u00b7der\u00b7au\u00b7ge", "noch", "die", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lind verbarg den blendend hellen Tag!", "tokens": ["Lind", "ver\u00b7barg", "den", "blen\u00b7dend", "hel\u00b7len", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Du entschwundne Welt,", "tokens": ["Du", "ent\u00b7schwund\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Klingst \u00fcber Wald und Feld", "tokens": ["Klingst", "\u00fc\u00b7ber", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hinter mir, wie ferner Wachtelschlag!", "tokens": ["Hin\u00b7ter", "mir", ",", "wie", "fer\u00b7ner", "Wach\u00b7tel\u00b7schlag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAV", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie so fabelhaft ist hingegangen", "tokens": ["Wie", "so", "fa\u00b7bel\u00b7haft", "ist", "hin\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jene Zeit voll zarter Fr\u00fchlingspracht,", "tokens": ["Je\u00b7ne", "Zeit", "voll", "zar\u00b7ter", "Fr\u00fch\u00b7lings\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo, von Mutterliebe noch umfangen,", "tokens": ["Wo", ",", "von", "Mut\u00b7ter\u00b7lie\u00b7be", "noch", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schon die Jugendliebe leis erwacht',", "tokens": ["Schon", "die", "Ju\u00b7gend\u00b7lie\u00b7be", "leis", "er\u00b7wacht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wie, vom Sonnenschein", "tokens": ["Wie", ",", "vom", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word"], "pos": ["PWAV", "$,", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Durchspielt, ein Edelstein,", "tokens": ["Durch\u00b7spielt", ",", "ein", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den ein Gl\u00fccklicher ans Licht gebracht.", "tokens": ["Den", "ein", "Gl\u00fcck\u00b7li\u00b7cher", "ans", "Licht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und die wei\u00dfe Rose in der Mitte,", "tokens": ["Und", "die", "wei\u00b7\u00dfe", "Ro\u00b7se", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tat sich auf der ganze Blumenflor,", "tokens": ["Tat", "sich", "auf", "der", "gan\u00b7ze", "Blu\u00b7men\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bl\u00fchte und erstarkte jede Sitte,", "tokens": ["Bl\u00fch\u00b7te", "und", "er\u00b7stark\u00b7te", "je\u00b7de", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Hoffnung stand am Lebenstor.", "tokens": ["Und", "die", "Hoff\u00b7nung", "stand", "am", "Le\u00b7bens\u00b7tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alles wundert' sich,", "tokens": ["Al\u00b7les", "wun\u00b7dert'", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ich aber freute mich,", "tokens": ["Ich", "a\u00b7ber", "freu\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis den Talisman ich selbst verlor!", "tokens": ["Bis", "den", "Ta\u00b7lis\u00b7man", "ich", "selbst", "ver\u00b7lor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn ich scheidend einst mu\u00df \u00fcberspringen", "tokens": ["Wenn", "ich", "schei\u00b7dend", "einst", "mu\u00df", "\u00fc\u00b7bers\u00b7prin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jene Kluft, die keine Br\u00fccke tr\u00e4gt,", "tokens": ["Je\u00b7ne", "Kluft", ",", "die", "kei\u00b7ne", "Br\u00fc\u00b7cke", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wird mir nicht ein Lied entgegenklingen,", "tokens": ["Wird", "mir", "nicht", "ein", "Lied", "ent\u00b7ge\u00b7gen\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das bekannt und ahnend mich erregt?", "tokens": ["Das", "be\u00b7kannt", "und", "ah\u00b7nend", "mich", "er\u00b7regt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O die Welt ist weit!", "tokens": ["O", "die", "Welt", "ist", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ob nicht die Jugendzeit", "tokens": ["Ob", "nicht", "die", "Ju\u00b7gend\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Irgendwo noch an das Herz mir schl\u00e4gt?", "tokens": ["Ir\u00b7gend\u00b7wo", "noch", "an", "das", "Herz", "mir", "schl\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Tr\u00e4umerei! Was sollten jene hoffen,", "tokens": ["Tr\u00e4u\u00b7me\u00b7rei", "!", "Was", "soll\u00b7ten", "je\u00b7ne", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die ein unnat\u00fcrlich Los getroffen,", "tokens": ["Die", "ein", "un\u00b7na\u00b7t\u00fcr\u00b7lich", "Los", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Frucht zu bringen ", "tokens": ["Frucht", "zu", "brin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ach, was man nicht kennt,", "tokens": ["Ach", ",", "was", "man", "nicht", "kennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Darnach das Herz nicht brennt", "tokens": ["Dar\u00b7nach", "das", "Herz", "nicht", "brennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bleibt kalt daf\u00fcr in Ewigkeit!", "tokens": ["Und", "bleibt", "kalt", "da\u00b7f\u00fcr", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PAV", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "In den Waldeskronen meines Lebens", "tokens": ["In", "den", "Wal\u00b7des\u00b7kro\u00b7nen", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00e4usle fort, du k\u00fchles Morgenwehn!", "tokens": ["S\u00e4us\u00b7le", "fort", ",", "du", "k\u00fch\u00b7les", "Mor\u00b7gen\u00b7wehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Leuchte hell, o Sonne meines Strebens,", "tokens": ["Leuch\u00b7te", "hell", ",", "o", "Son\u00b7ne", "mei\u00b7nes", "Stre\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ich will treu in deinem Scheine gehn!", "tokens": ["Ich", "will", "treu", "in", "dei\u00b7nem", "Schei\u00b7ne", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Rankend Immergr\u00fcn", "tokens": ["Ran\u00b7kend", "Im\u00b7mer\u00b7gr\u00fcn"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Soll meinen Stab umbl\u00fchn,", "tokens": ["Soll", "mei\u00b7nen", "Stab", "um\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch noch ", "tokens": ["Doch", "noch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "++", "measure": "spondeus"}}}}}