{"dta.poem.9735": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Einer braut die am hochzeit-tage gestorben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer ruht die/ die nicht frau und dennoch war getraut/", "tokens": ["Hjer", "ruht", "die", "/", "die", "nicht", "frau", "und", "den\u00b7noch", "war", "ge\u00b7traut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "$(", "PRELS", "PTKNEG", "ADJD", "KON", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die innig hat geliebt/ und wieder ward geliebet/", "tokens": ["Die", "in\u00b7nig", "hat", "ge\u00b7liebt", "/", "und", "wie\u00b7der", "ward", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "VVPP", "$(", "KON", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ihren mann durch nichts/ als ihren todt betr\u00fcbet/", "tokens": ["Die", "ih\u00b7ren", "mann", "durch", "nichts", "/", "als", "ih\u00b7ren", "todt", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PIS", "$(", "KOUS", "PPOSAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df er sie im sarg/ nicht in dem bett geschaut.", "tokens": ["Und", "da\u00df", "er", "sie", "im", "sarg", "/", "nicht", "in", "dem", "bett", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPRART", "VVFIN", "$(", "PTKNEG", "APPR", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}