{"textgrid.poem.39016": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft schon klang ein Ton her\u00fcber,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft schon klang ein Ton her\u00fcber,", "tokens": ["Oft", "schon", "klang", "ein", "Ton", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als wenn er jenseit der n\u00f6rdlichen Berge k\u00e4me,", "tokens": ["Als", "wenn", "er", "jen\u00b7seit", "der", "n\u00f6rd\u00b7li\u00b7chen", "Ber\u00b7ge", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und m\u00fcde mich und liebevoll gr\u00fc\u00dfte,", "tokens": ["Und", "m\u00fc\u00b7de", "mich", "und", "lie\u00b7be\u00b7voll", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ich dachte der Heimath", "tokens": ["Und", "ich", "dach\u00b7te", "der", "Hei\u00b7math"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Innig zwar doch ohne Schmerz.", "tokens": ["In\u00b7nig", "zwar", "doch", "oh\u00b7ne", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6r' ich auf den Gassen", "tokens": ["H\u00f6r'", "ich", "auf", "den", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Im Volksgedr\u00e4ng' ein deutsches Wort,", "tokens": ["Im", "Volks\u00b7ge\u00b7dr\u00e4ng'", "ein", "deut\u00b7sches", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fa\u00dft es mein Herz mit R\u00fchrung an;", "tokens": ["So", "fa\u00dft", "es", "mein", "Herz", "mit", "R\u00fch\u00b7rung", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch es wandelt vor\u00fcber", "tokens": ["Doch", "es", "wan\u00b7delt", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und l\u00e4\u00dft den heitern Geist mir frei.", "tokens": ["Und", "l\u00e4\u00dft", "den", "hei\u00b7tern", "Geist", "mir", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber heut' am fr\u00fchen Morgen", "tokens": ["A\u00b7ber", "heut'", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wacht' ich auf aus schweren Tr\u00e4umen,", "tokens": ["Wacht'", "ich", "auf", "aus", "schwe\u00b7ren", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Lieben sah' ich trauernd,", "tokens": ["Al\u00b7le", "Lie\u00b7ben", "sah'", "ich", "trau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Kindchen sprach in s\u00fc\u00dfen T\u00f6nen", "tokens": ["Mein", "Kind\u00b7chen", "sprach", "in", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und rief nach mir, \u2013", "tokens": ["Und", "rief", "nach", "mir", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da weint' ich heftig,", "tokens": ["Da", "weint'", "ich", "hef\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ein m\u00e4chtiger Schmerz ergriff mein Herz", "tokens": ["Ein", "m\u00e4ch\u00b7ti\u00b7ger", "Schmerz", "er\u00b7griff", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und dr\u00fcckt' und pre\u00dft' es", "tokens": ["Und", "dr\u00fcckt'", "und", "pre\u00dft'", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Als sollt' es zerbrechen,", "tokens": ["Als", "sollt'", "es", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ein Schwindel ergriff mich,", "tokens": ["Ein", "Schwin\u00b7del", "er\u00b7griff", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Mein Leben zerrann,", "tokens": ["Mein", "Le\u00b7ben", "zer\u00b7rann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Nichts war Wirklichkeit mehr um mich her,", "tokens": ["Nichts", "war", "Wirk\u00b7lich\u00b7keit", "mehr", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Alles zerflo\u00df in Tod,", "tokens": ["Al\u00b7les", "zer\u00b7flo\u00df", "in", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "Nur fern stand das Leben \u2013", "tokens": ["Nur", "fern", "stand", "das", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Da wu\u00dft' ich, was Heimweh sei,", "tokens": ["Da", "wu\u00dft'", "ich", ",", "was", "Heim\u00b7weh", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Da f\u00fchlt' ich, wie der Sohn der Alpen", "tokens": ["Da", "f\u00fchlt'", "ich", ",", "wie", "der", "Sohn", "der", "Al\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Sterben k\u00f6nne in der Fremde", "tokens": ["Ster\u00b7ben", "k\u00f6n\u00b7ne", "in", "der", "Frem\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "An dem m\u00e4chtig-schmerzlichen Gef\u00fchl.", "tokens": ["An", "dem", "m\u00e4ch\u00b7tig\u00b7schmerz\u00b7li\u00b7chen", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}