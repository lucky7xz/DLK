{"textgrid.poem.40558": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich h\u00f6re Lieder, ehrenwerthe, klagen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6re Lieder, ehrenwerthe, klagen,", "tokens": ["Ich", "h\u00f6\u00b7re", "Lie\u00b7der", ",", "eh\u00b7ren\u00b7wert\u00b7he", ",", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' edle Angesichter sich verschleiern,", "tokens": ["Seh'", "ed\u00b7le", "An\u00b7ge\u00b7sich\u00b7ter", "sich", "ver\u00b7schlei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Prophetisch trauernd, da\u00df in unsern Tagen", "tokens": ["Pro\u00b7phe\u00b7tisch", "trau\u00b7ernd", ",", "da\u00df", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Prosa Weltreich seinen Sieg will feiern;", "tokens": ["Der", "Pro\u00b7sa", "Welt\u00b7reich", "sei\u00b7nen", "Sieg", "will", "fei\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df Poesie, entsetzt, nun fliehen werde,", "tokens": ["Da\u00df", "Poe\u00b7sie", ",", "ent\u00b7setzt", ",", "nun", "flie\u00b7hen", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "VVPP", "$,", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf schnurgerader Eisenbahn entjagen,", "tokens": ["Auf", "schnur\u00b7ge\u00b7ra\u00b7der", "Ei\u00b7sen\u00b7bahn", "ent\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entf\u00fchrt auf Dampffregatten unsrer Erde,", "tokens": ["Ent\u00b7f\u00fchrt", "auf", "Dampf\u00b7fre\u00b7gat\u00b7ten", "uns\u00b7rer", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Dampfkarossen ferne fortgetragen!", "tokens": ["Auf", "Dampf\u00b7ka\u00b7ros\u00b7sen", "fer\u00b7ne", "fort\u00b7ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ei, wart ihr denn so hold den krummen Wegen,", "tokens": ["Ei", ",", "wart", "ihr", "denn", "so", "hold", "den", "krum\u00b7men", "We\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihr so sehr die graden scheuen k\u00f6nnet?", "tokens": ["Da\u00df", "ihr", "so", "sehr", "die", "gra\u00b7den", "scheu\u00b7en", "k\u00f6n\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ist euch's Poesie, auf Holperstegen", "tokens": ["Und", "ist", "euch's", "Poe\u00b7sie", ",", "auf", "Hol\u00b7pers\u00b7te\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu kriechen, wenn zu fliegen euch verg\u00f6nnet?", "tokens": ["Zu", "krie\u00b7chen", ",", "wenn", "zu", "flie\u00b7gen", "euch", "ver\u00b7g\u00f6n\u00b7net", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So macht euch auf, wohlan, auf alten Gleisen", "tokens": ["So", "macht", "euch", "auf", ",", "wo\u00b7hlan", ",", "auf", "al\u00b7ten", "Glei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Poesie, der fl\u00fccht'gen, nachzujagen,", "tokens": ["Der", "Poe\u00b7sie", ",", "der", "fl\u00fccht'\u00b7gen", ",", "nach\u00b7zu\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVIZU", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und knebelt mit Gebi\u00df und Strang und Eisen", "tokens": ["Und", "kne\u00b7belt", "mit", "Ge\u00b7bi\u00df", "und", "Strang", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Ro\u00df, das edle freie, vor den Wagen!", "tokens": ["Das", "Ro\u00df", ",", "das", "ed\u00b7le", "frei\u00b7e", ",", "vor", "den", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Haid' entlang! La\u00dft eures Leibs Gebeine", "tokens": ["Die", "Haid'", "ent\u00b7lang", "!", "La\u00dft", "eu\u00b7res", "Leibs", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "VVIMP", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Auferstehungstages R\u00fctteln ahnen,", "tokens": ["Des", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7ta\u00b7ges", "R\u00fct\u00b7teln", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Rosse Schnauben, Peitschenknall und Steine", "tokens": ["Der", "Ros\u00b7se", "Schnau\u00b7ben", ",", "Peit\u00b7schen\u00b7knall", "und", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Staubgew\u00f6lk euch der Verlornen mahnen!", "tokens": ["Im", "Staub\u00b7ge\u00b7w\u00f6lk", "euch", "der", "Ver\u00b7lor\u00b7nen", "mah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Springt dort ins Boot, la\u00dft rudern eure Rechte!", "tokens": ["Springt", "dort", "ins", "Boot", ",", "la\u00dft", "ru\u00b7dern", "eu\u00b7re", "Rech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,", "VVIMP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In saurem Schwei\u00df den Schiffer la\u00dft nicht zagen!", "tokens": ["In", "sau\u00b7rem", "Schwei\u00df", "den", "Schif\u00b7fer", "la\u00dft", "nicht", "za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob eure Br\u00fcder euch, die Ruderknechte,", "tokens": ["Ob", "eu\u00b7re", "Br\u00fc\u00b7der", "euch", ",", "die", "Ru\u00b7der\u00b7knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von der verlornen Poesie nicht sagen?", "tokens": ["Von", "der", "ver\u00b7lor\u00b7nen", "Poe\u00b7sie", "nicht", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Besteigt ein Schiff und fangt die Launenspende", "tokens": ["Be\u00b7steigt", "ein", "Schiff", "und", "fangt", "die", "Lau\u00b7nen\u00b7spen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des wind'gen Windgotts auf im Segeltuche,", "tokens": ["Des", "win\u00b7d'\u00b7gen", "Wind\u00b7gotts", "auf", "im", "Se\u00b7gel\u00b7tu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als ob ein Bettler mit dem Hut behende", "tokens": ["Als", "ob", "ein", "Bett\u00b7ler", "mit", "dem", "Hut", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Wandrers milden Sold zu haschen suche!", "tokens": ["Des", "Wand\u00b7rers", "mil\u00b7den", "Sold", "zu", "ha\u00b7schen", "su\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Will er's, so ruht windstill mit schlaffem Segel,", "tokens": ["Will", "er's", ",", "so", "ruht", "wind\u00b7still", "mit", "schlaf\u00b7fem", "Se\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "ADV", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seid festgefroren in den Sommertagen!", "tokens": ["Seid", "fest\u00b7ge\u00b7fro\u00b7ren", "in", "den", "Som\u00b7mer\u00b7ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht da\u00df Delphin euch und Seegev\u00f6gel", "tokens": ["Viel\u00b7leicht", "da\u00df", "Del\u00b7phin", "euch", "und", "See\u00b7ge\u00b7v\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NE", "PPER", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von jener, so ihr suchet, wei\u00df zu sagen!", "tokens": ["Von", "je\u00b7ner", ",", "so", "ihr", "su\u00b7chet", ",", "wei\u00df", "zu", "sa\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich will inde\u00df hinab die Bahn des Rheines", "tokens": ["Ich", "will", "in\u00b7de\u00df", "hin\u00b7ab", "die", "Bahn", "des", "Rhei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "ART", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf schwarzem Schwan, dem Dampfschiff, singend schwimmen,", "tokens": ["Auf", "schwar\u00b7zem", "Schwan", ",", "dem", "Dampf\u00b7schiff", ",", "sin\u00b7gend", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Becher schwingend voll des goldnen Weines,", "tokens": ["Den", "Be\u00b7cher", "schwin\u00b7gend", "voll", "des", "gold\u00b7nen", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir, Menschengeist, den Siegeshymnus stimmen!", "tokens": ["Dir", ",", "Men\u00b7schen\u00b7geist", ",", "den", "Sie\u00b7ges\u00b7hym\u00b7nus", "stim\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie dir der Feuergeist die Flammenkrone", "tokens": ["Wie", "dir", "der", "Feu\u00b7er\u00b7geist", "die", "Flam\u00b7men\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herab vom stolzen Haupt hat reichen m\u00fcssen,", "tokens": ["Her\u00b7ab", "vom", "stol\u00b7zen", "Haupt", "hat", "rei\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie du dem Erdengeiste, seinem Sohne,", "tokens": ["Wie", "du", "dem", "Er\u00b7den\u00b7geis\u00b7te", ",", "sei\u00b7nem", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das eh'rne Herz k\u00fchn aus der Brust gerissen;", "tokens": ["Das", "eh'r\u00b7ne", "Herz", "k\u00fchn", "aus", "der", "Brust", "ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wie du zu beiden sprachst: Ihr sollt nicht rasten!", "tokens": ["Wie", "du", "zu", "bei\u00b7den", "sprachst", ":", "Ihr", "sollt", "nicht", "ras\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIS", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df f\u00fcrder Mensch nicht Menschen knechten m\u00f6ge,", "tokens": ["Da\u00df", "f\u00fcr\u00b7der", "Mensch", "nicht", "Men\u00b7schen", "knech\u00b7ten", "m\u00f6\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geh, Feuer du, und trage deine Lasten!", "tokens": ["Geh", ",", "Feu\u00b7er", "du", ",", "und", "tra\u00b7ge", "dei\u00b7ne", "Las\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "PPER", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leb', Eisen du, und wandle seine Wege!", "tokens": ["Leb'", ",", "Ei\u00b7sen", "du", ",", "und", "wand\u00b7le", "sei\u00b7ne", "We\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "PPER", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ich wei\u00df, da\u00df deines Wandels Flammengleise", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "dei\u00b7nes", "Wan\u00b7dels", "Flam\u00b7men\u00b7glei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Bl\u00fcmchen im Poetenhain bedr\u00e4ngen,", "tokens": ["Kein", "Bl\u00fcm\u00b7chen", "im", "Poe\u00b7ten\u00b7hain", "be\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So wie des Heil'genscheines Gluthenkreise", "tokens": ["So", "wie", "des", "Heil'\u00b7gen\u00b7schei\u00b7nes", "Glu\u00b7then\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein L\u00f6ckchen am Madonnenhaupt versengen.", "tokens": ["Kein", "L\u00f6ck\u00b7chen", "am", "Ma\u00b7don\u00b7nen\u00b7haupt", "ver\u00b7sen\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Nein, Amt der Poesie in allen Tagen", "tokens": ["Nein", ",", "Amt", "der", "Poe\u00b7sie", "in", "al\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist's, hoher Geist, dein Siegesfest versch\u00f6nen,", "tokens": ["Ist's", ",", "ho\u00b7her", "Geist", ",", "dein", "Sie\u00b7ges\u00b7fest", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie der Victoria Goldbild \u00fcberm Wagen", "tokens": ["Wie", "der", "Vic\u00b7to\u00b7ria", "Gold\u00b7bild", "\u00fc\u00b7berm", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NE", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Triumphators schwebt, um ihn zu kr\u00f6nen.", "tokens": ["Des", "Tri\u00b7um\u00b7pha\u00b7tors", "schwebt", ",", "um", "ihn", "zu", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "w