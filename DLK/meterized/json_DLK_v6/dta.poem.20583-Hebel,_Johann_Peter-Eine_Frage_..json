{"dta.poem.20583": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Eine Frage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Sag, weisch denn selber au, du liebi Seel,             ", "tokens": ["Sag", ",", "weisch", "denn", "sel\u00b7ber", "au", ",", "du", "lie\u00b7bi", "Seel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADV", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was\u2019s Wienechtchindli isch, und heschs bidenkt?", "tokens": ["was's", "Wie\u00b7necht\u00b7chind\u00b7li", "isch", ",", "und", "heschs", "bi\u00b7denkt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denkwol i sag der\u2019s und i freu mi druf.", "tokens": ["Denk\u00b7wol", "i", "sag", "der's", "und", "i", "freu", "mi", "druf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O, \u2019s isch en Engel usem Paradies", "tokens": ["O", ",", "'s", "isch", "en", "En\u00b7gel", "u\u00b7sem", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "ADJD", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "mit sanften Augen und mit zartem Herz.", "tokens": ["mit", "sanf\u00b7ten", "Au\u00b7gen", "und", "mit", "zar\u00b7tem", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom reine Himmel abe het en Gott", "tokens": ["Vom", "rei\u00b7ne", "Him\u00b7mel", "a\u00b7be", "het", "en", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "VAFIN", "FM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "de Chindlene zum Trost und Sege gschickt.", "tokens": ["de", "Chind\u00b7le\u00b7ne", "zum", "Trost", "und", "Se\u00b7ge", "gschickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er h\u00fcetet sie am Bettli Tag und Nacht;", "tokens": ["Er", "h\u00fce\u00b7tet", "sie", "am", "Bett\u00b7li", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "er deckt sie mittem weiche Fegge zu,", "tokens": ["er", "deckt", "sie", "mit\u00b7tem", "wei\u00b7che", "Feg\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und weiht er sie mit reinem Othem a,", "tokens": ["und", "weiht", "er", "sie", "mit", "rei\u00b7nem", "O\u00b7them", "a", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "wird\u2019s Aeugli hell und \u2019s B\u00e4ckli rund und roth.", "tokens": ["wird's", "A\u00b7e\u00b7ug\u00b7li", "hell", "und", "'s", "B\u00e4ck\u00b7li", "rund", "und", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJD", "KON", "PPER", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Er treit sie uf de H\u00e4nden in der Gfohr,", "tokens": ["Er", "treit", "sie", "uf", "de", "H\u00e4n\u00b7den", "in", "der", "Gfohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "g\u00fcnnt Bl\u00fcemli f\u00fcr sie uf der gr\u00fcene Flur,", "tokens": ["g\u00fcnnt", "Bl\u00fcem\u00b7li", "f\u00fcr", "sie", "uf", "der", "gr\u00fce\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "und stoht im Schnee und Rege d\u2019 Wienecht do,", "tokens": ["und", "stoht", "im", "Schnee", "und", "Re\u00b7ge", "d'", "Wie\u00b7necht", "do", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KON", "NN", "NE", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "se henkt er \u2019nen im Wienechtchindli-Baum", "tokens": ["se", "henkt", "er", "'nen", "im", "Wienecht\u00b7chind\u00b7li\u00b7Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "e sch\u00f6ne Fr\u00fcehlig in der Stuben uf,", "tokens": ["e", "sch\u00f6\u00b7ne", "Fr\u00fceh\u00b7lig", "in", "der", "Stu\u00b7ben", "uf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "und l\u00e4chlet still, und het si s\u00fce\u00dfi Freud,", "tokens": ["und", "l\u00e4ch\u00b7let", "still", ",", "und", "het", "si", "s\u00fce\u00b7\u00dfi", "Freud", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "uud ", "tokens": ["u\u00b7ud"], "token_info": ["word"], "pos": ["KON"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Jo, liebi Seel, und gang vo Hus zu Hus,", "tokens": ["Jo", ",", "lie\u00b7bi", "Seel", ",", "und", "gang", "vo", "Hus", "zu", "Hus", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "KON", "NN", "NE", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "sag ", "tokens": ["sag"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "und lueg,", "tokens": ["und", "lu\u00b7eg", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "der Wienechtchindli-Baum verrothet bald,", "tokens": ["der", "Wienecht\u00b7chind\u00b7li\u00b7Baum", "ver\u00b7ro\u00b7thet", "bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "wie alli M\u00fcetter sin im ganze Dorf.", "tokens": ["wie", "al\u00b7li", "M\u00fcet\u00b7ter", "sin", "im", "gan\u00b7ze", "Dorf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Do hangt e Baum, nei lueg me doch", "tokens": ["Do", "hangt", "e", "Baum", ",", "nei", "lu\u00b7eg", "me", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--++-+-+", "measure": "dactylic.init"}, "line.25": {"text": "und lueg!", "tokens": ["und", "lu\u00b7eg", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "In alle N\u00e4ste n\u00fct as Zuckerbrod!", "tokens": ["In", "al\u00b7le", "N\u00e4s\u00b7te", "n\u00fct", "as", "Zu\u00b7cker\u00b7brod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "\u2019s isch nit viel nutz. Die het e n\u00e4rschi Freud", "tokens": ["'s", "isch", "nit", "viel", "nutz", ".", "Die", "het", "e", "n\u00e4r\u00b7schi", "Freud"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKNEG", "PIAT", "NN", "$.", "ART", "FM", "FM", "FM", "NN"], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.28": {"text": "an ihrem B\u00fcebli, will em alles s\u00fce\u00df", "tokens": ["an", "ih\u00b7rem", "B\u00fceb\u00b7li", ",", "will", "em", "al\u00b7les", "s\u00fce\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VMFIN", "PIS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "und liebli mache, thut em, was es will.", "tokens": ["und", "lieb\u00b7li", "ma\u00b7che", ",", "thut", "em", ",", "was", "es", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Gib acht, gib acht, es chunnt e mol e Zit,", "tokens": ["Gib", "acht", ",", "gib", "acht", ",", "es", "chunnt", "e", "mol", "e", "Zit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "VVIMP", "CARD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "se schlacht sie d\u2019 H\u00e4nd no zsemmen \u00fcberm", "tokens": ["se", "schlacht", "sie", "d'", "H\u00e4nd", "no", "zsem\u00b7men", "\u00fc\u00b7berm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NE", "NE", "VVFIN", "APPRART"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Chopf,", "tokens": ["Chopf", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.33": {"text": "und seit: \u201eDu gottlos Chind, isch das mi", "tokens": ["und", "seit", ":", "\u201e", "Du", "gott\u00b7los", "Chind", ",", "isch", "das", "mi"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$.", "$(", "PPER", "ADJD", "NN", "$,", "ADJD", "ART", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Dank?\u201c", "tokens": ["Dank", "?", "\u201c"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Jo weger M\u00fcetterli, das isch di Dank!", "tokens": ["Jo", "we\u00b7ger", "M\u00fcet\u00b7ter\u00b7li", ",", "das", "isch", "di", "Dank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ADJD", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Jez do siehts anderst dri ins Nochbers Hus.", "tokens": ["Jez", "do", "siehts", "an\u00b7derst", "dri", "ins", "Noch\u00b7bers", "Hus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Scharmanti bruni Bire, welschi Nu\u00df!", "tokens": ["Schar\u00b7man\u00b7ti", "bru\u00b7ni", "Bi\u00b7re", ",", "wel\u00b7schi", "Nu\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "PWAT", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.38": {"text": "Scharmanti rothi Oepfel ab der Hurt!", "tokens": ["Schar\u00b7man\u00b7ti", "ro\u00b7thi", "O\u00b7ep\u00b7fel", "ab", "der", "Hurt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.39": {"text": "e Gufeb\u00fcchsli, doch wills Gott der Her", "tokens": ["e", "Gu\u00b7fe\u00b7b\u00fcchs\u00b7li", ",", "doch", "wills", "Gott", "der", "Her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADV", "VMFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "ke Gufe drinn! Vom zarte Bese-Ris", "tokens": ["ke", "Gu\u00b7fe", "drinn", "!", "Vom", "zar\u00b7te", "Be\u00b7se\u00b7Ris"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "e goldig R\u00fcethli, schlank und nagelneu!", "tokens": ["e", "gol\u00b7dig", "R\u00fceth\u00b7li", ",", "schlank", "und", "na\u00b7gel\u00b7neu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Lueg, so ne Muetter het ihr Chindli lieb!", "tokens": ["Lu\u00b7eg", ",", "so", "ne", "Mu\u00b7et\u00b7ter", "het", "ihr", "Chind\u00b7li", "lieb", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Lueg, so ne Muetter ziehts verst\u00e4ndig uf,", "tokens": ["Lu\u00b7eg", ",", "so", "ne", "Mu\u00b7et\u00b7ter", "ziehts", "ver\u00b7st\u00e4n\u00b7dig", "uf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "und wird mi B\u00fcrstli meisterlos, und meint", "tokens": ["und", "wird", "mi", "B\u00fcrst\u00b7li", "meis\u00b7ter\u00b7los", ",", "und", "meint"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "NE", "NE", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "es seig der Her im Hus, se hebt si b\u2019herzt", "tokens": ["es", "seig", "der", "Her", "im", "Hus", ",", "se", "hebt", "si", "b'\u00b7herzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.46": {"text": "der Finger uf, und f\u00f6rcht ihr B\u00fcebli nit,", "tokens": ["der", "Fin\u00b7ger", "uf", ",", "und", "f\u00f6rcht", "ihr", "B\u00fceb\u00b7li", "nit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "und seit: \u201eWeisch nit, was hinterm Spie-", "tokens": ["und", "seit", ":", "\u201e", "Weisch", "nit", ",", "was", "hin\u00b7term", "Spie"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$.", "$(", "NN", "PTKNEG", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "gel steckt?\u201e", "tokens": ["gel", "steckt", "?", "\u201e"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.49": {"text": "Und \u2019s B\u00fcebli folgt, und wird e brave Chnab;", "tokens": ["Und", "'s", "B\u00fceb\u00b7li", "folgt", ",", "und", "wird", "e", "bra\u00b7ve", "Chnab", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "VVFIN", "$,", "KON", "VAFIN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.50": {"text": "Jez g\u00f6hn mer wieder witers um e Hus.", "tokens": ["Jez", "g\u00f6hn", "mer", "wie\u00b7der", "wi\u00b7ters", "um", "e", "Hus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADV", "ADV", "APPR", "