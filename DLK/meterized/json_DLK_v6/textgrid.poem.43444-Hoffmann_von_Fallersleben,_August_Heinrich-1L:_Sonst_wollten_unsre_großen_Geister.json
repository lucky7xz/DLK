{"textgrid.poem.43444": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sonst wollten unsre gro\u00dfen Geister", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonst wollten unsre gro\u00dfen Geister", "tokens": ["Sonst", "woll\u00b7ten", "uns\u00b7re", "gro\u00b7\u00dfen", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch mehr als andre Leute sein:", "tokens": ["Noch", "mehr", "als", "and\u00b7re", "Leu\u00b7te", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie nannten sich auf Deutsch Herr Meister,", "tokens": ["Sie", "nann\u00b7ten", "sich", "auf", "Deutsch", "Herr", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NE", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Magister aber auf Latein.", "tokens": ["Ma\u00b7gis\u00b7ter", "a\u00b7ber", "auf", "La\u00b7tein."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["NN", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt ist f\u00fcr sie doch ein Magister-", "tokens": ["Jetzt", "ist", "f\u00fcr", "sie", "doch", "ein", "Ma\u00b7gis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Doctor-Titel viel zu klein,", "tokens": ["Und", "Doc\u00b7tor\u00b7Ti\u00b7tel", "viel", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt m\u00f6chte jeder ein Minister,", "tokens": ["Jetzt", "m\u00f6ch\u00b7te", "je\u00b7der", "ein", "Mi\u00b7nis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Diener, und kein Meister sein.", "tokens": ["Ein", "Die\u00b7ner", ",", "und", "kein", "Meis\u00b7ter", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}