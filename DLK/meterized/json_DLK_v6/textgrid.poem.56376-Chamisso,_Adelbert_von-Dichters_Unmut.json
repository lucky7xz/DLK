{"textgrid.poem.56376": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Dichters Unmut", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir tragen gar im Herzen manche Pfeile,", "tokens": ["Wir", "tra\u00b7gen", "gar", "im", "Her\u00b7zen", "man\u00b7che", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und blutet's in dem stillen Scho\u00df der Nacht,", "tokens": ["Und", "blu\u00b7tet's", "in", "dem", "stil\u00b7len", "Scho\u00df", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wird vom Schmerz das Lied hervorgebracht,", "tokens": ["So", "wird", "vom", "Schmerz", "das", "Lied", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So reihet wunderbar sich Zeil an Zeile.", "tokens": ["So", "rei\u00b7het", "wun\u00b7der\u00b7bar", "sich", "Zeil", "an", "Zei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie lesen's nun, so, f\u00fcr die Langeweile,", "tokens": ["Sie", "le\u00b7sen's", "nun", ",", "so", ",", "f\u00fcr", "die", "Lan\u00b7ge\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann tr\u00e4g und la\u00df sie die Verdauung macht,", "tokens": ["Wann", "tr\u00e4g", "und", "la\u00df", "sie", "die", "Ver\u00b7dau\u00b7ung", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVIMP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und finden's h\u00fcbsch, und finden's schlecht erdacht,", "tokens": ["Und", "fin\u00b7den's", "h\u00fcbsch", ",", "und", "fin\u00b7den's", "schlecht", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hier ist's schwach, und dort entbehrt's der Feile.", "tokens": ["Und", "hier", "ist's", "schwach", ",", "und", "dort", "ent\u00b7behrt's", "der", "Fei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir haben's aber so in der Natur,", "tokens": ["Wir", "ha\u00b7ben's", "a\u00b7ber", "so", "in", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir schreiben ganz mit unsers Herzens Blut,", "tokens": ["Wir", "schrei\u00b7ben", "ganz", "mit", "un\u00b7sers", "Her\u00b7zens", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was sie bekritteln zwischen Schlaf und Wachen.", "tokens": ["Was", "sie", "be\u00b7krit\u00b7teln", "zwi\u00b7schen", "Schlaf", "und", "Wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Pelikanes-Wirtschaft! w\u00e4r's doch nur", "tokens": ["O", "Pe\u00b7li\u00b7ka\u00b7nes\u00b7Wirt\u00b7schaft", "!", "w\u00e4r's", "doch", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr keine gar so miserable Brut!", "tokens": ["F\u00fcr", "kei\u00b7ne", "gar", "so", "mi\u00b7se\u00b7rab\u00b7le", "Brut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was tut's, wir werden's drum nicht anders machen.", "tokens": ["Was", "tut's", ",", "wir", "wer\u00b7den's", "drum", "nicht", "an\u00b7ders", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "PAV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}