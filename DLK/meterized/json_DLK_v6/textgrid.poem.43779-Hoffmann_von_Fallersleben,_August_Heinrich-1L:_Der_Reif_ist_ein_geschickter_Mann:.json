{"textgrid.poem.43779": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Reif ist ein geschickter Mann:", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Reif ist ein geschickter Mann:", "tokens": ["Der", "Reif", "ist", "ein", "ge\u00b7schick\u00b7ter", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O seht doch, was er Alles kann!", "tokens": ["O", "seht", "doch", ",", "was", "er", "Al\u00b7les", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PWS", "PPER", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er haucht nur in den Wald hinein,", "tokens": ["Er", "haucht", "nur", "in", "den", "Wald", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ist verzuckert sch\u00f6n und fein", "tokens": ["Wie", "ist", "ver\u00b7zu\u00b7ckert", "sch\u00f6n", "und", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "VVPP", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jeder Zweig und Busch und Strauch", "tokens": ["Ein", "je\u00b7der", "Zweig", "und", "Busch", "und", "Strauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von seinem Hauch!", "tokens": ["Von", "sei\u00b7nem", "Hauch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie schnell es ihm von H\u00e4nden geht!", "tokens": ["Wie", "schnell", "es", "ihm", "von", "H\u00e4n\u00b7den", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Zuckerb\u00e4cker das versteht.", "tokens": ["Kein", "Zu\u00b7cker\u00b7b\u00e4\u00b7cker", "das", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Alles fein und silberrein,", "tokens": ["Und", "Al\u00b7les", "fein", "und", "sil\u00b7ber\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gl\u00e4nzt es doch im Sonnenschein!", "tokens": ["Wie", "gl\u00e4nzt", "es", "doch", "im", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4r' Alles doch nur Zucker auch", "tokens": ["W\u00e4r'", "Al\u00b7les", "doch", "nur", "Zu\u00b7cker", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von seinem Hauch!", "tokens": ["Von", "sei\u00b7nem", "Hauch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch nein, wir sind schon sehr erfreut,", "tokens": ["Doch", "nein", ",", "wir", "sind", "schon", "sehr", "er\u00b7freut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df uns der Reif so Sch\u00f6nes beut.", "tokens": ["Da\u00df", "uns", "der", "Reif", "so", "Sch\u00f6\u00b7nes", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Winter, deinen Reif auch gieb,", "tokens": ["O", "Win\u00b7ter", ",", "dei\u00b7nen", "Reif", "auch", "gieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "ADV", "VVIMP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns ist auch Augenweide lieb,", "tokens": ["Uns", "ist", "auch", "Au\u00b7gen\u00b7wei\u00b7de", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ohne Duft und Fr\u00fchlingshauch", "tokens": ["Und", "oh\u00b7ne", "Duft", "und", "Fr\u00fch\u00b7lings\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Freu'n wir uns auch.", "tokens": ["Freu'n", "wir", "uns", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}