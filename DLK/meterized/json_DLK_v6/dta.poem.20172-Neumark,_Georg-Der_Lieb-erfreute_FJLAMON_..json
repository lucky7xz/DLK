{"dta.poem.20172": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Mit welchen Sachen nur das Hertz ist \u00fcberflossen/", "tokens": ["Mit", "wel\u00b7chen", "Sa\u00b7chen", "nur", "das", "Hertz", "ist", "\u00fc\u00b7berf\u00b7los\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mu\u00df der Mund heraus hinwieder von sich stossen.", "tokens": ["Das", "mu\u00df", "der", "Mund", "he\u00b7raus", "hin\u00b7wie\u00b7der", "von", "sich", "stos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "APZR", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}