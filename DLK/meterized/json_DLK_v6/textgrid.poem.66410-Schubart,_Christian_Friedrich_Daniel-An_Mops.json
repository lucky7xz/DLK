{"textgrid.poem.66410": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Mops", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du k\u00fcssest mich zum neuen Jahr,", "tokens": ["Du", "k\u00fcs\u00b7sest", "mich", "zum", "neu\u00b7en", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprichst zu mir: du Lieber,", "tokens": ["Und", "sprichst", "zu", "mir", ":", "du", "Lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$.", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Busen flie\u00dft von W\u00fcnschen \u00fcber?", "tokens": ["Mein", "Bu\u00b7sen", "flie\u00dft", "von", "W\u00fcn\u00b7schen", "\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "APPR", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O falscher Mops! es ist nicht wahr,", "tokens": ["O", "fal\u00b7scher", "Mops", "!", "es", "ist", "nicht", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Freundschaft gl\u00e4nzet nur aus deinen Blicken;", "tokens": ["Die", "Freund\u00b7schaft", "gl\u00e4n\u00b7zet", "nur", "aus", "dei\u00b7nen", "Bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch Joabs Dolch blinkt auf dem R\u00fccken.", "tokens": ["Doch", "Joabs", "Dolch", "blinkt", "auf", "dem", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}}}}