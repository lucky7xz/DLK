{"textgrid.poem.32883": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stellt mir der weise Lisidor", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stellt mir der weise Lisidor", "tokens": ["Stellt", "mir", "der", "wei\u00b7se", "Li\u00b7si\u00b7dor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Gott der Liebe schrecklich vor,", "tokens": ["Den", "Gott", "der", "Lie\u00b7be", "schreck\u00b7lich", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schweren f\u00fcrchterlichen Pfeilen,", "tokens": ["Mit", "schwe\u00b7ren", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wovon die Wunden selten heilen:", "tokens": ["Wo\u00b7von", "die", "Wun\u00b7den", "sel\u00b7ten", "hei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So irrt er sich,", "tokens": ["So", "irrt", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Glaubt er, ich f\u00fcrchte mich.", "tokens": ["Glaubt", "er", ",", "ich", "f\u00fcrch\u00b7te", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch mahlt ein z\u00e4rtlicher Amynt", "tokens": ["Doch", "mahlt", "ein", "z\u00e4rt\u00b7li\u00b7cher", "A\u00b7mynt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn als ein liebensw\u00fcrdigs Kind,", "tokens": ["Ihn", "als", "ein", "lie\u00b7bens\u00b7w\u00fcr\u00b7digs", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sanft, schlau, zu schmeicheln stets beflissen,", "tokens": ["Sanft", ",", "schlau", ",", "zu", "schmei\u00b7cheln", "stets", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "PTKZU", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6n wie der Lenz, sch\u00f6n bis zum k\u00fcssen.", "tokens": ["Sch\u00f6n", "wie", "der", "Lenz", ",", "sch\u00f6n", "bis", "zum", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "ADJD", "APPR", "APPRART", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wie f\u00fcrchterlich", "tokens": ["Wie", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wird dieses Kind f\u00fcr mich!", "tokens": ["Wird", "die\u00b7ses", "Kind", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}