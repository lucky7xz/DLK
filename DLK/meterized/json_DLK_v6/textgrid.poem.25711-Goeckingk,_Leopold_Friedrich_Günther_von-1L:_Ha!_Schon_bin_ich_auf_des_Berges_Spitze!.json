{"textgrid.poem.25711": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ha! Schon bin ich auf des Berges Spitze!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha! Schon bin ich auf des Berges Spitze!", "tokens": ["Ha", "!", "Schon", "bin", "ich", "auf", "des", "Ber\u00b7ges", "Spit\u00b7ze", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ich hier so wohlbehaglich sitze,", "tokens": ["Wie", "ich", "hier", "so", "wohl\u00b7be\u00b7hag\u00b7lich", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "L\u00e4chelnd die zur\u00fcckgelegte rauhe", "tokens": ["L\u00e4\u00b7chelnd", "die", "zu\u00b7r\u00fcck\u00b7ge\u00b7leg\u00b7te", "rau\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stra\u00dfe beschaue!", "tokens": ["Stra\u00b7\u00dfe", "be\u00b7schau\u00b7e", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und der W\u00f6lfe, welche mit mir stritten,", "tokens": ["Und", "der", "W\u00f6l\u00b7fe", ",", "wel\u00b7che", "mit", "mir", "strit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und des Durstes, den ich oft erlitten,", "tokens": ["Und", "des", "Durs\u00b7tes", ",", "den", "ich", "oft", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und des Irrwischs t\u00fcckevoller R\u00e4nke", "tokens": ["Und", "des", "Irr\u00b7wischs", "t\u00fc\u00b7cke\u00b7vol\u00b7ler", "R\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nicht mehr gedenke.", "tokens": ["Nicht", "mehr", "ge\u00b7den\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Aber wohl an all' den Wasserf\u00e4llen", "tokens": ["A\u00b7ber", "wohl", "an", "all'", "den", "Was\u00b7ser\u00b7f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und von Rosen angehauchten Stellen,", "tokens": ["Und", "von", "Ro\u00b7sen", "an\u00b7ge\u00b7hauch\u00b7ten", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Wo ich sonst mich la\u00df gelagert habe,", "tokens": ["Wo", "ich", "sonst", "mich", "la\u00df", "ge\u00b7la\u00b7gert", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Immer noch labe!", "tokens": ["Im\u00b7mer", "noch", "la\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Sey des Weges andre Halbscheid rauher,", "tokens": ["Sey", "des", "We\u00b7ges", "and\u00b7re", "Halb\u00b7scheid", "rau\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Als die erste! Itzt will ich wohl schlauer,", "tokens": ["Als", "die", "ers\u00b7te", "!", "Itzt", "will", "ich", "wohl", "schlau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$.", "ADV", "VMFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Von der Landstra\u00df' ab, in Wiesengr\u00fcnden", "tokens": ["Von", "der", "Land\u00b7stra\u00df'", "ab", ",", "in", "Wie\u00b7sen\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Fu\u00dfstege finden.", "tokens": ["Fu\u00df\u00b7ste\u00b7ge", "fin\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Steh denn auf, lieb Weibchen, mit dem Jungen!", "tokens": ["Steh", "denn", "auf", ",", "lieb", "Weib\u00b7chen", ",", "mit", "dem", "Jun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "ADJD", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Hast den Weg mir schon so kurz gesungen!", "tokens": ["Hast", "den", "Weg", "mir", "schon", "so", "kurz", "ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Singe fort, bis, eh' wir's uns versehen,", "tokens": ["Sin\u00b7ge", "fort", ",", "bis", ",", "eh'", "wir's", "uns", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "$,", "KOUS", "PIS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Pl\u00f6tzlich wir stehen,", "tokens": ["Pl\u00f6tz\u00b7lich", "wir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Wo am Ende der vergn\u00fcgten Reise", "tokens": ["Wo", "am", "En\u00b7de", "der", "ver\u00b7gn\u00fcg\u00b7ten", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Meine Heimath ich von fern Euch weise;", "tokens": ["Mei\u00b7ne", "Hei\u00b7math", "ich", "von", "fern", "Euch", "wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Trennt Euch dann mit Muth von mir, ihr werthen", "tokens": ["Trennt", "Euch", "dann", "mit", "Muth", "von", "mir", ",", "ihr", "wert\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "PPER", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Reisegef\u00e4hrten!", "tokens": ["Rei\u00b7se\u00b7ge\u00b7f\u00e4hr\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}