{"textgrid.poem.63284": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: In manchen N\u00e4chten tanzen die Skelette", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In manchen N\u00e4chten tanzen die Skelette", "tokens": ["In", "man\u00b7chen", "N\u00e4ch\u00b7ten", "tan\u00b7zen", "die", "Ske\u00b7let\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Friedhof. Auf den Kreuzen sitzen Frauen", "tokens": ["Am", "Fried\u00b7hof", ".", "Auf", "den", "Kreu\u00b7zen", "sit\u00b7zen", "Frau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "APPR", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lassen sich von fleischlosen Kavalieren", "tokens": ["Und", "las\u00b7sen", "sich", "von", "fleischlo\u00b7sen", "Ka\u00b7va\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um die Wette", "tokens": ["Um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Auf Herz und Nieren", "tokens": ["Auf", "Herz", "und", "Nie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Pr\u00fcfen und bis ins Innerste ihres Herzens schauen.", "tokens": ["Pr\u00fc\u00b7fen", "und", "bis", "ins", "In\u00b7ners\u00b7te", "ih\u00b7res", "Her\u00b7zens", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.7": {"text": "Da aber ist nichts als leerer Raum:", "tokens": ["Da", "a\u00b7ber", "ist", "nichts", "als", "lee\u00b7rer", "Raum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Bloss", "tokens": ["Bloss"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Der Himmel h\u00e4ngt darin wie ein dunkelblauer Traum,", "tokens": ["Der", "Him\u00b7mel", "h\u00e4ngt", "da\u00b7rin", "wie", "ein", "dun\u00b7kel\u00b7blau\u00b7er", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und die Sterne wandeln zwischen den Rippen gelb und gross,", "tokens": ["Und", "die", "Ster\u00b7ne", "wan\u00b7deln", "zwi\u00b7schen", "den", "Rip\u00b7pen", "gelb", "und", "gross", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und der Mond liegt wie ein goldener Embryo in ihrem hohlen Schoss.", "tokens": ["Und", "der", "Mond", "liegt", "wie", "ein", "gol\u00b7de\u00b7ner", "Emb\u00b7ryo", "in", "ih\u00b7rem", "hoh\u00b7len", "Schoss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+-+-+-+", "measure": "anapaest.init"}}}}}