{"textgrid.poem.53885": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Wiederk\u00e4uer", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": " \u00bbWir nahen uns an diesem Tage", "tokens": ["\u00bb", "Wir", "na\u00b7hen", "uns", "an", "die\u00b7sem", "Ta\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": " als Kleeblatt, das ihr selber habt gepfl\u00fcckt,", "tokens": ["als", "Klee\u00b7blatt", ",", "das", "ihr", "sel\u00b7ber", "habt", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": " wir machten euch beim B\u00fccken viele Plage,", "tokens": ["wir", "mach\u00b7ten", "euch", "beim", "B\u00fc\u00b7cken", "vie\u00b7le", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": " denn dreimal habt ihr euch nach uns geb\u00fcckt.\u00ab", "tokens": ["denn", "drei\u00b7mal", "habt", "ihr", "euch", "nach", "uns", "ge\u00b7b\u00fcckt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}