{"textgrid.poem.43699": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem See", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum haben wir verlassen den Wald,", "tokens": ["Kaum", "ha\u00b7ben", "wir", "ver\u00b7las\u00b7sen", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den liebsten Sommeraufenthalt,", "tokens": ["Den", "liebs\u00b7ten", "Som\u00b7me\u00b7rau\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So liegt vor uns im Abendschein", "tokens": ["So", "liegt", "vor", "uns", "im", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein See so freundlich, als l\u00fcd' er uns ein.", "tokens": ["Ein", "See", "so", "freund\u00b7lich", ",", "als", "l\u00fcd'", "er", "uns", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "An seinen Ufern wartet ein Nachen,", "tokens": ["An", "sei\u00b7nen", "U\u00b7fern", "war\u00b7tet", "ein", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir steigen ein und rudern fort,", "tokens": ["Wir", "stei\u00b7gen", "ein", "und", "ru\u00b7dern", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fahren unter Singen und Lachen", "tokens": ["Und", "fah\u00b7ren", "un\u00b7ter", "Sin\u00b7gen", "und", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umher an des Seees blumigem Bord.", "tokens": ["Um\u00b7her", "an", "des", "See\u00b7es", "blu\u00b7mi\u00b7gem", "Bord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}