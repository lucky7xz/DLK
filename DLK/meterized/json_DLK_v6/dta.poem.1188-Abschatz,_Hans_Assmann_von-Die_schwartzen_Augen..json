{"dta.poem.1188": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die schwartzen Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin soll ich zu erst die Augen wenden/ ", "tokens": ["Wo\u00b7hin", "soll", "ich", "zu", "erst", "die", "Au\u00b7gen", "wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir zu einer Zeit zwey Sonnen blenden?", "tokens": ["Die", "mir", "zu", "ei\u00b7ner", "Zeit", "zwey", "Son\u00b7nen", "blen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo soll ich erstlich hin/", "tokens": ["Wo", "soll", "ich", "erst\u00b7lich", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dieweil in meinem Sinn", "tokens": ["Die\u00b7weil", "in", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich gantz entz\u00fccket bin/", "tokens": ["Ich", "gantz", "ent\u00b7z\u00fc\u00b7cket", "bin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Blicke senden?", "tokens": ["Die", "Bli\u00b7cke", "sen\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Steht unter Steinen nicht der Demant oben?", "tokens": ["Steht", "un\u00b7ter", "Stei\u00b7nen", "nicht", "der", "De\u00b7mant", "o\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKNEG", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Feuer macht die dunckle Folge loben?", "tokens": ["Sein", "Feu\u00b7er", "macht", "die", "dunck\u00b7le", "Fol\u00b7ge", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der schwartzen Augen Zier", "tokens": ["Der", "schwart\u00b7zen", "Au\u00b7gen", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wird billig auch von mir", "tokens": ["Wird", "bil\u00b7lig", "auch", "von", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr allen andern hier", "tokens": ["F\u00fcr", "al\u00b7len", "an\u00b7dern", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Ruhm erhoben.", "tokens": ["Mit", "Ruhm", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "La\u00df Ph\u00f6bus hohen Glantz den Himmel mahlen:", "tokens": ["La\u00df", "Ph\u00f6\u00b7bus", "ho\u00b7hen", "Glantz", "den", "Him\u00b7mel", "mah\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit tausend Sternen mag der Abend prahlen:", "tokens": ["Mit", "tau\u00b7send", "Ster\u00b7nen", "mag", "der", "A\u00b7bend", "prah\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Augen lichte Nacht/", "tokens": ["Der", "Au\u00b7gen", "lich\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit welchen ihre Pracht", "tokens": ["Mit", "wel\u00b7chen", "ih\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Amene kundbar macht/", "tokens": ["A\u00b7me\u00b7ne", "kund\u00b7bar", "macht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wirfft hellre Stralen.", "tokens": ["Wirfft", "hell\u00b7re", "Stra\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}