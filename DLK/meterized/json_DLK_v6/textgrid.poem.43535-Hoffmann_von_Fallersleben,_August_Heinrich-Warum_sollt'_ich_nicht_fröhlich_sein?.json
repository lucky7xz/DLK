{"textgrid.poem.43535": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Warum sollt' ich nicht fr\u00f6hlich sein?", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum soll ich nicht fr\u00f6hlich sein?", "tokens": ["Wa\u00b7rum", "soll", "ich", "nicht", "fr\u00f6h\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So denkt das liebe V\u00f6gelein,", "tokens": ["So", "denkt", "das", "lie\u00b7be", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und singt und h\u00fcpft im Laub umher,", "tokens": ["Und", "singt", "und", "h\u00fcpft", "im", "Laub", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob die Welt sein eigen w\u00e4r'", "tokens": ["Als", "ob", "die", "Welt", "sein", "ei\u00b7gen", "w\u00e4r'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPOSAT", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum soll ich nicht fr\u00f6hlich sein?", "tokens": ["Wa\u00b7rum", "soll", "ich", "nicht", "fr\u00f6h\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So denkt das muntre Bienelein,", "tokens": ["So", "denkt", "das", "mun\u00b7tre", "Bie\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nippt, was ihm der Lenz beschied", "tokens": ["Und", "nippt", ",", "was", "ihm", "der", "Lenz", "be\u00b7schied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und summt dazu ein lustig Lied.", "tokens": ["Und", "summt", "da\u00b7zu", "ein", "lus\u00b7tig", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum sollt' ich nicht fr\u00f6hlich sein?", "tokens": ["Wa\u00b7rum", "sollt'", "ich", "nicht", "fr\u00f6h\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht der ganze Fr\u00fchling mein?", "tokens": ["Ist", "nicht", "der", "gan\u00b7ze", "Fr\u00fch\u00b7ling", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der liebe Gott hat sichtbarlich", "tokens": ["Der", "lie\u00b7be", "Gott", "hat", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt geschaffen auch f\u00fcr mich.", "tokens": ["Die", "Welt", "ge\u00b7schaf\u00b7fen", "auch", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}