{"textgrid.poem.31487": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die S\u00fc\u00dfheit selbst an Br\u00fcsten seuget.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die S\u00fc\u00dfheit selbst an Br\u00fcsten seuget.", "tokens": ["Die", "S\u00fc\u00df\u00b7heit", "selbst", "an", "Br\u00fcs\u00b7ten", "seu\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wei\u00dfheit wird ein kleines Kind.", "tokens": ["Die", "Wei\u00df\u00b7heit", "wird", "ein", "klei\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Allmacht man mit Windeln bindt.", "tokens": ["Die", "All\u00b7macht", "man", "mit", "Win\u00b7deln", "bindt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gott hier sein Herz leibhafftig zeiget /", "tokens": ["Gott", "hier", "sein", "Herz", "leib\u00b7haff\u00b7tig", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ja solches gar zu uns herneiget.", "tokens": ["ja", "sol\u00b7ches", "gar", "zu", "uns", "her\u00b7nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "das Himmelreich im Stall sich findt.", "tokens": ["das", "Him\u00b7mel\u00b7reich", "im", "Stall", "sich", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "hie dient das Engel-Hofgesind.", "tokens": ["hie", "dient", "das", "En\u00b7gel\u00b7Hof\u00b7ge\u00b7sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "di\u00df alles uns zu Trost erzweiget.", "tokens": ["di\u00df", "al\u00b7les", "uns", "zu", "Trost", "er\u00b7zwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Keuschheit einen Sohn gebahr /", "tokens": ["Die", "Keuschheit", "ei\u00b7nen", "Sohn", "ge\u00b7bahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.10": {"text": "der doch schon vor den Sternen war.", "tokens": ["der", "doch", "schon", "vor", "den", "Ster\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "was Er nicht war / das ist er worden:", "tokens": ["was", "Er", "nicht", "war", "/", "das", "ist", "er", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VAFIN", "$(", "PDS", "VAFIN", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "und was Er war / das blieb er noch:", "tokens": ["und", "was", "Er", "war", "/", "das", "blieb", "er", "noch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "$(", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "da\u00df Er in diesem neuen Orden /", "tokens": ["da\u00df", "Er", "in", "die\u00b7sem", "neu\u00b7en", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "von uns wegn\u00e4hm das S\u00fcnden Joch.", "tokens": ["von", "uns", "weg\u00b7n\u00e4hm", "das", "S\u00fcn\u00b7den", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}