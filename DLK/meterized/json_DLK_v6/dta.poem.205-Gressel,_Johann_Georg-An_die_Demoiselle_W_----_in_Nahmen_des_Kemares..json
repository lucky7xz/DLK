{"dta.poem.205": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die  Demoiselle W  ---- in Nahmen  \n des  Kemares.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Erbrecht galantes Kind das Siegel und mein Hertze/", "tokens": ["Er\u00b7brecht", "ga\u00b7lan\u00b7tes", "Kind", "das", "Sie\u00b7gel", "und", "mein", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "KON", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schauet/ da\u00df es treu/ ob es schon bl\u00f6de ist/", "tokens": ["Und", "schau\u00b7et", "/", "da\u00df", "es", "treu", "/", "ob", "es", "schon", "bl\u00f6\u00b7de", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ADJD", "$(", "KOUS", "PPER", "ADV", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wurd von euch entz\u00fcndt durch eurer Augen-Kertze/", "tokens": ["Es", "wurd", "von", "euch", "ent\u00b7z\u00fcndt", "durch", "eu\u00b7rer", "Au\u00b7gen\u00b7Kert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bald es eure Pracht und deren Licht gegr\u00fc\u00dft.", "tokens": ["So", "bald", "es", "eu\u00b7re", "Pracht", "und", "de\u00b7ren", "Licht", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "KON", "PRELAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann als eur sch\u00f6ner Mund so viele Heimlichkeiten", "tokens": ["Dann", "als", "eur", "sch\u00f6\u00b7ner", "Mund", "so", "vie\u00b7le", "Heim\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPOSAT", "ADJA", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja gar eur gantzes Hertz mir k\u00fcndig liesse seyn;", "tokens": ["Ja", "gar", "eur", "gant\u00b7zes", "Hertz", "mir", "k\u00fcn\u00b7dig", "lies\u00b7se", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So fing an alsofort die Liebe zu bereiten/", "tokens": ["So", "fing", "an", "al\u00b7so\u00b7fort", "die", "Lie\u00b7be", "zu", "be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Neigungs-volles Feur mich euch dadurch zu weyhn.", "tokens": ["Ein", "Nei\u00b7gungs\u00b7vol\u00b7les", "Feur", "mich", "euch", "da\u00b7durch", "zu", "weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PPER", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch meine Bl\u00f6digkeit verh\u00e4lete die Flammen/", "tokens": ["Doch", "mei\u00b7ne", "Bl\u00f6\u00b7dig\u00b7keit", "ver\u00b7h\u00e4\u00b7le\u00b7te", "die", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich war nicht dreist genug sie/ Sch\u00f6nste/ kund zu thun/", "tokens": ["Ich", "war", "nicht", "dreist", "ge\u00b7nug", "sie", "/", "Sch\u00f6ns\u00b7te", "/", "kund", "zu", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "PPER", "$(", "NN", "$(", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich meynte/ weil sie her von euren Augen stammen/", "tokens": ["Ich", "meyn\u00b7te", "/", "weil", "sie", "her", "von", "eu\u00b7ren", "Au\u00b7gen", "stam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So w\u00fcrde Brand und Feur bey euch nicht minder ruhn.", "tokens": ["So", "w\u00fcr\u00b7de", "Brand", "und", "Feur", "bey", "euch", "nicht", "min\u00b7der", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "APPR", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich lie\u00df die sch\u00f6ne Zeit verschwenderisch verstreichen/", "tokens": ["Ich", "lie\u00df", "die", "sch\u00f6\u00b7ne", "Zeit", "ver\u00b7schwen\u00b7de\u00b7risch", "ver\u00b7strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da ich/ eur ", "tokens": ["Da", "ich", "/", "eur"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$(", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Bald k\u00f6nnen so ein Bild in eure Br\u00fcste zeichen/", "tokens": ["Bald", "k\u00f6n\u00b7nen", "so", "ein", "Bild", "in", "eu\u00b7re", "Br\u00fcs\u00b7te", "zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das mir vor andern stets gegeben s\u00fcssen Blick.", "tokens": ["Das", "mir", "vor", "an\u00b7dern", "stets", "ge\u00b7ge\u00b7ben", "s\u00fcs\u00b7sen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PIS", "ADV", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich dachte zwar sehr offt die Gluht zu offenbahren/", "tokens": ["Ich", "dach\u00b7te", "zwar", "sehr", "offt", "die", "Gluht", "zu", "of\u00b7fen\u00b7bah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und war abwesend mehr als ich verlangte dreist/", "tokens": ["Und", "war", "ab\u00b7we\u00b7send", "mehr", "als", "ich", "ver\u00b7lang\u00b7te", "dreist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "KOUS", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein! wenn ich euch sah/ so lie\u00df den Muth ich fahren/", "tokens": ["Al\u00b7lein", "!", "wenn", "ich", "euch", "sah", "/", "so", "lie\u00df", "den", "Muth", "ich", "fah\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Worte waren weg/ es war verirrt der Geist.", "tokens": ["Die", "Wor\u00b7te", "wa\u00b7ren", "weg", "/", "es", "war", "ver\u00b7irrt", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "PPER", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nun da eur sch\u00f6n Gesicht mein Hertze mu\u00df entb\u00e4hren/", "tokens": ["Nun", "da", "eur", "sch\u00f6n", "Ge\u00b7sicht", "mein", "Hert\u00b7ze", "mu\u00df", "ent\u00b7b\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJD", "NN", "PPOSAT", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So sage ich euch frey durch diese treue Schrifft/", "tokens": ["So", "sa\u00b7ge", "ich", "euch", "frey", "durch", "die\u00b7se", "treu\u00b7e", "Schrifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df eure Sch\u00f6nheit ich so lange will verehren", "tokens": ["Da\u00df", "eu\u00b7re", "Sch\u00f6n\u00b7heit", "ich", "so", "lan\u00b7ge", "will", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bis mir der blasse Tod das matte Hertze trifft.", "tokens": ["Bis", "mir", "der", "blas\u00b7se", "Tod", "das", "mat\u00b7te", "Hert\u00b7ze", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Auf bis Bek\u00e4nntni\u00df will ich diese Antwort hoffen/", "tokens": ["Auf", "bis", "Be\u00b7k\u00e4nnt\u00b7ni\u00df", "will", "ich", "die\u00b7se", "Ant\u00b7wort", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr werdet eurem Knecht noch ferner g\u00fcnstig seyn/", "tokens": ["Ihr", "wer\u00b7det", "eu\u00b7rem", "Knecht", "noch", "fer\u00b7ner", "g\u00fcns\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und lassen ihm den Port zu Brust und Hertzen offen", "tokens": ["Und", "las\u00b7sen", "ihm", "den", "Port", "zu", "Brust", "und", "Hert\u00b7zen", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NE", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So stellt bey euch sich stets als treuer Diener ein", "tokens": ["So", "stellt", "bey", "euch", "sich", "stets", "als", "treu\u00b7er", "Die\u00b7ner", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PRF", "ADV", "KOUS", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kemares.", "tokens": ["Ke\u00b7ma\u00b7res", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}