{"textgrid.poem.34465": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da stehen sie im schmutzigen, zerrissnen Rock.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da stehen sie im schmutzigen, zerrissnen Rock.", "tokens": ["Da", "ste\u00b7hen", "sie", "im", "schmut\u00b7zi\u00b7gen", ",", "zer\u00b7riss\u00b7nen", "Rock", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Koth der Gasse klebt an ihrem plumpen Fuss.", "tokens": ["Der", "Koth", "der", "Gas\u00b7se", "klebt", "an", "ih\u00b7rem", "plum\u00b7pen", "Fuss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da stehen sie und starren bl\u00f6den Augs dich an", "tokens": ["Da", "ste\u00b7hen", "sie", "und", "star\u00b7ren", "bl\u00f6\u00b7den", "Augs", "dich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADJA", "ADJA", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und bergen beide F\u00e4uste in den Taschen tief.", "tokens": ["und", "ber\u00b7gen", "bei\u00b7de", "F\u00e4us\u00b7te", "in", "den", "Ta\u00b7schen", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du gabst die Stimme jenem Mann, den sie erw\u00e4hlt,", "tokens": ["Du", "gabst", "die", "Stim\u00b7me", "je\u00b7nem", "Mann", ",", "den", "sie", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PDAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der ihrem Elend laute Worte leihen soll,", "tokens": ["der", "ih\u00b7rem", "E\u00b7lend", "lau\u00b7te", "Wor\u00b7te", "lei\u00b7hen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und ihrer Sache gabst du wohl weit Gr\u00f6ssres schon.", "tokens": ["und", "ih\u00b7rer", "Sa\u00b7che", "gabst", "du", "wohl", "weit", "Gr\u00f6ss\u00b7res", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun trittst du aus dem Haus. Sie f\u00fcllen rottenweis", "tokens": ["Nun", "trittst", "du", "aus", "dem", "Haus", ".", "Sie", "f\u00fcl\u00b7len", "rot\u00b7ten\u00b7weis"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "vor dir die Strasse. Schweigend schauen sie auf dich", "tokens": ["vor", "dir", "die", "Stras\u00b7se", ".", "Schwei\u00b7gend", "schau\u00b7en", "sie", "auf", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "$.", "NN", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit diesem stummgebornen Hass im tr\u00e4gen Blick.", "tokens": ["mit", "die\u00b7sem", "stumm\u00b7ge\u00b7bor\u00b7nen", "Hass", "im", "tr\u00e4\u00b7gen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem Tiger auf der Lauer funkelt das Auge doch,", "tokens": ["Dem", "Ti\u00b7ger", "auf", "der", "Lau\u00b7er", "fun\u00b7kelt", "das", "Au\u00b7ge", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "es geht ein Gluthauch vor des L\u00f6wen Rachen her,", "tokens": ["es", "geht", "ein", "Glut\u00b7hauch", "vor", "des", "L\u00f6\u00b7wen", "Ra\u00b7chen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "doch dieses Volk, es lastet, stumm wie der Felsenhang", "tokens": ["doch", "die\u00b7ses", "Volk", ",", "es", "las\u00b7tet", ",", "stumm", "wie", "der", "Fel\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "$,", "PPER", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "ob deinem Haupt \u2013 und pl\u00f6tzlich l\u00f6st es sich \u2013 und f\u00e4llt.", "tokens": ["ob", "dei\u00b7nem", "Haupt", "\u2013", "und", "pl\u00f6tz\u00b7lich", "l\u00f6st", "es", "sich", "\u2013", "und", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$(", "KON", "ADJD", "VVFIN", "PPER", "PRF", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht was du willst, noch was du immer sinnst und denkst \u2013", "tokens": ["Nicht", "was", "du", "willst", ",", "noch", "was", "du", "im\u00b7mer", "sinnst", "und", "denkst", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "VMFIN", "$,", "KON", "PWS", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nein: was du bist, und dass du also worden bist,", "tokens": ["nein", ":", "was", "du", "bist", ",", "und", "dass", "du", "al\u00b7so", "wor\u00b7den", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "PPER", "VAFIN", "$,", "KON", "KOUS", "PPER", "ADV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das ist die S\u00fcnde, unter deren Fluch du stehst.", "tokens": ["das", "ist", "die", "S\u00fcn\u00b7de", ",", "un\u00b7ter", "de\u00b7ren", "Fluch", "du", "stehst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bist das Opfer, und mit dir dein ganz Geschlecht. \u2013", "tokens": ["Du", "bist", "das", "Op\u00b7fer", ",", "und", "mit", "dir", "dein", "ganz", "Ge\u00b7schlecht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "APPR", "PPER", "PPOSAT", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Furchtbares Schicksal: ohne Recht geboren sein", "tokens": ["Furcht\u00b7ba\u00b7res", "Schick\u00b7sal", ":", "oh\u00b7ne", "Recht", "ge\u00b7bo\u00b7ren", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "NN", "VVPP", "VAINF"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "im Heute noch im Morgen! Ein verwelkter Wald,", "tokens": ["im", "Heu\u00b7te", "noch", "im", "Mor\u00b7gen", "!", "Ein", "ver\u00b7welk\u00b7ter", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der nie gegr\u00fcnt! Ein Kind, im Mutterleibe siech!", "tokens": ["der", "nie", "ge\u00b7gr\u00fcnt", "!", "Ein", "Kind", ",", "im", "Mut\u00b7ter\u00b7lei\u00b7be", "siech", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$.", "ART", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir sind die Opfer fremder, langgeh\u00e4ufter Schuld ...", "tokens": ["Wir", "sind", "die", "Op\u00b7fer", "frem\u00b7der", ",", "lang\u00b7ge\u00b7h\u00e4uf\u00b7ter", "Schuld", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir sind die Opfer einer fernen sch\u00f6neren Zeit!", "tokens": ["Wir", "sind", "die", "Op\u00b7fer", "ei\u00b7ner", "fer\u00b7nen", "sch\u00f6\u00b7ne\u00b7ren", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Wir sind die Saat! \u2013 \u2013 O m\u00f6gen goldene \u00c4hren einst", "tokens": ["Wir", "sind", "die", "Saat", "!", "\u2013", "\u2013", "O", "m\u00f6\u00b7gen", "gol\u00b7de\u00b7ne", "\u00c4h\u00b7ren", "einst"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$(", "$(", "NE", "VMFIN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "wogend verh\u00fcllen dunkeler Erde vergessenen Grund!", "tokens": ["wo\u00b7gend", "ver\u00b7h\u00fcl\u00b7len", "dun\u00b7ke\u00b7ler", "Er\u00b7de", "ver\u00b7ges\u00b7se\u00b7nen", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+--+--+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "M\u00f6gen der rote Mohn und der Cyanen Blau", "tokens": ["M\u00f6\u00b7gen", "der", "ro\u00b7te", "Mohn", "und", "der", "Cy\u00b7a\u00b7nen", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "als Edelsteine leuchten aus dem Goldgeschmeid!", "tokens": ["als", "E\u00b7del\u00b7stei\u00b7ne", "leuch\u00b7ten", "aus", "dem", "Gold\u00b7ge\u00b7schmeid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann flattern die Falter freudig in der Sonne Strahl,", "tokens": ["Dann", "flat\u00b7tern", "die", "Fal\u00b7ter", "freu\u00b7dig", "in", "der", "Son\u00b7ne", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "und Bienen summen honigtriefend \u00fcberall!", "tokens": ["und", "Bie\u00b7nen", "sum\u00b7men", "ho\u00b7nig\u00b7trie\u00b7fend", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}