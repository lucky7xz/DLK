{"textgrid.poem.40284": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sterbe d\u00fcrstend an der vollen Quelle;", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sterbe d\u00fcrstend an der vollen Quelle;", "tokens": ["Ich", "ster\u00b7be", "d\u00fcrs\u00b7tend", "an", "der", "vol\u00b7len", "Quel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ich, hei\u00df wie Glut, mir zittert Zahn an Zahn.", "tokens": ["ich", ",", "hei\u00df", "wie", "Glut", ",", "mir", "zit\u00b7tert", "Zahn", "an", "Zahn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "KOKOM", "NN", "$,", "PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Frostklappernd sitz'ich an der Feuerstelle,", "tokens": ["Frost\u00b7klap\u00b7pernd", "sitz'\u00b7ich", "an", "der", "Feu\u00b7er\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in meinem Vaterland ein fremder Mann.", "tokens": ["in", "mei\u00b7nem", "Va\u00b7ter\u00b7land", "ein", "frem\u00b7der", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nackt wie ein Wurm, geschm\u00fcckt wie Tamerlan,", "tokens": ["Nackt", "wie", "ein", "Wurm", ",", "ge\u00b7schm\u00fcckt", "wie", "Ta\u00b7mer\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$,", "VVPP", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "lach ich in Thr\u00e4nen, hoffe voller Leid", "tokens": ["lach", "ich", "in", "Thr\u00e4\u00b7nen", ",", "hof\u00b7fe", "vol\u00b7ler", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "und sch\u00f6pfe Trost aus meiner Traurigkeit,", "tokens": ["und", "sch\u00f6p\u00b7fe", "Trost", "aus", "mei\u00b7ner", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "ein Mann voll Macht, ein Mann in Acht und Bann,", "tokens": ["ein", "Mann", "voll", "Macht", ",", "ein", "Mann", "in", "Acht", "und", "Bann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "APPR", "CARD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und meine Not ist meine Seligkeit:", "tokens": ["und", "mei\u00b7ne", "Not", "ist", "mei\u00b7ne", "Se\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "ich, hoch geliebt, geflohn von Jedermann.", "tokens": ["ich", ",", "hoch", "ge\u00b7liebt", ",", "ge\u00b7flohn", "von", "Je\u00b7der\u00b7mann", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "VVPP", "$,", "VVPP", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nichts ist mir sicher als das nie Gewisse", "tokens": ["Nichts", "ist", "mir", "si\u00b7cher", "als", "das", "nie", "Ge\u00b7wis\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "KOKOM", "PDS", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und dunkel nur, was allen Andern klar,", "tokens": ["und", "dun\u00b7kel", "nur", ",", "was", "al\u00b7len", "An\u00b7dern", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "PRELS", "PIAT", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und fraglich Nichts als das f\u00fcr sie Gewisse,", "tokens": ["und", "frag\u00b7lich", "Nichts", "als", "das", "f\u00fcr", "sie", "Ge\u00b7wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIS", "KOKOM", "PDS", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "denn nur der Zufall meint es mit mir wahr.", "tokens": ["denn", "nur", "der", "Zu\u00b7fall", "meint", "es", "mit", "mir", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gewinner stets, verspiel'ich immerdar;", "tokens": ["Ge\u00b7win\u00b7ner", "stets", ",", "ver\u00b7spiel'\u00b7ich", "im\u00b7mer\u00b7dar", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "mein Fr\u00fchgebet: Gott, mach den Abend gut!", "tokens": ["mein", "Fr\u00fch\u00b7ge\u00b7bet", ":", "Gott", ",", "mach", "den", "A\u00b7bend", "gut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im Liegen vor dem Fallen auf der Hut,", "tokens": ["Im", "Lie\u00b7gen", "vor", "dem", "Fal\u00b7len", "auf", "der", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "bin reich ich, der ich nichts verlieren kann,", "tokens": ["bin", "reich", "ich", ",", "der", "ich", "nichts", "ver\u00b7lie\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "$,", "PRELS", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und hoff' auf Erbschaft, ich, ein rechtlos Blut \u2013", "tokens": ["und", "hoff'", "auf", "Erb\u00b7schaft", ",", "ich", ",", "ein", "recht\u00b7los", "Blut", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PPER", "$,", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "ich, hoch geliebt, geflohn von Jedermann.", "tokens": ["ich", ",", "hoch", "ge\u00b7liebt", ",", "ge\u00b7flohn", "von", "Je\u00b7der\u00b7mann", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "VVPP", "$,", "VVPP", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nichts macht mir Sorge als mein b\u00f6s Begehren", "tokens": ["Nichts", "macht", "mir", "Sor\u00b7ge", "als", "mein", "b\u00f6s", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "NN", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nach Gl\u00fcck und Gut, doch pfeif ich drauf zumeist.", "tokens": ["nach", "Gl\u00fcck", "und", "Gut", ",", "doch", "pfeif", "ich", "drauf", "zu\u00b7meist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Wahrste ist, wer mich mit L\u00fcgen speist.", "tokens": ["der", "Wahrs\u00b7te", "ist", ",", "wer", "mich", "mit", "L\u00fc\u00b7gen", "speist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Freund ist, wer mit klipp und klar beweist:", "tokens": ["Mein", "Freund", "ist", ",", "wer", "mit", "klipp", "und", "klar", "be\u00b7weist", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWS", "APPR", "NE", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ein grauer Kater ist ein bunter Pfau.", "tokens": ["ein", "grau\u00b7er", "Ka\u00b7ter", "ist", "ein", "bun\u00b7ter", "Pfau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wer mir schadet, lehrt mich: du, Dem trau!", "tokens": ["Und", "wer", "mir", "scha\u00b7det", ",", "lehrt", "mich", ":", "du", ",", "Dem", "trau", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$.", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wahrheit, Lug-Trug, mir Alles Eins fortan;", "tokens": ["Wahr\u00b7heit", ",", "Lug\u00b7Trug", ",", "mir", "Al\u00b7les", "Eins", "for\u00b7tan", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "begreif ich's nicht, behalt ich's nur genau!", "tokens": ["be\u00b7greif", "ich's", "nicht", ",", "be\u00b7halt", "ich's", "nur", "ge\u00b7nau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "PTKNEG", "$,", "VVFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "ich, hoch geliebt, geflohn von Jedermann.", "tokens": ["ich", ",", "hoch", "ge\u00b7liebt", ",", "ge\u00b7flohn", "von", "Je\u00b7der\u00b7mann", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "VVPP", "$,", "VVPP", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}