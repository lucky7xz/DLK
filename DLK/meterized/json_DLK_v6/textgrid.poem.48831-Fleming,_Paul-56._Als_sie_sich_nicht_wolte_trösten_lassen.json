{"textgrid.poem.48831": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "56. Als sie sich nicht wolte tr\u00f6sten lassen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst mir di\u00df und das von dir und mir und dem,", "tokens": ["Du", "sagst", "mir", "di\u00df", "und", "das", "von", "dir", "und", "mir", "und", "dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "KON", "ART", "APPR", "PPER", "KON", "PPER", "KON", "ART", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "was einst der Zweck soll sein nach diesen langen Plagen.", "tokens": ["was", "einst", "der", "Zweck", "soll", "sein", "nach", "die\u00b7sen", "lan\u00b7gen", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VMFIN", "PPOSAT", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Itzt hastu dieses da, dort jenes h\u00f6ren sagen,", "tokens": ["Itzt", "has\u00b7tu", "die\u00b7ses", "da", ",", "dort", "je\u00b7nes", "h\u00f6\u00b7ren", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "ADV", "$,", "ADV", "PDS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und frag' ich denn darnach, so wei\u00dft du nicht von wem.", "tokens": ["und", "frag'", "ich", "denn", "dar\u00b7nach", ",", "so", "wei\u00dft", "du", "nicht", "von", "wem", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PAV", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Sch\u00f6ne, w\u00e4r' ich dir von Herzen angenehm,", "tokens": ["O", "Sch\u00f6\u00b7ne", ",", "w\u00e4r'", "ich", "dir", "von", "Her\u00b7zen", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich wei\u00df, du w\u00fcrdest nicht nach fremden M\u00e4hren fragen,", "tokens": ["ich", "wei\u00df", ",", "du", "w\u00fcr\u00b7dest", "nicht", "nach", "frem\u00b7den", "M\u00e4h\u00b7ren", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die, wie sie mich bei dir, so dich bei mir verklagen,", "tokens": ["die", ",", "wie", "sie", "mich", "bei", "dir", ",", "so", "dich", "bei", "mir", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "PRF", "APPR", "PPER", "$,", "ADV", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ich aber halte mich auf allen Fall bequ\u00e4m.", "tokens": ["ich", "a\u00b7ber", "hal\u00b7te", "mich", "auf", "al\u00b7len", "Fall", "be\u00b7qu\u00e4m", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Stell deinen Zweifel ab und la\u00df die Leute l\u00fcgen,", "tokens": ["Stell", "dei\u00b7nen", "Zwei\u00b7fel", "ab", "und", "la\u00df", "die", "Leu\u00b7te", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "KON", "VVIMP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "es wird zu seiner Zeit sich Alles m\u00fcssen f\u00fcgen.", "tokens": ["es", "wird", "zu", "sei\u00b7ner", "Zeit", "sich", "Al\u00b7les", "m\u00fcs\u00b7sen", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PRF", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df deinen starken Trost mein festes Herze sein,", "tokens": ["La\u00df", "dei\u00b7nen", "star\u00b7ken", "Trost", "mein", "fes\u00b7tes", "Her\u00b7ze", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PPOSAT", "ADJA", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "wie meinem deines ist. Und wenn ich bin geschieden,", "tokens": ["wie", "mei\u00b7nem", "dei\u00b7nes", "ist", ".", "Und", "wenn", "ich", "bin", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "VAFIN", "$.", "KON", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so la\u00df di\u00df Einige dich sprechen stets zufrieden:", "tokens": ["so", "la\u00df", "di\u00df", "Ei\u00b7ni\u00b7ge", "dich", "spre\u00b7chen", "stets", "zu\u00b7frie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PDS", "PIS", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mein Herze steht bei Ja, wenn Alles schw\u00f6rt auf Nein.", "tokens": ["mein", "Her\u00b7ze", "steht", "bei", "Ja", ",", "wenn", "Al\u00b7les", "schw\u00f6rt", "auf", "Nein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PTKANT", "$,", "KOUS", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}