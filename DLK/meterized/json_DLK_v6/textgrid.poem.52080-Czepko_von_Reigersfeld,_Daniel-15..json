{"textgrid.poem.52080": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was gleich ist, gleicht sich wol, nun, wie ich h\u00f6re sagen,", "tokens": ["Was", "gleich", "ist", ",", "gleicht", "sich", "wol", ",", "nun", ",", "wie", "ich", "h\u00f6\u00b7re", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVFIN", "PRF", "ADV", "$,", "ADV", "$,", "PWAV", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So trifft man weit und breit nicht gleicher Eheleut an,", "tokens": ["So", "trifft", "man", "weit", "und", "breit", "nicht", "glei\u00b7cher", "E\u00b7he\u00b7leut", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "PTKNEG", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Sie ist das schlimmste Weib, er ist der schlimmste Mann,", "tokens": ["Sie", "ist", "das", "schlimms\u00b7te", "Weib", ",", "er", "ist", "der", "schlimms\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum wunder ich mich sehr, da\u00df sie sich nicht vertragen.", "tokens": ["Drum", "wun\u00b7der", "ich", "mich", "sehr", ",", "da\u00df", "sie", "sich", "nicht", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}