{"textgrid.poem.43595": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der wackere Reiter", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo mein Pferd den Winter stand?", "tokens": ["Wo", "mein", "Pferd", "den", "Win\u00b7ter", "stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Weidenzweigen:", "tokens": ["In", "den", "Wei\u00b7den\u00b7zwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weil ich's eben wiederfand,", "tokens": ["Weil", "ich's", "e\u00b7ben", "wie\u00b7der\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich's auch besteigen.", "tokens": ["Will", "ich's", "auch", "be\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Tummle dich und sei nicht faul!", "tokens": ["Tumm\u00b7le", "dich", "und", "sei", "nicht", "faul", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiten will ich heute.", "tokens": ["Rei\u00b7ten", "will", "ich", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bist du flei\u00dfig, lieber Gaul,", "tokens": ["Bist", "du", "flei\u00b7\u00dfig", ",", "lie\u00b7ber", "Gaul", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freu'n sich alle Leute.", "tokens": ["Freu'n", "sich", "al\u00b7le", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auch die kleinen Bl\u00fcmelein", "tokens": ["Auch", "die", "klei\u00b7nen", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicken auf und nieder:", "tokens": ["Ni\u00b7cken", "auf", "und", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gr\u00fc\u00df dich Gott, lieb' Br\u00fcderlein!", "tokens": ["Gr\u00fc\u00df", "dich", "Gott", ",", "lieb'", "Br\u00fc\u00b7derl\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so komm bald wieder!", "tokens": ["Nun", "so", "komm", "bald", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auch die V\u00f6gel freu'n sich sehr", "tokens": ["Auch", "die", "V\u00f6\u00b7gel", "freu'n", "sich", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings auf allen Seiten,", "tokens": ["Rings", "auf", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zwitschern fr\u00f6hlich um mich her:", "tokens": ["Zwit\u00b7schern", "fr\u00f6h\u00b7lich", "um", "mich", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, was kannst du reiten!", "tokens": ["Ei", ",", "was", "kannst", "du", "rei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Pferdchen geht den Trippeltrab", "tokens": ["Pferd\u00b7chen", "geht", "den", "Trip\u00b7pel\u00b7trab"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer frisch und heiter", "tokens": ["Im\u00b7mer", "frisch", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00fcgel auf und H\u00fcgel ab,", "tokens": ["H\u00fc\u00b7gel", "auf", "und", "H\u00fc\u00b7gel", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich bin der Reiter.", "tokens": ["Und", "ich", "bin", "der", "Rei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}