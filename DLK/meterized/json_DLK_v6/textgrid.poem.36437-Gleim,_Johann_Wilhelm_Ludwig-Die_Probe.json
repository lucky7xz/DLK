{"textgrid.poem.36437": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Probe", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweig, Doris! schweig, ich bin schon m\u00fcde,", "tokens": ["Schweig", ",", "Do\u00b7ris", "!", "schweig", ",", "ich", "bin", "schon", "m\u00fc\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$.", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00f6re nichts von deinem Liede;", "tokens": ["Ich", "h\u00f6\u00b7re", "nichts", "von", "dei\u00b7nem", "Lie\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei still und singe nur nicht mehr,", "tokens": ["Sei", "still", "und", "sin\u00b7ge", "nur", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Lied verdienet kein Geh\u00f6r.", "tokens": ["Dein", "Lied", "ver\u00b7die\u00b7net", "kein", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du wekkst mich, Doris! la\u00df mich schlafen,", "tokens": ["Du", "wekkst", "mich", ",", "Do\u00b7ris", "!", "la\u00df", "mich", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "$.", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Schlummer soll den Kaltsinn strafen,", "tokens": ["Mein", "Schlum\u00b7mer", "soll", "den", "Kal\u00b7tsinn", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den ich aus deinen Augen las,", "tokens": ["Den", "ich", "aus", "dei\u00b7nen", "Au\u00b7gen", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Damon dir zur Seite sa\u00df.", "tokens": ["Als", "Da\u00b7mon", "dir", "zur", "Sei\u00b7te", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du l\u00e4chelst, und du willst mich k\u00fcssen;", "tokens": ["Du", "l\u00e4\u00b7chelst", ",", "und", "du", "willst", "mich", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja, Doris! komm und la\u00df mir wissen:", "tokens": ["Ja", ",", "Do\u00b7ris", "!", "komm", "und", "la\u00df", "mir", "wis\u00b7sen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$.", "VVFIN", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob Falschheit in dem Kusse stekkt,", "tokens": ["Ob", "Falschheit", "in", "dem", "Kus\u00b7se", "stekkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob er nach Damons K\u00fcssen schmekkt.", "tokens": ["Ob", "er", "nach", "Da\u00b7mons", "K\u00fcs\u00b7sen", "schmekkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er schmekkt \u2013 \u2013 \u2013 allein du must nicht schelten,", "tokens": ["Er", "schmekkt", "\u2013", "\u2013", "\u2013", "al\u00b7lein", "du", "must", "nicht", "schel\u00b7ten", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "$(", "$(", "ADV", "PPER", "VMFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df dismal Grund und Argwohn gelten;", "tokens": ["La\u00df", "dis\u00b7mal", "Grund", "und", "Arg\u00b7wohn", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Ku\u00df, und nicht dein Lied beweist:", "tokens": ["Dein", "Ku\u00df", ",", "und", "nicht", "dein", "Lied", "be\u00b7weist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie treu, wie liebenswert du seist.", "tokens": ["Wie", "treu", ",", "wie", "lie\u00b7bens\u00b7wert", "du", "seist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun kann ich dich mit Grunde loben;", "tokens": ["Nun", "kann", "ich", "dich", "mit", "Grun\u00b7de", "lo\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nein, ich mu\u00df noch weiter proben,", "tokens": ["Doch", "nein", ",", "ich", "mu\u00df", "noch", "wei\u00b7ter", "pro\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Lied verdienet kein Geh\u00f6r,", "tokens": ["Dein", "Lied", "ver\u00b7die\u00b7net", "kein", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Ku\u00df beweist mir zehnmal mehr.", "tokens": ["Dein", "Ku\u00df", "be\u00b7weist", "mir", "zehn\u00b7mal", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}