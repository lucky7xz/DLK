{"textgrid.poem.62235": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[ich meint' es rechtschaffen und ehrlich]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich meint' es rechtschaffen und ehrlich,", "tokens": ["Ich", "meint'", "es", "recht\u00b7schaf\u00b7fen", "und", "ehr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "KON", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Doch zu mir selber nicht gut \u2013", "tokens": ["Doch", "zu", "mir", "sel\u00b7ber", "nicht", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit jeglichem Wesen viel besser \u2013", "tokens": ["Mit", "jeg\u00b7li\u00b7chem", "We\u00b7sen", "viel", "bes\u00b7ser", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADJD", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und schrieb meine Verse mit Blut! \u2013", "tokens": ["Und", "schrieb", "mei\u00b7ne", "Ver\u00b7se", "mit", "Blut", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}