{"textgrid.poem.43557": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der verwaisete Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt ein Berg im Meere", "tokens": ["Es", "liegt", "ein", "Berg", "im", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wei\u00df wie Wellenschaum,", "tokens": ["So", "wei\u00df", "wie", "Wel\u00b7len\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf dem Berge stehet", "tokens": ["Und", "auf", "dem", "Ber\u00b7ge", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein gr\u00fcner Lindenbaum.", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Lin\u00b7den\u00b7baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich Armer schwamm hin\u00fcber,", "tokens": ["Ich", "Ar\u00b7mer", "schwamm", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umarmt' ihn inniglich:", "tokens": ["Um\u00b7armt'", "ihn", "in\u00b7nig\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du lieber Baum, verwandle", "tokens": ["Du", "lie\u00b7ber", "Baum", ",", "ver\u00b7wand\u00b7le"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "NE", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinen Vater dich!", "tokens": ["In", "mei\u00b7nen", "Va\u00b7ter", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Reich deine gr\u00fcnen Zweige", "tokens": ["Reich", "dei\u00b7ne", "gr\u00fc\u00b7nen", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie wei\u00dfe H\u00e4ndelein,", "tokens": ["Wie", "wei\u00b7\u00dfe", "H\u00e4n\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df deine gr\u00fcnen Bl\u00e4tter", "tokens": ["La\u00df", "dei\u00b7ne", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir Liebesworte sein!", "tokens": ["Mir", "Lie\u00b7bes\u00b7wor\u00b7te", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich Armer ging von hinnen", "tokens": ["Ich", "Ar\u00b7mer", "ging", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und weinte bitterlich:", "tokens": ["Und", "wein\u00b7te", "bit\u00b7ter\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht hat in meinen Vater", "tokens": ["Nicht", "hat", "in", "mei\u00b7nen", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Baum verwandelt sich.", "tokens": ["Der", "Baum", "ver\u00b7wan\u00b7delt", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die gr\u00fcnen Zweige wurden", "tokens": ["Die", "gr\u00fc\u00b7nen", "Zwei\u00b7ge", "wur\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu wei\u00dfen H\u00e4nden nicht,", "tokens": ["Zu", "wei\u00b7\u00dfen", "H\u00e4n\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ach! kein gr\u00fcnes Bl\u00e4ttchen", "tokens": ["Und", "ach", "!", "kein", "gr\u00fc\u00b7nes", "Bl\u00e4tt\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir Liebesworte spricht.", "tokens": ["Mir", "Lie\u00b7bes\u00b7wor\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}