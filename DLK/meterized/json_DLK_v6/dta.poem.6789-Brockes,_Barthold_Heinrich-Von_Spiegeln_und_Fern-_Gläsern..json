{"dta.poem.6789": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es zeigen uns durch Kunst geschliffne Gl\u00e4ser an,", "tokens": ["Es", "zei\u00b7gen", "uns", "durch", "Kunst", "ge\u00b7schliff\u00b7ne", "Gl\u00e4\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie man die Bl\u00f6digkeit der Augen bessern,", "tokens": ["Wie", "man", "die", "Bl\u00f6\u00b7dig\u00b7keit", "der", "Au\u00b7gen", "bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entfernte Vorw\u00fcrff\u2019 nah, und zu uns ziehen kan,", "tokens": ["Ent\u00b7fern\u00b7te", "Vor\u00b7w\u00fcrff'", "nah", ",", "und", "zu", "uns", "zie\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,", "KON", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und solche C\u00f6rper starck vergr\u00f6ssern,", "tokens": ["Und", "sol\u00b7che", "C\u00f6r\u00b7per", "starck", "ver\u00b7gr\u00f6s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die durch die Kleinheit nicht zu sehn.", "tokens": ["Die", "durch", "die", "Klein\u00b7heit", "nicht", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie geben, was sich sonst verbarg, uns zu verstehn.", "tokens": ["Sie", "ge\u00b7ben", ",", "was", "sich", "sonst", "ver\u00b7barg", ",", "uns", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}