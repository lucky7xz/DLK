{"textgrid.poem.30971": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gulo hat Ged\u00e4rm im Kopff und Gehirn im Bauche;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gulo hat Ged\u00e4rm im Kopff und Gehirn im Bauche;", "tokens": ["Gu\u00b7lo", "hat", "Ge\u00b7d\u00e4rm", "im", "Kopff", "und", "Ge\u00b7hirn", "im", "Bau\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "APPRART", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann zu sorgen f\u00fcr den Bauch, hat er stets im Brauche.", "tokens": ["Dann", "zu", "sor\u00b7gen", "f\u00fcr", "den", "Bauch", ",", "hat", "er", "stets", "im", "Brau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie heist man die bey Hofe, die alle Gunst weg tragen?", "tokens": ["Wie", "heist", "man", "die", "bey", "Ho\u00b7fe", ",", "die", "al\u00b7le", "Gunst", "weg", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ART", "APPR", "NN", "$,", "PRELS", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Man heist sie da die Hertzen; ich nenne sie die Magen.", "tokens": ["Man", "heist", "sie", "da", "die", "Hert\u00b7zen", ";", "ich", "nen\u00b7ne", "sie", "die", "Ma\u00b7gen", "."], "token_info": ["word", "w