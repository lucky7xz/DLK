{"dta.poem.19548": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das beerdigte Herz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Mich denkt es eines alten Traums.", "tokens": ["Mich", "denkt", "es", "ei\u00b7nes", "al\u00b7ten", "Traums", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es war in meiner dumpfen Zeit,", "tokens": ["Es", "war", "in", "mei\u00b7ner", "dum\u00b7pfen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da junge Wildheit in mir gohr.", "tokens": ["Da", "jun\u00b7ge", "Wild\u00b7heit", "in", "mir", "gohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bek\u00fcmmert war die Mutter oft.", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "war", "die", "Mut\u00b7ter", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da kam einmal ein schlimmer Brief.", "tokens": ["Da", "kam", "ein\u00b7mal", "ein", "schlim\u00b7mer", "Brief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "(was er enthielt errieth ich nie.)", "tokens": ["(", "was", "er", "ent\u00b7hielt", "er\u00b7rieth", "ich", "nie", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PPER", "VVFIN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Mutter fuhr sich mit der Hand", "tokens": ["Die", "Mut\u00b7ter", "fuhr", "sich", "mit", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Herzen, fast als st\u00fcrb' es ihr.", "tokens": ["Zum", "Her\u00b7zen", ",", "fast", "als", "st\u00fcrb'", "es", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "KOKOM", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Nacht darauf hatt' ich den Traum:", "tokens": ["Die", "Nacht", "da\u00b7rauf", "hatt'", "ich", "den", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Mutter sah verstohlen ich", "tokens": ["Die", "Mut\u00b7ter", "sah", "ver\u00b7stoh\u00b7len", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nach unserm Tannenwinkel gehn,", "tokens": ["Nach", "un\u00b7serm", "Tan\u00b7nen\u00b7win\u00b7kel", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den Spaten in der zarten Hand,", "tokens": ["Den", "Spa\u00b7ten", "in", "der", "zar\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sie grub ein Grab und legt' ein Herz", "tokens": ["Sie", "grub", "ein", "Grab", "und", "legt'", "ein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hinunter sacht. Sie ebnete", "tokens": ["Hin\u00b7un\u00b7ter", "sacht", ".", "Sie", "eb\u00b7ne\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Erde dann und schlich davon.", "tokens": ["Die", "Er\u00b7de", "dann", "und", "schlich", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}