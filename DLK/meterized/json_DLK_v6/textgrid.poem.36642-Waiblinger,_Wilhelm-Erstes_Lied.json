{"textgrid.poem.36642": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und warum nicht, heitere Muse,", "tokens": ["Und", "wa\u00b7rum", "nicht", ",", "hei\u00b7te\u00b7re", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Lied und Lob dem Carnevale?", "tokens": ["Lied", "und", "Lob", "dem", "Car\u00b7ne\u00b7va\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bienen konntest du besingen,", "tokens": ["Bie\u00b7nen", "konn\u00b7test", "du", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Konntest sch\u00f6ne Frauen ehren,", "tokens": ["Konn\u00b7test", "sch\u00f6\u00b7ne", "Frau\u00b7en", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst den Duft der Blumen preisen \u2013", "tokens": ["Selbst", "den", "Duft", "der", "Blu\u00b7men", "prei\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und warum nicht all die Schw\u00e4rme", "tokens": ["Und", "wa\u00b7rum", "nicht", "all", "die", "Schw\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PTKNEG", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lust'ger, honigs\u00fc\u00dfer Feen,", "tokens": ["Lust'\u00b7ger", ",", "ho\u00b7nig\u00b7s\u00fc\u00b7\u00dfer", "Feen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Rom in Kr\u00e4nzen und in Blumen?", "tokens": ["Rom", "in", "Kr\u00e4n\u00b7zen", "und", "in", "Blu\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein, dem trunknen Taumel geb' ich", "tokens": ["Nein", ",", "dem", "trunk\u00b7nen", "Tau\u00b7mel", "geb'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungescheut mich hin, und singe,", "tokens": ["Un\u00b7ge\u00b7scheut", "mich", "hin", ",", "und", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe meiner Lieder Weise;", "tokens": ["Sin\u00b7ge", "mei\u00b7ner", "Lie\u00b7der", "Wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie auch im Vaterlande", "tokens": ["Wenn", "sie", "auch", "im", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drob mich einen Thoren schelten,", "tokens": ["Drob", "mich", "ei\u00b7nen", "Tho\u00b7ren", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dennoch sing' ich, denn sie kennen", "tokens": ["Den\u00b7noch", "sing'", "ich", ",", "denn", "sie", "ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Solche Lust und solch ein Fest", "tokens": ["Sol\u00b7che", "Lust", "und", "solch", "ein", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur im Land der ew'gen Freude.", "tokens": ["Nur", "im", "Land", "der", "ew'\u00b7gen", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch, was w\u00fcnsch' ich mir zum Liede?", "tokens": ["Doch", ",", "was", "w\u00fcn\u00b7sch'", "ich", "mir", "zum", "Lie\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bacchantin Glut, des Gottes", "tokens": ["Der", "Bac\u00b7chan\u00b7tin", "Glut", ",", "des", "Got\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brennend allbegeisternd Feuer?", "tokens": ["Bren\u00b7nend", "all\u00b7be\u00b7geis\u00b7ternd", "Feu\u00b7er", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder deine G\u00f6tterschalkheit,", "tokens": ["O\u00b7der", "dei\u00b7ne", "G\u00f6t\u00b7ter\u00b7schalk\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aristophanes, ein wenig", "tokens": ["A\u00b7ris\u00b7to\u00b7pha\u00b7nes", ",", "ein", "we\u00b7nig"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "PIS"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Nur vom Geiste deiner Maske?", "tokens": ["Nur", "vom", "Geis\u00b7te", "dei\u00b7ner", "Mas\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcnsch' ich, Grazien, eure Huld,", "tokens": ["W\u00fcn\u00b7sch'", "ich", ",", "Gra\u00b7zi\u00b7en", ",", "eu\u00b7re", "Huld", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Eure Sch\u00f6nheit, holde Veilchen?", "tokens": ["Eu\u00b7re", "Sch\u00f6n\u00b7heit", ",", "hol\u00b7de", "Veil\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und begreift ihr's nicht, und wolltet", "tokens": ["Und", "be\u00b7greift", "ih\u00b7r's", "nicht", ",", "und", "woll\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VMFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr dem trunknen S\u00e4nger z\u00fcrnen,", "tokens": ["Ihr", "dem", "trunk\u00b7nen", "S\u00e4n\u00b7ger", "z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O ihr sah't von Samnesertes", "tokens": ["O", "ihr", "sah't", "von", "Sam\u00b7ne\u00b7ser\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Obeliskus bis zum Grunde", "tokens": ["O\u00b7be\u00b7lis\u00b7kus", "bis", "zum", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu des Kapitoles Stufen,", "tokens": ["Zu", "des", "Ka\u00b7pi\u00b7to\u00b7les", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sah't noch nicht die goldgestickten", "tokens": ["Sah't", "noch", "nicht", "die", "gold\u00b7ge\u00b7stick\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bunten Purpurteppiche", "tokens": ["Bun\u00b7ten", "Pur\u00b7pur\u00b7tep\u00b7pi\u00b7che"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Von Balkon und Fenster wehen.", "tokens": ["Von", "Bal\u00b7kon", "und", "Fens\u00b7ter", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Schweiget still, ich bin im S\u00fcden;", "tokens": ["Schwei\u00b7get", "still", ",", "ich", "bin", "im", "S\u00fc\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfe Flocken st\u00e4uben nieder,", "tokens": ["Wei\u00b7\u00dfe", "Flo\u00b7cken", "st\u00e4u\u00b7ben", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber welch ein Schnee? o schweiget!", "tokens": ["A\u00b7ber", "welch", "ein", "Schnee", "?", "o", "schwei\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NN", "$.", "FM", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, es ist ein wilder Hagel,", "tokens": ["Ja", ",", "es", "ist", "ein", "wil\u00b7der", "Ha\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch von Zucker, und die Erde", "tokens": ["Doch", "von", "Zu\u00b7cker", ",", "und", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deckt er wei\u00df, von Frauenh\u00e4nden", "tokens": ["Deckt", "er", "wei\u00df", ",", "von", "Frau\u00b7en\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tr\u00e4uft und st\u00fcrmt er s\u00fc\u00df herab,", "tokens": ["Tr\u00e4uft", "und", "st\u00fcrmt", "er", "s\u00fc\u00df", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und bedeutet Fr\u00fchlingstage.", "tokens": ["Und", "be\u00b7deu\u00b7tet", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Blumen fliegen auf und nieder;", "tokens": ["Blu\u00b7men", "flie\u00b7gen", "auf", "und", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es nicht, als str\u00f6mten junge", "tokens": ["Ist", "es", "nicht", ",", "als", "str\u00f6m\u00b7ten", "jun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Neckisch kecke Liebesg\u00f6tter", "tokens": ["Ne\u00b7ckisch", "ke\u00b7cke", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Regen hier von Rosen,", "tokens": ["Ei\u00b7nen", "Re\u00b7gen", "hier", "von", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort von Veilchen in die Stra\u00dfe;", "tokens": ["Dort", "von", "Veil\u00b7chen", "in", "die", "Stra\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht, als schleuderten sie lachend", "tokens": ["Nicht", ",", "als", "schleu\u00b7der\u00b7ten", "sie", "la\u00b7chend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "KOUS", "VVFIN", "PPER", "ADJD"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Im Triumph auf Tausende", "tokens": ["Im", "Tri\u00b7umph", "auf", "Tau\u00b7sen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Zart verwundende Geschosse?", "tokens": ["Zart", "ver\u00b7wun\u00b7den\u00b7de", "Ge\u00b7schos\u00b7se", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Hat vielleicht die Abendsonne", "tokens": ["Hat", "viel\u00b7leicht", "die", "A\u00b7bend\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n're Farben, oder f\u00e4nd' ich", "tokens": ["Sch\u00f6n'\u00b7re", "Far\u00b7ben", ",", "o\u00b7der", "f\u00e4nd'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bunter noch die M\u00e4dchenreihen,", "tokens": ["Bun\u00b7ter", "noch", "die", "M\u00e4d\u00b7chen\u00b7rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So un\u00fcbersehbar schimmernd,", "tokens": ["So", "un\u00b7\u00fc\u00b7ber\u00b7seh\u00b7bar", "schim\u00b7mernd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie sie sind? Der Sel'gen Jubel", "tokens": ["Wie", "sie", "sind", "?", "Der", "Sel'\u00b7gen", "Ju\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In Elysium, er kl\u00e4nge", "tokens": ["In", "E\u00b7ly\u00b7si\u00b7um", ",", "er", "kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wohl harmonischer als dies", "tokens": ["Wohl", "har\u00b7mo\u00b7ni\u00b7scher", "als", "dies"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PDS"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Tausendstimmige Geschrille?", "tokens": ["Tau\u00b7sends\u00b7tim\u00b7mi\u00b7ge", "Ge\u00b7schril\u00b7le", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wo die Wirklichkeit zu finden,", "tokens": ["Wo", "die", "Wirk\u00b7lich\u00b7keit", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Gew\u00f6hnliche? Verzaubert", "tokens": ["Das", "Ge\u00b7w\u00f6hn\u00b7li\u00b7che", "?", "Ver\u00b7zau\u00b7bert"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Welt; der Mensch, er wandelt", "tokens": ["Ist", "die", "Welt", ";", "der", "Mensch", ",", "er", "wan\u00b7delt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$.", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderbar in seine Tr\u00e4ume,", "tokens": ["Wun\u00b7der\u00b7bar", "in", "sei\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine W\u00fcnsche, seine Sehnsucht,", "tokens": ["Sei\u00b7ne", "W\u00fcn\u00b7sche", ",", "sei\u00b7ne", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Phantasie verkleidet,", "tokens": ["Sei\u00b7ne", "Phan\u00b7ta\u00b7sie", "ver\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie er ist, er will sich nicht,", "tokens": ["Wie", "er", "ist", ",", "er", "will", "sich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "PPER", "VMFIN", "PRF", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie er m\u00f6chte sein, nur zeigen.", "tokens": ["Wie", "er", "m\u00f6ch\u00b7te", "sein", ",", "nur", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VAINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nur ein fl\u00fcchtiger Bewohner", "tokens": ["Nur", "ein", "fl\u00fcch\u00b7ti\u00b7ger", "Be\u00b7woh\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Welt, zum Scherz geboren,", "tokens": ["Die\u00b7ser", "Welt", ",", "zum", "Scherz", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zum Moment, will er sein Dasein,", "tokens": ["Zum", "Mo\u00b7ment", ",", "will", "er", "sein", "Da\u00b7sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich dem Schmetterling genie\u00dfen,", "tokens": ["Gleich", "dem", "Schmet\u00b7ter\u00b7ling", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dem dumpfen Haus der Puppe", "tokens": ["Und", "dem", "dum\u00b7pfen", "Haus", "der", "Pup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In vollendeter Entfaltung", "tokens": ["In", "voll\u00b7en\u00b7de\u00b7ter", "Ent\u00b7fal\u00b7tung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun entnommen, flattert er", "tokens": ["Nun", "ent\u00b7nom\u00b7men", ",", "flat\u00b7tert", "er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Buhlend unter seinen Blumen.", "tokens": ["Buh\u00b7lend", "un\u00b7ter", "sei\u00b7nen", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Jene m\u00e4chtigen Pal\u00e4ste,", "tokens": ["Je\u00b7ne", "m\u00e4ch\u00b7ti\u00b7gen", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur zur Lust des Augenblickes", "tokens": ["Nur", "zur", "Lust", "des", "Au\u00b7gen\u00b7bli\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheinen sie gebaut, es gibt ja", "tokens": ["Schei\u00b7nen", "sie", "ge\u00b7baut", ",", "es", "gibt", "ja"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kein Bed\u00fcrfni\u00df mehr, und Alles", "tokens": ["Kein", "Be\u00b7d\u00fcrf\u00b7ni\u00df", "mehr", ",", "und", "Al\u00b7les"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "ADV", "$,", "KON", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dient dem Schw\u00e4rmer nur zur Feier", "tokens": ["Dient", "dem", "Schw\u00e4r\u00b7mer", "nur", "zur", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seines Daseins, Noth und Sorgen", "tokens": ["Sei\u00b7nes", "Da\u00b7seins", ",", "Noth", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kannte ja die Puppe nur,", "tokens": ["Kann\u00b7te", "ja", "die", "Pup\u00b7pe", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht der schmucke Sommervogel.", "tokens": ["Nicht", "der", "schmu\u00b7cke", "Som\u00b7mer\u00b7vo\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und des eignen Lebens denk' ich,", "tokens": ["Und", "des", "eig\u00b7nen", "Le\u00b7bens", "denk'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jenes schnell zerflo\u00dfnen Zaubers", "tokens": ["Je\u00b7nes", "schnell", "zer\u00b7flo\u00df\u00b7nen", "Zau\u00b7bers"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Kindheit, da die Erde,", "tokens": ["Mei\u00b7ner", "Kind\u00b7heit", ",", "da", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da der Mensch mit seinen R\u00e4thseln", "tokens": ["Da", "der", "Mensch", "mit", "sei\u00b7nen", "R\u00e4th\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch so farbenreich und magisch", "tokens": ["Noch", "so", "far\u00b7ben\u00b7reich", "und", "ma\u00b7gisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem befang'nen Sinn erschienen,", "tokens": ["Dem", "be\u00b7fang'\u00b7nen", "Sinn", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Genu\u00df der Gegenwart", "tokens": ["Der", "Ge\u00b7nu\u00df", "der", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir das ganze Leben d\u00fcnkte.", "tokens": ["Mir", "das", "gan\u00b7ze", "Le\u00b7ben", "d\u00fcnk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}