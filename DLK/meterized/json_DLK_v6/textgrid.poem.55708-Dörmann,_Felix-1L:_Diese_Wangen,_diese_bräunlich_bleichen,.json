{"textgrid.poem.55708": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Diese Wangen, diese br\u00e4unlich bleichen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Wangen, diese br\u00e4unlich bleichen,", "tokens": ["Die\u00b7se", "Wan\u00b7gen", ",", "die\u00b7se", "br\u00e4un\u00b7lich", "blei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dieses dunkle, leichtgewellte Haar;", "tokens": ["Die\u00b7ses", "dunk\u00b7le", ",", "leicht\u00b7ge\u00b7well\u00b7te", "Haar", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Diese Lippen, diese tollkirschgleichen;", "tokens": ["Die\u00b7se", "Lip\u00b7pen", ",", "die\u00b7se", "toll\u00b7kirschglei\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "ADJA", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dieser Augen rabendunkles Paar;", "tokens": ["Die\u00b7ser", "Au\u00b7gen", "ra\u00b7ben\u00b7dunk\u00b7les", "Paar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Diese Brauen, dicht und schwer gezogen;", "tokens": ["Die\u00b7se", "Brau\u00b7en", ",", "dicht", "und", "schwer", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dieser Wimpern nachtgef\u00e4rbter Samt;", "tokens": ["Die\u00b7ser", "Wim\u00b7pern", "nacht\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Samt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dieser Leib, so m\u00fcd zur\u00fcckgebogen;", "tokens": ["Die\u00b7ser", "Leib", ",", "so", "m\u00fcd", "zu\u00b7r\u00fcck\u00b7ge\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Diese H\u00e4nde, weich und glutdurchflammt! \u2013", "tokens": ["Die\u00b7se", "H\u00e4n\u00b7de", ",", "weich", "und", "glut\u00b7durch\u00b7flammt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aus dem Herzen hast Du mir getrieben,", "tokens": ["Aus", "dem", "Her\u00b7zen", "hast", "Du", "mir", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Was es gut und stark und gro\u00df gemacht,", "tokens": ["Was", "es", "gut", "und", "stark", "und", "gro\u00df", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tot ist alles, nur ein krankes Lieben", "tokens": ["Tot", "ist", "al\u00b7les", ",", "nur", "ein", "kran\u00b7kes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIS", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zittert f\u00e4ulnisfahl durch meine Nacht.", "tokens": ["Zit\u00b7tert", "f\u00e4ul\u00b7nis\u00b7fahl", "durch", "mei\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nun, so la\u00df das irre Haupt mich pressen", "tokens": ["Nun", ",", "so", "la\u00df", "das", "ir\u00b7re", "Haupt", "mich", "pres\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVIMP", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Tief ins duftdurchtr\u00e4nkte, weiche Haar,", "tokens": ["Tief", "ins", "duft\u00b7durch\u00b7tr\u00e4nk\u00b7te", ",", "wei\u00b7che", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wahnwitz oder Liebe \u2013 nur vergessen,", "tokens": ["Wahn\u00b7witz", "o\u00b7der", "Lie\u00b7be", "\u2013", "nur", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$(", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Was ich bin und was ich war.", "tokens": ["Was", "ich", "bin", "und", "was", "ich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "KON", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}