{"textgrid.poem.39914": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herzog Christophs Kammerschreiber", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herzog Christophs Kammerschreiber", "tokens": ["Her\u00b7zog", "Chris\u00b7tophs", "Kam\u00b7mer\u00b7schrei\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War Franz Kurz, der Possentreiber,", "tokens": ["War", "Franz", "Kurz", ",", "der", "Pos\u00b7sen\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJD", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Obwohl im geheimen Rat", "tokens": ["Ob\u00b7wohl", "im", "ge\u00b7hei\u00b7men", "Rat"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er ganz ernst und sittig that.", "tokens": ["Er", "ganz", "ernst", "und", "sit\u00b7tig", "that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem St\u00fchlchen sa\u00df er unten;", "tokens": ["Auf", "dem", "St\u00fchl\u00b7chen", "sa\u00df", "er", "un\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die Herren R\u00e4t' im bunten,", "tokens": ["Wenn", "die", "Her\u00b7ren", "R\u00e4t'", "im", "bun\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weiten Kreise sa\u00dfen sinnend,", "tokens": ["Wei\u00b7ten", "Krei\u00b7se", "sa\u00b7\u00dfen", "sin\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lange Reden flossen rinnend,", "tokens": ["Lan\u00b7ge", "Re\u00b7den", "flos\u00b7sen", "rin\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sah man flink ihn mit der zieren", "tokens": ["Sah", "man", "flink", "ihn", "mit", "der", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Feder still protokolliren.", "tokens": ["Fe\u00b7der", "still", "pro\u00b7to\u00b7kol\u00b7li\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.11": {"text": "Keiner von den Herren dachte,", "tokens": ["Kei\u00b7ner", "von", "den", "Her\u00b7ren", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie Franz Kurz im Herzen lachte,", "tokens": ["Wie", "Franz", "Kurz", "im", "Her\u00b7zen", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Wenn ein schiefes W\u00f6rtlein fiel;", "tokens": ["Wenn", "ein", "schie\u00b7fes", "W\u00f6rt\u00b7lein", "fiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "R\u00fcstig lief sein Federkiel;", "tokens": ["R\u00fcs\u00b7tig", "lief", "sein", "Fe\u00b7der\u00b7kiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Aber heimlich hinterdrein", "tokens": ["A\u00b7ber", "heim\u00b7lich", "hin\u00b7ter\u00b7drein"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Gab er's preis bei'm Gl\u00e4schen Wein.", "tokens": ["Gab", "er's", "preis", "bei'm", "Gl\u00e4s\u00b7chen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einmal doch, da ward's ihm sauer,", "tokens": ["Ein\u00b7mal", "doch", ",", "da", "ward's", "ihm", "sau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als der B\u00fcrger und der Bauer", "tokens": ["Als", "der", "B\u00fcr\u00b7ger", "und", "der", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward mit Worten arg mi\u00dfhandelt,", "tokens": ["Ward", "mit", "Wor\u00b7ten", "arg", "mi\u00df\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Recht in Unrecht gar verwandelt;", "tokens": ["Recht", "in", "Un\u00b7recht", "gar", "ver\u00b7wan\u00b7delt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die alten weisen M\u00fcnde", "tokens": ["Und", "die", "al\u00b7ten", "wei\u00b7sen", "M\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm die allerfeinsten Gr\u00fcnde", "tokens": ["Ihm", "die", "al\u00b7ler\u00b7feins\u00b7ten", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zudiktirten f\u00fcr die Lehre:", "tokens": ["Zu\u00b7dik\u00b7tir\u00b7ten", "f\u00fcr", "die", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Volk, wie Schaf, sei f\u00fcr die Scheere.", "tokens": ["Volk", ",", "wie", "Schaf", ",", "sei", "f\u00fcr", "die", "Schee\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kaum hielt er die Sitzung aus;", "tokens": ["Kaum", "hielt", "er", "die", "Sit\u00b7zung", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und als Jeder ging nach Haus,", "tokens": ["Und", "als", "Je\u00b7der", "ging", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Blieb er noch im Saale stehen,", "tokens": ["Blieb", "er", "noch", "im", "Saa\u00b7le", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das Geschriebne durchzusehen;", "tokens": ["Das", "Ge\u00b7schrieb\u00b7ne", "durch\u00b7zu\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "St\u00f6\u00dft die Akten in die Scheide,", "tokens": ["St\u00f6\u00dft", "die", "Ak\u00b7ten", "in", "die", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Greift nach einem St\u00fccklein Kreide;", "tokens": ["Greift", "nach", "ei\u00b7nem", "St\u00fcck\u00b7lein", "Krei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Von Mutwill' und Zorn entbrannt,", "tokens": ["Von", "Mut\u00b7will'", "und", "Zorn", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Schreibt er an die Tafelwand", "tokens": ["Schreibt", "er", "an", "die", "Ta\u00b7fel\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Mit gro\u00dfm\u00e4cht'ger Schrift: \u00bbEi nu!", "tokens": ["Mit", "gro\u00df\u00b7m\u00e4cht'\u00b7ger", "Schrift", ":", "\u00bb", "Ei", "nu", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$(", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Es geht wunderlich hier zu!\u00ab", "tokens": ["Es", "geht", "wun\u00b7der\u00b7lich", "hier", "zu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Dann, die Akten in der Tasche,", "tokens": ["Dann", ",", "die", "Ak\u00b7ten", "in", "der", "Ta\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Will er wandern zu der Flasche.", "tokens": ["Will", "er", "wan\u00b7dern", "zu", "der", "Fla\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Auf der Treppe wird's ihm bang:", "tokens": ["Auf", "der", "Trep\u00b7pe", "wird's", "ihm", "bang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Einer, \u00fcber kurz und lang,", "tokens": ["Ei\u00b7ner", ",", "\u00fc\u00b7ber", "kurz", "und", "lang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "APPR", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Kann es lesen, und ein Jeder", "tokens": ["Kann", "es", "le\u00b7sen", ",", "und", "ein", "Je\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KON", "ART", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Kennt die Z\u00fcge seiner Feder.", "tokens": ["Kennt", "die", "Z\u00fc\u00b7ge", "sei\u00b7ner", "Fe\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ihn verlangt nach keiner W\u00e4schen,", "tokens": ["Ihn", "ver\u00b7langt", "nach", "kei\u00b7ner", "W\u00e4\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Besser ist's, es auszul\u00f6schen!", "tokens": ["Bes\u00b7ser", "ist's", ",", "es", "aus\u00b7zu\u00b7l\u00f6\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "So hinauf zum Saale wieder;", "tokens": ["So", "hin\u00b7auf", "zum", "Saa\u00b7le", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Doch ihm rieselt's durch die Glieder,", "tokens": ["Doch", "ihm", "rie\u00b7selt's", "durch", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.29": {"text": "Vor der Th\u00fcre macht er Halt,", "tokens": ["Vor", "der", "Th\u00fc\u00b7re", "macht", "er", "Halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Sorglich blickt er durch den Spalt:", "tokens": ["Sorg\u00b7lich", "blickt", "er", "durch", "den", "Spalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Sieh! da tritt zur andern Th\u00fcr \u2013", "tokens": ["Sieh", "!", "da", "tritt", "zur", "an\u00b7dern", "Th\u00fcr", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Weh! \u2013 der Herzog selbst herf\u00fcr: \u2013", "tokens": ["Weh", "!", "\u2013", "der", "Her\u00b7zog", "selbst", "her\u00b7f\u00fcr", ":", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "ART", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Meister Kurz steht auf der Schwelle,", "tokens": ["Meis\u00b7ter", "Kurz", "steht", "auf", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wie am Eingang zu der H\u00f6lle;", "tokens": ["Wie", "am", "Ein\u00b7gang", "zu", "der", "H\u00f6l\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Durch den Spalt sieht er mit Schrecken,", "tokens": ["Durch", "den", "Spalt", "sieht", "er", "mit", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.36": {"text": "Wie der Herr die Schrift entdecken,", "tokens": ["Wie", "der", "Herr", "die", "Schrift", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Alsbald n\u00e4her treten th\u00e4t;", "tokens": ["Als\u00b7bald", "n\u00e4\u00b7her", "tre\u00b7ten", "th\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Lesend davor stille steht.", "tokens": ["Le\u00b7send", "da\u00b7vor", "stil\u00b7le", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Und jetzt wird er auch sich drehen,", "tokens": ["Und", "jetzt", "wird", "er", "auch", "sich", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Wird den argen Schreiber sehen;", "tokens": ["Wird", "den", "ar\u00b7gen", "Schrei\u00b7ber", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Dann fahr' wohl du guter Dienst,", "tokens": ["Dann", "fahr'", "wohl", "du", "gu\u00b7ter", "Dienst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Morgensuppe, Beigewinnst!", "tokens": ["Mor\u00b7gen\u00b7sup\u00b7pe", ",", "Bei\u00b7ge\u00b7winnst", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Soll er fliehen, soll er bleiben?", "tokens": ["Soll", "er", "flie\u00b7hen", ",", "soll", "er", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Doch, was mag der Herzog schreiben,", "tokens": ["Doch", ",", "was", "mag", "der", "Her\u00b7zog", "schrei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Der zur Kreide selber greift,", "tokens": ["Der", "zur", "Krei\u00b7de", "sel\u00b7ber", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "W\u00e4hrend er die Schrift durchl\u00e4uft?", "tokens": ["W\u00e4h\u00b7rend", "er", "die", "Schrift", "durch\u00b7l\u00e4uft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "\u00bbes geht wunderlich hier zu!\u00ab", "tokens": ["\u00bb", "es", "geht", "wun\u00b7der\u00b7lich", "hier", "zu", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Schreibest Kammerschreiber du!", "tokens": ["Schrei\u00b7best", "Kam\u00b7mer\u00b7schrei\u00b7ber", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Und dein Herzog nimmt die Kreide,", "tokens": ["Und", "dein", "Her\u00b7zog", "nimmt", "die", "Krei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Sich zum Scherz, dir nicht zum Leide,", "tokens": ["Sich", "zum", "Scherz", ",", "dir", "nicht", "zum", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$,", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Schreibt er bei in guter Ruh:", "tokens": ["Schreibt", "er", "bei", "in", "gu\u00b7ter", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}