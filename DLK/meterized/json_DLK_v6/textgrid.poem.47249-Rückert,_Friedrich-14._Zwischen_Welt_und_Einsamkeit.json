{"textgrid.poem.47249": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14. Zwischen Welt und Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Welt und Einsamkeit", "tokens": ["Zwi\u00b7schen", "Welt", "und", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist das rechte Leben,", "tokens": ["Ist", "das", "rech\u00b7te", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht zu nah' und nicht zu weit", "tokens": ["Nicht", "zu", "nah'", "und", "nicht", "zu", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKA", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich mich begeben.", "tokens": ["Will", "ich", "mich", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In der Stra\u00dfen lautem Drang", "tokens": ["In", "der", "Stra\u00b7\u00dfen", "lau\u00b7tem", "Drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Find' ich mich zu bl\u00f6de,", "tokens": ["Find'", "ich", "mich", "zu", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber einen Schauer bang", "tokens": ["A\u00b7ber", "ei\u00b7nen", "Schau\u00b7er", "bang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchl' ich in der \u00d6de.", "tokens": ["F\u00fchl'", "ich", "in", "der", "\u00d6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Lieblich ist es, wo ich seh'", "tokens": ["Lieb\u00b7lich", "ist", "es", ",", "wo", "ich", "seh'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferne H\u00fctten rauchen,", "tokens": ["Fer\u00b7ne", "H\u00fct\u00b7ten", "rau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ins Gef\u00fchl der Gottesn\u00e4h'", "tokens": ["Ins", "Ge\u00b7f\u00fchl", "der", "Got\u00b7tes\u00b7n\u00e4h'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweigend mich zu tauchen.", "tokens": ["Schwei\u00b7gend", "mich", "zu", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}