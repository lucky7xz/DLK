{"textgrid.poem.58231": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "269. In inwendigen L\u00e4uterungen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schlacken siehet man, wenn sie vom Gold sich scheiden,", "tokens": ["Die", "Schla\u00b7cken", "sie\u00b7het", "man", ",", "wenn", "sie", "vom", "Gold", "sich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "KOUS", "PPER", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer Tugend haben will, mu\u00df erst Untugend leiden;", "tokens": ["Wer", "Tu\u00b7gend", "ha\u00b7ben", "will", ",", "mu\u00df", "erst", "Un\u00b7tu\u00b7gend", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAINF", "VMFIN", "$,", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist nur der Wille frei, das B\u00f6se dir zur Last,", "tokens": ["Ist", "nur", "der", "Wil\u00b7le", "frei", ",", "das", "B\u00f6\u00b7se", "dir", "zur", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie h\u00e4\u00dflich du dir scheinst, du wahrlich Tugend hast.", "tokens": ["Wie", "h\u00e4\u00df\u00b7lich", "du", "dir", "scheinst", ",", "du", "wahr\u00b7lich", "Tu\u00b7gend", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "VVFIN", "$,", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}