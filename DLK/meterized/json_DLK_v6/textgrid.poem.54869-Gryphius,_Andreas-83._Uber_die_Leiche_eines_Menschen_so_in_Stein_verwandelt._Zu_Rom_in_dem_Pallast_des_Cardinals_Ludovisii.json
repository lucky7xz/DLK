{"textgrid.poem.54869": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "83. Uber die Leiche eines Menschen/ so in Stein verwandelt. Zu Rom in dem Pallast des Cardinals Ludovisii", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Wunderwerck! Das Grab das was man siht verzehrt;", "tokens": ["O", "Wun\u00b7der\u00b7werck", "!", "Das", "Grab", "das", "was", "man", "siht", "ver\u00b7zehrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "ART", "PRELS", "PIS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat deine schwache Leich' in harten Stein verkehrt.", "tokens": ["Hat", "dei\u00b7ne", "schwa\u00b7che", "Leich'", "in", "har\u00b7ten", "Stein", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ists m\u00f6glich da\u00df ein Ort dich unverg\u00e4nglich macht/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "da\u00df", "ein", "Ort", "dich", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In welchem/ was man ehrt und sch\u00e4tzt und w\u00fcndscht/ verschmacht?", "tokens": ["In", "wel\u00b7chem", "/", "was", "man", "ehrt", "und", "sch\u00e4tzt", "und", "w\u00fcnd\u00b7scht", "/", "ver\u00b7schmacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PWAT", "$(", "PWS", "PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Sprecht mehr/ da\u00df sterbend wir in Staub und nichts vergehn/", "tokens": ["Sprecht", "mehr", "/", "da\u00df", "ster\u00b7bend", "wir", "in", "Staub", "und", "nichts", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "KOUS", "ADJD", "PPER", "APPR", "NN", "KON", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du kontest lebend nicht/ todt wirst du stets bestehn.", "tokens": ["Du", "kon\u00b7test", "le\u00b7bend", "nicht", "/", "todt", "wirst", "du", "stets", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PTKNEG", "$(", "ADJD", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}