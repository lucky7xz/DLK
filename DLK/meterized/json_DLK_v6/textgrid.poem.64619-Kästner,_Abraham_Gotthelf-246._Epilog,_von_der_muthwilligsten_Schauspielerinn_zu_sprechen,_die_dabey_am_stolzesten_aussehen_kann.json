{"textgrid.poem.64619": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "246. Epilog, von der muthwilligsten Schauspielerinn zu sprechen, die dabey am stolzesten aussehen kann", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df bey den Spielen, die ihr saht,", "tokens": ["Da\u00df", "bey", "den", "Spie\u00b7len", ",", "die", "ihr", "saht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein J\u00fcngling auf den Schauplatz trat,", "tokens": ["Kein", "J\u00fcng\u00b7ling", "auf", "den", "Schau\u00b7platz", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kann Euch Herren dort belehren,", "tokens": ["Das", "kann", "Euch", "Her\u00b7ren", "dort", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie leicht wir Damen Euch entbehren.", "tokens": ["Wie", "leicht", "wir", "Da\u00b7men", "Euch", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}