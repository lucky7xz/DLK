{"textgrid.poem.28293": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Ehe heist f\u00fcr sich und hinter sich die Ehe,", "tokens": ["Die", "E\u00b7he", "heist", "f\u00fcr", "sich", "und", "hin\u00b7ter", "sich", "die", "E\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PRF", "KON", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil sie niemand trennt, als nur das bittre Wehe.", "tokens": ["Die\u00b7weil", "sie", "nie\u00b7mand", "trennt", ",", "als", "nur", "das", "bitt\u00b7re", "We\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll W bey Ehe seyn, so bringt W hinten her", "tokens": ["Soll", "W", "bey", "E\u00b7he", "seyn", ",", "so", "bringt", "W", "hin\u00b7ten", "her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NE", "APPR", "NN", "VAINF", "$,", "ADV", "VVFIN", "NE", "ADV", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als da\u00df von fornen an weit nicht so viel beschwer;", "tokens": ["Als", "da\u00df", "von", "for\u00b7nen", "an", "weit", "nicht", "so", "viel", "be\u00b7schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "NN", "APPR", "ADJD", "PTKNEG", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Soll W bey Ehe seyn, ists besser, man begr\u00e4bet", "tokens": ["Soll", "W", "bey", "E\u00b7he", "seyn", ",", "ists", "bes\u00b7ser", ",", "man", "be\u00b7gr\u00e4\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "NE", "APPR", "NN", "VAINF", "$,", "VAFIN", "ADJD", "$,", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein from Weib, als da\u00df die, die b\u00f6s ist, immer lebet.", "tokens": ["Ein", "from", "Weib", ",", "als", "da\u00df", "die", ",", "die", "b\u00f6s", "ist", ",", "im\u00b7mer", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOKOM", "KOUS", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}