{"textgrid.poem.33180": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Cap. 4.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr nicht liebt, nichts von Gott wi\u00dft, sonst w\u00fcst't ihr: Da\u00df Gott Liebe ist.", "tokens": ["Die", "ihr", "nicht", "liebt", ",", "nichts", "von", "Gott", "wi\u00dft", ",", "sonst", "w\u00fcst't", "ihr", ":", "Da\u00df", "Gott", "Lie\u00b7be", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$,", "PIS", "APPR", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$.", "KOUS", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "(die liebe ist sein element; vor treu, vor treu wird er erkennt.)", "tokens": ["(", "die", "lie\u00b7be", "ist", "sein", "e\u00b7le\u00b7ment", ";", "vor", "treu", ",", "vor", "treu", "wird", "er", "er\u00b7kennt", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "VAFIN", "PPOSAT", "NN", "$.", "APPR", "ADJD", "$,", "APPR", "ADJD", "VAFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Wir lieben. Woher k\u00f6mt der trieb? Er hatt' uns lange vorher lieb.", "tokens": ["Wir", "lie\u00b7ben", ".", "Wo\u00b7her", "k\u00f6mt", "der", "trieb", "?", "Er", "hatt'", "uns", "lan\u00b7ge", "vor\u00b7her", "lieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VVFIN", "ART", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "(la\u00dft uns in seiner n\u00e4gelmahl erblikken diese gnaden wahl.)", "tokens": ["(", "la\u00dft", "uns", "in", "sei\u00b7ner", "n\u00e4\u00b7gel\u00b7mahl", "er\u00b7blik\u00b7ken", "die\u00b7se", "gna\u00b7den", "wahl", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "PPOSAT", "ADV", "VVINF", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}