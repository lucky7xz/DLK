{"dta.poem.21811": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Frisch bey der Liebe!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe lehrt im finstern gehen/\nsie lehret an der T\u00fchr uns stehen/\nsie lehrt uns geben manche Zeichen", "tokens": ["Die", "Lie\u00b7be", "lehrt", "im", "fins\u00b7tern", "ge\u00b7hen", "/", "sie", "leh\u00b7ret", "an", "der", "T\u00fchr", "uns", "ste\u00b7hen", "/", "sie", "lehrt", "uns", "ge\u00b7ben", "man\u00b7che", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "VVINF", "VVINF", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "ihr s\u00fc\u00df Vergn\u00fcgen zu erreichen.", "tokens": ["ihr", "s\u00fc\u00df", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie lehrt auff Kunst-gemachten Lettern", "tokens": ["Sie", "lehrt", "auff", "Kunst\u00b7ge\u00b7mach\u00b7ten", "Let\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zur Liebsten Fenster ein zu klettern/", "tokens": ["zur", "Liebs\u00b7ten", "Fens\u00b7ter", "ein", "zu", "klet\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die Liebe wei\u00df ein Loch zu zeigen", "tokens": ["die", "Lie\u00b7be", "wei\u00df", "ein", "Loch", "zu", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in ein verriegelt Hau\u00df zu steigen.", "tokens": ["in", "ein", "ver\u00b7rie\u00b7gelt", "Hau\u00df", "zu", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVPP", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie kan uns unvermerket f\u00fchren", "tokens": ["Sie", "kan", "uns", "un\u00b7ver\u00b7mer\u00b7ket", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "durch so viel wolverwahrte T\u00fchren", "tokens": ["durch", "so", "viel", "wol\u00b7ver\u00b7wahr\u00b7te", "T\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "den Tritt kan sie so leise lehren/", "tokens": ["den", "Tritt", "kan", "sie", "so", "lei\u00b7se", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Mutter solt\u2019 auff Kazzen schweeren.", "tokens": ["die", "Mut\u00b7ter", "solt'", "auff", "Kaz\u00b7zen", "schwee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Liebe lehrt den Atem hemmen/", "tokens": ["Die", "Lie\u00b7be", "lehrt", "den", "A\u00b7tem", "hem\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Di\u00df lehrt und sonst vielmehr das Lieben.", "tokens": ["Di\u00df", "lehrt", "und", "sonst", "viel\u00b7mehr", "das", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADV", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch willstu dich im Lieben \u00fcben:", "tokens": ["Doch", "will\u00b7stu", "dich", "im", "Lie\u00b7ben", "\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die Lieb\u2019 erfordert frische Leute.", "tokens": ["die", "Lieb'", "er\u00b7for\u00b7dert", "fri\u00b7sche", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer lieben wil und nichts nicht wagen/", "tokens": ["Wer", "lie\u00b7ben", "wil", "und", "nichts", "nicht", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "KON", "PIS", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wer bey dem Lieben wil verzagen", "tokens": ["wer", "bey", "dem", "Lie\u00b7ben", "wil", "ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der lasse Lieben unterwegen.", "tokens": ["der", "las\u00b7se", "Lie\u00b7ben", "un\u00b7ter\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Brate fleugt uns nicht entgegen.", "tokens": ["Der", "Bra\u00b7te", "fleugt", "uns", "nicht", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}