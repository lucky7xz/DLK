{"textgrid.poem.52115": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da noch ein Deutscher Ernst in Zucht und Kleidung war,", "tokens": ["Da", "noch", "ein", "Deut\u00b7scher", "Ernst", "in", "Zucht", "und", "Klei\u00b7dung", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bezwang das deutsche Volck der fremden helle Schaar:", "tokens": ["Be\u00b7zwang", "das", "deut\u00b7sche", "Volck", "der", "frem\u00b7den", "hel\u00b7le", "Schaar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Itzt, seit der Deutsche f\u00fchrt der frembden V\u00f6lcker Sitten,", "tokens": ["Itzt", ",", "seit", "der", "Deut\u00b7sche", "f\u00fchrt", "der", "fremb\u00b7den", "V\u00f6l\u00b7cker", "Sit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat der frembden Heer die Deutschen \u00fcberstritten.", "tokens": ["So", "hat", "der", "fremb\u00b7den", "Heer", "die", "Deut\u00b7schen", "\u00fc\u00b7bers\u00b7trit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir ha\u00dfen unsre Ehr', ein ieder sucht sein Theil,", "tokens": ["Wir", "ha\u00b7\u00dfen", "uns\u00b7re", "Ehr'", ",", "ein", "ie\u00b7der", "sucht", "sein", "Theil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und tr\u00e4gt den freyen Stand in dienstbarn Waffen feil:", "tokens": ["Und", "tr\u00e4gt", "den", "frey\u00b7en", "Stand", "in", "dienst\u00b7barn", "Waf\u00b7fen", "feil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich rede deutsch, wir sind im a la Mode Orden,", "tokens": ["Ich", "re\u00b7de", "deutsch", ",", "wir", "sind", "im", "a", "la", "Mo\u00b7de", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VAFIN", "APPRART", "FM", "FM", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zwar alte M\u00e4nner vor, itzt alle Memmen worden.", "tokens": ["Zwar", "al\u00b7te", "M\u00e4n\u00b7ner", "vor", ",", "itzt", "al\u00b7le", "Mem\u00b7men", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$,", "ADV", "PIAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}