{"textgrid.poem.41562": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am dreizehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchr' meine Zunge an,", "tokens": ["R\u00fchr'", "mei\u00b7ne", "Zun\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du kannst sie l\u00f6sen;", "tokens": ["Du", "kannst", "sie", "l\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Brich meines Ohres Bann,", "tokens": ["Brich", "mei\u00b7nes", "Oh\u00b7res", "Bann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich mag genesen!", "tokens": ["Ich", "mag", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nein, nicht verloren bin ich, milder Gott.", "tokens": ["Nein", ",", "nicht", "ver\u00b7lo\u00b7ren", "bin", "ich", ",", "mil\u00b7der", "Gott", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKNEG", "VVPP", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ob eingezw\u00e4ngt, ob meines Feindes Spott;", "tokens": ["Ob", "ein\u00b7ge\u00b7zw\u00e4ngt", ",", "ob", "mei\u00b7nes", "Fein\u00b7des", "Spott", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dich ruf' ich, Herr, bek\u00e4mpfe du den B\u00f6sen!", "tokens": ["Dich", "ruf'", "ich", ",", "Herr", ",", "be\u00b7k\u00e4mp\u00b7fe", "du", "den", "B\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gebrochen hat er mir", "tokens": ["Ge\u00b7bro\u00b7chen", "hat", "er", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Nerven F\u00e4den;", "tokens": ["Der", "Ner\u00b7ven", "F\u00e4\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nur durch der Augen T\u00fcr", "tokens": ["Nur", "durch", "der", "Au\u00b7gen", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gehn ein die Reden,", "tokens": ["Gehn", "ein", "die", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wenn fassend frommer Mienen Gotteslust,", "tokens": ["Wenn", "fas\u00b7send", "from\u00b7mer", "Mie\u00b7nen", "Got\u00b7tes\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Herz sich wenden m\u00f6chte in der Brust,", "tokens": ["Das", "Herz", "sich", "wen\u00b7den", "m\u00f6ch\u00b7te", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus bluten m\u00f6chten die verborgnen Sch\u00e4den.", "tokens": ["Aus", "blu\u00b7ten", "m\u00f6ch\u00b7ten", "die", "ver\u00b7borg\u00b7nen", "Sch\u00e4\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So bin ich g\u00e4nzlich doch", "tokens": ["So", "bin", "ich", "g\u00e4nz\u00b7lich", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht aufgegeben,", "tokens": ["Nicht", "auf\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So lang mir irgend noch", "tokens": ["So", "lang", "mir", "ir\u00b7gend", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich regt das Leben,", "tokens": ["Sich", "regt", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und w\u00e4r' es nur, wie in des Irren Stirn", "tokens": ["Und", "w\u00e4r'", "es", "nur", ",", "wie", "in", "des", "Ir\u00b7ren", "Stirn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zuckt leise auf das schlummernde Gehirn:", "tokens": ["Zuckt", "lei\u00b7se", "auf", "das", "schlum\u00b7mern\u00b7de", "Ge\u00b7hirn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es lebt, es atmet, m\u00f6chte sich erheben.", "tokens": ["Es", "lebt", ",", "es", "at\u00b7met", ",", "m\u00f6ch\u00b7te", "sich", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur Worte, Worte sind", "tokens": ["Nur", "Wor\u00b7te", ",", "Wor\u00b7te", "sind"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir nicht Verwandte.", "tokens": ["Mir", "nicht", "Ver\u00b7wand\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie abw\u00e4rts prallt der Wind", "tokens": ["Wie", "ab\u00b7w\u00e4rts", "prallt", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Berges Kante:", "tokens": ["Von", "Ber\u00b7ges", "Kan\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "So prallt, was andre r\u00fchrt und andre schreckt,", "tokens": ["So", "prallt", ",", "was", "and\u00b7re", "r\u00fchrt", "und", "and\u00b7re", "schreckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von jener Rinde, die mein Hirn bedeckt", "tokens": ["Von", "je\u00b7ner", "Rin\u00b7de", ",", "die", "mein", "Hirn", "be\u00b7deckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und die ich einstens Wacht und Mauer nannte.", "tokens": ["Und", "die", "ich", "eins\u00b7tens", "Wacht", "und", "Mau\u00b7er", "nann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht immer ist es gleich;", "tokens": ["Nicht", "im\u00b7mer", "ist", "es", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zuweilen schleichen", "tokens": ["Zu\u00b7wei\u00b7len", "schlei\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sich aus der T\u00f6ne Reich", "tokens": ["Sich", "aus", "der", "T\u00f6\u00b7ne", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gewalt'ge Zeichen,", "tokens": ["Ge\u00b7walt'\u00b7ge", "Zei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie eine Tr\u00e4ne sich zum Herzen dr\u00e4ngt,", "tokens": ["Wie", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "sich", "zum", "Her\u00b7zen", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie Bergeskluft den fernen Donner f\u00e4ngt:", "tokens": ["Wie", "Ber\u00b7ges\u00b7kluft", "den", "fer\u00b7nen", "Don\u00b7ner", "f\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O! dann vor Freude f\u00fchl' ich mich erbleichen.", "tokens": ["O", "!", "dann", "vor", "Freu\u00b7de", "f\u00fchl'", "ich", "mich", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "APPR", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nein, meine Lippe kann", "tokens": ["Nein", ",", "mei\u00b7ne", "Lip\u00b7pe", "kann"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es aus nicht sprechen,", "tokens": ["Es", "aus", "nicht", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie aus der Tiefe dann", "tokens": ["Wie", "aus", "der", "Tie\u00b7fe", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Tr\u00e4nen brechen.", "tokens": ["Die", "Tr\u00e4\u00b7nen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nein, was so fremd sich in die Seele fl\u00f6\u00dft,", "tokens": ["Nein", ",", "was", "so", "fremd", "sich", "in", "die", "See\u00b7le", "fl\u00f6\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das hat noch nicht der Zunge Band gel\u00f6st,", "tokens": ["Das", "hat", "noch", "nicht", "der", "Zun\u00b7ge", "Band", "ge\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Rinnt halbverstanden nur in warmen B\u00e4chen.", "tokens": ["Rinnt", "halb\u00b7ver\u00b7stan\u00b7den", "nur", "in", "war\u00b7men", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O lege, starker Hort,", "tokens": ["O", "le\u00b7ge", ",", "star\u00b7ker", "Hort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die gn\u00e4d'gen H\u00e4nde", "tokens": ["Die", "gn\u00e4d'\u00b7gen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "An meines Ohres Port!", "tokens": ["An", "mei\u00b7nes", "Oh\u00b7res", "Port", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O aufw\u00e4rts wende", "tokens": ["O", "auf\u00b7w\u00e4rts", "wen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Um mich auch deiner Blicke friedreich Flehn,", "tokens": ["Um", "mich", "auch", "dei\u00b7ner", "Bli\u00b7cke", "fried\u00b7reich", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sprich \u00bbEphephata\u00ab, dann ist's geschehn,", "tokens": ["Und", "sprich", "\u00bb", "E\u00b7phe\u00b7pha\u00b7ta", "\u00ab", ",", "dann", "ist's", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "FM.la", "$(", "$,", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich bin erl\u00f6st, der Fluch, er hat ein Ende!", "tokens": ["Ich", "bin", "er\u00b7l\u00f6st", ",", "der", "Fluch", ",", "er", "hat", "ein", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}