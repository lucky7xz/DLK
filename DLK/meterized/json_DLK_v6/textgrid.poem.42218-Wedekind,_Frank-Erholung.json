{"textgrid.poem.42218": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Erholung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie die Erde wackelt,", "tokens": ["Sieh", ",", "wie", "die", "Er\u00b7de", "wa\u00b7ckelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie alles niederst\u00fcrzt,", "tokens": ["Wie", "al\u00b7les", "nie\u00b7ders\u00b7t\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne \u00e4ngstlich fackelt", "tokens": ["Die", "Son\u00b7ne", "\u00e4ngst\u00b7lich", "fa\u00b7ckelt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihre Flammen k\u00fcrzt,", "tokens": ["Und", "ih\u00b7re", "Flam\u00b7men", "k\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ich dich halte Brust an Brust", "tokens": ["Wenn", "ich", "dich", "hal\u00b7te", "Brust", "an", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du mit scharfen Z\u00e4hnen", "tokens": ["Und", "du", "mit", "schar\u00b7fen", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Verbissen dich in wilder Lust", "tokens": ["Ver\u00b7bis\u00b7sen", "dich", "in", "wil\u00b7der", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In meine gl\u00fchnden Venen.", "tokens": ["In", "mei\u00b7ne", "gl\u00fchn\u00b7den", "Ve\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Es wogt dein Leib, es dr\u00f6hnt dein Herz,", "tokens": ["Es", "wogt", "dein", "Leib", ",", "es", "dr\u00f6hnt", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dein Odem z\u00fcngelt h\u00f6llenw\u00e4rts,", "tokens": ["Dein", "O\u00b7dem", "z\u00fcn\u00b7gelt", "h\u00f6l\u00b7len\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und aus der Tiefe steigen", "tokens": ["Und", "aus", "der", "Tie\u00b7fe", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Miasmen freud- und leidenschwer;", "tokens": ["Mi\u00b7as\u00b7men", "freu\u00b7d", "und", "lei\u00b7den\u00b7schwer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dein Kichern tanzt dar\u00fcber her", "tokens": ["Dein", "Ki\u00b7chern", "tanzt", "da\u00b7r\u00fc\u00b7ber", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den fahlen Elfenreigen.", "tokens": ["Den", "fah\u00b7len", "El\u00b7fen\u00b7rei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und zuckt die Flamme \u00fcbers Haus,", "tokens": ["Und", "zuckt", "die", "Flam\u00b7me", "\u00fc\u00b7bers", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie sinkt das All in Nacht und Graus;", "tokens": ["Wie", "sinkt", "das", "All", "in", "Nacht", "und", "Graus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der Himmelslichter Glanz verblich,", "tokens": ["Der", "Him\u00b7mels\u00b7lich\u00b7ter", "Glanz", "ver\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die St\u00fcrme heulen f\u00fcrchterlich,", "tokens": ["Die", "St\u00fcr\u00b7me", "heu\u00b7len", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Es schmettern die Posaunen.", "tokens": ["Es", "schmet\u00b7tern", "die", "Po\u00b7sau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Die Jugend rei\u00dft die Ohren auf,", "tokens": ["Die", "Ju\u00b7gend", "rei\u00dft", "die", "Oh\u00b7ren", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Das Alter hemmt den Tageslauf;", "tokens": ["Das", "Al\u00b7ter", "hemmt", "den", "Ta\u00b7ges\u00b7lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sie schaudern und erstaunen.", "tokens": ["Sie", "schau\u00b7dern", "und", "er\u00b7stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Der Sieger nimmt ein Bad und blickt", "tokens": ["Der", "Sie\u00b7ger", "nimmt", "ein", "Bad", "und", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ver\u00e4chtlich nach dem Pf\u00fchle,", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "nach", "dem", "Pf\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Die Seele frei, der Leib erquickt", "tokens": ["Die", "See\u00b7le", "frei", ",", "der", "Leib", "er\u00b7quickt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Von frischer Morgenk\u00fchle.", "tokens": ["Von", "fri\u00b7scher", "Mor\u00b7gen\u00b7k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Die ganze Welt ist Jubelsang,", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "Ju\u00b7bel\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die Sonne lacht den Wald entlang;", "tokens": ["Die", "Son\u00b7ne", "lacht", "den", "Wald", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dann lacht auch der Ver\u00e4chter", "tokens": ["Dann", "lacht", "auch", "der", "Ver\u00b7\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Sein gellend Hohngel\u00e4chter.", "tokens": ["Sein", "gel\u00b7lend", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}