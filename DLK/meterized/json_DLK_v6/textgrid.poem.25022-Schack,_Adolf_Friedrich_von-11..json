{"textgrid.poem.25022": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In deinem Blick sich ewig sonnen,", "tokens": ["In", "dei\u00b7nem", "Blick", "sich", "e\u00b7wig", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl w\u00e4r' es Himmelsseligkeit;", "tokens": ["Wohl", "w\u00e4r'", "es", "Him\u00b7mels\u00b7se\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein auch mit dem Mindern schon", "tokens": ["Al\u00b7lein", "auch", "mit", "dem", "Min\u00b7dern", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zufrieden sei der Erdensohn!", "tokens": ["Zu\u00b7frie\u00b7den", "sei", "der", "Er\u00b7den\u00b7sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn in der Liebe gro\u00dfen Wonnen", "tokens": ["Denn", "in", "der", "Lie\u00b7be", "gro\u00b7\u00dfen", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird Gl\u00fcck sogar das Trennungsleid!", "tokens": ["Wird", "Gl\u00fcck", "so\u00b7gar", "das", "Tren\u00b7nungs\u00b7leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcck nenn' ich's, wenn im Abschiedsharme", "tokens": ["Gl\u00fcck", "nenn'", "ich's", ",", "wenn", "im", "Ab\u00b7schieds\u00b7har\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stimme fl\u00fcstert: noch einmal!", "tokens": ["Die", "Stim\u00b7me", "fl\u00fcs\u00b7tert", ":", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aneinander wiederum", "tokens": ["Und", "an\u00b7ein\u00b7an\u00b7der", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lippen zittern freudestumm,", "tokens": ["Die", "Lip\u00b7pen", "zit\u00b7tern", "freu\u00b7des\u00b7tumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis langsam sich der Arm dem Arme", "tokens": ["Bis", "lang\u00b7sam", "sich", "der", "Arm", "dem", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Entwindet in des Scheidens Qual;", "tokens": ["Ent\u00b7win\u00b7det", "in", "des", "Schei\u00b7dens", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Gl\u00fcck dann, wenn ein teurer Name,", "tokens": ["Und", "Gl\u00fcck", "dann", ",", "wenn", "ein", "teu\u00b7rer", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Rose gleich, die einsam bl\u00fcht,", "tokens": ["Der", "Ro\u00b7se", "gleich", ",", "die", "ein\u00b7sam", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Duft des Fernseins Oede f\u00fcllt,", "tokens": ["Mit", "Duft", "des", "Fern\u00b7seins", "O\u00b7e\u00b7de", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bis sich das Weh in Seufzern stillt", "tokens": ["Bis", "sich", "das", "Weh", "in", "Seuf\u00b7zern", "stillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hei\u00dfer nach dem Trennungsgrame", "tokens": ["Und", "hei\u00b7\u00dfer", "nach", "dem", "Tren\u00b7nungs\u00b7gra\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Ku\u00df des Wiedersehens gl\u00fcht.", "tokens": ["Der", "Ku\u00df", "des", "Wie\u00b7der\u00b7se\u00b7hens", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}