{"textgrid.poem.46852": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn der Tag sich neiget]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Tag sich neiget,", "tokens": ["Wenn", "der", "Tag", "sich", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das Get\u00f6se schweiget,", "tokens": ["Das", "Ge\u00b7t\u00f6\u00b7se", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das er mitgebracht;", "tokens": ["Das", "er", "mit\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie der Mond dann steiget", "tokens": ["Wie", "der", "Mond", "dann", "stei\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lieblich aus der Nacht;", "tokens": ["Lieb\u00b7lich", "aus", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "So in lauten Stunden", "tokens": ["So", "in", "lau\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn dem Geist entschwunden", "tokens": ["Wenn", "dem", "Geist", "ent\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seiner Sehnsucht Bild;", "tokens": ["Sei\u00b7ner", "Sehn\u00b7sucht", "Bild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hat sich's eingefunden,", "tokens": ["Hat", "sich's", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Traum in Tr\u00e4umen mild.", "tokens": ["Traum", "in", "Tr\u00e4u\u00b7men", "mild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber wie entfalten", "tokens": ["A\u00b7ber", "wie", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wechselnde Gestalten", "tokens": ["Wech\u00b7seln\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Mu\u00df das n\u00e4cht'ge Licht;", "tokens": ["Mu\u00df", "das", "n\u00e4cht'\u00b7ge", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gleichen Schimmer halten", "tokens": ["Glei\u00b7chen", "Schim\u00b7mer", "hal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kann dein Bild auch nicht.", "tokens": ["Kann", "dein", "Bild", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wie der Vollmond schwindet", "tokens": ["Wie", "der", "Voll\u00b7mond", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mehr und mehr erblindet", "tokens": ["Mehr", "und", "mehr", "er\u00b7blin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "KON", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Also mir dein Glanz;", "tokens": ["Al\u00b7so", "mir", "dein", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bald, und wieder findet", "tokens": ["Bald", ",", "und", "wie\u00b7der", "fin\u00b7det"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Er sich voll und ganz.", "tokens": ["Er", "sich", "voll", "und", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}