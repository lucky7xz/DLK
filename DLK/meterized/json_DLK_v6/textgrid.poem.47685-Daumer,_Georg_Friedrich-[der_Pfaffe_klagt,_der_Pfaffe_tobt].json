{"textgrid.poem.47685": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[der Pfaffe klagt, der Pfaffe tobt]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pfaffe klagt, der Pfaffe tobt,", "tokens": ["Der", "Pfaf\u00b7fe", "klagt", ",", "der", "Pfaf\u00b7fe", "tobt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Pfaffe von Sankt Peter.", "tokens": ["Der", "Pfaf\u00b7fe", "von", "Sankt", "Pe\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was that ich ihm, dem Albernen,", "tokens": ["Was", "that", "ich", "ihm", ",", "dem", "Al\u00b7ber\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Da\u00df er so heult und winselt?", "tokens": ["Da\u00df", "er", "so", "heult", "und", "win\u00b7selt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich hatte seine Schnur im Arm", "tokens": ["Ich", "hat\u00b7te", "sei\u00b7ne", "Schnur", "im", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine beiden T\u00f6chter,", "tokens": ["Und", "sei\u00b7ne", "bei\u00b7den", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und einige hundert St\u00fccke Gold", "tokens": ["Und", "ei\u00b7ni\u00b7ge", "hun\u00b7dert", "St\u00fc\u00b7cke", "Gold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "CARD", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Aus seiner Kiste nahm ich.", "tokens": ["Aus", "sei\u00b7ner", "Kis\u00b7te", "nahm", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}