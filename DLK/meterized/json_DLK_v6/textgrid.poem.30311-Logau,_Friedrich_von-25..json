{"textgrid.poem.30311": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann ein indianisch Mann stirbt und wird verbrennt,", "tokens": ["Wann", "ein", "in\u00b7di\u00b7a\u00b7nisch", "Mann", "stirbt", "und", "wird", "ver\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "VVFIN", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dann wird seines Weibes Treu richtig dran erkennt,", "tokens": ["Dann", "wird", "sei\u00b7nes", "Wei\u00b7bes", "Treu", "rich\u00b7tig", "dran", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "PAV", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wann sie springet in die Glut. O, in unsrer Welt", "tokens": ["Wann", "sie", "sprin\u00b7get", "in", "die", "Glut", ".", "O", ",", "in", "uns\u00b7rer", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Springt kein Weib, dieweil sie sich einem andren helt.", "tokens": ["Springt", "kein", "Weib", ",", "die\u00b7weil", "sie", "sich", "ei\u00b7nem", "an\u00b7dren", "helt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "KOUS", "PPER", "PRF", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}