{"dta.poem.13568": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Unverbesserliche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt den Vogel gefangen,", "tokens": ["Ihr", "habt", "den", "Vo\u00b7gel", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der war so frank und frei,", "tokens": ["Der", "war", "so", "frank", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun ist ihm's Fliegen vergangen,", "tokens": ["Nun", "ist", "ihm's", "Flie\u00b7gen", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Sommer ist lange vorbei.", "tokens": ["Der", "Som\u00b7mer", "ist", "lan\u00b7ge", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+---", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Es liegen wohl Federn neben", "tokens": ["Es", "lie\u00b7gen", "wohl", "Fe\u00b7dern", "ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und unter und \u00fcber mir,", "tokens": ["Und", "un\u00b7ter", "und", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KON", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie k\u00f6nnen mich alle nicht heben", "tokens": ["Sie", "k\u00f6n\u00b7nen", "mich", "al\u00b7le", "nicht", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "PTKNEG", "VVINF"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus diesem Meer von Papier.", "tokens": ["Aus", "die\u00b7sem", "Meer", "von", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Papier! wie h\u00f6r' ich Dich schreien,", "tokens": ["Pa\u00b7pier", "!", "wie", "h\u00f6r'", "ich", "Dich", "schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da alles die Federn schwenkt", "tokens": ["Da", "al\u00b7les", "die", "Fe\u00b7dern", "schwenkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In langen emsigen Reihen \u2014", "tokens": ["In", "lan\u00b7gen", "em\u00b7si\u00b7gen", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So wird der Staat nun gelenkt.", "tokens": ["So", "wird", "der", "Staat", "nun", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Mein Fenster am Pulte steht offen,", "tokens": ["Mein", "Fens\u00b7ter", "am", "Pul\u00b7te", "steht", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Sonnenschein schweift \u00fcber's Dach,", "tokens": ["Der", "Son\u00b7nen\u00b7schein", "schweift", "\u00fc\u00b7ber's", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wird so uraltes Hoffen", "tokens": ["Da", "wird", "so", "ur\u00b7al\u00b7tes", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und W\u00fcnschen im Herzen wach.", "tokens": ["Und", "W\u00fcn\u00b7schen", "im", "Her\u00b7zen", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die lustigen Kameraden,", "tokens": ["Die", "lus\u00b7ti\u00b7gen", "Ka\u00b7me\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lerchen, Quellen und Wald,", "tokens": ["Ler\u00b7chen", ",", "Quel\u00b7len", "und", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie rauschen schon wieder und laden:", "tokens": ["Sie", "rau\u00b7schen", "schon", "wie\u00b7der", "und", "la\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Geselle, kommst du nicht bald?", "tokens": ["Ge\u00b7sel\u00b7le", ",", "kommst", "du", "nicht", "bald", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und wie ich durch die Gardinen", "tokens": ["Und", "wie", "ich", "durch", "die", "Gar\u00b7di\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinaussah in keckem Muth,", "tokens": ["Hin\u00b7aus\u00b7sah", "in", "ke\u00b7ckem", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da h\u00f6rt' ich lachen im Gr\u00fcnen,", "tokens": ["Da", "h\u00f6rt'", "ich", "la\u00b7chen", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich kannte das Stimmlein recht gut.", "tokens": ["Ich", "kann\u00b7te", "das", "Stimm\u00b7lein", "recht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Und wie ich hinaustrat zur Schwelle,", "tokens": ["Und", "wie", "ich", "hin\u00b7aust\u00b7rat", "zur", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da bl\u00fchten die B\u00e4ume schon all'", "tokens": ["Da", "bl\u00fch\u00b7ten", "die", "B\u00e4u\u00b7me", "schon", "all'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PIS"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und Liebchen so fr\u00fchlingshelle", "tokens": ["Und", "Lieb\u00b7chen", "so", "fr\u00fch\u00b7lings\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sa\u00df drunter beim Vogelschall.", "tokens": ["Sa\u00df", "drun\u00b7ter", "beim", "Vo\u00b7gel\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und eh wir uns beide besannen,", "tokens": ["Und", "eh", "wir", "uns", "bei\u00b7de", "be\u00b7san\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da wiehert' das Fl\u00fcgelro\u00df \u2014", "tokens": ["Da", "wie\u00b7hert'", "das", "Fl\u00fc\u00b7gel\u00b7ro\u00df"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wir flogen selbander von dannen,", "tokens": ["Wir", "flo\u00b7gen", "sel\u00b7ban\u00b7der", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da\u00df es unten die Schreiber verdro\u00df.", "tokens": ["Da\u00df", "es", "un\u00b7ten", "die", "Schrei\u00b7ber", "ver\u00b7dro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}