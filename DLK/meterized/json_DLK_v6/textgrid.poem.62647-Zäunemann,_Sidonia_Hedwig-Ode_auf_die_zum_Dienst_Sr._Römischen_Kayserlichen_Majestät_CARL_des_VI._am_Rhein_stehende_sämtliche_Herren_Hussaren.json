{"textgrid.poem.62647": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Ode auf die zum Dienst Sr. R\u00f6mischen Kayserlichen Majest\u00e4t CARL des VI. am Rhein stehende s\u00e4mtliche Herren Hussaren", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Du mit deinem Feind gek\u00e4mpfet und gekrieget,", "tokens": ["Wenn", "Du", "mit", "dei\u00b7nem", "Feind", "ge\u00b7k\u00e4mp\u00b7fet", "und", "ge\u00b7krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und scharf gefochten hast. Dein grosser Helden Geist,", "tokens": ["Und", "scharf", "ge\u00b7foch\u00b7ten", "hast", ".", "Dein", "gros\u00b7ser", "Hel\u00b7den", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$.", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sich in ", "tokens": ["Der", "sich", "in"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Der hat dich nicht allein zur Sternen-Burg erhoben;", "tokens": ["Der", "hat", "dich", "nicht", "al\u00b7lein", "zur", "Ster\u00b7nen\u00b7Burg", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wormit ", "tokens": ["Wor\u00b7mit"], "token_info": ["word"], "pos": ["PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die w\u00fcrken, da\u00df du schon, ", "tokens": ["Die", "w\u00fcr\u00b7ken", ",", "da\u00df", "du", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "(doch rechne mir es nicht zur K\u00fcnheit noch zur S\u00fcnde,", "tokens": ["(", "doch", "rech\u00b7ne", "mir", "es", "nicht", "zur", "K\u00fcn\u00b7heit", "noch", "zur", "S\u00fcn\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "APPRART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und mein zwar ungeschickt: doch warheitvolles Blat,", "tokens": ["Und", "mein", "zwar", "un\u00b7ge\u00b7schickt", ":", "doch", "war\u00b7heit\u00b7vol\u00b7les", "Blat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "ADJD", "$.", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Dir widme, und zugleich die Zuschrift an ", "tokens": ["Dir", "wid\u00b7me", ",", "und", "zu\u00b7gleich", "die", "Zu\u00b7schrift", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Ehrfurcht ist hierbey der Hauptzweck meiner Sache.", "tokens": ["Die", "Ehr\u00b7furcht", "ist", "hier\u00b7bey", "der", "Haupt\u00b7zweck", "mei\u00b7ner", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich bitte demuthsvoll, ob gleich die Kunst gebricht,", "tokens": ["Ich", "bit\u00b7te", "de\u00b7muths\u00b7voll", ",", "ob", "gleich", "die", "Kunst", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "verschm\u00e4h mein Schreiben nicht.", "tokens": ["ver\u00b7schm\u00e4h", "mein", "Schrei\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Dichter! wie so tr\u00e4g und kalt,", "tokens": ["Ihr", "Dich\u00b7ter", "!", "wie", "so", "tr\u00e4g", "und", "kalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Helden jetzt ein Lied zu singen?", "tokens": ["Den", "Hel\u00b7den", "jetzt", "ein", "Lied", "zu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kan denn ihr Adel und Gewalt", "tokens": ["Kan", "denn", "ihr", "A\u00b7del", "und", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kiel zu keinen Reimen bringen?", "tokens": ["Den", "Kiel", "zu", "kei\u00b7nen", "Rei\u00b7men", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Soll Trau-Ring, Wiege, Leichenstein", "tokens": ["Soll", "Trau\u00b7Ring", ",", "Wie\u00b7ge", ",", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur blo\u00df der Lieder w\u00fcrdig seyn?", "tokens": ["Nur", "blo\u00df", "der", "Lie\u00b7der", "w\u00fcr\u00b7dig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcrwahr des Grossen CARLS Hussaren,", "tokens": ["F\u00fcr\u00b7wahr", "des", "Gros\u00b7sen", "CaRLS", "Hus\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihrer tapfern Tapferkeit,", "tokens": ["Mit", "ih\u00b7rer", "tap\u00b7fern", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verstatten nicht bey dieser Zeit", "tokens": ["Ver\u00b7stat\u00b7ten", "nicht", "bey", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Dichten l\u00e4nger zu verspahren.", "tokens": ["Mein", "Dich\u00b7ten", "l\u00e4n\u00b7ger", "zu", "ver\u00b7spah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kan wohl der Themis Richter-Schwerd", "tokens": ["Kan", "wohl", "der", "The\u00b7mis", "Rich\u00b7ter\u00b7Schwerd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Purpur nur allein besch\u00fctzen?", "tokens": ["Den", "Pur\u00b7pur", "nur", "al\u00b7lein", "be\u00b7sch\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df nicht Bellonens Opfer-Heerd", "tokens": ["Mu\u00df", "nicht", "Bel\u00b7lo\u00b7nens", "Op\u00b7fer\u00b7He\u00b7erd"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "NN", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Zugleich den Scepter unterst\u00fctzen?", "tokens": ["Zu\u00b7gleich", "den", "Scep\u00b7ter", "un\u00b7ter\u00b7st\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man f\u00e4llt nicht allzeit Schl\u00fcssen bey;", "tokens": ["Man", "f\u00e4llt", "nicht", "all\u00b7zeit", "Schl\u00fcs\u00b7sen", "bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stahl, Eisen, Pulver, Schwerd und Bley,", "tokens": ["Stahl", ",", "Ei\u00b7sen", ",", "Pul\u00b7ver", ",", "Schwerd", "und", "Bley", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Carthaunen, M\u00f6rser und Soldaten,", "tokens": ["Car\u00b7thau\u00b7nen", ",", "M\u00f6r\u00b7ser", "und", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die w\u00fcrken, da\u00df die Majest\u00e4t,", "tokens": ["Die", "w\u00fcr\u00b7ken", ",", "da\u00df", "die", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auf ihrem Throne feste steht,", "tokens": ["Auf", "ih\u00b7rem", "Thro\u00b7ne", "fes\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und k\u00e4mpfen vor den Flor der Staaten.", "tokens": ["Und", "k\u00e4mp\u00b7fen", "vor", "den", "Flor", "der", "Staa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das sch\u00f6ne Lager an dem Rhein", "tokens": ["Das", "sch\u00f6\u00b7ne", "La\u00b7ger", "an", "dem", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kan meinen Worten Beyfall schaffen.", "tokens": ["Kan", "mei\u00b7nen", "Wor\u00b7ten", "Bey\u00b7fall", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier schauet man, was Streiter seyn;", "tokens": ["Hier", "schau\u00b7et", "man", ",", "was", "Strei\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWS", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier findt man Pulver, Schwerd und Waffen.", "tokens": ["Hier", "findt", "man", "Pul\u00b7ver", ",", "Schwerd", "und", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "CaRL, der Germanien regiert,", "tokens": ["CaRL", ",", "der", "Ger\u00b7ma\u00b7ni\u00b7en", "re\u00b7giert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hat hier sein Lager aufgef\u00fchrt,", "tokens": ["Hat", "hier", "sein", "La\u00b7ger", "auf\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und zeigt den Schauplatz seiner Helden.", "tokens": ["Und", "zeigt", "den", "Schau\u00b7platz", "sei\u00b7ner", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hat es durch Klugheit so gef\u00fcgt,", "tokens": ["Hat", "es", "durch", "Klug\u00b7heit", "so", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df Frankreichs Schwerdt sich nicht darf melden.", "tokens": ["Da\u00df", "Fran\u00b7kreichs", "Schwerdt", "sich", "nicht", "darf", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "PRF", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Welch Auge schauet sich wohl satt,", "tokens": ["Welch", "Au\u00b7ge", "schau\u00b7et", "sich", "wohl", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wofern es CARLS Hussaren siehet?", "tokens": ["Wo\u00b7fern", "es", "CaRLS", "Hus\u00b7sa\u00b7ren", "sie\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es wird nicht von Betrachtung matt,", "tokens": ["Es", "wird", "nicht", "von", "Be\u00b7trach\u00b7tung", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warum? weil hier die Gro\u00dfmuth bl\u00fchet.", "tokens": ["Wa\u00b7rum", "?", "weil", "hier", "die", "Gro\u00df\u00b7muth", "bl\u00fc\u00b7het", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Sechsten CARLS Hussaren sind", "tokens": ["Des", "Sechs\u00b7ten", "CaRLS", "Hus\u00b7sa\u00b7ren", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Klug, tapfer, eifrig und geschwind", "tokens": ["Klug", ",", "tap\u00b7fer", ",", "eif\u00b7rig", "und", "ge\u00b7schwind"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Volk der Liljen anzufallen:", "tokens": ["Das", "Volk", "der", "Lil\u00b7jen", "an\u00b7zu\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie lassen nicht mit K\u00e4mpfen nach,", "tokens": ["Sie", "las\u00b7sen", "nicht", "mit", "K\u00e4mp\u00b7fen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bis da\u00df dieselben allgemach", "tokens": ["Bis", "da\u00df", "die\u00b7sel\u00b7ben", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ged\u00e4mpft, besiegt, zur\u00fccke prallen.", "tokens": ["Ge\u00b7d\u00e4mpft", ",", "be\u00b7siegt", ",", "zu\u00b7r\u00fc\u00b7cke", "pral\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die St\u00e4rke und die Tapferkeit?", "tokens": ["Die", "St\u00e4r\u00b7ke", "und", "die", "Tap\u00b7fer\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "wo