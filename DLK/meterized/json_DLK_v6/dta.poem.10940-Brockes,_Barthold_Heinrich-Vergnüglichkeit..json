{"dta.poem.10940": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcglichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hast du dein Brodt, und deinen Trank;", "tokens": ["Hast", "du", "dein", "Brodt", ",", "und", "dei\u00b7nen", "Trank", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du ein Kleid, und bist nicht krank:", "tokens": ["Hast", "du", "ein", "Kleid", ",", "und", "bist", "nicht", "krank", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bist du nicht befugt zu klagen,", "tokens": ["So", "bist", "du", "nicht", "be\u00b7fugt", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00dft dem Sch\u00f6pfer Lob und Dank,", "tokens": ["Und", "mu\u00dft", "dem", "Sch\u00f6p\u00b7fer", "Lob", "und", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit froher Ehrfurcht, billig sagen.", "tokens": ["Mit", "fro\u00b7her", "Ehr\u00b7furcht", ",", "bil\u00b7lig", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist ein wunderlichs Betragen,", "tokens": ["Es", "ist", "ein", "wun\u00b7der\u00b7lichs", "Be\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df wir uns, unser lebenlang,", "tokens": ["Da\u00df", "wir", "uns", ",", "un\u00b7ser", "le\u00b7ben\u00b7lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit selbst gebundnen Ruthen, schlagen,", "tokens": ["Mit", "selbst", "ge\u00b7bund\u00b7nen", "Ru\u00b7then", ",", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und, um ein Blendwerk zu erjagen,", "tokens": ["Und", ",", "um", "ein", "Blend\u00b7werk", "zu", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Uns selber unaufh\u00f6rlich plagen.", "tokens": ["Uns", "sel\u00b7ber", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}