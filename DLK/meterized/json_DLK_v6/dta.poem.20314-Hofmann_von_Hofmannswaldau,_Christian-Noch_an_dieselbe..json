{"dta.poem.20314": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Noch an dieselbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als gestern abend ich bey meinen b\u00fcchern sa\u00df/", "tokens": ["Als", "ge\u00b7stern", "a\u00b7bend", "ich", "bey", "mei\u00b7nen", "b\u00fc\u00b7chern", "sa\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und beym studieren auch fast meiner selbst verga\u00df/", "tokens": ["Und", "beym", "stu\u00b7die\u00b7ren", "auch", "fast", "mei\u00b7ner", "selbst", "ver\u00b7ga\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "VVFIN", "ADV", "ADV", "PPOSAT", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sah\u2019 ich gantz unverhofft die liebe zu mir kommen.", "tokens": ["Sah'", "ich", "gantz", "un\u00b7ver\u00b7hofft", "die", "lie\u00b7be", "zu", "mir", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "VVFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie sprang/ wie sie gewohnt/ sehr freudig in die th\u00fcr/", "tokens": ["Sie", "sprang", "/", "wie", "sie", "ge\u00b7wohnt", "/", "sehr", "freu\u00b7dig", "in", "die", "th\u00fcr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PPER", "VVPP", "$(", "ADV", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und rieff: Auff! schicke dich/ die Phillis folget mir;", "tokens": ["Und", "rieff", ":", "Auff", "!", "schi\u00b7cke", "dich", "/", "die", "Phil\u00b7lis", "fol\u00b7get", "mir", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "APPR", "$.", "VVFIN", "PPER", "$(", "ART", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die heute dich zu sehn ihr g\u00fcnstig vorgenommen.", "tokens": ["Die", "heu\u00b7te", "dich", "zu", "sehn", "ihr", "g\u00fcns\u00b7tig", "vor\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "PTKZU", "VVINF", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich f\u00fchlte/ wie di\u00df wort mir alle sinnen band/", "tokens": ["Ich", "f\u00fchl\u00b7te", "/", "wie", "di\u00df", "wort", "mir", "al\u00b7le", "sin\u00b7nen", "band", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "PDS", "NN", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und warff mit ungest\u00fcmm die b\u00fccher aus der hand/", "tokens": ["Und", "warff", "mit", "un\u00b7ge\u00b7st\u00fcmm", "die", "b\u00fc\u00b7cher", "aus", "der", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "ART", "ADJA", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gleich als die Phillis drauff recht englisch kam gegangen/", "tokens": ["Gleich", "als", "die", "Phil\u00b7lis", "drauff", "recht", "eng\u00b7lisch", "kam", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NE", "PAV", "ADV", "ADJD", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von allen Gratien gefolget und begleit/", "tokens": ["Von", "al\u00b7len", "Gra\u00b7ti\u00b7en", "ge\u00b7fol\u00b7get", "und", "be\u00b7gleit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nicht anders als der mond bey sp\u00e4ter abend-zeit/", "tokens": ["Nicht", "an\u00b7ders", "als", "der", "mond", "bey", "sp\u00e4\u00b7ter", "a\u00b7ben\u00b7dzeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn man um seinen krey\u00df sieht tausend sterne prangen.", "tokens": ["Wenn", "man", "um", "sei\u00b7nen", "krey\u00df", "sieht", "tau\u00b7send", "ster\u00b7ne", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "CARD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Cupido sah mich noch mit b\u00f6sen augen an/", "tokens": ["Cu\u00b7pi\u00b7do", "sah", "mich", "noch", "mit", "b\u00f6\u00b7sen", "au\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil ich vor kurtzem ihm den argen schimpff gethan/", "tokens": ["Weil", "ich", "vor", "kurt\u00b7zem", "ihm", "den", "ar\u00b7gen", "schimpff", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und ihn von mir hinweg zur Phillis fortgetrieben.", "tokens": ["Und", "ihn", "von", "mir", "hin\u00b7weg", "zur", "Phil\u00b7lis", "fort\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "ADV", "APPRART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er muste dazumahl auch wider willen gehn.", "tokens": ["Er", "mus\u00b7te", "da\u00b7zu\u00b7mahl", "auch", "wi\u00b7der", "wil\u00b7len", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Itzt aber blieb er mir zum possen stille stehn/", "tokens": ["Itzt", "a\u00b7ber", "blieb", "er", "mir", "zum", "pos\u00b7sen", "stil\u00b7le", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und fieng zur seiten sich mit schiessen an zu \u00fcben.", "tokens": ["Und", "fi\u00b7eng", "zur", "sei\u00b7ten", "sich", "mit", "schies\u00b7sen", "an", "zu", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "VVFIN", "PRF", "APPR", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Ja/ weil er sonderlich den abend war bedacht/", "tokens": ["Ja", "/", "weil", "er", "son\u00b7der\u00b7lich", "den", "a\u00b7bend", "war", "be\u00b7dacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PPER", "ADJD", "ART", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie zeit und weile w\u00fcrd erfreulich zugebracht/", "tokens": ["Wie", "zeit", "und", "wei\u00b7le", "w\u00fcrd", "er\u00b7freu\u00b7lich", "zu\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Lie\u00df er zu mehrer lust vermischte karten bringen/", "tokens": ["Lie\u00df", "er", "zu", "meh\u00b7rer", "lust", "ver\u00b7mischte", "kar\u00b7ten", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.22": {"text": "Und suchte selber ihm voll eyfer und begier/", "tokens": ["Und", "such\u00b7te", "sel\u00b7ber", "ihm", "voll", "ey\u00b7fer", "und", "be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADJD", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als ein verschmitzter schalck/ ein solches spiel herf\u00fcr/", "tokens": ["Als", "ein", "ver\u00b7schmitz\u00b7ter", "schalck", "/", "ein", "sol\u00b7ches", "spiel", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$(", "ART", "PIS", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da ihm nothwendig must ein ieder stich gelingen.", "tokens": ["Da", "ihm", "noth\u00b7wen\u00b7dig", "must", "ein", "ie\u00b7der", "stich", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "ART", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er spielte hertzen aus/ und als man ihm bekannt/", "tokens": ["Er", "spiel\u00b7te", "hert\u00b7zen", "aus", "/", "und", "als", "man", "ihm", "be\u00b7kannt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$(", "KON", "KOUS", "PIS", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "F\u00fchrt er der Phillis selbst die perlen-weisse hand", "tokens": ["F\u00fchrt", "er", "der", "Phil\u00b7lis", "selbst", "die", "per\u00b7len\u00b7weis\u00b7se", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NE", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So gl\u00fccklich/ da\u00df sie meins vor andern weggestochen.", "tokens": ["So", "gl\u00fcck\u00b7lich", "/", "da\u00df", "sie", "meins", "vor", "an\u00b7dern", "weg\u00b7ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "PPER", "ADV", "APPR", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hier nahm ich allererst die scheitrende gefahr/", "tokens": ["Hier", "nahm", "ich", "al\u00b7le\u00b7rerst", "die", "schei\u00b7tren\u00b7de", "ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Darinn er mich gest\u00fcrtzt/ mit gr\u00f6stem schaden wahr;", "tokens": ["Da\u00b7rinn", "er", "mich", "ge\u00b7st\u00fcrtzt", "/", "mit", "gr\u00f6s\u00b7tem", "scha\u00b7den", "wahr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVPP", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und fand/ vor neulich/ ihn nur allzuwohl gerochen.", "tokens": ["Und", "fand", "/", "vor", "neu\u00b7lich", "/", "ihn", "nur", "all\u00b7zu\u00b7wohl", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "APPR", "ADV", "$(", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Itzt such ich/ Phillis/ dich und deinen holden blick/", "tokens": ["Itzt", "such", "ich", "/", "Phil\u00b7lis", "/", "dich", "und", "dei\u00b7nen", "hol\u00b7den", "blick", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NE", "$(", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ach g\u00f6nne mir doch einst ein angenehmer gl\u00fcck/", "tokens": ["Ach", "g\u00f6n\u00b7ne", "mir", "doch", "einst", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "gl\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und rei\u00df den strick entzwey/ der meine freyheit bindet.", "tokens": ["Und", "rei\u00df", "den", "strick", "ent\u00b7zwey", "/", "der", "mei\u00b7ne", "frey\u00b7heit", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich thue/ sch\u00f6nste/ ja nichts wider die natur/", "tokens": ["Ich", "thue", "/", "sch\u00f6ns\u00b7te", "/", "ja", "nichts", "wi\u00b7der", "die", "na\u00b7tur", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "ADV", "PIS", "APPR", "ART", "NN", "$("], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.35": {"text": "Und folge blo\u00df hierinn der allgemeinen spur/", "tokens": ["Und", "fol\u00b7ge", "blo\u00df", "hie\u00b7rinn", "der", "all\u00b7ge\u00b7mei\u00b7nen", "spur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn wer will nicht dahin/ wo man sein hertze findet.", "tokens": ["Denn", "wer", "will", "nicht", "da\u00b7hin", "/", "wo", "man", "sein", "hert\u00b7ze", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PTKNEG", "PAV", "$(", "PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}