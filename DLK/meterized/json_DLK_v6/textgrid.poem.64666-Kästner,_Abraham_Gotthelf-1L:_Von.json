{"textgrid.poem.64666": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von ", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Hat dir einst unsre Stadt den ersten Dank geweyht", "tokens": ["Hat", "dir", "einst", "uns\u00b7re", "Stadt", "den", "ers\u00b7ten", "Dank", "ge\u00b7weyht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den die Bataver jetzt als ihren Schutzgott ehren", "tokens": ["Den", "die", "Ba\u00b7ta\u00b7ver", "jetzt", "als", "ih\u00b7ren", "Schutz\u00b7gott", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor Raserei, die Freiheit will gew\u00e4hren.", "tokens": ["Vor", "Ra\u00b7se\u00b7rei", ",", "die", "Frei\u00b7heit", "will", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Genie\u00df' in Ruhe nun der edlen Thaten Zoll,", "tokens": ["Ge\u00b7ni\u00b7e\u00df'", "in", "Ru\u00b7he", "nun", "der", "ed\u00b7len", "Tha\u00b7ten", "Zoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Dich kr\u00f6nt nicht Mars allein mit Lorbeern, auch Apoll.", "tokens": ["Dich", "kr\u00f6nt", "nicht", "Mars", "al\u00b7lein", "mit", "Lor\u00b7beern", ",", "auch", "A\u00b7poll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "APPR", "NN", "$,", "ADV", "NE", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}