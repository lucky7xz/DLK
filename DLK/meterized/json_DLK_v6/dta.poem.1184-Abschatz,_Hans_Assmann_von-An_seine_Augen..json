{"dta.poem.1184": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "An seine Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Augen/ deren Licht mit diesem Lichte spielt/", "tokens": ["Ihr", "Au\u00b7gen", "/", "de\u00b7ren", "Licht", "mit", "die\u00b7sem", "Lich\u00b7te", "spielt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das eure Stralen dunckel macht/", "tokens": ["Das", "eu\u00b7re", "Stra\u00b7len", "dun\u00b7ckel", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebt wohl auff eure Sachen acht/", "tokens": ["Gebt", "wohl", "auff", "eu\u00b7re", "Sa\u00b7chen", "acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "CARD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seht/ wie mein Feind bereits auff unser Ungl\u00fcck zielet.", "tokens": ["Seht", "/", "wie", "mein", "Feind", "be\u00b7reits", "auff", "un\u00b7ser", "Un\u00b7gl\u00fcck", "zie\u00b7let", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOKOM", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich kan den Angelstern in mein Gem\u00fctte schl\u00fcssen", "tokens": ["Ich", "kan", "den", "An\u00b7gels\u00b7tern", "in", "mein", "Ge\u00b7m\u00fct\u00b7te", "schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der in gew\u00fcnschten Hafen f\u00fchrt;", "tokens": ["Der", "in", "ge\u00b7w\u00fcnschten", "Ha\u00b7fen", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr aber/ Augen/ ihr verliert", "tokens": ["Ihr", "a\u00b7ber", "/", "Au\u00b7gen", "/", "ihr", "ver\u00b7liert"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$(", "NN", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Licht/ ohn das ihr irrt in tr\u00fcben Finsternissen.", "tokens": ["Das", "Licht", "/", "ohn", "das", "ihr", "irrt", "in", "tr\u00fc\u00b7ben", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Seht/ weil ihr sehen k\u00f6nt/ eh Nacht und Regen kommen/", "tokens": ["Seht", "/", "weil", "ihr", "se\u00b7hen", "k\u00f6nt", "/", "eh", "Nacht", "und", "Re\u00b7gen", "kom\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$(", "KOUS", "NN", "KON", "NN", "VVINF", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sch\u00f6pfft kurtzen Trost vor lange Pein", "tokens": ["Sch\u00f6pfft", "kurt\u00b7zen", "Trost", "vor", "lan\u00b7ge", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von diesen s\u00fcssen Augen ein/", "tokens": ["Von", "die\u00b7sen", "s\u00fcs\u00b7sen", "Au\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh euch Gelegenheit durchs Scheiden wird benommen.", "tokens": ["Eh", "euch", "Ge\u00b7le\u00b7gen\u00b7heit", "durchs", "Schei\u00b7den", "wird", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}