{"textgrid.poem.36168": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Gott dich loben wir!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Gott dich loben wir!", "tokens": ["Herr", "Gott", "dich", "lo\u00b7ben", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Herr Gott dir danken wir!", "tokens": ["Herr", "Gott", "dir", "dan\u00b7ken", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich, Gott Vater in Ewigkeit,", "tokens": ["Dich", ",", "Gott", "Va\u00b7ter", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ehret die Welt weit und breit.", "tokens": ["Eh\u00b7ret", "die", "Welt", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "All Engel und all Himmelsheer,", "tokens": ["All", "En\u00b7gel", "und", "all", "Him\u00b7mels\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "All was da dienet deiner Ehr,", "tokens": ["All", "was", "da", "die\u00b7net", "dei\u00b7ner", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PWS", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch Cherubim und Seraphim", "tokens": ["Auch", "Che\u00b7ru\u00b7bim", "und", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Singen immer mit hoher Stimm:", "tokens": ["Sin\u00b7gen", "im\u00b7mer", "mit", "ho\u00b7her", "Stimm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Heilig ist unser Gott!", "tokens": ["Hei\u00b7lig", "ist", "un\u00b7ser", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Heilig ist unser Gott!", "tokens": ["Hei\u00b7lig", "ist", "un\u00b7ser", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Heilig ist unser Gott! Der Herr, Herr Zebaoth!", "tokens": ["Hei\u00b7lig", "ist", "un\u00b7ser", "Gott", "!", "Der", "Herr", ",", "Herr", "Ze\u00b7bao\u00b7th", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "$,", "NN", "NE", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Herr Zebaoh, du bist gerecht,", "tokens": ["Herr", "Ze\u00b7baoh", ",", "du", "bist", "ge\u00b7recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du hassest, Herr, was schn\u00f6d' und schlecht;", "tokens": ["Du", "has\u00b7sest", ",", "Herr", ",", "was", "schn\u00f6d'", "und", "schlecht", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "PWS", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du strafst, wer treu und Glauben bricht,", "tokens": ["Du", "strafst", ",", "wer", "treu", "und", "Glau\u00b7ben", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hilfst dem, der das Recht verficht.", "tokens": ["Und", "hilfst", "dem", ",", "der", "das", "Recht", "ver\u00b7ficht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein war die Sach', Herr, dein der Krieg;", "tokens": ["Dein", "war", "die", "Sach'", ",", "Herr", ",", "dein", "der", "Krieg", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "NN", "$,", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "D'rum hast du uns beschert den Sieg.", "tokens": ["D'\u00b7rum", "hast", "du", "uns", "be\u00b7schert", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Herr Zebaoth, wer dir vertraut,", "tokens": ["Herr", "Ze\u00b7bao\u00b7th", ",", "wer", "dir", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du starker Held, hat wohl gebaut!", "tokens": ["Du", "star\u00b7ker", "Held", ",", "hat", "wohl", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kyrie Eleis! Die Noth war gro\u00df,", "tokens": ["Ky\u00b7rie", "El\u00b7eis", "!", "Die", "Noth", "war", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Reich der hei\u00dfen H\u00f6lle los!", "tokens": ["Das", "Reich", "der", "hei\u00b7\u00dfen", "H\u00f6l\u00b7le", "los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es st\u00fcrzten rechts, es st\u00fcrzten links", "tokens": ["Es", "st\u00fcrz\u00b7ten", "rechts", ",", "es", "st\u00fcrz\u00b7ten", "links"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die frommen Br\u00fcder Eines Winks.", "tokens": ["Die", "from\u00b7men", "Br\u00fc\u00b7der", "Ei\u00b7nes", "Winks", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schrie'n wir auf mit lautem Schrei;", "tokens": ["Da", "schrie'n", "wir", "auf", "mit", "lau\u00b7tem", "Schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Steh, du im Himmel, steh uns bei!", "tokens": ["Steh", ",", "du", "im", "Him\u00b7mel", ",", "steh", "uns", "bei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "APPRART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da winktest du! Du sprachst das Ja!", "tokens": ["Da", "wink\u00b7test", "du", "!", "Du", "sprachst", "das", "Ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da floh der Feind. Hallelujah!", "tokens": ["Da", "floh", "der", "Feind", ".", "Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ITJ", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Hallelujah, Hallelujah!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", ",", "Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sieg, Sieg, Triumph, Victoria!", "tokens": ["Sieg", ",", "Sieg", ",", "Tri\u00b7umph", ",", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Amalek geraubt den Raub,", "tokens": ["Der", "A\u00b7ma\u00b7lek", "ge\u00b7raubt", "den", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Goliath gestreckt in Staub,", "tokens": ["Den", "Go\u00b7li\u00b7ath", "ge\u00b7streckt", "in", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Holofern das Haupt gek\u00fcrzt,", "tokens": ["Dem", "Ho\u00b7lo\u00b7fern", "das", "Haupt", "ge\u00b7k\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Sanherib vom Stuhl gest\u00fcrzt,", "tokens": ["Den", "San\u00b7he\u00b7rib", "vom", "Stuhl", "ge\u00b7st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er half auch uns! Er war uns nah!", "tokens": ["Er", "half", "auch", "uns", "!", "Er", "war", "uns", "nah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sieg, Sieg! Heil, Heil! Hallelujah!", "tokens": ["Sieg", ",", "Sieg", "!", "Heil", ",", "Heil", "!", "Hal\u00b7le\u00b7lu\u00b7jah", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "NN", "$,", "NN", "$.", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Herr unser Gott, so gro\u00df als gut,", "tokens": ["Herr", "un\u00b7ser", "Gott", ",", "so", "gro\u00df", "als", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Du wei\u00dft, wir dursten nicht nach Blut.", "tokens": ["Du", "wei\u00dft", ",", "wir", "durs\u00b7ten", "nicht", "nach", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gieb unsern Feinden Freundessinn,", "tokens": ["Gieb", "un\u00b7sern", "Fein\u00b7den", "Freun\u00b7des\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ziehn wir heim, so ziehn sie hin,", "tokens": ["So", "ziehn", "wir", "heim", ",", "so", "ziehn", "sie", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Freude lacht und Wonne weint,", "tokens": ["Und", "Freu\u00b7de", "lacht", "und", "Won\u00b7ne", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So weit die Sonne reis't und scheint!", "tokens": ["So", "weit", "die", "Son\u00b7ne", "reis't", "und", "scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "T\u00e4glich, Herr Gott, wir loben dich,", "tokens": ["T\u00e4g\u00b7lich", ",", "Herr", "Gott", ",", "wir", "lo\u00b7ben", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und ehren deinen Namen stetiglich!", "tokens": ["Und", "eh\u00b7ren", "dei\u00b7nen", "Na\u00b7men", "ste\u00b7tig\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Herr, Herr, in deiner starken Hand", "tokens": ["Herr", ",", "Herr", ",", "in", "dei\u00b7ner", "star\u00b7ken", "Hand"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Halt Altar, Heerd und Vaterland!", "tokens": ["Halt", "Al\u00b7tar", ",", "Heerd", "und", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Schirm' unsern K\u00f6nig, deinen Knecht!", "tokens": ["Schirm'", "un\u00b7sern", "K\u00f6\u00b7nig", ",", "dei\u00b7nen", "Knecht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00f6rdr' unter uns Zucht, Licht und Recht!", "tokens": ["F\u00f6rdr'", "un\u00b7ter", "uns", "Zucht", ",", "Licht", "und", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Steur' und wehr' aller Tyrannei,", "tokens": ["Steur'", "und", "wehr'", "al\u00b7ler", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Da\u00df Friede rings auf Erden sey!", "tokens": ["Da\u00df", "Frie\u00b7de", "rings", "auf", "Er\u00b7den", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf dich hoffen wir, lieber Herr,", "tokens": ["Auf", "dich", "hof\u00b7fen", "wir", ",", "lie\u00b7ber", "Herr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In Schanden la\u00df uns nimmermehr!", "tokens": ["In", "Schan\u00b7den", "la\u00df", "uns", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}