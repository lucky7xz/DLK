{"textgrid.poem.43421": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00dft uns unsern Geist versenken", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns unsern Geist versenken", "tokens": ["La\u00dft", "uns", "un\u00b7sern", "Geist", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Wissens tiefes Meer!", "tokens": ["In", "des", "Wis\u00b7sens", "tie\u00b7fes", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns denken, immer denken!", "tokens": ["La\u00dft", "uns", "den\u00b7ken", ",", "im\u00b7mer", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, das ziert den Deutschen sehr.", "tokens": ["Ei", ",", "das", "ziert", "den", "Deut\u00b7schen", "sehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn man uns fragt: wie geht's?", "tokens": ["Und", "wenn", "man", "uns", "fragt", ":", "wie", "geht's", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "VVFIN", "$.", "PWAV", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sagen wir: wir denken stets.", "tokens": ["Sa\u00b7gen", "wir", ":", "wir", "den\u00b7ken", "stets", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles denkt bei uns zu Lande,", "tokens": ["Al\u00b7les", "denkt", "bei", "uns", "zu", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist deutsche Sitt' und Brauch;", "tokens": ["Das", "ist", "deut\u00b7sche", "Sitt'", "und", "Brauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, man denkt in jedem Stande,", "tokens": ["Ja", ",", "man", "denkt", "in", "je\u00b7dem", "Stan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuster, Schneider denken auch.", "tokens": ["Schus\u00b7ter", ",", "Schnei\u00b7der", "den\u00b7ken", "auch", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn sie auch nichts gemacht,", "tokens": ["Und", "wenn", "sie", "auch", "nichts", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Sagen sie: wir ha'n gedacht.", "tokens": ["Sa\u00b7gen", "sie", ":", "wir", "ha'n", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denken mu\u00df der Deutsche immer,", "tokens": ["Den\u00b7ken", "mu\u00df", "der", "Deut\u00b7sche", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo er sitzt und geht und steht,", "tokens": ["Wo", "er", "sitzt", "und", "geht", "und", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er l\u00e4\u00dft das Denken nimmer,", "tokens": ["Und", "er", "l\u00e4\u00dft", "das", "Den\u00b7ken", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn's auch noch so schlecht ihm geht;", "tokens": ["Wenn's", "auch", "noch", "so", "schlecht", "ihm", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein Trost, sein Gl\u00fcck und Heil", "tokens": ["Und", "sein", "Trost", ",", "sein", "Gl\u00fcck", "und", "Heil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist: ich denke mir mein Theil.", "tokens": ["Ist", ":", "ich", "den\u00b7ke", "mir", "mein", "Theil", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdu Gedankenland auf Erden,", "tokens": ["\u00bb", "du", "Ge\u00b7dan\u00b7ken\u00b7land", "auf", "Er\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dein Denken wird zur That,", "tokens": ["Wenn", "dein", "Den\u00b7ken", "wird", "zur", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, was kann aus dir noch werden!", "tokens": ["Ei", ",", "was", "kann", "aus", "dir", "noch", "wer\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "APPR", "PPER", "ADV", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt's nur etwa nicht zu spat,", "tokens": ["Kommt's", "nur", "et\u00b7wa", "nicht", "zu", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKNEG", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df man fragt: was machtet ihr?", "tokens": ["Da\u00df", "man", "fragt", ":", "was", "mach\u00b7tet", "ihr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ihr sagt: stets dachten wir.\u00ab", "tokens": ["Und", "ihr", "sagt", ":", "stets", "dach\u00b7ten", "wir", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}