{"textgrid.poem.46032": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ich nahm das Kreuz und zog durchs Labyrinth]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich nahm das Kreuz und zog durchs Labyrinth,", "tokens": ["Ich", "nahm", "das", "Kreuz", "und", "zog", "durchs", "La\u00b7by\u00b7rinth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das wie ein Garten voll von Dornen war,", "tokens": ["Das", "wie", "ein", "Gar\u00b7ten", "voll", "von", "Dor\u00b7nen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "ADJD", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drin sa\u00df das Mitleid, ein verschleiert Kind,", "tokens": ["Drin", "sa\u00df", "das", "Mit\u00b7leid", ",", "ein", "ver\u00b7schlei\u00b7ert", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und weihte sich als Opfer am Altar,", "tokens": ["Und", "weih\u00b7te", "sich", "als", "Op\u00b7fer", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KOUS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erhob sich in jungfr\u00e4ulicher Gestalt,", "tokens": ["Er\u00b7hob", "sich", "in", "jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und war ein Engel und der Satan bebte,", "tokens": ["Und", "war", "ein", "En\u00b7gel", "und", "der", "Sa\u00b7tan", "beb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn Huld und Treu und Flei\u00df tat ihm Gewalt,", "tokens": ["Denn", "Huld", "und", "Treu", "und", "Flei\u00df", "tat", "ihm", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wo die geweihte Jungfrau helfend schwebte.", "tokens": ["Wo", "die", "ge\u00b7weih\u00b7te", "Jung\u00b7frau", "hel\u00b7fend", "schweb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Kreuzweg baute sie am H\u00f6llenrande,", "tokens": ["Den", "Kreuz\u00b7weg", "bau\u00b7te", "sie", "am", "H\u00f6l\u00b7len\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Trug dornbekr\u00e4nzt ihr Kreuz dem Herren nach,", "tokens": ["Trug", "dorn\u00b7be\u00b7kr\u00e4nzt", "ihr", "Kreuz", "dem", "Her\u00b7ren", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Rose lehrt err\u00f6ten da die Schande,", "tokens": ["Die", "Ro\u00b7se", "lehrt", "er\u00b7r\u00f6\u00b7ten", "da", "die", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Lilie Reinheit teilte S\u00fcnderschmach;", "tokens": ["Der", "Li\u00b7lie", "Rein\u00b7heit", "teil\u00b7te", "S\u00fcn\u00b7der\u00b7schmach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da ward die Sitte Keuschheit, Freundschaft Jesusliebe,", "tokens": ["Da", "ward", "die", "Sit\u00b7te", "Keuschheit", ",", "Freund\u00b7schaft", "Je\u00b7sus\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Die Treue Christentum, die Anmut Himmelswerber,", "tokens": ["Die", "Treu\u00b7e", "Chris\u00b7ten\u00b7tum", ",", "die", "An\u00b7mut", "Him\u00b7mels\u00b7wer\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Glaube Werk, Pflichtweihe ward zum Triebe,", "tokens": ["Der", "Glau\u00b7be", "Werk", ",", "Pflicht\u00b7wei\u00b7he", "ward", "zum", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die Hand der Einfalt pfleget den Verderber,", "tokens": ["Die", "Hand", "der", "Ein\u00b7falt", "pfle\u00b7get", "den", "Ver\u00b7der\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und f\u00fchrt Verzweiflung in die Kinderlehre,", "tokens": ["Und", "f\u00fchrt", "Ver\u00b7zwei\u00b7flung", "in", "die", "Kin\u00b7der\u00b7leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der Unschuld Tr\u00e4nen heilten feile Pest;", "tokens": ["Der", "Un\u00b7schuld", "Tr\u00e4\u00b7nen", "heil\u00b7ten", "fei\u00b7le", "Pest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Um Jesu Kreuz und Schmach war ihre Ehre,", "tokens": ["Um", "Je\u00b7su", "Kreuz", "und", "Schmach", "war", "ih\u00b7re", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "In seiner Seite war der Taube Nest.", "tokens": ["In", "sei\u00b7ner", "Sei\u00b7te", "war", "der", "Tau\u00b7be", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ihr sah ich zu und nicht den Tageshelden,", "tokens": ["Ihr", "sah", "ich", "zu", "und", "nicht", "den", "Ta\u00b7ges\u00b7hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "F\u00fcr deren Glorie ward ich taub und blind und lahm,", "tokens": ["F\u00fcr", "de\u00b7ren", "Glo\u00b7rie", "ward", "ich", "taub", "und", "blind", "und", "lahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und konnte Freundes Drohung L\u00fcgen schelten,", "tokens": ["Und", "konn\u00b7te", "Freun\u00b7des", "Dro\u00b7hung", "L\u00fc\u00b7gen", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Weil besser ich hinweggieng, als ich kam.", "tokens": ["Weil", "bes\u00b7ser", "ich", "hin\u00b7weg\u00b7gieng", ",", "als", "ich", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}