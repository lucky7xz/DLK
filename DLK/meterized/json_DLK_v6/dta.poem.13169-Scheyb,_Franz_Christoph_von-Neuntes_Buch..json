{"dta.poem.13169": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Auge wird geschw\u00e4cht, worein die Sonne strahlt,", "tokens": ["Das", "Au\u00b7ge", "wird", "ge\u00b7schw\u00e4cht", ",", "wo\u00b7rein", "die", "Son\u00b7ne", "strahlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "350So geht es meinem Geist, da er den Anblick mahlt.", "tokens": ["geht", "es", "mei\u00b7nem", "Geist", ",", "da", "er", "den", "An\u00b7blick", "mahlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Was unbegreiffliche, was Anmuths-volle Z\u00fcge?", "tokens": ["Was", "un\u00b7be\u00b7greif\u00b7fli\u00b7che", ",", "was", "An\u00b7muths\u00b7vol\u00b7le", "Z\u00fc\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "$,", "PWS", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Kein Wunder, da\u00df so gar ein Feind vor ihr sich schmiege.", "tokens": ["Kein", "Wun\u00b7der", ",", "da\u00df", "so", "gar", "ein", "Feind", "vor", "ihr", "sich", "schmie\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer das lebhafte Blau des Augen-Paars erblickt,", "tokens": ["Wer", "das", "leb\u00b7haf\u00b7te", "Blau", "des", "Au\u00b7gen\u00b7Paars", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird von der Zauber-Macht desselben Feurs entz\u00fcckt.", "tokens": ["Wird", "von", "der", "Zau\u00b7ber\u00b7Macht", "des\u00b7sel\u00b7ben", "Feurs", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "355Der heitern Stirne Pracht, die Munterkeit der Wangen", "tokens": ["hei\u00b7tern", "Stir\u00b7ne", "Pracht", ",", "die", "Mun\u00b7ter\u00b7keit", "der", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Erh\u00f6ht die Majest\u00e4t, mit der sie pflegt zu prangen.", "tokens": ["Er\u00b7h\u00f6ht", "die", "Ma\u00b7jes\u00b7t\u00e4t", ",", "mit", "der", "sie", "pflegt", "zu", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was ihre Freundlichkeit in eine Seele senckt,", "tokens": ["Was", "ih\u00b7re", "Freund\u00b7lich\u00b7keit", "in", "ei\u00b7ne", "See\u00b7le", "senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wird durch den Ehrfurchts-Trieb des Ernstes eingeschr\u00e4nckt.", "tokens": ["Wird", "durch", "den", "Ehr\u00b7furchts\u00b7Trieb", "des", "Erns\u00b7tes", "ein\u00b7ge\u00b7schr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Freud, Anmuth, Lieb und Huld erscheint in ihren Blicken,", "tokens": ["Freud", ",", "An\u00b7muth", ",", "Lieb", "und", "Huld", "er\u00b7scheint", "in", "ih\u00b7ren", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "360Wie Strahlen, die das Licht vergn\u00fcgter Tage schm\u00fccken.", "tokens": ["Strah\u00b7len", ",", "die", "das", "Licht", "ver\u00b7gn\u00fcg\u00b7ter", "Ta\u00b7ge", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Der Lippen Jugend-Reiz zwingt Herz, Gem\u00fcth und Sinn;", "tokens": ["Der", "Lip\u00b7pen", "Ju\u00b7gend\u00b7Reiz", "zwingt", "Herz", ",", "Ge\u00b7m\u00fcth", "und", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Man nennt sie seines Geists Trieb und Beherrscherinn.", "tokens": ["Man", "nennt", "sie", "sei\u00b7nes", "Geists", "Trieb", "und", "Be\u00b7herr\u00b7sche\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Will jemand sich in Schmerz und Mi\u00dfvergn\u00fcgen laben,", "tokens": ["Will", "je\u00b7mand", "sich", "in", "Schmerz", "und", "Mi\u00df\u00b7ver\u00b7gn\u00fc\u00b7gen", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der kann durch ihren Blick des Trostes Vortheil haben.", "tokens": ["Der", "kann", "durch", "ih\u00b7ren", "Blick", "des", "Tros\u00b7tes", "Vor\u00b7theil", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "365Gang, Regung und Gestallt, ein jedes zeiget an,", "tokens": [",", "Re\u00b7gung", "und", "Ge\u00b7stallt", ",", "ein", "je\u00b7des", "zei\u00b7get", "an", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$,", "NN", "KON", "VVPP", "$,", "ART", "PIAT", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Da\u00df nur ", "tokens": ["Da\u00df", "nur"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Ein herrlicher Begriff von auserw\u00e4hlten Sch\u00e4zen,", "tokens": ["Ein", "herr\u00b7li\u00b7cher", "Be\u00b7griff", "von", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "Sch\u00e4\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "An denen die Natur die Sorge kann erg\u00f6zen.", "tokens": ["An", "de\u00b7nen", "die", "Na\u00b7tur", "die", "Sor\u00b7ge", "kann", "er\u00b7g\u00f6\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201eseht an was ich f\u00fcr euch und euern Trost gespart! \u201e", "tokens": ["\u201e", "seht", "an", "was", "ich", "f\u00fcr", "euch", "und", "eu\u00b7ern", "Trost", "ge\u00b7spart", "!", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "APPR", "PRELS", "PPER", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "370So spr\u00e4che sie gewi\u00df zu dieser Gegenwart:", "tokens": ["spr\u00e4\u00b7che", "sie", "ge\u00b7wi\u00df", "zu", "die\u00b7ser", "Ge\u00b7gen\u00b7wart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.23": {"text": "\u201edie\u00df ist das Meisterst\u00fcck, das euch der Himmel schicket,", "tokens": ["\u201e", "die\u00df", "ist", "das", "Meis\u00b7ter\u00b7st\u00fcck", ",", "das", "euch", "der", "Him\u00b7mel", "schi\u00b7cket", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201ewodurch er Land und Leut und Kron und Thron begl\u00fccket.", "tokens": ["\u201e", "wo\u00b7durch", "er", "Land", "und", "Leut", "und", "Kron", "und", "Thron", "be\u00b7gl\u00fc\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201ebetrachtet die Gestallt, den Hergang, das Gesicht;", "tokens": ["\u201e", "be\u00b7trach\u00b7tet", "die", "Ge\u00b7stallt", ",", "den", "Her\u00b7gang", ",", "das", "Ge\u00b7sicht", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u201escheints nicht, da\u00df jeder Theil besonders rufft und spricht:", "tokens": ["\u201e", "scheints", "nicht", ",", "da\u00df", "je\u00b7der", "Theil", "be\u00b7son\u00b7ders", "rufft", "und", "spricht", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "$,", "KOUS", "PIAT", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "375\u201dSeht der Monarchen Haupt! die K\u00f6niginn der Sch\u00f6nen!", "tokens": ["\"", "Seht", "der", "Mon\u00b7ar\u00b7chen", "Haupt", "!", "die", "K\u00f6\u00b7ni\u00b7ginn", "der", "Sch\u00f6\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "NN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201edie\u00df ist die, welche Prei\u00df, Verdienst und Tugend kr\u00f6nen.", "tokens": ["\u201e", "die\u00df", "ist", "die", ",", "wel\u00b7che", "Prei\u00df", ",", "Ver\u00b7dienst", "und", "Tu\u00b7gend", "kr\u00f6\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "$,", "PWAT", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}