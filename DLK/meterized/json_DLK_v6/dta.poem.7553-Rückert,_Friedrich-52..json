{"dta.poem.7553": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sich vorm Andern sch\u00e4mt, f\u00fchlt sich vor ihm gel\u00e4hmt;", "tokens": ["Wer", "sich", "vorm", "An\u00b7dern", "sch\u00e4mt", ",", "f\u00fchlt", "sich", "vor", "ihm", "ge\u00b7l\u00e4hmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "ADJA", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch sich gekr\u00e4ftigt f\u00fchlt, wer vor sich selbst sich sch\u00e4mt.", "tokens": ["Doch", "sich", "ge\u00b7kr\u00e4f\u00b7tigt", "f\u00fchlt", ",", "wer", "vor", "sich", "selbst", "sich", "sch\u00e4mt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVPP", "VVFIN", "$,", "PWS", "APPR", "PRF", "ADV", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn deinem Bilde leiht ein Maler sch\u00f6nern Schein,", "tokens": ["Wenn", "dei\u00b7nem", "Bil\u00b7de", "leiht", "ein", "Ma\u00b7ler", "sch\u00f6\u00b7nern", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besch\u00e4mt dich das Gef\u00fchl, da\u00df du nicht so kannst seyn.", "tokens": ["Be\u00b7sch\u00e4mt", "dich", "das", "Ge\u00b7f\u00fchl", ",", "da\u00df", "du", "nicht", "so", "kannst", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch wenn ins Sch\u00f6nre dich dein innrer Maler malt,", "tokens": ["Doch", "wenn", "ins", "Sch\u00f6n\u00b7re", "dich", "dein", "inn\u00b7rer", "Ma\u00b7ler", "malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spornt dichs, zu stralen selbst, wie jetzt dein Bild nur stralt.", "tokens": ["Spornt", "dichs", ",", "zu", "stra\u00b7len", "selbst", ",", "wie", "jetzt", "dein", "Bild", "nur", "stralt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PTKZU", "VVFIN", "ADV", "$,", "PWAV", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}