{"dta.poem.12138": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Held, wer durch die Flucht Versuchungen entgeht;", "tokens": ["Kein", "Held", ",", "wer", "durch", "die", "Flucht", "Ver\u00b7su\u00b7chun\u00b7gen", "ent\u00b7geht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Held ist, wer, versucht, der Lockung widersteht.", "tokens": ["Ein", "Held", "ist", ",", "wer", ",", "ver\u00b7sucht", ",", "der", "Lo\u00b7ckung", "wi\u00b7der\u00b7steht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "$,", "VVPP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch ist das ein garsehr gef\u00e4rlich Heldenthum;", "tokens": ["Doch", "ist", "das", "ein", "gar\u00b7sehr", "ge\u00b7f\u00e4r\u00b7lich", "Hel\u00b7den\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "ART", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Such du die Sicherheit, und nicht den Heldenruhm.", "tokens": ["Such", "du", "die", "Si\u00b7cher\u00b7heit", ",", "und", "nicht", "den", "Hel\u00b7den\u00b7ruhm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "$."], "mete