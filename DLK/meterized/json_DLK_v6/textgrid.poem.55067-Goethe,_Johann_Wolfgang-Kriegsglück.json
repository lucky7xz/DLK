{"textgrid.poem.55067": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Kriegsgl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verw\u00fcnschter wei\u00df ich, nichts im Krieg,", "tokens": ["Ver\u00b7w\u00fcnschter", "wei\u00df", "ich", ",", "nichts", "im", "Krieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als nicht blessiert zu sein.", "tokens": ["Als", "nicht", "bles\u00b7siert", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man geht getrost von Sieg zu Sieg", "tokens": ["Man", "geht", "ge\u00b7trost", "von", "Sieg", "zu", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gefahrgewohnt hinein;", "tokens": ["Ge\u00b7fahr\u00b7ge\u00b7wohnt", "hin\u00b7ein", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat abgepackt und aufgepackt", "tokens": ["Hat", "ab\u00b7ge\u00b7packt", "und", "auf\u00b7ge\u00b7packt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und weiter nichts ereilt,", "tokens": ["Und", "wei\u00b7ter", "nichts", "er\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als da\u00df man auf dem Marsch sich plackt,", "tokens": ["Als", "da\u00df", "man", "auf", "dem", "Marsch", "sich", "plackt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Lager langeweilt.", "tokens": ["Im", "La\u00b7ger", "lan\u00b7ge\u00b7weilt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann geht das Kantonieren an,", "tokens": ["Dann", "geht", "das", "Kan\u00b7to\u00b7nie\u00b7ren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Bauer eine Last,", "tokens": ["Dem", "Bau\u00b7er", "ei\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verdrie\u00dflich jedem Edelmann", "tokens": ["Ver\u00b7drie\u00df\u00b7lich", "je\u00b7dem", "E\u00b7del\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und B\u00fcrgern gar verha\u00dft.", "tokens": ["Und", "B\u00fcr\u00b7gern", "gar", "ver\u00b7ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sei h\u00f6flich, man bedient dich schlecht,", "tokens": ["Sei", "h\u00f6f\u00b7lich", ",", "man", "be\u00b7dient", "dich", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PIS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Grobian zur Not;", "tokens": ["Den", "Gro\u00b7bi\u00b7an", "zur", "Not", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und nimmt man selbst am Wirte Recht,", "tokens": ["Und", "nimmt", "man", "selbst", "am", "Wir\u00b7te", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "I\u00dft man Profossenbrot.", "tokens": ["I\u00dft", "man", "Pro\u00b7fos\u00b7sen\u00b7brot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Wenn endlich die Kanone brummt", "tokens": ["Wenn", "end\u00b7lich", "die", "Ka\u00b7no\u00b7ne", "brummt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und knattert 's klein Gewehr,", "tokens": ["Und", "knat\u00b7tert", "'s", "klein", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Trompet und Trab und Trommel summt,", "tokens": ["Trom\u00b7pet", "und", "Trab", "und", "Trom\u00b7mel", "summt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da geht's wohl lustig her;", "tokens": ["Da", "geht's", "wohl", "lus\u00b7tig", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie nun das Gefecht befiehlt,", "tokens": ["Und", "wie", "nun", "das", "Ge\u00b7fecht", "be\u00b7fiehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man weichet, man erneut's,", "tokens": ["Man", "wei\u00b7chet", ",", "man", "er\u00b7neut's", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man retiriert, man avanciert \u2013", "tokens": ["Man", "re\u00b7ti\u00b7riert", ",", "man", "a\u00b7van\u00b7ciert", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und immer ohne Kreuz.", "tokens": ["Und", "im\u00b7mer", "oh\u00b7ne", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun endlich pfeift Musketenblei", "tokens": ["Nun", "end\u00b7lich", "pfeift", "Mus\u00b7ke\u00b7ten\u00b7blei"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trifft, will's Gott, das Bein,", "tokens": ["Und", "trifft", ",", "will's", "Gott", ",", "das", "Bein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VMFIN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nun ist alle Not vorbei,", "tokens": ["Und", "nun", "ist", "al\u00b7le", "Not", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man schleppt uns gleich hinein", "tokens": ["Man", "schleppt", "uns", "gleich", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum St\u00e4dtchen, das der Sieger deckt,", "tokens": ["Zum", "St\u00e4dt\u00b7chen", ",", "das", "der", "Sie\u00b7ger", "deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin man grimmig kam;", "tokens": ["Wo\u00b7hin", "man", "grim\u00b7mig", "kam", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Frauen, die man erst erschreckt,", "tokens": ["Die", "Frau\u00b7en", ",", "die", "man", "erst", "er\u00b7schreckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sind liebensw\u00fcrdig zahm.", "tokens": ["Sind", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "zahm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da tut sich Herz und Keller los,", "tokens": ["Da", "tut", "sich", "Herz", "und", "Kel\u00b7ler", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die K\u00fcche darf nicht ruhn;", "tokens": ["Die", "K\u00fc\u00b7che", "darf", "nicht", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf weicher Betten Flaumenscho\u00df", "tokens": ["Auf", "wei\u00b7cher", "Bet\u00b7ten", "Flau\u00b7men\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann man sich g\u00fctlich tun.", "tokens": ["Kann", "man", "sich", "g\u00fct\u00b7lich", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der kleine Fl\u00fcgelbube hupft,", "tokens": ["Der", "klei\u00b7ne", "Fl\u00fc\u00b7gel\u00b7bu\u00b7be", "hupft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wirtin rastet nie,", "tokens": ["Die", "Wir\u00b7tin", "ras\u00b7tet", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sogar das Hemdchen wird zerzupft,", "tokens": ["So\u00b7gar", "das", "Hemd\u00b7chen", "wird", "zer\u00b7zupft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das nenn ich doch Scharpie!", "tokens": ["Das", "nenn", "ich", "doch", "Schar\u00b7pie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Hat eine sich den Helden nun", "tokens": ["Hat", "ei\u00b7ne", "sich", "den", "Hel\u00b7den", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "PRF", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beinah herangepflegt,", "tokens": ["Bei\u00b7nah", "her\u00b7an\u00b7ge\u00b7pflegt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So kann die Nachbarin nicht ruhn,", "tokens": ["So", "kann", "die", "Nach\u00b7ba\u00b7rin", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihn gesellig hegt.", "tokens": ["Die", "ihn", "ge\u00b7sel\u00b7lig", "hegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Drittes kommt wohl emsiglich,", "tokens": ["Ein", "Drit\u00b7tes", "kommt", "wohl", "em\u00b7sig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Ende fehlet keins,", "tokens": ["Am", "En\u00b7de", "feh\u00b7let", "keins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und in der Mitte sieht er sich", "tokens": ["Und", "in", "der", "Mit\u00b7te", "sieht", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des s\u00e4mtlichen Vereins.", "tokens": ["Des", "s\u00e4mt\u00b7li\u00b7chen", "Ver\u00b7eins", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der K\u00f6nig h\u00f6rt von guter Hand,", "tokens": ["Der", "K\u00f6\u00b7nig", "h\u00f6rt", "von", "gu\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man sei voll Kampfeslust;", "tokens": ["Man", "sei", "voll", "Kamp\u00b7fes\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da k\u00f6mmt behende Kreuz und Band", "tokens": ["Da", "k\u00f6mmt", "be\u00b7hen\u00b7de", "Kreuz", "und", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zieret Rock und Brust.", "tokens": ["Und", "zie\u00b7ret", "Rock", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sagt, ob's f\u00fcr einen Martismann", "tokens": ["Sagt", ",", "ob's", "f\u00fcr", "ei\u00b7nen", "Mar\u00b7tis\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl etwas Be\u00dfres gibt!", "tokens": ["Wohl", "et\u00b7was", "Be\u00df\u00b7res", "gibt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und unter Tr\u00e4nen scheidet man", "tokens": ["Und", "un\u00b7ter", "Tr\u00e4\u00b7nen", "schei\u00b7det", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geehrt so wie geliebt.", "tokens": ["Ge\u00b7ehrt", "so", "wie", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "KOKOM", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}