{"dta.poem.3571": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aufs G\u00e4ssel bin ich gangen,", "tokens": ["Aufs", "G\u00e4s\u00b7sel", "bin", "ich", "gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aufs G\u00e4ssel geh ich noch;", "tokens": ["Aufs", "G\u00e4s\u00b7sel", "geh", "ich", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Scherg will mich fangen,", "tokens": ["Der", "Scherg", "will", "mich", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ey h\u00e4tt er mich doch.", "tokens": ["Ey", "h\u00e4tt", "er", "mich", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie soll er mich denn fangen,", "tokens": ["Wie", "soll", "er", "mich", "denn", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bey Tag geh ich nit;", "tokens": ["Bey", "Tag", "geh", "ich", "nit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Bey der Nacht is stockfinster,", "tokens": ["Bey", "der", "Nacht", "is", "stock\u00b7fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "FM", "FM", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Da sieht er mich nit.", "tokens": ["Da", "sieht", "er", "mich", "nit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}