{"dta.poem.8210": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Zur Taufe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Bedeutsam sind die Namen;", "tokens": ["Be\u00b7deut\u00b7sam", "sind", "die", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und fasse mir dein liebes Bild", "tokens": ["Und", "fas\u00b7se", "mir", "dein", "lie\u00b7bes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun in den rechten Rahmen.", "tokens": ["Nun", "in", "den", "rech\u00b7ten", "Rah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn ob der Nam' den Menschen macht,", "tokens": ["Denn", "ob", "der", "Nam'", "den", "Men\u00b7schen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob sich der Mensch den Namen,", "tokens": ["Ob", "sich", "der", "Mensch", "den", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ist, we\u00dfhalb mir oft, mein Freund,", "tokens": ["Das", "ist", ",", "we\u00df\u00b7halb", "mir", "oft", ",", "mein", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bescheidene Zweifel kamen;", "tokens": ["Be\u00b7schei\u00b7de\u00b7ne", "Zwei\u00b7fel", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Eins aber wei\u00df ich ganz gewi\u00df,", "tokens": ["Eins", "a\u00b7ber", "wei\u00df", "ich", "ganz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bedeutsam sind die Namen!", "tokens": ["Be\u00b7deut\u00b7sam", "sind", "die", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So schickt f\u00fcr M\u00e4dchen Lisbeth sich,", "tokens": ["So", "schickt", "f\u00fcr", "M\u00e4d\u00b7chen", "Lis\u00b7be\u00b7th", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NE", "PRF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Elisabeth f\u00fcr Damen;", "tokens": ["E\u00b7lis\u00b7a\u00b7beth", "f\u00fcr", "Da\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Auch fing sich oft ein Freier schon,", "tokens": ["Auch", "fing", "sich", "oft", "ein", "Frei\u00b7er", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dem Fischlein gleich am Hamen,", "tokens": ["Dem", "Fisc\u00b7hlein", "gleich", "am", "Ha\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "An einem ambraduftigem,", "tokens": ["An", "ei\u00b7nem", "am\u00b7bra\u00b7duf\u00b7ti\u00b7gem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.15": {"text": "Klanghaftem M\u00e4dchennamen.", "tokens": ["Klang\u00b7haf\u00b7tem", "M\u00e4d\u00b7chen\u00b7na\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}