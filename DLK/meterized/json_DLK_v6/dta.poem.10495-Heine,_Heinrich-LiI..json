{"dta.poem.10495": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Andre beten zur Madonne,", "tokens": ["And\u00b7re", "be\u00b7ten", "zur", "Ma\u00b7don\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre auch zu Paul und Peter;", "tokens": ["And\u00b7re", "auch", "zu", "Paul", "und", "Pe\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NE", "KON", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich jedoch, ich will nur beten,", "tokens": ["Ich", "je\u00b7doch", ",", "ich", "will", "nur", "be\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur zu dir, du sch\u00f6ne Sonne.", "tokens": ["Nur", "zu", "dir", ",", "du", "sch\u00f6\u00b7ne", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gieb mir K\u00fcsse, gieb mir Wonne,", "tokens": ["Gieb", "mir", "K\u00fcs\u00b7se", ",", "gieb", "mir", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey mir g\u00fctig, sey mir gn\u00e4dig,", "tokens": ["Sey", "mir", "g\u00fc\u00b7tig", ",", "sey", "mir", "gn\u00e4\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nste Sonne unter den M\u00e4dchen,", "tokens": ["Sch\u00f6ns\u00b7te", "Son\u00b7ne", "un\u00b7ter", "den", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sch\u00f6nstes M\u00e4dchen unter der Sonne!", "tokens": ["Sch\u00f6ns\u00b7tes", "M\u00e4d\u00b7chen", "un\u00b7ter", "der", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}