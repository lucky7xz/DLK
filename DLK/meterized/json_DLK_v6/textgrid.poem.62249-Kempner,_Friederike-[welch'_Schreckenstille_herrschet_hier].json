{"textgrid.poem.62249": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[welch' Schreckenstille herrschet hier]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch' Schreckenstille herrschet hier,", "tokens": ["Welch'", "Schre\u00b7ckens\u00b7til\u00b7le", "herr\u00b7schet", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich allein, ich bin allein!", "tokens": ["Bin", "ich", "al\u00b7lein", ",", "ich", "bin", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entsetzen, ach, ein Grauen schier", "tokens": ["Ent\u00b7set\u00b7zen", ",", "ach", ",", "ein", "Grau\u00b7en", "schier"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erfa\u00dft mich, so allein zu sein. \u2013", "tokens": ["Er\u00b7fa\u00dft", "mich", ",", "so", "al\u00b7lein", "zu", "sein", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADV", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es nahm mir viel, \u2013 fast r\u00e4tselhaft", "tokens": ["Es", "nahm", "mir", "viel", ",", "\u2013", "fast", "r\u00e4t\u00b7sel\u00b7haft"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "$(", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist des Geschickes Grausamkeit,", "tokens": ["Ist", "des", "Ge\u00b7schi\u00b7ckes", "Grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beeil dich, Mut, beeil dich, Kraft,", "tokens": ["Be\u00b7eil", "dich", ",", "Mut", ",", "be\u00b7eil", "dich", ",", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu k\u00fcrzen mir die \u00f6de Zeit.", "tokens": ["Zu", "k\u00fcr\u00b7zen", "mir", "die", "\u00f6\u00b7de", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Menschheit Traum \u2013 die Kunst \u2013 verhindert hin \u2013", "tokens": ["Der", "Menschheit", "Traum", "\u2013", "die", "Kunst", "\u2013", "ver\u00b7hin\u00b7dert", "hin", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "$(", "VVPP", "PTKVZ", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Nichts als des Daseins anspruchsvoller Sinn!", "tokens": ["Nichts", "als", "des", "Da\u00b7seins", "an\u00b7spruchs\u00b7vol\u00b7ler", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch halt, doch nein; das Gr\u00f6\u00dfte ist bei Dir:", "tokens": ["Doch", "halt", ",", "doch", "nein", ";", "das", "Gr\u00f6\u00df\u00b7te", "ist", "bei", "Dir", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "PTKANT", "$.", "ART", "ADJA", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gott ist, ist \u00fcberall, und ist auch hier.", "tokens": ["Gott", "ist", ",", "ist", "\u00fc\u00b7be\u00b7rall", ",", "und", "ist", "auch", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VAFIN", "ADV", "$,", "KON", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}