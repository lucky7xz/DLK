{"textgrid.poem.53277": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[ich bin auf mich in Zorn entbrand]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin auf mich in Zorn entbrand,", "tokens": ["Ich", "bin", "auf", "mich", "in", "Zorn", "ent\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empfinde Gram und Schmertzen,", "tokens": ["Emp\u00b7fin\u00b7de", "Gram", "und", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du, O Welt, du Kummer-Land,", "tokens": ["Da\u00df", "du", ",", "O", "Welt", ",", "du", "Kum\u00b7mer\u00b7Land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir oft noch gehst zu Hertzen,", "tokens": ["Mir", "oft", "noch", "gehst", "zu", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich aus diesem Schlamm vnd Wust", "tokens": ["Da\u00df", "ich", "aus", "die\u00b7sem", "Schlamm", "vnd", "Wust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht allzeit gern wil scheiden,", "tokens": ["Nicht", "all\u00b7zeit", "gern", "wil", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Such' in der Eitelkeiten Lust", "tokens": ["Such'", "in", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch l\u00e4nger mich zu weiden.", "tokens": ["Noch", "l\u00e4n\u00b7ger", "mich", "zu", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja wenn nur das geringste Gut", "tokens": ["Ja", "wenn", "nur", "das", "ge\u00b7rings\u00b7te", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hie were zu befinden,", "tokens": ["Hie", "we\u00b7re", "zu", "be\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darauf ich sicher meinen Muth", "tokens": ["Da\u00b7rauf", "ich", "si\u00b7cher", "mei\u00b7nen", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Hoffnung k\u00f6nte gr\u00fcnden,", "tokens": ["In", "Hoff\u00b7nung", "k\u00f6n\u00b7te", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wolt' auf mich den Sturm vnd Neid", "tokens": ["Ich", "wolt'", "auf", "mich", "den", "Sturm", "vnd", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In etwas fahren lassen,", "tokens": ["In", "et\u00b7was", "fah\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wolt' in mich fressen alles Leid", "tokens": ["Wolt'", "in", "mich", "fres\u00b7sen", "al\u00b7les", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd mit Gedult mich fassen.", "tokens": ["Vnd", "mit", "Ge\u00b7dult", "mich", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nein, nein, ich schawe hin vnd her,", "tokens": ["Nein", ",", "nein", ",", "ich", "scha\u00b7we", "hin", "vnd", "her", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kan aber nichts ersehen,", "tokens": ["Kan", "a\u00b7ber", "nichts", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als nur ein boden-loses Meer", "tokens": ["Als", "nur", "ein", "bo\u00b7den\u00b7lo\u00b7ses", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der S\u00fcnden, so geschehen:", "tokens": ["Der", "S\u00fcn\u00b7den", ",", "so", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da Tugend Schiffbruch leiden mu\u00df,", "tokens": ["Da", "Tu\u00b7gend", "Schiff\u00b7bruch", "lei\u00b7den", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da Bosheit wird erh\u00f6het,", "tokens": ["Da", "Bos\u00b7heit", "wird", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd (ach! den Frommen zum verdru\u00df)", "tokens": ["Vnd", "(", "ach", "!", "den", "From\u00b7men", "zum", "ver\u00b7dru\u00df", ")"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ITJ", "$.", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr vollem Segel gehet.", "tokens": ["F\u00fcr", "vol\u00b7lem", "Se\u00b7gel", "ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ist irgends wo ein guter Mann,", "tokens": ["Ist", "ir\u00b7gends", "wo", "ein", "gu\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den heist der Tod erkalten,", "tokens": ["Den", "heist", "der", "Tod", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit die B\u00f6sen vmb vnd an", "tokens": ["Da\u00b7mit", "die", "B\u00f6\u00b7sen", "vmb", "vnd", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja Platz allein behalten.", "tokens": ["Ja", "Platz", "al\u00b7lein", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Betracht ich die\u00df in meinem Sinn,", "tokens": ["Be\u00b7tracht", "ich", "die\u00df", "in", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So z\u00fcrn' ich aller massen,", "tokens": ["So", "z\u00fcrn'", "ich", "al\u00b7ler", "mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich nicht recht begierig bin", "tokens": ["Da\u00df", "ich", "nicht", "recht", "be\u00b7gie\u00b7rig", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die\u00df Leben zu verlassen.", "tokens": ["Die\u00df", "Le\u00b7ben", "zu", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Christe, halt' ich, solte fast", "tokens": ["Ein", "Chris\u00b7te", ",", "halt'", "ich", ",", "sol\u00b7te", "fast"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vmb Creutz den H\u00f6chsten bitten,", "tokens": ["Vmb", "Creutz", "den", "H\u00f6chs\u00b7ten", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er von dessen Wetters Last", "tokens": ["Da\u00df", "er", "von", "des\u00b7sen", "Wet\u00b7ters", "Last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PRELAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur w\u00fcrde recht bestritten,", "tokens": ["Nur", "w\u00fcr\u00b7de", "recht", "be\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Damit die Eitelkeiten jhm", "tokens": ["Da\u00b7mit", "die", "Ei\u00b7tel\u00b7kei\u00b7ten", "jhm"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Versaltzet m\u00f6chten werden,", "tokens": ["Ver\u00b7salt\u00b7zet", "m\u00f6ch\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er sp\u00fcrt' hie nichts, als Vngest\u00fcm,", "tokens": ["Er", "sp\u00fcrt'", "hie", "nichts", ",", "als", "Vn\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd st\u00fcrb' ein Feind der Erden.", "tokens": ["Vnd", "st\u00fcrb'", "ein", "Feind", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}