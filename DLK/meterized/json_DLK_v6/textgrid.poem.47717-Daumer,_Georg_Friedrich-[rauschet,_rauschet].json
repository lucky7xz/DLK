{"textgrid.poem.47717": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[rauschet, rauschet]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rauschet, rauschet,", "tokens": ["Rau\u00b7schet", ",", "rau\u00b7schet", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ihr M\u00fchlensteine! \u2013", "tokens": ["Ihr", "M\u00fch\u00b7len\u00b7stei\u00b7ne", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es schien mir, ich mahle", "tokens": ["Es", "schien", "mir", ",", "ich", "mah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So ganz alleine;", "tokens": ["So", "ganz", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Es schien mir, ich w\u00e4re", "tokens": ["Es", "schien", "mir", ",", "ich", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So ganz in Ruh';", "tokens": ["So", "ganz", "in", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich mahlt' und sang mir", "tokens": ["Ich", "mahlt'", "und", "sang", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ein Lied dazu.", "tokens": ["Ein", "Lied", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nicht mahlt' ich aber", "tokens": ["Nicht", "mahlt'", "ich", "a\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So ganz alleine,", "tokens": ["So", "ganz", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Denn nahe trat", "tokens": ["Denn", "na\u00b7he", "trat"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mir jener Feine;", "tokens": ["Mir", "je\u00b7ner", "Fei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da hat vor Freude", "tokens": ["Da", "hat", "vor", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mein Blick gestrahlt,", "tokens": ["Mein", "Blick", "ge\u00b7strahlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da hab' ich leise", "tokens": ["Da", "hab'", "ich", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Mein Gl\u00fcck gemahlt.", "tokens": ["Mein", "Gl\u00fcck", "ge\u00b7mahlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}