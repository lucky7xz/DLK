{"textgrid.poem.47831": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Es gibt eine Sorte", "genre": "verse", "period": "N.A.", "pub_year": 1864, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt eine Sorte im deutschen Volk,", "tokens": ["Es", "gibt", "ei\u00b7ne", "Sor\u00b7te", "im", "deut\u00b7schen", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die wollen zum Volk nicht geh\u00f6ren;", "tokens": ["Die", "wol\u00b7len", "zum", "Volk", "nicht", "ge\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie sind auch nur die Tropfen Gift,", "tokens": ["Sie", "sind", "auch", "nur", "die", "Trop\u00b7fen", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die uns im Blute g\u00e4ren.", "tokens": ["Die", "uns", "im", "Blu\u00b7te", "g\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und weil der lebenskr\u00e4ftige Leib", "tokens": ["Und", "weil", "der", "le\u00b7bens\u00b7kr\u00e4f\u00b7ti\u00b7ge", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie auszuscheiden trachtet,", "tokens": ["Sie", "aus\u00b7zu\u00b7schei\u00b7den", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So hassen sie nach Verm\u00f6gen ihn", "tokens": ["So", "has\u00b7sen", "sie", "nach", "Ver\u00b7m\u00f6\u00b7gen", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und h\u00e4tten ihn gern verachtet.", "tokens": ["Und", "h\u00e4t\u00b7ten", "ihn", "gern", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und was f\u00fcr Zeichen am Himmel stehn,", "tokens": ["Und", "was", "f\u00fcr", "Zei\u00b7chen", "am", "Him\u00b7mel", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Licht oder Wetterwolke,", "tokens": ["Licht", "o\u00b7der", "Wet\u00b7ter\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie gehen mit dem P\u00f6bel zwar,", "tokens": ["Sie", "ge\u00b7hen", "mit", "dem", "P\u00f6\u00b7bel", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nimmer mit dem Volke.", "tokens": ["Doch", "nim\u00b7mer", "mit", "dem", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}