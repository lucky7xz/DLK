{"dta.poem.8786": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wie nach dem Stab die Wicke sehn' ich mich;", "tokens": ["Wie", "nach", "dem", "Stab", "die", "Wi\u00b7cke", "sehn'", "ich", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach deines Mundes Duft, nach deines Haars", "tokens": ["Nach", "dei\u00b7nes", "Mun\u00b7des", "Duft", ",", "nach", "dei\u00b7nes", "Haars"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geringel am Genicke sehn' ich mich;", "tokens": ["Ge\u00b7rin\u00b7gel", "am", "Ge\u00b7ni\u00b7cke", "sehn'", "ich", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich sehne mich, da\u00df poche mir dein Herz,", "tokens": ["Ich", "seh\u00b7ne", "mich", ",", "da\u00df", "po\u00b7che", "mir", "dein", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ADJA", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df mich dein Arm umstricke, sehn' ich mich;", "tokens": ["Da\u00df", "mich", "dein", "Arm", "um\u00b7stri\u00b7cke", ",", "sehn'", "ich", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du gehst, o Sch\u00f6nheit, mich so stolz vorbey,", "tokens": ["Du", "gehst", ",", "o", "Sch\u00f6n\u00b7heit", ",", "mich", "so", "stolz", "vor\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nach einem zweyten Blicke sehn' ich mich.", "tokens": ["Nach", "ei\u00b7nem", "zwey\u00b7ten", "Bli\u00b7cke", "sehn'", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}