{"textgrid.poem.42735": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[die Nacht erstarb. Und der Tag erwachte. -]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht erstarb. Und der Tag erwachte. \u2013", "tokens": ["Die", "Nacht", "er\u00b7starb", ".", "Und", "der", "Tag", "er\u00b7wach\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drau\u00dfen unter dem Sternenhimmel", "tokens": ["Drau\u00b7\u00dfen", "un\u00b7ter", "dem", "Ster\u00b7nen\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Stand ein Droschkenpferd, ein Schimmel,", "tokens": ["Stand", "ein", "Droschken\u00b7pferd", ",", "ein", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und lachte.", "tokens": ["Und", "lach\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Der Tag entwich und die Nacht begann.", "tokens": ["Der", "Tag", "ent\u00b7wich", "und", "die", "Nacht", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf steiniger Ebene ruhte das Pferd.", "tokens": ["Auf", "stei\u00b7ni\u00b7ger", "E\u00b7be\u00b7ne", "ruh\u00b7te", "das", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es hatte die Beine gen Himmel gekehrt", "tokens": ["Es", "hat\u00b7te", "die", "Bei\u00b7ne", "gen", "Him\u00b7mel", "ge\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und sann.", "tokens": ["Und", "sann", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Und wieder durchzuckten die Sterne den Himmel. \u2013 \u2013", "tokens": ["Und", "wie\u00b7der", "durch\u00b7zuck\u00b7ten", "die", "Ster\u00b7ne", "den", "Him\u00b7mel", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das rechte Auge des Pferdes tr\u00e4nte. \u2013 \u2013", "tokens": ["Das", "rech\u00b7te", "Au\u00b7ge", "des", "Pfer\u00b7des", "tr\u00e4n\u00b7te", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Mann auf dem Kutschersitze g\u00e4hnte", "tokens": ["Der", "Mann", "auf", "dem", "Kut\u00b7scher\u00b7sit\u00b7ze", "g\u00e4hn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und trank einen K\u00fcmmel.", "tokens": ["Und", "trank", "ei\u00b7nen", "K\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}