{"textgrid.poem.53386": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch der Tag ist geschlossen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch der Tag ist geschlossen,", "tokens": ["Auch", "der", "Tag", "ist", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die dunkle Nacht ist hier.", "tokens": ["Die", "dunk\u00b7le", "Nacht", "ist", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Hertz, sey unverdrossen", "tokens": ["Mein", "Hertz", ",", "sey", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprich: ich danke dir,", "tokens": ["Und", "sprich", ":", "ich", "dan\u00b7ke", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df du dein' Hut, o Gott,", "tokens": ["Da\u00df", "du", "dein'", "Hut", ",", "o", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hast ob mir lassen walten", "tokens": ["Hast", "ob", "mir", "las\u00b7sen", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOUS", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und von mir abgehalten", "tokens": ["Und", "von", "mir", "ab\u00b7ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Bo\u00dfheit finstre Rott,", "tokens": ["Der", "Bo\u00df\u00b7heit", "finst\u00b7re", "Rott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Indem der H\u00f6llen Rachen", "tokens": ["In\u00b7dem", "der", "H\u00f6l\u00b7len", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn Abla\u00df offen steht", "tokens": ["Ohn", "Ab\u00b7la\u00df", "of\u00b7fen", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tausend Stricke wachen,", "tokens": ["Und", "tau\u00b7send", "Stri\u00b7cke", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch die man irre geht.", "tokens": ["Durch", "die", "man", "ir\u00b7re", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer kennt, die \u00fcberall", "tokens": ["Wer", "kennt", ",", "die", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur heut sind aufgerieben?", "tokens": ["Nur", "heut", "sind", "auf\u00b7ge\u00b7rie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die sind durch Mord geblieben", "tokens": ["Die", "sind", "durch", "Mord", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und die durch andern Fall!", "tokens": ["Und", "die", "durch", "an\u00b7dern", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und der h\u00e4tt \u00fcber Hoffen", "tokens": ["Und", "der", "h\u00e4tt", "\u00fc\u00b7ber", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar leicht auch mich entwandt;", "tokens": ["Gar", "leicht", "auch", "mich", "ent\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich nicht bin getroffen,", "tokens": ["Da\u00df", "ich", "nicht", "bin", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Thut deine GnadenHand", "tokens": ["Thut", "dei\u00b7ne", "Gna\u00b7den", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und gro\u00dfe Treu allein,", "tokens": ["Und", "gro\u00b7\u00dfe", "Treu", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die bey den schwehren S\u00fcnden,", "tokens": ["Die", "bey", "den", "schweh\u00b7ren", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch die wir dich entz\u00fcnden,", "tokens": ["Durch", "die", "wir", "dich", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns noch l\u00e4\u00dft \u00fcbrig seyn,", "tokens": ["Uns", "noch", "l\u00e4\u00dft", "\u00fcb\u00b7rig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Uns v\u00e4terlich besch\u00fctzet", "tokens": ["Uns", "v\u00e4\u00b7ter\u00b7lich", "be\u00b7sch\u00fct\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Mauren fest umschr\u00e4nkt", "tokens": ["Mit", "Mau\u00b7ren", "fest", "um\u00b7schr\u00e4nkt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alles was uns n\u00fctzet", "tokens": ["Und", "al\u00b7les", "was", "uns", "n\u00fct\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar \u00fcberfl\u00fcssig schenkt.", "tokens": ["Gar", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "schenkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Th\u00e4t einig die es nicht,", "tokens": ["Th\u00e4t", "ei\u00b7nig", "die", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRELS", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es w\u00e4r um mich geschehen,", "tokens": ["Es", "w\u00e4r", "um", "mich", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich w\u00fcrde nimmer sehen", "tokens": ["Ich", "w\u00fcr\u00b7de", "nim\u00b7mer", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das sch\u00f6ne Sonnen Licht.", "tokens": ["Das", "sch\u00f6\u00b7ne", "Son\u00b7nen", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Um solcher G\u00fcte willen", "tokens": ["Um", "sol\u00b7cher", "G\u00fc\u00b7te", "wil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trag, Herr, mit mir Geduld,", "tokens": ["Trag", ",", "Herr", ",", "mit", "mir", "Ge\u00b7duld", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df deinen Sohn dich stillen", "tokens": ["La\u00df", "dei\u00b7nen", "Sohn", "dich", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von wegen meiner Schuld,", "tokens": ["Von", "we\u00b7gen", "mei\u00b7ner", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der hat mich lo\u00df geb\u00fcrgt,", "tokens": ["Der", "hat", "mich", "lo\u00df", "ge\u00b7b\u00fcrgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der alles abgetragen,", "tokens": ["Der", "al\u00b7les", "ab\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als er ward wund geschlagen", "tokens": ["Als", "er", "ward", "wund", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und an dem Creutz gew\u00fcrgt.", "tokens": ["Und", "an", "dem", "Creutz", "ge\u00b7w\u00fcrgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und weil ich jetzt soll schlafen,", "tokens": ["Und", "weil", "ich", "jetzt", "soll", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn also hastu mich", "tokens": ["Denn", "al\u00b7so", "has\u00b7tu", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sammt andren, Gott, geschaffen,", "tokens": ["Sammt", "an\u00b7dren", ",", "Gott", ",", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So bitt ich ferner dich,", "tokens": ["So", "bitt", "ich", "fer\u00b7ner", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df deiner Engel Schaar", "tokens": ["La\u00df", "dei\u00b7ner", "En\u00b7gel", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mich diese Nacht beh\u00fcten", "tokens": ["Mich", "die\u00b7se", "Nacht", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr Satans List und W\u00fctten,", "tokens": ["F\u00fcr", "Sa\u00b7tans", "List", "und", "W\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr Schrecken und Gefahr.", "tokens": ["F\u00fcr", "Schre\u00b7cken", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Halt Noth und Todt im Z\u00fcgel", "tokens": ["Halt", "Noth", "und", "Todt", "im", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und deck mich flei\u00dfig zu", "tokens": ["Und", "deck", "mich", "flei\u00b7\u00dfig", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PPER", "ADJD", "PTKZU"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch deiner Gnade Fl\u00fcgel,", "tokens": ["Durch", "dei\u00b7ner", "Gna\u00b7de", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Damit ich sicher ruh.", "tokens": ["Da\u00b7mit", "ich", "si\u00b7cher", "ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sollte diese Nacht", "tokens": ["Und", "soll\u00b7te", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Todt mich \u00fcberfallen,", "tokens": ["Der", "Todt", "mich", "\u00fc\u00b7berf\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So nimm, o Gott, f\u00fcr allen", "tokens": ["So", "nimm", ",", "o", "Gott", ",", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "$,", "FM", "NN", "$,", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein arme Seel in acht.", "tokens": ["Mein", "ar\u00b7me", "Seel", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "La\u00df sie in deine H\u00e4nde", "tokens": ["La\u00df", "sie", "in", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir jetzt befohlen seyn,", "tokens": ["Dir", "jetzt", "be\u00b7foh\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gieb mir ein seelig Ende", "tokens": ["Gieb", "mir", "ein", "see\u00b7lig", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimm mich Himmel-ein.", "tokens": ["Und", "nimm", "mich", "Him\u00b7mel\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Daf\u00fcr soll meine Zeit", "tokens": ["Da\u00b7f\u00fcr", "soll", "mei\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dir stets ein Lob darbringen,", "tokens": ["Dir", "stets", "ein", "Lob", "dar\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bi\u00df ich dir dort kan singen", "tokens": ["Bi\u00df", "ich", "dir", "dort", "kan", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In alle Ewigkeit!", "tokens": ["In", "al\u00b7le", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}