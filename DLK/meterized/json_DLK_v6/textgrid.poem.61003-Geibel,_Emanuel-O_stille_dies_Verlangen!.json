{"textgrid.poem.61003": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "O stille dies Verlangen!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O stille dies Verlangen,", "tokens": ["O", "stil\u00b7le", "dies", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stille die s\u00fc\u00dfe Pein!", "tokens": ["Stil\u00b7le", "die", "s\u00fc\u00b7\u00dfe", "Pein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu seligem Umfangen", "tokens": ["Zu", "se\u00b7li\u00b7gem", "Um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df den Geliebten ein!", "tokens": ["La\u00df", "den", "Ge\u00b7lieb\u00b7ten", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schon liegt die Welt im Traume,", "tokens": ["Schon", "liegt", "die", "Welt", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bl\u00fchet die duft'ge Nacht;", "tokens": ["Bl\u00fc\u00b7het", "die", "duft'\u00b7ge", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Der Mond im blauen Raume", "tokens": ["Der", "Mond", "im", "blau\u00b7en", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "H\u00e4lt f\u00fcr die Liebe Wacht.", "tokens": ["H\u00e4lt", "f\u00fcr", "die", "Lie\u00b7be", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wo zwei sich treu umfangen,", "tokens": ["Wo", "zwei", "sich", "treu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da gibt er den holdesten Schein.", "tokens": ["Da", "gibt", "er", "den", "hol\u00b7des\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "O stille dies Verlangen,", "tokens": ["O", "stil\u00b7le", "dies", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "La\u00df den Geliebten ein!", "tokens": ["La\u00df", "den", "Ge\u00b7lieb\u00b7ten", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du bist das s\u00fc\u00dfe Feuer,", "tokens": ["Du", "bist", "das", "s\u00fc\u00b7\u00dfe", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mir am Herzen zehrt;", "tokens": ["Das", "mir", "am", "Her\u00b7zen", "zehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00fcfte, l\u00fcfte den Schleier,", "tokens": ["L\u00fcf\u00b7te", ",", "l\u00fcf\u00b7te", "den", "Schlei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der nun so lang mir wehrt!", "tokens": ["Der", "nun", "so", "lang", "mir", "wehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df mich vom rosigen Munde", "tokens": ["La\u00df", "mich", "vom", "ro\u00b7si\u00b7gen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "K\u00fcssen die Seele dir,", "tokens": ["K\u00fcs\u00b7sen", "die", "See\u00b7le", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Aus meines Busens Grunde", "tokens": ["Aus", "mei\u00b7nes", "Bu\u00b7sens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nimm meine Seele daf\u00fcr -", "tokens": ["Nimm", "mei\u00b7ne", "See\u00b7le", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "PAV", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "O stille dies Verlangen,", "tokens": ["O", "stil\u00b7le", "dies", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Stille die s\u00fc\u00dfe Pein,", "tokens": ["Stil\u00b7le", "die", "s\u00fc\u00b7\u00dfe", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Zu seligem Umfangen", "tokens": ["Zu", "se\u00b7li\u00b7gem", "Um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "La\u00df den Geliebten ein!", "tokens": ["La\u00df", "den", "Ge\u00b7lieb\u00b7ten", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Die goldnen Sterne gr\u00fc\u00dfen", "tokens": ["Die", "gold\u00b7nen", "Ster\u00b7ne", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So klar vom Himmelszelt,", "tokens": ["So", "klar", "vom", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es geht ein Wehn und K\u00fcssen", "tokens": ["Es", "geht", "ein", "Wehn", "und", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heimlich durch alle Welt,", "tokens": ["Heim\u00b7lich", "durch", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die Blumen selber neigen", "tokens": ["Die", "Blu\u00b7men", "sel\u00b7ber", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sehns\u00fcchtig einander sich zu,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "ein\u00b7an\u00b7der", "sich", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Die Nachtigall singt in den Zweigen -", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Tr\u00e4ume, liebe auch du!", "tokens": ["Tr\u00e4u\u00b7me", ",", "lie\u00b7be", "auch", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "O stille dies Verlangen,", "tokens": ["O", "stil\u00b7le", "dies", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "La\u00df den Geliebten ein!", "tokens": ["La\u00df", "den", "Ge\u00b7lieb\u00b7ten", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Von Lieb' und Traum umfangen", "tokens": ["Von", "Lieb'", "und", "Traum", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wollen wir selig sein.", "tokens": ["Wol\u00b7len", "wir", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}