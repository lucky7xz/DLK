{"textgrid.poem.44578": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[f\u00fcr Fr\u00e4ulein Therese Uetsch]", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarz und gelb, wie ich, du selber,", "tokens": ["Schwarz", "und", "gelb", ",", "wie", "ich", ",", "du", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "PWAV", "PPER", "$,", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fanden wir uns auf der Flucht;", "tokens": ["Fan\u00b7den", "wir", "uns", "auf", "der", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schw\u00e4rzer ich, du etwas gelber,", "tokens": ["Schw\u00e4r\u00b7zer", "ich", ",", "du", "et\u00b7was", "gel\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast du geschimpft und ich geflucht.", "tokens": ["Hast", "du", "ge\u00b7schimpft", "und", "ich", "ge\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und so, dem Feind zu gro\u00dfem Schaden,", "tokens": ["Und", "so", ",", "dem", "Feind", "zu", "gro\u00b7\u00dfem", "Scha\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bek\u00e4mpften wir ihn bis aufs Blut;", "tokens": ["Be\u00b7k\u00e4mpf\u00b7ten", "wir", "ihn", "bis", "aufs", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er war in Wien, und wir in Baden:", "tokens": ["Er", "war", "in", "Wi\u00b7en", ",", "und", "wir", "in", "Ba\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$,", "KON", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Abstand st\u00e4rkte unsern Mut.", "tokens": ["Der", "Ab\u00b7stand", "st\u00e4rk\u00b7te", "un\u00b7sern", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nun, besiegt des Krieges Wehe,", "tokens": ["Doch", "nun", ",", "be\u00b7siegt", "des", "Krie\u00b7ges", "We\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir von neuem Harm gesucht:", "tokens": ["Sind", "wir", "von", "neu\u00b7em", "Harm", "ge\u00b7sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Waffenb\u00fcndnis unsrer N\u00e4he", "tokens": ["Das", "Waf\u00b7fen\u00b7b\u00fcnd\u00b7nis", "uns\u00b7rer", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Begibt sich selber in die Flucht.", "tokens": ["Be\u00b7gibt", "sich", "sel\u00b7ber", "in", "die", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du schw\u00f6rst zu einer andern Fahne,", "tokens": ["Du", "schw\u00f6rst", "zu", "ei\u00b7ner", "an\u00b7dern", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die, hei\u00dfts, ein Rosenband umflicht;", "tokens": ["Die", ",", "hei\u00dfts", ",", "ein", "Ro\u00b7sen\u00b7band", "um\u00b7flicht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein das Neue, wie ich ahne,", "tokens": ["Al\u00b7lein", "das", "Neu\u00b7e", ",", "wie", "ich", "ah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verdr\u00e4ngt bei dir das Alte nicht.", "tokens": ["Ver\u00b7dr\u00e4ngt", "bei", "dir", "das", "Al\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gelb sind ja Bl\u00e4tter, welche starben,", "tokens": ["Gelb", "sind", "ja", "Bl\u00e4t\u00b7ter", ",", "wel\u00b7che", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schwarz ist der Tod, der f\u00fcrchterlich;", "tokens": ["Schwarz", "ist", "der", "Tod", ",", "der", "f\u00fcrch\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimm nur getrost der Liebe Farben,", "tokens": ["Nimm", "nur", "ge\u00b7trost", "der", "Lie\u00b7be", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch wei\u00df und rot ist kaiserlich.", "tokens": ["Auch", "wei\u00df", "und", "rot", "ist", "kai\u00b7ser\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}