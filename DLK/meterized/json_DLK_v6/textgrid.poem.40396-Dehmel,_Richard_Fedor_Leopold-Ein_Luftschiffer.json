{"textgrid.poem.40396": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Luftschiffer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Luftschiffer werden,", "tokens": ["Ich", "kann", "ein", "Luft\u00b7schif\u00b7fer", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "immer h\u00f6her schl\u00e4gt mein Herz;", "tokens": ["im\u00b7mer", "h\u00f6\u00b7her", "schl\u00e4gt", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da fliehn die Fl\u00fcsse unter mir", "tokens": ["da", "fliehn", "die", "Fl\u00fcs\u00b7se", "un\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie d\u00fcnne Adern Erz,", "tokens": ["wie", "d\u00fcn\u00b7ne", "A\u00b7dern", "Erz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "meine Gondel steigt und steigt.", "tokens": ["mei\u00b7ne", "Gon\u00b7del", "steigt", "und", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Luft wird immer reiner;", "tokens": ["Die", "Luft", "wird", "im\u00b7mer", "rei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das wirre Erdgew\u00fchl", "tokens": ["das", "wir\u00b7re", "Erd\u00b7ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wird alles klein und kleiner,", "tokens": ["wird", "al\u00b7les", "klein", "und", "klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wird alles wie ein Spiel.", "tokens": ["wird", "al\u00b7les", "wie", "ein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich gleite dr\u00fcber hin.", "tokens": ["Ich", "glei\u00b7te", "dr\u00fc\u00b7ber", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hin, wo die Wolken schweigen;", "tokens": ["Hin", ",", "wo", "die", "Wol\u00b7ken", "schwei\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "kaum noch ein Berghaupt blinkt.", "tokens": ["kaum", "noch", "ein", "Berg\u00b7haupt", "blinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fchle mich nicht mehr steigen,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "nicht", "mehr", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "nur die Erde sinkt und sinkt;", "tokens": ["nur", "die", "Er\u00b7de", "sinkt", "und", "sinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "mir tr\u00e4umt ein Schaukellied.", "tokens": ["mir", "tr\u00e4umt", "ein", "Schau\u00b7kel\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich schwebe nur und schwebe,", "tokens": ["Ich", "schwe\u00b7be", "nur", "und", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "in die blaue Welt hinein.", "tokens": ["in", "die", "blau\u00b7e", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer wei\u00df wohin \u2013 ade, ade \u2013", "tokens": ["Wer", "wei\u00df", "wo\u00b7hin", "\u2013", "a\u00b7de", ",", "a\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PWAV", "$(", "ADJD", "$,", "FM.la", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "der Himmel wiegt mich ein:", "tokens": ["der", "Him\u00b7mel", "wiegt", "mich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "fahr wohl, du kleiner Held.", "tokens": ["fahr", "wohl", ",", "du", "klei\u00b7ner", "Held", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}