{"dta.poem.9759": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines w\u00fcrmchens/ das zwischen einer sch\u00f6-  \n nen damen augen umkommen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einer sonnen hab ichs leben erst bekommen/", "tokens": ["Von", "ei\u00b7ner", "son\u00b7nen", "hab", "ichs", "le\u00b7ben", "erst", "be\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VAFIN", "PIS", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwey sonnen haben drauff mir wieder-es-genommen.", "tokens": ["Zwey", "son\u00b7nen", "ha\u00b7ben", "drauff", "mir", "wie\u00b7der\u00b7es\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PAV", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie solt ich \u00e4rmster wurm ertragen doppel-schein/", "tokens": ["Wie", "solt", "ich", "\u00e4rms\u00b7ter", "wurm", "er\u00b7tra\u00b7gen", "dop\u00b7pel\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da f\u00fcr zwey sonnen selbst die welt ist viel zu klein.", "tokens": ["Da", "f\u00fcr", "zwey", "son\u00b7nen", "selbst", "die", "welt", "ist", "viel", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "ADV", "ADV", "ART", "NN", "VAFIN", "ADV", "PTKA