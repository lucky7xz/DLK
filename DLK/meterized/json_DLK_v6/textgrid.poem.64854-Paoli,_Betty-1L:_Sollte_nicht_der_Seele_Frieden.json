{"textgrid.poem.64854": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sollte nicht der Seele Frieden", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollte nicht der Seele Frieden", "tokens": ["Soll\u00b7te", "nicht", "der", "See\u00b7le", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Untergeh'n in Qual und Pein,", "tokens": ["Un\u00b7ter\u00b7geh'n", "in", "Qual", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00dfte unser Bund geschieden", "tokens": ["Mu\u00df\u00b7te", "un\u00b7ser", "Bund", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und f\u00fcr stets gel\u00f6set sein.", "tokens": ["Und", "f\u00fcr", "stets", "ge\u00b7l\u00f6\u00b7set", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als der Kampf nun ausgerungen,", "tokens": ["Als", "der", "Kampf", "nun", "aus\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und getroffen war die Wahl,", "tokens": ["Und", "ge\u00b7trof\u00b7fen", "war", "die", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hieltst du schweigend mich umschlungen,", "tokens": ["Hieltst", "du", "schwei\u00b7gend", "mich", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheidend, noch zum letzten Mal.", "tokens": ["Schei\u00b7dend", ",", "noch", "zum", "letz\u00b7ten", "Mal", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und von deinem Arm umschlossen", "tokens": ["Und", "von", "dei\u00b7nem", "Arm", "um\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab die Thr\u00e4ne ich gef\u00fchlt,", "tokens": ["Hab", "die", "Thr\u00e4\u00b7ne", "ich", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, aus deinem Aug' geflossen,", "tokens": ["Die", ",", "aus", "dei\u00b7nem", "Aug'", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine hei\u00dfe Stirn gek\u00fchlt.", "tokens": ["Mei\u00b7ne", "hei\u00b7\u00dfe", "Stirn", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tr\u00e4um'risch falt' ich jetzt die H\u00e4nde,", "tokens": ["Tr\u00e4um'\u00b7risch", "falt'", "ich", "jetzt", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fragend: war die Thr\u00e4ne licht,", "tokens": ["Fra\u00b7gend", ":", "war", "die", "Thr\u00e4\u00b7ne", "licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Liebe letzte Spende,", "tokens": ["Dei\u00b7ner", "Lie\u00b7be", "letz\u00b7te", "Spen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine letzte Oelung nicht?", "tokens": ["Mei\u00b7ne", "letz\u00b7te", "O\u00b7e\u00b7lung", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}