{"textgrid.poem.35009": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Orpheisch", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gab den Dolch in deine Hand", "tokens": ["Es", "gab", "den", "Dolch", "in", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein b\u00f6ser D\u00e4mon in der b\u00f6sen Stunde \u2013", "tokens": ["Ein", "b\u00f6\u00b7ser", "D\u00e4\u00b7mon", "in", "der", "b\u00f6\u00b7sen", "Stun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df nicht, wie der D\u00e4mon hie\u00df \u2013", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "der", "D\u00e4\u00b7mon", "hie\u00df", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df nur, da\u00df vergiftet war die Wunde.", "tokens": ["Ich", "wei\u00df", "nur", ",", "da\u00df", "ver\u00b7gif\u00b7tet", "war", "die", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In stillen N\u00e4chten denk ich oft,", "tokens": ["In", "stil\u00b7len", "N\u00e4ch\u00b7ten", "denk", "ich", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du solltest mal dem Schattenreich entsteigen", "tokens": ["Du", "soll\u00b7test", "mal", "dem", "Schat\u00b7ten\u00b7reich", "ent\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00f6sen alle R\u00e4tsel mir", "tokens": ["Und", "l\u00f6\u00b7sen", "al\u00b7le", "R\u00e4t\u00b7sel", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich von deiner Unschuld \u00fcberzeugen.", "tokens": ["Und", "mich", "von", "dei\u00b7ner", "Un\u00b7schuld", "\u00fc\u00b7berz\u00b7eu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich harre dein \u2013 o komme bald!", "tokens": ["Ich", "har\u00b7re", "dein", "\u2013", "o", "kom\u00b7me", "bald", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$(", "FM", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kommst du nicht, so steig ich selbst zur H\u00f6lle,", "tokens": ["Und", "kommst", "du", "nicht", ",", "so", "steig", "ich", "selbst", "zur", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich alldort vor Satanas", "tokens": ["Da\u00df", "ich", "all\u00b7dort", "vor", "Sa\u00b7ta\u00b7nas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und allen Teufeln dich zur Rede stelle.", "tokens": ["Und", "al\u00b7len", "Teu\u00b7feln", "dich", "zur", "Re\u00b7de", "stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich komme, und wie Orpheus einst", "tokens": ["Ich", "kom\u00b7me", ",", "und", "wie", "Or\u00b7pheus", "einst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PWAV", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trotz ich der Unterwelt mit ihren Schrecken \u2013", "tokens": ["Trotz", "ich", "der", "Un\u00b7ter\u00b7welt", "mit", "ih\u00b7ren", "Schre\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich finde dich, und wolltest du", "tokens": ["Ich", "fin\u00b7de", "dich", ",", "und", "woll\u00b7test", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im tiefsten H\u00f6llenpfuhle dich verstecken.", "tokens": ["Im", "tiefs\u00b7ten", "H\u00f6l\u00b7len\u00b7pfuh\u00b7le", "dich", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hinunter jetzt ins Land der Qual,", "tokens": ["Hin\u00b7un\u00b7ter", "jetzt", "ins", "Land", "der", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo H\u00e4nderingen nur und Z\u00e4hneklappen \u2013", "tokens": ["Wo", "H\u00e4n\u00b7de\u00b7rin\u00b7gen", "nur", "und", "Z\u00e4h\u00b7ne\u00b7klap\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich rei\u00dfe dir die Larve ab,", "tokens": ["Ich", "rei\u00b7\u00dfe", "dir", "die", "Lar\u00b7ve", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der angeprahlten Gro\u00dfmut Purpurlappen \u2013", "tokens": ["Der", "an\u00b7ge\u00b7prahl\u00b7ten", "Gro\u00df\u00b7mut", "Pur\u00b7pur\u00b7lap\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Jetzt wei\u00df ich, was ich wissen wollt,", "tokens": ["Jetzt", "wei\u00df", "ich", ",", "was", "ich", "wis\u00b7sen", "wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gern, mein M\u00f6rder, will ich dir verzeihen;", "tokens": ["Und", "gern", ",", "mein", "M\u00f6r\u00b7der", ",", "will", "ich", "dir", "ver\u00b7zei\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch hindern kann ich nicht, da\u00df jetzt", "tokens": ["Doch", "hin\u00b7dern", "kann", "ich", "nicht", ",", "da\u00df", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schmachvoll die Teufel dir ins Antlitz speien.", "tokens": ["Schmach\u00b7voll", "die", "Teu\u00b7fel", "dir", "ins", "Ant\u00b7litz", "spei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}