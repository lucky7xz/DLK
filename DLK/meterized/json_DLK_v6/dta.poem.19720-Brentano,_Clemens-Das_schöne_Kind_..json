{"dta.poem.19720": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das sch\u00f6ne Kind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie war ich doch so wonnereich,               ", "tokens": ["Wie", "war", "ich", "doch", "so", "won\u00b7ne\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Kaiser und dem K\u00f6nig gleich", "tokens": ["Dem", "Kai\u00b7ser", "und", "dem", "K\u00f6\u00b7nig", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinen jungen Jahren,", "tokens": ["In", "mei\u00b7nen", "jun\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Julia das sch\u00f6ne Kind,", "tokens": ["Als", "Ju\u00b7lia", "das", "sch\u00f6\u00b7ne", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sch\u00f6n wie die lieben Engel sind,", "tokens": ["Sch\u00f6n", "wie", "die", "lie\u00b7ben", "En\u00b7gel", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich beysammen waren.", "tokens": ["Und", "ich", "bey\u00b7sam\u00b7men", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Mutter nannt mich Br\u00e4utigam,", "tokens": ["Die", "Mut\u00b7ter", "nannt", "mich", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wurden gar nicht roth vor Scham,", "tokens": ["Wir", "wur\u00b7den", "gar", "nicht", "roth", "vor", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir mochten gern so spielen,", "tokens": ["Wir", "moch\u00b7ten", "gern", "so", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch Julia das sch\u00f6ne Kind,", "tokens": ["Doch", "Ju\u00b7lia", "das", "sch\u00f6\u00b7ne", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das gieng schon fort im kalten Wind,", "tokens": ["Das", "gieng", "schon", "fort", "im", "kal\u00b7ten", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mochte es nicht f\u00fchlen.", "tokens": ["Und", "moch\u00b7te", "es", "nicht", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun bin ich gar nicht wonnereich,", "tokens": ["Nun", "bin", "ich", "gar", "nicht", "won\u00b7ne\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem alten Manne bin ich gleich,", "tokens": ["Dem", "al\u00b7ten", "Man\u00b7ne", "bin", "ich", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bin doch jung von Jahren,", "tokens": ["Und", "bin", "doch", "jung", "von", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin ein K\u00f6nig ohne Land,", "tokens": ["Ich", "bin", "ein", "K\u00f6\u00b7nig", "oh\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Julia an deiner Hand,", "tokens": ["Denn", "Ju\u00b7lia", "an", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da tanzen Engelschaaren.", "tokens": ["Da", "tan\u00b7zen", "En\u00b7gel\u00b7schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}