{"textgrid.poem.47776": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[meine Lebenszeit verstreicht]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Lebenszeit verstreicht,", "tokens": ["Mei\u00b7ne", "Le\u00b7bens\u00b7zeit", "ver\u00b7streicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcndlich eil' ich hin zu Grabe,", "tokens": ["St\u00fcnd\u00b7lich", "eil'", "ich", "hin", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nie wenig ist's vielleicht,", "tokens": ["Und", "nie", "we\u00b7nig", "ist's", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VAFIN", "ADV", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df ich noch zu leben habe!", "tokens": ["Da\u00df", "ich", "noch", "zu", "le\u00b7ben", "ha\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum, Geliebte, s\u00e4ume nicht,", "tokens": ["Drum", ",", "Ge\u00b7lieb\u00b7te", ",", "s\u00e4u\u00b7me", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spende mir mit holdem Munde,", "tokens": ["Spen\u00b7de", "mir", "mit", "hol\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehe mir die Seel' entweicht,", "tokens": ["E\u00b7he", "mir", "die", "Seel'", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine letzte s\u00fc\u00dfe Labe!", "tokens": ["Ei\u00b7ne", "letz\u00b7te", "s\u00fc\u00b7\u00dfe", "La\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}