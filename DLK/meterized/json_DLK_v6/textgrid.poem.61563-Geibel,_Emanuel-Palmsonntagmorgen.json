{"textgrid.poem.61563": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Palmsonntagmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fiel ein Tau vom Himmel himmlisch mild,", "tokens": ["Es", "fiel", "ein", "Tau", "vom", "Him\u00b7mel", "himm\u00b7lisch", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der alle Pflanzen bis zur Wurzel stillt;", "tokens": ["Der", "al\u00b7le", "Pflan\u00b7zen", "bis", "zur", "Wur\u00b7zel", "stillt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df dein Sehnen,", "tokens": ["La\u00df", "dein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "La\u00df die Tr\u00e4nen!", "tokens": ["La\u00df", "die", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Es fiel ein Tau, der alles D\u00fcrsten stillt.", "tokens": ["Es", "fiel", "ein", "Tau", ",", "der", "al\u00b7les", "D\u00fcrs\u00b7ten", "stillt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein sanftes Sausen kommt aus hoher Luft,", "tokens": ["Ein", "sanf\u00b7tes", "Sau\u00b7sen", "kommt", "aus", "ho\u00b7her", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Still gr\u00fcnt das Tal und steht in Veilchenduft;", "tokens": ["Still", "gr\u00fcnt", "das", "Tal", "und", "steht", "in", "Veil\u00b7chen\u00b7duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "G\u00f6ttlich Leben", "tokens": ["G\u00f6tt\u00b7lich", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "F\u00fchl' ich weben,", "tokens": ["F\u00fchl'", "ich", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ein sanftes Sausen kommt aus hoher Luft.", "tokens": ["Ein", "sanf\u00b7tes", "Sau\u00b7sen", "kommt", "aus", "ho\u00b7her", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie Engelsfl\u00fcgel blitzt es \u00fcber Land;", "tokens": ["Wie", "En\u00b7gels\u00b7fl\u00fc\u00b7gel", "blitzt", "es", "\u00fc\u00b7ber", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun schm\u00fcck' dich, Herz, tu an ein rein Gewand!", "tokens": ["Nun", "schm\u00fcck", "dich", ",", "Herz", ",", "tu", "an", "ein", "rein", "Ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "NE", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieh, die Sonne", "tokens": ["Sieh", ",", "die", "Son\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Steigt in Wonne,", "tokens": ["Steigt", "in", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wie Engelsfl\u00fcgel blitzt es \u00fcber Land.", "tokens": ["Wie", "En\u00b7gels\u00b7fl\u00fc\u00b7gel", "blitzt", "es", "\u00fc\u00b7ber", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Macht weit das Tor! Der K\u00f6nig ziehet ein,", "tokens": ["Macht", "weit", "das", "Tor", "!", "Der", "K\u00f6\u00b7nig", "zie\u00b7het", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt soll jung und lauter Friede sein;", "tokens": ["Die", "Welt", "soll", "jung", "und", "lau\u00b7ter", "Frie\u00b7de", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "KON", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Streuet Palmen!", "tokens": ["Streu\u00b7et", "Pal\u00b7men", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Singet Psalmen!", "tokens": ["Sin\u00b7get", "Psal\u00b7men", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Hosanna singt, der K\u00f6nig ziehet ein.", "tokens": ["Ho\u00b7san\u00b7na", "singt", ",", "der", "K\u00f6\u00b7nig", "zie\u00b7het", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}