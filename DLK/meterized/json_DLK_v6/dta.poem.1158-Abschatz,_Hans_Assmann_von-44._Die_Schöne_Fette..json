{"dta.poem.1158": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "44. Die Sch\u00f6ne Fette.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00fcst im Golde selbst mit Armutt seyn umfangen/ ", "tokens": ["Ich", "m\u00fcst", "im", "Gol\u00b7de", "selbst", "mit", "Ar\u00b7mutt", "seyn", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ADV", "APPR", "NN", "VAINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Hunger leiden Durst/ wenn ich mein sch\u00f6nes Kind/", "tokens": ["Im", "Hun\u00b7ger", "lei\u00b7den", "Durst", "/", "wenn", "ich", "mein", "sch\u00f6\u00b7nes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$(", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey der die k\u00fchne Hand ihr voll Vergn\u00fcgen findt/", "tokens": ["Bey", "der", "die", "k\u00fch\u00b7ne", "Hand", "ihr", "voll", "Ver\u00b7gn\u00fc\u00b7gen", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PPER", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht sch\u00e4zte Lobens-werth die auffgepau\u00dften Wangen.", "tokens": ["Nicht", "sch\u00e4z\u00b7te", "Lo\u00b7bens\u00b7werth", "die", "auff\u00b7ge\u00b7pau\u00df\u00b7ten", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wolt ihr mit Mumien/ Geripp und Geistern prangen/", "tokens": ["Wolt", "ihr", "mit", "Mu\u00b7mi\u00b7en", "/", "Ge\u00b7ripp", "und", "Geis\u00b7tern", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$(", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die ihr/ was mager/ liebt/ so f\u00fcrchtet Glutt und Wind.", "tokens": ["Die", "ihr", "/", "was", "ma\u00b7ger", "/", "liebt", "/", "so", "f\u00fcrch\u00b7tet", "Glutt", "und", "Wind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "$(", "PWS", "ADJD", "$(", "VVFIN", "$(", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verbrennt ein d\u00fcrres Scheit nicht/ wenn es kaum entz\u00fcndt/", "tokens": ["Ver\u00b7brennt", "ein", "d\u00fcr\u00b7res", "Scheit", "nicht", "/", "wenn", "es", "kaum", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein gr\u00fcner Stock voll Safft l\u00e4st lange W\u00e4rmd\u2019 empfangen.", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Stock", "voll", "Safft", "l\u00e4st", "lan\u00b7ge", "W\u00e4rmd'", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VVFIN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Im Uberflu\u00df besteht der Schatz der ", "tokens": ["Im", "U\u00b7ber\u00b7flu\u00df", "be\u00b7steht", "der", "Schatz", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der volle Monden leucht am sch\u00f6nsten in der H\u00f6h:", "tokens": ["Der", "vol\u00b7le", "Mon\u00b7den", "leucht", "am", "sch\u00f6ns\u00b7ten", "in", "der", "H\u00f6h", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man solte wohl dein Lob bi\u00df an die Sonne treiben/", "tokens": ["Man", "sol\u00b7te", "wohl", "dein", "Lob", "bi\u00df", "an", "die", "Son\u00b7ne", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein Apffel-runder Schatz; doch sag ich di\u00df allein", "tokens": ["Mein", "Apf\u00b7fel\u00b7run\u00b7der", "Schatz", ";", "doch", "sag", "ich", "di\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "PDS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dir noch zum Lobe nach/ soltstu gleich nackend seyn/", "tokens": ["Dir", "noch", "zum", "Lo\u00b7be", "nach", "/", "solt\u00b7stu", "gleich", "na\u00b7ckend", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "APPR", "$(", "VMFIN", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So wird dir doch noch viel zu zeigen \u00fcbrig bleiben.", "tokens": ["So", "wird", "dir", "doch", "noch", "viel", "zu", "zei\u00b7gen", "\u00fcb\u00b7rig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}