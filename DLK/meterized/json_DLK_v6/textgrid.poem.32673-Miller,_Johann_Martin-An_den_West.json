{"textgrid.poem.32673": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An den West", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald wirst du junger West nicht mehr,", "tokens": ["Bald", "wirst", "du", "jun\u00b7ger", "West", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wangen mir zu k\u00fchlen,", "tokens": ["Die", "Wan\u00b7gen", "mir", "zu", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am stillen Abend um mich her", "tokens": ["Am", "stil\u00b7len", "A\u00b7bend", "um", "mich", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit leisem Fittich spielen.", "tokens": ["Mit", "lei\u00b7sem", "Fit\u00b7tich", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich liebte Daphnen; mein Geschick", "tokens": ["Ich", "lieb\u00b7te", "Daph\u00b7nen", ";", "mein", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War, einsam mich zu qu\u00e4len,", "tokens": ["War", ",", "ein\u00b7sam", "mich", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, was ich f\u00fchlte, vor dem Blick", "tokens": ["Und", ",", "was", "ich", "f\u00fchl\u00b7te", ",", "vor", "dem", "Blick"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des M\u00e4dchens zu verhehlen.", "tokens": ["Des", "M\u00e4d\u00b7chens", "zu", "ver\u00b7heh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht Gold und Silber schm\u00fcckten mich,", "tokens": ["Nicht", "Gold", "und", "Sil\u00b7ber", "schm\u00fcck\u00b7ten", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Redlichkeit im Busen,", "tokens": ["Doch", "Red\u00b7lich\u00b7keit", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Herz, das keinem Laster wich,", "tokens": ["Ein", "Herz", ",", "das", "kei\u00b7nem", "Las\u00b7ter", "wich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eure Gunst, ihr Musen!", "tokens": ["Und", "eu\u00b7re", "Gunst", ",", "ihr", "Mu\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu wenig nur f\u00fcr diese Welt,", "tokens": ["Zu", "we\u00b7nig", "nur", "f\u00fcr", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich Liebe zu erlangen!", "tokens": ["Sich", "Lie\u00b7be", "zu", "er\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, euren Blick, ihr M\u00e4dchen, h\u00e4lt", "tokens": ["Ach", ",", "eu\u00b7ren", "Blick", ",", "ihr", "M\u00e4d\u00b7chen", ",", "h\u00e4lt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Au\u00dfenglanz gefangen!", "tokens": ["Nur", "Au\u00b7\u00dfen\u00b7glanz", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zwar dich, o Daphne, nicht! Doch giebt", "tokens": ["Zwar", "dich", ",", "o", "Daph\u00b7ne", ",", "nicht", "!", "Doch", "giebt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$,", "PTKNEG", "$.", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vater dir Gesetze,", "tokens": ["Ein", "Va\u00b7ter", "dir", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der deine Ruhe minder liebt,", "tokens": ["Der", "dei\u00b7ne", "Ru\u00b7he", "min\u00b7der", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Unverdienst und Sch\u00e4tze.", "tokens": ["Als", "Un\u00b7ver\u00b7dienst", "und", "Sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Oft sah mein Auge seitw\u00e4rts hin", "tokens": ["Oft", "sah", "mein", "Au\u00b7ge", "seit\u00b7w\u00e4rts", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach dem geliebten deinen,", "tokens": ["Nach", "dem", "ge\u00b7lieb\u00b7ten", "dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da sah ich Z\u00e4rtlichkeit darin,", "tokens": ["Da", "sah", "ich", "Z\u00e4rt\u00b7lich\u00b7keit", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dich verstohlen weinen.", "tokens": ["Und", "dich", "ver\u00b7stoh\u00b7len", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Daphne, la\u00df nur einmal noch", "tokens": ["O", "Daph\u00b7ne", ",", "la\u00df", "nur", "ein\u00b7mal", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVIMP", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich den Gedanken denken:", "tokens": ["Mich", "den", "Ge\u00b7dan\u00b7ken", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du w\u00fcrdest, frei vom harten Joch,", "tokens": ["Du", "w\u00fcr\u00b7dest", ",", "frei", "vom", "har\u00b7ten", "Joch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir deine Seele schenken!", "tokens": ["Mir", "dei\u00b7ne", "See\u00b7le", "schen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bald wird mir der Gedank' allein", "tokens": ["Bald", "wird", "mir", "der", "Ge\u00b7dank'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Todestrank vers\u00fc\u00dfen,", "tokens": ["Den", "To\u00b7des\u00b7trank", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und einen Tropfen Trost darein,", "tokens": ["Und", "ei\u00b7nen", "Trop\u00b7fen", "Trost", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wann ich ihn trinke, gie\u00dfen.", "tokens": ["Wann", "ich", "ihn", "trin\u00b7ke", ",", "gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann soll mich hier, wo schon, der Wut", "tokens": ["Dann", "soll", "mich", "hier", ",", "wo", "schon", ",", "der", "Wut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Mi\u00dfgeschicks entrissen,", "tokens": ["Des", "Mi\u00df\u00b7ge\u00b7schicks", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Liebling meiner Seele ruht,", "tokens": ["Ein", "Lieb\u00b7ling", "mei\u00b7ner", "See\u00b7le", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df deinen leisen Fittich dann,", "tokens": ["La\u00df", "dei\u00b7nen", "lei\u00b7sen", "Fit\u00b7tich", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Zephyr, sanfter wehen,", "tokens": ["O", "Ze\u00b7phyr", ",", "sanf\u00b7ter", "we\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Tausendsch\u00f6n und Thymian", "tokens": ["Und", "Tau\u00b7send\u00b7sch\u00f6n", "und", "Thy\u00b7mi\u00b7an"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf meinem Grab entstehen!", "tokens": ["Auf", "mei\u00b7nem", "Grab", "ent\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich sang, ihr Bl\u00fcmchen, eure Zier", "tokens": ["Ich", "sang", ",", "ihr", "Bl\u00fcm\u00b7chen", ",", "eu\u00b7re", "Zier"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hienieden mit Entz\u00fccken,", "tokens": ["Hien\u00b7ie\u00b7den", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und willig also werdet ihr", "tokens": ["Und", "wil\u00b7lig", "al\u00b7so", "wer\u00b7det", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Dichters H\u00fcgel schm\u00fccken!", "tokens": ["Des", "Dich\u00b7ters", "H\u00fc\u00b7gel", "schm\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Oft wird mein Damon stumm und bleich", "tokens": ["Oft", "wird", "mein", "Da\u00b7mon", "stumm", "und", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich auf den H\u00fcgel setzen,", "tokens": ["Sich", "auf", "den", "H\u00fc\u00b7gel", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mit der Liebe Thr\u00e4nen euch", "tokens": ["Und", "mit", "der", "Lie\u00b7be", "Thr\u00e4\u00b7nen", "euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Mondenschein benetzen.", "tokens": ["Im", "Mon\u00b7den\u00b7schein", "be\u00b7net\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vielleicht k\u00f6mmt dann auch Daphne her,", "tokens": ["Viel\u00b7leicht", "k\u00f6mmt", "dann", "auch", "Daph\u00b7ne", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich im stillen liebte,", "tokens": ["Die", "mich", "im", "stil\u00b7len", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und klagt im \u00f6den Feld umher,", "tokens": ["Und", "klagt", "im", "\u00f6\u00b7den", "Feld", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sie mein Tod betr\u00fcbte.", "tokens": ["Wie", "sie", "mein", "Tod", "be\u00b7tr\u00fcb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie dr\u00fcckt vielleicht, mit bangem Schmerz", "tokens": ["Sie", "dr\u00fcckt", "viel\u00b7leicht", ",", "mit", "ban\u00b7gem", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wehmutsvollen T\u00f6nen,", "tokens": ["Und", "weh\u00b7muts\u00b7vol\u00b7len", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Euch an ihr unbescholtnes Herz,", "tokens": ["Euch", "an", "ihr", "un\u00b7be\u00b7scholt\u00b7nes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und netzet euch mit Thr\u00e4nen.", "tokens": ["Und", "net\u00b7zet", "euch", "mit", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gelinde flattre dann, o West,", "tokens": ["Ge\u00b7lin\u00b7de", "flatt\u00b7re", "dann", ",", "o", "West", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das M\u00e4dchen zu erfrischen,", "tokens": ["Das", "M\u00e4d\u00b7chen", "zu", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Thr\u00e4nen, die der Gram erpre\u00dft,", "tokens": ["Und", "Thr\u00e4\u00b7nen", ",", "die", "der", "Gram", "er\u00b7pre\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr vom Gesicht zu wischen!", "tokens": ["Ihr", "vom", "Ge\u00b7sicht", "zu", "wi\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich aber singe dann aus dir,", "tokens": ["Ich", "a\u00b7ber", "sin\u00b7ge", "dann", "aus", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geliebte Philomele,", "tokens": ["Ge\u00b7lieb\u00b7te", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und gie\u00dfe milde Tr\u00f6stung ihr", "tokens": ["Und", "gie\u00b7\u00dfe", "mil\u00b7de", "Tr\u00f6s\u00b7tung", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In die betr\u00fcbte Seele.", "tokens": ["In", "die", "be\u00b7tr\u00fcb\u00b7te", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}