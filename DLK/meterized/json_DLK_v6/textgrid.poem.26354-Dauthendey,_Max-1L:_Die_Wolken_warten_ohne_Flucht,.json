{"textgrid.poem.26354": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Wolken warten ohne Flucht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken warten ohne Flucht,", "tokens": ["Die", "Wol\u00b7ken", "war\u00b7ten", "oh\u00b7ne", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wasserfall zischt aus der Schlucht.", "tokens": ["Der", "Was\u00b7ser\u00b7fall", "zischt", "aus", "der", "Schlucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Grasbl\u00fcten zittern im Morgenhauch.", "tokens": ["Gras\u00b7bl\u00fc\u00b7ten", "zit\u00b7tern", "im", "Mor\u00b7gen\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Gedanken, wie der blaue Rauch,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "wie", "der", "blau\u00b7e", "Rauch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie eilen hin zum Meeresrand.", "tokens": ["Sie", "ei\u00b7len", "hin", "zum", "Mee\u00b7res\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sehnende lebt ohne Land,", "tokens": ["Der", "Seh\u00b7nen\u00b7de", "lebt", "oh\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie die Wolke im Leeren h\u00e4ngt,", "tokens": ["Wie", "die", "Wol\u00b7ke", "im", "Lee\u00b7ren", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wie der Wasserfall eingezw\u00e4ngt.", "tokens": ["Wie", "der", "Was\u00b7ser\u00b7fall", "ein\u00b7ge\u00b7zw\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Er bebt empfindlich wie zartes Gras.", "tokens": ["Er", "bebt", "emp\u00b7find\u00b7lich", "wie", "zar\u00b7tes", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wie der Meeresspiegel bla\u00df,", "tokens": ["Und", "wie", "der", "Mee\u00b7res\u00b7spie\u00b7gel", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sucht ruhlos atmend er die Ruh'.", "tokens": ["Sucht", "ruh\u00b7los", "at\u00b7mend", "er", "die", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein L\u00e4cheln deckt Abgr\u00fcnde zu.", "tokens": ["Sein", "L\u00e4\u00b7cheln", "deckt", "Ab\u00b7gr\u00fcn\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}