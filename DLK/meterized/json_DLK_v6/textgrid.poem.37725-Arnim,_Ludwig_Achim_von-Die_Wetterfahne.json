{"textgrid.poem.37725": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Wetterfahne", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Fahnen sind verloren,", "tokens": ["Viel", "Fah\u00b7nen", "sind", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch eine ist erkoren,", "tokens": ["Doch", "ei\u00b7ne", "ist", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schwebt noch obenauf", "tokens": ["Sie", "schwebt", "noch", "o\u00b7ben\u00b7auf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und zeigt der St\u00fcrme Lauf.", "tokens": ["Und", "zeigt", "der", "St\u00fcr\u00b7me", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So steht die Wetterfahne,", "tokens": ["So", "steht", "die", "Wet\u00b7ter\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df sie uns all' ermahne", "tokens": ["Da\u00df", "sie", "uns", "all'", "er\u00b7mah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu k\u00fcnft'ger Wachsamkeit,", "tokens": ["Zu", "k\u00fcnft'\u00b7ger", "Wach\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In Leid und auch in Freud'.", "tokens": ["In", "Leid", "und", "auch", "in", "Freud'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie drehet sich geschwinde,", "tokens": ["Sie", "dre\u00b7het", "sich", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und steht doch fest im Winde,", "tokens": ["Und", "steht", "doch", "fest", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es spielt drinn der Wind,", "tokens": ["Es", "spielt", "drinn", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie ein unartig Kind.", "tokens": ["Wie", "ein", "un\u00b7ar\u00b7tig", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er kommt nicht, wenn wir glauben,", "tokens": ["Er", "kommt", "nicht", ",", "wenn", "wir", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch steht die Fahn' im Glauben", "tokens": ["Doch", "steht", "die", "Fahn'", "im", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf eines Kirchthurms Spitz", "tokens": ["Auf", "ei\u00b7nes", "Kirch\u00b7thurms", "Spitz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und leitet ab den Blitz.", "tokens": ["Und", "lei\u00b7tet", "ab", "den", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Gl\u00fcck kann Gott nur geben,", "tokens": ["Das", "Gl\u00fcck", "kann", "Gott", "nur", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonne will sich heben.", "tokens": ["Die", "Son\u00b7ne", "will", "sich", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Fahne klirrt in Glanz,", "tokens": ["Die", "Fah\u00b7ne", "klirrt", "in", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Wetter \u00e4ndert ganz.", "tokens": ["Das", "Wet\u00b7ter", "\u00e4n\u00b7dert", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wo die Thr\u00e4nen flossen,", "tokens": ["Und", "wo", "die", "Thr\u00e4\u00b7nen", "flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die gr\u00fcnen Bl\u00e4tter sprossen,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Bl\u00fcten allzumal,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich gr\u00fc\u00df dich freundlich Thal.", "tokens": ["Ich", "gr\u00fc\u00df", "dich", "freund\u00b7lich", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich gr\u00fc\u00df dich von der H\u00f6he,", "tokens": ["Ich", "gr\u00fc\u00df", "dich", "von", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Thurme weit ich sehe,", "tokens": ["Vom", "Thur\u00b7me", "weit", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh' der Erde Rand,", "tokens": ["Ich", "seh'", "der", "Er\u00b7de", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Wellen ohne Stand.", "tokens": ["Die", "Wel\u00b7len", "oh\u00b7ne", "Stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hoch auf des Thurmes Zinnen,", "tokens": ["Hoch", "auf", "des", "Thur\u00b7mes", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich den Geist gewinnen,", "tokens": ["Will", "ich", "den", "Geist", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er mir frei und klar", "tokens": ["Da\u00df", "er", "mir", "frei", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Schicksal sage wahr.", "tokens": ["Das", "Schick\u00b7sal", "sa\u00b7ge", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das Land ist aufger\u00e4umet,", "tokens": ["Das", "Land", "ist", "auf\u00b7ge\u00b7r\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Meer hat ausgesch\u00e4umet,", "tokens": ["Das", "Meer", "hat", "aus\u00b7ge\u00b7sch\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Taub' den \u00d6lzweig bringt,", "tokens": ["Die", "Taub'", "den", "\u00d6l\u00b7zweig", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr armen V\u00f6lker singt.", "tokens": ["Ihr", "ar\u00b7men", "V\u00f6l\u00b7ker", "singt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "La\u00dft euer Lied ert\u00f6nen,", "tokens": ["La\u00dft", "eu\u00b7er", "Lied", "er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Erdkreis zu vers\u00f6hnen,", "tokens": ["Den", "Erd\u00b7kreis", "zu", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott in der H\u00f6h' sei Ehr',", "tokens": ["Gott", "in", "der", "H\u00f6h'", "sei", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Er straft euch nun nicht mehr.", "tokens": ["Er", "straft", "euch", "nun", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gott hat von uns entr\u00fccket", "tokens": ["Gott", "hat", "von", "uns", "ent\u00b7r\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Feind, der uns erdr\u00fccket,", "tokens": ["Den", "Feind", ",", "der", "uns", "er\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6gel singen laut,", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df sie den Herrn geschaut.", "tokens": ["Da\u00df", "sie", "den", "Herrn", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}