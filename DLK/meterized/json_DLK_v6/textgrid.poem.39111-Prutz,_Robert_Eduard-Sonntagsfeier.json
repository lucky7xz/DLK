{"textgrid.poem.39111": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagsfeier", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schwebt dort auf des Wohllauts Schwingen", "tokens": ["Was", "schwebt", "dort", "auf", "des", "Wohl\u00b7lauts", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu mir her\u00fcber durch die Luft?", "tokens": ["Zu", "mir", "her\u00b7\u00fc\u00b7ber", "durch", "die", "Luft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6r es rauschen, h\u00f6r es klingen", "tokens": ["Ich", "h\u00f6r", "es", "rau\u00b7schen", ",", "h\u00f6r", "es", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In s\u00fc\u00dfem morgentlichem Duft:", "tokens": ["In", "s\u00fc\u00b7\u00dfem", "mor\u00b7gent\u00b7li\u00b7chem", "Duft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Das ist die Orgel, sind die Glocken", "tokens": ["Das", "ist", "die", "Or\u00b7gel", ",", "sind", "die", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und der Posaunen ernster Klang,", "tokens": ["Und", "der", "Po\u00b7sau\u00b7nen", "erns\u00b7ter", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O horch, sie laden mich und locken", "tokens": ["O", "horch", ",", "sie", "la\u00b7den", "mich", "und", "lo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$,", "PPER", "VVFIN", "PPER", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu einem l\u00e4ngst entw\u00f6hnten Gang. \u2013", "tokens": ["Zu", "ei\u00b7nem", "l\u00e4ngst", "ent\u00b7w\u00f6hn\u00b7ten", "Gang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, vor der Kirche, welch Gedr\u00e4nge!", "tokens": ["Sieh", ",", "vor", "der", "Kir\u00b7che", ",", "welch", "Ge\u00b7dr\u00e4n\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Staub des Werkeltages rein,", "tokens": ["Vom", "Staub", "des", "Wer\u00b7kel\u00b7ta\u00b7ges", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00e4ngt alt und jung, in bunter Menge", "tokens": ["Dr\u00e4ngt", "alt", "und", "jung", ",", "in", "bun\u00b7ter", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich in das Heiligtum hinein:", "tokens": ["Sich", "in", "das", "Hei\u00b7lig\u00b7tum", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hier, im sonnt\u00e4glichen Kleide,", "tokens": ["Und", "hier", ",", "im", "sonn\u00b7t\u00e4g\u00b7li\u00b7chen", "Klei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Kranz im glattgestrichnen Haar,", "tokens": ["Den", "Kranz", "im", "glatt\u00b7ge\u00b7strich\u00b7nen", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gesenkten Augs, doch Augenweide,", "tokens": ["Ge\u00b7senk\u00b7ten", "Augs", ",", "doch", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Jungfraun wunderholde Schar.", "tokens": ["Der", "Jung\u00b7fraun", "wun\u00b7der\u00b7hol\u00b7de", "Schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie gehen all mit leisen Schritten,", "tokens": ["Sie", "ge\u00b7hen", "all", "mit", "lei\u00b7sen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erw\u00e4gend ihres Herzens Not,", "tokens": ["Er\u00b7w\u00e4\u00b7gend", "ih\u00b7res", "Her\u00b7zens", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wollen beten, wollen bitten", "tokens": ["Sie", "wol\u00b7len", "be\u00b7ten", ",", "wol\u00b7len", "bit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um Haus und Hof und t\u00e4glich Brot:", "tokens": ["Um", "Haus", "und", "Hof", "und", "t\u00e4g\u00b7lich", "Brot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df sich die Krankheit endlich wende,", "tokens": ["Da\u00df", "sich", "die", "Krank\u00b7heit", "end\u00b7lich", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df auf dem Feld die Frucht gedeih", "tokens": ["Da\u00df", "auf", "dem", "Feld", "die", "Frucht", "ge\u00b7deih"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df die Arbeit ihrer H\u00e4nde", "tokens": ["Und", "da\u00df", "die", "Ar\u00b7beit", "ih\u00b7rer", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit gutem Zins gesegnet sei.", "tokens": ["Mit", "gu\u00b7tem", "Zins", "ge\u00b7seg\u00b7net", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Wahn des Glaubens, s\u00fc\u00dfe Stille,", "tokens": ["O", "Wahn", "des", "Glau\u00b7bens", ",", "s\u00fc\u00b7\u00dfe", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In der das Herz sich selbst verlor,", "tokens": ["In", "der", "das", "Herz", "sich", "selbst", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du meiner Kinderwelt Idylle,", "tokens": ["Du", "mei\u00b7ner", "Kin\u00b7der\u00b7welt", "I\u00b7dyl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was steigst du heute mir empor?", "tokens": ["Was", "steigst", "du", "heu\u00b7te", "mir", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00fcrde mir die Welt zu eigen", "tokens": ["Und", "w\u00fcr\u00b7de", "mir", "die", "Welt", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und neigten alle Sterne sich:", "tokens": ["Und", "neig\u00b7ten", "al\u00b7le", "Ster\u00b7ne", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich k\u00f6nnte doch mein Knie nicht neigen,", "tokens": ["Ich", "k\u00f6nn\u00b7te", "doch", "mein", "Knie", "nicht", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht deine Psalmen r\u00fchren mich! \u2013", "tokens": ["Nicht", "dei\u00b7ne", "Psal\u00b7men", "r\u00fch\u00b7ren", "mich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn andre Glocken h\u00f6r ich t\u00f6nen,", "tokens": ["Denn", "and\u00b7re", "Glo\u00b7cken", "h\u00f6r", "ich", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein andres Lied steigt himmelw\u00e4rts,", "tokens": ["Ein", "and\u00b7res", "Lied", "steigt", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und anders str\u00f6mt mit m\u00e4cht'gem Dr\u00f6hnen", "tokens": ["Und", "an\u00b7ders", "str\u00f6mt", "mit", "m\u00e4cht'\u00b7gem", "Dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drommetenklang mir in das Herz!", "tokens": ["Drom\u00b7me\u00b7ten\u00b7klang", "mir", "in", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Wir stehen auch gedr\u00e4ngt in Scharen,", "tokens": ["Wir", "ste\u00b7hen", "auch", "ge\u00b7dr\u00e4ngt", "in", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir M\u00e4nner, die der Tag erweckt;", "tokens": ["Wir", "M\u00e4n\u00b7ner", ",", "die", "der", "Tag", "er\u00b7weckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch keinen Kranz in unsern Haaren,", "tokens": ["Doch", "kei\u00b7nen", "Kranz", "in", "un\u00b7sern", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Myrten nur das Schwert bedeckt!", "tokens": ["Mit", "Myr\u00b7ten", "nur", "das", "Schwert", "be\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir glauben auch an einen Morgen,", "tokens": ["Wir", "glau\u00b7ben", "auch", "an", "ei\u00b7nen", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An einen Sonntag hell und licht,", "tokens": ["An", "ei\u00b7nen", "Sonn\u00b7tag", "hell", "und", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, bl\u00f6den Augen noch verborgen,", "tokens": ["Der", ",", "bl\u00f6\u00b7den", "Au\u00b7gen", "noch", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wolken endlich doch durchbricht!", "tokens": ["Die", "Wol\u00b7ken", "end\u00b7lich", "doch", "durch\u00b7bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir beten auch \u2013 unausgesprochen,", "tokens": ["Wir", "be\u00b7ten", "auch", "\u2013", "un\u00b7aus\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADJA", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ein Hauch, der unsre Brust durchweht,", "tokens": ["Ein", "Hauch", ",", "der", "uns\u00b7re", "Brust", "durch\u00b7weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein stummer Schwur, ein Herzenspochen,", "tokens": ["Ein", "stum\u00b7mer", "Schwur", ",", "ein", "Her\u00b7zen\u00b7spo\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und eine Tat \u2013 das ist Gebet!", "tokens": ["Und", "ei\u00b7ne", "Tat", "\u2013", "das", "ist", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Drum sollt ihr uns nicht gottlos schm\u00e4hen,", "tokens": ["Drum", "sollt", "ihr", "uns", "nicht", "gott\u00b7los", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nennt uns nicht Ketzer, treibt nicht Spott:", "tokens": ["Nennt", "uns", "nicht", "Ket\u00b7zer", ",", "treibt", "nicht", "Spott", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "$,", "VVFIN", "PTKNEG", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Auch hier, wo unsre Fahnen wehen,", "tokens": ["Auch", "hier", ",", "wo", "uns\u00b7re", "Fah\u00b7nen", "we\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der freie Geist ist auch ein Gott!", "tokens": ["Der", "frei\u00b7e", "Geist", "ist", "auch", "ein", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von allem Finstern, allem B\u00f6sen,", "tokens": ["Von", "al\u00b7lem", "Fins\u00b7tern", ",", "al\u00b7lem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Sklavenketten gro\u00df und klein,", "tokens": ["Von", "Skla\u00b7ve