{"textgrid.poem.65673": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die letzten Griechen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir fragen nichts nach unserm Ruhm, nach unsrer Namen Preis.", "tokens": ["Wir", "fra\u00b7gen", "nichts", "nach", "un\u00b7serm", "Ruhm", ",", "nach", "uns\u00b7rer", "Na\u00b7men", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Was frommt's, ob Welt und Nachwelt einst von unsern Thaten wei\u00df?", "tokens": ["Was", "frommt's", ",", "ob", "Welt", "und", "Nach\u00b7welt", "einst", "von", "un\u00b7sern", "Tha\u00b7ten", "wei\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wenn Hellas sinken mu\u00df in's Grab, was soll der Leichenstein", "tokens": ["Wenn", "Hel\u00b7las", "sin\u00b7ken", "mu\u00df", "in's", "Grab", ",", "was", "soll", "der", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVINF", "VMFIN", "APPRART", "NN", "$,", "PWS", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Auf unsern H\u00fcgeln? La\u00dft sie leer! Wir woll'n vergessen sein.", "tokens": ["Auf", "un\u00b7sern", "H\u00fc\u00b7geln", "?", "La\u00dft", "sie", "leer", "!", "Wir", "woll'n", "ver\u00b7ges\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "ADJD", "$.", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Die Namen unsrer V\u00e4ter gehn den Fremden durch den Mund,", "tokens": ["Die", "Na\u00b7men", "uns\u00b7rer", "V\u00e4\u00b7ter", "gehn", "den", "Frem\u00b7den", "durch", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Sind ihnen in der Schule recht, f\u00fcr Alt und Jung gesund.", "tokens": ["Sind", "ih\u00b7nen", "in", "der", "Schu\u00b7le", "recht", ",", "f\u00fcr", "Alt", "und", "Jung", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,", "APPR", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Ach, wenn kein freier Grieche mehr euch griechisch nennen kann,", "tokens": ["Ach", ",", "wenn", "kein", "frei\u00b7er", "Grie\u00b7che", "mehr", "euch", "grie\u00b7chisch", "nen\u00b7nen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PIAT", "ADJA", "NN", "ADV", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Miltiades, Leonidas, was ist eur Nachruhm dann!", "tokens": ["Mil\u00b7ti\u00b7a\u00b7des", ",", "Leo\u00b7ni\u00b7das", ",", "was", "ist", "eur", "Nach\u00b7ruhm", "dann", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PWS", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Auf welcher keine Sage steht und sch\u00f6ne Namen ruft.", "tokens": ["Auf", "wel\u00b7cher", "kei\u00b7ne", "Sa\u00b7ge", "steht", "und", "sch\u00f6\u00b7ne", "Na\u00b7men", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Barbaren, ihr versteht sie nicht! Sie klingen euch in's Ohr,", "tokens": ["Bar\u00b7ba\u00b7ren", ",", "ihr", "ver\u00b7steht", "sie", "nicht", "!", "Sie", "klin\u00b7gen", "euch", "in's", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Hinein zum einen und heraus alsbald zum andern Thor;", "tokens": ["Hin\u00b7ein", "zum", "ei\u00b7nen", "und", "he\u00b7raus", "als\u00b7bald", "zum", "an\u00b7dern", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ART", "KON", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Doch ewig taub wird euer Herz f\u00fcr Hellas Namen sein,", "tokens": ["Doch", "e\u00b7wig", "taub", "wird", "eu\u00b7er", "Herz", "f\u00fcr", "Hel\u00b7las", "Na\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Er sog von unsrer V\u00e4ter Geist nicht ", "tokens": ["Er", "sog", "von", "uns\u00b7rer", "V\u00e4\u00b7ter", "Geist", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "PTKNEG"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und umgest\u00fcrzt der morsche Thurm der stolzen Tyrannei.", "tokens": ["Und", "um\u00b7ge\u00b7st\u00fcrzt", "der", "mor\u00b7sche", "Thurm", "der", "stol\u00b7zen", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Was habt ihr, V\u00f6lker, denn gelernt von Hellas alter Kunst?", "tokens": ["Was", "habt", "ihr", ",", "V\u00f6l\u00b7ker", ",", "denn", "ge\u00b7lernt", "von", "Hel\u00b7las", "al\u00b7ter", "Kunst", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "KON", "VVPP", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Den ihr euch als hellenisch preist, seid ihr so frei noch nicht,", "tokens": ["Den", "ihr", "euch", "als", "hel\u00b7le\u00b7nisch", "preist", ",", "seid", "ihr", "so", "frei", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "KOUS", "ADJD", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "PTKNEG", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Zu helfen frei mit Wort und That, von Freiheit Ketten bricht!", "tokens": ["Zu", "hel\u00b7fen", "frei", "mit", "Wort", "und", "That", ",", "von", "Frei\u00b7heit", "Ket\u00b7ten", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "Wir fragen nichts nach unserm Ruhm, nach unsrer Namen Preis.", "tokens": ["Wir", "fra\u00b7gen", "nichts", "nach", "un\u00b7serm", "Ruhm", ",", "nach", "uns\u00b7rer", "Na\u00b7men", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "Was frommt's, ob der Barbaren Schwarm von unsern Thaten wei\u00df?", "tokens": ["Was", "frommt's", ",", "ob", "der", "Bar\u00b7ba\u00b7ren", "Schwarm", "von", "un\u00b7sern", "Tha\u00b7ten", "wei\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Wenn Hellas sinken mu\u00df in's Grab, wir wollen keinen Stein", "tokens": ["Wenn", "Hel\u00b7las", "sin\u00b7ken", "mu\u00df", "in's", "Grab", ",", "wir", "wol\u00b7len", "kei\u00b7nen", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVINF", "VMFIN", "APPRART", "NN", "$,", "PPER", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "F\u00fcr unsre Gruft. La\u00dft ungenannt die letzten Griechen sein!", "tokens": ["F\u00fcr", "uns\u00b7re", "Gruft", ".", "La\u00dft", "un\u00b7ge\u00b7nannt", "die", "letz\u00b7ten", "Grie\u00b7chen", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVIMP", "ADJD", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}