{"textgrid.poem.54012": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der verrutschte Hut", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ein berliner Kavalier ist,", "tokens": ["Was", "ein", "ber\u00b7li\u00b7ner", "Ka\u00b7va\u00b7lier", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der bringt \u2013 ist die Gesellschaft aus \u2013", "tokens": ["der", "bringt", "\u2013", "ist", "die", "Ge\u00b7sell\u00b7schaft", "aus", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$(", "VAFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wenn es morgens fr\u00fch um vier ist,", "tokens": ["und", "wenn", "es", "mor\u00b7gens", "fr\u00fch", "um", "vier", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "APPR", "CARD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Dame, welche . . . stets nach Haus.", "tokens": ["die", "Da\u00b7me", ",", "wel\u00b7che", ".", ".", ".", "stets", "nach", "Haus", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$.", "$.", "$.", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Auto soll man Bande kn\u00fcpfen.", "tokens": ["Im", "Au\u00b7to", "soll", "man", "Ban\u00b7de", "kn\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das mu\u00df so sein und hebt den Herrn.", "tokens": ["Das", "mu\u00df", "so", "sein", "und", "hebt", "den", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VAINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Name \u203aSchl\u00fcpfer\u2039 kommt von: schl\u00fcpfen.", "tokens": ["Der", "Na\u00b7me", "\u203a", "Schl\u00fcp\u00b7fer", "\u2039", "kommt", "von", ":", "schl\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "VVFIN", "APPR", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er glaubt: die Frauen haben das gern . . .", "tokens": ["Er", "glaubt", ":", "die", "Frau\u00b7en", "ha\u00b7ben", "das", "gern", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "PDS", "ADV", "$.", "$.", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Das wollen sie aber gar nicht!", "tokens": ["Das", "wol\u00b7len", "sie", "a\u00b7ber", "gar", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Das m\u00f6gen sie aber gar nicht!", "tokens": ["Das", "m\u00f6\u00b7gen", "sie", "a\u00b7ber", "gar", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Das tut ihnen gar nicht gut!", "tokens": ["Das", "tut", "ih\u00b7nen", "gar", "nicht", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Wie kommen sie denn nun nach Haus?", "tokens": ["Wie", "kom\u00b7men", "sie", "denn", "nun", "nach", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbund wie seh ich \u00fcberhaupt jetzt aus?", "tokens": ["\u00bb", "und", "wie", "seh", "ich", "\u00fc\u00b7ber\u00b7haupt", "jetzt", "aus", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "\u2013 und einen ganz verrutschten Hut!\u00ab", "tokens": ["\u2013", "und", "ei\u00b7nen", "ganz", "ver\u00b7rutschten", "Hut", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ART", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es erben sich Gesetz und Rechte", "tokens": ["Es", "er\u00b7ben", "sich", "Ge\u00b7setz", "und", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie eine ewige Krankheit fort.", "tokens": ["wie", "ei\u00b7ne", "e\u00b7wi\u00b7ge", "Krank\u00b7heit", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er meint, wenn er das nicht vollbr\u00e4chte,", "tokens": ["Er", "meint", ",", "wenn", "er", "das", "nicht", "voll\u00b7br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dann sei er kein mond\u00e4ner Lord.", "tokens": ["dann", "sei", "er", "kein", "mon\u00b7d\u00e4\u00b7ner", "Lord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er mu\u00df. Teils gn\u00e4dig und teils m\u00fcde", "tokens": ["Er", "mu\u00df", ".", "Teils", "gn\u00e4\u00b7dig", "und", "teils", "m\u00fc\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$.", "NN", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und \u00fcberhaupt, weils dunkel ist.", "tokens": ["und", "\u00fc\u00b7ber\u00b7haupt", ",", "weils", "dun\u00b7kel", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbach, der Chauffeur . . . sei doch nicht pr\u00fcde . . .!\u00ab", "tokens": ["\u00bb", "ach", ",", "der", "Chauf\u00b7feur", ".", ".", ".", "sei", "doch", "nicht", "pr\u00fc\u00b7de", ".", ".", ".", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "ART", "NN", "$.", "$.", "$.", "VAFIN", "ADV", "PTKNEG", "ADJD", "$.", "$.", "$.", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ein Mann ist stets ein Egoist.", "tokens": ["Ein", "Mann", "ist", "stets", "ein", "E\u00b7goist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sein Motor will auf Touren laufen.", "tokens": ["Sein", "Mo\u00b7tor", "will", "auf", "Tou\u00b7ren", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Frau braucht Zeit. Es saust die Fahrt.", "tokens": ["Die", "Frau", "braucht", "Zeit", ".", "Es", "saust", "die", "Fahrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie will nicht um die Liebe raufen:", "tokens": ["Sie", "will", "nicht", "um", "die", "Lie\u00b7be", "rau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Haare apart und Bouletten apart.", "tokens": ["Haa\u00b7re", "a\u00b7part", "und", "Bou\u00b7let\u00b7ten", "a\u00b7part", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "NN", "ADJD", "$."], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Doch jener wird gleich handgemein.", "tokens": ["Doch", "je\u00b7ner", "wird", "gleich", "hand\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Jetzt oder nie . . .! Die Hand ans Bein . . .", "tokens": ["Jetzt", "o\u00b7der", "nie", ".", ".", ".", "!", "Die", "Hand", "ans", "Bein", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "KON", "ADV", "$.", "$.", "$.", "$.", "ART", "NN", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das wollen sie aber gar nicht!", "tokens": ["Das", "wol\u00b7len", "sie", "a\u00b7ber", "gar", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Das m\u00f6gen sie aber gar nicht!", "tokens": ["Das", "m\u00f6\u00b7gen", "sie", "a\u00b7ber", "gar", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "Das tut ihnen gar nicht gut!", "tokens": ["Das", "tut", "ih\u00b7nen", "gar", "nicht", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Berliner Autoliebe st\u00f6rt.", "tokens": ["Ber\u00b7li\u00b7ner", "Au\u00b7to\u00b7lie\u00b7be", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Immer h\u00fcbsch alles, wos hingeh\u00f6rt \u2013", "tokens": ["Im\u00b7mer", "h\u00fcbsch", "al\u00b7les", ",", "wos", "hin\u00b7ge\u00b7h\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "$,", "PWAV", "VVPP", "$("], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "ohne verrutschten Hut \u2013!", "tokens": ["oh\u00b7ne", "ver\u00b7rutschten", "Hut", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}