{"textgrid.poem.58556": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "594. F\u00fcrchte dich nicht, du kleine Herde!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Armes Zion,", "tokens": ["Ar\u00b7mes", "Zi\u00b7on", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Gott dein Lohn,", "tokens": ["Gott", "dein", "Lohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Bleibe du nur ihm getreu;", "tokens": ["Blei\u00b7be", "du", "nur", "ihm", "ge\u00b7treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei geduldig,", "tokens": ["Sei", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Leb unschuldig", "tokens": ["Leb", "un\u00b7schul\u00b7dig"], "token_info": ["word", "word"], "pos": ["VVIMP", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Vor der Welt und rede frei!", "tokens": ["Vor", "der", "Welt", "und", "re\u00b7de", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}