{"textgrid.poem.33529": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Erotisches Vari\u00e9t\u00e9", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf offner Stra\u00dfe in der Nacht", "tokens": ["Auf", "off\u00b7ner", "Stra\u00b7\u00dfe", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entkleidet sich ein Kneipenwirt.", "tokens": ["Ent\u00b7klei\u00b7det", "sich", "ein", "Knei\u00b7pen\u00b7wirt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Ingenieur ist aufgebracht,", "tokens": ["Ein", "In\u00b7ge\u00b7ni\u00b7eur", "ist", "auf\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der sich bei seinem Weib verirrt.", "tokens": ["Der", "sich", "bei", "sei\u00b7nem", "Weib", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach gleichgesinnten Viechern schielt", "tokens": ["Nach", "gleich\u00b7ge\u00b7sinn\u00b7ten", "Vie\u00b7chern", "schielt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein homosexueller Hund.", "tokens": ["Ein", "ho\u00b7mo\u00b7se\u00b7xu\u00b7el\u00b7ler", "Hund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Greis, der mit sich selber spielt,", "tokens": ["Ein", "Greis", ",", "der", "mit", "sich", "sel\u00b7ber", "spielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Merkt: Allzuviel ist ungesund.", "tokens": ["Merkt", ":", "All\u00b7zu\u00b7viel", "ist", "un\u00b7ge\u00b7sund", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In schmutzig gr\u00fcner Tunke hockt", "tokens": ["In", "schmut\u00b7zig", "gr\u00fc\u00b7ner", "Tun\u00b7ke", "hockt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein blauer Syphilitiker.", "tokens": ["Ein", "blau\u00b7er", "Sy\u00b7phi\u00b7li\u00b7ti\u00b7ker", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Boxer bebt. Ein Baby bockt.", "tokens": ["Ein", "Bo\u00b7xer", "bebt", ".", "Ein", "Ba\u00b7by", "bockt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verstiert fault ein Zylinderherr.", "tokens": ["Ver\u00b7stiert", "fault", "ein", "Zy\u00b7lin\u00b7der\u00b7herr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Auto bringt ein Fr\u00e4ulein um.", "tokens": ["Ein", "Au\u00b7to", "bringt", "ein", "Fr\u00e4u\u00b7lein", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Junge bricht ein M\u00e4dchen an.", "tokens": ["Ein", "Jun\u00b7ge", "bricht", "ein", "M\u00e4d\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verbittert ist ein Mensch. Warum?", "tokens": ["Ver\u00b7bit\u00b7tert", "ist", "ein", "Mensch", ".", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil er nicht coitieren kann.", "tokens": ["Weil", "er", "nicht", "coi\u00b7tie\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}