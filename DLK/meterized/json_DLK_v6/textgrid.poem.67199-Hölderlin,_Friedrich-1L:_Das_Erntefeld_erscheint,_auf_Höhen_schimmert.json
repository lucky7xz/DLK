{"textgrid.poem.67199": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Erntefeld erscheint, auf H\u00f6hen schimmert", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Erntefeld erscheint, auf H\u00f6hen schimmert", "tokens": ["Das", "Ern\u00b7te\u00b7feld", "er\u00b7scheint", ",", "auf", "H\u00f6\u00b7hen", "schim\u00b7mert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hellen Wolke Pracht, indes am weiten Himmel", "tokens": ["Der", "hel\u00b7len", "Wol\u00b7ke", "Pracht", ",", "in\u00b7des", "am", "wei\u00b7ten", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In stiller Nacht die Zahl der Sterne flimmert,", "tokens": ["In", "stil\u00b7ler", "Nacht", "die", "Zahl", "der", "Ster\u00b7ne", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gro\u00df ist und weit von Wolken das Gewimmel.", "tokens": ["Gro\u00df", "ist", "und", "weit", "von", "Wol\u00b7ken", "das", "Ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Pfade gehn entfernter hin, der Menschen Leben,", "tokens": ["Die", "Pfa\u00b7de", "gehn", "ent\u00b7fern\u00b7ter", "hin", ",", "der", "Men\u00b7schen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es zeiget sich auf Meeren unverborgen,", "tokens": ["Es", "zei\u00b7get", "sich", "auf", "Mee\u00b7ren", "un\u00b7ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Sonne Tag ist zu der Menschen Streben", "tokens": ["Der", "Son\u00b7ne", "Tag", "ist", "zu", "der", "Men\u00b7schen", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein hohes Bild, und golden gl\u00e4nzt der Morgen.", "tokens": ["Ein", "ho\u00b7hes", "Bild", ",", "und", "gol\u00b7den", "gl\u00e4nzt", "der", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit neuen Farben ist geschm\u00fcckt der G\u00e4rten Breite,", "tokens": ["Mit", "neu\u00b7en", "Far\u00b7ben", "ist", "ge\u00b7schm\u00fcckt", "der", "G\u00e4r\u00b7ten", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mensch verwundert sich, da\u00df sein Bem\u00fchn gelinget,", "tokens": ["Der", "Mensch", "ver\u00b7wun\u00b7dert", "sich", ",", "da\u00df", "sein", "Be\u00b7m\u00fchn", "ge\u00b7lin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was er mit Tugend schafft, und was er hoch vollbringet,", "tokens": ["Was", "er", "mit", "Tu\u00b7gend", "schafft", ",", "und", "was", "er", "hoch", "voll\u00b7brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,", "KON", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es steht mit der Vergangenheit in pr\u00e4chtigem Geleite.", "tokens": ["Es", "steht", "mit", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "in", "pr\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}