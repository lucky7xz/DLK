{"textgrid.poem.33049": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Baum' hab' ich gestanden,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Baum' hab' ich gestanden,", "tokens": ["Am", "Baum'", "hab'", "ich", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der war so hoffnungsgr\u00fcn,", "tokens": ["Der", "war", "so", "hoff\u00b7nungs\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht lange mehr kann's dauern", "tokens": ["Nicht", "lan\u00b7ge", "mehr", "kann's", "dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und freudig wird er bl\u00fch'n.", "tokens": ["Und", "freu\u00b7dig", "wird", "er", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Zweiglein nur streckt trauernd", "tokens": ["Ein", "Zwei\u00b7glein", "nur", "streckt", "trau\u00b7ernd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Arme nach mir aus,", "tokens": ["Die", "Ar\u00b7me", "nach", "mir", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist so kahl und d\u00fcrre,", "tokens": ["Es", "ist", "so", "kahl", "und", "d\u00fcr\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schl\u00e4gt nirgends knospend aus.", "tokens": ["Schl\u00e4gt", "nir\u00b7gends", "knos\u00b7pend", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O, Zweiglein! was erwachest", "tokens": ["O", ",", "Zwei\u00b7glein", "!", "was", "er\u00b7wa\u00b7chest"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du nicht im Fr\u00fchlingshauch?", "tokens": ["Du", "nicht", "im", "Fr\u00fch\u00b7lings\u00b7hauch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne k\u00fc\u00dft die Fluren,", "tokens": ["Die", "Son\u00b7ne", "k\u00fc\u00dft", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie k\u00fcsset dich ja auch!", "tokens": ["Sie", "k\u00fcs\u00b7set", "dich", "ja", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lockt nicht des Himmels Bl\u00e4ue,", "tokens": ["Lockt", "nicht", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der lauen L\u00fcfte Weh'n,", "tokens": ["Der", "lau\u00b7en", "L\u00fcf\u00b7te", "Weh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich, wie die Nachbarzweige", "tokens": ["Dich", ",", "wie", "die", "Nach\u00b7bar\u00b7zwei\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Bl\u00fcthenschmuck zu steh'n?", "tokens": ["Im", "Bl\u00fc\u00b7then\u00b7schmuck", "zu", "steh'n", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df deine Rinde schwellen", "tokens": ["La\u00df", "dei\u00b7ne", "Rin\u00b7de", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von frischem Lebenssaft \u2013", "tokens": ["Von", "fri\u00b7schem", "Le\u00b7bens\u00b7saft", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, Zweiglein, ach! ich sehe", "tokens": ["Doch", ",", "Zwei\u00b7glein", ",", "ach", "!", "ich", "se\u00b7he"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "ITJ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir fehlt die inn're Kraft!", "tokens": ["Dir", "fehlt", "die", "inn'\u00b7re", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dein Mark, ach! ist erstorben,", "tokens": ["Dein", "Mark", ",", "ach", "!", "ist", "ers\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$.", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Winterfrost verzehrt,", "tokens": ["Vom", "Win\u00b7ter\u00b7frost", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein zartes Leben haben", "tokens": ["Dein", "zar\u00b7tes", "Le\u00b7ben", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die St\u00fcrme rauh zerst\u00f6rt.", "tokens": ["Die", "St\u00fcr\u00b7me", "rauh", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "F\u00fcr dich scheint keine Sonne,", "tokens": ["F\u00fcr", "dich", "scheint", "kei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weht keine Fr\u00fchlingsluft,", "tokens": ["Weht", "kei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir sind die Lenzgefilde", "tokens": ["Dir", "sind", "die", "Lenz\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur eine Todtengruft. \u2013", "tokens": ["Nur", "ei\u00b7ne", "Tod\u00b7ten\u00b7gruft", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich gehe still von dannen,", "tokens": ["Ich", "ge\u00b7he", "still", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und denk' an dich zur\u00fcck,", "tokens": ["Und", "denk'", "an", "dich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und an so mancher Herzen", "tokens": ["Und", "an", "so", "man\u00b7cher", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dahin gewelktes Gl\u00fcck.", "tokens": ["Da\u00b7hin", "ge\u00b7welk\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In deren zarte Bl\u00fcthe", "tokens": ["In", "de\u00b7ren", "zar\u00b7te", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch drang so eisig Weh'n,", "tokens": ["Auch", "drang", "so", "ei\u00b7sig", "Weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df unter den Lebend'gen", "tokens": ["Da\u00df", "un\u00b7ter", "den", "Le\u00b7ben\u00b7d'\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie wie Gestorb'ne steh'n!", "tokens": ["Sie", "wie", "Ge\u00b7stor\u00b7b'\u00b7ne", "steh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}