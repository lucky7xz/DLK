{"textgrid.poem.43235": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Fremde starb. \u2013 Nur ich sah sie verlodern.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fremde starb. \u2013 Nur ich sah sie verlodern.", "tokens": ["Die", "Frem\u00b7de", "starb", ".", "\u2013", "Nur", "ich", "sah", "sie", "ver\u00b7lo\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADV", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erloschen ist ihr Leben und ihr Lieben.", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "ihr", "Le\u00b7ben", "und", "ihr", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Knabe ist als Erbe mir geblieben,", "tokens": ["Der", "Kna\u00b7be", "ist", "als", "Er\u00b7be", "mir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie ist dahin \u2013 und bald wird sie vermodern.", "tokens": ["Sie", "ist", "da\u00b7hin", "\u2013", "und", "bald", "wird", "sie", "ver\u00b7mo\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$(", "KON", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf einem H\u00fcgel hab' ich sie begraben,", "tokens": ["Auf", "ei\u00b7nem", "H\u00fc\u00b7gel", "hab'", "ich", "sie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um den sich Epheu dicht und Reben schlingen,", "tokens": ["Um", "den", "sich", "E\u00b7pheu", "dicht", "und", "Re\u00b7ben", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PRF", "NN", "ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und morgen will ich ihr ein Kreuzlein bringen", "tokens": ["Und", "mor\u00b7gen", "will", "ich", "ihr", "ein", "Kreuz\u00b7lein", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wilde Rosen soll sie n\u00e4chstens haben.", "tokens": ["Und", "wil\u00b7de", "Ro\u00b7sen", "soll", "sie", "n\u00e4chs\u00b7tens", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir ist so weh! \u2013 ich h\u00f6r' den Knaben lallen,", "tokens": ["Mir", "ist", "so", "weh", "!", "\u2013", "ich", "h\u00f6r'", "den", "Kna\u00b7ben", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er l\u00e4\u00dft sich l\u00e4chelnd von den Hunden tragen,", "tokens": ["Er", "l\u00e4\u00dft", "sich", "l\u00e4\u00b7chelnd", "von", "den", "Hun\u00b7den", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lauscht auf die Drosseln, die im Busche schlagen,", "tokens": ["Lauscht", "auf", "die", "Dros\u00b7seln", ",", "die", "im", "Bu\u00b7sche", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein V\u00f6glein selbst \u2013 das aus dem Nest gefallen. \u2013", "tokens": ["Ein", "V\u00f6\u00b7glein", "selbst", "\u2013", "das", "aus", "dem", "Nest", "ge\u00b7fal\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$(", "PDS", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}