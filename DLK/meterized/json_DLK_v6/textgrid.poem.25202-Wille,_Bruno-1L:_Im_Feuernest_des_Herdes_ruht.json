{"textgrid.poem.25202": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Feuernest des Herdes ruht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Feuernest des Herdes ruht", "tokens": ["Im", "Feu\u00b7er\u00b7nest", "des", "Her\u00b7des", "ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verloren mein d\u00fcstres Auge;", "tokens": ["Ver\u00b7lo\u00b7ren", "mein", "d\u00fcst\u00b7res", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und gr\u00fcbelnd starrt die Glut", "tokens": ["Und", "gr\u00fc\u00b7belnd", "starrt", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur\u00fcck mit rotem Auge. \u2013", "tokens": ["Zu\u00b7r\u00fcck", "mit", "ro\u00b7tem", "Au\u00b7ge", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Glut, was starrst du?", "tokens": ["Glut", ",", "was", "starrst", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Drau\u00dfen an der Mauer r\u00fcttelt", "tokens": ["Drau\u00b7\u00dfen", "an", "der", "Mau\u00b7er", "r\u00fct\u00b7telt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Sturm mit drohendem Gebraus;", "tokens": ["Der", "Sturm", "mit", "dro\u00b7hen\u00b7dem", "Ge\u00b7braus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An morschen Gliedern", "tokens": ["An", "mor\u00b7schen", "Glie\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zittert das Haus, \u2013", "tokens": ["Zit\u00b7tert", "das", "Haus", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie ein zagender Greis.", "tokens": ["Wie", "ein", "za\u00b7gen\u00b7der", "Greis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ein Bangen kommt geschlichen", "tokens": ["Ein", "Ban\u00b7gen", "kommt", "ge\u00b7schli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und fl\u00fcstert in mein Ohr;", "tokens": ["Und", "fl\u00fcs\u00b7tert", "in", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zur Decke huscht", "tokens": ["Und", "zur", "De\u00b7cke", "huscht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Scheu mein Blick empor:", "tokens": ["Scheu", "mein", "Blick", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn die Decke birst, \u2013!", "tokens": ["Wenn", "die", "De\u00b7cke", "birst", ",", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Da raunt es und zischelt:", "tokens": ["Da", "raunt", "es", "und", "zi\u00b7schelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbja, ducke dich nur", "tokens": ["\u00bb", "ja", ",", "du\u00b7cke", "dich", "nur"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und drehe die Augen nach oben!", "tokens": ["Und", "dre\u00b7he", "die", "Au\u00b7gen", "nach", "o\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sieh die Faust der Vernichtung erhoben!", "tokens": ["Sieh", "die", "Faust", "der", "Ver\u00b7nich\u00b7tung", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Horch, wie die Balken st\u00f6hnen!", "tokens": ["Horch", ",", "wie", "die", "Bal\u00b7ken", "st\u00f6h\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie ahnen, da\u00df dies Haus", "tokens": ["Sie", "ah\u00b7nen", ",", "da\u00df", "dies", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PDS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Einst im Sturze dr\u00f6hnen", "tokens": ["Einst", "im", "Stur\u00b7ze", "dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wird wie ein gef\u00e4llter Riese.", "tokens": ["Wird", "wie", "ein", "ge\u00b7f\u00e4ll\u00b7ter", "Rie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn dann der aufgewirbelte Staub", "tokens": ["Wenn", "dann", "der", "auf\u00b7ge\u00b7wir\u00b7bel\u00b7te", "Staub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich senkt auf w\u00fcste Tr\u00fcmmer,", "tokens": ["Sich", "senkt", "auf", "w\u00fcs\u00b7te", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kommt das Unkraut", "tokens": ["Kommt", "das", "Un\u00b7kraut"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mit tastender Wurzel", "tokens": ["Mit", "tas\u00b7ten\u00b7der", "Wur\u00b7zel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Geschlichen und w\u00fchlt sich", "tokens": ["Ge\u00b7schli\u00b7chen", "und", "w\u00fchlt", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In morsches Gestein;", "tokens": ["In", "mor\u00b7sches", "Ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Halb vergraben aber im Schutt,", "tokens": ["Halb", "ver\u00b7gra\u00b7ben", "a\u00b7ber", "im", "Schutt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Lugt zum m\u00fcrrischen Himmel empor", "tokens": ["Lugt", "zum", "m\u00fcr\u00b7ri\u00b7schen", "Him\u00b7mel", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Mit leeren Augenh\u00f6hlen", "tokens": ["Mit", "lee\u00b7ren", "Au\u00b7gen\u00b7h\u00f6h\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein bleicher Sch\u00e4del \u2013", "tokens": ["Ein", "blei\u00b7cher", "Sch\u00e4\u00b7del", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Dein kostbar Haupt!\u00ab \u2013 \u2013", "tokens": ["Dein", "kost\u00b7bar", "Haupt", "!", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Mein Haupt! \u2013", "tokens": ["Mein", "Haupt", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Was ich bedient mit t\u00e4glicher Plage,", "tokens": ["Was", "ich", "be\u00b7dient", "mit", "t\u00e4g\u00b7li\u00b7cher", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie eine Mutter ihr einziges Kind,", "tokens": ["Wie", "ei\u00b7ne", "Mut\u00b7ter", "ihr", "ein\u00b7zi\u00b7ges", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ich beim Rascheln der Gefahr", "tokens": ["Was", "ich", "beim", "Ra\u00b7scheln", "der", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geschirmt mit zuckendem Arm \u2013", "tokens": ["Ge\u00b7schirmt", "mit", "zu\u00b7cken\u00b7dem", "Arm", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Das liegt nun hoffnungslos verworfen", "tokens": ["Das", "liegt", "nun", "hoff\u00b7nungs\u00b7los", "ver\u00b7wor\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zwischen Schutt und h\u00e4mischem Unkraut,", "tokens": ["Zwi\u00b7schen", "Schutt", "und", "h\u00e4\u00b7mi\u00b7schem", "Un\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wie ein zertr\u00fcmmertes Thongef\u00e4\u00df! ...", "tokens": ["Wie", "ein", "zer\u00b7tr\u00fcm\u00b7mer\u00b7tes", "Thon\u00b7ge\u00b7f\u00e4\u00df", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Im Feuernest des Herdes ruht", "tokens": ["Im", "Feu\u00b7er\u00b7nest", "des", "Her\u00b7des", "ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verloren mein angstvoll Auge;", "tokens": ["Ver\u00b7lo\u00b7ren", "mein", "angst\u00b7voll", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und gr\u00fcbelnd starrt die Glut", "tokens": ["Und", "gr\u00fc\u00b7belnd", "starrt", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur\u00fcck mit rotem Auge.", "tokens": ["Zu\u00b7r\u00fcck", "mit", "ro\u00b7tem", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Glut, was starrst du? \u2013", "tokens": ["Glut", ",", "was", "starrst", "du", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Da spr\u00fcht es in dem roten Auge", "tokens": ["Da", "spr\u00fcht", "es", "in", "dem", "ro\u00b7ten", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begeistert auf;", "tokens": ["Be\u00b7geis\u00b7tert", "auf", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Heiliges Feuer wallt empor", "tokens": ["Hei\u00b7li\u00b7ges", "Feu\u00b7er", "wallt", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und st\u00fcrzt auf meine Seele", "tokens": ["Und", "st\u00fcrzt", "auf", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie einer Sturmflut Woge;", "tokens": ["Wie", "ei\u00b7ner", "Sturm\u00b7flut", "Wo\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und die Flammen singen summend", "tokens": ["Und", "die", "Flam\u00b7men", "sin\u00b7gen", "sum\u00b7mend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie Orgelton, wie Sturmesbrausen:", "tokens": ["Wie", "Or\u00b7gel\u00b7ton", ",", "wie", "Stur\u00b7mes\u00b7brau\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbgieb es auf, dein nichtig Haupt!", "tokens": ["\u00bb", "gieb", "es", "auf", ",", "dein", "nich\u00b7tig", "Haupt", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dann magst du es getrost verlieren.", "tokens": ["Dann", "magst", "du", "es", "ge\u00b7trost", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sei gleich uns, verbrenne dich!", "tokens": ["Sei", "gleich", "uns", ",", "ver\u00b7bren\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Viele tasten im Dunkeln und frieren.", "tokens": ["Vie\u00b7le", "tas\u00b7ten", "im", "Dun\u00b7keln", "und", "frie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "KON", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Sieh die fromme Flammenrose", "tokens": ["Sieh", "die", "from\u00b7me", "Flam\u00b7men\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Bl\u00e4tter\u00fcppig bl\u00fchen,", "tokens": ["Bl\u00e4t\u00b7te\u00b7r\u00fcp\u00b7pig", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Licht und W\u00e4rme, Liebesgaben,", "tokens": ["Licht", "und", "W\u00e4r\u00b7me", ",", "Lie\u00b7bes\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ihrem Kelch entspr\u00fchen! \u2013", "tokens": ["Ih\u00b7rem", "Kelch", "ent\u00b7spr\u00fc\u00b7hen", "!", "\u2013"], 