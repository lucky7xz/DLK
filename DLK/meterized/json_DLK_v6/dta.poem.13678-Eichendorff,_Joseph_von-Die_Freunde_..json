{"dta.poem.13678": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Qualm dann klingend brach die Morgenstunde,", "tokens": ["Durch", "Qualm", "dann", "klin\u00b7gend", "brach", "die", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da schweiften Ritter blank durch Nebelstreifen,", "tokens": ["Da", "schweif\u00b7ten", "Rit\u00b7ter", "blank", "durch", "Ne\u00b7bel\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Winde scharf, die auf der Haide pfeifen,", "tokens": ["Durch", "Win\u00b7de", "scharf", ",", "die", "auf", "der", "Hai\u00b7de", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Harfner sang, lobt' Gott aus Herzensgrunde.", "tokens": ["Ein", "Harf\u00b7ner", "sang", ",", "lobt'", "Gott", "aus", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}