{"textgrid.poem.60380": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rings trauern die Entlaubten,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rings trauern die Entlaubten,", "tokens": ["Rings", "trau\u00b7ern", "die", "Ent\u00b7laub\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom kalten Wind durchweht,", "tokens": ["Vom", "kal\u00b7ten", "Wind", "durch\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tannen nur behaupten", "tokens": ["Die", "Tan\u00b7nen", "nur", "be\u00b7haup\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr dunkles Gr\u00fcn so sp\u00e4t.", "tokens": ["Ihr", "dunk\u00b7les", "Gr\u00fcn", "so", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenns Voglern baut sein Lager,", "tokens": ["Wenns", "Vog\u00b7lern", "baut", "sein", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So gr\u00fcnt das Tannenreis", "tokens": ["So", "gr\u00fcnt", "das", "Tan\u00b7nen\u00b7reis"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gr\u00fcnt, wenns Wild sich hager", "tokens": ["Und", "gr\u00fcnt", ",", "wenns", "Wild", "sich", "ha\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NE", "PRF", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Scharrt Wurzeln aus dem Eis.", "tokens": ["Scharrt", "Wur\u00b7zeln", "aus", "dem", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Buche seh ich schwinden", "tokens": ["Die", "Bu\u00b7che", "seh", "ich", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Froste, lebenssatt,", "tokens": ["Im", "Fros\u00b7te", ",", "le\u00b7bens\u00b7satt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie den kalten Winden", "tokens": ["Wie", "sie", "den", "kal\u00b7ten", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinwirft das letzte Blatt.", "tokens": ["Hin\u00b7wirft", "das", "letz\u00b7te", "Blatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu meiner Seele Trauer", "tokens": ["Zu", "mei\u00b7ner", "See\u00b7le", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Buche besser stimmt,", "tokens": ["Die", "Bu\u00b7che", "bes\u00b7ser", "stimmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie den Winterschauer", "tokens": ["Da\u00df", "sie", "den", "Win\u00b7ter\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich so zu Herzen nimmt.", "tokens": ["Sich", "so", "zu", "Her\u00b7zen", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}