{"textgrid.poem.37364": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Habt ihr denn wirklich keinen Schimmer", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr denn wirklich keinen Schimmer", "tokens": ["Habt", "ihr", "denn", "wirk\u00b7lich", "kei\u00b7nen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Angst, da\u00df ihr noch ruhig schlaft?", "tokens": ["Von", "Angst", ",", "da\u00df", "ihr", "noch", "ru\u00b7hig", "schlaft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird denn in dieser Welt nicht immer", "tokens": ["Wird", "denn", "in", "die\u00b7ser", "Welt", "nicht", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das Leben mit dem Tod bestraft?", "tokens": ["Das", "Le\u00b7ben", "mit", "dem", "Tod", "be\u00b7straft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr lebt vergn\u00fcgt trotz dem Verh\u00e4ngnis,", "tokens": ["Ihr", "lebt", "ver\u00b7gn\u00fcgt", "trotz", "dem", "Ver\u00b7h\u00e4ng\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das n\u00e4her stets und n\u00e4her zieht.", "tokens": ["Das", "n\u00e4\u00b7her", "stets", "und", "n\u00e4\u00b7her", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So stiehlt der Dieb, dem das Gef\u00e4ngnis", "tokens": ["So", "stiehlt", "der", "Dieb", ",", "dem", "das", "Ge\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sp\u00e4ter gar der Galgen bl\u00fcht.", "tokens": ["Und", "sp\u00e4\u00b7ter", "gar", "der", "Gal\u00b7gen", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r auf, entgegnet frech die Jugend,", "tokens": ["H\u00f6r", "auf", ",", "ent\u00b7geg\u00b7net", "frech", "die", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du altes Jammerinstrument.", "tokens": ["Du", "al\u00b7tes", "Jam\u00b7me\u00b7rinst\u00b7ru\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man merkt es gleich: du bist die Tugend", "tokens": ["Man", "merkt", "es", "gleich", ":", "du", "bist", "die", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die keinem sein Vergn\u00fcgen g\u00f6nnt.", "tokens": ["Die", "kei\u00b7nem", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}