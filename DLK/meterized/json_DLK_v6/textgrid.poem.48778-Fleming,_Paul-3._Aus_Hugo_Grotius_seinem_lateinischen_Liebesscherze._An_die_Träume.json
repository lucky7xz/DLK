{"textgrid.poem.48778": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Aus Hugo Grotius seinem lateinischen Liebesscherze. An die Tr\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Tr\u00e4ume, die ihr seid das beste Teil im Leben,", "tokens": ["Ihr", "Tr\u00e4u\u00b7me", ",", "die", "ihr", "seid", "das", "bes\u00b7te", "Teil", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Nichts als Tr\u00fcbn\u00fc\u00df ist, die ihr euch habet mein", "tokens": ["das", "Nichts", "als", "Tr\u00fcb\u00b7n\u00fc\u00df", "ist", ",", "die", "ihr", "euch", "ha\u00b7bet", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "KOKOM", "NN", "VAFIN", "$,", "PRELS", "PPER", "PPER", "VAFIN", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so oft und oft erbarmt, so es mag sicher sein,", "tokens": ["so", "oft", "und", "oft", "er\u00b7barmt", ",", "so", "es", "mag", "si\u00b7cher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VVPP", "$,", "ADV", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "da\u00df man die Warheit sagt, mehr Ehre soll ich geben", "tokens": ["da\u00df", "man", "die", "War\u00b7heit", "sagt", ",", "mehr", "Eh\u00b7re", "soll", "ich", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "euch, als der Liebsten selbst. Durch euch seh' ich sie schweben", "tokens": ["euch", ",", "als", "der", "Liebs\u00b7ten", "selbst", ".", "Durch", "euch", "seh'", "ich", "sie", "schwe\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "ADV", "$.", "APPR", "PPER", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "vor mir so gut und from, ohn' allen falschen Schein.", "tokens": ["vor", "mir", "so", "gut", "und", "from", ",", "ohn'", "al\u00b7len", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm ich denn drauf zu ihr, so spricht sie lauter nein!", "tokens": ["Komm", "ich", "denn", "drauf", "zu", "ihr", ",", "so", "spricht", "sie", "lau\u00b7ter", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und machts ihr unbekant. Sie z\u00fcrnet noch darneben,", "tokens": ["und", "machts", "ihr", "un\u00b7be\u00b7kant", ".", "Sie", "z\u00fcr\u00b7net", "noch", "dar\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "da\u00df ich ein wenig mich ergetzen will an ihr.", "tokens": ["da\u00df", "ich", "ein", "we\u00b7nig", "mich", "er\u00b7get\u00b7zen", "will", "an", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "PRF", "VVINF", "VMFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was k\u00f6nnen, Neidische, dir meine N\u00e4cht' entf\u00fchren?", "tokens": ["Was", "k\u00f6n\u00b7nen", ",", "Nei\u00b7di\u00b7sche", ",", "dir", "mei\u00b7ne", "N\u00e4cht'", "ent\u00b7f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "NN", "$,", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch gleichwol will sich nicht dein stolzer Zorn verlieren.", "tokens": ["Noch", "gleich\u00b7wol", "will", "sich", "nicht", "dein", "stol\u00b7zer", "Zorn", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer aber w\u00e4r' als ich gl\u00fcckseliger allhier,", "tokens": ["Wer", "a\u00b7ber", "w\u00e4r'", "als", "ich", "gl\u00fcck\u00b7se\u00b7li\u00b7ger", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "KOUS", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wenn nur der s\u00fc\u00dfe Schlaf, in dem ich viel mu\u00df lachen,", "tokens": ["wenn", "nur", "der", "s\u00fc\u00b7\u00dfe", "Schlaf", ",", "in", "dem", "ich", "viel", "mu\u00df", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "sich kehrte bei mir um und w\u00fcrd' ein solches Wachen.", "tokens": ["sich", "kehr\u00b7te", "bei", "mir", "um", "und", "w\u00fcrd'", "ein", "sol\u00b7ches", "Wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPER", "PTKVZ", "KON", "VAFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}