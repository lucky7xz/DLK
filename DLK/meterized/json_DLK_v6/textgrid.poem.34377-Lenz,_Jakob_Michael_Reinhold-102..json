{"textgrid.poem.34377": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja theures Paar! das schon auf manchen Dornen-Wegen", "tokens": ["Ja", "theu\u00b7res", "Paar", "!", "das", "schon", "auf", "man\u00b7chen", "Dor\u00b7nen\u00b7We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADJA", "NN", "$.", "ART", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dieser Pilger-Welt mit M\u00fch gewandelt hat,", "tokens": ["In", "die\u00b7ser", "Pil\u00b7ger\u00b7Welt", "mit", "M\u00fch", "ge\u00b7wan\u00b7delt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Genie\u00dfe nur im Herbst den ganzen Erndte-Segen", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "nur", "im", "Herbst", "den", "gan\u00b7zen", "Ernd\u00b7te\u00b7Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von jeder Pr\u00fcfungszeit, von jeder Tr\u00e4hnen Saat.", "tokens": ["Von", "je\u00b7der", "Pr\u00fc\u00b7fungs\u00b7zeit", ",", "von", "je\u00b7der", "Tr\u00e4h\u00b7nen", "Saat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar lacht nicht mehr der Herbst sowie ein Fr\u00fchlings-Morgen,", "tokens": ["Zwar", "lacht", "nicht", "mehr", "der", "Herbst", "so\u00b7wie", "ein", "Fr\u00fch\u00b7lings\u00b7Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alles \u00fcbersonnt und Feld und Fluhr verj\u00fcngt,", "tokens": ["Der", "al\u00b7les", "\u00fc\u00b7ber\u00b7sonnt", "und", "Feld", "und", "Fluhr", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nie schl\u00e4ft ein Silber-Haupt so frey von allen Sorgen,", "tokens": ["Nie", "schl\u00e4ft", "ein", "Sil\u00b7ber\u00b7Haupt", "so", "frey", "von", "al\u00b7len", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie noch der J\u00fcngling schl\u00e4ft, dem alles Freude bringt.", "tokens": ["Wie", "noch", "der", "J\u00fcng\u00b7ling", "schl\u00e4ft", ",", "dem", "al\u00b7les", "Freu\u00b7de", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Daf\u00fcr ist auch der Greis schon viele Schritte weiter,", "tokens": ["Da\u00b7f\u00fcr", "ist", "auch", "der", "Greis", "schon", "vie\u00b7le", "Schrit\u00b7te", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon manchen Berg \u2013 den noch der J\u00fcngling steigen mu\u00df. \u2013", "tokens": ["Schon", "man\u00b7chen", "Berg", "\u2013", "den", "noch", "der", "J\u00fcng\u00b7ling", "stei\u00b7gen", "mu\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "ART", "ADV", "ART", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch ein November-Tag ist Dankenswehrt, wenn heiter", "tokens": ["Auch", "ein", "No\u00b7vem\u00b7ber\u00b7Tag", "ist", "Dan\u00b7kens\u00b7wehrt", ",", "wenn", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Sonne Strahl ihn gr\u00fc\u00dft. \u2013 Dank auch dem kurzen Gru\u00df! \u2013", "tokens": ["Der", "Son\u00b7ne", "Strahl", "ihn", "gr\u00fc\u00dft", ".", "\u2013", "Dank", "auch", "dem", "kur\u00b7zen", "Gru\u00df", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$.", "$(", "APPR", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So gr\u00fc\u00dfet jetzt auch Euch nach manchen tr\u00fcben Tagen", "tokens": ["So", "gr\u00fc\u00b7\u00dfet", "jetzt", "auch", "Euch", "nach", "man\u00b7chen", "tr\u00fc\u00b7ben", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein sonnigt froher Tag, der Freuden bringend lacht.", "tokens": ["Ein", "son\u00b7nigt", "fro\u00b7her", "Tag", ",", "der", "Freu\u00b7den", "brin\u00b7gend", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und gleich der Nacht, entfliehn vor ihm jetzt Schmerz und Klagen,", "tokens": ["Und", "gleich", "der", "Nacht", ",", "ent\u00b7fliehn", "vor", "ihm", "jetzt", "Schmerz", "und", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "VVFIN", "APPR", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Freuden werden Euch gl\u00fcckw\u00fcnschend dargebracht. \u2013", "tokens": ["Und", "Freu\u00b7den", "wer\u00b7den", "Euch", "gl\u00fcck\u00b7w\u00fcn\u00b7schend", "dar\u00b7ge\u00b7bracht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dort rief einst Gott! \u2013 dein Knecht, o Sonne stehe stille,", "tokens": ["Dort", "rief", "einst", "Gott", "!", "\u2013", "dein", "Knecht", ",", "o", "Son\u00b7ne", "ste\u00b7he", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$.", "$(", "PPOSAT", "NN", "$,", "FM", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf dein Allmachts-Wort mu\u00dft sie nicht untergehn.", "tokens": ["Und", "auf", "dein", "All\u00b7machts\u00b7Wort", "mu\u00dft", "sie", "nicht", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sieh Kinder, Enkel, hier dir flehn: Ists Herr dein Wille,", "tokens": ["Sieh", "Kin\u00b7der", ",", "En\u00b7kel", ",", "hier", "dir", "flehn", ":", "Ists", "Herr", "dein", "Wil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "ADV", "PPER", "VVINF", "$.", "NE", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So la\u00df dem theuren Paar der Freuden Sonne stille stehn. \u2013", "tokens": ["So", "la\u00df", "dem", "theu\u00b7ren", "Paar", "der", "Freu\u00b7den", "Son\u00b7ne", "stil\u00b7le", "stehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Sanft flie\u00df er Ihnen fort, der Herbst des theuren Lebens,", "tokens": ["Sanft", "flie\u00df", "er", "Ih\u00b7nen", "fort", ",", "der", "Herbst", "des", "theu\u00b7ren", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Dir geheiligt war. \u2013 Der Rest sey Sonnen-Schein,", "tokens": ["Das", "Dir", "ge\u00b7hei\u00b7ligt", "war", ".", "\u2013", "Der", "Rest", "sey", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "$.", "$(", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und heiter jeder Tag, uns Muster des Bestrebens", "tokens": ["Und", "hei\u00b7ter", "je\u00b7der", "Tag", ",", "uns", "Mus\u00b7ter", "des", "Be\u00b7stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PIAT", "NN", "$,", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch einst in unserm Herbst so fromm und froh zu seyn. \u2013", "tokens": ["Auch", "einst", "in", "un\u00b7serm", "Herbst", "so", "fromm", "und", "froh", "zu", "seyn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}