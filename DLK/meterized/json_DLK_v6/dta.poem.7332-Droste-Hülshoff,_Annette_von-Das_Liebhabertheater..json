{"dta.poem.7332": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Liebhabertheater.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Meinst du, wir h\u00e4tten jetzt Decemberschnee?", "tokens": ["Meinst", "du", ",", "wir", "h\u00e4t\u00b7ten", "jetzt", "De\u00b7cem\u00b7ber\u00b7schnee", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch eben stand ich vor dem sch\u00f6nsten Hain,", "tokens": ["Noch", "e\u00b7ben", "stand", "ich", "vor", "dem", "sch\u00f6ns\u00b7ten", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So gr\u00fcn und kr\u00e4ftig sah ich keinen je.", "tokens": ["So", "gr\u00fcn", "und", "kr\u00e4f\u00b7tig", "sah", "ich", "kei\u00b7nen", "je", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "PIAT", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Windsbraut fuhr, der Donner knallte drein,", "tokens": ["Die", "Winds\u00b7braut", "fuhr", ",", "der", "Don\u00b7ner", "knall\u00b7te", "drein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und seine Zweige trotzten wie gegossen,", "tokens": ["Und", "sei\u00b7ne", "Zwei\u00b7ge", "trotz\u00b7ten", "wie", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gleich an des Parkes Thor ein H\u00e4uschen stand,", "tokens": ["Gleich", "an", "des", "Par\u00b7kes", "Thor", "ein", "H\u00e4usc\u00b7hen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit Kr\u00e4nzen war geschm\u00fcckt die schlichte Wand,", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", "war", "ge\u00b7schm\u00fcckt", "die", "schlich\u00b7te", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die haben nicht gezittert vor den Schlossen,", "tokens": ["Die", "ha\u00b7ben", "nicht", "ge\u00b7zit\u00b7tert", "vor", "den", "Schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das nenn' ich Kr\u00e4nze doch und einen Hain:", "tokens": ["Das", "nenn'", "ich", "Kr\u00e4n\u00b7ze", "doch", "und", "ei\u00b7nen", "Hain", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "ADV", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und denkst du wohl, wir h\u00e4tten finstre Nacht?", "tokens": ["Und", "denkst", "du", "wohl", ",", "wir", "h\u00e4t\u00b7ten", "finst\u00b7re", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Morgens Gluten wallten eben noch,", "tokens": ["Des", "Mor\u00b7gens", "Glu\u00b7ten", "wall\u00b7ten", "e\u00b7ben", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nothgl\u00fchend, wie des Lavastromes Macht", "tokens": ["Noth\u00b7gl\u00fc\u00b7hend", ",", "wie", "des", "La\u00b7vast\u00b7ro\u00b7mes", "Macht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hernieder knistert von Vesuves Joch;", "tokens": ["Her\u00b7nie\u00b7der", "knis\u00b7tert", "von", "Ve\u00b7su\u00b7ves", "Joch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nie sah so pr\u00e4chtig man Auroren ziehen!", "tokens": ["Nie", "sah", "so", "pr\u00e4ch\u00b7tig", "man", "Au\u00b7ro\u00b7ren", "zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An unsre Augen schlugen wir die Hand,", "tokens": ["An", "uns\u00b7re", "Au\u00b7gen", "schlu\u00b7gen", "wir", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dachten schier, der Felsen steh' in Brand,", "tokens": ["Und", "dach\u00b7ten", "schier", ",", "der", "Fel\u00b7sen", "steh'", "in", "Brand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Hirten sahn wir wie D\u00e4mone gl\u00fchen;", "tokens": ["Die", "Hir\u00b7ten", "sahn", "wir", "wie", "D\u00e4\u00b7mo\u00b7ne", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das nenn' ich einen Sonnenaufgang doch!", "tokens": ["Das", "nenn'", "ich", "ei\u00b7nen", "Son\u00b7nen\u00b7auf\u00b7gang", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und sprichst du unsres Landes Nymphen Hohn?", "tokens": ["Und", "sprichst", "du", "uns\u00b7res", "Lan\u00b7des", "Nym\u00b7phen", "Hohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch eben schl\u00fcpfte durch des Forstes Hau", "tokens": ["Noch", "e\u00b7ben", "schl\u00fcpf\u00b7te", "durch", "des", "Fors\u00b7tes", "Hau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein M\u00e4dchen, voll und sinnig wie der Mohn,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "voll", "und", "sin\u00b7nig", "wie", "der", "Mohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gewi\u00df, sie war die allersch\u00f6nste Frau!", "tokens": ["Ge\u00b7wi\u00df", ",", "sie", "war", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr wei\u00dfes H\u00e4ndchen hielt den blanken Spaten,", "tokens": ["Ihr", "wei\u00b7\u00dfes", "H\u00e4nd\u00b7chen", "hielt", "den", "blan\u00b7ken", "Spa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der kleine Fu\u00df, in Zwickelstrumpf und Schuh,", "tokens": ["Der", "klei\u00b7ne", "Fu\u00df", ",", "in", "Zwi\u00b7ckel\u00b7strumpf", "und", "Schuh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hob sich so schwebend, trat so zierlich zu,", "tokens": ["Hob", "sich", "so", "schwe\u00b7bend", ",", "trat", "so", "zier\u00b7lich", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "$,", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und h\u00f6r', ich will es dir nur gleich verrathen,", "tokens": ["Und", "h\u00f6r'", ",", "ich", "will", "es", "dir", "nur", "gleich", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der sch\u00f6nen Clara glich sie ganz genau.", "tokens": ["Der", "sch\u00f6\u00b7nen", "Cla\u00b7ra", "glich", "sie", "ganz", "ge\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sagst du, diese habe mein gelacht?", "tokens": ["Und", "sagst", "du", ",", "die\u00b7se", "ha\u00b7be", "mein", "ge\u00b7lacht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O h\u00e4ttest du sie heute nur gesehn,", "tokens": ["O", "h\u00e4t\u00b7test", "du", "sie", "heu\u00b7te", "nur", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie schlau sie meine Blicke hat bewacht,", "tokens": ["Wie", "schlau", "sie", "mei\u00b7ne", "Bli\u00b7cke", "hat", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie z\u00e4rtlich konnte ihre Augen drehn,", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "konn\u00b7te", "ih\u00b7re", "Au\u00b7gen", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und welche s\u00fc\u00dfe Worte ihr entquollen!", "tokens": ["Und", "wel\u00b7che", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", "ihr", "ent\u00b7quol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Recht wo ich stand, dorthin hat sie geweint:", "tokens": ["Recht", "wo", "ich", "stand", ",", "dor\u00b7thin", "hat", "sie", "ge\u00b7weint", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.7": {"text": "\u201emein theures Herz, mein Leben, einz'ger Freund!\u201c", "tokens": ["\u201e", "mein", "theu\u00b7res", "Herz", ",", "mein", "Le\u00b7ben", ",", "einz'\u00b7ger", "Freund", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das schien ihr von den Lippen nur zu rollen.", "tokens": ["Das", "schien", "ihr", "von", "den", "Lip\u00b7pen", "nur", "zu", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "War das nicht richtig angebracht, und sch\u00f6n?", "tokens": ["War", "das", "nicht", "rich\u00b7tig", "an\u00b7ge\u00b7bracht", ",", "und", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "VVPP", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch Eins nur, Eines noch verhehlt' ich dir,", "tokens": ["Doch", "Eins", "nur", ",", "Ei\u00b7nes", "noch", "ver\u00b7hehlt'", "ich", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "PIS", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fcrchte sehr, es trage wenig ein;", "tokens": ["Und", "f\u00fcrch\u00b7te", "sehr", ",", "es", "tra\u00b7ge", "we\u00b7nig", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wald war brettern und der Kranz Papier,", "tokens": ["Der", "Wald", "war", "bret\u00b7tern", "und", "der", "Kranz", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Morgenroth Bengalens Feuerschein,", "tokens": ["Das", "Mor\u00b7gen\u00b7roth", "Ben\u00b7ga\u00b7lens", "Feu\u00b7er\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und als sie lie\u00df so s\u00fc\u00dfe Worte wandern,", "tokens": ["Und", "als", "sie", "lie\u00df", "so", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ach, ob sie gleich dabei mich angeblickt,", "tokens": ["Ach", ",", "ob", "sie", "gleich", "da\u00b7bei", "mich", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "PAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der dicht an das Orchester war ger\u00fcckt,", "tokens": ["Der", "dicht", "an", "das", "Or\u00b7ches\u00b7ter", "war", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Doch f\u00fcrcht' ich fast, sie galten einem Andern!", "tokens": ["Doch", "f\u00fcrcht'", "ich", "fast", ",", "sie", "gal\u00b7ten", "ei\u00b7nem", "An\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Was meinst du, sollte das wohl m\u00f6glich seyn?", "tokens": ["Was", "meinst", "du", ",", "soll\u00b7te", "das", "wohl", "m\u00f6g\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VMFIN", "PDS", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}