{"dta.poem.10166": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abermahlige Betrachtung G\u00f6ttlicher  \n Gr\u00f6sse im Kleinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da unser Sch\u00f6pfer in dem Grossen, und auch im Klei-\nnen, kurtz: in allen", "tokens": ["Da", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "in", "dem", "Gros\u00b7sen", ",", "und", "auch", "im", "Klei", "nen", ",", "kurtz", ":", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "KON", "ADV", "APPRART", "TRUNC", "VVINF", "$,", "ADJD", "$.", "APPR", "PIAT"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Unendlich gegenw\u00e4rtig ist, als worin unsers Geistes Augen", "tokens": ["Un\u00b7end\u00b7lich", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "ist", ",", "als", "wo\u00b7rin", "un\u00b7sers", "Geis\u00b7tes", "Au\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VAFIN", "$,", "KOUS", "NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Jhn, wenn wir sie vern\u00fcnftig brauchen, zu sehen und zu", "tokens": ["Jhn", ",", "wenn", "wir", "sie", "ver\u00b7n\u00fcnf\u00b7tig", "brau\u00b7chen", ",", "zu", "se\u00b7hen", "und", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "PTKZU", "VVINF", "KON", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So ist vor allen Dingen n\u00f6thig, von GOTT im Kleinen", "tokens": ["So", "ist", "vor", "al\u00b7len", "Din\u00b7gen", "n\u00f6\u00b7thig", ",", "von", "GoTT", "im", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "$,", "APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und, \u00fcm den Ruhm von Seiner Allmacht an allen Orten", "tokens": ["Und", ",", "\u00fcm", "den", "Ruhm", "von", "Sei\u00b7ner", "All\u00b7macht", "an", "al\u00b7len", "Or\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die Gr\u00f6ss\u2019 im Kleinen auch zu sehn. Denn, wenn wir", "tokens": ["Die", "Gr\u00f6ss'", "im", "Klei\u00b7nen", "auch", "zu", "sehn", ".", "Denn", ",", "wenn", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$.", "KON", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Geschicht es \u00f6ffters, als im Grossen: Man trifft es allent-", "tokens": ["Ge\u00b7schicht", "es", "\u00f6ff\u00b7ters", ",", "als", "im", "Gros\u00b7sen", ":", "Man", "trifft", "es", "al\u00b7lent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "APPRART", "NN", "$.", "PIS", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Denn es ist auf der Welt kein Kraut, kein Gras, kein", "tokens": ["Denn", "es", "ist", "auf", "der", "Welt", "kein", "Kraut", ",", "kein", "Gras", ",", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT"], "meter": "-----+-+-+-", "measure": "unknown.measure.tri"}, "line.9": {"text": "Kein fast unsichtbar K\u00f6rnchen Sand so klein, ja nicht ein-", "tokens": ["Kein", "fast", "un\u00b7sicht\u00b7bar", "K\u00f6rn\u00b7chen", "Sand", "so", "klein", ",", "ja", "nicht", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADV", "ADJD", "NN", "NN", "ADV", "ADJD", "$,", "ADV", "PTKNEG", "TRUNC"], "meter": "+----+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Das uns nicht viel, in seinem Wesen, von eines Sch\u00f6pfers", "tokens": ["Das", "uns", "nicht", "viel", ",", "in", "sei\u00b7nem", "We\u00b7sen", ",", "von", "ei\u00b7nes", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "PTKNEG", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Jhn unserm Geiste deutlich zeigen, und Seine Macht uns", "tokens": ["Jhn", "un\u00b7serm", "Geis\u00b7te", "deut\u00b7lich", "zei\u00b7gen", ",", "und", "Sei\u00b7ne", "Macht", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$,", "KON", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die allen menschlichen Verstand, auch selber in der", "tokens": ["Die", "al\u00b7len", "menschli\u00b7chen", "Ver\u00b7stand", ",", "auch", "sel\u00b7ber", "in", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "ART"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur blo\u00df allein durch ihre Menge, besiegende Beschaffen-", "tokens": ["Nur", "blo\u00df", "al\u00b7lein", "durch", "ih\u00b7re", "Men\u00b7ge", ",", "be\u00b7sie\u00b7gen\u00b7de", "Be\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die alle Zahlen \u00fcbertrifft, erheitert, und beweiset mehr,", "tokens": ["Die", "al\u00b7le", "Zah\u00b7len", "\u00fc\u00b7bert\u00b7rifft", ",", "er\u00b7hei\u00b7tert", ",", "und", "be\u00b7wei\u00b7set", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "VVPP", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Zu des Allm\u00e4chtgen Sch\u00f6pfers Ruhm, und zu Vermehrung", "tokens": ["Zu", "des", "All\u00b7m\u00e4cht\u00b7gen", "Sch\u00f6p\u00b7fers", "Ruhm", ",", "und", "zu", "Ver\u00b7meh\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein\u2019 unaussprechlich\u2019 unerme\u00dflich\u2019 Anbetungs-w\u00fcrdge", "tokens": ["Ein'", "un\u00b7aus\u00b7sprech\u00b7lich'", "un\u00b7er\u00b7me\u00df\u00b7lich'", "An\u00b7be\u00b7tungs\u00b7w\u00fcrd\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo iemand die\u00df verneinen wolte; der \u00fcberlege die\u00df:", "tokens": ["Wo", "ie\u00b7mand", "die\u00df", "ver\u00b7nei\u00b7nen", "wol\u00b7te", ";", "der", "\u00fc\u00b7berl\u00b7e\u00b7ge", "die\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PDS", "VVINF", "VMFIN", "$.", "ART", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die weite Lufft, das tieffe Meer bestehn, trotz ihrer Gr\u00f6\u00df\u2019", "tokens": ["Die", "wei\u00b7te", "Lufft", ",", "das", "tief\u00b7fe", "Meer", "be\u00b7stehn", ",", "trotz", "ih\u00b7rer", "Gr\u00f6\u00df'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Aus Sand, aus Theilchen, und aus Tropfen. Nun steckt im", "tokens": ["Aus", "Sand", ",", "aus", "Theil\u00b7chen", ",", "und", "aus", "Trop\u00b7fen", ".", "Nun", "steckt", "im"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "KON", "APPR", "NN", "$.", "ADV", "VVFIN", "APPRART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Wirckung, die Natur, das Wesen, und eben so viel", "tokens": ["Die", "Wir\u00b7ckung", ",", "die", "Na\u00b7tur", ",", "das", "We\u00b7sen", ",", "und", "e\u00b7ben", "so", "viel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Als wie im allergr\u00f6ssesten: In dem die Gr\u00f6ss\u2019 aus der Ver-", "tokens": ["Als", "wie", "im", "al\u00b7ler\u00b7gr\u00f6s\u00b7ses\u00b7ten", ":", "In", "dem", "die", "Gr\u00f6ss'", "aus", "der", "Ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPRART", "ADJA", "$.", "APPR", "ART", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der kleinen Theil\u2019 allein besteht. Verdient daher die", "tokens": ["Der", "klei\u00b7nen", "Theil'", "al\u00b7lein", "be\u00b7steht", ".", "Ver\u00b7di\u00b7ent", "da\u00b7her", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$.", "VVPP", "PAV", "ART"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Der kleinsten Erd- und Wasser-Theilchen, gewi\u00df nicht weni-", "tokens": ["Der", "kleins\u00b7ten", "Erd", "und", "Was\u00b7ser\u00b7T\u00b7heil\u00b7chen", ",", "ge\u00b7wi\u00df", "nicht", "we\u00b7ni"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$,", "ADV", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Nicht weniger Bewunderung, als wie das Gr\u00f6ste was wir", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "Be\u00b7wun\u00b7de\u00b7rung", ",", "als", "wie", "das", "Gr\u00f6s\u00b7te", "was", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "PWS", "PPER"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.9": {"text": "Weil, wie gesagt, die gr\u00f6sten C\u00f6rper aus Theilchen blo\u00df", "tokens": ["Weil", ",", "wie", "ge\u00b7sagt", ",", "die", "gr\u00f6s\u00b7ten", "C\u00f6r\u00b7per", "aus", "Theil\u00b7chen", "blo\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "VVPP", "$,", "ART", "ADJA", "NN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Daher ich, wenn ich offtermahls an einem strengen", "tokens": ["Da\u00b7her", "ich", ",", "wenn", "ich", "off\u00b7ter\u00b7mahls", "an", "ei\u00b7nem", "stren\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine rege Wasser-Last sich immer vorw\u00e4rts weltzen", "tokens": ["Und", "sei\u00b7ne", "re\u00b7ge", "Was\u00b7ser\u00b7Last", "sich", "im\u00b7mer", "vor\u00b7w\u00e4rts", "welt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Zwar ob des C\u00f6rpers Gr\u00f6ss\u2019 erstaune, und in der grossen", "tokens": ["Zwar", "ob", "des", "C\u00f6r\u00b7pers", "Gr\u00f6ss'", "er\u00b7stau\u00b7ne", ",", "und", "in", "der", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Beschaffenheit, Bewegung, Nutzen, von Jhres Sch\u00f6pfers", "tokens": ["Be\u00b7schaf\u00b7fen\u00b7heit", ",", "Be\u00b7we\u00b7gung", ",", "Nut\u00b7zen", ",", "von", "Ih\u00b7res", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ein \u00fcberzeugend Lehr-Bild finde; iedoch zugleich auch", "tokens": ["Ein", "\u00fc\u00b7berz\u00b7eu\u00b7gend", "Lehr\u00b7Bild", "fin\u00b7de", ";", "ie\u00b7doch", "zu\u00b7gleich", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$.", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Wie auch das allerkleinste Tr\u00f6pfchen bewunderns-wehrte", "tokens": ["Wie", "auch", "das", "al\u00b7ler\u00b7kleins\u00b7te", "Tr\u00f6pf\u00b7chen", "be\u00b7wun\u00b7derns\u00b7wehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach! la\u00df, geliebter Mensch, forthin, mehr als bisher,", "tokens": ["Ach", "!", "la\u00df", ",", "ge\u00b7lieb\u00b7ter", "Mensch", ",", "for\u00b7thin", ",", "mehr", "als", "bis\u00b7her", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "$,", "ADJA", "NN", "$,", "PTKVZ", "$,", "PIAT", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die \u00fcberall den Sch\u00f6pfer zeigen, in Feuer, Wasser, Lufft", "tokens": ["Die", "\u00fc\u00b7be\u00b7rall", "den", "Sch\u00f6p\u00b7fer", "zei\u00b7gen", ",", "in", "Feu\u00b7er", ",", "Was\u00b7ser", ",", "Lufft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "$,", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Dein Freuden-reicher Gottes-Dienst, und dein erbaulichstes", "tokens": ["Dein", "Freu\u00b7den\u00b7rei\u00b7cher", "Got\u00b7tes\u00b7Dienst", ",", "und", "dein", "er\u00b7bau\u00b7lichs\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Zu des allgegenw\u00e4rtgen GOttes Bewunderung und Lobe,", "tokens": ["Zu", "des", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "Got\u00b7tes", "Be\u00b7wun\u00b7de\u00b7rung", "und", "Lo\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-----+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}