{"textgrid.poem.31011": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "125.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein Artzt; vom Wasser hat er zuvor sein Leben,", "tokens": ["Hier", "liegt", "ein", "Artzt", ";", "vom", "Was\u00b7ser", "hat", "er", "zu\u00b7vor", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "APPRART", "NN", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ietzt hat er von dem Wasser den Geist hin m\u00fcssen geben;", "tokens": ["Ietzt", "hat", "er", "von", "dem", "Was\u00b7ser", "den", "Geist", "hin", "m\u00fcs\u00b7sen", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schau, wie wir offt von einem ietzt Nutz, ietzt Schaden heben!", "tokens": ["Schau", ",", "wie", "wir", "offt", "von", "ei\u00b7nem", "ietzt", "Nutz", ",", "ietzt", "Scha\u00b7den", "he\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "APPR", "ART", "ADV", "NN", "$,", "ADV", "NN", "VVINF", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}}}}}