{"dta.poem.2713": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Mumia  \n  Bey Beedigung Hn. J. S. den 13. Julii  1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Sch\u00f6ps/ dem offt der Tod das Feld hat r\u00e4umen m\u00fcs-\nsen/", "tokens": ["Herr", "Sch\u00f6ps", "/", "dem", "offt", "der", "Tod", "das", "Feld", "hat", "r\u00e4u\u00b7men", "m\u00fcs", "sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ART", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun r\u00e4um er wiederumb ihm Leib und Glieder ein.", "tokens": ["Nun", "r\u00e4um", "er", "wie\u00b7de\u00b7rumb", "ihm", "Leib", "und", "Glie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Uberwinder herrschst/ er liegt zu seinen F\u00fcssen", "tokens": ["Der", "U\u00b7ber\u00b7win\u00b7der", "herrschst", "/", "er", "liegt", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was verwesen kan beschleust der schwartze Schrein.", "tokens": ["Und", "was", "ver\u00b7we\u00b7sen", "kan", "be\u00b7schleust", "der", "schwart\u00b7ze", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie hoch er die Natur durch Kunst ihm sonst verbunden/", "tokens": ["Wie", "hoch", "er", "die", "Na\u00b7tur", "durch", "Kunst", "ihm", "sonst", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "APPR", "NN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie fleissig er ihr Gut und Sch\u00e4tze hat gesucht/", "tokens": ["Wie", "fleis\u00b7sig", "er", "ihr", "Gut", "und", "Sch\u00e4t\u00b7ze", "hat", "ge\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird hinfortkein Rath noch H\u00fclffe mehr gefunden", "tokens": ["So", "wird", "hin\u00b7fort\u00b7kein", "Rath", "noch", "H\u00fclf\u00b7fe", "mehr", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "ADV", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Tinctur/ Essentz und Oel ist ohne Geist und Frucht.", "tokens": ["Tinc\u00b7tur", "/", "Es\u00b7sentz", "und", "O\u00b7el", "ist", "oh\u00b7ne", "Geist", "und", "Frucht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Alkermes kan nicht mehr noch Rob noch Lohoch st\u00e4rcken/", "tokens": ["Al\u00b7ker\u00b7mes", "kan", "nicht", "mehr", "noch", "Rob", "noch", "Lo\u00b7hoch", "st\u00e4r\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "ADV", "ADV", "NE", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Seele von dem Gold beseelt den C\u00f6rper nicht.", "tokens": ["Die", "See\u00b7le", "von", "dem", "Gold", "be\u00b7seelt", "den", "C\u00f6r\u00b7per", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Machaon geht leer ab mit seinen besten Wercken", "tokens": ["Ma\u00b7cha\u00b7on", "geht", "leer", "ab", "mit", "sei\u00b7nen", "bes\u00b7ten", "Wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun eine gr\u00f6\u00dfre Hand das Stunden-Gla\u00df zerbricht.", "tokens": ["Nun", "ei\u00b7ne", "gr\u00f6\u00df\u00b7re", "Hand", "das", "Stun\u00b7den\u00b7Gla\u00df", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Alleine/ werther Freund/ wir wolten ihn gern halten/", "tokens": ["Al\u00b7lei\u00b7ne", "/", "wert\u00b7her", "Freund", "/", "wir", "wol\u00b7ten", "ihn", "gern", "hal\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Di\u00df gibt der Tochter Hertz/ des Eydams Mund an Tag.", "tokens": ["Di\u00df", "gibt", "der", "Toch\u00b7ter", "Hertz", "/", "des", "Ey\u00b7dams", "Mund", "an", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie salben wir ihn ein? Sind Gummen und Asphalten", "tokens": ["Wie", "sal\u00b7ben", "wir", "ihn", "ein", "?", "Sind", "Gum\u00b7men", "und", "As\u00b7phal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Genung/ da\u00df man vor F\u00e4ul und Wurm ihn sch\u00fctzen mag.", "tokens": ["Ge\u00b7nung", "/", "da\u00df", "man", "vor", "F\u00e4ul", "und", "Wurm", "ihn", "sch\u00fct\u00b7zen", "mag."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "$(", "KOUS", "PIS", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Kan Socrotiner Safft die Glieder frisch bewahren?", "tokens": ["Kan", "So\u00b7cro\u00b7ti\u00b7ner", "Safft", "die", "Glie\u00b7der", "frisch", "be\u00b7wah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "H\u00e4lt auch die Macis noch was die Verwesung anff?", "tokens": ["H\u00e4lt", "auch", "die", "Ma\u00b7cis", "noch", "was", "die", "Ver\u00b7we\u00b7sung", "anff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "ADV", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wir wollen keinen Geist von Elementen sparen/", "tokens": ["Wir", "wol\u00b7len", "kei\u00b7nen", "Geist", "von", "E\u00b7le\u00b7men\u00b7ten", "spa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wo sich verl\u00e4ngern kan Herr Sch\u00f6psens Lebens-Lauff.", "tokens": ["Wo", "sich", "ver\u00b7l\u00e4n\u00b7gern", "kan", "Herr", "Sch\u00f6p\u00b7sens", "Le\u00b7bens\u00b7Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVINF", "VMFIN", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Umbsonst/ und schickt uns gleich Canopus seine Mittel", "tokens": ["Um\u00b7bsonst", "/", "und", "schickt", "uns", "gleich", "Ca\u00b7no\u00b7pus", "sei\u00b7ne", "Mit\u00b7tel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KON", "VVFIN", "PPER", "ADV", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was Memphis heilig h\u00e4lt und was der Nil hoch sch\u00e4tzt.", "tokens": ["Was", "Mem\u00b7phis", "hei\u00b7lig", "h\u00e4lt", "und", "was", "der", "Nil", "hoch", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADJD", "VVFIN", "KON", "PWS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Herr Sch\u00f6ps der lieget schon in seinem Sterbe-Kittel/", "tokens": ["Herr", "Sch\u00f6ps", "der", "lie\u00b7get", "schon", "in", "sei\u00b7nem", "Ster\u00b7be\u00b7Kit\u00b7tel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wird selbst eine Leich\u2019 jetzt Leichen beygesetzt.", "tokens": ["Und", "wird", "selbst", "ei\u00b7ne", "Leich'", "jetzt", "Lei\u00b7chen", "bey\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch sol er uns auch tod zu unserm Nutzen dienen/", "tokens": ["Doch", "sol", "er", "uns", "auch", "tod", "zu", "un\u00b7serm", "Nut\u00b7zen", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und eine Mumia der besten Artzney seyn.", "tokens": ["Und", "ei\u00b7ne", "Mu\u00b7mia", "der", "bes\u00b7ten", "Artz\u00b7ney", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Das Wort bleibt ewig wahr: Der Todten Beine gr\u00fcnen", "tokens": ["Das", "Wort", "bleibt", "e\u00b7wig", "wahr", ":", "Der", "Tod\u00b7ten", "Bei\u00b7ne", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$.", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dieweil des HErren Geist und Odem bl\u00e4st darein.", "tokens": ["Die\u00b7weil", "des", "Her\u00b7ren", "Geist", "und", "O\u00b7dem", "bl\u00e4st", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es mag Egyptenland mit seinen C\u00f6rpern prangen/", "tokens": ["Es", "mag", "E\u00b7gyp\u00b7ten\u00b7land", "mit", "sei\u00b7nen", "C\u00f6r\u00b7pern", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Durch Kunst und Specerey die F\u00e4uln\u00fc\u00df hinterziehn.", "tokens": ["Durch", "Kunst", "und", "Spe\u00b7ce\u00b7rey", "die", "F\u00e4ul\u00b7n\u00fc\u00df", "hin\u00b7ter\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es hat das blinde Volck sich nur zu weit vergangen", "tokens": ["Es", "hat", "das", "blin\u00b7de", "Volck", "sich", "nur", "zu", "weit", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PRF", "ADV", "PTKA", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und m\u00fchte sich umbsonst dem Tode zu entfliehn.", "tokens": ["Und", "m\u00fch\u00b7te", "sich", "um\u00b7bsonst", "dem", "To\u00b7de", "zu", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie fleissig es auch wusch das innerst\u2019 Eingeweide", "tokens": ["Wie", "fleis\u00b7sig", "es", "auch", "wusch", "das", "in\u00b7ner\u00b7st'", "Ein\u00b7ge\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Und drein der Cedern Safft/ Nard und Amomen go\u00df/", "tokens": ["Und", "drein", "der", "Ce\u00b7dern", "Safft", "/", "Nard", "und", "A\u00b7mo\u00b7men", "go\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$(", "NE", "KON", "NE", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Ja gantzer dreissig Tag in gleicher Klag und Leide", "tokens": ["Ja", "gant\u00b7zer", "dreis\u00b7sig", "Tag", "in", "glei\u00b7cher", "Klag", "und", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADJA", "CARD", "NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dein Leib zu balsamirn aus Andacht sich entschlo\u00df;", "tokens": ["Dein", "Leib", "zu", "bal\u00b7sa\u00b7mirn", "aus", "An\u00b7dacht", "sich", "ent\u00b7schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So war es weiter nichts als nur ein Dienst der G\u00f6tzen.", "tokens": ["So", "war", "es", "wei\u00b7ter", "nichts", "als", "nur", "ein", "Dienst", "der", "G\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "KOKOM", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie die geheime Schrifft noch giebet zuverstehn.", "tokens": ["Wie", "die", "ge\u00b7hei\u00b7me", "Schrifft", "noch", "gie\u00b7bet", "zu\u00b7ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn auff der Leiche Brust sie Fisch und Vogel setzen/", "tokens": ["Wenn", "auff", "der", "Lei\u00b7che", "Brust", "sie", "Fisch", "und", "Vo\u00b7gel", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "NN", "KON", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und ihrer Deutung nach die Jsis vor mu\u00df gehn.", "tokens": ["Und", "ih\u00b7rer", "Deu\u00b7tung", "nach", "die", "Jsis", "vor", "mu\u00df", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.41": {"text": "Sie meinten/ wenn sie nun den Todten so beschicket", "tokens": ["Sie", "mein\u00b7ten", "/", "wenn", "sie", "nun", "den", "Tod\u00b7ten", "so", "be\u00b7schi\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word", "w