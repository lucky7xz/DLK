{"dta.poem.23554": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Statistische Gl\u00fcckseligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsers ganzen Wohlstands Quellen", "tokens": ["Un\u00b7sers", "gan\u00b7zen", "Wohl\u00b7stands", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst du alle hell und klar", "tokens": ["Siehst", "du", "al\u00b7le", "hell", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uebersichtlich in Tabellen", "tokens": ["Ue\u00b7ber\u00b7sicht\u00b7lich", "in", "Ta\u00b7bel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jahr f\u00fcr Jahr und bis auf's Haar.", "tokens": ["Jahr", "f\u00fcr", "Jahr", "und", "bis", "auf's", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier zehn Schafe ", "tokens": ["Hier", "zehn", "Scha\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["ADV", "CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dort ein ", "tokens": ["Dort", "ein"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Dort drei Ochsen ", "tokens": ["Dort", "drei", "Och\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und ein Drittel Seele hier.", "tokens": ["Und", "ein", "Drit\u00b7tel", "See\u00b7le", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welch ein Wachsthum zum Entz\u00fccken!", "tokens": ["Welch", "ein", "Wach\u00b7sthum", "zum", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauter h\u00f6here Cultur,", "tokens": ["Lau\u00b7ter", "h\u00f6\u00b7he\u00b7re", "Cul\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauter Streben zum Begl\u00fccken!", "tokens": ["Lau\u00b7ter", "Stre\u00b7ben", "zum", "Be\u00b7gl\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}