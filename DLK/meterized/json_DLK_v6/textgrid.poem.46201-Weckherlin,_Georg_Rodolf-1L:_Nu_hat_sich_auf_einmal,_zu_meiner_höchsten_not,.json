{"textgrid.poem.46201": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nu hat sich auf einmal, zu meiner h\u00f6chsten not,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nu hat sich auf einmal, zu meiner h\u00f6chsten not,", "tokens": ["Nu", "hat", "sich", "auf", "ein\u00b7mal", ",", "zu", "mei\u00b7ner", "h\u00f6chs\u00b7ten", "not", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "APPR", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verkehret mein gesang, mein dichten, meine freud,", "tokens": ["ver\u00b7keh\u00b7ret", "mein", "ge\u00b7sang", ",", "mein", "dich\u00b7ten", ",", "mei\u00b7ne", "freud", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "$,", "PPOSAT", "ADJA", "$,", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mein lachen und kurzweil, mein lust, mein liecht, mein tag", "tokens": ["mein", "la\u00b7chen", "und", "kurz\u00b7weil", ",", "mein", "lust", ",", "mein", "liecht", ",", "mein", "tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "VVINF", "KON", "ADJD", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "in seufzen, ach und weh, und in verdru\u00df und leid,", "tokens": ["in", "seuf\u00b7zen", ",", "ach", "und", "weh", ",", "und", "in", "ver\u00b7dru\u00df", "und", "leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "XY", "KON", "PTKVZ", "$,", "KON", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "in weinen, unlust, nacht, in finsternus und klag,", "tokens": ["in", "wei\u00b7nen", ",", "un\u00b7lust", ",", "nacht", ",", "in", "fins\u00b7ter\u00b7nus", "und", "klag", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADJD", "$,", "NN", "$,", "APPR", "NE", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "durch meines liebs verlust, durch meiner Myrten tod.", "tokens": ["durch", "mei\u00b7nes", "liebs", "ver\u00b7lust", ",", "durch", "mei\u00b7ner", "Myr\u00b7ten", "tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O kl\u00e4glicher todfall! o blind und tauber tod,", "tokens": ["O", "kl\u00e4g\u00b7li\u00b7cher", "tod\u00b7fall", "!", "o", "blind", "und", "tau\u00b7ber", "tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "der du mit schmerzlich schwer endlos und h\u00f6chster not", "tokens": ["der", "du", "mit", "schmerz\u00b7lich", "schwer", "end\u00b7los", "und", "h\u00f6chs\u00b7ter", "not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJD", "ADJD", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "verdammest meinen geist und mund zu steter klag!", "tokens": ["ver\u00b7dam\u00b7mest", "mei\u00b7nen", "geist", "und", "mund", "zu", "ste\u00b7ter", "klag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ade mein s\u00fc\u00dfe lieb! ach, ade meine freud!", "tokens": ["a\u00b7de", "mein", "s\u00fc\u00b7\u00dfe", "lieb", "!", "ach", ",", "a\u00b7de", "mei\u00b7ne", "freud", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "ADJD", "$.", "XY", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "willkom hingegen du ganz t\u00f6dliches herzleid,", "tokens": ["will\u00b7kom", "hin\u00b7ge\u00b7gen", "du", "ganz", "t\u00f6d\u00b7li\u00b7ches", "herz\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+++", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "dadurch geendet wird mein trostzeit und lebtag.", "tokens": ["da\u00b7durch", "ge\u00b7en\u00b7det", "wird", "mein", "trost\u00b7zeit", "und", "leb\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ach, wa ist nu mein trost, wa meine sonn und tag!", "tokens": ["Ach", ",", "wa", "ist", "nu", "mein", "trost", ",", "wa", "mei\u00b7ne", "sonn", "und", "tag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "XY", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ohn die ich anders nichts zu w\u00fcnschen dan den tod,", "tokens": ["ohn", "die", "ich", "an\u00b7ders", "nichts", "zu", "w\u00fcn\u00b7schen", "dan", "den", "tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "und doch mehr nicht erlang, dan kummer, angst und leid,", "tokens": ["und", "doch", "mehr", "nicht", "er\u00b7lang", ",", "dan", "kum\u00b7mer", ",", "angst", "und", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKNEG", "VVFIN", "$,", "ADV", "ADJD", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und doch allzeit verbleib in jammer, qual und not:", "tokens": ["und", "doch", "all\u00b7zeit", "ver\u00b7bleib", "in", "jam\u00b7mer", ",", "qual", "und", "not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "o menschliche torheit, hoch r\u00fchmend lust und freud,", "tokens": ["o", "menschli\u00b7che", "tor\u00b7heit", ",", "hoch", "r\u00fch\u00b7mend", "lust", "und", "freud", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$,", "ADJD", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "die sich doch \u00e4ndern schnell in langes leid und klag!", "tokens": ["die", "sich", "doch", "\u00e4n\u00b7dern", "schnell", "in", "lan\u00b7ges", "leid", "und", "klag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "VVINF", "ADJD", "APPR", "ADJA", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach! aber wie kan ich mit taugenlicher klag,", "tokens": ["Ach", "!", "a\u00b7ber", "wie", "kan", "ich", "mit", "tau\u00b7gen\u00b7li\u00b7cher", "klag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "PWAV", "VMFIN", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "mit gnugsam tiefem leid begehen diesen tag,", "tokens": ["mit", "gnug\u00b7sam", "tie\u00b7fem", "leid", "be\u00b7ge\u00b7hen", "die\u00b7sen", "tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "ADJD", "VVINF", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "an welchem auf einmal die h\u00f6chste sch\u00f6nheit, freud", "tokens": ["an", "wel\u00b7chem", "auf", "ein\u00b7mal", "die", "h\u00f6chs\u00b7te", "sch\u00f6n\u00b7heit", ",", "freud"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PRELS", "APPR", "ADV", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "und tugend in das grab gebracht der schn\u00f6de tod,", "tokens": ["und", "tu\u00b7gend", "in", "das", "grab", "ge\u00b7bracht", "der", "schn\u00f6\u00b7de", "tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "an welchem er auch mich mit herzgebrochner not", "tokens": ["an", "wel\u00b7chem", "er", "auch", "mich", "mit", "herz\u00b7ge\u00b7broch\u00b7ner", "not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "begraben lebendig in ein endloses leid.", "tokens": ["be\u00b7gra\u00b7ben", "le\u00b7ben\u00b7dig", "in", "ein", "end\u00b7lo\u00b7ses", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Wie sehr mir unlangst lieb das liebgeborne leid,", "tokens": ["Wie", "sehr", "mir", "un\u00b7langst", "lieb", "das", "lieb\u00b7ge\u00b7bor\u00b7ne", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "ADJD", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "wie gern ich unlangst sang ein liebgezeugte klag,", "tokens": ["wie", "gern", "ich", "un\u00b7langst", "sang", "ein", "lieb\u00b7ge\u00b7zeug\u00b7te", "klag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "und wie gern ich beschrib ein liebgeliebte not:", "tokens": ["und", "wie", "gern", "ich", "be\u00b7schrib", "ein", "lieb\u00b7ge\u00b7lieb\u00b7te", "not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "so dunkel ist mir jetz ohn meine sonn der tag,", "tokens": ["so", "dun\u00b7kel", "ist", "mir", "jetz", "ohn", "mei\u00b7ne", "sonn", "der", "tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "und ohn mein leben jetz w\u00e4r mir willkom der tod,", "tokens": ["und", "ohn", "mein", "le\u00b7ben", "jetz", "w\u00e4r", "mir", "will\u00b7kom", "der", "tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "VVFIN", "ADV", "VAFIN", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "und nichts verdr\u00fc\u00dflicher ist mir jetz, dan die freud.", "tokens": ["und", "nichts", "ver\u00b7dr\u00fc\u00df\u00b7li\u00b7cher", "ist", "mir", "jetz", ",", "dan", "die", "freud", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VAFIN", "PPER", "ADV", "$,", "ADV", "ART", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Da kein mensch, wie ich hat, hat jemal lieb und freud,", "tokens": ["Da", "kein", "mensch", ",", "wie", "ich", "hat", ",", "hat", "je\u00b7mal", "lieb", "und", "freud", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.32": {"text": "nu aber hat kein mensch, wie ich, verlust und leid,", "tokens": ["nu", "a\u00b7ber", "hat", "kein", "mensch", ",", "wie", "ich", ",", "ver\u00b7lust", "und", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "kein mensch war je wie ich verletzet durch den tod.", "tokens": ["kein", "mensch", "war", "je", "wie", "ich", "ver\u00b7let\u00b7zet", "durch", "den", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VAFIN", "ADV", "KOKOM", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "daher solt billich ich ein so schallreiche klag,", "tokens": ["da\u00b7her", "solt", "bil\u00b7lich", "ich", "ein", "so", "schall\u00b7rei\u00b7che", "klag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADJD", "PPER", "ART", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "darab der umkreis selbs solt trauren, an den tag", "tokens": ["da\u00b7rab", "der", "um\u00b7kreis", "selbs", "solt", "trau\u00b7ren", ",", "an", "den", "tag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "f\u00fcrbringen, weil die welt mit mir in gleicher not.", "tokens": ["f\u00fcr\u00b7brin\u00b7gen", ",", "weil", "die", "welt", "mit", "mir", "in", "glei\u00b7cher", "not", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dan meiner Myrten aug, vertreibend alle not,", "tokens": ["Dan", "mei\u00b7ner", "Myr\u00b7ten", "aug", ",", "ver\u00b7trei\u00b7bend", "al\u00b7le", "not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NE", "$,", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "war ja der ganzen welt ein rechte zierd und freud,", "tokens": ["war", "ja", "der", "gan\u00b7zen", "welt", "ein", "rech\u00b7te", "zierd", "und", "freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "der lieb und jugend liecht, der zucht und tugend tag;", "tokens": ["der", "lieb", "und", "ju\u00b7gend", "liecht", ",", "der", "zucht", "und", "tu\u00b7gend", "tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "$,", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "darum solt der erdkreis mit allgemeinem leid", "tokens": ["da\u00b7rum", "solt", "der", "erd\u00b7kreis", "mit", "all\u00b7ge\u00b7mei\u00b7nem", "leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "bezeugen dankbar auch sein allgemeine klag,", "tokens": ["be\u00b7zeu\u00b7gen", "dank\u00b