{"dta.poem.16197": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "251.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Tonspiel kennen mu\u00df, wer's brauchen will zum Spiele;", "tokens": ["Das", "Ton\u00b7spiel", "ken\u00b7nen", "mu\u00df", ",", "wer's", "brau\u00b7chen", "will", "zum", "Spie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "VAFIN", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so die Menschen wer sie leiten will zum Ziele.", "tokens": ["Und", "so", "die", "Men\u00b7schen", "wer", "sie", "lei\u00b7ten", "will", "zum", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PWS", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Niemand will allein und kann zum Ziele schreiten,", "tokens": ["Denn", "Nie\u00b7mand", "will", "al\u00b7lein", "und", "kann", "zum", "Zie\u00b7le", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "KON", "VMFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht zum gleichen Ziel der andern viele schreiten.", "tokens": ["Wo", "nicht", "zum", "glei\u00b7chen", "Ziel", "der", "an\u00b7dern", "vie\u00b7le", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "APPRART", "ADJA", "NN", "ART", "ADJA", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ist das Ziel nur gut, so ist nichts einzuwenden,", "tokens": ["Und", "ist", "das", "Ziel", "nur", "gut", ",", "so", "ist", "nichts", "ein\u00b7zu\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du zu deinem Ziel wei\u00dft andre fein zu wenden.", "tokens": ["Wenn", "du", "zu", "dei\u00b7nem", "Ziel", "wei\u00dft", "and\u00b7re", "fein", "zu", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn leider ohne Ziel gehn in der Irre viel,", "tokens": ["Denn", "lei\u00b7der", "oh\u00b7ne", "Ziel", "gehn", "in", "der", "Ir\u00b7re", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die es dir danken, wenn du ihnen zeigst ein Ziel.", "tokens": ["Die", "es", "dir", "dan\u00b7ken", ",", "wenn", "du", "ih\u00b7nen", "zeigst", "ein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVINF", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}