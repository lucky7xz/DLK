{"textgrid.poem.67410": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Mondenschein", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Mondenschein", "tokens": ["Im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "R\u00fchrt' sie die Saiten und sang darein;", "tokens": ["R\u00fchrt'", "sie", "die", "Sai\u00b7ten", "und", "sang", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "PAV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die L\u00fcfte des Himmels umflossen sie,", "tokens": ["Die", "L\u00fcf\u00b7te", "des", "Him\u00b7mels", "um\u00b7flos\u00b7sen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Des Himmels Schimmer umstrahlten sie.", "tokens": ["Des", "Him\u00b7mels", "Schim\u00b7mer", "um\u00b7strahl\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Umflossen sie,", "tokens": ["Um\u00b7flos\u00b7sen", "sie", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Des Himmels Schimmer umstrahlten sie.", "tokens": ["Des", "Him\u00b7mels", "Schim\u00b7mer", "um\u00b7strahl\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie sang mich hin\u00fcber in welch ein Thal!", "tokens": ["Sie", "sang", "mich", "hin\u00b7\u00fc\u00b7ber", "in", "welch", "ein", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PWAT", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Voll Ruh im seligen Mondesstrahl.", "tokens": ["Voll", "Ruh", "im", "se\u00b7li\u00b7gen", "Mon\u00b7des\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "In welch ein Thal!", "tokens": ["In", "welch", "ein", "Thal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Voll Ruh im seligen Mondesstrahl.", "tokens": ["Voll", "Ruh", "im", "se\u00b7li\u00b7gen", "Mon\u00b7des\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Elysische Lauben umbl\u00fchten mich,", "tokens": ["E\u00b7ly\u00b7si\u00b7sche", "Lau\u00b7ben", "um\u00b7bl\u00fch\u00b7ten", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Edeln Geister umfingen mich.", "tokens": ["Der", "E\u00b7deln", "Geis\u00b7ter", "um\u00b7fin\u00b7gen", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Umbl\u00fchten mich,", "tokens": ["Um\u00b7bl\u00fch\u00b7ten", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Edeln Geister umfingen mich.", "tokens": ["Der", "E\u00b7deln", "Geis\u00b7ter", "um\u00b7fin\u00b7gen", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und all ihr Leben ein s\u00fc\u00dfer Glanz,", "tokens": ["Und", "all", "ihr", "Le\u00b7ben", "ein", "s\u00fc\u00b7\u00dfer", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Unschuld Thr\u00e4nen ein Perlenkranz.", "tokens": ["Der", "Un\u00b7schuld", "Thr\u00e4\u00b7nen", "ein", "Per\u00b7len\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein s\u00fc\u00dfer Glanz,", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Unschuld Thr\u00e4nen ein Perlenkranz.", "tokens": ["Der", "Un\u00b7schuld", "Thr\u00e4\u00b7nen", "ein", "Per\u00b7len\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Sph\u00e4ren sangen mir Harmonie.", "tokens": ["Die", "Sph\u00e4\u00b7ren", "san\u00b7gen", "mir", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Stimmen s\u00fc\u00dfeste, Sympathie.", "tokens": ["Der", "Stim\u00b7men", "s\u00fc\u00b7\u00dfes\u00b7te", ",", "Sym\u00b7pa\u00b7thie", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Mir Harmonie,", "tokens": ["Mir", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Stimmen s\u00fc\u00dfeste, Sympathie.", "tokens": ["Der", "Stim\u00b7men", "s\u00fc\u00b7\u00dfes\u00b7te", ",", "Sym\u00b7pa\u00b7thie", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die T\u00f6ne schwanden, und himmelw\u00e4rts", "tokens": ["Die", "T\u00f6\u00b7ne", "schwan\u00b7den", ",", "und", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zog sanft ihr nach mein stilles Herz.", "tokens": ["Zog", "sanft", "ihr", "nach", "mein", "stil\u00b7les", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und himmelw\u00e4rts", "tokens": ["Und", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Zog sanft ihr nach mein stilles Herz.", "tokens": ["Zog", "sanft", "ihr", "nach", "mein", "stil\u00b7les", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O k\u00e4mst Du, Himmlische, wieder mir!", "tokens": ["O", "k\u00e4mst", "Du", ",", "Himm\u00b7li\u00b7sche", ",", "wie\u00b7der", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Stimme lispelt': \u00bbIch bin bei Dir.\u00ab", "tokens": ["Die", "Stim\u00b7me", "lis\u00b7pelt'", ":", "\u00bb", "Ich", "bin", "bei", "Dir", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VAFIN", "APPR", "PPER", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}