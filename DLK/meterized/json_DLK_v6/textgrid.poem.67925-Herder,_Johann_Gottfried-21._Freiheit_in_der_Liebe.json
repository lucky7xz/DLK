{"textgrid.poem.67925": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "21. Freiheit in der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zwingt mich auf der Welt mich also hinzugeben?", "tokens": ["Was", "zwingt", "mich", "auf", "der", "Welt", "mich", "al\u00b7so", "hin\u00b7zu\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ists wohl der Rede werth, gefangen m\u00fcssen leben?", "tokens": ["Ists", "wohl", "der", "Re\u00b7de", "werth", ",", "ge\u00b7fan\u00b7gen", "m\u00fcs\u00b7sen", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Vogel w\u00fcnschet ihm in freier Luft zu seyn,", "tokens": ["Ein", "Vo\u00b7gel", "w\u00fcn\u00b7schet", "ihm", "in", "frei\u00b7er", "Luft", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sperret man ihn gleich in Gold und Silber ein.", "tokens": ["Und", "sper\u00b7ret", "man", "ihn", "gleich", "in", "Gold", "und", "Sil\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jetzt lieb' ich was ich will, jetzt will ich was ich liebe,", "tokens": ["Jetzt", "lieb'", "ich", "was", "ich", "will", ",", "jetzt", "will", "ich", "was", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wei\u00df, da\u00df nichts entgeht, was ich zur Zeit verschiebe.", "tokens": ["Und", "wei\u00df", ",", "da\u00df", "nichts", "ent\u00b7geht", ",", "was", "ich", "zur", "Zeit", "ver\u00b7schie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Tage mach ich Nacht, und aus der Nacht den Tag,", "tokens": ["Aus", "Ta\u00b7ge", "mach", "ich", "Nacht", ",", "und", "aus", "der", "Nacht", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und prange, da\u00df ich selbst mein Herr und Knecht seyn mag.", "tokens": ["Und", "pran\u00b7ge", ",", "da\u00df", "ich", "selbst", "mein", "Herr", "und", "Knecht", "seyn", "mag."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weg, weg du Dienstbarkeit, bei der nichts ist zu finden,", "tokens": ["Weg", ",", "weg", "du", "Dienst\u00b7bar\u00b7keit", ",", "bei", "der", "nichts", "ist", "zu", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "NN", "$,", "APPR", "ART", "PIS", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Gut da Mangel ist, als Was, das bald kann[n] schwinden,", "tokens": ["Als", "Gut", "da", "Man\u00b7gel", "ist", ",", "als", "Was", ",", "das", "bald", "kann", "n", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "NN", "VAFIN", "$,", "KOUS", "NN", "$,", "PRELS", "ADV", "VMFIN", "$(", "XY", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Ha\u00df, mit Gunst vermengt, als Lust, die Unlust bringt,", "tokens": ["Als", "Ha\u00df", ",", "mit", "Gunst", "ver\u00b7mengt", ",", "als", "Lust", ",", "die", "Un\u00b7lust", "bringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "NN", "VVPP", "$,", "KOUS", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Arbeit bei der Ruh, als Freiheit die mich zwingt.", "tokens": ["Als", "Ar\u00b7beit", "bei", "der", "Ruh", ",", "als", "Frei\u00b7heit", "die", "mich", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$,", "KOUS", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch, Venus, deren Lob ich oftmals ausgebreitet,", "tokens": ["Doch", ",", "Ve\u00b7nus", ",", "de\u00b7ren", "Lob", "ich", "oft\u00b7mals", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PRELAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist mir ein Stamm allein an Waldesstatt bereitet;", "tokens": ["Ist", "mir", "ein", "Stamm", "al\u00b7lein", "an", "Wal\u00b7des\u00b7statt", "be\u00b7rei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So f\u00fcge mir hinfort Sinn, Will und Augen bei,", "tokens": ["So", "f\u00fc\u00b7ge", "mir", "hin\u00b7fort", "Sinn", ",", "Will", "und", "Au\u00b7gen", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,", "VMFIN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Recht zu ersehn den Baum, der meiner w\u00fcrdig sey.", "tokens": ["Recht", "zu", "er\u00b7sehn", "den", "Baum", ",", "der", "mei\u00b7ner", "w\u00fcr\u00b7dig", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}