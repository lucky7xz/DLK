{"textgrid.poem.25524": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lobt dich ein guter weiser Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lobt dich ein guter weiser Mann,", "tokens": ["Lobt", "dich", "ein", "gu\u00b7ter", "wei\u00b7ser", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie tanzt mein Herz vor Freuden!", "tokens": ["Wie", "tanzt", "mein", "Herz", "vor", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lobt dich ein Weib, wie bin ich dann", "tokens": ["Lobt", "dich", "ein", "Weib", ",", "wie", "bin", "ich", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Stillen zu beneiden!", "tokens": ["Im", "Stil\u00b7len", "zu", "be\u00b7nei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur, tadeln sie, als Sonderling,", "tokens": ["Nur", ",", "ta\u00b7deln", "sie", ",", "als", "Son\u00b7der\u00b7ling", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich l\u00e4chelnd und bescheiden:", "tokens": ["Dich", "l\u00e4\u00b7chelnd", "und", "be\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was mu\u00df ich, ach! ich armes Ding,", "tokens": ["Was", "mu\u00df", "ich", ",", "ach", "!", "ich", "ar\u00b7mes", "Ding", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "ITJ", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit stummem Munde leiden!", "tokens": ["Mit", "stum\u00b7mem", "Mun\u00b7de", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch tadelt, Freund, ein M\u00e4dchen dich,", "tokens": ["Doch", "ta\u00b7delt", ",", "Freund", ",", "ein", "M\u00e4d\u00b7chen", "dich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So denk' ich: La\u00df sie neiden!", "tokens": ["So", "denk'", "ich", ":", "La\u00df", "sie", "nei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VVIMP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein ihr Lob \u2013 wie wunderlich! \u2013", "tokens": ["Al\u00b7lein", "ihr", "Lob", "\u2013", "wie", "wun\u00b7der\u00b7lich", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann ich durchaus nicht leiden!", "tokens": ["Kann", "ich", "durc\u00b7haus", "nicht", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}