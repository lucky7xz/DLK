{"dta.poem.8648": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Thr\u00e4nen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Thr\u00e4nen, euch, ihr trauten, lieben,", "tokens": ["Thr\u00e4\u00b7nen", ",", "euch", ",", "ihr", "trau\u00b7ten", ",", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPER", "$,", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bring' ich diesen Dankgesang!", "tokens": ["Bring'", "ich", "die\u00b7sen", "Dank\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seyd ja auch nicht ausgeblieben,", "tokens": ["Seyd", "ja", "auch", "nicht", "aus\u00b7ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mein Herz im Liede klang;", "tokens": ["Wenn", "mein", "Herz", "im", "Lie\u00b7de", "klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schlichet die bekannten Gleise", "tokens": ["Schli\u00b7chet", "die", "be\u00b7kann\u00b7ten", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still herab, als wolltet ihr", "tokens": ["Still", "her\u00b7ab", ",", "als", "woll\u00b7tet", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "KOUS", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen Schmerz behorchen leise,", "tokens": ["Mei\u00b7nen", "Schmerz", "be\u00b7hor\u00b7chen", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Lied quoll sanfter mir.", "tokens": ["Und", "das", "Lied", "quoll", "sanf\u00b7ter", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn der Dolch im Busen w\u00fchlte,", "tokens": ["Wenn", "der", "Dolch", "im", "Bu\u00b7sen", "w\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief vom Ungl\u00fcck eingebohrt,", "tokens": ["Tief", "vom", "Un\u00b7gl\u00fcck", "ein\u00b7ge\u00b7bohrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam der Trost von euch, und sp\u00fclte", "tokens": ["Kam", "der", "Trost", "von", "euch", ",", "und", "sp\u00fcl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Linde die Verzweiflung fort.", "tokens": ["Lin\u00b7de", "die", "Ver\u00b7zwei\u00b7flung", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O flieht keinen Wildumdrohten", "tokens": ["O", "flieht", "kei\u00b7nen", "Wil\u00b7dum\u00b7droh\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Orkan und Wetterschein!", "tokens": ["Von", "Or\u00b7kan", "und", "Wet\u00b7ter\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Naht ihm, naht ihm, Friedensboten,", "tokens": ["Naht", "ihm", ",", "naht", "ihm", ",", "Frie\u00b7dens\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft den Armen nicht allein!", "tokens": ["La\u00dft", "den", "Ar\u00b7men", "nicht", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ist die Nacht vorbei, so fehle", "tokens": ["Ist", "die", "Nacht", "vor\u00b7bei", ",", "so", "feh\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm doch eure Treue nicht,", "tokens": ["Ihm", "doch", "eu\u00b7re", "Treu\u00b7e", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Traufe seiner Seele,", "tokens": ["Und", "die", "Trau\u00b7fe", "sei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Netze mild sein Angesicht", "tokens": ["Net\u00b7ze", "mild", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit der Wehmuth s\u00fc\u00dfen Tropfen,", "tokens": ["Mit", "der", "Weh\u00b7muth", "s\u00fc\u00b7\u00dfen", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sein Herz, war's auch gequ\u00e4lt,", "tokens": ["Da\u00df", "sein", "Herz", ",", "wa\u00b7r's", "auch", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nie verlerne doch zu klopfen", "tokens": ["Nie", "ver\u00b7ler\u00b7ne", "doch", "zu", "klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser sch\u00f6nen Gotteswelt.", "tokens": ["Die\u00b7ser", "sch\u00f6\u00b7nen", "Got\u00b7tes\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht nur, wo der Herzensnager", "tokens": ["Nicht", "nur", ",", "wo", "der", "Her\u00b7zens\u00b7na\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schmerz w\u00fchlt, habt ihr euern Lauf,", "tokens": ["Schmerz", "w\u00fchlt", ",", "habt", "ihr", "eu\u00b7ern", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch wo Lust ihr Reiselager", "tokens": ["Auch", "wo", "Lust", "ihr", "Rei\u00b7se\u00b7la\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4gt in einem Busen auf:", "tokens": ["Schl\u00e4gt", "in", "ei\u00b7nem", "Bu\u00b7sen", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ha, wie wogt das Festget\u00fcmmel", "tokens": ["Ha", ",", "wie", "wogt", "das", "Fest\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem engen K\u00e4mmerlein,", "tokens": ["In", "dem", "en\u00b7gen", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wenn der ganze reiche Himmel", "tokens": ["Wenn", "der", "gan\u00b7ze", "rei\u00b7che", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberf\u00fcllend will hinein!", "tokens": ["Ue\u00b7ber\u00b7f\u00fcl\u00b7lend", "will", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und die Thr\u00e4nen seh' ich blinken", "tokens": ["Und", "die", "Thr\u00e4\u00b7nen", "seh'", "ich", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Wang' im Freudenglast,", "tokens": ["Auf", "der", "Wang'", "im", "Freu\u00b7den\u00b7glast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie zittern und sie winken", "tokens": ["Und", "sie", "zit\u00b7tern", "und", "sie", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Welt herein zu Gast. \u2014", "tokens": ["Al\u00b7le", "Welt", "her\u00b7ein", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Als ich einst am Sterbebette", "tokens": ["Als", "ich", "einst", "am", "Ster\u00b7be\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines lieben Freundes stand,", "tokens": ["Ei\u00b7nes", "lie\u00b7ben", "Freun\u00b7des", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Tod die Rosenkette", "tokens": ["Und", "der", "Tod", "die", "Ro\u00b7sen\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kalt uns aus den H\u00e4nden wand;", "tokens": ["Kalt", "uns", "aus", "den", "H\u00e4n\u00b7den", "wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Weint' ich ihm die lezte Oehlung,", "tokens": ["Weint'", "ich", "ihm", "die", "lez\u00b7te", "O\u00b7eh\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und \u2014 schon lag er still und bla\u00df,", "tokens": ["Und", "schon", "lag", "er", "still", "und", "bla\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und in seines Auges H\u00f6hlung", "tokens": ["Und", "in", "sei\u00b7nes", "Au\u00b7ges", "H\u00f6h\u00b7lung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mild noch eine Thr\u00e4ne sa\u00df,", "tokens": ["Mild", "noch", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "War so heilig anzuschauen,", "tokens": ["War", "so", "hei\u00b7lig", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wies die Sehnsucht himmelan,", "tokens": ["Wies", "die", "Sehn\u00b7sucht", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Engel, den die Frauen", "tokens": ["Wie", "der", "En\u00b7gel", ",", "den", "die", "Frau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst am Grabe Jesu sahn.", "tokens": ["Einst", "am", "Gra\u00b7be", "Je\u00b7su", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}