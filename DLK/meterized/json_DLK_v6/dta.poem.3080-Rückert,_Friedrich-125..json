{"dta.poem.3080": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "125.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf alle Fragen die ich thun mag, h\u00f6rt ein Geist,", "tokens": ["Auf", "al\u00b7le", "Fra\u00b7gen", "die", "ich", "thun", "mag", ",", "h\u00f6rt", "ein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der bald mich deutlicher bald dunkler unterweist.", "tokens": ["Der", "bald", "mich", "deut\u00b7li\u00b7cher", "bald", "dunk\u00b7ler", "un\u00b7ter\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und auf die Fragen die nun ihr m\u00f6gt thun hinwieder,", "tokens": ["Und", "auf", "die", "Fra\u00b7gen", "die", "nun", "ihr", "m\u00f6gt", "thun", "hin\u00b7wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADV", "PPER", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Antworten deutlicher und dunkler diese Lieder.", "tokens": ["Ant\u00b7wor\u00b7ten", "deut\u00b7li\u00b7cher", "und", "dunk\u00b7ler", "die\u00b7se", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJA", "PDAT", "NN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Und wenn die deutlichen Antworten euch erfreuten,", "tokens": ["Und", "wenn", "die", "deut\u00b7li\u00b7chen", "Ant\u00b7wor\u00b7ten", "euch", "er\u00b7freu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Freun dunkle mehr noch euch, wenn ihr sie wi\u00dft zu deuten.", "tokens": ["Freun", "dunk\u00b7le", "mehr", "noch", "euch", ",", "wenn", "ihr", "sie", "wi\u00dft", "zu", "deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}