{"textgrid.poem.64377": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "100. Lied", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin! de\u00df freuet sich mein Herz!", "tokens": ["Ich", "bin", "!", "de\u00df", "freu\u00b7et", "sich", "mein", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "ART", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin, und werde sein!", "tokens": ["Ich", "bin", ",", "und", "wer\u00b7de", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KON", "VAFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein St\u00e4ubchen ist des Lebens Schmerz,", "tokens": ["Ein", "St\u00e4ub\u00b7chen", "ist", "des", "Le\u00b7bens", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gesehn im Sonnenschein.", "tokens": ["Ge\u00b7sehn", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gesehn in jener Sonne Schein,", "tokens": ["Ge\u00b7sehn", "in", "je\u00b7ner", "Son\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nimmer untergeht,", "tokens": ["Die", "nim\u00b7mer", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch die, was war, was ist, wird sein,", "tokens": ["Durch", "die", ",", "was", "war", ",", "was", "ist", ",", "wird", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "VAFIN", "$,", "PWS", "VAFIN", "$,", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Emporging und besteht.", "tokens": ["Em\u00b7por\u00b7ging", "und", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Froh wandl' ich auf des Lebens Bahn", "tokens": ["Froh", "wandl'", "ich", "auf", "des", "Le\u00b7bens", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entgegen ihrem Licht,", "tokens": ["Ent\u00b7ge\u00b7gen", "ih\u00b7rem", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das manchen Nebel, manchen Wahn", "tokens": ["Das", "man\u00b7chen", "Ne\u00b7bel", ",", "man\u00b7chen", "Wahn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit goldnem Strahl durchbricht.", "tokens": ["Mit", "gold\u00b7nem", "Strahl", "durch\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es f\u00fchre mich des Glaubens Hand,", "tokens": ["Es", "f\u00fch\u00b7re", "mich", "des", "Glau\u00b7bens", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir schwebe Hoffnung vor,", "tokens": ["Mir", "schwe\u00b7be", "Hoff\u00b7nung", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Liebe heb' an sanftem Band", "tokens": ["Und", "Lie\u00b7be", "heb'", "an", "sanf\u00b7tem", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich aus dem Staub empor!", "tokens": ["Mich", "aus", "dem", "Staub", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Ihr Odem haucht auf Land und Meer,", "tokens": ["Ihr", "O\u00b7dem", "haucht", "auf", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie steu'rt des Mondes Kahn,", "tokens": ["Sie", "steu'rt", "des", "Mon\u00b7des", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie leitet der Gestirne Heer,", "tokens": ["Sie", "lei\u00b7tet", "der", "Ge\u00b7stir\u00b7ne", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie facht die Sonnen an.", "tokens": ["Sie", "facht", "die", "Son\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch w\u00e4rmer haucht und heller facht", "tokens": ["Doch", "w\u00e4r\u00b7mer", "haucht", "und", "hel\u00b7ler", "facht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Odem Geister an,", "tokens": ["Ihr", "O\u00b7dem", "Geis\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fchrt durch kurze Erdennacht", "tokens": ["Und", "f\u00fchrt", "durch", "kur\u00b7ze", "Er\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie auf den Ocean,", "tokens": ["Sie", "auf", "den", "O\u00b7cean", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Wo laute Flut des Jubels hallt,", "tokens": ["Wo", "lau\u00b7te", "Flut", "des", "Ju\u00b7bels", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Licht dem Licht entspr\u00fcht,", "tokens": ["Wo", "Licht", "dem", "Licht", "ent\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Wonn' an Wonne wogt und wallt,", "tokens": ["Wo", "Wonn'", "an", "Won\u00b7ne", "wogt", "und", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Lieb' an Lieb' ergl\u00fcht!", "tokens": ["Und", "Lieb'", "an", "Lieb'", "er\u00b7gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}