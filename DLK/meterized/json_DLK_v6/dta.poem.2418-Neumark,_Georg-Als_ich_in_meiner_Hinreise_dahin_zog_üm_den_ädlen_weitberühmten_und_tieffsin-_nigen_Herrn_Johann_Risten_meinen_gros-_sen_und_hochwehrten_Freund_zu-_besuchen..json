word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$(", "PWAV", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der in die Westsee fleust/ tagt\u00e4glich einen Gru\u00df", "tokens": ["Der", "in", "die", "West\u00b7see", "fleust", "/", "tag\u00b7t\u00e4g\u00b7lich", "ei\u00b7nen", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Von seinem Gott Neptun allm\u00e4hlich bringt zur\u00fckke:", "tokens": ["Von", "sei\u00b7nem", "Gott", "Nep\u00b7tun", "all\u00b7m\u00e4h\u00b7lich", "bringt", "zu\u00b7r\u00fck\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So d\u00e4nk dem reifflich nach was dieses vor ein Gl\u00fckke", "tokens": ["So", "d\u00e4nk", "dem", "reif\u00b7flich", "nach", "was", "die\u00b7ses", "vor", "ein", "Gl\u00fck\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "APPR", "PRELS", "PDAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und sch\u00f6nes Wesen sey/ da\u00df du den bey dir hast", "tokens": ["Und", "sch\u00f6\u00b7nes", "We\u00b7sen", "sey", "/", "da\u00df", "du", "den", "bey", "dir", "hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$(", "KOUS", "PPER", "ART", "APPR", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}