{"dta.poem.2418": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als ich in meiner Hinreise/ dahin zog/  \n \u00fcm den \u00e4dlen/ weitber\u00fchmten und tieffsin-  \n nigen Herrn Johann Risten meinen gros-  \n sen und hochwehrten Freund zu-  \n besuchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Sey tausend mal gegr\u00fcsst du weitberuffnes Wedel", "tokens": ["Sey", "tau\u00b7send", "mal", "ge\u00b7gr\u00fcs\u00b7st", "du", "weit\u00b7be\u00b7ruff\u00b7nes", "We\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Welchs eben so ber\u00fchmt/ so herrlich und so \u00e4del", "tokens": ["Welchs", "e\u00b7ben", "so", "be\u00b7r\u00fchmt", "/", "so", "herr\u00b7lich", "und", "so", "\u00e4\u00b7del"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADJD", "$(", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als etwan eine Stadt mag in Europa seyn", "tokens": ["Als", "et\u00b7wan", "ei\u00b7ne", "Stadt", "mag", "in", "Eu\u00b7ro\u00b7pa", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "APPR", "NE", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(doch sag\u2019 ich itzo nicht von grosser H\u00e4user Schein.)", "tokens": ["(", "doch", "sag'", "ich", "it\u00b7zo", "nicht", "von", "gros\u00b7ser", "H\u00e4u\u00b7ser", "Schein", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df Dantzig seyn belobt durch grosse Zahl Gesch\u00fctze;", "tokens": ["La\u00df", "Dant\u00b7zig", "seyn", "be\u00b7lobt", "durch", "gros\u00b7se", "Zahl", "Ge\u00b7sch\u00fct\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "VAINF", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stadt Ulm durch vieles Geld; La\u00df durch viel F\u00fcrsten", "tokens": ["Stadt", "Ulm", "durch", "vie\u00b7les", "Geld", ";", "La\u00df", "durch", "viel", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "PIAT", "NN", "$.", "VVIMP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sitze", "tokens": ["Sit\u00b7ze"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und pr\u00e4chtige Gebeu Venedig seyn bekandt;", "tokens": ["Und", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7beu", "Ve\u00b7ne\u00b7dig", "seyn", "be\u00b7kandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NE", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Und Rom durch seinen Pabst/ den die\u00df und jenes Land", "tokens": ["Und", "Rom", "durch", "sei\u00b7nen", "Pabst", "/", "den", "die\u00df", "und", "je\u00b7nes", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "PPOSAT", "NN", "$(", "ART", "PDS", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vor seinen Abgott h\u00e4lt; La\u00df Amsteldamm sich breiten", "tokens": ["Vor", "sei\u00b7nen", "Ab\u00b7gott", "h\u00e4lt", ";", "La\u00df", "A\u00b7mstel\u00b7damm", "sich", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "VVIMP", "NE", "PRF", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit seiner Kauffmannschaft; La\u00df auch bey diesen Zeiten", "tokens": ["Mit", "sei\u00b7ner", "Kauf\u00b7fmann\u00b7schaft", ";", "La\u00df", "auch", "bey", "die\u00b7sen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVIMP", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die grosse Neronsburg die weltber\u00fchmte Stadt/", "tokens": ["Die", "gros\u00b7se", "Ne\u00b7rons\u00b7burg", "die", "welt\u00b7be\u00b7r\u00fchm\u00b7te", "Stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die jenen \u00e4dlen Mann anitzo bey ihr hat/", "tokens": ["Die", "je\u00b7nen", "\u00e4d\u00b7len", "Mann", "a\u00b7nit\u00b7zo", "bey", "ihr", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "ADV", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der sich durch unsre Sprach\u2019 hat allbereits verg\u00f6ttert/", "tokens": ["Der", "sich", "durch", "uns\u00b7re", "Sprach'", "hat", "all\u00b7be\u00b7reits", "ver\u00b7g\u00f6t\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ber\u00fchmet seyn durch Kunst/ die alle Welt besp\u00f6ttert", "tokens": ["Be\u00b7r\u00fch\u00b7met", "seyn", "durch", "Kunst", "/", "die", "al\u00b7le", "Welt", "be\u00b7sp\u00f6t\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "APPR", "NN", "$(", "ART", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was ihren Witz betrifft; La\u00df Stra\u00dfburg durch den", "tokens": ["Was", "ih\u00b7ren", "Witz", "be\u00b7tr\u00b7ifft", ";", "La\u00df", "Stra\u00df\u00b7burg", "durch", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$.", "VVIMP", "NE", "APPR", "ART"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Thurm;", "tokens": ["Thurm", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Und Prag beruffen seyn durch letzten Kriegessturm", "tokens": ["Und", "Prag", "be\u00b7ruf\u00b7fen", "seyn", "durch", "letz\u00b7ten", "Krie\u00b7ges\u00b7sturm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVPP", "VAINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "La\u00df die und jene Stadt durch die\u00df und jenes Prangen/", "tokens": ["La\u00df", "die", "und", "je\u00b7ne", "Stadt", "durch", "die\u00df", "und", "je\u00b7nes", "Pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "KON", "PDAT", "NN", "APPR", "PDS", "KON", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du kanst ingleichen auch dir einen Ruhm erlangen", "tokens": ["Du", "kanst", "in\u00b7glei\u00b7chen", "auch", "dir", "ei\u00b7nen", "Ruhm", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der nicht verg\u00e4nglich ist. Doch/ hastu ihn doch schon/", "tokens": ["Der", "nicht", "ver\u00b7g\u00e4ng\u00b7lich", "ist", ".", "Doch", "/", "has\u00b7tu", "ihn", "doch", "schon", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "VAFIN", "$.", "KON", "$(", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du \u00e4dles Wedel du! Dein grosser F\u00f6bus Sohn", "tokens": ["Du", "\u00e4d\u00b7les", "We\u00b7del", "du", "!", "Dein", "gros\u00b7ser", "F\u00f6\u00b7bus", "Sohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der itzo bey dir wohnt wird dir durch seine Schrifften", "tokens": ["Der", "it\u00b7zo", "bey", "dir", "wohnt", "wird", "dir", "durch", "sei\u00b7ne", "Schriff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die lauter himmlisch sind/ ein solches Denkmal stifften", "tokens": ["Die", "lau\u00b7ter", "himm\u00b7lisch", "sind", "/", "ein", "sol\u00b7ches", "Denk\u00b7mal", "stiff\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$(", "ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da\u00df dein vorschlechtes Lob nun wird bey Buntzlau", "tokens": ["Da\u00df", "dein", "vor\u00b7schlech\u00b7tes", "Lob", "nun", "wird", "bey", "Buntz\u00b7lau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "stehn", "tokens": ["stehn"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Auch dort bey jenem Solm/ die zwar schlecht anzusehn", "tokens": ["Auch", "dort", "bey", "je\u00b7nem", "Solm", "/", "die", "zwar", "schlecht", "an\u00b7zu\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$(", "PRELS", "ADV", "ADJD", "VVIZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch gleichwol weit ber\u00fchmt. Du aber wirst hergegen", "tokens": ["Doch", "gleich\u00b7wol", "weit", "be\u00b7r\u00fchmt", ".", "Du", "a\u00b7ber", "wirst", "her\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADJD", "$.", "PPER", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du wehrter Flekken du dein Gl\u00fckke wol erwegen", "tokens": ["Du", "wehr\u00b7ter", "Flek\u00b7ken", "du", "dein", "Gl\u00fck\u00b7ke", "wol", "er\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das dir der Himmel g\u00f6nnt. Du wirst den \u00e4dlen", "tokens": ["Das", "dir", "der", "Him\u00b7mel", "g\u00f6nnt", ".", "Du", "wirst", "den", "\u00e4d\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Mann/", "tokens": ["Mann", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Den Mann/ der so geschikkt und trefflich schreiben kan/", "tokens": ["Den", "Mann", "/", "der", "so", "ge\u00b7schikkt", "und", "treff\u00b7lich", "schrei\u00b7ben", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "VVPP", "KON", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der durch die Poesie hat viel zu ihm gezogen/", "tokens": ["Der", "durch", "die", "Poe\u00b7sie", "hat", "viel", "zu", "ihm", "ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Dem jener tapffre F\u00fcrst von Hertzen ist gewogen", "tokens": ["Dem", "je\u00b7ner", "tapf\u00b7fre", "F\u00fcrst", "von", "Hert\u00b7zen", "ist", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der \u00e4dle