{"textgrid.poem.31055": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "169.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df man mag in Ha\u00df und Neid wider seinen Nechsten leben,", "tokens": ["Da\u00df", "man", "mag", "in", "Ha\u00df", "und", "Neid", "wi\u00b7der", "sei\u00b7nen", "Nechs\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Soll uns die Religion einen sch\u00f6nen Mantel geben.", "tokens": ["Soll", "uns", "die", "Re\u00b7li\u00b7gi\u00b7on", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Man\u00b7tel", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Ehr mir Gott Religion, die gleich rein und heilig gl\u00e4ubet,", "tokens": ["Ehr", "mir", "Gott", "Re\u00b7li\u00b7gi\u00b7on", ",", "die", "gleich", "rein", "und", "hei\u00b7lig", "gl\u00e4u\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Immer aber Ha\u00df und Neid wider ihren Nechsten treibet!", "tokens": ["Im\u00b7mer", "a\u00b7ber", "Ha\u00df", "und", "Neid", "wi\u00b7der", "ih\u00b7ren", "Nechs\u00b7ten", "trei\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}}}}}