{"textgrid.poem.52481": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "63. Auf Dr. J. G. Amsteins Tod", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein heller Geist, ein mildes Herz,", "tokens": ["Ein", "hel\u00b7ler", "Geist", ",", "ein", "mil\u00b7des", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr alles Hohe Sinn, und reines, treues Streben,", "tokens": ["F\u00fcr", "al\u00b7les", "Ho\u00b7he", "Sinn", ",", "und", "rei\u00b7nes", ",", "treu\u00b7es", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gef\u00fchl f\u00fcr fremde Not, f\u00fcr des Verlass'nen Schmerz,", "tokens": ["Ge\u00b7f\u00fchl", "f\u00fcr", "frem\u00b7de", "Not", ",", "f\u00fcr", "des", "Ver\u00b7lass'\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat Gott zur Weihe dir, du edler Arzt, gegeben!", "tokens": ["Hat", "Gott", "zur", "Wei\u00b7he", "dir", ",", "du", "ed\u00b7ler", "Arzt", ",", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er fristete durch dich so manches Menschenleben,", "tokens": ["Er", "fris\u00b7te\u00b7te", "durch", "dich", "so", "man\u00b7ches", "Men\u00b7schen\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein fr\u00fcher Opfertod zog deines himmelw\u00e4rts!", "tokens": ["Ein", "fr\u00fc\u00b7her", "Op\u00b7fer\u00b7tod", "zog", "dei\u00b7nes", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu k\u00fchn und oft bestiegst du schon das Ruttungsboot;", "tokens": ["Zu", "k\u00fchn", "und", "oft", "be\u00b7stiegst", "du", "schon", "das", "Rut\u00b7tungs\u00b7boot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach! diesmal solltest du nicht mehr uns wiederkehren!", "tokens": ["Ach", "!", "dies\u00b7mal", "soll\u00b7test", "du", "nicht", "mehr", "uns", "wie\u00b7der\u00b7keh\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer eilend naht dem ersten Ruf der Not,", "tokens": ["Wer", "ei\u00b7lend", "naht", "dem", "ers\u00b7ten", "Ruf", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sieht bald sein Irdisches im Flammenschlund verzehren!", "tokens": ["Sieht", "bald", "sein", "Ir\u00b7di\u00b7sches", "im", "Flam\u00b7men\u00b7schlund", "ver\u00b7zeh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verkl\u00e4rten Geistern scheint die Glut ein Morgenrot,", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ten", "Geis\u00b7tern", "scheint", "die", "Glut", "ein", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und auf die Asche flie\u00dfen Z\u00e4hren.", "tokens": ["Und", "auf", "die", "A\u00b7sche", "flie\u00b7\u00dfen", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bescheidner! nun ist uns verg\u00f6nnt, dich laut zu ehren,", "tokens": ["Be\u00b7scheid\u00b7ner", "!", "nun", "ist", "uns", "ver\u00b7g\u00f6nnt", ",", "dich", "laut", "zu", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df manches Treugepflegten Dank,", "tokens": ["Da\u00df", "man\u00b7ches", "Treu\u00b7ge\u00b7pfleg\u00b7ten", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Thr\u00e4nen, wenn dein Sarg schon l\u00e4ngst versank,", "tokens": ["Da\u00df", "Thr\u00e4\u00b7nen", ",", "wenn", "dein", "Sarg", "schon", "l\u00e4ngst", "ver\u00b7sank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein edles Wirken auch die Welt noch kennen lehren!", "tokens": ["Dein", "ed\u00b7les", "Wir\u00b7ken", "auch", "die", "Welt", "noch", "ken\u00b7nen", "leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}