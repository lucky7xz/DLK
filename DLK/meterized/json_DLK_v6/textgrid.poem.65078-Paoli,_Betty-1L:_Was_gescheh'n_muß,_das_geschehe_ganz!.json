{"textgrid.poem.65078": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was gescheh'n mu\u00df, das geschehe ganz!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was gescheh'n mu\u00df, das geschehe ganz!", "tokens": ["Was", "ge\u00b7scheh'n", "mu\u00df", ",", "das", "ge\u00b7sche\u00b7he", "ganz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "$,", "PDS", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Trennung heischt des Schicksals strenges Walten,", "tokens": ["Tren\u00b7nung", "heischt", "des", "Schick\u00b7sals", "stren\u00b7ges", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Suche nicht von dem verwelkten Kranz'", "tokens": ["Su\u00b7che", "nicht", "von", "dem", "ver\u00b7welk\u00b7ten", "Kranz'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Einzler Bl\u00e4tter bang zur\u00fcckzuhalten.", "tokens": ["Einz\u00b7ler", "Bl\u00e4t\u00b7ter", "bang", "zu\u00b7r\u00fcck\u00b7zu\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Der du einsogst Sonnenlichtes Gluth,", "tokens": ["Der", "du", "ein\u00b7sogst", "Son\u00b7nen\u00b7lich\u00b7tes", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00df' dir's nicht am D\u00e4mmerschein' gen\u00fcgen!", "tokens": ["La\u00df'", "dir's", "nicht", "am", "D\u00e4m\u00b7mer\u00b7schein'", "ge\u00b7n\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wirf den gold'nen Becher in die Fluth,", "tokens": ["Wirf", "den", "gold'\u00b7nen", "Be\u00b7cher", "in", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da du ihn geleert mit raschen Z\u00fcgen.", "tokens": ["Da", "du", "ihn", "ge\u00b7leert", "mit", "ra\u00b7schen", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Was die Bl\u00fcthe deines Lebens war,", "tokens": ["Was", "die", "Bl\u00fc\u00b7the", "dei\u00b7nes", "Le\u00b7bens", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Soll nicht langsam sterben und vermodern,", "tokens": ["Soll", "nicht", "lang\u00b7sam", "ster\u00b7ben", "und", "ver\u00b7mo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nein, in Schmerzensflammen, rein und klar,", "tokens": ["Nein", ",", "in", "Schmer\u00b7zens\u00b7flam\u00b7men", ",", "rein", "und", "klar", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "La\u00df die sch\u00f6ne Leiche aufw\u00e4rts lodern!", "tokens": ["La\u00df", "die", "sch\u00f6\u00b7ne", "Lei\u00b7che", "auf\u00b7w\u00e4rts", "lo\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "O entweihe nicht ihr l\u00e4uternd Grab", "tokens": ["O", "ent\u00b7wei\u00b7he", "nicht", "ihr", "l\u00e4u\u00b7ternd", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Durch ein fruchtlos w\u00fchlendes Begehren!", "tokens": ["Durch", "ein", "frucht\u00b7los", "w\u00fch\u00b7len\u00b7des", "Be\u00b7geh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schlie\u00dfe kr\u00e4ftig mit der Zukunft ab,", "tokens": ["Schlie\u00b7\u00dfe", "kr\u00e4f\u00b7tig", "mit", "der", "Zu\u00b7kunft", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Um entschwund'ne Tage fromm zu ehren.", "tokens": ["Um", "ent\u00b7schwun\u00b7d'\u00b7ne", "Ta\u00b7ge", "fromm", "zu", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}