{"dta.poem.20773": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  einen jungen Herrn von Baronoff ,  \n jetzt Ru\u00df. Kaiserl. Kreisrichter in Ehstland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn im Geschlecht der Baronoffen               ", "tokens": ["Wenn", "im", "Ge\u00b7schlecht", "der", "Ba\u00b7ro\u00b7nof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit viel Jahrhunderten der Sohn", "tokens": ["Seit", "viel", "Jahr\u00b7hun\u00b7der\u00b7ten", "der", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Den Vater stets an Tugend \u00fcbertroffen,", "tokens": ["Den", "Va\u00b7ter", "stets", "an", "Tu\u00b7gend", "\u00fc\u00b7ber\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann wei\u00dft du deine Pflichten schon,", "tokens": ["Dann", "wei\u00dft", "du", "dei\u00b7ne", "Pflich\u00b7ten", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sind nicht leicht, doch solcher Ehre willen", "tokens": ["Sie", "sind", "nicht", "leicht", ",", "doch", "sol\u00b7cher", "Eh\u00b7re", "wil\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "ADV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "S\u00fc\u00df zu erf\u00fcllen.", "tokens": ["S\u00fc\u00df", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}