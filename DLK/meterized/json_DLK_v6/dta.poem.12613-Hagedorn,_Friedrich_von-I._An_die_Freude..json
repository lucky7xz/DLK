{"dta.poem.12613": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  An die Freude.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freude, G\u00f6ttinn edler Herzen!", "tokens": ["Freu\u00b7de", ",", "G\u00f6t\u00b7tinn", "ed\u00b7ler", "Her\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re mich.", "tokens": ["H\u00f6\u00b7re", "mich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "La\u00df die Lieder, die hier schallen,", "tokens": ["La\u00df", "die", "Lie\u00b7der", ",", "die", "hier", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich vergr\u00f6ssern, dir gefallen:", "tokens": ["Dich", "ver\u00b7gr\u00f6s\u00b7sern", ",", "dir", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was hier t\u00f6net, t\u00f6nt durch dich.", "tokens": ["Was", "hier", "t\u00f6\u00b7net", ",", "t\u00f6nt", "durch", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Muntre Schwester s\u00fcsser Liebe!", "tokens": ["Mun\u00b7tre", "Schwes\u00b7ter", "s\u00fcs\u00b7ser", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmels-Kind!", "tokens": ["Him\u00b7mels\u00b7Kind", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Kraft der Seelen! Halbes Leben!", "tokens": ["Kraft", "der", "See\u00b7len", "!", "Hal\u00b7bes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! was kann das Gl\u00fcck uns geben,", "tokens": ["Ach", "!", "was", "kann", "das", "Gl\u00fcck", "uns", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn man dich nicht auch gewinnt?", "tokens": ["Wenn", "man", "dich", "nicht", "auch", "ge\u00b7winnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stumme H\u00fcter todter Sch\u00e4tze", "tokens": ["Stum\u00b7me", "H\u00fc\u00b7ter", "tod\u00b7ter", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind nur reich.", "tokens": ["Sind", "nur", "reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dem, der keinen Schatz bewachet,", "tokens": ["Dem", ",", "der", "kei\u00b7nen", "Schatz", "be\u00b7wa\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinnreich scherzt und singt und lachet,", "tokens": ["Sinn\u00b7reich", "scherzt", "und", "singt", "und", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist kein karger K\u00f6nig gleich.", "tokens": ["Ist", "kein", "kar\u00b7ger", "K\u00f6\u00b7nig", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gib den Kennern, die dich ehren,", "tokens": ["Gib", "den", "Ken\u00b7nern", ",", "die", "dich", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neuen Muth:", "tokens": ["Neu\u00b7en", "Muth", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Neuen Scherz den regen Zungen,", "tokens": ["Neu\u00b7en", "Scherz", "den", "re\u00b7gen", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neue Fertigkeit den Jungen,", "tokens": ["Neu\u00b7e", "Fer\u00b7tig\u00b7keit", "den", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Alten neues Blut.", "tokens": ["Und", "den", "Al\u00b7ten", "neu\u00b7es", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du erheiterst, holde Freude,", "tokens": ["Du", "er\u00b7hei\u00b7terst", ",", "hol\u00b7de", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Vernunft.", "tokens": ["Die", "Ver\u00b7nunft", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Flieh, auf ewig, die Gesichter", "tokens": ["Flieh", ",", "auf", "e\u00b7wig", ",", "die", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJD", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller finstern Splitter-Richter,", "tokens": ["Al\u00b7ler", "fins\u00b7tern", "Split\u00b7ter\u00b7Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die ganze Heuchler-Zunft!", "tokens": ["Und", "die", "gan\u00b7ze", "Heuch\u00b7ler\u00b7Zunft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}