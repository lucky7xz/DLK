{"textgrid.poem.52120": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Pfaffen Kind rieff mir ein Glaubens Pein'ger zu:", "tokens": ["Ein", "Pfaf\u00b7fen", "Kind", "rieff", "mir", "ein", "Glau\u00b7bens", "Pein'\u00b7ger", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist ein Huren Kind. Ich sprach: Freund, wer bist du?", "tokens": ["Das", "ist", "ein", "Hu\u00b7ren", "Kind", ".", "Ich", "sprach", ":", "Freund", ",", "wer", "bist", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$.", "PPER", "VVFIN", "$.", "NN", "$,", "PWS", "VAFIN", "PPER", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Wo man die Ehe bricht, und sie verbeut nicht minder,", "tokens": ["Wo", "man", "die", "E\u00b7he", "bricht", ",", "und", "sie", "ver\u00b7beut", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verzeih es mir, da sind die meisten Huren Kinder.", "tokens": ["Ver\u00b7zeih", "es", "mir", ",", "da", "sind", "die", "meis\u00b7ten", "Hu\u00b7ren", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "ADV", "VAFIN", "ART", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}