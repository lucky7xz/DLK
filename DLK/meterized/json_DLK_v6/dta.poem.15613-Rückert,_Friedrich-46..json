{"dta.poem.15613": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist nur, weil du bist K\u00f6rper, k\u00f6rperlich;", "tokens": ["Die", "Welt", "ist", "nur", ",", "weil", "du", "bist", "K\u00f6r\u00b7per", ",", "k\u00f6r\u00b7per\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "PPER", "VAFIN", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist geht frei hindurch und nirgend st\u00f6\u00dft er sich.", "tokens": ["Der", "Geist", "geht", "frei", "hin\u00b7durch", "und", "nir\u00b7gend", "st\u00f6\u00dft", "er", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV", "KON", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ist der Vorschub, den die Geistigkeit dir leistet:", "tokens": ["Das", "ist", "der", "Vor\u00b7schub", ",", "den", "die", "Geis\u00b7tig\u00b7keit", "dir", "leis\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Welt st\u00f6\u00dft minder dich, jemehr du dich ergeistet.", "tokens": ["Die", "Welt", "st\u00f6\u00dft", "min\u00b7der", "dich", ",", "je\u00b7mehr", "du", "dich", "er\u00b7geis\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "wor