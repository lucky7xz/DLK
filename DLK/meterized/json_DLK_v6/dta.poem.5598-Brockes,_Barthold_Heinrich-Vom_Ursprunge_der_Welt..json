{"dta.poem.5598": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Ursprunge der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den ersten Augenblick, wenn unser Auge schaut", "tokens": ["Den", "ers\u00b7ten", "Au\u00b7gen\u00b7blick", ",", "wenn", "un\u00b7ser", "Au\u00b7ge", "schaut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Gegenw\u00fcrffe Zahl, die wir begreiffen wollen,", "tokens": ["Der", "Ge\u00b7gen\u00b7w\u00fcrf\u00b7fe", "Zahl", ",", "die", "wir", "be\u00b7greif\u00b7fen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erfodert es das Recht, da\u00df wir uns fragen sollen:", "tokens": ["Er\u00b7fo\u00b7dert", "es", "das", "Recht", ",", "da\u00df", "wir", "uns", "fra\u00b7gen", "sol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ist die Welt? Durch wen, und wie ward sie gebaut,", "tokens": ["Was", "ist", "die", "Welt", "?", "Durch", "wen", ",", "und", "wie", "ward", "sie", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "APPR", "PWS", "$,", "KON", "PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und in den Stand gesetzt? Ohn Anfangs gleich zu gehn", "tokens": ["Und", "in", "den", "Stand", "ge\u00b7setzt", "?", "Ohn", "An\u00b7fangs", "gleich", "zu", "gehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$.", "APPR", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zur Heilgen Schrifft, zu fassen diese Wahrheit;", "tokens": ["Zur", "Heil\u00b7gen", "Schrifft", ",", "zu", "fas\u00b7sen", "die\u00b7se", "Wahr\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gebrauche man zuerst, um dieses zu verstehn,", "tokens": ["Ge\u00b7brau\u00b7che", "man", "zu\u00b7erst", ",", "um", "die\u00b7ses", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "$,", "KOUI", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des menschlichen Verstandes Licht und Klarheit.", "tokens": ["Des", "menschli\u00b7chen", "Ver\u00b7stan\u00b7des", "Licht", "und", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Es leget selbiger uns hier,", "tokens": ["Es", "le\u00b7get", "sel\u00b7bi\u00b7ger", "uns", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von sonderer Beschaffenheit,", "tokens": ["Von", "son\u00b7de\u00b7rer", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die allergr\u00f6ste Aufgab f\u00fcr:", "tokens": ["Die", "al\u00b7ler\u00b7gr\u00f6s\u00b7te", "Auf\u00b7gab", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wie ward die Welt? Ist sie von Ewigkeit?", "tokens": ["Wie", "ward", "die", "Welt", "?", "Ist", "sie", "von", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}