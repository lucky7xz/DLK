{"dta.poem.21727": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Zweifel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einmal noch, o Auserw&#228;hlte,", "tokens": ["Ein\u00b7mal", "noch", ",", "o", "Au\u00b7serw", "&#228;", "hl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "XML_entity", "word", "punct"], "pos": ["ADV", "ADV", "$,", "FM", "FM", "$(", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sink' ich an dein athmend Herz.", "tokens": ["Sink'", "ich", "an", "dein", "ath\u00b7mend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir vertraut der Tiefgequ\u00e4lte", "tokens": ["Dir", "ver\u00b7traut", "der", "Tief\u00b7ge\u00b7qu\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Sorgen, seinen Schmerz.", "tokens": ["Sei\u00b7ne", "Sor\u00b7gen", ",", "sei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir am Busen wimmernd liegend,", "tokens": ["Dir", "am", "Bu\u00b7sen", "wim\u00b7mernd", "lie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werd' ich alles Zagens los.", "tokens": ["Werd'", "ich", "al\u00b7les", "Za\u00b7gens", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich umflechtend, dich umschmiegend", "tokens": ["Dich", "um\u00b7flech\u00b7tend", ",", "dich", "um\u00b7schmie\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVPP", "$,", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchl' ich stark mich, k\u00fchn und gross.", "tokens": ["F\u00fchl'", "ich", "stark", "mich", ",", "k\u00fchn", "und", "gross", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf des Zweifelmuthes Wellen", "tokens": ["Auf", "des", "Zwei\u00b7fel\u00b7mu\u00b7thes", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwankt der Hoffnung leichter Kahn;", "tokens": ["Schwankt", "der", "Hoff\u00b7nung", "leich\u00b7ter", "Kahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrme geisseln, Strudel schwellen", "tokens": ["St\u00fcr\u00b7me", "geis\u00b7seln", ",", "Stru\u00b7del", "schwel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den geschw\u00e4rzten Ozean;", "tokens": ["Den", "ge\u00b7schw\u00e4rz\u00b7ten", "O\u00b7ze\u00b7an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schleudern itzt den morschen Nachen", "tokens": ["Schleu\u00b7dern", "itzt", "den", "mor\u00b7schen", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00e4umend bis zum Sirius,", "tokens": ["Sch\u00e4u\u00b7mend", "bis", "zum", "Si\u00b7rius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.7": {"text": "St\u00fcrzen dann mit lautem Krachen", "tokens": ["St\u00fcr\u00b7zen", "dann", "mit", "lau\u00b7tem", "Kra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nieder ihn zum Erebus.", "tokens": ["Nie\u00b7der", "ihn", "zum", "E\u00b7re\u00b7bus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manches schmeichlerische Hoffen", "tokens": ["Man\u00b7ches", "schmeich\u00b7le\u00b7ri\u00b7sche", "Hof\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00fcstert leisen Trost mir zu,", "tokens": ["Fl\u00fcs\u00b7tert", "lei\u00b7sen", "Trost", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Elysium steht mir offen,", "tokens": ["Und", "E\u00b7ly\u00b7si\u00b7um", "steht", "mir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Und der Orkus schliesst sich zu.", "tokens": ["Und", "der", "Or\u00b7kus", "schliesst", "sich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um mich s\u00e4useln Edensl\u00fcfte,", "tokens": ["Um", "mich", "s\u00e4u\u00b7seln", "E\u00b7dens\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um mich lispelt Lautenklang.", "tokens": ["Um", "mich", "lis\u00b7pelt", "Lau\u00b7ten\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fernher wehn Violend\u00fcfte,", "tokens": ["Fern\u00b7her", "wehn", "Vi\u00b7o\u00b7len\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fernher fl\u00f6tet Brautgesang.", "tokens": ["Fern\u00b7her", "fl\u00f6\u00b7tet", "Braut\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seligkeit, die mich durchschaudert,", "tokens": ["Se\u00b7lig\u00b7keit", ",", "die", "mich", "durch\u00b7schau\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ahnung, die mich himmelw\u00e4rts", "tokens": ["Ah\u00b7nung", ",", "die", "mich", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Fl\u00fcgelt, dir zu glauben, zaudert", "tokens": ["Fl\u00fc\u00b7gelt", ",", "dir", "zu", "glau\u00b7ben", ",", "zau\u00b7dert"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Verzagten zweifelnd Herz.", "tokens": ["Des", "Ver\u00b7zag\u00b7ten", "zwei\u00b7felnd", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, den Bl\u00f6den, nein, den Armen", "tokens": ["Nein", ",", "den", "Bl\u00f6\u00b7den", ",", "nein", ",", "den", "Ar\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "PTKANT", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meint Adelens Liebe nicht.", "tokens": ["Meint", "A\u00b7de\u00b7lens", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nein, nicht Lieb' ists, nur Erbarmen,", "tokens": ["Nein", ",", "nicht", "Lieb'", "ists", ",", "nur", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKNEG", "NN", "VAFIN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was Adelens Auge spricht.", "tokens": ["Was", "A\u00b7de\u00b7lens", "Au\u00b7ge", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}