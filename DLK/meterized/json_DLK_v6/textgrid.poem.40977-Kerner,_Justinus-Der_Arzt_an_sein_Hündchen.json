{"textgrid.poem.40977": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Arzt an sein H\u00fcndchen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treues Tier! wenn freudig du", "tokens": ["Treu\u00b7es", "Tier", "!", "wenn", "freu\u00b7dig", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KOUS", "ADJD", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufspringst und dein Schw\u00e4nzlein lacht,", "tokens": ["Auf\u00b7springst", "und", "dein", "Schw\u00e4nz\u00b7lein", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn man weckt aus Schlafes Ruh'", "tokens": ["Wenn", "man", "weckt", "aus", "Schla\u00b7fes", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen Herrn um Mitternacht,", "tokens": ["Dei\u00b7nen", "Herrn", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn dann du vor der T\u00fcr", "tokens": ["Und", "wenn", "dann", "du", "vor", "der", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Bellst und eilest mitzugehn,", "tokens": ["Bellst", "und", "ei\u00b7lest", "mit\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein' ich, da\u00df du, gutes Tier!", "tokens": ["Mein'", "ich", ",", "da\u00df", "du", ",", "gu\u00b7tes", "Tier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr als ich f\u00fchlst Menschenwehn.", "tokens": ["Mehr", "als", "ich", "f\u00fchlst", "Men\u00b7schen\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als ein leichter Geist voran", "tokens": ["Als", "ein", "leich\u00b7ter", "Geist", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Laufst du deinem schweren Herrn,", "tokens": ["Laufst", "du", "dei\u00b7nem", "schwe\u00b7ren", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der geht seufzend seine Bahn,", "tokens": ["Der", "geht", "seuf\u00b7zend", "sei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du doch gehst sie freudig gern.", "tokens": ["Du", "doch", "gehst", "sie", "freu\u00b7dig", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In dir lebt ein innres Schaun,", "tokens": ["In", "dir", "lebt", "ein", "inn\u00b7res", "Schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das dem Menschenhirn gebricht,", "tokens": ["Das", "dem", "Men\u00b7schen\u00b7hirn", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6chte oft dich fragen, traun!", "tokens": ["M\u00f6ch\u00b7te", "oft", "dich", "fra\u00b7gen", ",", "traun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$,", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stirbt ", "tokens": ["Stirbt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Denn schon kam's, da\u00df meinem Blick", "tokens": ["Denn", "schon", "kam's", ",", "da\u00df", "mei\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer schien noch frisch und rot,", "tokens": ["Ei\u00b7ner", "schien", "noch", "frisch", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du doch flohst vor ihm zur\u00fcck,", "tokens": ["Du", "doch", "flohst", "vor", "ihm", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dann folgte bald sein Tod.", "tokens": ["Und", "dann", "folg\u00b7te", "bald", "sein", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schaust wohl auch, du gutes Tier,", "tokens": ["Schaust", "wohl", "auch", ",", "du", "gu\u00b7tes", "Tier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir den Tod schon im Gesicht,", "tokens": ["Mir", "den", "Tod", "schon", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treu doch gehst du nicht von mir,", "tokens": ["Treu", "doch", "gehst", "du", "nicht", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4ssest den bald Toten nicht.", "tokens": ["L\u00e4s\u00b7sest", "den", "bald", "To\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Durch die Stra\u00dfen dieser Stadt", "tokens": ["Durch", "die", "Stra\u00b7\u00dfen", "die\u00b7ser", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst du wohl noch mit mir gehn,", "tokens": ["Wirst", "du", "wohl", "noch", "mit", "mir", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn den Leib die Erde hat,", "tokens": ["Wenn", "den", "Leib", "die", "Er\u00b7de", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du nur wirst den Geist noch sehn.", "tokens": ["Du", "nur", "wirst", "den", "Geist", "noch", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}