{"textgrid.poem.48215": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "7. Erfolganbeter", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie hab' ich ein dummeres St\u00fcck gelesen.", "tokens": ["Nie", "hab'", "ich", "ein", "dum\u00b7me\u00b7res", "St\u00fcck", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbdas Haus ist ausverkauft gewesen.\u00ab", "tokens": ["\u00bb", "das", "Haus", "ist", "aus\u00b7ver\u00b7kauft", "ge\u00b7we\u00b7sen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "VAPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Farbe, Linien, alles verschwommen.", "tokens": ["Far\u00b7be", ",", "Li\u00b7ni\u00b7en", ",", "al\u00b7les", "ver\u00b7schwom\u00b7men", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PIS", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "\u00bbdie Jury hat es angenommen.\u00ab", "tokens": ["\u00bb", "die", "Ju\u00b7ry", "hat", "es", "an\u00b7ge\u00b7nom\u00b7men", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NE", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Skandal ist seine Art zu leben.", "tokens": ["Ein", "Skan\u00b7dal", "ist", "sei\u00b7ne", "Art", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u00bbder Botschafter hat ihm ein Fest gegeben.\u00ab", "tokens": ["\u00bb", "der", "Bot\u00b7schaf\u00b7ter", "hat", "ihm", "ein", "Fest", "ge\u00b7ge\u00b7ben", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Glauben Sie mir: er ist ein Kujon.", "tokens": ["Glau\u00b7ben", "Sie", "mir", ":", "er", "ist", "ein", "Ku\u00b7jon", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00bbhat aber eine Taler-Million.\u00ab", "tokens": ["\u00bb", "hat", "a\u00b7ber", "ei\u00b7ne", "Ta\u00b7le\u00b7rMil\u00b7li\u00b7on", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}