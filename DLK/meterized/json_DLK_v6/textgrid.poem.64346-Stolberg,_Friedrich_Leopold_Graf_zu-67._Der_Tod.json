{"textgrid.poem.64346": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "67. Der Tod", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00e4uscht' ich mich selber? oder t\u00f6nt mir lieblich,", "tokens": ["T\u00e4uscht'", "ich", "mich", "sel\u00b7ber", "?", "o\u00b7der", "t\u00f6nt", "mir", "lieb\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie der Nachtigall Lied, des Todes Name?", "tokens": ["Wie", "der", "Nach\u00b7ti\u00b7gall", "Lied", ",", "des", "To\u00b7des", "Na\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wird mir auch sein rauschender naher Fittich", "tokens": ["Wird", "mir", "auch", "sein", "rau\u00b7schen\u00b7der", "na\u00b7her", "Fit\u00b7tich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwanenflug t\u00f6nen?", "tokens": ["Schwa\u00b7nen\u00b7flug", "t\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Trank ich nicht s\u00fc\u00dfen Nektar aus der Jugend", "tokens": ["Trank", "ich", "nicht", "s\u00fc\u00b7\u00dfen", "Nek\u00b7tar", "aus", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Freudeduftenden Becher, den die Freundschaft", "tokens": ["Freu\u00b7de\u00b7duf\u00b7ten\u00b7den", "Be\u00b7cher", ",", "den", "die", "Freund\u00b7schaft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mir mit Blumen, den die Natur mit Blumen", "tokens": ["Mir", "mit", "Blu\u00b7men", ",", "den", "die", "Na\u00b7tur", "mit", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "L\u00e4chelnd umwanden?", "tokens": ["L\u00e4\u00b7chelnd", "um\u00b7wan\u00b7den", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Freunde, den trank ich, und ihr freutet mein euch!", "tokens": ["Freun\u00b7de", ",", "den", "trank", "ich", ",", "und", "ihr", "freu\u00b7tet", "mein", "euch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "PPER", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wenn ich leere den Kelch des Todes, wollt ihr", "tokens": ["Wenn", "ich", "lee\u00b7re", "den", "Kelch", "des", "To\u00b7des", ",", "wollt", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "VMFIN", "PPER"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dann euch nicht der h\u00f6heren Freuden eures", "tokens": ["Dann", "euch", "nicht", "der", "h\u00f6\u00b7he\u00b7ren", "Freu\u00b7den", "eu\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PTKNEG", "ART", "ADJA", "NN", "PPOSAT"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Freundes erfreuen?", "tokens": ["Freun\u00b7des", "er\u00b7freu\u00b7en", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "---+-", "measure": "unknown.measure.single"}}, "stanza.4": {"line.1": {"text": "Freunde, wenn eure Thr\u00e4ne nur des Todes", "tokens": ["Freun\u00b7de", ",", "wenn", "eu\u00b7re", "Thr\u00e4\u00b7ne", "nur", "des", "To\u00b7des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kelch nicht bitter, das brechende Herz nicht weich macht,", "tokens": ["Kelch", "nicht", "bit\u00b7ter", ",", "das", "bre\u00b7chen\u00b7de", "Herz", "nicht", "weich", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "$,", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Krankheit mag mit zischenden Schlangen, Schmerz mit", "tokens": ["Krank\u00b7heit", "mag", "mit", "zi\u00b7schen\u00b7den", "Schlan\u00b7gen", ",", "Schmerz", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN", "$,", "NN", "APPR"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Dornen ihn kr\u00e4nzen!", "tokens": ["Dor\u00b7nen", "ihn", "kr\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Z\u00fcrnt ihr, Geliebte? Hab' ich denn dem Tode,", "tokens": ["Z\u00fcrnt", "ihr", ",", "Ge\u00b7lieb\u00b7te", "?", "Hab'", "ich", "denn", "dem", "To\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er komme, gerufen? schlingt, wie Weinlaub,", "tokens": ["Da\u00df", "er", "kom\u00b7me", ",", "ge\u00b7ru\u00b7fen", "?", "schlingt", ",", "wie", "Wein\u00b7laub", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVPP", "$.", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nicht um meiner nervichten Jugend Glieder", "tokens": ["Nicht", "um", "mei\u00b7ner", "ner\u00b7vich\u00b7ten", "Ju\u00b7gend", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sich die Gesundheit?", "tokens": ["Sich", "die", "Ge\u00b7sund\u00b7heit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Dennoch, wofern ich mich nicht t\u00e4usche, t\u00f6nt mir,", "tokens": ["Den\u00b7noch", ",", "wo\u00b7fern", "ich", "mich", "nicht", "t\u00e4u\u00b7sche", ",", "t\u00f6nt", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie der Nachtigall Lied, des Todes Name.", "tokens": ["Wie", "der", "Nach\u00b7ti\u00b7gall", "Lied", ",", "des", "To\u00b7des", "Na\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wird mir auch sein rauschender naher Fittich", "tokens": ["Wird", "mir", "auch", "sein", "rau\u00b7schen\u00b7der", "na\u00b7her", "Fit\u00b7tich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwanenflug t\u00f6nen?", "tokens": ["Schwa\u00b7nen\u00b7flug", "t\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}