{"dta.poem.23488": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fromb f\u00fcr Sich selbs/ vnd f\u00fcr jeder-", "tokens": ["Fromb", "f\u00fcr", "Sich", "selbs", "/", "vnd", "f\u00fcr", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "PRF", "ADV", "$(", "KON", "APPR", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "man gut", "tokens": ["man", "gut"], "token_info": ["word", "word"], "pos": ["PIS", "ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Thut Er l\u00f6blich sein volck regieren/", "tokens": ["Thut", "Er", "l\u00f6b\u00b7lich", "sein", "volck", "re\u00b7gie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ja wa m\u00fcglich will Er der b\u00f6sen blut", "tokens": ["Ja", "wa", "m\u00fcg\u00b7lich", "will", "Er", "der", "b\u00f6\u00b7sen", "blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "XY", "ADJD", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Vilmehr purgieren dan verlieren:", "tokens": ["Vil\u00b7mehr", "pur\u00b7gie\u00b7ren", "dan", "ver\u00b7lie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Das thut in Jhm der Gerechtigkeit macht/", "tokens": ["Das", "thut", "in", "Jhm", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Welche des Lands gewi\u00dfe vhr vnd wacht/", "tokens": ["Wel\u00b7che", "des", "Lands", "ge\u00b7wi\u00b7\u00dfe", "vhr", "vnd", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "NN", "KON", "VVFIN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Die Jhn niemahl lasset entschlafen/", "tokens": ["Die", "Jhn", "nie\u00b7mahl", "las\u00b7set", "ent\u00b7schla\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "VVINF", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wan zu verzeihen vnd zustrafen:", "tokens": ["Wan", "zu", "ver\u00b7zei\u00b7hen", "vnd", "zu\u00b7stra\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKZU", "VVINF", "KON", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Durch sie der Stand vnd das Geschlecht", "tokens": ["Durch", "sie", "der", "Stand", "vnd", "das", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zichen bey jhm die Wag nicht auf die seite\u0303;", "tokens": ["Zi\u00b7chen", "bey", "jhm", "die", "Wag", "nicht", "auf", "die", "seit\u1ebd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Durch Sie ist Recht niemahl Vnrecht/", "tokens": ["Durch", "Sie", "ist", "Recht", "nie\u00b7mahl", "Vn\u00b7recht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "ADV", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Noch Vnrecht Recht an jhm noch andern", "tokens": ["Noch", "Vn\u00b7recht", "Recht", "an", "jhm", "noch", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "NN", "APPR", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Leuten:", "tokens": ["Leu\u00b7ten", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Ja Sie ist gleich f\u00fcr gro\u00df vnd klein/", "tokens": ["Ja", "Sie", "ist", "gleich", "f\u00fcr", "gro\u00df", "vnd", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "ADV", "APPR", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Vnd Sie selbs hat mit guldinen buchstabe\u0303", "tokens": ["Vnd", "Sie", "selbs", "hat", "mit", "gul\u00b7di\u00b7nen", "buch\u00b7stab\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Dise wort in sein hertz gegraben:", "tokens": ["Di\u00b7se", "wort", "in", "sein", "hertz", "ge\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Des volcks wolfahrt soll dz h\u00f6chst Gesatz sein.", "tokens": ["Des", "volcks", "wol\u00b7fahrt", "soll", "dz", "h\u00f6chst", "Ge\u00b7satz", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "KOUS", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}