{"textgrid.poem.39854": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sahn wir bl\u00fchen dich vor wenig Wochen,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sahn wir bl\u00fchen dich vor wenig Wochen,", "tokens": ["Wie", "sahn", "wir", "bl\u00fc\u00b7hen", "dich", "vor", "we\u00b7nig", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Holdselig l\u00e4chelnd unter deinen Kleinen", "tokens": ["Hold\u00b7se\u00b7lig", "l\u00e4\u00b7chelnd", "un\u00b7ter", "dei\u00b7nen", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie eine Rose zwischen Knospen scheinen,", "tokens": ["Wie", "ei\u00b7ne", "Ro\u00b7se", "zwi\u00b7schen", "Knos\u00b7pen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die deine Sch\u00f6nheit vielfach ausgesprochen.", "tokens": ["Die", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "viel\u00b7fach", "aus\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt hat der eitle Schimmer dich bestochen,", "tokens": ["Jetzt", "hat", "der", "eit\u00b7le", "Schim\u00b7mer", "dich", "be\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich abgerissen von dem Kranz der Deinen,", "tokens": ["Dich", "ab\u00b7ge\u00b7ris\u00b7sen", "von", "dem", "Kranz", "der", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt, die lose Dirne, mu\u00dft' erscheinen", "tokens": ["Die", "Welt", ",", "die", "lo\u00b7se", "Dir\u00b7ne", ",", "mu\u00dft'", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hat die sch\u00f6ne Blume sich gebrochen.", "tokens": ["Und", "hat", "die", "sch\u00f6\u00b7ne", "Blu\u00b7me", "sich", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Durchzittert dich nicht je und je ein Schaudern?", "tokens": ["Durch\u00b7zit\u00b7tert", "dich", "nicht", "je", "und", "je", "ein", "Schau\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Los bist du von der Wurzel, von den Zweigen,", "tokens": ["Los", "bist", "du", "von", "der", "Wur\u00b7zel", ",", "von", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wirst verwelken am gemeinen Busen!", "tokens": ["Und", "wirst", "ver\u00b7wel\u00b7ken", "am", "ge\u00b7mei\u00b7nen", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So fl\u00fcstern hinter dir die keuschen Musen;", "tokens": ["So", "fl\u00fcs\u00b7tern", "hin\u00b7ter", "dir", "die", "keu\u00b7schen", "Mu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kehrst dich ab und eilest nach dem Reigen:", "tokens": ["Du", "kehrst", "dich", "ab", "und", "ei\u00b7lest", "nach", "dem", "Rei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbwas wollen diese M\u00e4gde mit mir plaudern?\u00ab", "tokens": ["\u00bb", "was", "wol\u00b7len", "die\u00b7se", "M\u00e4g\u00b7de", "mit", "mir", "plau\u00b7dern", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "PDAT", "NN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}