{"dta.poem.21978": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "79. Gott tr\u00e4gt vollkom\u0303ne Fr\u00fcchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wer mir Vollkom\u0303enheit wie Gott hat ab-wil-spreche\u0303/", "tokens": ["Wer", "mir", "Voll\u00b7kom\u0303en\u00b7heit", "wie", "Gott", "hat", "ab\u00b7wil\u00b7sprech\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KOKOM", "NN", "VAFIN", "NE", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Der m\u00fcste mich zuvor von seinem Weinstok brechen.", "tokens": ["Der", "m\u00fcs\u00b7te", "mich", "zu\u00b7vor", "von", "sei\u00b7nem", "Weins\u00b7tok", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}