{"textgrid.poem.38211": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ade zur guten Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond, der steht am h\u00f6chsten,", "tokens": ["Der", "Mond", ",", "der", "steht", "am", "h\u00f6chs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonn will untergehn,", "tokens": ["Die", "Sonn", "will", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Feinslieb liegt in N\u00f6then,", "tokens": ["Mein", "Feins\u00b7lieb", "liegt", "in", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach Gott, wie solls ihr gehn,", "tokens": ["Ach", "Gott", ",", "wie", "solls", "ihr", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Regen und in Wind,", "tokens": ["In", "Re\u00b7gen", "und", "in", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wo soll ich mich hinkehren,", "tokens": ["Wo", "soll", "ich", "mich", "hin\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da ich mein Feinslieb find!", "tokens": ["Da", "ich", "mein", "Feins\u00b7lieb", "find", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Feinslieb wollt mich lehren,", "tokens": ["Mein", "Feins\u00b7lieb", "wollt", "mich", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ich ihr dienen soll,", "tokens": ["Wie", "ich", "ihr", "die\u00b7nen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Z\u00fcchten und in Ehren,", "tokens": ["In", "Z\u00fcch\u00b7ten", "und", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das wei\u00df ich selbst gar wohl,", "tokens": ["Das", "wei\u00df", "ich", "selbst", "gar", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und kann auch noch viel mehr,", "tokens": ["Und", "kann", "auch", "noch", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wer sich seins Buhlen r\u00fchmet,", "tokens": ["Wer", "sich", "seins", "Buh\u00b7len", "r\u00fch\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem bringt es wenig Ehr.", "tokens": ["Dem", "bringt", "es", "we\u00b7nig", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mancher geht zu seinem Buhlen", "tokens": ["Man\u00b7cher", "geht", "zu", "sei\u00b7nem", "Buh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei lichtem Mondenschein,", "tokens": ["Bei", "lich\u00b7tem", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was giebt sie ihm zum Lohne?", "tokens": ["Was", "giebt", "sie", "ihm", "zum", "Loh\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Rosenkr\u00e4nzelein,", "tokens": ["Ein", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist gr\u00fcner als der Klee,", "tokens": ["Ist", "gr\u00fc\u00b7ner", "als", "der", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich mu\u00df mich von dir scheiden,", "tokens": ["Ich", "mu\u00df", "mich", "von", "dir", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Thut meinem Herzen weh.", "tokens": ["Thut", "mei\u00b7nem", "Her\u00b7zen", "weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach Scheiden \u00fcber Scheiden,", "tokens": ["Ach", "Schei\u00b7den", "\u00fc\u00b7ber", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer hat dich doch erdacht,", "tokens": ["Wer", "hat", "dich", "doch", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast mir mein junges Herze", "tokens": ["Hast", "mir", "mein", "jun\u00b7ges", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Freud in Trauren bracht,", "tokens": ["Aus", "Freud", "in", "Trau\u00b7ren", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dazu in Ungemach.", "tokens": ["Da\u00b7zu", "in", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dir ists sch\u00f6ns Lieb gesungen,", "tokens": ["Dir", "ists", "sch\u00f6ns", "Lieb", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ade zu guter Nacht.", "tokens": ["A\u00b7de", "zu", "gu\u00b7ter", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}