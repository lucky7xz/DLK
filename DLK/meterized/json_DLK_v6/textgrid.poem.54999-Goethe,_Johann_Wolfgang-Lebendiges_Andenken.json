{"textgrid.poem.54999": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Lebendiges Andenken", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Liebsten Band und Schleife rauben,", "tokens": ["Der", "Liebs\u00b7ten", "Band", "und", "Schlei\u00b7fe", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halb mag sie z\u00fcrnen, halb erlauben,", "tokens": ["Halb", "mag", "sie", "z\u00fcr\u00b7nen", ",", "halb", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Euch ist es viel, ich will es glauben", "tokens": ["Euch", "ist", "es", "viel", ",", "ich", "will", "es", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und g\u00f6nn euch solchen Selbstbetrug:", "tokens": ["Und", "g\u00f6nn", "euch", "sol\u00b7chen", "Selbst\u00b7be\u00b7trug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Schleier, Halstuch, Strumpfband, Ringe", "tokens": ["Ein", "Schlei\u00b7er", ",", "Hal\u00b7stuch", ",", "Strumpf\u00b7band", ",", "Rin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind wahrlich keine kleinen Dinge;", "tokens": ["Sind", "wahr\u00b7lich", "kei\u00b7ne", "klei\u00b7nen", "Din\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Allein mir sind sie nicht genug.", "tokens": ["Al\u00b7lein", "mir", "sind", "sie", "nicht", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lebend'gen Teil von ihrem Leben,", "tokens": ["Le\u00b7ben\u00b7d'\u00b7gen", "Teil", "von", "ih\u00b7rem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihn hat nach leisem Widerstreben", "tokens": ["Ihn", "hat", "nach", "lei\u00b7sem", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Allerliebste mir gegeben,", "tokens": ["Die", "Al\u00b7ler\u00b7liebs\u00b7te", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jene Herrlichkeit wird nichts.", "tokens": ["Und", "je\u00b7ne", "Herr\u00b7lich\u00b7keit", "wird", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lach ich all der Tr\u00f6delware!", "tokens": ["Wie", "lach", "ich", "all", "der", "Tr\u00f6\u00b7del\u00b7wa\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schenkte mir die sch\u00f6nen Haare,", "tokens": ["Sie", "schenk\u00b7te", "mir", "die", "sch\u00f6\u00b7nen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Schmuck des sch\u00f6nsten Angesichts.", "tokens": ["Den", "Schmuck", "des", "sch\u00f6ns\u00b7ten", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Soll ich dich gleich, Geliebte, missen,", "tokens": ["Soll", "ich", "dich", "gleich", ",", "Ge\u00b7lieb\u00b7te", ",", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "$,", "NN", "$,", "VVPP", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wirst du mir doch nicht ganz entrissen:", "tokens": ["Wirst", "du", "mir", "doch", "nicht", "ganz", "ent\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu schaun, zu t\u00e4ndeln und zu k\u00fcssen", "tokens": ["Zu", "schaun", ",", "zu", "t\u00e4n\u00b7deln", "und", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt die Reliquie von dir. \u2013", "tokens": ["Bleibt", "die", "Re\u00b7li\u00b7qui\u00b7e", "von", "dir", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Gleich ist des Haars und mein Geschicke;", "tokens": ["Gleich", "ist", "des", "Haars", "und", "mein", "Ge\u00b7schi\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst buhlten wir mit ", "tokens": ["Sonst", "buhl\u00b7ten", "wir", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Um sie, jetzt sind wir fern von ihr.", "tokens": ["Um", "sie", ",", "jetzt", "sind", "wir", "fern", "von", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fest waren wir an sie gehangen;", "tokens": ["Fest", "wa\u00b7ren", "wir", "an", "sie", "ge\u00b7han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wir streichelten die runden Wangen,", "tokens": ["Wir", "strei\u00b7chel\u00b7ten", "die", "run\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Uns lockt' und zog ein s\u00fc\u00df Verlangen,", "tokens": ["Uns", "lockt'", "und", "zog", "ein", "s\u00fc\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir gleiteten zur vollern Brust.", "tokens": ["Wir", "glei\u00b7te\u00b7ten", "zur", "vol\u00b7lern", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Nebenbuhler, frei von Neide,", "tokens": ["O", "Ne\u00b7ben\u00b7buh\u00b7ler", ",", "frei", "von", "Nei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du s\u00fc\u00df Geschenk, du sch\u00f6ne Beute,", "tokens": ["Du", "s\u00fc\u00df", "Ge\u00b7schenk", ",", "du", "sch\u00f6\u00b7ne", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erinnre mich an Gl\u00fcck und Lust!", "tokens": ["E\u00b7rinn\u00b7re", "mich", "an", "Gl\u00fcck", "und", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}