{"dta.poem.7490": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Einem Schauspieler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, ich will Kugeln gie\u00dfen aus den Lettern,", "tokens": ["Ja", ",", "ich", "will", "Ku\u00b7geln", "gie\u00b7\u00dfen", "aus", "den", "Let\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6r' ich die Stunde der Erl\u00f6sung schlagen,", "tokens": ["H\u00f6r'", "ich", "die", "Stun\u00b7de", "der", "Er\u00b7l\u00f6\u00b7sung", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die Welt nicht suchen mehr auf Deinen Brettern.", "tokens": ["Die", "Welt", "nicht", "su\u00b7chen", "mehr", "auf", "Dei\u00b7nen", "Bret\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gilt es, der Erde G\u00f6tzen zu zerschmettern,", "tokens": ["Gilt", "es", ",", "der", "Er\u00b7de", "G\u00f6t\u00b7zen", "zu", "zer\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Ich kenne Dich, Du wirst Dein Leben wagen.", "tokens": ["Ich", "ken\u00b7ne", "Dich", ",", "Du", "wirst", "Dein", "Le\u00b7ben", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer unsers Friedens dr\u00fcckend Joch getragen,", "tokens": ["Wer", "un\u00b7sers", "Frie\u00b7dens", "dr\u00fc\u00b7ckend", "Joch", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem graut auch wahrlich nicht vor Sturm und Wettern.", "tokens": ["Dem", "graut", "auch", "wahr\u00b7lich", "nicht", "vor", "Sturm", "und", "Wet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bis dahin aber opfere dem Sch\u00f6nen", "tokens": ["Bis", "da\u00b7hin", "a\u00b7ber", "op\u00b7fe\u00b7re", "dem", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PAV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So treu, wie jetzt, und heisse nicht despotisch", "tokens": ["So", "treu", ",", "wie", "jetzt", ",", "und", "heis\u00b7se", "nicht", "des\u00b7po\u00b7tisch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dein Herz zu fr\u00fch desselben sich entw\u00f6hnen.", "tokens": ["Dein", "Herz", "zu", "fr\u00fch", "des\u00b7sel\u00b7ben", "sich", "ent\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKA", "ADJD", "PDS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So Manche macht die Freiheit jetzt zelotisch,", "tokens": ["So", "Man\u00b7che", "macht", "die", "Frei\u00b7heit", "jetzt", "ze\u00b7lo\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie, Barbaren gleich, die Kunst verh\u00f6hnen;", "tokens": ["Da\u00df", "sie", ",", "Bar\u00b7ba\u00b7ren", "gleich", ",", "die", "Kunst", "ver\u00b7h\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-----+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sei lieber g\u00f6thisch, theurer Freund, als gothisch!", "tokens": ["Sei", "lie\u00b7ber", "g\u00f6\u00b7thisch", ",", "theu\u00b7rer", "Freund", ",", "als", "go\u00b7thisch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "ADJD", "NN", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}