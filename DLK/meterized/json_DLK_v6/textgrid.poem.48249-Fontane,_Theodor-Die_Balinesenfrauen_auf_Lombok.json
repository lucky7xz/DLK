{"textgrid.poem.48249": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Balinesenfrauen auf Lombok", "genre": "verse", "period": "N.A.", "pub_year": 1895, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unerh\u00f6rt,", "tokens": ["Un\u00b7er\u00b7h\u00f6rt", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Auf Lombok hat man sich emp\u00f6rt,", "tokens": ["Auf", "Lom\u00b7bok", "hat", "man", "sich", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PIS", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf der Insel Lombok die Balinesen", "tokens": ["Auf", "der", "In\u00b7sel", "Lom\u00b7bok", "die", "Ba\u00b7li\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Sind mit Mynheer unzufrieden gewesen.", "tokens": ["Sind", "mit", "My\u00b7nheer", "un\u00b7zu\u00b7frie\u00b7den", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und die Mynheers fa\u00dft ein Z\u00fcrnen und Schaudern,", "tokens": ["Und", "die", "My\u00b7nheers", "fa\u00dft", "ein", "Z\u00fcr\u00b7nen", "und", "Schau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbaus mit dem Brand, ohne Z\u00f6gern und Zaudern,\u00ab", "tokens": ["\u00bb", "aus", "mit", "dem", "Brand", ",", "oh\u00b7ne", "Z\u00f6\u00b7gern", "und", "Zau\u00b7dern", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "APPR", "ART", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und allerlei Volk, verkracht, verdorben,", "tokens": ["Und", "al\u00b7ler\u00b7lei", "Volk", ",", "ver\u00b7kracht", ",", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wird von Mynheer angeworben,", "tokens": ["Wird", "von", "My\u00b7nheer", "an\u00b7ge\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Allerlei Leute mit Mausergewehren", "tokens": ["Al\u00b7ler\u00b7lei", "Leu\u00b7te", "mit", "Mau\u00b7ser\u00b7ge\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN"], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Sollen die Balinesen bekehren.", "tokens": ["Sol\u00b7len", "die", "Ba\u00b7li\u00b7ne\u00b7sen", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Vorw\u00e4rts, ohne Sinn und Plan,", "tokens": ["Vor\u00b7w\u00e4rts", ",", "oh\u00b7ne", "Sinn", "und", "Plan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber auch planlos wird es getan,", "tokens": ["A\u00b7ber", "auch", "plan\u00b7los", "wird", "es", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Hinterlader arbeitete gut,", "tokens": ["Hin\u00b7ter\u00b7la\u00b7der", "ar\u00b7bei\u00b7te\u00b7te", "gut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Und die M\u00e4nner liegen in ihrem Blut.", "tokens": ["Und", "die", "M\u00e4n\u00b7ner", "lie\u00b7gen", "in", "ih\u00b7rem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die M\u00e4nner. Aber gro\u00df anzuschaun", "tokens": ["Die", "M\u00e4n\u00b7ner", ".", "A\u00b7ber", "gro\u00df", "an\u00b7zu\u00b7schaun"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "ADJD", "VVIZU"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind da noch sechzig stolze Fraun,", "tokens": ["Sind", "da", "noch", "sech\u00b7zig", "stol\u00b7ze", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "All eingeschlossen zu Wehr und Trutz", "tokens": ["All", "ein\u00b7ge\u00b7schlos\u00b7sen", "zu", "Wehr", "und", "Trutz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In eines Buddha-Tempels Schutz.", "tokens": ["In", "ei\u00b7nes", "Bud\u00b7dha\u00b7Tem\u00b7pels", "Schutz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Reichgekleidet, goldgeschm\u00fcckt,", "tokens": ["Reich\u00b7ge\u00b7klei\u00b7det", ",", "gold\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr j\u00fcngstes Kind an die Brust gedr\u00fcckt,", "tokens": ["Ihr", "j\u00fcngs\u00b7tes", "Kind", "an", "die", "Brust", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Hochaufgericht't eine jede stand,", "tokens": ["Hoch\u00b7auf\u00b7ge\u00b7richt't", "ei\u00b7ne", "je\u00b7de", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIAT", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Feind im Auge, den Dolch in der Hand.", "tokens": ["Den", "Feind", "im", "Au\u00b7ge", ",", "den", "Dolch", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Kugeln durchschlagen Trepp' und Dach,", "tokens": ["Die", "Ku\u00b7geln", "durch\u00b7schla\u00b7gen", "Trepp'", "und", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbwozu hier noch warten, feig und schwach?\u00ab", "tokens": ["\u00bb", "wo\u00b7zu", "hier", "noch", "war\u00b7ten", ",", "feig", "und", "schwach", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADV", "ADV", "VVINF", "$,", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die T\u00fcren auf und hinab ins Tal,", "tokens": ["Und", "die", "T\u00fc\u00b7ren", "auf", "und", "hin\u00b7ab", "ins", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hoch ihr Kind und hoch den Stahl", "tokens": ["Hoch", "ihr", "Kind", "und", "hoch", "den", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "KON", "ADJD", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "(am Griffe funkelt der Edelstein),", "tokens": ["(", "am", "Grif\u00b7fe", "fun\u00b7kelt", "der", "E\u00b7del\u00b7stein", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "VVFIN", "ART", "NN", "$(", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So st\u00fcrzen sie sich in des Feindes Reihn.", "tokens": ["So", "st\u00fcr\u00b7zen", "sie", "sich", "in", "des", "Fein\u00b7des", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die H\u00e4lfte f\u00e4llt tot, die H\u00e4lfte f\u00e4llt wund,", "tokens": ["Die", "H\u00e4lf\u00b7te", "f\u00e4llt", "tot", ",", "die", "H\u00e4lf\u00b7te", "f\u00e4llt", "wund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Aber jede will sterben zu dieser Stund,", "tokens": ["A\u00b7ber", "je\u00b7de", "will", "ster\u00b7ben", "zu", "die\u00b7ser", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "VVINF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und die Letzten, in stolzer Todeslust,", "tokens": ["Und", "die", "Letz\u00b7ten", ",", "in", "stol\u00b7zer", "To\u00b7des\u00b7lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Sto\u00dfen den Dolch sich in die Brust.", "tokens": ["Sto\u00b7\u00dfen", "den", "Dolch", "sich", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Mynheer derweilen, in seinem Kontor,", "tokens": ["My\u00b7nheer", "der\u00b7wei\u00b7len", ",", "in", "sei\u00b7nem", "Kon\u00b7tor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Malt sich christlich Kulturelles vor.", "tokens": ["Malt", "sich", "christ\u00b7lich", "Kul\u00b7tu\u00b7rel\u00b7les", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}