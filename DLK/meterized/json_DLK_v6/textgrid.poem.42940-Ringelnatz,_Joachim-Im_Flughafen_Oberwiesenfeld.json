{"textgrid.poem.42940": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Im Flughafen Oberwiesenfeld", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Flugplatz vor der Restauration", "tokens": ["Am", "Flug\u00b7platz", "vor", "der", "Res\u00b7tau\u00b7ra\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sitzen wir morgens im Garten,", "tokens": ["Sit\u00b7zen", "wir", "mor\u00b7gens", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Trinken Whisky und warten. \u2013", "tokens": ["Trin\u00b7ken", "Whis\u00b7ky", "und", "war\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "KON", "VVINF", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ein Russe singt aus dem Grammophon.", "tokens": ["Ein", "Rus\u00b7se", "singt", "aus", "dem", "Gram\u00b7mo\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Flugzeuge landen von Zeit zu Zeit,", "tokens": ["Flug\u00b7zeu\u00b7ge", "lan\u00b7den", "von", "Zeit", "zu", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und jedes aus anderer Gegend.", "tokens": ["Und", "je\u00b7des", "aus", "an\u00b7de\u00b7rer", "Ge\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich h\u00f6re, da\u00df es in Bozen schneit", "tokens": ["Ich", "h\u00f6\u00b7re", ",", "da\u00df", "es", "in", "Bo\u00b7zen", "schneit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NE", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und da\u00df es in Hamburg regnet.", "tokens": ["Und", "da\u00df", "es", "in", "Ham\u00b7burg", "reg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hab eine arktische Landschaft gemalt.", "tokens": ["Ich", "hab", "ei\u00b7ne", "ark\u00b7ti\u00b7sche", "Land\u00b7schaft", "ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein Herr hat das Bild gekauft und bezahlt,", "tokens": ["Ein", "Herr", "hat", "das", "Bild", "ge\u00b7kauft", "und", "be\u00b7zahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und ich wei\u00df, da\u00df er darauf wartet.", "tokens": ["Und", "ich", "wei\u00df", ",", "da\u00df", "er", "da\u00b7rauf", "war\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PAV", "VVFIN", "$."], "meter": "--+--+++-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wir setzen das Bild \u2013 als w\u00e4r es ein Hauch \u2013", "tokens": ["Wir", "set\u00b7zen", "das", "Bild", "\u2013", "als", "w\u00e4r", "es", "ein", "Hauch", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KOKOM", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ganz zart in eines Flugzeuges Bauch.", "tokens": ["Ganz", "zart", "in", "ei\u00b7nes", "Flug\u00b7zeu\u00b7ges", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und nun: Dieses Flugzeug startet.", "tokens": ["Und", "nun", ":", "Die\u00b7ses", "Flug\u00b7zeug", "star\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Flieg wohl, du Junkers, du stolzer,", "tokens": ["Flieg", "wohl", ",", "du", "Jun\u00b7kers", ",", "du", "stol\u00b7zer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit meinem eiskalten Bild im Leib!", "tokens": ["Mit", "mei\u00b7nem", "eis\u00b7kal\u00b7ten", "Bild", "im", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gr\u00fc\u00df Z\u00fcrich, H\u00fcgin und dessen Weib", "tokens": ["Gr\u00fc\u00df", "Z\u00fc\u00b7rich", ",", "H\u00fc\u00b7gin", "und", "des\u00b7sen", "Weib"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NN", "KON", "PRELAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und euren Herrn Mittelholzer!", "tokens": ["Und", "eu\u00b7ren", "Herrn", "Mit\u00b7tel\u00b7hol\u00b7zer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}