{"textgrid.poem.46649": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[das ist meine Klage]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist meine Klage,", "tokens": ["Das", "ist", "mei\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Da\u00df vor dieser Plage", "tokens": ["Da\u00df", "vor", "die\u00b7ser", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Selbst verstummt die Klage.", "tokens": ["Selbst", "ver\u00b7stummt", "die", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie ich mich am Tage", "tokens": ["Wie", "ich", "mich", "am", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit den Sorgen schlage,", "tokens": ["Mit", "den", "Sor\u00b7gen", "schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ich n\u00e4chtlich zage,", "tokens": ["Wie", "ich", "n\u00e4cht\u00b7lich", "za\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Was ich st\u00fcndlich trage,", "tokens": ["Was", "ich", "st\u00fcnd\u00b7lich", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "L\u00e4\u00dft nicht Raum der Klage.", "tokens": ["L\u00e4\u00dft", "nicht", "Raum", "der", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wann, o Himmel, sage,", "tokens": ["Wann", ",", "o", "Him\u00b7mel", ",", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "L\u00f6sest du die Frage", "tokens": ["L\u00f6\u00b7sest", "du", "die", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der Entscheidungswage,", "tokens": ["Der", "Ent\u00b7schei\u00b7dungs\u00b7wa\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df ich nicht mehr zage,", "tokens": ["Da\u00df", "ich", "nicht", "mehr", "za\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sondern \u00fcberschlage,", "tokens": ["Son\u00b7dern", "\u00fc\u00b7bersc\u00b7hla\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mit Geduld ertrage,", "tokens": ["Mit", "Ge\u00b7duld", "er\u00b7tra\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und in Ruh beklage!", "tokens": ["Und", "in", "Ruh", "be\u00b7kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sonnenschein, o schlage,", "tokens": ["Son\u00b7nen\u00b7schein", ",", "o", "schla\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In die Flucht, verjage,", "tokens": ["In", "die", "Flucht", ",", "ver\u00b7ja\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Diese Nacht der Plage!", "tokens": ["Die\u00b7se", "Nacht", "der", "Pla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sommer, komm, ich trage", "tokens": ["Som\u00b7mer", ",", "komm", ",", "ich", "tra\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lust nach l\u00e4ngstem Tage,", "tokens": ["Lust", "nach", "l\u00e4ngs\u00b7tem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wann ich nicht mehr zage", "tokens": ["Wann", "ich", "nicht", "mehr", "za\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Neuer Niederlage,", "tokens": ["Neu\u00b7er", "Nie\u00b7der\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": 