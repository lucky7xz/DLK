{"textgrid.poem.61081": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich blick' in mein Herz, und ich blick' in die Welt,", "tokens": ["Ich", "blick'", "in", "mein", "Herz", ",", "und", "ich", "blick'", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bis vom Auge die brennende Tr\u00e4ne mir f\u00e4llt;", "tokens": ["Bis", "vom", "Au\u00b7ge", "die", "bren\u00b7nen\u00b7de", "Tr\u00e4\u00b7ne", "mir", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Wohl leuchtet die Ferne mit goldenem Licht,", "tokens": ["Wohl", "leuch\u00b7tet", "die", "Fer\u00b7ne", "mit", "gol\u00b7de\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch h\u00e4lt mich der Nord, ich erreiche sie nicht.", "tokens": ["Doch", "h\u00e4lt", "mich", "der", "Nord", ",", "ich", "er\u00b7rei\u00b7che", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "O die Schranken so eng, und die Welt so weit,", "tokens": ["O", "die", "Schran\u00b7ken", "so", "eng", ",", "und", "die", "Welt", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADJD", "$,", "KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und so fl\u00fcchtig die Zeit!", "tokens": ["Und", "so", "fl\u00fcch\u00b7tig", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df ein Land, wo aus sonnigem Gr\u00fcn,", "tokens": ["Ich", "wei\u00df", "ein", "Land", ",", "wo", "aus", "son\u00b7ni\u00b7gem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um versunkene Tempel die Trauben gl\u00fchn,", "tokens": ["Um", "ver\u00b7sun\u00b7ke\u00b7ne", "Tem\u00b7pel", "die", "Trau\u00b7ben", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wo die purpurne Woge das Ufer besch\u00e4umt,", "tokens": ["Wo", "die", "pur\u00b7pur\u00b7ne", "Wo\u00b7ge", "das", "U\u00b7fer", "be\u00b7sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und von kommenden S\u00e4ngern der Lorbeer tr\u00e4umt.", "tokens": ["Und", "von", "kom\u00b7men\u00b7den", "S\u00e4n\u00b7gern", "der", "Lor\u00b7beer", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Fern lockt es und winkt dem verlangenden Sinn,", "tokens": ["Fern", "lockt", "es", "und", "winkt", "dem", "ver\u00b7lan\u00b7gen\u00b7den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und ich kann nicht hin!", "tokens": ["Und", "ich", "kann", "nicht", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "O h\u00e4tt' ich Fl\u00fcgel, durchs Blau der Luft", "tokens": ["O", "h\u00e4tt'", "ich", "Fl\u00fc\u00b7gel", ",", "durchs", "Blau", "der", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wie wollt' ich baden im Sonnenduft!", "tokens": ["Wie", "wollt'", "ich", "ba\u00b7den", "im", "Son\u00b7nen\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch umsonst! Und Stund' auf Stunde entflieht -", "tokens": ["Doch", "um\u00b7sonst", "!", "Und", "Stund'", "auf", "Stun\u00b7de", "ent\u00b7flieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "KON", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vertraure die Jugend, begrabe das Lied! -", "tokens": ["Ver\u00b7trau\u00b7re", "die", "Ju\u00b7gend", ",", "be\u00b7gra\u00b7be", "das", "Lied", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "O die Schranken so eng, und die Welt so weit,", "tokens": ["O", "die", "Schran\u00b7ken", "so", "eng", ",", "und", "die", "Welt", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADJD", "$,", "KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und so fl\u00fcchtig die Zeit!", "tokens": ["Und", "so", "fl\u00fcch\u00b7tig", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}