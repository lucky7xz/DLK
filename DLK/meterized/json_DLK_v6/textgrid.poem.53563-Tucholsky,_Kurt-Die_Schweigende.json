{"textgrid.poem.53563": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Schweigende", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst haben wir davon gesprochen", "tokens": ["Erst", "ha\u00b7ben", "wir", "da\u00b7von", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 du h\u00f6rtest freundlich zu \u2013,", "tokens": ["\u2013", "du", "h\u00f6r\u00b7test", "freund\u00b7lich", "zu", "\u2013", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "PTKZU", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ob unsre alten M\u00e4nnerknochen", "tokens": ["ob", "uns\u00b7re", "al\u00b7ten", "M\u00e4n\u00b7ner\u00b7kno\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sich niemals in den H\u00f6rselberg verkrochen . . .", "tokens": ["sich", "nie\u00b7mals", "in", "den", "H\u00f6r\u00b7sel\u00b7berg", "ver\u00b7kro\u00b7chen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und du?", "tokens": ["Und", "du", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Er sagte: \u00bbAch, ich bin ein b\u00f6ses Luder!", "tokens": ["Er", "sag\u00b7te", ":", "\u00bb", "Ach", ",", "ich", "bin", "ein", "b\u00f6\u00b7ses", "Lu\u00b7der", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ITJ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Frauen fehlen mir.", "tokens": ["Die", "Frau\u00b7en", "feh\u00b7len", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fresse jedes Jahr ein halbes Fuder,", "tokens": ["Ich", "fres\u00b7se", "je\u00b7des", "Jahr", "ein", "hal\u00b7bes", "Fu\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wild tobt mein Herz, st\u00e4ubt nur ihr wei\u00dfer Puder . . . \u00ab", "tokens": ["wild", "tobt", "mein", "Herz", ",", "st\u00e4ubt", "nur", "ihr", "wei\u00b7\u00dfer", "Pu\u00b7der", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was klopft denn dir?", "tokens": ["Was", "klopft", "denn", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Er sagte: \u00bbRausch! Nur Rausch vor allen Dingen!", "tokens": ["Er", "sag\u00b7te", ":", "\u00bb", "Rausch", "!", "Nur", "Rausch", "vor", "al\u00b7len", "Din\u00b7gen", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$.", "ADV", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor dem Verstand verblich", "tokens": ["Vor", "dem", "Ver\u00b7stand", "ver\u00b7blich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "schon manche G\u00f6ttin mit den Strahlenschwingen \u2013", "tokens": ["schon", "man\u00b7che", "G\u00f6t\u00b7tin", "mit", "den", "Strah\u00b7len\u00b7schwin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich packt es j\u00e4h, wenn meine Sinne singen . . . \u00ab", "tokens": ["Mich", "packt", "es", "j\u00e4h", ",", "wenn", "mei\u00b7ne", "Sin\u00b7ne", "sin\u00b7gen", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dich?", "tokens": ["Und", "dich", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Ich sagte: \u00bbRausch ist eine sch\u00f6ne Sache,", "tokens": ["Ich", "sag\u00b7te", ":", "\u00bb", "Rausch", "ist", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Sa\u00b7che", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "deckt er uns zu.", "tokens": ["deckt", "er", "uns", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch geben Sie mir auch die eine wache", "tokens": ["Doch", "ge\u00b7ben", "Sie", "mir", "auch", "die", "ei\u00b7ne", "wa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ART", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sekunde nur, in der ich rauschlos lache . . . \u00ab", "tokens": ["Se\u00b7kun\u00b7de", "nur", ",", "in", "der", "ich", "rau\u00b7schlos", "la\u00b7che", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und du?", "tokens": ["Und", "du", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Du sprichst kein Wort. Du siehst nur so auf jeden", "tokens": ["Du", "sprichst", "kein", "Wort", ".", "Du", "siehst", "nur", "so", "auf", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "PPER", "VVFIN", "ADV", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von uns \u2013 und w\u00e4hrend alles weit verklingt,", "tokens": ["von", "uns", "\u2013", "und", "w\u00e4h\u00b7rend", "al\u00b7les", "weit", "ver\u00b7klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "KON", "KOUS", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und w\u00e4hrend wir voll M\u00e4nnerweisheit reden:", "tokens": ["und", "w\u00e4h\u00b7rend", "wir", "voll", "M\u00e4n\u00b7ner\u00b7weis\u00b7heit", "re\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "blitzt auf in einem dunkeln Garten Eden", "tokens": ["blitzt", "auf", "in", "ei\u00b7nem", "dun\u00b7keln", "Gar\u00b7ten", "E\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "dein sieghafter Instinkt.", "tokens": ["dein", "sieg\u00b7haf\u00b7ter", "Ins\u00b7tinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}