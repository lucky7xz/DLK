{"textgrid.poem.33071": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Abend sinkt mit seinem Frieden", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abend sinkt mit seinem Frieden", "tokens": ["A\u00b7bend", "sinkt", "mit", "sei\u00b7nem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die Berge, in das Thal \u2013", "tokens": ["Auf", "die", "Ber\u00b7ge", ",", "in", "das", "Thal", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Holdes Bild! das mir beschieden", "tokens": ["Hol\u00b7des", "Bild", "!", "das", "mir", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PRELS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist, im letzten Tagesstrahl!", "tokens": ["Ist", ",", "im", "letz\u00b7ten", "Ta\u00b7ges\u00b7strahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An den blauen, duft'gen H\u00f6hen", "tokens": ["An", "den", "blau\u00b7en", ",", "duft'\u00b7gen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dehnt sich gl\u00fchend dort im West \u2013", "tokens": ["Dehnt", "sich", "gl\u00fc\u00b7hend", "dort", "im", "West", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines Gru\u00dfes letztes Wehen \u2013", "tokens": ["Ei\u00b7nes", "Gru\u00b7\u00dfes", "letz\u00b7tes", "We\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch des Abendgoldes Rest.", "tokens": ["Noch", "des", "A\u00b7bend\u00b7gol\u00b7des", "Rest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und des Stromes glatte Bahnen", "tokens": ["Und", "des", "Stro\u00b7mes", "glat\u00b7te", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum die Welle mehr bewegt,", "tokens": ["Kaum", "die", "Wel\u00b7le", "mehr", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft die ew'ge Macht nur ahnen,", "tokens": ["L\u00e4\u00dft", "die", "ew'\u00b7ge", "Macht", "nur", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihn rastlos weiter tr\u00e4gt!", "tokens": ["Die", "ihn", "rast\u00b7los", "wei\u00b7ter", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "An des Himmels fernsten R\u00e4umen", "tokens": ["An", "des", "Him\u00b7mels", "ferns\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst die Wolke z\u00f6gert still,", "tokens": ["Selbst", "die", "Wol\u00b7ke", "z\u00f6\u00b7gert", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Harrend, ob mit goldnen S\u00e4umen", "tokens": ["Har\u00b7rend", ",", "ob", "mit", "gold\u00b7nen", "S\u00e4u\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie das Mondlicht kr\u00e4nzen will.", "tokens": ["Sie", "das", "Mond\u00b7licht", "kr\u00e4n\u00b7zen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Friede! s\u00fc\u00dfes Gl\u00fcck von Oben!", "tokens": ["Frie\u00b7de", "!", "s\u00fc\u00b7\u00dfes", "Gl\u00fcck", "von", "O\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch' ein Zauber dich umflicht!", "tokens": ["Welch'", "ein", "Zau\u00b7ber", "dich", "um\u00b7flicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hemmst des Stromes wildes Toben,", "tokens": ["Hemmst", "des", "Stro\u00b7mes", "wil\u00b7des", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bannst in Ruhe Luft und Licht.", "tokens": ["Bannst", "in", "Ru\u00b7he", "Luft", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und mein Herz, es wird so milde,", "tokens": ["Und", "mein", "Herz", ",", "es", "wird", "so", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird so stille, wie noch nie,", "tokens": ["Wird", "so", "stil\u00b7le", ",", "wie", "noch", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "$,", "PWAV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlt sich mit dem sanften Bilde", "tokens": ["F\u00fchlt", "sich", "mit", "dem", "sanf\u00b7ten", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz in reinster Harmonie.", "tokens": ["Ganz", "in", "reins\u00b7ter", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn so jeder Wunsch kann schweigen,", "tokens": ["Wenn", "so", "je\u00b7der", "Wunsch", "kann", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Brust, die menschlich f\u00fchlt,", "tokens": ["In", "der", "Brust", ",", "die", "menschlich", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Jede Sehnsucht mu\u00df erbleichen", "tokens": ["Je\u00b7de", "Sehn\u00b7sucht", "mu\u00df", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Ruhe Gl\u00fcck umspielt.", "tokens": ["Von", "der", "Ru\u00b7he", "Gl\u00fcck", "um\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Darf nur einen Wunsch entbieten", "tokens": ["Darf", "nur", "ei\u00b7nen", "Wunsch", "ent\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch des reichen Herzens Schlag:", "tokens": ["Noch", "des", "rei\u00b7chen", "Her\u00b7zens", "Schlag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es stets in gleichem Frieden", "tokens": ["Da\u00df", "es", "stets", "in", "glei\u00b7chem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solch ein Bild genie\u00dfen mag!", "tokens": ["Solch", "ein", "Bild", "ge\u00b7nie\u00b7\u00dfen", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}