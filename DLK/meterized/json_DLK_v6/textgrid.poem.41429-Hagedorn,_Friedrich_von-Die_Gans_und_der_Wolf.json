{"textgrid.poem.41429": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Gans und der Wolf", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir G\u00e4nse retteten das Capitolium!", "tokens": ["Wir", "G\u00e4n\u00b7se", "ret\u00b7te\u00b7ten", "das", "Ca\u00b7pi\u00b7to\u00b7li\u00b7um", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach eine Gans, und schwimmt; blos dieses kann bezeugen,", "tokens": ["Sprach", "ei\u00b7ne", "Gans", ",", "und", "schwimmt", ";", "blos", "die\u00b7ses", "kann", "be\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVFIN", "$.", "ADV", "PDS", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Unerschrockenheit sei auch den G\u00e4nsen eigen.", "tokens": ["Die", "Un\u00b7er\u00b7schro\u00b7cken\u00b7heit", "sei", "auch", "den", "G\u00e4n\u00b7sen", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Am Ufer prahlt' ein Wolf: Den gro\u00dfen Romulum", "tokens": ["Am", "U\u00b7fer", "prahlt'", "ein", "Wolf", ":", "Den", "gro\u00b7\u00dfen", "Ro\u00b7mu\u00b7lum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NE", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "S\u00e4ugt' einer W\u00f6lfin Brust. Nichts gleicht, zu allen Zeiten,", "tokens": ["S\u00e4ugt'", "ei\u00b7ner", "W\u00f6l\u00b7fin", "Brust", ".", "Nichts", "gleicht", ",", "zu", "al\u00b7len", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$.", "PIS", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der guten W\u00f6lfe Z\u00e4rtlichkeiten.", "tokens": ["Der", "gu\u00b7ten", "W\u00f6l\u00b7fe", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja! schnattert jene drauf: wenn doch das Mannthier", "tokens": ["Ja", "!", "schnat\u00b7tert", "je\u00b7ne", "drauf", ":", "wenn", "doch", "das", "Mannt\u00b7hier"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVFIN", "PDS", "PTKVZ", "$.", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Einst unsre Tugenden erriethe!", "tokens": ["Einst", "uns\u00b7re", "Tu\u00b7gen\u00b7den", "er\u00b7rie\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ja! die beseelende Natur", "tokens": ["Ja", "!", "die", "be\u00b7see\u00b7len\u00b7de", "Na\u00b7tur"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gab G\u00e4nsen Muth und W\u00f6lfen G\u00fcte.", "tokens": ["Gab", "G\u00e4n\u00b7sen", "Muth", "und", "W\u00f6l\u00b7fen", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Habicht zeigt sich ihr, der Feind voll schneller List:", "tokens": ["Ein", "Ha\u00b7bicht", "zeigt", "sich", "ihr", ",", "der", "Feind", "voll", "schnel\u00b7ler", "List", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPER", "$,", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gleich schreit die T\u00e4ucherin, und Hals und Fu\u00df wird rege.", "tokens": ["Gleich", "schreit", "die", "T\u00e4u\u00b7che\u00b7rin", ",", "und", "Hals", "und", "Fu\u00df", "wird", "re\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "NN", "KON", "NN", "VAFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Wolf entdeckt ein armes Kind am Wege,", "tokens": ["Der", "Wolf", "ent\u00b7deckt", "ein", "ar\u00b7mes", "Kind", "am", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das er beschleicht, und ohn' Erbarmen fri\u00dft.", "tokens": ["Das", "er", "be\u00b7schleicht", ",", "und", "ohn'", "Er\u00b7bar\u00b7men", "fri\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "$,", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie viele r\u00fchmen sich der Tugenden und Gaben,", "tokens": ["Wie", "vie\u00b7le", "r\u00fch\u00b7men", "sich", "der", "Tu\u00b7gen\u00b7den", "und", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sie doch nicht erhalten haben!", "tokens": ["Die", "sie", "doch", "nicht", "er\u00b7hal\u00b7ten", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}