{"dta.poem.6962": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein schlechtes Instrument kan solchen Ton gew\u00e4hren,", "tokens": ["Kein", "schlech\u00b7tes", "Inst\u00b7ru\u00b7ment", "kan", "sol\u00b7chen", "Ton", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als man ihn sonsten w\u00fcrd\u2019 von guten Meister h\u00f6ren.", "tokens": ["Als", "man", "ihn", "sons\u00b7ten", "w\u00fcrd'", "von", "gu\u00b7ten", "Meis\u00b7ter", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "W\u00e4r selber in Arions Hand", "tokens": ["W\u00e4r", "sel\u00b7ber", "in", "A\u00b7rions", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NE", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Laut entzwey, die Saiten abgerissen;", "tokens": ["Die", "Laut", "ent\u00b7zwey", ",", "die", "Sai\u00b7ten", "ab\u00b7ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "W\u00fcrd\u2019 er darauf doch nicht zu spielen wissen,", "tokens": ["W\u00fcrd'", "er", "da\u00b7rauf", "doch", "nicht", "zu", "spie\u00b7len", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist ihm die Kunst gleich nach als vor bekannt,", "tokens": ["Ist", "ihm", "die", "Kunst", "gleich", "nach", "als", "vor", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "KOKOM", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die wir so offt bewundern m\u00fcssen.", "tokens": ["Die", "wir", "so", "offt", "be\u00b7wun\u00b7dern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}