{"textgrid.poem.37945": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kukuk auf dem Zaune sa\u00df,", "tokens": ["Der", "Ku\u00b7kuk", "auf", "dem", "Zau\u00b7ne", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es regnet sehr und er ward na\u00df,", "tokens": ["Es", "reg\u00b7net", "sehr", "und", "er", "ward", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam ein hoher Sonnenschein,", "tokens": ["Da", "kam", "ein", "ho\u00b7her", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kukuk, der ward h\u00fcbsch und fein,", "tokens": ["Der", "Ku\u00b7kuk", ",", "der", "ward", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann schwang er sein Gefieder", "tokens": ["Dann", "schwang", "er", "sein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl \u00fcbern See hin\u00fcber.", "tokens": ["Wohl", "\u00fc\u00b7bern", "See", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kukuk, Kukuk, Kukuk.", "tokens": ["Ku\u00b7kuk", ",", "Ku\u00b7kuk", ",", "Ku\u00b7kuk", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da wandte er sich schnelle her,", "tokens": ["Da", "wand\u00b7te", "er", "sich", "schnel\u00b7le", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sang so traurig, bange, schwer:", "tokens": ["Er", "sang", "so", "trau\u00b7rig", ",", "ban\u00b7ge", ",", "schwer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbvon rothem Gold ein Ringelein,", "tokens": ["\u00bb", "von", "ro\u00b7them", "Gold", "ein", "Rin\u00b7ge\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lie\u00df ich im Bett der Liebsten mein,", "tokens": ["Lie\u00df", "ich", "im", "Bett", "der", "Liebs\u00b7ten", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schwing nicht mein Gefieder,", "tokens": ["Ich", "schwing", "nicht", "mein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis mir das Ringlein wieder.", "tokens": ["Bis", "mir", "das", "Rin\u00b7glein", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kukuk, Kukuk, Kukuk.\u00ab", "tokens": ["Ku\u00b7kuk", ",", "Ku\u00b7kuk", ",", "Ku\u00b7kuk", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbach Goldschmidt, lieber Goldschmidt mein,", "tokens": ["\u00bb", "ach", "Gold\u00b7schmidt", ",", "lie\u00b7ber", "Gold\u00b7schmidt", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NE", "$,", "ADV", "NE", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schmied' mir von Gold ein Ringelein,", "tokens": ["Schmied'", "mir", "von", "Gold", "ein", "Rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schmied mir ihn an die rechte Hand,", "tokens": ["Schmied", "mir", "ihn", "an", "die", "rech\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich nehm ihn mit ins Vaterland,", "tokens": ["Ich", "nehm", "ihn", "mit", "ins", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann schwing ich mein Gefieder,", "tokens": ["Dann", "schwing", "ich", "mein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl \u00fcbern See hin\u00fcber.", "tokens": ["Wohl", "\u00fc\u00b7bern", "See", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kukuk, Kukuk, Kukuk.\u00ab", "tokens": ["Ku\u00b7kuk", ",", "Ku\u00b7kuk", ",", "Ku\u00b7kuk", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbach Kukuk, lieber Kukuk mein,", "tokens": ["\u00bb", "ach", "Ku\u00b7kuk", ",", "lie\u00b7ber", "Ku\u00b7kuk", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADV", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schmied ich dich an ein Ringelein,", "tokens": ["Schmied", "ich", "dich", "an", "ein", "Rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schmied ich dir an die rechte Hand,", "tokens": ["Schmied", "ich", "dir", "an", "die", "rech\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kannst nicht ziehn ins Vaterland,", "tokens": ["Du", "kannst", "nicht", "ziehn", "ins", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schwingst nimmer dein Gefieder,", "tokens": ["Schwingst", "nim\u00b7mer", "dein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da \u00fcbern See hin\u00fcber:", "tokens": ["Da", "\u00fc\u00b7bern", "See", "hin\u00b7\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kukuk, Kukuk, Kukuk.\u00ab", "tokens": ["Ku\u00b7kuk", ",", "Ku\u00b7kuk", ",", "Ku\u00b7kuk", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}