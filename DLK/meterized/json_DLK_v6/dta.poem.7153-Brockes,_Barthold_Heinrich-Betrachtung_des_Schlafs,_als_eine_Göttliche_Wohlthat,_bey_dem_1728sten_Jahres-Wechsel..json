{"dta.poem.7153": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie hast Du nicht, o GOTT! das abgewichne Jahr", "tokens": ["Wie", "hast", "Du", "nicht", ",", "o", "GoTT", "!", "das", "ab\u00b7ge\u00b7wich\u00b7ne", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "$,", "FM", "NE", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit unbeschreiblichem Heil, Ueberflu\u00df und Seegen,", "tokens": ["Mit", "un\u00b7be\u00b7schreib\u00b7li\u00b7chem", "Heil", ",", "Ue\u00b7berf\u00b7lu\u00df", "und", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So Stadt als Land gekr\u00f6nt! wie mancherley Gefahr,", "tokens": ["So", "Stadt", "als", "Land", "ge\u00b7kr\u00f6nt", "!", "wie", "man\u00b7cher\u00b7ley", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "VVPP", "$.", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Feuers-Brunst, von Wassers-Fluthen,", "tokens": ["Von", "Feu\u00b7er\u00b7sBrunst", ",", "von", "Was\u00b7ser\u00b7sFlu\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von st\u00fcrmerischer Lufft, die Deines Eifers Ruthen,", "tokens": ["Von", "st\u00fcr\u00b7me\u00b7ri\u00b7scher", "Lufft", ",", "die", "Dei\u00b7nes", "Ei\u00b7fers", "Ru\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sind, HERR, durch Deine Gnaden-Hand,", "tokens": ["Sind", ",", "HeRR", ",", "durch", "Dei\u00b7ne", "Gna\u00b7den\u00b7Hand", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So treulich von uns abgewandt.", "tokens": ["So", "treu\u00b7lich", "von", "uns", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du hast auf unsern Gassen,", "tokens": ["Du", "hast", "auf", "un\u00b7sern", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "In eines jeden Haus, o ew\u2019ger Seegens-Born!", "tokens": ["In", "ei\u00b7nes", "je\u00b7den", "Haus", ",", "o", "ew'\u00b7ger", "See\u00b7gens\u00b7Born", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dich, voller Gnad und Huld, nicht unbezeigt gelassen.", "tokens": ["Dich", ",", "vol\u00b7ler", "Gnad", "und", "Huld", ",", "nicht", "un\u00b7be\u00b7zeigt", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "KON", "NN", "$,", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}