{"textgrid.poem.52166": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ewig jung ist nur die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1883, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute fanden meine Schritte mein verge\u00dfnes Jugendtal,", "tokens": ["Heu\u00b7te", "fan\u00b7den", "mei\u00b7ne", "Schrit\u00b7te", "mein", "ver\u00b7ge\u00df\u00b7nes", "Ju\u00b7gend\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seine Sohle lag ver\u00f6det, seine Berge standen kahl.", "tokens": ["Sei\u00b7ne", "Soh\u00b7le", "lag", "ver\u00b7\u00f6\u00b7det", ",", "sei\u00b7ne", "Ber\u00b7ge", "stan\u00b7den", "kahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Meine B\u00e4ume, meine Tr\u00e4ume, meine buchendunkeln H\u00f6hn \u2013", "tokens": ["Mei\u00b7ne", "B\u00e4u\u00b7me", ",", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ",", "mei\u00b7ne", "bu\u00b7chen\u00b7dun\u00b7keln", "H\u00f6hn", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ewig jung ist nur die Sonne, sie allein ist ewig sch\u00f6n.", "tokens": ["E\u00b7wig", "jung", "ist", "nur", "die", "Son\u00b7ne", ",", "sie", "al\u00b7lein", "ist", "e\u00b7wig", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "PPER", "ADV", "VAFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Dr\u00fcben dort in schilf'gem Grunde, wo die m\u00fcde Lache liegt,", "tokens": ["Dr\u00fc\u00b7ben", "dort", "in", "schil\u00b7f'\u00b7gem", "Grun\u00b7de", ",", "wo", "die", "m\u00fc\u00b7de", "La\u00b7che", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Hat zu meiner Jugendstunde sich lebend'ge Flut gewiegt,", "tokens": ["Hat", "zu", "mei\u00b7ner", "Ju\u00b7gend\u00b7stun\u00b7de", "sich", "le\u00b7ben\u00b7d'\u00b7ge", "Flut", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "PRF", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Durch die Heiden, durch die Weiden ging ein wandernd Herdget\u00f6n \u2013", "tokens": ["Durch", "die", "Hei\u00b7den", ",", "durch", "die", "Wei\u00b7den", "ging", "ein", "wan\u00b7dernd", "Herd\u00b7ge\u00b7t\u00f6n", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ewig jung ist nur die Sonne, sie allein ist ewig sch\u00f6n.", "tokens": ["E\u00b7wig", "jung", "ist", "nur", "die", "Son\u00b7ne", ",", "sie", "al\u00b7lein", "ist", "e\u00b7wig", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "PPER", "ADV", "VAFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}