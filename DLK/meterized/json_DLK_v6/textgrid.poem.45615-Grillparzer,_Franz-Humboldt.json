{"textgrid.poem.45615": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Humboldt", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df er die Welt zum Begriff gebracht,", "tokens": ["Da\u00df", "er", "die", "Welt", "zum", "Be\u00b7griff", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist mir ein leeres Gemunkel,", "tokens": ["Ist", "mir", "ein", "lee\u00b7res", "Ge\u00b7mun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Es hat sie schon Hegel durchsichtig gemacht,", "tokens": ["Es", "hat", "sie", "schon", "He\u00b7gel", "durch\u00b7sich\u00b7tig", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und gleich drauf war sie wieder dunkel.", "tokens": ["Und", "gleich", "drauf", "war", "sie", "wie\u00b7der", "dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}