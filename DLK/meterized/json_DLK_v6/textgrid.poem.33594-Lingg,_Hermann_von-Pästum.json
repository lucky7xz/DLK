{"textgrid.poem.33594": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "P\u00e4stum", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fctend liegt der Mittag \u00fcber", "tokens": ["Br\u00fc\u00b7tend", "liegt", "der", "Mit\u00b7tag", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "P\u00e4stum's \u00f6der Fiebergegend,", "tokens": ["P\u00e4\u00b7stum's", "\u00f6\u00b7der", "Fie\u00b7ber\u00b7ge\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schw\u00fcle Nebel niederlegend,", "tokens": ["Schw\u00fc\u00b7le", "Ne\u00b7bel", "nie\u00b7der\u00b7le\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst die Sonne schimmert tr\u00fcber,", "tokens": ["Selbst", "die", "Son\u00b7ne", "schim\u00b7mert", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die alte Stadt Poseidon's,", "tokens": ["Und", "die", "al\u00b7te", "Stadt", "Po\u00b7sei\u00b7don's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Stumm und einsam liegt sie da,", "tokens": ["Stumm", "und", "ein\u00b7sam", "liegt", "sie", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein zerst\u00f6rtes Sodoma.", "tokens": ["Ein", "zer\u00b7st\u00f6r\u00b7tes", "So\u00b7do\u00b7ma", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf zerbrochnen Steinkolossen", "tokens": ["Auf", "zer\u00b7broch\u00b7nen", "Stein\u00b7ko\u00b7los\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Umgest\u00fcrzter Architrave", "tokens": ["Um\u00b7ge\u00b7st\u00fcrz\u00b7ter", "Ar\u00b7chit\u00b7ra\u00b7ve"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Bl\u00fchen Kaktus und Agave,", "tokens": ["Bl\u00fc\u00b7hen", "Kak\u00b7tus", "und", "A\u00b7ga\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Um die alten Mauern sprossen", "tokens": ["Um", "die", "al\u00b7ten", "Mau\u00b7ern", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rote Blumen und Akanthus;", "tokens": ["Ro\u00b7te", "Blu\u00b7men", "und", "A\u00b7kan\u00b7thus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Duftig wuchern dr\u00fcberhin", "tokens": ["Duf\u00b7tig", "wu\u00b7chern", "dr\u00fc\u00b7berh\u00b7in"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Thymian und Rosmarin.", "tokens": ["Thy\u00b7mi\u00b7an", "und", "Ros\u00b7ma\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur ein gelber Tempelriese", "tokens": ["Nur", "ein", "gel\u00b7ber", "Tem\u00b7pel\u00b7rie\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tr\u00e4gt noch seine Quaderbalken;", "tokens": ["Tr\u00e4gt", "noch", "sei\u00b7ne", "Qua\u00b7der\u00b7bal\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um den Giebel fliegen Falken,", "tokens": ["Um", "den", "Gie\u00b7bel", "flie\u00b7gen", "Fal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Efeu rankt sich um die Friese,", "tokens": ["E\u00b7feu", "rankt", "sich", "um", "die", "Frie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und die Natter und die Eidechs", "tokens": ["Und", "die", "Nat\u00b7ter", "und", "die", "Ei\u00b7dechs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonnt sich an der Tempelwand,", "tokens": ["Sonnt", "sich", "an", "der", "Tem\u00b7pel\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo geflammt der Opferbrand.", "tokens": ["Wo", "ge\u00b7flammt", "der", "Op\u00b7fer\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ungebrochen stehn die schlanken", "tokens": ["Un\u00b7ge\u00b7bro\u00b7chen", "stehn", "die", "schlan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dorers\u00e4ulen; ein Jahrtausend", "tokens": ["Do\u00b7rer\u00b7s\u00e4u\u00b7len", ";", "ein", "Jahr\u00b7tau\u00b7send"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sahen sie vor\u00fcberbrausend;", "tokens": ["Sa\u00b7hen", "sie", "vor\u00b7\u00fc\u00b7berb\u00b7rau\u00b7send", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Throne st\u00fcrzten, V\u00f6lker sanken;", "tokens": ["Thro\u00b7ne", "st\u00fcrz\u00b7ten", ",", "V\u00f6l\u00b7ker", "san\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber ihre Marmorh\u00e4upter", "tokens": ["\u00dc\u00b7ber", "ih\u00b7re", "Mar\u00b7mor\u00b7h\u00e4up\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie durchs Meer, dem sie geweiht,", "tokens": ["Wie", "durchs", "Meer", ",", "dem", "sie", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weht ein Hauch der Ewigkeit.", "tokens": ["Weht", "ein", "Hauch", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}