{"textgrid.poem.25124": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An die Franzosen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr z\u00fcrnt, da\u00df wir, mit Raubkrieg \u00fcberzogen,", "tokens": ["Ihr", "z\u00fcrnt", ",", "da\u00df", "wir", ",", "mit", "Raub\u00b7krieg", "\u00fc\u00b7berz\u00b7o\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch blutend wieder heimgesandt,", "tokens": ["Euch", "blu\u00b7tend", "wie\u00b7der", "heim\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deutsche Gaun, um die ihr uns betrogen,", "tokens": ["Und", "deut\u00b7sche", "Gaun", ",", "um", "die", "ihr", "uns", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entrissen eurer Frevelhand?", "tokens": ["Ent\u00b7ris\u00b7sen", "eu\u00b7rer", "Fre\u00b7vel\u00b7hand", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fcr viele Missethat war das die S\u00fchne,", "tokens": ["F\u00fcr", "vie\u00b7le", "Mis\u00b7se\u00b7that", "war", "das", "die", "S\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PDS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit mehr Recht habt ihr geb\u00fc\u00dft,", "tokens": ["Und", "mit", "mehr", "Recht", "habt", "ihr", "ge\u00b7b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als jener F\u00fcrst, den auf der Henkerb\u00fchne", "tokens": ["Als", "je\u00b7ner", "F\u00fcrst", ",", "den", "auf", "der", "Hen\u00b7ker\u00b7b\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr fremde Schuld ihr sterben lie\u00dft.", "tokens": ["F\u00fcr", "frem\u00b7de", "Schuld", "ihr", "ster\u00b7ben", "lie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht an die Str\u00f6me Bluts, aus deutschen Adern", "tokens": ["Nicht", "an", "die", "Str\u00f6\u00b7me", "Bluts", ",", "aus", "deut\u00b7schen", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschlagen vom Franzosenschwert,", "tokens": ["Ge\u00b7schla\u00b7gen", "vom", "Fran\u00b7zo\u00b7sen\u00b7schwert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr d\u00e4chten wir fortan, noch altes Hadern,", "tokens": ["Mehr", "d\u00e4ch\u00b7ten", "wir", "for\u00b7tan", ",", "noch", "al\u00b7tes", "Ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ihr nicht die Verstockten w\u00e4r't.", "tokens": ["Wenn", "ihr", "nicht", "die", "Ver\u00b7stock\u00b7ten", "w\u00e4r'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vereint nun sollten wir den Feind bekriegen", "tokens": ["Ver\u00b7eint", "nun", "soll\u00b7ten", "wir", "den", "Feind", "be\u00b7krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den argen Sohn der Finsternis,", "tokens": ["Den", "ar\u00b7gen", "Sohn", "der", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem eurer Besten einer von den Z\u00fcgen", "tokens": ["Dem", "eu\u00b7rer", "Bes\u00b7ten", "ei\u00b7ner", "von", "den", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die L\u00fcgenmaske lachend ri\u00df.", "tokens": ["Die", "L\u00fc\u00b7gen\u00b7mas\u00b7ke", "la\u00b7chend", "ri\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ihr, berauscht vom Trank des Taumelweines,", "tokens": ["Doch", "ihr", ",", "be\u00b7rauscht", "vom", "Trank", "des", "Tau\u00b7mel\u00b7wei\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der euch so oft den Sinn beth\u00f6rt,", "tokens": ["Der", "euch", "so", "oft", "den", "Sinn", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schreit Rache, weil wir euch beim Raub des Rheines,", "tokens": ["Schreit", "Ra\u00b7che", ",", "weil", "wir", "euch", "beim", "Raub", "des", "Rhei\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem langgebr\u00fcteten, gest\u00f6rt.", "tokens": ["Dem", "lang\u00b7ge\u00b7br\u00fc\u00b7te\u00b7ten", ",", "ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl, w\u00e4hlt, verb\u00fcndet mit dem Vatikane,", "tokens": ["Wohl", ",", "w\u00e4hlt", ",", "ver\u00b7b\u00fcn\u00b7det", "mit", "dem", "Va\u00b7ti\u00b7ka\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Menschheit tausendj\u00e4hr'gem Fluch,", "tokens": ["Der", "Menschheit", "tau\u00b7send\u00b7j\u00e4hr'\u00b7gem", "Fluch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Fledermaus zum Sinnbild eurer Fahne", "tokens": ["Die", "Fle\u00b7der\u00b7maus", "zum", "Sinn\u00b7bild", "eu\u00b7rer", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Anstatt des Adlers, den sie trug!", "tokens": ["An\u00b7statt", "des", "Ad\u00b7lers", ",", "den", "sie", "trug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00dft Priester sie mit Segensspr\u00fcchen weihen,", "tokens": ["La\u00dft", "Pries\u00b7ter", "sie", "mit", "Se\u00b7gens\u00b7spr\u00fc\u00b7chen", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u2013 edles B\u00fcndnis! \u2013 Afrika", "tokens": ["Und", "\u2013", "ed\u00b7les", "B\u00fcnd\u00b7nis", "!", "\u2013", "Af\u00b7ri\u00b7ka"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["KON", "$(", "ADJA", "NN", "$.", "$(", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tiger seiner W\u00fcsten nach uns speien \u2013", "tokens": ["Die", "Ti\u00b7ger", "sei\u00b7ner", "W\u00fcs\u00b7ten", "nach", "uns", "spei\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir stehen kampfger\u00fcstet da;", "tokens": ["Wir", "ste\u00b7hen", "kampf\u00b7ge\u00b7r\u00fcs\u00b7tet", "da", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und durch das Rasseln der Kanonenr\u00e4der", "tokens": ["Und", "durch", "das", "Ras\u00b7seln", "der", "Ka\u00b7no\u00b7nen\u00b7r\u00e4\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch rufen wir ins taube Ohr:", "tokens": ["Euch", "ru\u00b7fen", "wir", "ins", "tau\u00b7be", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gez\u00e4hlt die Teuren hat der Unsern jeder,", "tokens": ["Ge\u00b7z\u00e4hlt", "die", "Teu\u00b7ren", "hat", "der", "Un\u00b7sern", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "VAFIN", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die durch eu'r Mordschwert er verlor.", "tokens": ["Die", "durch", "eu'r", "Mord\u00b7schwert", "er", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Pal\u00e4ste sind in Deutschand nicht noch H\u00fctten,", "tokens": ["Pa\u00b7l\u00e4s\u00b7te", "sind", "in", "Deutsc\u00b7hand", "nicht", "noch", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NE", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo nicht die Trauer hauptverh\u00fcllt", "tokens": ["Wo", "nicht", "die", "Trau\u00b7er", "haupt\u00b7ver\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst nach einer Stimme lauscht, nach Tritten,", "tokens": ["Um\u00b7sonst", "nach", "ei\u00b7ner", "Stim\u00b7me", "lauscht", ",", "nach", "Trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sonst sie frohen Klangs erf\u00fcllt.", "tokens": ["Die", "sonst", "sie", "fro\u00b7hen", "Klangs", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und, soll nochmals des Krieges Flamme lodern,", "tokens": ["Und", ",", "soll", "noch\u00b7mals", "des", "Krie\u00b7ges", "Flam\u00b7me", "lo\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein furchtbar W\u00fcrgen wird es sein;", "tokens": ["Ein", "furcht\u00b7bar", "W\u00fcr\u00b7gen", "wird", "es", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "All die Erschlagnen, die in Frankreich modern,", "tokens": ["All", "die", "Er\u00b7schlag\u00b7nen", ",", "die", "in", "Fran\u00b7kreich", "mo\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie k\u00e4mpfen mit in unsern Reihn!", "tokens": ["Sie", "k\u00e4mp\u00b7fen", "mit", "in", "un\u00b7sern", "Reihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ja, einen Toten hat ein jeder drunten,", "tokens": ["Ja", ",", "ei\u00b7nen", "To\u00b7ten", "hat", "ein", "je\u00b7der", "drun\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Geist zum Rachewerk ihn st\u00e4hlt", "tokens": ["Des", "Geist", "zum", "Ra\u00b7che\u00b7werk", "ihn", "st\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hell das Feuer spr\u00fchn l\u00e4\u00dft von den Lunten", "tokens": ["Und", "hell", "das", "Feu\u00b7er", "spr\u00fchn", "l\u00e4\u00dft", "von", "den", "Lun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sorgt, da\u00df nicht die Kugel fehlt.", "tokens": ["Und", "sorgt", ",", "da\u00df", "nicht", "die", "Ku\u00b7gel", "fehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "So wi\u00dft, eh ihr beginnt das Unerh\u00f6rte", "tokens": ["So", "wi\u00dft", ",", "eh", "ihr", "be\u00b7ginnt", "das", "Un\u00b7er\u00b7h\u00f6r\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und neu mit Blut die Erde netzt:", "tokens": ["Und", "neu", "mit", "Blut", "die", "Er\u00b7de", "netzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist der eigne Untergang, Beth\u00f6rte,", "tokens": ["Es", "ist", "der", "eig\u00b7ne", "Un\u00b7ter\u00b7gang", ",", "Be\u00b7th\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den auf den ", "tokens": ["Den", "auf", "den"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.13": {"line.1": {"text": "Denn enden wird der Kampf erst, ob Millionen", "tokens": ["Denn", "en\u00b7den", "wird", "der", "Kampf", "erst", ",", "ob", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "ADV", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Leben auch das Schlachtschwert fri\u00dft,", "tokens": ["Von", "Le\u00b7ben", "auch", "das", "Schlacht\u00b7schwert", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "VV