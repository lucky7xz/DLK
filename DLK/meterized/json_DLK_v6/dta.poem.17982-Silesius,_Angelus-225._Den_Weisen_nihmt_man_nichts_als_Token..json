{"dta.poem.17982": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "225.  Den Weisen nihmt man nichts als  \n Token.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weise lacht darzu wenn man ihn alls genom\u0304en.", "tokens": ["Der", "Wei\u00b7se", "lacht", "dar\u00b7zu", "wenn", "man", "ihn", "alls", "ge\u00b7nom\u0304en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "KOUS", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Warumb? er ist umb nichts als nur umb Token kom\u0304en.", "tokens": ["Wa\u00b7rumb", "?", "er", "ist", "umb", "nichts", "als", "nur", "umb", "To\u00b7ken", "kom\u0304en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "APPR", "PIS", "KOKOM", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}