{"textgrid.poem.39479": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf seinem Pegasus, dem magern Rappen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf seinem Pegasus, dem magern Rappen", "tokens": ["Auf", "sei\u00b7nem", "Pe\u00b7ga\u00b7sus", ",", "dem", "ma\u00b7gern", "Rap\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Reit't in die Ritterpoesie Quixote,", "tokens": ["Reit't", "in", "die", "Rit\u00b7ter\u00b7po\u00b7e\u00b7sie", "Qui\u00b7xo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NE", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Und h\u00e4lt anmuthiglich, in Gl\u00fcck und Nothe,", "tokens": ["Und", "h\u00e4lt", "an\u00b7mut\u00b7hig\u00b7lich", ",", "in", "Gl\u00fcck", "und", "No\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gespr\u00e4che mit der Prosa seines Knappen.", "tokens": ["Ge\u00b7spr\u00e4\u00b7che", "mit", "der", "Pro\u00b7sa", "sei\u00b7nes", "Knap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erst, wie sie blind nach Abenteuern tappen,", "tokens": ["Erst", ",", "wie", "sie", "blind", "nach", "A\u00b7bent\u00b7eu\u00b7ern", "tap\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trifft sie der Weltlauf mit gar harter Pfote;", "tokens": ["Trifft", "sie", "der", "Welt\u00b7lauf", "mit", "gar", "har\u00b7ter", "Pfo\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann kommt der Scherz als huldigender Bote,", "tokens": ["Dann", "kommt", "der", "Scherz", "als", "hul\u00b7di\u00b7gen\u00b7der", "Bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sch\u00fcttelt schelmisch ihre Schellenkappen.", "tokens": ["Und", "sch\u00fct\u00b7telt", "schel\u00b7misch", "ih\u00b7re", "Schel\u00b7len\u00b7kap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Liebe webt drein r\u00fchrende Geschichten;", "tokens": ["Und", "Lie\u00b7be", "webt", "drein", "r\u00fch\u00b7ren\u00b7de", "Ge\u00b7schich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Verstand der Menschen Sitten, Tracht, Geberden;", "tokens": ["Ver\u00b7stand", "der", "Men\u00b7schen", "Sit\u00b7ten", ",", "Tracht", ",", "Ge\u00b7ber\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gaukelt Phantasie in farb'ger Glorie.", "tokens": ["Es", "gau\u00b7kelt", "Phan\u00b7ta\u00b7sie", "in", "fa\u00b7rb'\u00b7ger", "Glo\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich schw\u00f6r' es, und Urgande selbst soll richten:", "tokens": ["Ich", "schw\u00f6r'", "es", ",", "und", "Ur\u00b7gan\u00b7de", "selbst", "soll", "rich\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Was auch hinf\u00fcro mag ersonnen werden,", "tokens": ["Was", "auch", "hin\u00b7f\u00fc\u00b7ro", "mag", "er\u00b7son\u00b7nen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VMFIN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die\u00df bleibt die unvergleichlichste Historie!", "tokens": ["Die\u00df", "bleibt", "die", "un\u00b7ver\u00b7gleich\u00b7lichs\u00b7te", "His\u00b7to\u00b7rie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}