{"textgrid.poem.57907": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Erkennungszeichen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zehn Jahre ist er im Ausland gewesen,", "tokens": ["Zehn", "Jah\u00b7re", "ist", "er", "im", "Aus\u00b7land", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat nicht gesehen den Leinestrand,", "tokens": ["Hat", "nicht", "ge\u00b7se\u00b7hen", "den", "Lei\u00b7nes\u00b7trand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und als er zur\u00fcckkam nach Hannover,", "tokens": ["Und", "als", "er", "zu\u00b7r\u00fcck\u00b7kam", "nach", "Han\u00b7no\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da hat er es fast nicht wiedererkannt.", "tokens": ["Da", "hat", "er", "es", "fast", "nicht", "wie\u00b7der\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Durchbruchsviertel, die Asphaltstra\u00dfen,", "tokens": ["Das", "Durch\u00b7bruchs\u00b7vier\u00b7tel", ",", "die", "As\u00b7phalt\u00b7stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die elektrische Bahn, ihm war alles neu,", "tokens": ["Die", "e\u00b7lekt\u00b7ri\u00b7sche", "Bahn", ",", "ihm", "war", "al\u00b7les", "neu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und der vordere Teil der Eilenriede,", "tokens": ["Und", "der", "vor\u00b7de\u00b7re", "Teil", "der", "Ei\u00b7len\u00b7rie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wo war die alte W\u00fcstenei?", "tokens": ["Wo", "war", "die", "al\u00b7te", "W\u00fcs\u00b7te\u00b7nei", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihm wurde ganz wild und fremd zumute,", "tokens": ["Ihm", "wur\u00b7de", "ganz", "wild", "und", "fremd", "zu\u00b7mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als er das sah, und er dachte dabei,", "tokens": ["Als", "er", "das", "sah", ",", "und", "er", "dach\u00b7te", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es w\u00e4re ihm wirklich, als ob das gar nicht", "tokens": ["Es", "w\u00e4\u00b7re", "ihm", "wirk\u00b7lich", ",", "als", "ob", "das", "gar", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KOKOM", "KOUS", "PDS", "ADV", "PTKNEG"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das alte gute Hannover noch sei.", "tokens": ["Das", "al\u00b7te", "gu\u00b7te", "Han\u00b7no\u00b7ver", "noch", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da sah er die aufgerissenen Stra\u00dfen,", "tokens": ["Da", "sah", "er", "die", "auf\u00b7ge\u00b7ris\u00b7se\u00b7nen", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Grand und die Steine und Loch an Loch,", "tokens": ["Den", "Grand", "und", "die", "Stei\u00b7ne", "und", "Loch", "an", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und sprach: \u00bbEs ist doch mein altes Hannover,", "tokens": ["Und", "sprach", ":", "\u00bb", "Es", "ist", "doch", "mein", "al\u00b7tes", "Han\u00b7no\u00b7ver", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich kenne es wieder: sie buddeln noch!\u00ab", "tokens": ["Ich", "ken\u00b7ne", "es", "wie\u00b7der", ":", "sie", "bud\u00b7deln", "noch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}