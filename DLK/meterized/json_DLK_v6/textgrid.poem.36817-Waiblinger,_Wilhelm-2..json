{"textgrid.poem.36817": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Schlachten will ich preisen, noch K\u00f6nige", "tokens": ["Nicht", "Schlach\u00b7ten", "will", "ich", "prei\u00b7sen", ",", "noch", "K\u00f6\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "NN", "VMFIN", "PPER", "VVFIN", "$,", "ADV", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch forschen, wer Rom's w\u00fcrd'ger, ob's C\u00e4sar ist,", "tokens": ["Noch", "for\u00b7schen", ",", "wer", "Rom's", "w\u00fcrd'\u00b7ger", ",", "ob's", "C\u00e4\u00b7sar", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "NE", "ADJD", "$,", "KOUS", "NE", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ob Brutus, Namen der Geschichte,", "tokens": ["Ob", "Bru\u00b7tus", ",", "Na\u00b7men", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Gl\u00e4nzende nicht und ger\u00fchmte Schatten.", "tokens": ["Gl\u00e4n\u00b7zen\u00b7de", "nicht", "und", "ge\u00b7r\u00fchm\u00b7te", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich singe meinen Freund, und auf stolzeren,", "tokens": ["Ich", "sin\u00b7ge", "mei\u00b7nen", "Freund", ",", "und", "auf", "stol\u00b7ze\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "APPR", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf tiefern Wogen k\u00fchnen Gesangs sei mir", "tokens": ["Auf", "tie\u00b7fern", "Wo\u00b7gen", "k\u00fch\u00b7nen", "Ge\u00b7sangs", "sei", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verg\u00f6nnt, mit Stromsgewalt und Kraft ihn", "tokens": ["Ver\u00b7g\u00f6nnt", ",", "mit", "Stroms\u00b7ge\u00b7walt", "und", "Kraft", "ihn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "NN", "KON", "NN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jauchzend zu tragen zum Oceane,", "tokens": ["Jauch\u00b7zend", "zu", "tra\u00b7gen", "zum", "O\u00b7cea\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Da sich die Zukunft eint mit Vergangenheit,", "tokens": ["Da", "sich", "die", "Zu\u00b7kunft", "eint", "mit", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beid' aber unverg\u00e4ngliche Gegenwart;", "tokens": ["Beid'", "a\u00b7ber", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7che", "Ge\u00b7gen\u00b7wart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ohn' Anfang beid' und ohne Ende,", "tokens": ["Ohn'", "An\u00b7fang", "beid'", "und", "oh\u00b7ne", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beide die g\u00f6ttliche Ewigkeit sind.", "tokens": ["Bei\u00b7de", "die", "g\u00f6tt\u00b7li\u00b7che", "E\u00b7wig\u00b7keit", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Dich kenn' ich, seit ich kenne, was sch\u00f6n ist, Freund,", "tokens": ["Dich", "kenn'", "ich", ",", "seit", "ich", "ken\u00b7ne", ",", "was", "sch\u00f6n", "ist", ",", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dich lieb' ich, seit ich liebe, was gut ist, Freund!", "tokens": ["Dich", "lieb'", "ich", ",", "seit", "ich", "lie\u00b7be", ",", "was", "gut", "ist", ",", "Freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In meinem Herzen lebst du einzig,", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "lebst", "du", "ein\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit es der delphische Gott bewohnet.", "tokens": ["Seit", "es", "der", "del\u00b7phi\u00b7sche", "Gott", "be\u00b7woh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Dein Lob, es d\u00fcnkte schon mir Unsterblichkeit,", "tokens": ["Dein", "Lob", ",", "es", "d\u00fcnk\u00b7te", "schon", "mir", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "PPER", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Erweckte Bl\u00fcth' und Fr\u00fchling, wie Sonnenschein,", "tokens": ["Er\u00b7weck\u00b7te", "Bl\u00fcth'", "und", "Fr\u00fch\u00b7ling", ",", "wie", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein Tadel reinigte, gleich Wettern,", "tokens": ["Dein", "Ta\u00b7del", "rei\u00b7nig\u00b7te", ",", "gleich", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "D\u00fcnste der Erde, die mich umfiengen.", "tokens": ["D\u00fcns\u00b7te", "der", "Er\u00b7de", ",", "die", "mich", "um\u00b7fi\u00b7en\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Entrissen sind wir uns, und im kalten Hauch", "tokens": ["Ent\u00b7ris\u00b7sen", "sind", "wir", "uns", ",", "und", "im", "kal\u00b7ten", "Hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Nordens athmest Seufzer der Sehnsucht du", "tokens": ["Des", "Nor\u00b7dens", "ath\u00b7mest", "Seuf\u00b7zer", "der", "Sehn\u00b7sucht", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nach meinem S\u00fcden, wo einst Menschen", "tokens": ["Nach", "mei\u00b7nem", "S\u00fc\u00b7den", ",", "wo", "einst", "Men\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wandelten besserer Art, dir \u00e4hnlich.", "tokens": ["Wan\u00b7del\u00b7ten", "bes\u00b7se\u00b7rer", "Art", ",", "dir", "\u00e4hn\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PPER", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Dir hat, uralter r\u00f6m'scher Tage werth,", "tokens": ["Dir", "hat", ",", "ur\u00b7al\u00b7ter", "r\u00f6m'\u00b7scher", "Ta\u00b7ge", "werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kraftvollen Geists und hohen Gem\u00fcths ein Weib", "tokens": ["Kraft\u00b7vol\u00b7len", "Geists", "und", "ho\u00b7hen", "Ge\u00b7m\u00fcths", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Leben sch\u00f6n begr\u00e4nzt und ewig", "tokens": ["Das", "Le\u00b7ben", "sch\u00f6n", "be\u00b7gr\u00e4nzt", "und", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt in erm\u00fcdender Wirksamkeit es", "tokens": ["H\u00e4lt", "in", "er\u00b7m\u00fc\u00b7den\u00b7der", "Wirk\u00b7sam\u00b7keit", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Lebendig dir der Grazie sch\u00f6nern Dienst:", "tokens": ["Le\u00b7ben\u00b7dig", "dir", "der", "Gra\u00b7zie", "sch\u00f6\u00b7nern", "Dienst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mir nimmt aufopfernd keines des Herzens Gram", "tokens": ["Mir", "nimmt", "auf\u00b7op\u00b7fernd", "kei\u00b7nes", "des", "Her\u00b7zens", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "PIS", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Sorg' ab, kein verj\u00fcngtes Abbild", "tokens": ["Und", "Sor\u00b7g'", "ab", ",", "kein", "ver\u00b7j\u00fcng\u00b7tes", "Ab\u00b7bild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "PTKVZ", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "L\u00e4chelt mir z\u00e4rtlich mein Selbst entgegen.", "tokens": ["L\u00e4\u00b7chelt", "mir", "z\u00e4rt\u00b7lich", "mein", "Selbst", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Die Gr\u00e4ber Roms sind meine Vertrauten nur;", "tokens": ["Die", "Gr\u00e4\u00b7ber", "Roms", "sind", "mei\u00b7ne", "Ver\u00b7trau\u00b7ten", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oftmals jedoch am Fu\u00dfe des aschigen", "tokens": ["Oft\u00b7mals", "je\u00b7doch", "am", "Fu\u00b7\u00dfe", "des", "asc\u00b7hi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vulkans, am blauen Meer, im Glanze", "tokens": ["Vul\u00b7kans", ",", "am", "blau\u00b7en", "Meer", ",", "im", "Glan\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Parthenopeischer L\u00fcfte f\u00fchl' ich", "tokens": ["Par\u00b7the\u00b7no\u00b7pei\u00b7scher", "L\u00fcf\u00b7te", "f\u00fchl'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Die Seel' aus jener Gr\u00e4ber Melancholei", "tokens": ["Die", "Seel'", "aus", "je\u00b7ner", "Gr\u00e4\u00b7ber", "Me\u00b7lan\u00b7cho\u00b7lei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erstehn, mit Psyches seliger Lust am Strand", "tokens": ["Er\u00b7stehn", ",", "mit", "Psy\u00b7ches", "se\u00b7li\u00b7ger", "Lust", "am", "Strand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Lethe schw\u00e4rmen, und in D\u00fcften", "tokens": ["Des", "Le\u00b7the", "schw\u00e4r\u00b7men", ",", "und", "in", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwelgen der purpurnen Hesperiden.", "tokens": ["Schwel\u00b7gen", "der", "pur\u00b7pur\u00b7nen", "Hes\u00b7pe\u00b7ri\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Wenn dann in Baj\u00e4's tr\u00fcmmerumgeb'nem Golf,", "tokens": ["Wenn", "dann", "in", "Ba\u00b7j\u00e4's", "tr\u00fcm\u00b7mer\u00b7um\u00b7ge\u00b7b'\u00b7nem", "Golf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo gern im Kahn ich \u00fcber die Spiegelfluth", "tokens": ["Wo", "gern", "im", "Kahn", "ich", "\u00fc\u00b7ber", "die", "Spie\u00b7gel\u00b7fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hingankle zu Misenums Felsen,", "tokens": ["Hin\u00b7gank\u00b7le", "zu", "Mi\u00b7se\u00b7nums", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Oder zum Tempelgew\u00f6lb' der Venus,", "tokens": ["O\u00b7der", "zum", "Tem\u00b7pel\u00b7ge\u00b7w\u00f6lb'", "der", "Ve\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.12": {"line.1": {"text": "Mir wohl erhab'ne Namen der Vorwelt sich", "tokens": ["Mir", "wohl", "er\u00b7hab'\u00b7ne", "Na\u00b7men", "der", "Vor\u00b7welt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN", "ART", "NN", "PRF"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gebietrisch zeigen, bringst dem gepeinigten", "tokens": ["Ge\u00b7bie\u00b7trisch", "zei\u00b7gen", ",", "bringst", "dem", "ge\u00b7pei\u00b7nig\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Orest doch du des weisern Freundes", "tokens": ["O\u00b7rest", "doch", "du", "des", "wei\u00b7sern", "Freun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Theuerstes, heiligstes Bild zur\u00fccke.", "tokens": ["Theu\u00b7ers\u00b7tes", ",", "hei\u00b7ligs\u00b7tes", "Bild", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}