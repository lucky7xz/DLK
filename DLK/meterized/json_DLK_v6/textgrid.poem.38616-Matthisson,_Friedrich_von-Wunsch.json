{"textgrid.poem.38616": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einmal m\u00f6cht' ich, eh' in die Schattenwelt", "tokens": ["Noch", "ein\u00b7mal", "m\u00f6cht'", "ich", ",", "eh'", "in", "die", "Schat\u00b7ten\u00b7welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Elysiums mein seliger Geist sich senkt,", "tokens": ["E\u00b7ly\u00b7si\u00b7ums", "mein", "se\u00b7li\u00b7ger", "Geist", "sich", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Flur begr\u00fcssen, wo der Kindheit", "tokens": ["Die", "Flur", "be\u00b7gr\u00fcs\u00b7sen", ",", "wo", "der", "Kind\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Himmlische Tr\u00e4ume mein Haupt umschwebten.", "tokens": ["Himm\u00b7li\u00b7sche", "Tr\u00e4u\u00b7me", "mein", "Haupt", "um\u00b7schweb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Der Strauch der Heimath, welcher des H\u00e4nflings Nest", "tokens": ["Der", "Strauch", "der", "Hei\u00b7math", ",", "wel\u00b7cher", "des", "H\u00e4nf\u00b7lings", "Nest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit K\u00fchlung deckte, s\u00e4uselt doch lieblicher,", "tokens": ["Mit", "K\u00fch\u00b7lung", "deck\u00b7te", ",", "s\u00e4u\u00b7selt", "doch", "lieb\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O Freund, als alle Lorbeerw\u00e4lder", "tokens": ["O", "Freund", ",", "als", "al\u00b7le", "Lor\u00b7beer\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber der Asche der Weltbezwinger.", "tokens": ["Ue\u00b7ber", "der", "A\u00b7sche", "der", "Welt\u00b7be\u00b7zwin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Der Bach der Blumenwiese, wo ich als Kind", "tokens": ["Der", "Bach", "der", "Blu\u00b7men\u00b7wie\u00b7se", ",", "wo", "ich", "als", "Kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "KOUS", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Violen pfl\u00fcckte, murmelt melodischer,", "tokens": ["Vi\u00b7o\u00b7len", "pfl\u00fcck\u00b7te", ",", "mur\u00b7melt", "me\u00b7lo\u00b7di\u00b7scher", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Erlen die mein Vater pflanzte,", "tokens": ["Durch", "Er\u00b7len", "die", "mein", "Va\u00b7ter", "pflanz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als die blandusische Silberquelle.", "tokens": ["Als", "die", "blan\u00b7du\u00b7si\u00b7sche", "Sil\u00b7ber\u00b7quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Der H\u00fcgel, wo der jauchzende Knabenreihn", "tokens": ["Der", "H\u00fc\u00b7gel", ",", "wo", "der", "jauch\u00b7zen\u00b7de", "Kna\u00b7ben\u00b7reihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich um den Stamm der bl\u00fchenden Linde schwang,", "tokens": ["Sich", "um", "den", "Stamm", "der", "bl\u00fc\u00b7hen\u00b7den", "Lin\u00b7de", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entz\u00fcckt mich h\u00f6her als der Alpen", "tokens": ["Ent\u00b7z\u00fcckt", "mich", "h\u00f6\u00b7her", "als", "der", "Al\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Blendende Gipfel im Rosenschimmer.", "tokens": ["Blen\u00b7den\u00b7de", "Gip\u00b7fel", "im", "Ro\u00b7sen\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Drum m\u00f6cht' ich einmal, eh' in die Schattenwelt", "tokens": ["Drum", "m\u00f6cht'", "ich", "ein\u00b7mal", ",", "eh'", "in", "die", "Schat\u00b7ten\u00b7welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Elysiums mein seliger Geist sich senkt,", "tokens": ["E\u00b7ly\u00b7si\u00b7ums", "mein", "se\u00b7li\u00b7ger", "Geist", "sich", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Flur noch segnen, wo der Kindheit", "tokens": ["Die", "Flur", "noch", "seg\u00b7nen", ",", "wo", "der", "Kind\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Himmlische Tr\u00e4ume mein Haupt umschwebten.", "tokens": ["Himm\u00b7li\u00b7sche", "Tr\u00e4u\u00b7me", "mein", "Haupt", "um\u00b7schweb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Dann mag des Todes l\u00e4chelnder Genius", "tokens": ["Dann", "mag", "des", "To\u00b7des", "l\u00e4\u00b7cheln\u00b7der", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Fackel pl\u00f6zlich l\u00f6schen; ich eile froh", "tokens": ["Die", "Fa\u00b7ckel", "pl\u00f6z\u00b7lich", "l\u00f6\u00b7schen", ";", "ich", "ei\u00b7le", "froh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVINF", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu Xenophons und Platons Weisheit", "tokens": ["Zu", "Xe\u00b7no\u00b7phons", "und", "Pla\u00b7tons", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zu Anakreons Mirthenlaube.", "tokens": ["Und", "zu", "A\u00b7nak\u00b7re\u00b7ons", "Mir\u00b7then\u00b7lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}