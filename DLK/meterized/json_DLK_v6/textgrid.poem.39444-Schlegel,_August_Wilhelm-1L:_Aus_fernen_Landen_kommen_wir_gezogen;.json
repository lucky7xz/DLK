{"textgrid.poem.39444": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus fernen Landen kommen wir gezogen;", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus fernen Landen kommen wir gezogen;", "tokens": ["Aus", "fer\u00b7nen", "Lan\u00b7den", "kom\u00b7men", "wir", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Weisheit strebten wir seit langen Jahren,", "tokens": ["Nach", "Weis\u00b7heit", "streb\u00b7ten", "wir", "seit", "lan\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wandern wir in unsern Silberhaaren.", "tokens": ["Doch", "wan\u00b7dern", "wir", "in", "un\u00b7sern", "Sil\u00b7ber\u00b7haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein sch\u00f6ner Stern ist vor uns hergeflogen.", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Stern", "ist", "vor", "uns", "her\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun steht er winkend still am Himmelsbogen:", "tokens": ["Nun", "steht", "er", "win\u00b7kend", "still", "am", "Him\u00b7mels\u00b7bo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den F\u00fcrsten Juda's mu\u00df die\u00df Haus bewahren.", "tokens": ["Den", "F\u00fcrs\u00b7ten", "Ju\u00b7da's", "mu\u00df", "die\u00df", "Haus", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was hast du, kleines Bethlehem, erfahren?", "tokens": ["Was", "hast", "du", ",", "klei\u00b7nes", "Beth\u00b7le\u00b7hem", ",", "er\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir ist der Herr vor allen hochgewogen.", "tokens": ["Dir", "ist", "der", "Herr", "vor", "al\u00b7len", "hoch\u00b7ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Holdselig Kind, la\u00df auf den Knie'n dich gr\u00fc\u00dfen!", "tokens": ["Hold\u00b7se\u00b7lig", "Kind", ",", "la\u00df", "auf", "den", "Knie'n", "dich", "gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVIMP", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Womit die Sonne unsre Heimat segnet,", "tokens": ["Wo\u00b7mit", "die", "Son\u00b7ne", "uns\u00b7re", "Hei\u00b7mat", "seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das bringen wir, obschon geringe Gaben.", "tokens": ["Das", "brin\u00b7gen", "wir", ",", "ob\u00b7schon", "ge\u00b7rin\u00b7ge", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gold, Weihrauch, Myrrhen, liegen dir zu F\u00fc\u00dfen;", "tokens": ["Gold", ",", "Weih\u00b7rauch", ",", "Myr\u00b7rhen", ",", "lie\u00b7gen", "dir", "zu", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Weisheit ist uns sichtbarlich begegnet,", "tokens": ["Die", "Weis\u00b7heit", "ist", "uns", "sicht\u00b7bar\u00b7lich", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Willst du uns nur mit Einem Blicke laben.", "tokens": ["Willst", "du", "uns", "nur", "mit", "Ei\u00b7nem", "Bli\u00b7cke", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}