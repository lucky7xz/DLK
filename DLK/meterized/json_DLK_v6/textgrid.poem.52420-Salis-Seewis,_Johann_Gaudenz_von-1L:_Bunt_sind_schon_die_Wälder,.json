{"textgrid.poem.52420": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bunt sind schon die W\u00e4lder,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bunt sind schon die W\u00e4lder,", "tokens": ["Bunt", "sind", "schon", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gelb die Stoppelfelder,", "tokens": ["Gelb", "die", "Stop\u00b7pel\u00b7fel\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Herbst beginnt.", "tokens": ["Und", "der", "Herbst", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "Rote Bl\u00e4tter fallen,", "tokens": ["Ro\u00b7te", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Graue Nebel wallen,", "tokens": ["Grau\u00b7e", "Ne\u00b7bel", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "K\u00fchler weht der Wind.", "tokens": ["K\u00fch\u00b7ler", "weht", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie die volle Traube,", "tokens": ["Wie", "die", "vol\u00b7le", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aus dem Rebenlaube,", "tokens": ["Aus", "dem", "Re\u00b7ben\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Purpurfarbig strahlt!", "tokens": ["Pur\u00b7purf\u00b7ar\u00b7big", "strahlt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Am Gel\u00e4nder reifen", "tokens": ["Am", "Ge\u00b7l\u00e4n\u00b7der", "rei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Pfirsiche mit Streifen", "tokens": ["Pfir\u00b7si\u00b7che", "mit", "Strei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Rot und wei\u00df bemalt.", "tokens": ["Rot", "und", "wei\u00df", "be\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sieh! Wie hier die Dirne", "tokens": ["Sieh", "!", "Wie", "hier", "die", "Dir\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Emsig Pflaum' und Birne", "tokens": ["Em\u00b7sig", "Pflaum'", "und", "Bir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In ihr K\u00f6rbchen legt!", "tokens": ["In", "ihr", "K\u00f6rb\u00b7chen", "legt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dort, mit leichten Schritten,", "tokens": ["Dort", ",", "mit", "leich\u00b7ten", "Schrit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jene goldne Quitten", "tokens": ["Je\u00b7ne", "gold\u00b7ne", "Quit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "In den Landhof tr\u00e4gt!", "tokens": ["In", "den", "Land\u00b7hof", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Flinke Tr\u00e4ger springen,", "tokens": ["Flin\u00b7ke", "Tr\u00e4\u00b7ger", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und die M\u00e4dchen singen,", "tokens": ["Und", "die", "M\u00e4d\u00b7chen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alles jubelt froh!", "tokens": ["Al\u00b7les", "ju\u00b7belt", "froh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bunte B\u00e4nder schweben,", "tokens": ["Bun\u00b7te", "B\u00e4n\u00b7der", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Zwischen hohen Reben,", "tokens": ["Zwi\u00b7schen", "ho\u00b7hen", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Auf dem Hut von Stroh!", "tokens": ["Auf", "dem", "Hut", "von", "Stroh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Geige t\u00f6nt und Fl\u00f6te", "tokens": ["Gei\u00b7ge", "t\u00f6nt", "und", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bei der Abendr\u00f6te", "tokens": ["Bei", "der", "A\u00b7bend\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und im Mondenglanz;", "tokens": ["Und", "im", "Mon\u00b7den\u00b7glanz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Junge Winzerinnen", "tokens": ["Jun\u00b7ge", "Win\u00b7ze\u00b7rin\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Winken und beginnen", "tokens": ["Win\u00b7ken", "und", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Deutschen Ringeltanz.", "tokens": ["Deut\u00b7schen", "Rin\u00b7gel\u00b7tanz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}