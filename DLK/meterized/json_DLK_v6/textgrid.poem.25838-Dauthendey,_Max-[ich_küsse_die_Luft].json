{"textgrid.poem.25838": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[ich k\u00fcsse die Luft]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich k\u00fcsse die Luft,", "tokens": ["Ich", "k\u00fcs\u00b7se", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich umarme die W\u00e4rme der N\u00e4chte.", "tokens": ["Ich", "um\u00b7ar\u00b7me", "die", "W\u00e4r\u00b7me", "der", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Mir ist, es m\u00fcsse von meinem Harme, meinem Sehnen", "tokens": ["Mir", "ist", ",", "es", "m\u00fcs\u00b7se", "von", "mei\u00b7nem", "Har\u00b7me", ",", "mei\u00b7nem", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aus der Leere dein Auge aufsprie\u00dfen,", "tokens": ["Aus", "der", "Lee\u00b7re", "dein", "Au\u00b7ge", "auf\u00b7sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Zu mir flie\u00dfen dein blauender Blick.", "tokens": ["Zu", "mir", "flie\u00b7\u00dfen", "dein", "blau\u00b7en\u00b7der", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Sonne br\u00fctet,", "tokens": ["Son\u00b7ne", "br\u00fc\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sommergras gl\u00fcht,", "tokens": ["Som\u00b7mer\u00b7gras", "gl\u00fcht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Vom roten Mohn spr\u00fcht br\u00fcnstiger Schein.", "tokens": ["Vom", "ro\u00b7ten", "Mohn", "spr\u00fcht", "br\u00fcns\u00b7ti\u00b7ger", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Ich strecke die Arme,", "tokens": ["Ich", "stre\u00b7cke", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Erbarme dich, Licht,", "tokens": ["Er\u00b7bar\u00b7me", "dich", ",", "Licht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Mich k\u00fcssen hungrige N\u00e4chte.", "tokens": ["Mich", "k\u00fcs\u00b7sen", "hung\u00b7ri\u00b7ge", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}