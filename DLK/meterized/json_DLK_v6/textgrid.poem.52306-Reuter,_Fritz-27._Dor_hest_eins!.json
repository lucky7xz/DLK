{"textgrid.poem.52306": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "27. Dor hest eins!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Herr von Buck, en finen Eddelmann,", "tokens": ["De", "Herr", "von", "Buck", ",", "en", "fi\u00b7nen", "Ed\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$,", "FM", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "De ok Regierungsrat ded sin,", "tokens": ["De", "ok", "Re\u00b7gie\u00b7rungs\u00b7rat", "ded", "sin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "De kamm einmal des Abends tau Malchin", "tokens": ["De", "kamm", "ein\u00b7mal", "des", "A\u00b7bends", "tau", "Mal\u00b7chin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "CARD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In st\u00e4dtische Gesch\u00e4ften an.", "tokens": ["In", "st\u00e4d\u00b7ti\u00b7sche", "Ge\u00b7sch\u00e4f\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hei s\u00f6cht en Gasthof sick, en rechten stillen \u2013", "tokens": ["Hei", "s\u00f6cht", "en", "Gast\u00b7hof", "sick", ",", "en", "rech\u00b7ten", "stil\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$,", "FM", "ADJA", "VVINF", "$("], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Wenn ick nich irr', was dat bi B\u00fcllen \u2013,", "tokens": ["Wenn", "ick", "nich", "irr'", ",", "was", "dat", "bi", "B\u00fcl\u00b7len", "\u2013", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PRELS", "ART", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Un as hei dor wat eten hett,", "tokens": ["Un", "as", "hei", "dor", "wat", "e\u00b7ten", "hett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Dunn ward hei m\u00e4ud un geiht tau Bedd.", "tokens": ["Dunn", "ward", "hei", "m\u00e4ud", "un", "geiht", "tau", "Bedd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Na, as hei nu des Morgens fr\u00fch", "tokens": ["Na", ",", "as", "hei", "nu", "des", "Mor\u00b7gens", "fr\u00fch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "FM", "FM", "ADV", "ART", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ganz m\u00e4ud noch in de Feddern liggt,", "tokens": ["Ganz", "m\u00e4ud", "noch", "in", "de", "Fed\u00b7dern", "liggt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dunn tut't de Kauhird sine Melodi,", "tokens": ["Dunn", "tut't", "de", "Kau\u00b7hird", "si\u00b7ne", "Me\u00b7lo\u00b7di", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Un wohr is't, tuten ded hei slicht,", "tokens": ["Un", "wohr", "is't", ",", "tu\u00b7ten", "ded", "hei", "slicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Un unser Herr von Buck, de Herr Regierungsrat,", "tokens": ["Un", "un\u00b7ser", "Herr", "von", "Buck", ",", "de", "Herr", "Re\u00b7gie\u00b7rungs\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "NN", "$,", "NE", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "De w\u00fcrd dor\u00e4wer ganz kasprat.", "tokens": ["De", "w\u00fcrd", "do\u00b7r\u00e4\u00b7wer", "ganz", "ka\u00b7sprat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Kum sl\u00f6ppt hei wedder nu en Happen,", "tokens": ["Kum", "sl\u00f6ppt", "hei", "wed\u00b7der", "nu", "en", "Hap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Dunn f\u00e4ngt de Swinhird an tau klappen", "tokens": ["Dunn", "f\u00e4ngt", "de", "Swin\u00b7hird", "an", "tau", "klap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "NE", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Un weckt em ut den Drom, worin", "tokens": ["Un", "weckt", "em", "ut", "den", "Drom", ",", "wo\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Hei grad v\u00f6r den Gro\u00dfherzog st\u00fcnn", "tokens": ["Hei", "grad", "v\u00f6r", "den", "Gro\u00df\u00b7her\u00b7zog", "st\u00fcnn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Un de Gro\u00dfherzog tau em s\u00e4d", "tokens": ["Un", "de", "Gro\u00df\u00b7her\u00b7zog", "tau", "em", "s\u00e4d"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Un em de Hand up sine Schuller l\u00e4d:", "tokens": ["Un", "em", "de", "Hand", "up", "si\u00b7ne", "Schul\u00b7ler", "l\u00e4d", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.21": {"text": "\u00bbmin leiwe Herr von Buck, ick w\u00fcnschte, ick", "tokens": ["\u00bb", "min", "lei\u00b7we", "Herr", "von", "Buck", ",", "ick", "w\u00fcnschte", ",", "ick"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,", "PPER", "VVFIN", "$,", "PPER"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.22": {"text": "Hadd mihr von so'ne B\u00fcck in minen Land,", "tokens": ["Hadd", "mihr", "von", "so'\u00b7ne", "B\u00fcck", "in", "mi\u00b7nen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Denn wir de ganze Sak bewandt;", "tokens": ["Denn", "wir", "de", "gan\u00b7ze", "Sak", "be\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Tau G\u00e4rtners makt ick all de B\u00fcck,", "tokens": ["Tau", "G\u00e4rt\u00b7ners", "makt", "ick", "all", "de", "B\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "V\u00f6r allen Sei un Ehr Geswister;", "tokens": ["V\u00f6r", "al\u00b7len", "Sei", "un", "Ehr", "Ges\u00b7wis\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "FM", "FM", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ick heww nu einmal so'ne Grappen.", "tokens": ["Ick", "heww", "nu", "ein\u00b7mal", "so'\u00b7ne", "Grap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Sei s\u00fcnd von jetzt an \u00f6bberster Minister.\u00ab", "tokens": ["Sei", "s\u00fcnd", "von", "jetzt", "an", "\u00f6b\u00b7bers\u00b7ter", "Mi\u00b7nis\u00b7ter", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Dunn fung de Swinhird an tau klappen. \u2013", "tokens": ["Dunn", "fung", "de", "Swin\u00b7hird", "an", "tau", "klap\u00b7pen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "NE", "NE", "APPR", "NE", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Na, dat em dit woll argern m\u00fc\u00dft,", "tokens": ["Na", ",", "dat", "em", "dit", "woll", "ar\u00b7gern", "m\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM.nl", "FM.nl", "FM.nl", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Dat oll Geklapp, dat kann'ck mi denken;", "tokens": ["Dat", "oll", "Ge\u00b7klapp", ",", "dat", "kann'ck", "mi", "den\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Villicht hadd de Gro\u00dfherzog em noch k\u00fc\u00dft,", "tokens": ["Vil\u00b7licht", "hadd", "de", "Gro\u00df\u00b7her\u00b7zog", "em", "noch", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "W\u00fcrd em villicht en Gaud noch schenken,", "tokens": ["W\u00fcrd", "em", "vil\u00b7licht", "en", "Gaud", "noch", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Un nu kreg hei ok nich en Happen,", "tokens": ["Un", "nu", "kreg", "hei", "ok", "nich", "en", "Hap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Un dat kamm ganz allein von't olle Klappen.", "tokens": ["Un", "dat", "kamm", "ganz", "al\u00b7lein", "von't", "ol\u00b7le", "Klap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Na, hei sprung rute ut de Posen", "tokens": ["Na", ",", "hei", "sprung", "ru\u00b7te", "ut", "de", "Po\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "So arg, as einer warden kann,", "tokens": ["So", "arg", ",", "as", "ei\u00b7ner", "war\u00b7den", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Un fohrt herinner in de Hosen", "tokens": ["Un", "fohrt", "he\u00b7rin\u00b7ner", "in", "de", "Ho\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NE", "NE"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.38": {"text": "Un treckt sick de Kledaschen an.", "tokens": ["Un", "treckt", "sick", "de", "Kle\u00b7da\u00b7schen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "NE", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.39": {"text": "\u00bbut so'n Drom so rut tau kamen!", "tokens": ["\u00bb", "ut", "so'n", "Drom", "so", "rut", "tau", "ka\u00b7men", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Ick gl\u00f6w, all't Veih up dese Ir", "tokens": ["Ick", "gl\u00f6w", ",", "all't", "Veih", "up", "de\u00b7se", "Ir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.41": {"text": "K\u00fcmmt hir in dit oll Lock tausamen.", "tokens": ["K\u00fcmmt", "hir", "in", "dit", "oll", "Lock", "tau\u00b7sa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Wenn nich dit d\u00e4mlich Klappen wir", "tokens": ["Wenn", "nich", "dit", "d\u00e4m\u00b7lich", "Klap\u00b7pen", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ADJD", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Un wenn'ck nich dorvon wir upwakt,", "tokens": ["Un", "wenn'ck", "nich", "dor\u00b7von", "wir", "up\u00b7wakt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "PAV", "PPER", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.44": {"text": "Hei hadd mi w\u00fcrklich taum Minister makt,", "tokens": ["Hei", "hadd", "mi", "w\u00fcrk\u00b7lich", "taum", "Mi\u00b7nis\u00b7ter", "makt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Un dat, dat s\u00fcll mi nich verdreiten?\u00ab", "tokens": ["Un", 