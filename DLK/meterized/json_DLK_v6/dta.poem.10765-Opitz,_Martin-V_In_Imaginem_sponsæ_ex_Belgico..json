{"dta.poem.10765": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "V  \n In Imaginem spons\u00e6 ex Belgico.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Vermessen Mahlerhand/ wie hastu d\u00f6rffen mahlen", "tokens": ["Ver\u00b7mes\u00b7sen", "Mah\u00b7ler\u00b7hand", "/", "wie", "has\u00b7tu", "d\u00f6rf\u00b7fen", "mah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$(", "PWAV", "ADJD", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Fewer das mich brent/ die wundersch\u00f6ne stralen", "tokens": ["Das", "Fe\u00b7wer", "das", "mich", "brent", "/", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00df G\u00f6ttlichen Gesichts/ die G\u00f6tter wollen nicht/", "tokens": ["De\u00df", "G\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sichts", "/", "die", "G\u00f6t\u00b7ter", "wol\u00b7len", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VMFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df jemands sie nach macht/ vnd jhrer sch\u00f6nheit licht.", "tokens": ["Da\u00df", "je\u00b7mands", "sie", "nach", "macht", "/", "vnd", "jhrer", "sch\u00f6n\u00b7heit", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "VVFIN", "$(", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wo ist die lieblich art/ die mir mein Hertz kan brechen/", "tokens": ["Wo", "ist", "die", "lieb\u00b7lich", "art", "/", "die", "mir", "mein", "Hertz", "kan", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJD", "NN", "$(", "PRELS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd mich selbst stelen mir? wo ist das s\u00fcsse sprechen?", "tokens": ["Vnd", "mich", "selbst", "ste\u00b7len", "mir", "?", "wo", "ist", "das", "s\u00fcs\u00b7se", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "$.", "PWAV", "VAFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo ist die sch\u00f6ne lach? wo ist der trotze gang?", "tokens": ["Wo", "ist", "die", "sch\u00f6\u00b7ne", "lach", "?", "wo", "ist", "der", "trot\u00b7ze", "gang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "VVFIN", "$.", "PWAV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "De\u00df Geistes lustigkeit? das Spielen? der Gesang?", "tokens": ["De\u00df", "Geis\u00b7tes", "lus\u00b7tig\u00b7keit", "?", "das", "Spie\u00b7len", "?", "der", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die frewden allzumahl/ die mir mein Hertz au\u00dfsaugen?", "tokens": ["Die", "frew\u00b7den", "all\u00b7zu\u00b7mahl", "/", "die", "mir", "mein", "Hertz", "au\u00df\u00b7sau\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$(", "PRELS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das wincken mit der Hand? das reden mit den Augen?", "tokens": ["Das", "win\u00b7cken", "mit", "der", "Hand", "?", "das", "re\u00b7den", "mit", "den", "Au\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$.", "PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das meiste seh ich hier/ vnd suche noch das meist/", "tokens": ["Das", "meis\u00b7te", "seh", "ich", "hier", "/", "vnd", "su\u00b7che", "noch", "das", "meist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADV", "$(", "KON", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00df stehn den Leih/ wo nicht/ so mahle auch den Geist.", "tokens": ["La\u00df", "stehn", "den", "Leih", "/", "wo", "nicht", "/", "so", "mah\u00b7le", "auch", "den", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$(", "PWAV", "PTKNEG", "$(", "ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}