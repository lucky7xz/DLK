{"textgrid.poem.47852": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Geh nicht hinein", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fl\u00fcgel oben hinterm Korridor,", "tokens": ["Im", "Fl\u00fc\u00b7gel", "o\u00b7ben", "hin\u00b7term", "Kor\u00b7ri\u00b7dor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo es so j\u00e4hlings einsam worden ist", "tokens": ["Wo", "es", "so", "j\u00e4h\u00b7lings", "ein\u00b7sam", "wor\u00b7den", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "VAPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 Nicht in dem ersten Zimmer, wo man sonst", "tokens": ["\u2013", "Nicht", "in", "dem", "ers\u00b7ten", "Zim\u00b7mer", ",", "wo", "man", "sonst"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihn finden mochte, in die blasse Hand", "tokens": ["Ihn", "fin\u00b7den", "moch\u00b7te", ",", "in", "die", "blas\u00b7se", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das junge Haupt gest\u00fctzt, die Augen tr\u00e4umend", "tokens": ["Das", "jun\u00b7ge", "Haupt", "ge\u00b7st\u00fctzt", ",", "die", "Au\u00b7gen", "tr\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entlang den W\u00e4nden streifend, wo im Laub", "tokens": ["Ent\u00b7lang", "den", "W\u00e4n\u00b7den", "strei\u00b7fend", ",", "wo", "im", "Laub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVPP", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von Tropenpflanzen ausgeb\u00e4lgt Getier", "tokens": ["Von", "Tro\u00b7pen\u00b7pflan\u00b7zen", "aus\u00b7ge\u00b7b\u00e4lgt", "Ge\u00b7tier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Fl\u00fcgel spreizte und die Tatzen reckte,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "spreiz\u00b7te", "und", "die", "Tat\u00b7zen", "reck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Halb Wunder noch, halb Wissensr\u00e4tsel ihm", "tokens": ["Halb", "Wun\u00b7der", "noch", ",", "halb", "Wis\u00b7sens\u00b7r\u00e4t\u00b7sel", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "ADV", "$,", "ADJD", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u2013 Nicht dort; der Stuhl ist leer, die Pflanzen lassen", "tokens": ["\u2013", "Nicht", "dort", ";", "der", "Stuhl", "ist", "leer", ",", "die", "Pflan\u00b7zen", "las\u00b7sen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKNEG", "ADV", "$.", "ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Verd\u00fcrstend ihre sch\u00f6nen Bl\u00e4tter h\u00e4ngen;", "tokens": ["Ver\u00b7d\u00fcrs\u00b7tend", "ih\u00b7re", "sch\u00f6\u00b7nen", "Bl\u00e4t\u00b7ter", "h\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Staub sinkt herab; \u2013 nein, nebenan die T\u00fcr,", "tokens": ["Staub", "sinkt", "her\u00b7ab", ";", "\u2013", "nein", ",", "ne\u00b7be\u00b7nan", "die", "T\u00fcr", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$.", "$(", "PTKANT", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.13": {"text": "In jenem hohen d\u00e4mmrigen Gemach", "tokens": ["In", "je\u00b7nem", "ho\u00b7hen", "d\u00e4mm\u00b7ri\u00b7gen", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u2013 Beklommne Schw\u00fcle ist drin eingeschlossen \u2013,", "tokens": ["\u2013", "Be\u00b7klomm\u00b7ne", "Schw\u00fc\u00b7le", "ist", "drin", "ein\u00b7ge\u00b7schlos\u00b7sen", "\u2013", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Dort hinterm Wandschirm auf dem Bette liegt", "tokens": ["Dort", "hin\u00b7term", "Wand\u00b7schirm", "auf", "dem", "Bet\u00b7te", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Etwas \u2013 geh nicht hinein! Es schaut dich fremd", "tokens": ["Et\u00b7was", "\u2013", "geh", "nicht", "hin\u00b7ein", "!", "Es", "schaut", "dich", "fremd"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "VVFIN", "PTKNEG", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und furchtbar an.", "tokens": ["Und", "furcht\u00b7bar", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Vor wenig Stunden noch", "tokens": ["Vor", "we\u00b7nig", "Stun\u00b7den", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "Auf jenen Kissen lag sein blondes Haupt;", "tokens": ["Auf", "je\u00b7nen", "Kis\u00b7sen", "lag", "sein", "blon\u00b7des", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zwar bleich von Qualen, denn des Lebens F\u00e4den", "tokens": ["Zwar", "bleich", "von", "Qua\u00b7len", ",", "denn", "des", "Le\u00b7bens", "F\u00e4\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Zerrissen j\u00e4h; doch seine Augen sprachen", "tokens": ["Zer\u00b7ris\u00b7sen", "j\u00e4h", ";", "doch", "sei\u00b7ne", "Au\u00b7gen", "spra\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$.", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Noch z\u00e4rtlich, und mitunter l\u00e4chelt' er,", "tokens": ["Noch", "z\u00e4rt\u00b7lich", ",", "und", "mi\u00b7tun\u00b7ter", "l\u00e4\u00b7chelt'", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.23": {"text": "Als s\u00e4h er noch in goldne Erdenferne.", "tokens": ["Als", "s\u00e4h", "er", "noch", "in", "gold\u00b7ne", "Er\u00b7den\u00b7fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Da pl\u00f6tzlich losch es aus; er wu\u00dft es pl\u00f6tzlich", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "losch", "es", "aus", ";", "er", "wu\u00dft", "es", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "\u2013 Und ein Entsetzen schrie aus seiner Brust,", "tokens": ["\u2013", "Und", "ein", "Ent\u00b7set\u00b7zen", "schrie", "aus", "sei\u00b7ner", "Brust", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df ratlos Mitleid, die am Lager sa\u00dfen,", "tokens": ["Da\u00df", "rat\u00b7los", "Mit\u00b7leid", ",", "die", "am", "La\u00b7ger", "sa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "In Stein verwandelte \u2013, er lag am Abgrund;", "tokens": ["In", "Stein", "ver\u00b7wan\u00b7del\u00b7te", "\u2013", ",", "er", "lag", "am", "Ab\u00b7grund", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$(", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Bodenlos, ganz ohne Boden. \u2013 \u00bbHilf!", "tokens": ["Bo\u00b7den\u00b7los", ",", "ganz", "oh\u00b7ne", "Bo\u00b7den", ".", "\u2013", "\u00bb", "Hilf", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "NN", "$.", "$(", "$(", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Ach Vater, lieber Vater!\u00ab Taumelnd schlug", "tokens": ["Ach", "Va\u00b7ter", ",", "lie\u00b7ber", "Va\u00b7ter", "!", "\u00ab", "Tau\u00b7melnd", "schlug"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["ITJ", "NN", "$,", "ADV", "NN", "$.", "$(", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Er um sich mit den Armen; ziellos griffen", "tokens": ["Er", "um", "sich", "mit", "den", "Ar\u00b7men", ";", "ziel\u00b7los", "grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "PRF", "APPR", "ART", "NN", "$.", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "In leere Luft die H\u00e4nde; noch ein Schrei \u2013", "tokens": ["In", "lee\u00b7re", "Luft", "die", "H\u00e4n\u00b7de", ";", "noch", "ein", "Schrei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$.", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und dann verschwand er.", "tokens": ["Und", "dann", "ver\u00b7schwand", "er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "Dort, wo er gelegen,", "tokens": ["Dort", ",", "wo", "er", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Dort hinterm Wandschirm, stumm und einsam liegt", "tokens": ["Dort", "hin\u00b7term", "Wand\u00b7schirm", ",", "stumm", "und", "ein\u00b7sam", "liegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Jetzt etwas; \u2013 bleib, geh nicht hinein! Es schaut", "tokens": ["Jetzt", "et\u00b7was", ";", "\u2013", "bleib", ",", "geh", "nicht", "hin\u00b7ein", "!", "Es", "schaut"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIS", "$.", "$(", "VVFIN", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Dich fremd und furchtbar an; f\u00fcr viele Tage", "tokens": ["Dich", "fremd", "und", "furcht\u00b7bar", "an", ";", "f\u00fcr", "vie\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Kannst du nicht leben, wenn du es erblickt.", "tokens": ["Kannst", "du", "nicht", "le\u00b7ben", ",", "wenn", "du", "es", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "\u00bbund weiter \u2013 du, der du ihn liebtest \u2013, hast", "tokens": ["\u00bb", "und", "wei\u00b7ter", "\u2013", "du", ",", "der", "du", "ihn", "lieb\u00b7test", "\u2013", ",", "hast"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "KON", "ADV", "$(", "PPER", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$(", "$,", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Nichts weiter du zu sagen?\u00ab", "tokens": ["Nichts", "wei\u00b7ter", "du", "zu", "sa\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "ADV", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Weiter nichts.", "tokens": ["Wei\u00b7ter", "nichts", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PIS", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}