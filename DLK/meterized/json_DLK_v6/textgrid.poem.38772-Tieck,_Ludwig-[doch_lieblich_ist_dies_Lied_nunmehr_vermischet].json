{"textgrid.poem.38772": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[doch lieblich ist dies Lied nunmehr vermischet]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch lieblich ist dies Lied nunmehr vermischet,", "tokens": ["Doch", "lieb\u00b7lich", "ist", "dies", "Lied", "nun\u00b7mehr", "ver\u00b7mi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PDS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein fremd Wort liegt dem deinigen am Herzen,", "tokens": ["Ein", "fremd", "Wort", "liegt", "dem", "dei\u00b7ni\u00b7gen", "am", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "PPOSS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir ist als s\u00e4h ich spielend Engel scherzen,", "tokens": ["Mir", "ist", "als", "s\u00e4h", "ich", "spie\u00b7lend", "En\u00b7gel", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Ton sich liebevoll im Ton erfrischet.", "tokens": ["Wie", "Ton", "sich", "lie\u00b7be\u00b7voll", "im", "Ton", "er\u00b7fri\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wenn mir alles Leben nun erlischet,", "tokens": ["Und", "wenn", "mir", "al\u00b7les", "Le\u00b7ben", "nun", "er\u00b7li\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So brennen doch die beiden sch\u00f6nen Kerzen,", "tokens": ["So", "bren\u00b7nen", "doch", "die", "bei\u00b7den", "sch\u00f6\u00b7nen", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie leuchten nun von selbst zweifache Schmerzen,", "tokens": ["Sie", "leuch\u00b7ten", "nun", "von", "selbst", "zwei\u00b7fa\u00b7che", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was jeder klagt unkenntlich hier vermischet.", "tokens": ["Was", "je\u00b7der", "klagt", "un\u00b7kennt\u00b7lich", "hier", "ver\u00b7mi\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie s\u00fc\u00dfe Ahndung, da\u00df zur dunkeln Pforte,", "tokens": ["Wie", "s\u00fc\u00b7\u00dfe", "Ahn\u00b7dung", ",", "da\u00df", "zur", "dun\u00b7keln", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur ewgen Mutter, zu der unbekannten,", "tokens": ["Zur", "ew\u00b7gen", "Mut\u00b7ter", ",", "zu", "der", "un\u00b7be\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein unnennbares Weh mich niederziehet!", "tokens": ["Ein", "un\u00b7nenn\u00b7ba\u00b7res", "Weh", "mich", "nie\u00b7der\u00b7zie\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich weine nicht, da\u00df mir kein Fr\u00fchling bl\u00fchet,", "tokens": ["Ich", "wei\u00b7ne", "nicht", ",", "da\u00df", "mir", "kein", "Fr\u00fch\u00b7ling", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da mir sie deine s\u00fcssen Lippen nannten", "tokens": ["Da", "mir", "sie", "dei\u00b7ne", "s\u00fcs\u00b7sen", "Lip\u00b7pen", "nann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind Tod und Grab mir nun die sch\u00f6nsten Worte.", "tokens": ["Sind", "Tod", "und", "Grab", "mir", "nun", "die", "sch\u00f6ns\u00b7ten", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}