{"dta.poem.2629": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seelige Nachfolge/  \n Fr. S. E. g. R. den 21. May 1671.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war der h\u00f6chste Wunsch bey treu-verliebten Hertzen/", "tokens": ["Es", "war", "der", "h\u00f6chs\u00b7te", "Wunsch", "bey", "treu\u00b7ver\u00b7lieb\u00b7ten", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die noch das Alterthum der ersten Zeit erkant/", "tokens": ["Die", "noch", "das", "Al\u00b7ter\u00b7thum", "der", "ers\u00b7ten", "Zeit", "er\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df/ wie sie sich verm\u00e4hlt bey gleichen Hochzeit-Kertzen/", "tokens": ["Da\u00df", "/", "wie", "sie", "sich", "ver\u00b7m\u00e4hlt", "bey", "glei\u00b7chen", "Hoch\u00b7zeit\u00b7Kert\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "PPER", "PRF", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie auch in ihrem Tod umbfieng ein gleicher Brand.", "tokens": ["Sie", "auch", "in", "ih\u00b7rem", "Tod", "umb\u00b7fieng", "ein", "glei\u00b7cher", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie sehnlich w\u00fcnschten sie/ da\u00df die entleibten Schatten/", "tokens": ["Wie", "sehn\u00b7lich", "w\u00fcnschten", "sie", "/", "da\u00df", "die", "ent\u00b7leib\u00b7ten", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "In dem Elyser-Feld beysammen m\u00f6chten seyn!", "tokens": ["In", "dem", "E\u00b7ly\u00b7ser\u00b7Feld", "bey\u00b7sam\u00b7men", "m\u00f6ch\u00b7ten", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wie sie sich verkn\u00fcpfft in diesem Leben hatten/", "tokens": ["Und", "wie", "sie", "sich", "ver\u00b7kn\u00fcpfft", "in", "die\u00b7sem", "Le\u00b7ben", "hat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "APPR", "PDAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hofften sie auch dort di\u00df B\u00fcndn\u00fc\u00df zu verneu\u2019n.", "tokens": ["So", "hoff\u00b7ten", "sie", "auch", "dort", "di\u00df", "B\u00fcnd\u00b7n\u00fc\u00df", "zu", "ver\u00b7neu'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PDS", "NN", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es kan das Morgen Land sein Frauen-Zimmer zeigen/", "tokens": ["Es", "kan", "das", "Mor\u00b7gen", "Land", "sein", "Frau\u00b7en\u00b7Zim\u00b7mer", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So voller Brunst und Geist sprang in die Todten Glut:", "tokens": ["So", "vol\u00b7ler", "Brunst", "und", "Geist", "sprang", "in", "die", "Tod\u00b7ten", "Glut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es wird auch Sina nicht von solchen Weibern schweigen/", "tokens": ["Es", "wird", "auch", "Si\u00b7na", "nicht", "von", "sol\u00b7chen", "Wei\u00b7bern", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "PTKNEG", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die eben di\u00df gethan mit tapfferm Helden-Muth.", "tokens": ["Die", "e\u00b7ben", "di\u00df", "ge\u00b7than", "mit", "tapf\u00b7ferm", "Hel\u00b7den\u00b7Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PDS", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Arria kr\u00f6nt noch ein ewiges Ger\u00fcchte;", "tokens": ["Die", "Ar\u00b7ria", "kr\u00f6nt", "noch", "ein", "e\u00b7wi\u00b7ges", "Ge\u00b7r\u00fcch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Paulina schneidet ihr die Adern frisch entzwey/", "tokens": ["Pau\u00b7li\u00b7na", "schnei\u00b7det", "ihr", "die", "A\u00b7dern", "frisch", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Als sie den Seneca mit blassem Angesichte", "tokens": ["Als", "sie", "den", "Se\u00b7ne\u00b7ca", "mit", "blas\u00b7sem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Allm\u00e4hlich ziehen sieht zu der entseelten Rey.", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "zie\u00b7hen", "sieht", "zu", "der", "ent\u00b7seel\u00b7ten", "Rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Tranck Arthemisia nicht ihres Ehmanns Asche/", "tokens": ["Tranck", "Ar\u00b7the\u00b7mi\u00b7sia", "nicht", "ih\u00b7res", "Eh\u00b7manns", "A\u00b7sche", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "PPOSAT", "NN", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und baut ein k\u00f6stlich Grab zum Zeichen reiner Gunst?", "tokens": ["Und", "baut", "ein", "k\u00f6st\u00b7lich", "Grab", "zum", "Zei\u00b7chen", "rei\u00b7ner", "Gunst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und da\u00df sich Porcia von aller Schuld abwasche/", "tokens": ["Und", "da\u00df", "sich", "Por\u00b7cia", "von", "al\u00b7ler", "Schuld", "ab\u00b7wa\u00b7sche", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "NE", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "--+-+-+-++--", "measure": "anapaest.init"}, "line.20": {"text": "Sind Kohlen nicht so hei\u00df als ihre Liebes-Brunst.", "tokens": ["Sind", "Koh\u00b7len", "nicht", "so", "hei\u00df", "als", "ih\u00b7re", "Lie\u00b7bes\u00b7Brunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Allein ein eitler Wahn verblendte die Gem\u00fcther;", "tokens": ["Al\u00b7lein", "ein", "eit\u00b7ler", "Wahn", "ver\u00b7blend\u00b7te", "die", "Ge\u00b7m\u00fc\u00b7ther", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die rasende Begier nach der Unsterbligkeit/", "tokens": ["Die", "ra\u00b7sen\u00b7de", "Be\u00b7gier", "nach", "der", "U\u00b7nster\u00b7blig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Schlug ihnen gleichsam au\u00df Heil/ Leben/ Stand und G\u00fcter/", "tokens": ["Schlug", "ih\u00b7nen", "gleich\u00b7sam", "au\u00df", "Heil", "/", "Le\u00b7ben", "/", "Stand", "und", "G\u00fc\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn nur ein pr\u00e4chtig Lob ihr Grab-Mahl hat erfreut.", "tokens": ["Wenn", "nur", "ein", "pr\u00e4ch\u00b7tig", "Lob", "ihr", "Grab\u00b7Mahl", "hat", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So weit kam die Vernunfft mit irrigen Gedancken.", "tokens": ["So", "weit", "kam", "die", "Ver\u00b7nunfft", "mit", "ir\u00b7ri\u00b7gen", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nein/ wahrer Christen Lieb ist st\u00e4rcker als der Tod/", "tokens": ["Nein", "/", "wah\u00b7rer", "Chris\u00b7ten", "Lieb", "ist", "st\u00e4r\u00b7cker", "als", "der", "Tod", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ADJA", "NN", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie ist so fest gegr\u00fcndt/ und kan nicht leichtlich wancken/", "tokens": ["Sie", "ist", "so", "fest", "ge\u00b7gr\u00fcndt", "/", "und", "kan", "nicht", "leicht\u00b7lich", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$(", "KON", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ob sie gleich \u00fcberf\u00e4llt die allergr\u00f6ste Noth.", "tokens": ["Ob", "sie", "gleich", "\u00fc\u00b7berf\u00b7\u00e4llt", "die", "al\u00b7ler\u00b7gr\u00f6s\u00b7te", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und diese Liebe thut die gr\u00f6sten Wunderwercke/", "tokens": ["Und", "die\u00b7se", "Lie\u00b7be", "thut", "die", "gr\u00f6s\u00b7ten", "Wun\u00b7der\u00b7wer\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es ist das Heydenthum nur lauter Possenspiel.", "tokens": ["Es", "ist", "das", "Hey\u00b7den\u00b7thum", "nur", "lau\u00b7ter", "Pos\u00b7sen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer kennt nicht ihre Macht/ wer wei\u00df nicht ihre St\u00e4rcke?", "tokens": ["Wer", "kennt", "nicht", "ih\u00b7re", "Macht", "/", "wer", "wei\u00df", "nicht", "ih\u00b7re", "St\u00e4r\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$(", "PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wie nur Best\u00e4ndigkeit ihr ausgesetztes Ziel/", "tokens": ["Wie", "nur", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "ihr", "aus\u00b7ge\u00b7setz\u00b7tes", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein Zeugn\u00fc\u00df wahrer Treu und ungef\u00e4rbter Liebe/", "tokens": ["Ein", "Zeug\u00b7n\u00fc\u00df", "wah\u00b7rer", "Treu", "und", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die auch dem Tode sich entgegen hat gesetzt/", "tokens": ["Die", "auch", "dem", "To\u00b7de", "sich", "ent\u00b7ge\u00b7gen", "hat", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PRF", "PTKVZ", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und w\u00fcrdig da\u00df man sie in Ertz und Marmel schriebe/", "tokens": ["Und", "w\u00fcr\u00b7dig", "da\u00df", "man", "sie", "in", "Ertz", "und", "Mar\u00b7mel", "schrie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOUS", "PIS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Daran die grimme Zeit nicht ihre Z\u00e4hne wetzt/", "tokens": ["Da\u00b7ran", "die", "grim\u00b7me", "Zeit", "nicht", "ih\u00b7re", "Z\u00e4h\u00b7ne", "wetzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Zeigt der Frau Doctorin nunmehr entseelte Leiche/", "tokens": ["Zeigt", "der", "Frau", "Doc\u00b7to\u00b7rin", "nun\u00b7mehr", "ent\u00b7seel\u00b7te", "Lei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADV", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Die ihrem Ehgemahl folgt auf dem Fusse nach/", "tokens": ["Die", "ih\u00b7rem", "Eh\u00b7ge\u00b7mahl", "folgt", "auf", "dem", "Fus\u00b7se", "nach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und da\u00df sie nimmermehr von seiner Seiten weiche/", "tokens": ["Und", "da\u00df", "sie", "nim\u00b7mer\u00b7mehr", "von", "sei\u00b7ner", "Sei\u00b7ten", "wei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Deckt beyder C\u00f6rper auch ein gleiches Schlaff-Gemach.", "tokens": ["Deckt", "bey\u00b7der", "C\u00f6r\u00b7per", "auch", "ein", "glei\u00b7ches", "Schlaff\u00b7Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie hatte ihrem Schatz die Augen kaum geschlossen/", "tokens": ["Sie", "hat\u00b7te", "ih\u00b7rem", "Schatz", "die", "Au\u00b7gen", "kaum", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das Sterbekleid geneht/ den Todten-Zeug bestellt/", "tokens": ["Das", "Ster\u00b7be\u00b7kleid", "ge\u00