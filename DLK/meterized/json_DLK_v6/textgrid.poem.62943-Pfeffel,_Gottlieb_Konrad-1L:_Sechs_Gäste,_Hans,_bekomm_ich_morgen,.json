{"textgrid.poem.62943": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sechs G\u00e4ste, Hans, bekomm ich morgen,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sechs G\u00e4ste, Hans, bekomm ich morgen,", "tokens": ["Sechs", "G\u00e4s\u00b7te", ",", "Hans", ",", "be\u00b7komm", "ich", "mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach Junker Veit zu seinem Koch;", "tokens": ["Sprach", "Jun\u00b7ker", "Veit", "zu", "sei\u00b7nem", "Koch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "APPR", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du magst f\u00fcr eine Mahlzeit sorgen:", "tokens": ["Du", "magst", "f\u00fcr", "ei\u00b7ne", "Mahl\u00b7zeit", "sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geh, schlacht ein Kalb. Was wollt ihr doch,", "tokens": ["Geh", ",", "schlacht", "ein", "Kalb", ".", "Was", "wollt", "ihr", "doch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Rief Hans, mit einem ganzen Kalbe", "tokens": ["Rief", "Hans", ",", "mit", "ei\u00b7nem", "gan\u00b7zen", "Kal\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr sechs? der Rest wird euch zum Aas,", "tokens": ["F\u00fcr", "sechs", "?", "der", "Rest", "wird", "euch", "zum", "Aas", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "$.", "ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bey dieser Hitze. Wei\u00dft du was,", "tokens": ["Bey", "die\u00b7ser", "Hit\u00b7ze", ".", "Wei\u00dft", "du", "was", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sprach Veit, so schlachte nur das halbe.", "tokens": ["Sprach", "Veit", ",", "so", "schlach\u00b7te", "nur", "das", "hal\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "VVFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}