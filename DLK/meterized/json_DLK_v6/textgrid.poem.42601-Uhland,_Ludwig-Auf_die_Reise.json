{"textgrid.poem.42601": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Reise", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Mitternacht, auf pfadlos weitem Meer,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", ",", "auf", "pfad\u00b7los", "wei\u00b7tem", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wann alle Lichter l\u00e4ngst im Schiff erloschen,", "tokens": ["Wann", "al\u00b7le", "Lich\u00b7ter", "l\u00e4ngst", "im", "Schiff", "er\u00b7lo\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann auch am Himmel nirgends gl\u00e4nzt ein Stern,", "tokens": ["Wann", "auch", "am", "Him\u00b7mel", "nir\u00b7gends", "gl\u00e4nzt", "ein", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann gl\u00fcht ein L\u00e4mpchen noch auf dem Verdeck,", "tokens": ["Dann", "gl\u00fcht", "ein", "L\u00e4mp\u00b7chen", "noch", "auf", "dem", "Ver\u00b7deck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Docht, vor Windesungest\u00fcm verwahrt,", "tokens": ["Ein", "Docht", ",", "vor", "Win\u00b7de\u00b7sun\u00b7ge\u00b7st\u00fcm", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und h\u00e4lt dem Steuermann die Nadel hell,", "tokens": ["Und", "h\u00e4lt", "dem", "Steu\u00b7er\u00b7mann", "die", "Na\u00b7del", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die ihm untr\u00fcglich seine Richtung weist.", "tokens": ["Die", "ihm", "un\u00b7tr\u00fcg\u00b7lich", "sei\u00b7ne", "Rich\u00b7tung", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ja! wenn wir's h\u00fcten, f\u00fchrt durch jedes Dunkel", "tokens": ["Ja", "!", "wenn", "wir's", "h\u00fc\u00b7ten", ",", "f\u00fchrt", "durch", "je\u00b7des", "Dun\u00b7kel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "PIS", "VVINF", "$,", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein Licht uns, stille brennend in der Brust.", "tokens": ["Ein", "Licht", "uns", ",", "stil\u00b7le", "bren\u00b7nend", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}