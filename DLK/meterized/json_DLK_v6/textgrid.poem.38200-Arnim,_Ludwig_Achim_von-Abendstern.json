{"textgrid.poem.38200": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf nur ein geliebtes Leben,", "tokens": ["Schlaf", "nur", "ein", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlaf, ich will ja gern zufrieden seyn,", "tokens": ["Schlaf", ",", "ich", "will", "ja", "gern", "zu\u00b7frie\u00b7den", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deine lieben Augen geben", "tokens": ["Dei\u00b7ne", "lie\u00b7ben", "Au\u00b7gen", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch deinem Diener hellen Schein.", "tokens": ["Den\u00b7noch", "dei\u00b7nem", "Die\u00b7ner", "hel\u00b7len", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Hast du dich verschlossen,", "tokens": ["Hast", "du", "dich", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Will ich unverdrossen", "tokens": ["Will", "ich", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Liebend doch vor deiner Th\u00fcre stehn;", "tokens": ["Lie\u00b7bend", "doch", "vor", "dei\u00b7ner", "Th\u00fc\u00b7re", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df sie Liebe qu\u00e4le,", "tokens": ["Da\u00df", "sie", "Lie\u00b7be", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Jauchzet meine Seele,", "tokens": ["Jauch\u00b7zet", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Darf ich liebend doch an deiner Th\u00fcre stehn.", "tokens": ["Darf", "ich", "lie\u00b7bend", "doch", "an", "dei\u00b7ner", "Th\u00fc\u00b7re", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Schlaf nur ein, dein Sternenschimmer", "tokens": ["Schlaf", "nur", "ein", ",", "dein", "Ster\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "PTKVZ", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft mich nie zu meinem Bette gehn,", "tokens": ["L\u00e4\u00dft", "mich", "nie", "zu", "mei\u00b7nem", "Bet\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine m\u00fcden Augen sehn dich immer,", "tokens": ["Mei\u00b7ne", "m\u00fc\u00b7den", "Au\u00b7gen", "sehn", "dich", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis sie vor den deinen untergehn,", "tokens": ["Bis", "sie", "vor", "den", "dei\u00b7nen", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wie die ", "tokens": ["Wie", "die"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Also werd ich fallen,", "tokens": ["Al\u00b7so", "werd", "ich", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Unter deinem Fu\u00dfe rauschen hin,", "tokens": ["Un\u00b7ter", "dei\u00b7nem", "Fu\u00b7\u00dfe", "rau\u00b7schen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Mild bist du den Armen,", "tokens": ["Mild", "bist", "du", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Trage mir Erbarmen,", "tokens": ["Tra\u00b7ge", "mir", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Unter deinem Fu\u00dfe rausch ich hin.", "tokens": ["Un\u00b7ter", "dei\u00b7nem", "Fu\u00b7\u00dfe", "rausch", "ich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Schlaf nur ein, und hei\u00df mich wachend gehen,", "tokens": ["Schlaf", "nur", "ein", ",", "und", "hei\u00df", "mich", "wa\u00b7chend", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,", "KON", "ADJD", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herz und Seele bleibet doch bei dir,", "tokens": ["Herz", "und", "See\u00b7le", "blei\u00b7bet", "doch", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Will mir mit dem Tag die Sonne untergehen,", "tokens": ["Will", "mir", "mit", "dem", "Tag", "die", "Son\u00b7ne", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ist ein Liebeshimmel doch in mir,", "tokens": ["Ist", "ein", "Lie\u00b7bes\u00b7him\u00b7mel", "doch", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn da seh ich immer", "tokens": ["Denn", "da", "seh", "ich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Deiner Sterne Schimmer,", "tokens": ["Dei\u00b7ner", "Ster\u00b7ne", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie sie fl\u00fcchtig auf mein Herze gehn,", "tokens": ["Wie", "sie", "fl\u00fcch\u00b7tig", "auf", "mein", "Her\u00b7ze", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "S\u00e4h ich dich doch morgen,", "tokens": ["S\u00e4h", "ich", "dich", "doch", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Lie\u00df ich alle Sorgen", "tokens": ["Lie\u00df", "ich", "al\u00b7le", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Also fl\u00fcchtig durch mein Herze gehn.", "tokens": ["Al\u00b7so", "fl\u00fcch\u00b7tig", "durch", "mein", "Her\u00b7ze", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}