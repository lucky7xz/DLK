{"dta.poem.7893": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des Hirten Winterlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "O Winter, schlimmer Winter!", "tokens": ["O", "Win\u00b7ter", ",", "schlim\u00b7mer", "Win\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ist die Welt so klein!", "tokens": ["Wie", "ist", "die", "Welt", "so", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du dr\u00e4ngst uns all in die Th\u00e4ler,", "tokens": ["Du", "dr\u00e4ngst", "uns", "all", "in", "die", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In die engen H\u00fctten hinein.", "tokens": ["In", "die", "en\u00b7gen", "H\u00fct\u00b7ten", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und geh\u2019 ich auch vor\u00fcber", "tokens": ["Und", "geh'", "ich", "auch", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meiner Liebsten Haus:", "tokens": ["An", "mei\u00b7ner", "Liebs\u00b7ten", "Haus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum sieht sie mit dem K\u00f6pfchen", "tokens": ["Kaum", "sieht", "sie", "mit", "dem", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum kleinen Fenster heraus.", "tokens": ["Zum", "klei\u00b7nen", "Fens\u00b7ter", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und nehm\u2019 ich\u2019s Herz in die H\u00e4nde,", "tokens": ["Und", "nehm'", "ich's", "Herz", "in", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und geh\u2019 hinauf in\u2019s Haus:", "tokens": ["Und", "geh'", "hin\u00b7auf", "in's", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sitzt zwischen Vater und Mutter,", "tokens": ["Sie", "sitzt", "zwi\u00b7schen", "Va\u00b7ter", "und", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schaut kaum zu den Aeuglein heraus.", "tokens": ["Schaut", "kaum", "zu", "den", "A\u00b7e\u00b7u\u00b7glein", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "O Sommer, sch\u00f6ner Sommer!", "tokens": ["O", "Som\u00b7mer", ",", "sch\u00f6\u00b7ner", "Som\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie wird die Welt so weit!", "tokens": ["Wie", "wird", "die", "Welt", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Je h\u00f6her man steigt auf die Berge,", "tokens": ["Je", "h\u00f6\u00b7her", "man", "steigt", "auf", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Je weiter sie sich verbreit\u2019t.", "tokens": ["Je", "wei\u00b7ter", "sie", "sich", "ver\u00b7breit'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVFIN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und stehest du auf dem Felsen,", "tokens": ["Und", "ste\u00b7hest", "du", "auf", "dem", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Traut Liebchen! ich rufe dir zu.", "tokens": ["Traut", "Lieb\u00b7chen", "!", "ich", "ru\u00b7fe", "dir", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Halle sagen es weiter,", "tokens": ["Die", "Hal\u00b7le", "sa\u00b7gen", "es", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch Niemand h\u00f6rt es, als du.", "tokens": ["Doch", "Nie\u00b7mand", "h\u00f6rt", "es", ",", "als", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und halt\u2019 ich dich in den Armen", "tokens": ["Und", "halt'", "ich", "dich", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf freien Bergesh\u00f6hn:", "tokens": ["Auf", "frei\u00b7en", "Ber\u00b7ges\u00b7h\u00f6hn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sehn in die weiten Lande,", "tokens": ["Wir", "sehn", "in", "die", "wei\u00b7ten", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und werden doch nicht gesehn.", "tokens": ["Und", "wer\u00b7den", "doch", "nicht", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}