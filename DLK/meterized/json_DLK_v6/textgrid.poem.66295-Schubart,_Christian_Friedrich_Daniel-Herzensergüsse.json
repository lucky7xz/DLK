{"textgrid.poem.66295": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Herzenserg\u00fcsse", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir! Erster! Letzter!", "tokens": ["Dir", "!", "Ers\u00b7ter", "!", "Letz\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Allgewaltiger Wesenvater, dir,", "tokens": ["All\u00b7ge\u00b7wal\u00b7ti\u00b7ger", "We\u00b7sen\u00b7va\u00b7ter", ",", "dir", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dir Schauer voll Huld", "tokens": ["Dir", "Schau\u00b7er", "voll", "Huld"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "ADJD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Aus b\u00fc\u00dfende S\u00fcnder!", "tokens": ["Aus", "b\u00fc\u00b7\u00dfen\u00b7de", "S\u00fcn\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dir Schauer voll Huld", "tokens": ["Dir", "Schau\u00b7er", "voll", "Huld"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "ADJD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Auf mich, den b\u00fc\u00dfenden S\u00fcnder!", "tokens": ["Auf", "mich", ",", "den", "b\u00fc\u00b7\u00dfen\u00b7den", "S\u00fcn\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Dir flie\u00dfe mein Lied innig und hei\u00df,", "tokens": ["Dir", "flie\u00b7\u00dfe", "mein", "Lied", "in\u00b7nig", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie der Liebe erste Z\u00e4hre mir entflo\u00df.", "tokens": ["Wie", "der", "Lie\u00b7be", "ers\u00b7te", "Z\u00e4h\u00b7re", "mir", "ent\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Ehernen Bergen gleich", "tokens": ["E\u00b7her\u00b7nen", "Ber\u00b7gen", "gleich"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lag meiner Verschuldungen Last", "tokens": ["Lag", "mei\u00b7ner", "Ver\u00b7schul\u00b7dun\u00b7gen", "Last"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf mir! Weltrichter, auf mir!", "tokens": ["Auf", "mir", "!", "Welt\u00b7rich\u00b7ter", ",", "auf", "mir", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "NN", "$,", "APPR", "PPER", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich sah den m\u00e4chtigen Verkl\u00e4ger", "tokens": ["Ich", "sah", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Ver\u00b7kl\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Satan stehn zu meiner Rechten.", "tokens": ["Sa\u00b7tan", "stehn", "zu", "mei\u00b7ner", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hinauf schrie er zum Throne,", "tokens": ["Hin\u00b7auf", "schrie", "er", "zum", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df Gluthasche mit des Kl\u00e4gers", "tokens": ["Da\u00df", "Glut\u00b7ha\u00b7sche", "mit", "des", "Kl\u00e4\u00b7gers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Odem flog. Hinauf schrie er:", "tokens": ["O\u00b7dem", "flog", ".", "Hin\u00b7auf", "schrie", "er", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Du bist heilig! Er deiner Heiligkeit", "tokens": ["Du", "bist", "hei\u00b7lig", "!", "Er", "dei\u00b7ner", "Hei\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "PPOSAT", "NN"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sch\u00e4ndlicher Entweiher!", "tokens": ["Sch\u00e4nd\u00b7li\u00b7cher", "Ent\u00b7wei\u00b7her", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Verwirf ihn!", "tokens": ["Ver\u00b7wirf", "ihn", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Du bist gerecht! Er soff das Unrecht", "tokens": ["Du", "bist", "ge\u00b7recht", "!", "Er", "soff", "das", "Un\u00b7recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In sich wie Wasser.", "tokens": ["In", "sich", "wie", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KOKOM", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Verwirf ihn!", "tokens": ["Ver\u00b7wirf", "ihn", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Du reiner, als das Lichtgewand,", "tokens": ["Du", "rei\u00b7ner", ",", "als", "das", "Licht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das dich umgibt!", "tokens": ["Das", "dich", "um\u00b7gibt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Er ein Schlammbewohner,", "tokens": ["Er", "ein", "Schlamm\u00b7be\u00b7woh\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Von faulem Wasser", "tokens": ["Von", "fau\u00b7lem", "Was\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Stinkender Sinnlichkeit tr\u00e4ufend.", "tokens": ["Stin\u00b7ken\u00b7der", "Sinn\u00b7lich\u00b7keit", "tr\u00e4u\u00b7fend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Verwirf ihn!", "tokens": ["Ver\u00b7wirf", "ihn", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Du ein Gott voll Licht und Wahrheit,", "tokens": ["Du", "ein", "Gott", "voll", "Licht", "und", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er des Urdunkels Genosse,", "tokens": ["Er", "des", "Ur\u00b7dun\u00b7kels", "Ge\u00b7nos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ein Schm\u00e4her der gottgesandten Wahrheit!", "tokens": ["Ein", "Schm\u00e4\u00b7her", "der", "gott\u00b7ge\u00b7sand\u00b7ten", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deines Sohnes Schm\u00e4her!", "tokens": ["Dei\u00b7nes", "Soh\u00b7nes", "Schm\u00e4\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Deines Geistes Schm\u00e4her!", "tokens": ["Dei\u00b7nes", "Geis\u00b7tes", "Schm\u00e4\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Verwirf ihn!", "tokens": ["Ver\u00b7wirf", "ihn", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Gebiete dem ruhenden Donner", "tokens": ["Ge\u00b7bie\u00b7te", "dem", "ru\u00b7hen\u00b7den", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "An deines Thrones Fu\u00dfe,", "tokens": ["An", "dei\u00b7nes", "Thro\u00b7nes", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er zuck' und schlag' und t\u00f6dte", "tokens": ["Da\u00df", "er", "zuck'", "und", "schlag'", "und", "t\u00f6d\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Emp\u00f6rer!", "tokens": ["Den", "Em\u00b7p\u00f6\u00b7rer", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Oder la\u00df mich,", "tokens": ["O\u00b7der", "la\u00df", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Zaudrer auf deinem Richtthrone!", "tokens": ["Zaud\u00b7rer", "auf", "dei\u00b7nem", "Richt\u00b7thro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df ich ihn h\u00fcll' in Wettergew\u00f6lk,", "tokens": ["Da\u00df", "ich", "ihn", "h\u00fcll'", "in", "Wet\u00b7ter\u00b7ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und ihn fortw\u00e4lz' unterm Geheul", "tokens": ["Und", "ihn", "fort\u00b7w\u00e4lz'", "un\u00b7term", "Ge\u00b7heul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und dem Wehausruf meiner Sklaven", "tokens": ["Und", "dem", "We\u00b7haus\u00b7ruf", "mei\u00b7ner", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "--++-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Hinab in der H\u00f6lle g\u00e4hnenden Schlund!", "tokens": ["Hin\u00b7ab", "in", "der", "H\u00f6l\u00b7le", "g\u00e4h\u00b7nen\u00b7den", "Schlund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da\u00df ich ihn an meines Thrones Wurzel", "tokens": ["Da\u00df", "ich", "ihn", "an", "mei\u00b7nes", "Thro\u00b7nes", "Wur\u00b7zel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schmiede mit ewigen Ketten;", "tokens": ["Schmie\u00b7de", "mit", "e\u00b7wi\u00b7gen", "Ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Da\u00df ich ihn taufe mit Flammen", "tokens": ["Da\u00df", "ich", "ihn", "tau\u00b7fe", "mit", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und ihn weihe zum Genossen der H\u00f6lle!", "tokens": ["Und", "ihn", "wei\u00b7he", "zum", "Ge\u00b7nos\u00b7sen", "der", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Zaudrer deines Throns,", "tokens": ["Zaud\u00b7rer", "dei\u00b7nes", "Throns", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "La\u00df mich, la\u00df mich, da\u00df ich ihn weihe!", "tokens": ["La\u00df", "mich", ",", "la\u00df", "mich", ",", "da\u00df", "ich", "ihn", "wei\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Still ward's im Himmel. Ich h\u00f6rte", "tokens": ["Still", "ward's", "im", "Him\u00b7mel", ".", "Ich", "h\u00f6r\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die m\u00e4chtige Klage", "tokens": ["Die", "m\u00e4ch\u00b7ti\u00b7ge", "Kla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hinunterdonnern die Seele.", "tokens": ["Hin\u00b7un\u00b7ter\u00b7don\u00b7nern", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zersplittern wollt' ich den hallenden Sch\u00e4del", "tokens": ["Zer\u00b7split\u00b7tern", "wollt'", "ich", "den", "hal\u00b7len\u00b7den", "Sch\u00e4\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "An den Felsenrippen meines Gekl\u00fcfts;", "tokens": ["An", "den", "Fel\u00b7sen\u00b7rip\u00b7pen", "mei\u00b7nes", "Ge\u00b7kl\u00fcfts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Aber deine voreilende Gnade, Erbarmer,", "tokens": ["A\u00b7ber", "dei\u00b7ne", "vor\u00b7ei\u00b7len\u00b7de", "Gna\u00b7de", ",", "Er\u00b7bar\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Warf einen der erquickendsten Lichtstrahlen", "tokens": ["Warf", "ei\u00b7nen", "der", "er\u00b7qui\u00b7ckends\u00b7ten", "Licht\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In meine Seele voll Nacht.", "tokens": ["In", "mei\u00b7ne", "See\u00b7le", "voll", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Ich sank auf die Ziegel meines Kerkergrabs,", "tokens": ["Ich", "sank", "auf", "die", "Zie\u00b7gel", "mei\u00b7nes", "Ker\u00b7ker\u00b7grabs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Thr\u00e4nen st\u00fcrzten, wie Blut,", "tokens": ["Und", "Thr\u00e4\u00b7nen", "st\u00fcrz\u00b7ten", ",", "wie", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["K