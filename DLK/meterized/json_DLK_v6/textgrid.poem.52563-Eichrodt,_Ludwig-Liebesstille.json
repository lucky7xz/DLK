{"textgrid.poem.52563": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Liebesstille", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00dfe, Liebchen, schlie\u00dfe zu die Augenlieder,", "tokens": ["Schlie\u00b7\u00dfe", ",", "Lieb\u00b7chen", ",", "schlie\u00b7\u00dfe", "zu", "die", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "La\u00df versiegen deiner Rede holden Flu\u00df!", "tokens": ["La\u00df", "ver\u00b7sie\u00b7gen", "dei\u00b7ner", "Re\u00b7de", "hol\u00b7den", "Flu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Deine Wange presse st\u00fcrmisch an die meine,", "tokens": ["Dei\u00b7ne", "Wan\u00b7ge", "pres\u00b7se", "st\u00fcr\u00b7misch", "an", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PRELS", "PPOSAT", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Auf den Lippen schlummre s\u00fc\u00df ein ewger Ku\u00df.", "tokens": ["Auf", "den", "Lip\u00b7pen", "schlumm\u00b7re", "s\u00fc\u00df", "ein", "ew\u00b7ger", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "La\u00df uns tr\u00e4umen, theure Seele,", "tokens": ["La\u00df", "uns", "tr\u00e4u\u00b7men", ",", "theu\u00b7re", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hingeklungne Stunden wieder,", "tokens": ["Hin\u00b7ge\u00b7klung\u00b7ne", "Stun\u00b7den", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlie\u00dfe, Liebchen, schlie\u00dfe zu die Augenlieder,", "tokens": ["Schlie\u00b7\u00dfe", ",", "Lieb\u00b7chen", ",", "schlie\u00b7\u00dfe", "zu", "die", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Aber zittre du nicht, weil ich weinen mu\u00df.", "tokens": ["A\u00b7ber", "zitt\u00b7re", "du", "nicht", ",", "weil", "ich", "wei\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Denk an alle, all die hei\u00dfen Augenblicke,", "tokens": ["Denk", "an", "al\u00b7le", ",", "all", "die", "hei\u00b7\u00dfen", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "$,", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Da die Liebe mir, du mir, ich dir gestand;", "tokens": ["Da", "die", "Lie\u00b7be", "mir", ",", "du", "mir", ",", "ich", "dir", "ge\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "$,", "PPER", "PPER", "$,", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Wenn dich eine stille Wonne \u00fcberwallet,", "tokens": ["Wenn", "dich", "ei\u00b7ne", "stil\u00b7le", "Won\u00b7ne", "\u00fc\u00b7berw\u00b7al\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Heb die Wimper, dr\u00fccke feurig mir die Hand!", "tokens": ["Heb", "die", "Wim\u00b7per", ",", "dr\u00fc\u00b7cke", "feu\u00b7rig", "mir", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Schw\u00e4rme in den Seligkeiten,", "tokens": ["Schw\u00e4r\u00b7me", "in", "den", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwelg in dem verrauschten Gl\u00fccke,", "tokens": ["Schwelg", "in", "dem", "ver\u00b7rauschten", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Denk an alle, all die hei\u00dfen Augenblicke,", "tokens": ["Denk", "an", "al\u00b7le", ",", "all", "die", "hei\u00b7\u00dfen", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "$,", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Da dein Herz das meine, ich das deine fand.", "tokens": ["Da", "dein", "Herz", "das", "mei\u00b7ne", ",", "ich", "das", "dei\u00b7ne", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "PPOSAT", "$,", "PPER", "ART", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach! Erinnerung und reiches Angedenken", "tokens": ["Ach", "!", "E\u00b7rin\u00b7ne\u00b7rung", "und", "rei\u00b7ches", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ist allein, du wei\u00dft es, aller Liebe Lohn;", "tokens": ["Ist", "al\u00b7lein", ",", "du", "wei\u00dft", "es", ",", "al\u00b7ler", "Lie\u00b7be", "Lohn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Sollte Eines je von uns, Geliebte, sterben \u2013", "tokens": ["Soll\u00b7te", "Ei\u00b7nes", "je", "von", "uns", ",", "Ge\u00b7lieb\u00b7te", ",", "ster\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPR", "PPER", "$,", "NN", "$,", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "So die Saite springet, zittert ", "tokens": ["So", "die", "Sai\u00b7te", "sprin\u00b7get", ",", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Wolle drum die theuren Perlen", "tokens": ["Wol\u00b7le", "drum", "die", "theu\u00b7ren", "Per\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Busen tief versenken!", "tokens": ["In", "den", "Bu\u00b7sen", "tief", "ver\u00b7sen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach! Erinnerung und reiches Angedenken", "tokens": ["Ach", "!", "E\u00b7rin\u00b7ne\u00b7rung", "und", "rei\u00b7ches", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Lindert, heilet heute k\u00fcnftge Schmerzen schon.", "tokens": ["Lin\u00b7dert", ",", "hei\u00b7let", "heu\u00b7te", "k\u00fcnft\u00b7ge", "Schmer\u00b7zen", "schon", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}