{"textgrid.poem.37073": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Sangrich", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch wohnet Unschuld, die von der Marn' entfloh,", "tokens": ["Noch", "woh\u00b7net", "Un\u00b7schuld", ",", "die", "von", "der", "Marn'", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deutschen M\u00e4dchen. Tugend und Sanftmuth blickt", "tokens": ["In", "deut\u00b7schen", "M\u00e4d\u00b7chen", ".", "Tu\u00b7gend", "und", "Sanft\u00b7muth", "blickt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus ihren gro\u00dfen blauen Augen,", "tokens": ["Aus", "ih\u00b7ren", "gro\u00b7\u00dfen", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sich der Engel, die Seele, spiegelt.", "tokens": ["Wo", "sich", "der", "En\u00b7gel", ",", "die", "See\u00b7le", ",", "spie\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Nicht Purpurrosen, welche die Schminke schafft,", "tokens": ["Nicht", "Pur\u00b7pur\u00b7ro\u00b7sen", ",", "wel\u00b7che", "die", "Schmin\u00b7ke", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entknospen auf den Wangen der M\u00e4dchen sich;", "tokens": ["Ent\u00b7knos\u00b7pen", "auf", "den", "Wan\u00b7gen", "der", "M\u00e4d\u00b7chen", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PRF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die m\u00f6gen auf den Wangen Deiner", "tokens": ["Die", "m\u00f6\u00b7gen", "auf", "den", "Wan\u00b7gen", "Dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6chter, Lutetien, sich entfalten!", "tokens": ["T\u00f6ch\u00b7ter", ",", "Lu\u00b7te\u00b7ti\u00b7en", ",", "sich", "ent\u00b7fal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PRF", "VVINF", "$."], "meter": "+-+-----+-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Die s\u00fc\u00dfe R\u00f6the sch\u00fcchterner Sittsamkeit", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "R\u00f6\u00b7the", "sch\u00fcch\u00b7ter\u00b7ner", "Sitt\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umstr\u00f6mt ihr Antlitz, wenn sich der J\u00fcngling naht,", "tokens": ["Um\u00b7str\u00f6mt", "ihr", "Ant\u00b7litz", ",", "wenn", "sich", "der", "J\u00fcng\u00b7ling", "naht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den ihre Seelen lieben, und dann", "tokens": ["Den", "ih\u00b7re", "See\u00b7len", "lie\u00b7ben", ",", "und", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,", "KON", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Blicke den Blicken entgegenschmachten.", "tokens": ["Bli\u00b7cke", "den", "Bli\u00b7cken", "ent\u00b7ge\u00b7gen\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ein freudenseelig L\u00e4cheln entschwebet oft", "tokens": ["Ein", "freu\u00b7den\u00b7see\u00b7lig", "L\u00e4\u00b7cheln", "ent\u00b7schwe\u00b7bet", "oft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Gr\u00fcbchen ihrer Wangen und blitzet flugs", "tokens": ["Den", "Gr\u00fcb\u00b7chen", "ih\u00b7rer", "Wan\u00b7gen", "und", "blit\u00b7zet", "flugs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Eden in die Brust \u2013 \u2013 O Wonne!", "tokens": ["Ein", "E\u00b7den", "in", "die", "Brust", "\u2013", "\u2013", "O", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "$(", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wonne dem S\u00e4nger der deutschen M\u00e4dchen!", "tokens": ["Won\u00b7ne", "dem", "S\u00e4n\u00b7ger", "der", "deut\u00b7schen", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Sie lieben deutsche Lieder, beseelen oft", "tokens": ["Sie", "lie\u00b7ben", "deut\u00b7sche", "Lie\u00b7der", ",", "be\u00b7see\u00b7len", "oft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Klavier und Laute, gie\u00dfen den Silberstrom", "tokens": ["Kla\u00b7vier", "und", "Lau\u00b7te", ",", "gie\u00b7\u00dfen", "den", "Sil\u00b7ber\u00b7strom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des Zaubersangs darinn \u2013 \u2013 O Wonne!", "tokens": ["Des", "Zau\u00b7ber\u00b7sangs", "da\u00b7rinn", "\u2013", "\u2013", "O", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$(", "$(", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wonne dem S\u00e4nger der deutschen M\u00e4dchen!", "tokens": ["Won\u00b7ne", "dem", "S\u00e4n\u00b7ger", "der", "deut\u00b7schen", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Sey, Freund, ihr S\u00e4nger! Mutter Natur verlieh", "tokens": ["Sey", ",", "Freund", ",", "ihr", "S\u00e4n\u00b7ger", "!", "Mut\u00b7ter", "Na\u00b7tur", "ver\u00b7lieh"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "NN", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir zart Gef\u00fchl und zaubernden Harfengriff;", "tokens": ["Dir", "zart", "Ge\u00b7f\u00fchl", "und", "zau\u00b7bern\u00b7den", "Har\u00b7fen\u00b7griff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es wandeln sich ob Deinen Liedern", "tokens": ["Es", "wan\u00b7deln", "sich", "ob", "Dei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrme des Busens ins Westges\u00e4usel.", "tokens": ["St\u00fcr\u00b7me", "des", "Bu\u00b7sens", "ins", "West\u00b7ge\u00b7s\u00e4u\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Besing die Wonnen, welche die Liebe giebt,", "tokens": ["Be\u00b7sing", "die", "Won\u00b7nen", ",", "wel\u00b7che", "die", "Lie\u00b7be", "giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Tugend Schwester, wenn sich der Geist bes\u00e4uft,", "tokens": ["Der", "Tu\u00b7gend", "Schwes\u00b7ter", ",", "wenn", "sich", "der", "Geist", "be\u00b7s\u00e4uft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch tausend Irren schwankt, dem offnen", "tokens": ["Durch", "tau\u00b7send", "Ir\u00b7ren", "schwankt", ",", "dem", "off\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Himmel der Himmel entgegentaumelt.", "tokens": ["Him\u00b7mel", "der", "Him\u00b7mel", "ent\u00b7ge\u00b7gen\u00b7tau\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Lobpreis' auch Unschuld, Unschuld, den Genius", "tokens": ["Lob\u00b7preis'", "auch", "Un\u00b7schuld", ",", "Un\u00b7schuld", ",", "den", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "NN", "$,", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der deutschen M\u00e4dchen; S\u00e4nger, Dein s\u00fc\u00dfes Lied", "tokens": ["Der", "deut\u00b7schen", "M\u00e4d\u00b7chen", ";", "S\u00e4n\u00b7ger", ",", "Dein", "s\u00fc\u00b7\u00dfes", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Soll einst das M\u00e4dchen wirbeln, das mich", "tokens": ["Soll", "einst", "das", "M\u00e4d\u00b7chen", "wir\u00b7beln", ",", "das", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,", "PRELS", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "K\u00fcnftig, so fl\u00fcstert mein Engel, liebet;", "tokens": ["K\u00fcnf\u00b7tig", ",", "so", "fl\u00fcs\u00b7tert", "mein", "En\u00b7gel", ",", "lie\u00b7bet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "In Bl\u00fcthenlauben wirbeln, wenn D\u00e4mmerung", "tokens": ["In", "Bl\u00fc\u00b7then\u00b7lau\u00b7ben", "wir\u00b7beln", ",", "wenn", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bestr\u00f6mt mit R\u00f6the winket \u2013 \u2013 Wir kosen dann", "tokens": ["Be\u00b7str\u00f6mt", "mit", "R\u00f6\u00b7the", "win\u00b7ket", "\u2013", "\u2013", "Wir", "ko\u00b7sen", "dann"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "VVFIN", "$(", "$(", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Abendstern ins Meer hinunter,", "tokens": ["Den", "A\u00b7bends\u00b7tern", "ins", "Meer", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kosen von Dir und unsrer Freundschaft.", "tokens": ["Ko\u00b7sen", "von", "Dir", "und", "uns\u00b7rer", "Freund\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}