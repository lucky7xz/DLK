{"textgrid.poem.27905": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott wird den Himmel neu und schaffen neu die Erde;", "tokens": ["Gott", "wird", "den", "Him\u00b7mel", "neu", "und", "schaf\u00b7fen", "neu", "die", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was soll die alte Welt? Sie wird zur H\u00f6lle werden;", "tokens": ["Was", "soll", "die", "al\u00b7te", "Welt", "?", "Sie", "wird", "zur", "H\u00f6l\u00b7le", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie ist die H\u00f6lle schon, in ihr ist lauter Pein,", "tokens": ["Sie", "ist", "die", "H\u00f6l\u00b7le", "schon", ",", "in", "ihr", "ist", "lau\u00b7ter", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "APPR", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil Krieg wie Feuer brennt, weil Menschen Teuffel seyn.", "tokens": ["Weil", "Krieg", "wie", "Feu\u00b7er", "brennt", ",", "weil", "Men\u00b7schen", "Teuf\u00b7fel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KOKOM", "NN", "VVFIN", "$,", "KOUS", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}