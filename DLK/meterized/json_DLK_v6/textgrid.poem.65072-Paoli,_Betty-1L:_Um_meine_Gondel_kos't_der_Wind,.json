{"textgrid.poem.65072": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um meine Gondel kos't der Wind,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um meine Gondel kos't der Wind,", "tokens": ["Um", "mei\u00b7ne", "Gon\u00b7del", "kos't", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht die Wellen spielend sch\u00e4umen.", "tokens": ["Und", "macht", "die", "Wel\u00b7len", "spie\u00b7lend", "sch\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Geisterhauch umweht mich lind,", "tokens": ["Sein", "Geis\u00b7ter\u00b7hauch", "um\u00b7weht", "mich", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich denke: Also mu\u00df ein Kind", "tokens": ["Ich", "den\u00b7ke", ":", "Al\u00b7so", "mu\u00df", "ein", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In seiner Wiege ruh'n und tr\u00e4umen.", "tokens": ["In", "sei\u00b7ner", "Wie\u00b7ge", "ruh'n", "und", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gondel tr\u00e4gt mich weit hinaus", "tokens": ["Die", "Gon\u00b7del", "tr\u00e4gt", "mich", "weit", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach mondbegl\u00e4nzten Meeresr\u00e4umen,", "tokens": ["Nach", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Mee\u00b7res\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bei der Fluthen leisem Braus", "tokens": ["Und", "bei", "der", "Flut\u00b7hen", "lei\u00b7sem", "Braus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denk' ich: So mag's im letzten Haus,", "tokens": ["Denk'", "ich", ":", "So", "mag's", "im", "letz\u00b7ten", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "ADV", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Sarg' sich wonnig ruh'n und tr\u00e4umen.", "tokens": ["Im", "Sar\u00b7g'", "sich", "won\u00b7nig", "ruh'n", "und", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}