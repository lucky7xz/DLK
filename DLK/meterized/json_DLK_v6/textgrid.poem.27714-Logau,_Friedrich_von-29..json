{"textgrid.poem.27714": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mein Buch die theure Gabe", "tokens": ["Da\u00df", "mein", "Buch", "die", "theu\u00b7re", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allen zu gefallen habe,", "tokens": ["Al\u00b7len", "zu", "ge\u00b7fal\u00b7len", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4ub ich nicht; doch wil ich hoffen,", "tokens": ["Gl\u00e4ub", "ich", "nicht", ";", "doch", "wil", "ich", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das, was folgt, sei gar getroffen:", "tokens": ["Das", ",", "was", "folgt", ",", "sei", "gar", "ge\u00b7trof\u00b7fen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O es m\u00fcsse h\u00f6llisch Feuer", "tokens": ["O", "es", "m\u00fcs\u00b7se", "h\u00f6l\u00b7lisch", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fressen die verfluchte Steuer!", "tokens": ["Fres\u00b7sen", "die", "ver\u00b7fluch\u00b7te", "Steu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}