{"textgrid.poem.58721": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, schau die Seel' gefangen sitzen,", "tokens": ["Herr", ",", "schau", "die", "Seel'", "ge\u00b7fan\u00b7gen", "sit\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Teufel und das Fleisch versperren T\u00fcr und Ritzen;", "tokens": ["Der", "Teu\u00b7fel", "und", "das", "Fleisch", "ver\u00b7sper\u00b7ren", "T\u00fcr", "und", "Rit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da du mich so geliebet, ach,", "tokens": ["Da", "du", "mich", "so", "ge\u00b7lie\u00b7bet", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$,", "ITJ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erl\u00f6se mich, weil's keiner sonst vermag!", "tokens": ["Er\u00b7l\u00f6\u00b7se", "mich", ",", "weil's", "kei\u00b7ner", "sonst", "ver\u00b7mag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gott, den mein Geist anbetend ehret,", "tokens": ["Gott", ",", "den", "mein", "Geist", "an\u00b7be\u00b7tend", "eh\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Machst du mich hievon frei, wird mein Gebet erh\u00f6ret,", "tokens": ["Machst", "du", "mich", "hie\u00b7von", "frei", ",", "wird", "mein", "Ge\u00b7bet", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PAV", "PTKVZ", "$,", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann mach durch deine Wahrheit doch", "tokens": ["Dann", "mach", "durch", "dei\u00b7ne", "Wahr\u00b7heit", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich alle Tage freier noch!", "tokens": ["Mich", "al\u00b7le", "Ta\u00b7ge", "frei\u00b7er", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Entrei\u00dft du mich des Feindes Ketten,", "tokens": ["Ent\u00b7rei\u00dft", "du", "mich", "des", "Fein\u00b7des", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df nimmer, nimmer mich ins Netz zur\u00fccke treten,", "tokens": ["La\u00df", "nim\u00b7mer", ",", "nim\u00b7mer", "mich", "ins", "Netz", "zu\u00b7r\u00fc\u00b7cke", "tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ADV", "PPER", "APPRART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich erliege, la\u00df nicht zu!", "tokens": ["Da\u00df", "ich", "er\u00b7lie\u00b7ge", ",", "la\u00df", "nicht", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVIMP", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht halb, ganz selig machest du.", "tokens": ["Nicht", "halb", ",", "ganz", "se\u00b7lig", "ma\u00b7chest", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Halt mich dir nahe unabwendig,", "tokens": ["Halt", "mich", "dir", "na\u00b7he", "un\u00b7ab\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADJD", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es liebe dich mein Herz, es bete an best\u00e4ndig,", "tokens": ["Es", "lie\u00b7be", "dich", "mein", "Herz", ",", "es", "be\u00b7te", "an", "be\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist mein Br\u00e4utigam allein,", "tokens": ["Du", "bist", "mein", "Br\u00e4u\u00b7ti\u00b7gam", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur du fl\u00f6\u00dft solche Gnaden ein!", "tokens": ["Nur", "du", "fl\u00f6\u00dft", "sol\u00b7che", "Gna\u00b7den", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}