{"textgrid.poem.26475": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Tages laute Stimmen schweigen", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Tages laute Stimmen schweigen", "tokens": ["Des", "Ta\u00b7ges", "lau\u00b7te", "Stim\u00b7men", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dunkeln will es allgemach;", "tokens": ["Und", "dun\u00b7keln", "will", "es", "all\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein letztes Schimmern in den Zweigen \u2013", "tokens": ["Ein", "letz\u00b7tes", "Schim\u00b7mern", "in", "den", "Zwei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann zieht auch dies der Sonne nach.", "tokens": ["Dann", "zieht", "auch", "dies", "der", "Son\u00b7ne", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch leuchten ihre Purpurgluthen", "tokens": ["Noch", "leuch\u00b7ten", "ih\u00b7re", "Pur\u00b7pur\u00b7glu\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um jene H\u00f6hen, kahl und fern;", "tokens": ["Um", "je\u00b7ne", "H\u00f6\u00b7hen", ",", "kahl", "und", "fern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch in des Aethers klaren Fluthen", "tokens": ["Doch", "in", "des", "A\u00b7e\u00b7thers", "kla\u00b7ren", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erzittert schon ein blasser Stern.", "tokens": ["Er\u00b7zit\u00b7tert", "schon", "ein", "blas\u00b7ser", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr m\u00fcden Seelen rings im Kreise,", "tokens": ["Ihr", "m\u00fc\u00b7den", "See\u00b7len", "rings", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So ist euch wieder Ruh' gebracht;", "tokens": ["So", "ist", "euch", "wie\u00b7der", "Ruh'", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufathmen h\u00f6r' ich euch noch leise \u2013", "tokens": ["Auf\u00b7ath\u00b7men", "h\u00f6r'", "ich", "euch", "noch", "lei\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann k\u00fc\u00dft euch still und mild die Nacht.", "tokens": ["Dann", "k\u00fc\u00dft", "euch", "still", "und", "mild", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}