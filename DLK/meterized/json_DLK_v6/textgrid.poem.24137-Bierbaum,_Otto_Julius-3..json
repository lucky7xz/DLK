{"textgrid.poem.24137": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, du liebst mich. Oh, ich danke dir!", "tokens": ["Du", "sagst", ",", "du", "liebst", "mich", ".", "Oh", ",", "ich", "dan\u00b7ke", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$.", "ITJ", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zwar kenn ich dieses Wort als L\u00fcge nur,", "tokens": ["Zwar", "kenn", "ich", "die\u00b7ses", "Wort", "als", "L\u00fc\u00b7ge", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch klingt es s\u00fc\u00df, wie liebliche Musik,", "tokens": ["Doch", "klingt", "es", "s\u00fc\u00df", ",", "wie", "lieb\u00b7li\u00b7che", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gerne glaubt man, was so lieblich klingt.", "tokens": ["Und", "ger\u00b7ne", "glaubt", "man", ",", "was", "so", "lieb\u00b7lich", "klingt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Nimm diesen Glauben als Entgegnung an.", "tokens": ["Nimm", "die\u00b7sen", "Glau\u00b7ben", "als", "Ent\u00b7geg\u00b7nung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mir selber will das Wort \u00bbIch liebe dich\u00ab", "tokens": ["Mir", "sel\u00b7ber", "will", "das", "Wort", "\u00bb", "Ich", "lie\u00b7be", "dich", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht mehr vom Herzen auf die Lippen gehn.", "tokens": ["Nicht", "mehr", "vom", "Her\u00b7zen", "auf", "die", "Lip\u00b7pen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dem Boden, der von mitleidlosem Fu\u00df", "tokens": ["Dem", "Bo\u00b7den", ",", "der", "von", "mit\u00b7leid\u00b7lo\u00b7sem", "Fu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zerstampft ward, dem der Bosheit d\u00fcrre Hand", "tokens": ["Zer\u00b7stampft", "ward", ",", "dem", "der", "Bos\u00b7heit", "d\u00fcr\u00b7re", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Salz in die Furchen streute, wollen Rosen nicht", "tokens": ["Salz", "in", "die", "Fur\u00b7chen", "streu\u00b7te", ",", "wol\u00b7len", "Ro\u00b7sen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,", "VMFIN", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Entbl\u00fchen, \u2013 blasse Nesseln bringt er nur.", "tokens": ["Ent\u00b7bl\u00fc\u00b7hen", ",", "\u2013", "blas\u00b7se", "Nes\u00b7seln", "bringt", "er", "nur", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So sieht mein Garten aus, \u2013 ein Nesselbeet.", "tokens": ["So", "sieht", "mein", "Gar\u00b7ten", "aus", ",", "\u2013", "ein", "Nes\u00b7sel\u00b7beet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Willst du ihn lieben? Wunder sind geschehn!", "tokens": ["Willst", "du", "ihn", "lie\u00b7ben", "?", "Wun\u00b7der", "sind", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Liebe ist die beste G\u00e4rtnerin.", "tokens": ["Die", "Lie\u00b7be", "ist", "die", "bes\u00b7te", "G\u00e4rt\u00b7ne\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}