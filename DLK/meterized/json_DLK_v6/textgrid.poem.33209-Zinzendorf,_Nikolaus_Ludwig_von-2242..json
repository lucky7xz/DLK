{"textgrid.poem.33209": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2242.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesus nahm zu an weisheit, alter und gnade bey Gott und den menschen.", "tokens": ["Je\u00b7sus", "nahm", "zu", "an", "weis\u00b7heit", ",", "al\u00b7ter", "und", "gna\u00b7de", "bey", "Gott", "und", "den", "men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "APPR", "NN", "$,", "ADJA", "KON", "VVFIN", "APPR", "NN", "KON", "ART", "NN", "$."], "meter": "--+--+-+--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Seine heilge j\u00fcnglingschaft sey der ledgen Ch\u00f6re cranz.", "tokens": ["Sei\u00b7ne", "heil\u00b7ge", "j\u00fcng\u00b7ling\u00b7schaft", "sey", "der", "led\u00b7gen", "Ch\u00f6\u00b7re", "cranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Das L\u00e4mmlein hat kein wort geredt \u2013 das amtsgeheimni\u00df volle glied, das alle m\u00e4nner tragen. ", "tokens": ["Das", "L\u00e4mm\u00b7lein", "hat", "kein", "wort", "ge\u00b7redt", "\u2013", "das", "amts\u00b7ge\u00b7heim\u00b7ni\u00df", "vol\u00b7le", "glied", ",", "das", "al\u00b7le", "m\u00e4n\u00b7ner", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$(", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-++--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}