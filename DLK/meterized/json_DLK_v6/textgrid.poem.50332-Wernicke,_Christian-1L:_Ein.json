{"textgrid.poem.50332": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.85", "pl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "So wie ", "tokens": ["So", "wie"], "token_info": ["word", "word"], "pos": ["ADV", "KOKOM"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Doch so dass ", "tokens": ["Doch", "so", "dass"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und dieser ", "tokens": ["Und", "die\u00b7ser"], "token_info": ["word", "word"], "pos": ["KON", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Es suchen beyde nach der Spur", "tokens": ["Es", "su\u00b7chen", "bey\u00b7de", "nach", "der", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Gl\u00fcckes an der Sonnen Uhr;", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "an", "der", "Son\u00b7nen", "Uhr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein, wenn dieses ", "tokens": ["Al\u00b7lein", ",", "wenn", "die\u00b7ses"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}}}}}