{"dta.poem.2785": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ewig Vaterland! Ach Wohnhau\u00df voller Freude!", "tokens": ["Ach", "e\u00b7wig", "Va\u00b7ter\u00b7land", "!", "Ach", "Wohn\u00b7hau\u00df", "vol\u00b7ler", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "NN", "$.", "ITJ", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sehn ich mich nach dir/ wie hoff ich Tag und Nacht/", "tokens": ["Wie", "sehn", "ich", "mich", "nach", "dir", "/", "wie", "hoff", "ich", "Tag", "und", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$(", "PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df mich ein seelig Tod frey von den Fesseln macht/", "tokens": ["Da\u00df", "mich", "ein", "see\u00b7lig", "Tod", "frey", "von", "den", "Fes\u00b7seln", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Darinn der sieche Leib mu\u00df unauffh\u00f6rlich leiden.", "tokens": ["Da\u00b7rinn", "der", "sie\u00b7che", "Leib", "mu\u00df", "un\u00b7auff\u00b7h\u00f6r\u00b7lich", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich brenne vor Begier aus dieser Welt zu scheiden/", "tokens": ["Ich", "bren\u00b7ne", "vor", "Be\u00b7gier", "aus", "die\u00b7ser", "Welt", "zu", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Nothstall aller Angst/ wo bleicher Kummer wacht/", "tokens": ["Dem", "Noth\u00b7stall", "al\u00b7ler", "Angst", "/", "wo", "blei\u00b7cher", "Kum\u00b7mer", "wacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "PWAV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und neue Tr\u00fcbsal bl\u00fcht/ und Laster-reiche Pracht", "tokens": ["Und", "neu\u00b7e", "Tr\u00fcb\u00b7sal", "bl\u00fcht", "/", "und", "Las\u00b7ter\u00b7rei\u00b7che", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Offt das Gewissen lockt/ des H\u00f6chsten Wort zu meiden.", "tokens": ["Offt", "das", "Ge\u00b7wis\u00b7sen", "lockt", "/", "des", "H\u00f6chs\u00b7ten", "Wort", "zu", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Auff Seele! Schwinge dich zu der gestirnten H\u00f6h!", "tokens": ["Auff", "See\u00b7le", "!", "Schwin\u00b7ge", "dich", "zu", "der", "ge\u00b7stirn\u00b7ten", "H\u00f6h", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du bist als Pilgerinn in Thr\u00e4nen hier gegangen/", "tokens": ["Du", "bist", "als", "Pil\u00b7ge\u00b7rinn", "in", "Thr\u00e4\u00b7nen", "hier", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun aber wirst du dort den Gnaden-Lohn empfangen/", "tokens": ["Nun", "a\u00b7ber", "wirst", "du", "dort", "den", "Gna\u00b7den\u00b7Lohn", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wahre Himmels-Lust folgt auf der Erden Weh.", "tokens": ["Und", "wah\u00b7re", "Him\u00b7mels\u00b7Lust", "folgt", "auf", "der", "Er\u00b7den", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "La\u00df/ was verwesen kan/ im Schos der Erden ligen/", "tokens": ["La\u00df", "/", "was", "ver\u00b7we\u00b7sen", "kan", "/", "im", "Schos", "der", "Er\u00b7den", "li\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "PWS", "VVINF", "VMFIN", "$(", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du must nach Adlers Art zur Lebens-Sonne fliegen.", "tokens": ["Du", "must", "nach", "Ad\u00b7lers", "Art", "zur", "Le\u00b7bens\u00b7Son\u00b7ne", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}