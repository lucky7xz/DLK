{"dta.poem.1210": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Jedwedes Thier das wohnt auff dieser weiten Erde/", "tokens": ["Jed\u00b7we\u00b7des", "Thier", "das", "wohnt", "auff", "die\u00b7ser", "wei\u00b7ten", "Er\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDS", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ha\u00df und fliehe denn/ gleich Eulen/ Licht und ", "tokens": ["Es", "ha\u00df", "und", "flie\u00b7he", "denn", "/", "gleich", "Eu\u00b7len", "/", "Licht", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$(", "ADV", "NN", "$(", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lebt/ wie man sieht/ allein in Arbeit bey dem ", "tokens": ["Lebt", "/", "wie", "man", "sieht", "/", "al\u00b7lein", "in", "Ar\u00b7beit", "bey", "dem"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "PWAV", "PIS", "VVFIN", "$(", "ADV", "APPR", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn aber sich das Haubt des Himmels kr\u00f6nt mit ", "tokens": ["Wenn", "a\u00b7ber", "sich", "das", "Haubt", "des", "Him\u00b7mels", "kr\u00f6nt", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Geht di\u00df dem Stalle zu/ und jenes nach dem ", "tokens": ["Geht", "di\u00df", "dem", "Stal\u00b7le", "zu", "/", "und", "je\u00b7nes", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "ART", "NN", "PTKZU", "$(", "KON", "PDS", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein jedes ruhet aus bi\u00df zu der ", "tokens": ["Ein", "je\u00b7des", "ru\u00b7het", "aus", "bi\u00df", "zu", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "VVFIN", "APPR", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich/ wenn sich sehen l\u00e4st der Glantz der ", "tokens": ["Ich", "/", "wenn", "sich", "se\u00b7hen", "l\u00e4st", "der", "Glantz", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "KOUS", "PRF", "VVINF", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die braune Finsternis zu jagen von der ", "tokens": ["Die", "brau\u00b7ne", "Fins\u00b7ter\u00b7nis", "zu", "ja\u00b7gen", "von", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel wilder denn ein Thier/ ein wildes Thier im ", "tokens": ["Viel", "wil\u00b7der", "denn", "ein", "Thier", "/", "ein", "wil\u00b7des", "Thier", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "KON", "ART", "NN", "$(", "ART", "ADJA", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begr\u00fcsse Traurens-voll mit Seufftzen Licht und ", "tokens": ["Be\u00b7gr\u00fcs\u00b7se", "Trau\u00b7rens\u00b7voll", "mit", "Seufft\u00b7zen", "Licht", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit einer herben Bach von Thr\u00e4nen Mond und ", "tokens": ["Mit", "ei\u00b7ner", "her\u00b7ben", "Bach", "von", "Thr\u00e4\u00b7nen", "Mond", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In h\u00f6chster Ungedult nach kaum verwichnem ", "tokens": ["In", "h\u00f6chs\u00b7ter", "Un\u00b7ge\u00b7dult", "nach", "kaum", "ver\u00b7wich\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn izt der Abendstern sagt ab dem hellen ", "tokens": ["Wenn", "izt", "der", "A\u00b7bends\u00b7tern", "sagt", "ab", "dem", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und unsre D\u00e4mmerung bringt andern ", "tokens": ["Und", "uns\u00b7re", "D\u00e4m\u00b7me\u00b7rung", "bringt", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So schrey ich kl\u00e4glich an die mir befeindten ", "tokens": ["So", "schrey", "ich", "kl\u00e4g\u00b7lich", "an", "die", "mir", "be\u00b7feind\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mich gemacht zum Spiel und Schauspiel aller ", "tokens": ["Die", "mich", "ge\u00b7macht", "zum", "Spiel", "und", "Schau\u00b7spiel", "al\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "APPRART", "NN", "KON", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Beklage meine Noth bey Himmel/ Lufft und ", "tokens": ["Be\u00b7kla\u00b7ge", "mei\u00b7ne", "Noth", "bey", "Him\u00b7mel", "/", "Lufft", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$(", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ich mehr elend bin denn iedes Thier im ", "tokens": ["Da\u00df", "ich", "mehr", "e\u00b7lend", "bin", "denn", "ie\u00b7des", "Thier", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "ADV", "PIAT", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kein grimmes Tiger-Thier/ kein frecher Lew im ", "tokens": ["Kein", "grim\u00b7mes", "Ti\u00b7ger\u00b7Thier", "/", "kein", "fre\u00b7cher", "Lew", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$(", "PIAT", "ADJA", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleicht der/ die mir geraubt die Freude meiner ", "tokens": ["Gleicht", "der", "/", "die", "mir", "ge\u00b7raubt", "die", "Freu\u00b7de", "mei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "$(", "PRELS", "PPER", "VVPP", "ART", "NN", "PPOSAT"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und dennoch sieht mich treu und ohne Falsch die ", "tokens": ["Und", "den\u00b7noch", "sieht", "mich", "treu", "und", "oh\u00b7ne", "Falsch", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "APPR", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets m\u00fcde/ nimmer satt von Leid die ", "tokens": ["Stets", "m\u00fc\u00b7de", "/", "nim\u00b7mer", "satt", "von", "Leid", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$(", "ADV", "ADJD", "APPR", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Zeichen/ da\u00df der Leib zwar ist von schwacher ", "tokens": ["Zum", "Zei\u00b7chen", "/", "da\u00df", "der", "Leib", "zwar", "ist", "von", "schwa\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "KOUS", "ART", "NN", "ADV", "VAFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch mein demantner Sinn sich gleicht dem Oel der ", "tokens": ["Doch", "mein", "de\u00b7mant\u00b7ner", "Sinn", "sich", "gleicht", "dem", "O\u00b7el", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach k\u00f6nt ich/ eh der Geist sich setzet bey den ", "tokens": ["Ach", "k\u00f6nt", "ich", "/", "eh", "der", "Geist", "sich", "set\u00b7zet", "bey", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "$(", "KOUS", "ART", "NN", "PRF", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh sich mein Schatten findt im Elyseer-", "tokens": ["Eh", "sich", "mein", "Schat\u00b7ten", "findt", "im", "E\u00b7ly\u00b7seer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "APPRART", "TRUNC"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Geschieden von der Last/ die werden soll zur ", "tokens": ["Ge\u00b7schie\u00b7den", "von", "der", "Last", "/", "die", "wer\u00b7den", "soll", "zur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$(", "ART", "VAINF", "VMFIN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gen\u00fcssen ihrer Gunst! die Zeit von einem ", "tokens": ["Ge\u00b7n\u00fcs\u00b7sen", "ih\u00b7rer", "Gunst", "!", "die", "Zeit", "von", "ei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bringt funffzig Wochen ein/ ein Blick der ", "tokens": ["Bringt", "funff\u00b7zig", "Wo\u00b7chen", "ein", "/", "ein", "Blick", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "CARD", "NN", "ART", "$(", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein s\u00fcsser Blick ist mir der Mittag heller ", "tokens": ["Ein", "s\u00fcs\u00b7ser", "Blick", "ist", "mir", "der", "Mit\u00b7tag", "hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der lichten Augen Paar l\u00e4st hinter sich die ", "tokens": ["Der", "lich\u00b7ten", "Au\u00b7gen", "Paar", "l\u00e4st", "hin\u00b7ter", "sich", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "APPR", "PRF", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sternen-Himmel prangt mit diesen Angel-", "tokens": ["Der", "Ster\u00b7nen\u00b7Him\u00b7mel", "prangt", "mit", "die\u00b7sen", "An\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Rosen-Wangen Zier besch\u00e4mt die ", "tokens": ["Der", "Ro\u00b7sen\u00b7Wan\u00b7gen", "Zier", "be\u00b7sch\u00e4mt", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der s\u00fcssen Stimme Schall die Nachtigall im ", "tokens": ["Der", "s\u00fcs\u00b7sen", "Stim\u00b7me", "Schall", "die", "Nach\u00b7ti\u00b7gall", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "ART", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer sch\u00e4zte nicht mit Ihr beseligt seine ", "tokens": ["Wer", "sch\u00e4z\u00b7te", "nicht", "mit", "Ihr", "be\u00b7se\u00b7ligt", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ach aber/ was verlangt der leichte Staub der ", "tokens": ["Ach", "a\u00b7ber", "/", "was", "ver\u00b7langt", "der", "leich\u00b7te", "Staub", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mich decket in der ", "tokens": ["Mich", "de\u00b7cket", "in", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Eh mir so s\u00fcssen ", "tokens": ["Eh", "mir", "so", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Eh mir die ", "tokens": ["Eh", "mir", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}}}}}