{"textgrid.poem.38523": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Engelges\u00e4nge t\u00f6nten durch die Wipfel,", "tokens": ["En\u00b7gel\u00b7ge\u00b7s\u00e4n\u00b7ge", "t\u00f6n\u00b7ten", "durch", "die", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Als den heiligen Erstlingsku\u00df der Liebe", "tokens": ["Als", "den", "hei\u00b7li\u00b7gen", "Erst\u00b7lings\u00b7ku\u00df", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "In die freudigzitternde Seele Maja's", "tokens": ["In", "die", "freu\u00b7dig\u00b7zit\u00b7tern\u00b7de", "See\u00b7le", "Ma\u00b7ja's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lippen mir gl\u00fchten!", "tokens": ["Lip\u00b7pen", "mir", "gl\u00fch\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Rosiger wallte da der Abend nieder,", "tokens": ["Ro\u00b7si\u00b7ger", "wall\u00b7te", "da", "der", "A\u00b7bend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "S\u00fcsser dufteten alle Gartenblumen,", "tokens": ["S\u00fcs\u00b7ser", "duf\u00b7te\u00b7ten", "al\u00b7le", "Gar\u00b7ten\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Auf den B\u00e4umen wirbelten Nachtigallen", "tokens": ["Auf", "den", "B\u00e4u\u00b7men", "wir\u00b7bel\u00b7ten", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lieder der Liebe!", "tokens": ["Lie\u00b7der", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Seliger Abend! den ich nie vergesse,", "tokens": ["Se\u00b7li\u00b7ger", "A\u00b7bend", "!", "den", "ich", "nie", "ver\u00b7ges\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Jede d\u00fcstere Stunde wird mir lichter,", "tokens": ["Je\u00b7de", "d\u00fcs\u00b7te\u00b7re", "Stun\u00b7de", "wird", "mir", "lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schwebt dein Bild, in Himmelsgestalt, um meine", "tokens": ["Schwebt", "dein", "Bild", ",", "in", "Him\u00b7mels\u00b7ge\u00b7stalt", ",", "um", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "KOUI", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Traurende Seele!", "tokens": ["Trau\u00b7ren\u00b7de", "See\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}