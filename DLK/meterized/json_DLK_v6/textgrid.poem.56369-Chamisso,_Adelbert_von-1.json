{"textgrid.poem.56369": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Salas y Gomez raget aus den Fluten", "tokens": ["Sa\u00b7las", "y", "Go\u00b7mez", "ra\u00b7get", "aus", "den", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des stillen Meers, ein Felsen kahl und blo\u00df,", "tokens": ["Des", "stil\u00b7len", "Meers", ",", "ein", "Fel\u00b7sen", "kahl", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verbrannt von scheitelrechter Sonne Gluten,", "tokens": ["Ver\u00b7brannt", "von", "schei\u00b7tel\u00b7rech\u00b7ter", "Son\u00b7ne", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Steingestell ohn alles Gras und Moos,", "tokens": ["Ein", "Stein\u00b7ge\u00b7stell", "ohn", "al\u00b7les", "Gras", "und", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das sich das Volk der V\u00f6gel auserkor", "tokens": ["Das", "sich", "das", "Volk", "der", "V\u00f6\u00b7gel", "au\u00b7ser\u00b7kor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ART", "NN", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zur Ruhstatt im bewegten Meeresscho\u00df.", "tokens": ["Zur", "Ruh\u00b7statt", "im", "be\u00b7weg\u00b7ten", "Mee\u00b7res\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So stieg vor unsern Blicken sie empor,", "tokens": ["So", "stieg", "vor", "un\u00b7sern", "Bli\u00b7cken", "sie", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als auf dem \u00bbRurik\u00ab: \u00bbLand im Westen! Land!\u00ab", "tokens": ["Als", "auf", "dem", "\u00bb", "Ru\u00b7rik", "\u00ab", ":", "\u00bb", "Land", "im", "Wes\u00b7ten", "!", "Land", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "ART", "$(", "NN", "$(", "$.", "$(", "NN", "APPRART", "NN", "$.", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Ruf vom Mastkorb drang zu unserm Ohr.", "tokens": ["Der", "Ruf", "vom", "Mast\u00b7korb", "drang", "zu", "un\u00b7serm", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als uns die Klippe nah vor Augen stand,", "tokens": ["Als", "uns", "die", "Klip\u00b7pe", "nah", "vor", "Au\u00b7gen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Gewahrten wir der Meeresv\u00f6gel Scharen", "tokens": ["Ge\u00b7wahr\u00b7ten", "wir", "der", "Mee\u00b7res\u00b7v\u00f6\u00b7gel", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und ihre Br\u00fctepl\u00e4tze l\u00e4ngs dem Strand.", "tokens": ["Und", "ih\u00b7re", "Br\u00fc\u00b7te\u00b7pl\u00e4t\u00b7ze", "l\u00e4ngs", "dem", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da frischer Nahrung wir bed\u00fcrftig waren,", "tokens": ["Da", "fri\u00b7scher", "Nah\u00b7rung", "wir", "be\u00b7d\u00fcrf\u00b7tig", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So ward beschlossen den Versuch zu wagen,", "tokens": ["So", "ward", "be\u00b7schlos\u00b7sen", "den", "Ver\u00b7such", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "In zweien Booten an das Land zu fahren.", "tokens": ["In", "zwei\u00b7en", "Boo\u00b7ten", "an", "das", "Land", "zu", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Es ward dabei zu sein mir angetragen.", "tokens": ["Es", "ward", "da\u00b7bei", "zu", "sein", "mir", "an\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "PTKZU", "VAINF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Das Schrecknis, das der Ort mir offenbart,", "tokens": ["Das", "Schreck\u00b7nis", ",", "das", "der", "Ort", "mir", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ich werd es jetzt mit schlichten Worten sagen.", "tokens": ["Ich", "werd", "es", "jetzt", "mit", "schlich\u00b7ten", "Wor\u00b7ten", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wir legten bei, bestiegen wohlbewahrt", "tokens": ["Wir", "leg\u00b7ten", "bei", ",", "be\u00b7stie\u00b7gen", "wohl\u00b7be\u00b7wahrt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die ausgesetzten Boote, stie\u00dfen ab,", "tokens": ["Die", "aus\u00b7ge\u00b7setz\u00b7ten", "Boo\u00b7te", ",", "stie\u00b7\u00dfen", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und l\u00e4ngs der Brandung rudernd ging die Fahrt.", "tokens": ["Und", "l\u00e4ngs", "der", "Bran\u00b7dung", "ru\u00b7dernd", "ging", "die", "Fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wo unterm Wind das Ufer Schutz uns gab,", "tokens": ["Wo", "un\u00b7term", "Wind", "das", "U\u00b7fer", "Schutz", "uns", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ward angelegt bei einer Felsengruppe,", "tokens": ["Ward", "an\u00b7ge\u00b7legt", "bei", "ei\u00b7ner", "Fel\u00b7sen\u00b7grup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wir setzten auf das Trockne unsern Stab.", "tokens": ["Wir", "setz\u00b7ten", "auf", "das", "Trock\u00b7ne", "un\u00b7sern", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und eine rechts, und links die andre Truppe,", "tokens": ["Und", "ei\u00b7ne", "rechts", ",", "und", "links", "die", "and\u00b7re", "Trup\u00b7pe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Verteilten sich den Strand entlang die Mannen,", "tokens": ["Ver\u00b7teil\u00b7ten", "sich", "den", "Strand", "ent\u00b7lang", "die", "Man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ich aber stieg hinan die Felsenkuppe.", "tokens": ["Ich", "a\u00b7ber", "stieg", "hi\u00b7nan", "die", "Fel\u00b7sen\u00b7kup\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Vor meinen F\u00fc\u00dfen wichen kaum von dannen", "tokens": ["Vor", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "wi\u00b7chen", "kaum", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Die V\u00f6gel, welche die Gefahr nicht kannten,", "tokens": ["Die", "V\u00f6\u00b7gel", ",", "wel\u00b7che", "die", "Ge\u00b7fahr", "nicht", "kann\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und mit gestreckten H\u00e4lsen sich besannen.", "tokens": ["Und", "mit", "ge\u00b7streck\u00b7ten", "H\u00e4l\u00b7sen", "sich", "be\u00b7san\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Der Gipfel war erreicht, die Sohlen brannten", "tokens": ["Der", "Gip\u00b7fel", "war", "er\u00b7reicht", ",", "die", "Soh\u00b7len", "brann\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Mir auf dem hei\u00dfen Schieferstein, indessen", "tokens": ["Mir", "auf", "dem", "hei\u00b7\u00dfen", "Schie\u00b7fers\u00b7tein", ",", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Die Blicke den Gesichtskreis rings umspannten.", "tokens": ["Die", "Bli\u00b7cke", "den", "Ge\u00b7sichts\u00b7kreis", "rings", "um\u00b7spann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und wie die W\u00fcstenei sie erst ermessen,", "tokens": ["Und", "wie", "die", "W\u00fcs\u00b7te\u00b7nei", "sie", "erst", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und wieder erdw\u00e4rts sich gesenket haben,", "tokens": ["Und", "wie\u00b7der", "erd\u00b7w\u00e4rts", "sich", "ge\u00b7sen\u00b7ket", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "L\u00e4\u00dft eines alles andre mich vergessen.", "tokens": ["L\u00e4\u00dft", "ei\u00b7nes", "al\u00b7les", "and\u00b7re", "mich", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Es hat die Hand des Menschen eingegraben", "tokens": ["Es", "hat", "die", "Hand", "des", "Men\u00b7schen", "ein\u00b7ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Das Siegel seines Geistes in den Stein,", "tokens": ["Das", "Sie\u00b7gel", "sei\u00b7nes", "Geis\u00b7tes", "in", "den", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Worauf ich steh, \u2013 Schriftzeichen sind's, Buchstaben.", "tokens": ["Wo\u00b7rauf", "ich", "steh", ",", "\u2013", "Schrift\u00b7zei\u00b7chen", "sin\u00b7d's", ",", "Buch\u00b7sta\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$,", "$(", "NN", "VAFIN", "$,", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Der Kreuze f\u00fcnfmal zehn in gleichen Reihn,", "tokens": ["Der", "Kreu\u00b7ze", "f\u00fcnf\u00b7mal", "zehn", "in", "glei\u00b7chen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "CARD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Es will mich d\u00fcnken, da\u00df sie lang bestehen,", "tokens": ["Es", "will", "mich", "d\u00fcn\u00b7ken", ",", "da\u00df", "sie", "lang", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Doch mu\u00df die fl\u00fccht'ge Schrift hier j\u00fcnger sein.", "tokens": ["Doch", "mu\u00df", "die", "fl\u00fccht'\u00b7ge", "Schrift", "hier", "j\u00fcn\u00b7ger", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und nicht zu lesen! \u2013 deutlich noch zu sehen", "tokens": ["Und", "nicht", "zu", "le\u00b7sen", "!", "\u2013", "deut\u00b7lich", "noch", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "$.", "$(", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Der Tritte Spur, die sie verl\u00f6schet fast;", "tokens": ["Der", "Trit\u00b7te", "Spur", ",", "die", "sie", "ver\u00b7l\u00f6\u00b7schet", "fast", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Es scheint ein Pfad dar\u00fcber hin zu gehen.", "tokens": ["Es", "scheint", "ein", "Pfad", "da\u00b7r\u00fc\u00b7ber", "hin", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Und dort am Abhang war ein Ort der Rast,", "tokens": ["Und", "dort", "am", "Ab\u00b7hang", "war", "ein", "Ort", "der", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Dort nahm er Nahrung ein, dort Eierschalen!", "tokens": ["Dort", "nahm", "er", "Nah\u00b7rung", "ein", ",", "dort", "Ei\u00b7er\u00b7scha\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "W