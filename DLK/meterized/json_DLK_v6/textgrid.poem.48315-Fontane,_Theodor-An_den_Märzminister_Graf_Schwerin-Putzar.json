{"textgrid.poem.48315": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An den M\u00e4rzminister Graf Schwerin-Putzar", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Ahnherr \u2013 mit dem Schwerte,", "tokens": ["Dein", "Ahn\u00b7herr", "\u2013", "mit", "dem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "APPR", "ART", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du selber \u2013 mit dem Wort!", "tokens": ["Du", "sel\u00b7ber", "\u2013", "mit", "dem", "Wort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lebt das Ruhmeswerte", "tokens": ["So", "lebt", "das", "Ruh\u00b7mes\u00b7wer\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis auf den Enkel fort.", "tokens": ["Bis", "auf", "den", "En\u00b7kel", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was einst in letzter Stunde", "tokens": ["Was", "einst", "in", "letz\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der greise Feldmarschall sprach,", "tokens": ["Der", "grei\u00b7se", "Feld\u00b7mar\u00b7schall", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Aufs neu aus deinem Munde", "tokens": ["Aufs", "neu", "aus", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erklang es uns: \u00bb", "tokens": ["Er\u00b7klang", "es", "uns", ":", "\u00bb"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du stehst, in Lieb' und Treue,", "tokens": ["Du", "stehst", ",", "in", "Lieb'", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Thron und Herrscherhaus,", "tokens": ["Zu", "Thron", "und", "Herr\u00b7scher\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und baust doch, f\u00fcr das Neue,", "tokens": ["Und", "baust", "doch", ",", "f\u00fcr", "das", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alten Pfeiler aus.", "tokens": ["Die", "al\u00b7ten", "Pfei\u00b7ler", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht tr\u00e4gst du der Verneinung", "tokens": ["Nicht", "tr\u00e4gst", "du", "der", "Ver\u00b7nei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Kampfe die Fahne vor,", "tokens": ["Im", "Kamp\u00b7fe", "die", "Fah\u00b7ne", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch f\u00fcr die freie ", "tokens": ["Doch", "f\u00fcr", "die", "frei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Schwingst du sie hoch empor.", "tokens": ["Schwingst", "du", "sie", "hoch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du bist von jenen ", "tokens": ["Du", "bist", "von", "je\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im Geiste noch gezeugt,", "tokens": ["Im", "Geis\u00b7te", "noch", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die keinem Stirnefalten", "tokens": ["Die", "kei\u00b7nem", "Stir\u00b7ne\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jemalen sich gebeugt.", "tokens": ["Je\u00b7ma\u00b7len", "sich", "ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du sprichst noch, wie der ", "tokens": ["Du", "sprichst", "noch", ",", "wie", "der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sonst wohl bei Hofe sprach,", "tokens": ["Sonst", "wohl", "bei", "Ho\u00b7fe", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was dem die Schranzen rieten,", "tokens": ["Was", "dem", "die", "Schran\u00b7zen", "rie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er fragte nichts danach.", "tokens": ["Er", "frag\u00b7te", "nichts", "da\u00b7nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Zu Tafel sa\u00df er gern,", "tokens": ["Zu", "Ta\u00b7fel", "sa\u00df", "er", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einst aber andres D\u00fcrsten", "tokens": ["Einst", "a\u00b7ber", "and\u00b7res", "D\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trieb ihn zum Tisch des Herrn;", "tokens": ["Trieb", "ihn", "zum", "Tisch", "des", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Erst als er da genossen", "tokens": ["Erst", "als", "er", "da", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Christi heil'gem Mahl,", "tokens": ["Von", "Chris\u00b7ti", "heil'\u00b7gem", "Mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ernst noch und abgeschlossen", "tokens": ["Ernst", "noch", "und", "ab\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Trat er in Schlo\u00df und Saal.", "tokens": ["Trat", "er", "in", "Schlo\u00df", "und", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig sieht den Degen", "tokens": ["Der", "K\u00f6\u00b7nig", "sieht", "den", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wie so fromm er schaut;", "tokens": ["Und", "wie", "so", "fromm", "er", "schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ruft er ihm entgegen:", "tokens": ["Da", "ruft", "er", "ihm", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbhe, ", "tokens": ["\u00bb", "he", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "FM", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Der h\u00f6rt es; unter Blitzen", "tokens": ["Der", "h\u00f6rt", "es", ";", "un\u00b7ter", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blickt er den K\u00f6nig an,", "tokens": ["Blickt", "er", "den", "K\u00f6\u00b7nig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Da\u00df selbst das Aug' des ", "tokens": ["Da\u00df", "selbst", "das", "Aug'", "des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Nicht Stich ihm halten kann.", "tokens": ["Nicht", "Stich", "ihm", "hal\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann laut: \u00bbF\u00fcr Euch in N\u00e4chten", "tokens": ["Dann", "laut", ":", "\u00bb", "F\u00fcr", "Euch", "in", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "$(", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geblutet hab' ich gern,", "tokens": ["Ge\u00b7blu\u00b7tet", "hab'", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun will ich auch mal fechten", "tokens": ["Nun", "will", "ich", "auch", "mal", "fech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Christum, meinen Herrn!\u00ab", "tokens": ["F\u00fcr", "Chris\u00b7tum", ",", "mei\u00b7nen", "Herrn", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl stutzet da und staunet", "tokens": ["Wohl", "stut\u00b7zet", "da", "und", "stau\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das h\u00f6fische Geschlecht,", "tokens": ["Das", "h\u00f6\u00b7fi\u00b7sche", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der K\u00f6nig aber raunet:", "tokens": ["Der", "K\u00f6\u00b7nig", "a\u00b7ber", "rau\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00bbstill, ", "tokens": ["\u00bb", "still", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "PTKVZ", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "So war's und \u2013 ist's geblieben", "tokens": ["So", "wa\u00b7r's", "und", "\u2013", "ist's", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "KON", "$(", "VAFIN", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Durch ein Jahrhundert fort:", "tokens": ["Durch", "ein", "Jahr\u00b7hun\u00b7dert", "fort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Hohenzollern lieben", "tokens": ["Die", "Ho\u00b7hen\u00b7zol\u00b7lern", "lie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein freies Manneswort.", "tokens": ["Ein", "frei\u00b7es", "Man\u00b7nes\u00b7wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Ficht weiter, sonder Scheu:", "tokens": ["Ficht", "wei\u00b7ter", ",", "son\u00b7der", "Scheu", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}