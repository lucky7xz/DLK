{"textgrid.poem.67758": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gelegenheit!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelegenheit!", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du Mutter jeder Albernheit!", "tokens": ["Du", "Mut\u00b7ter", "je\u00b7der", "Al\u00b7bern\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie viele Weisen wurden Thoren,", "tokens": ["Wie", "vie\u00b7le", "Wei\u00b7sen", "wur\u00b7den", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viele Plane schnell geboren", "tokens": ["Wie", "vie\u00b7le", "Pla\u00b7ne", "schnell", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und schneller noch durch Dich zerstreut!", "tokens": ["Und", "schnel\u00b7ler", "noch", "durch", "Dich", "zer\u00b7streut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Tugend, die wir ausge\u00fcbet,", "tokens": ["Die", "Tu\u00b7gend", ",", "die", "wir", "aus\u00b7ge\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Fehler, den wir oft geliebet,", "tokens": ["Der", "Feh\u00b7ler", ",", "den", "wir", "oft", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist unser? Nein, ist Dein, o Zeit,", "tokens": ["Ist", "un\u00b7ser", "?", "Nein", ",", "ist", "Dein", ",", "o", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "$.", "PTKANT", "$,", "VAFIN", "PPOSAT", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gelegenheit!", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Gelegenheit!", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dir dank' ich meine Trunkenheit!", "tokens": ["Dir", "dank'", "ich", "mei\u00b7ne", "Trun\u00b7ken\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Wein, wie hatt' ich ihn verschworen!", "tokens": ["Den", "Wein", ",", "wie", "hatt'", "ich", "ihn", "ver\u00b7schwo\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie leicht hab' ich den Schwur verloren,", "tokens": ["Wie", "leicht", "hab'", "ich", "den", "Schwur", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ohn' da\u00df ein Tyrann gedr\u00e4ut!", "tokens": ["Und", "ohn'", "da\u00df", "ein", "Ty\u00b7rann", "ge\u00b7dr\u00e4ut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sah den Wein im Glase blinken,", "tokens": ["Ich", "sah", "den", "Wein", "im", "Gla\u00b7se", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und immer wuchs der Durst im Trinken;", "tokens": ["Und", "im\u00b7mer", "wuchs", "der", "Durst", "im", "Trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich trank, ward trunken und erfreut;", "tokens": ["Ich", "trank", ",", "ward", "trun\u00b7ken", "und", "er\u00b7freut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gelegenheit!", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}