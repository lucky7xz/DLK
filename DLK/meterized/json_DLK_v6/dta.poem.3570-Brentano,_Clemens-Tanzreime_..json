{"dta.poem.3570": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00e4tzle ist h\u00fcbsch,               ", "tokens": ["Mein", "Sch\u00e4tz\u00b7le", "ist", "h\u00fcbsch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Aber reich ist es nit;", "tokens": ["A\u00b7ber", "reich", "ist", "es", "nit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Was n\u00fctzt mir der Reichthum,", "tokens": ["Was", "n\u00fctzt", "mir", "der", "Reicht\u00b7hum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das Geld k\u00fc\u00df ich nit.", "tokens": ["Das", "Geld", "k\u00fc\u00df", "ich", "nit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n bin ich nit, reich bin ich wohl,", "tokens": ["Sch\u00f6n", "bin", "ich", "nit", ",", "reich", "bin", "ich", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "$,", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geld Hab ich auch a ganz Beuterl voll;", "tokens": ["Geld", "Hab", "ich", "auch", "a", "ganz", "Beu\u00b7terl", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "ADV", "NE", "ADV", "NE", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gehn mer noch drey Batze ab,", "tokens": ["Gehn", "mer", "noch", "drey", "Bat\u00b7ze", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich grad zw\u00f6lf Kreutzer hab.", "tokens": ["Da\u00df", "ich", "grad", "zw\u00f6lf", "Kreut\u00b7zer", "hab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "CARD", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "'s Kranzerle weg,", "tokens": ["'s", "Kran\u00b7zer\u00b7le", "weg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und 's H\u00e4uberle her;", "tokens": ["Und", "'s", "H\u00e4u\u00b7ber\u00b7le", "her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Jungfrau gewest,", "tokens": ["Jung\u00b7frau", "ge\u00b7west", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und nimmermehr.", "tokens": ["Und", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}