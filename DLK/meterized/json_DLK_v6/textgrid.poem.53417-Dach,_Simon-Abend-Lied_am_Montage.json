{"textgrid.poem.53417": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Lied am Montage", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich heut von dir gebeten,", "tokens": ["Was", "ich", "heut", "von", "dir", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesu, hast du mir gewehrt,", "tokens": ["Je\u00b7su", ",", "hast", "du", "mir", "ge\u00b7wehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist nicht fern von mir getreten,", "tokens": ["Bist", "nicht", "fern", "von", "mir", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df kein Unfall mich gef\u00e4hrt,", "tokens": ["Da\u00df", "kein", "Un\u00b7fall", "mich", "ge\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hab ich mich auch vorgesehn,", "tokens": ["Hab", "ich", "mich", "auch", "vor\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df kein Fehltritt ist geschehn?", "tokens": ["Da\u00df", "kein", "Fehl\u00b7tritt", "ist", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein Herr Jesu, hab Erbarmen,", "tokens": ["Mein", "Herr", "Je\u00b7su", ",", "hab", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hilff aus aller Noht mir Armen.", "tokens": ["Hilff", "aus", "al\u00b7ler", "Noht", "mir", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist \u00fcberall verbrochen,", "tokens": ["Es", "ist", "\u00fc\u00b7be\u00b7rall", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber deiner Wunden Zahl,", "tokens": ["A\u00b7ber", "dei\u00b7ner", "Wun\u00b7den", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als du wurdest durchgestochen,", "tokens": ["Als", "du", "wur\u00b7dest", "durch\u00b7ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tilgt die S\u00fcnden allzumahl,", "tokens": ["Tilgt", "die", "S\u00fcn\u00b7den", "all\u00b7zu\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wasch auch von den meinen mich:", "tokens": ["Wasch", "auch", "von", "den", "mei\u00b7nen", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein Gebein erquicke sich,", "tokens": ["Mein", "Ge\u00b7bein", "er\u00b7qui\u00b7cke", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welches du Herr durch die Plagen", "tokens": ["Wel\u00b7ches", "du", "Herr", "durch", "die", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Wahrer Busse sehr zerschlagen.", "tokens": ["Wah\u00b7rer", "Bus\u00b7se", "sehr", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und weil jetzt die Nacht vorhanden,", "tokens": ["Und", "weil", "jetzt", "die", "Nacht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Nimm mich Herr in deine Ruh,", "tokens": ["Nimm", "mich", "Herr", "in", "dei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halt die H\u00f6ll in ihren Banden,", "tokens": ["Halt", "die", "H\u00f6ll", "in", "ih\u00b7ren", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie mir nicht Schaden thu,", "tokens": ["Da\u00df", "sie", "mir", "nicht", "Scha\u00b7den", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df von mir seyn abgewand", "tokens": ["La\u00df", "von", "mir", "seyn", "ab\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wassers-Noth, Raub, Mord und Brand,", "tokens": ["Was\u00b7ser\u00b7sNoth", ",", "Raub", ",", "Mord", "und", "Brand", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und den b\u00f6sen Tod f\u00fcr allen", "tokens": ["Und", "den", "b\u00f6\u00b7sen", "Tod", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich nicht pl\u00f6tzlich \u00fcberfallen.", "tokens": ["Mich", "nicht", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7berf\u00b7al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darumb ich zu dieser Stunden,", "tokens": ["Da\u00b7rumb", "ich", "zu", "die\u00b7ser", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meinen Leib zwar in die Still,", "tokens": ["Mei\u00b7nen", "Leib", "zwar", "in", "die", "Still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber mehr in deine Wunden", "tokens": ["A\u00b7ber", "mehr", "in", "dei\u00b7ne", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Seele legen wil,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "le\u00b7gen", "wil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sich birgt ein V\u00f6gelein,", "tokens": ["Wie", "sich", "birgt", "ein", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wettert es, in hole Stein,", "tokens": ["Wet\u00b7tert", "es", ",", "in", "ho\u00b7le", "Stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hierau\u00df sol kein Todt mich heben,", "tokens": ["Hier\u00b7au\u00df", "sol", "kein", "Todt", "mich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn du bist und bleibst mein Leben.", "tokens": ["Denn", "du", "bist", "und", "bleibst", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}