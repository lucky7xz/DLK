{"dta.poem.18878": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeines .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Jesus, der die Welt durchwandert,", "tokens": ["Herr", "Je\u00b7sus", ",", "der", "die", "Welt", "durch\u00b7wan\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ging einst an einem Markt vorbey,", "tokens": ["Ging", "einst", "an", "ei\u00b7nem", "Markt", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein todter Hund lag auf dem Wege,", "tokens": ["Ein", "tod\u00b7ter", "Hund", "lag", "auf", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschleppet vor des Hauses Thor,", "tokens": ["Ge\u00b7schlep\u00b7pet", "vor", "des", "Hau\u00b7ses", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Haufe stand ums Aas umher", "tokens": ["Ein", "Hau\u00b7fe", "stand", "ums", "Aas", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Geyer sich um Aeser sammlen.", "tokens": ["Wie", "Ge\u00b7yer", "sich", "um", "A\u00b7e\u00b7ser", "samm\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Der Eine sprach: mir wird das Hirn", "tokens": ["Der", "Ei\u00b7ne", "sprach", ":", "mir", "wird", "das", "Hirn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dem Gestank ganz ausgel\u00f6scht.", "tokens": ["Von", "dem", "Ge\u00b7stank", "ganz", "aus\u00b7ge\u00b7l\u00f6scht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Andere sprach: was braucht es viel", "tokens": ["Der", "An\u00b7de\u00b7re", "sprach", ":", "was", "braucht", "es", "viel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der Gr\u00e4ber Auswurf bringt nur Ungl\u00fcck.", "tokens": ["Der", "Gr\u00e4\u00b7ber", "Aus\u00b7wurf", "bringt", "nur", "Un\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So sang ein Jeder seine Weise,", "tokens": ["So", "sang", "ein", "Je\u00b7der", "sei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Des todten Hundes Leib zu schm\u00e4hen.", "tokens": ["Des", "tod\u00b7ten", "Hun\u00b7des", "Leib", "zu", "schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Als nun an Jesus kam die Reih\u2019,", "tokens": ["Als", "nun", "an", "Je\u00b7sus", "kam", "die", "Reih'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sprach, ohne Schm\u00e4h\u2019n, er guten Sinns,", "tokens": ["Sprach", ",", "oh\u00b7ne", "Schm\u00e4h'n", ",", "er", "gu\u00b7ten", "Sinns", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er sprach aus g\u00fctiger Natur:", "tokens": ["Er", "sprach", "aus", "g\u00fc\u00b7ti\u00b7ger", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Z\u00e4hne sind wie Perlen weiss.", "tokens": ["Die", "Z\u00e4h\u00b7ne", "sind", "wie", "Per\u00b7len", "weiss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dies Wort macht den Umstehenden,", "tokens": ["Dies", "Wort", "macht", "den", "Um\u00b7ste\u00b7hen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.18": {"text": "Durchgl\u00fchten Muscheln \u00e4hnlich, heiss.", "tokens": ["Durch\u00b7gl\u00fch\u00b7ten", "Mu\u00b7scheln", "\u00e4hn\u00b7lich", ",", "heiss", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}