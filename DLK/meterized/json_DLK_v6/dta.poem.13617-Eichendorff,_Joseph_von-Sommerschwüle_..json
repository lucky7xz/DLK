{"dta.poem.13617": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sommerschw\u00fcle .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall schweigt, sie hat ihr Nest gefunden", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "schweigt", ",", "sie", "hat", "ihr", "Nest", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tr\u00e4g' ziehn die Quellen, die so k\u00fchle sprangen,", "tokens": ["Tr\u00e4g'", "ziehn", "die", "Quel\u00b7len", ",", "die", "so", "k\u00fch\u00b7le", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJA", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Von tr\u00fcber Schw\u00fcle liegt die Welt umfangen,", "tokens": ["Von", "tr\u00fc\u00b7ber", "Schw\u00fc\u00b7le", "liegt", "die", "Welt", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So hat den Lenz der Sommer \u00fcberwunden.", "tokens": ["So", "hat", "den", "Lenz", "der", "Som\u00b7mer", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}