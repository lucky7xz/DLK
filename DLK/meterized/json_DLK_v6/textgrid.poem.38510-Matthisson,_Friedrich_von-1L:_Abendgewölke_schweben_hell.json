{"textgrid.poem.38510": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Abendgew\u00f6lke schweben hell", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abendgew\u00f6lke schweben hell", "tokens": ["A\u00b7bend\u00b7ge\u00b7w\u00f6l\u00b7ke", "schwe\u00b7ben", "hell"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Am bepurpurten Himmel;", "tokens": ["Am", "be\u00b7pur\u00b7pur\u00b7ten", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Hesperus schaut, mit Liebesblik,", "tokens": ["Hes\u00b7pe\u00b7rus", "schaut", ",", "mit", "Lie\u00b7bes\u00b7blik", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch den bl\u00fchenden Lindenhain,", "tokens": ["Durch", "den", "bl\u00fc\u00b7hen\u00b7den", "Lin\u00b7den\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Und ihr schmelzendes Trauerlied", "tokens": ["Und", "ihr", "schmel\u00b7zen\u00b7des", "Trau\u00b7er\u00b7lied"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zirpt im Kraute die Grille!", "tokens": ["Zirpt", "im", "Krau\u00b7te", "die", "Gril\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Freuden der Liebe harren dein!", "tokens": ["Freu\u00b7den", "der", "Lie\u00b7be", "har\u00b7ren", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Fl\u00fcstern leise die Winde;", "tokens": ["Fl\u00fcs\u00b7tern", "lei\u00b7se", "die", "Win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Freuden der Liebe harren dein!", "tokens": ["Freu\u00b7den", "der", "Lie\u00b7be", "har\u00b7ren", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "T\u00f6nt die Kehle der Nachtigall,", "tokens": ["T\u00f6nt", "die", "Keh\u00b7le", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Hoch vom Sternengew\u00f6lb' herab", "tokens": ["Hoch", "vom", "Ster\u00b7nen\u00b7ge\u00b7w\u00f6lb'", "her\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Schallt mir Stimme der Liebe!", "tokens": ["Schallt", "mir", "Stim\u00b7me", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Himmel! aus jenem Schattengang", "tokens": ["Him\u00b7mel", "!", "aus", "je\u00b7nem", "Schat\u00b7ten\u00b7gang"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PDAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wandelt Maja die Fromme!", "tokens": ["Wan\u00b7delt", "Ma\u00b7ja", "die", "From\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Heftet den Engelblik auf mich,", "tokens": ["Hef\u00b7tet", "den", "En\u00b7gel\u00b7blik", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Fleugt dem seligen J\u00fcngling zu!", "tokens": ["Fleugt", "dem", "se\u00b7li\u00b7gen", "J\u00fcng\u00b7ling", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Heil mir! da\u00df du auch ihr get\u00f6nt,", "tokens": ["Heil", "mir", "!", "da\u00df", "du", "auch", "ihr", "ge\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "KOUS", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "S\u00fcsse Stimme der Liebe!", "tokens": ["S\u00fcs\u00b7se", "Stim\u00b7me", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}