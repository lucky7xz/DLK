{"textgrid.poem.55360": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Nett und niedlich", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du das M\u00e4dchen gesehn", "tokens": ["Hast", "du", "das", "M\u00e4d\u00b7chen", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Fl\u00fcchtig vor\u00fcbergehn?", "tokens": ["Fl\u00fcch\u00b7tig", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wollt, sie w\u00e4r meine Braut!", "tokens": ["Wollt", ",", "sie", "w\u00e4r", "mei\u00b7ne", "Braut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Jawohl! die Blonde, die Falbe!", "tokens": ["Ja\u00b7wohl", "!", "die", "Blon\u00b7de", ",", "die", "Fal\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie fitticht so zierlich wie die Schwalbe,", "tokens": ["Sie", "fit\u00b7ticht", "so", "zier\u00b7lich", "wie", "die", "Schwal\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die ihr Nest baut.", "tokens": ["Die", "ihr", "Nest", "baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Du bist mein und bist so zierlich,", "tokens": ["Du", "bist", "mein", "und", "bist", "so", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist mein und so manierlich,", "tokens": ["Du", "bist", "mein", "und", "so", "ma\u00b7nier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber etwas fehlt dir noch:", "tokens": ["A\u00b7ber", "et\u00b7was", "fehlt", "dir", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcssest mit so spitzen Lippen,", "tokens": ["K\u00fcs\u00b7sest", "mit", "so", "spit\u00b7zen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Tauben Wasser nippen;", "tokens": ["Wie", "die", "Tau\u00b7ben", "Was\u00b7ser", "nip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allzu zierlich bist du doch.", "tokens": ["All\u00b7zu", "zier\u00b7lich", "bist", "du", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}