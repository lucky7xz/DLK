{"textgrid.poem.52288": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "9. Adj\u00fcs, Herr Leutnant", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Ludwigslust stunn bi de Granedier", "tokens": ["In", "Lud\u00b7wigs\u00b7lust", "stunn", "bi", "de", "Gra\u00b7ne\u00b7dier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einmal en Leutnant, Herr von Fink.", "tokens": ["Ein\u00b7mal", "en", "Leut\u00b7nant", ",", "Herr", "von", "Fink", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dat was ein wohres Kr\u00e4tending,", "tokens": ["Dat", "was", "ein", "woh\u00b7res", "Kr\u00e4\u00b7ten\u00b7ding", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Obglik de Kirl man keshoch wir.", "tokens": ["Ob\u00b7glik", "de", "Kirl", "man", "kes\u00b7hoch", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Na, de let mal Rekruten inexieren", "tokens": ["Na", ",", "de", "let", "mal", "Rek\u00b7ru\u00b7ten", "i\u00b7ne\u00b7xie\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "VVFIN", "ADV", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Un let sei rechtsch un linksch marschieren.", "tokens": ["Un", "let", "sei", "rechtsch", "un", "link\u00b7sch", "mar\u00b7schie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "FM", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dat Ding sprung allentwegen 'r\u00fcmmer", "tokens": ["Dat", "Ding", "sprung", "al\u00b7lent\u00b7we\u00b7gen", "'r\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Un schreg un kummandierte \u00fcmmer", "tokens": ["Un", "schreg", "un", "kum\u00b7man\u00b7dier\u00b7te", "\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "VVFIN", "ADV"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Un makt dorbi so'n dullen Larm", "tokens": ["Un", "makt", "dor\u00b7bi", "so'n", "dul\u00b7len", "Larm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Un smet un fuchtelt mit de Arm,", "tokens": ["Un", "smet", "un", "fuch\u00b7telt", "mit", "de", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Ja, liksterwelt grad as so'n Hampelmann,", "tokens": ["Ja", ",", "liks\u00b7ter\u00b7welt", "grad", "as", "so'n", "Ham\u00b7pel\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Un jeden snauzt dat Dingschen an.", "tokens": ["Un", "je\u00b7den", "snauzt", "dat", "Ding\u00b7schen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Un \u00bbRechten, Linken, Speck und Schinken,", "tokens": ["Un", "\u00bb", "Rech\u00b7ten", ",", "Lin\u00b7ken", ",", "Speck", "und", "Schin\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Donnerwetter! Eins, zwei, eins, zwei,", "tokens": ["Don\u00b7ner\u00b7wet\u00b7ter", "!", "Eins", ",", "zwei", ",", "eins", ",", "zwei", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "CARD", "$,", "PIS", "$,", "CARD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Stroh und Heu, Stroh und Heu!", "tokens": ["Stroh", "und", "Heu", ",", "Stroh", "und", "Heu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.16": {"text": "Werft die Bein und reckt die Glieder,", "tokens": ["Werft", "die", "Bein", "und", "reckt", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Absatz hoch und Spitzen nieder!\u00ab", "tokens": ["Ab\u00b7satz", "hoch", "und", "Spit\u00b7zen", "nie\u00b7der", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "KON", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "So schreg dat Ding un kummandiert,", "tokens": ["So", "schreg", "dat", "Ding", "un", "kum\u00b7man\u00b7diert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "FM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dat ein sin eigen Wurd nich h\u00fcrt.", "tokens": ["Dat", "ein", "sin", "ei\u00b7gen", "Wurd", "nich", "h\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Un as hei mit de Hauptsak fahrig was,", "tokens": ["Un", "as", "hei", "mit", "de", "Haupt\u00b7sak", "fah\u00b7rig", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Namm hei den einen Kirl sick noch apart", "tokens": ["Namm", "hei", "den", "ei\u00b7nen", "Kirl", "sick", "noch", "a\u00b7part"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ART", "NN", "VVIMP", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Un slog \u00bbmit gro\u00dfer Geistesgegenwart\u00ab", "tokens": ["Un", "slog", "\u00bb", "mit", "gro\u00b7\u00dfer", "Geis\u00b7tes\u00b7ge\u00b7gen\u00b7wart", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Den dummen Bengel hellsch verdwas", "tokens": ["Den", "dum\u00b7men", "Ben\u00b7gel", "hell\u00b7sch", "verd\u00b7was"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Mit dat Gef\u00e4\u00df von sinen Degen", "tokens": ["Mit", "dat", "Ge\u00b7f\u00e4\u00df", "von", "si\u00b7nen", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Bald unner't Kinn, bald up den Bregen.", "tokens": ["Bald", "un\u00b7ner't", "Kinn", ",", "bald", "up", "den", "Bre\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "De Kirl, dat was en groten Bengel,", "tokens": ["De", "Kirl", ",", "dat", "was", "en", "gro\u00b7ten", "Ben\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "So lang un d\u00fcnn, just as en Pumpenswengel,", "tokens": ["So", "lang", "un", "d\u00fcnn", ",", "just", "as", "en", "Pum\u00b7pens\u00b7wen\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "FM", "FM", "$,", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Hei stunn denn ok so grad un stiw,", "tokens": ["Hei", "stunn", "denn", "ok", "so", "grad", "un", "stiw", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "KON", "NE", "ADV", "ADV", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "De Leutnant reikt em man an't halwe Liw;", "tokens": ["De", "Leut\u00b7nant", "reikt", "em", "man", "an't", "hal\u00b7we", "Liw", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Un't Ding h\u00f6ll doch nich up tau slahn,", "tokens": ["Un't", "Ding", "h\u00f6ll", "doch", "nich", "up", "tau", "slahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "De Kirl s\u00fcll \u00fcmmer grader stahn;", "tokens": ["De", "Kirl", "s\u00fcll", "\u00fcm\u00b7mer", "gra\u00b7der", "stahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "De Bost s\u00fcll 'rut, de Buk s\u00fcll 'rin;", "tokens": ["De", "Bost", "s\u00fcll", "'rut", ",", "de", "Buk", "s\u00fcll", "'rin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "NE", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Bald slog hei'n an de Bein,", "tokens": ["Bald", "slog", "hei'n", "an", "de", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Bald st\u00f6dd hei'n unner't Kinn.", "tokens": ["Bald", "st\u00f6dd", "hei'n", "un\u00b7ner't", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Doch as hei sach, hei k\u00fcnn't nich wider driwen,", "tokens": ["Doch", "as", "hei", "sach", ",", "hei", "k\u00fcnn't", "nich", "wi\u00b7der", "dri\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "$,", "ADJD", "VVFIN", "PTKNEG", "APPR", "NE", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.36": {"text": "Dunn s\u00e4d hei tau den Kirl: \u00bbSo soll es sein!", "tokens": ["Dunn", "s\u00e4d", "hei", "tau", "den", "Kirl", ":", "\u00bb", "So", "soll", "es", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "ART", "NN", "$.", "$(", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "So, du Carnallie, so, nun steh!\u00ab", "tokens": ["So", ",", "du", "Car\u00b7nal\u00b7lie", ",", "so", ",", "nun", "steh", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PPER", "NE", "$,", "ADV", "$,", "ADV", "VVFIN", "$.", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.38": {"text": "\u00bbso sall'ck nu \u00fcmmer stahn hir bliwen?\u00ab", "tokens": ["\u00bb", "so", "sall'ck", "nu", "\u00fcm\u00b7mer", "stahn", "hir", "bli\u00b7wen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "ADV", "ADV", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "\u00bbso stehst du mir! Kopf in die H\u00f6h,", "tokens": ["\u00bb", "so", "stehst", "du", "mir", "!", "Kopf", "in", "die", "H\u00f6h", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPER", "$.", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.40": {"text": "Die Arme 'ran, ausw\u00e4rts die F\u00fc\u00df,", "tokens": ["Die", "Ar\u00b7me", "'ran", ",", "aus\u00b7w\u00e4rts", "die", "F\u00fc\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.41": {"text": "Die Brust heraus, den Bauch herein!\u00ab", "tokens": ["Die", "Brust", "he\u00b7raus", ",", "den", "Bauch", "her\u00b7ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "\u00bbna denn, Herr Leutnant, denn adj\u00fcs!", "tokens": ["\u00bb", "na", "denn", ",", "Herr", "Leut\u00b7nant", ",", "denn", "ad\u00b7j\u00fcs", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "NN", "NN", "$,", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Denn krig'ck Sei n\u00fcmmer mihr tau seihn.\u00ab", "tokens": ["Denn", "krig'ck", "Sei", "n\u00fcm\u00b7mer", "mihr", "tau", "seihn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "ADV", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}