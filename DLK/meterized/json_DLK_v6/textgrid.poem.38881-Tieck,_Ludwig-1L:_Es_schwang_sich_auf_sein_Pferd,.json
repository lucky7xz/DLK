{"textgrid.poem.38881": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schwang sich auf sein Pferd,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schwang sich auf sein Pferd,", "tokens": ["Es", "schwang", "sich", "auf", "sein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Eckart, der edle Held,", "tokens": ["E\u00b7ckart", ",", "der", "ed\u00b7le", "Held", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sprach: in aller Welt", "tokens": ["Und", "sprach", ":", "in", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist mir nun nichts mehr werth.", "tokens": ["Ist", "mir", "nun", "nichts", "mehr", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die S\u00f6hn' hab' ich verloren,", "tokens": ["Die", "S\u00f6hn'", "hab'", "ich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So find' ich nirgend Trost,", "tokens": ["So", "find'", "ich", "nir\u00b7gend", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der F\u00fcrst ist mir erbost,", "tokens": ["Der", "F\u00fcrst", "ist", "mir", "er\u00b7bost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat meinen Tod geschworen.", "tokens": ["Hat", "mei\u00b7nen", "Tod", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da reitet er zu Wald", "tokens": ["Da", "rei\u00b7tet", "er", "zu", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und klagt aus vollem Herzen", "tokens": ["Und", "klagt", "aus", "vol\u00b7lem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die \u00fcbergro\u00dfen Schmerzen,", "tokens": ["Die", "\u00fc\u00b7ber\u00b7gro\u00b7\u00dfen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df weit die Stimme schallt:", "tokens": ["Da\u00df", "weit", "die", "Stim\u00b7me", "schallt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Menschen sind mir todt,", "tokens": ["Die", "Men\u00b7schen", "sind", "mir", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich mu\u00df mir Freunde suchen", "tokens": ["Ich", "mu\u00df", "mir", "Freun\u00b7de", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Eichen, wilden Buchen,", "tokens": ["In", "Ei\u00b7chen", ",", "wil\u00b7den", "Bu\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn'n klagen meine Noth.", "tokens": ["Ihn'n", "kla\u00b7gen", "mei\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kein Kind, das mich erg\u00f6tzt,", "tokens": ["Kein", "Kind", ",", "das", "mich", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erw\u00fcrgt vom schlimmen Leuen", "tokens": ["Er\u00b7w\u00fcrgt", "vom", "schlim\u00b7men", "Leu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Blieb keiner von den dreien,", "tokens": ["Blieb", "kei\u00b7ner", "von", "den", "drei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "CARD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebste starb zuletzt.", "tokens": ["Der", "Liebs\u00b7te", "starb", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie Eckart also klagte", "tokens": ["Wie", "E\u00b7ckart", "al\u00b7so", "klag\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verlor er Sinn und Muth,", "tokens": ["Ver\u00b7lor", "er", "Sinn", "und", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er reit't in Zorneswuth", "tokens": ["Er", "reit't", "in", "Zor\u00b7nes\u00b7wuth"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als schon der Morgen tagte.", "tokens": ["Als", "schon", "der", "Mor\u00b7gen", "tag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Ro\u00df, das treu geblieben", "tokens": ["Das", "Ro\u00df", ",", "das", "treu", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "St\u00fcrzt hin im wilden Lauf,", "tokens": ["St\u00fcrzt", "hin", "im", "wil\u00b7den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er achtet nicht darauf", "tokens": ["Er", "ach\u00b7tet", "nicht", "da\u00b7rauf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und will nun nichts mehr lieben.", "tokens": ["Und", "will", "nun", "nichts", "mehr", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er thut die R\u00fcstung abe,", "tokens": ["Er", "thut", "die", "R\u00fcs\u00b7tung", "a\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wirft sich zu Boden hin,", "tokens": ["Wirft", "sich", "zu", "Bo\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Sterben steht sein Sinn,", "tokens": ["Auf", "Ster\u00b7ben", "steht", "sein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Wunsch nur nach dem Grabe.", "tokens": ["Sein", "Wunsch", "nur", "nach", "dem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}