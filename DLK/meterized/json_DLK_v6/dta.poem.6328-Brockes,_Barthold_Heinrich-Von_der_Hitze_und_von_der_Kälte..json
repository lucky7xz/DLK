{"dta.poem.6328": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Arm des Schmidts, indem er tapffer schl\u00e4get", "tokens": ["Der", "Arm", "des", "Schmidts", ",", "in\u00b7dem", "er", "tapf\u00b7fer", "schl\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Eisen, wenn es gl\u00fcht, und es dadurch beweget;", "tokens": ["Auf", "Ei\u00b7sen", ",", "wenn", "es", "gl\u00fcht", ",", "und", "es", "da\u00b7durch", "be\u00b7we\u00b7get", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Macht es geschickt zu seiner Kunst.", "tokens": ["Macht", "es", "ge\u00b7schickt", "zu", "sei\u00b7ner", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hingegen h\u00e4rtet sie die C\u00f6rper, welche feucht,", "tokens": ["Hin\u00b7ge\u00b7gen", "h\u00e4r\u00b7tet", "sie", "die", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "feucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Und angef\u00fcllet sind, von einem nassen Dunst,", "tokens": ["Und", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "sind", ",", "von", "ei\u00b7nem", "nas\u00b7sen", "Dunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ihre Feuchtigkeit aus ihren L\u00f6chern weicht.", "tokens": ["Wenn", "ih\u00b7re", "Feuch\u00b7tig\u00b7keit", "aus", "ih\u00b7ren", "L\u00f6\u00b7chern", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}