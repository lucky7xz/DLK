{"dta.poem.13144": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewer in dem Tugend-Krei\u00df an Ruhm die Reichste sey;", "tokens": ["\u201e", "wer", "in", "dem", "Tu\u00b7gen\u00b7dKrei\u00df", "an", "Ruhm", "die", "Reichs\u00b7te", "sey", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "NN", "APPR", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "\u201ewas man vor einen Bau derselben Ehre weih;", "tokens": ["\u201e", "was", "man", "vor", "ei\u00b7nen", "Bau", "der\u00b7sel\u00b7ben", "Eh\u00b7re", "weih", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PIS", "APPR", "ART", "NN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edas untersuchte man durch so vielfache Stimmen,", "tokens": ["\u201e", "das", "un\u00b7ter\u00b7such\u00b7te", "man", "durch", "so", "viel\u00b7fa\u00b7che", "Stim\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PIS", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "180\u201dDa\u00df man am Anfang ist: die\u00df nenn' ich irrend schwimmen,", "tokens": ["\"", "Da\u00df", "man", "am", "An\u00b7fang", "ist", ":", "die\u00df", "nenn'", "ich", "ir\u00b7rend", "schwim\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "APPRART", "NN", "VAFIN", "$.", "PDS", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eund weder Strand noch Port erreichen, oder sehn;", "tokens": ["\u201e", "und", "we\u00b7der", "Strand", "noch", "Port", "er\u00b7rei\u00b7chen", ",", "o\u00b7der", "sehn", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "KON", "NN", "ADV", "NE", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eja sich den Winden nach wie schlancke Binsen drehn.", "tokens": ["\u201e", "ja", "sich", "den", "Win\u00b7den", "nach", "wie", "schlan\u00b7cke", "Bin\u00b7sen", "drehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PRF", "ART", "NN", "APPR", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eallein mir f\u00e4llt nicht ein, euch etwas abzustreiten;", "tokens": ["\u201e", "al\u00b7lein", "mir", "f\u00e4llt", "nicht", "ein", ",", "euch", "et\u00b7was", "ab\u00b7zu\u00b7strei\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ees seynd euch insgesamt zu viele Treflichkeiten", "tokens": ["\u201e", "es", "seynd", "euch", "ins\u00b7ge\u00b7samt", "zu", "vie\u00b7le", "Tref\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "185\u201dUnd Wirckungen gemein. Die Welt ist \u00fcberzeugt,", "tokens": ["\"", "Und", "Wir\u00b7ckun\u00b7gen", "ge\u00b7mein", ".", "Die", "Welt", "ist", "\u00fc\u00b7berz\u00b7eugt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "ADJD", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "\u201ewie weit ", "tokens": ["\u201e", "wie", "weit"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PWAV", "ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "\u201eund dannoch wurd es ihr an Wissenschaft gebrechen,", "tokens": ["\u201e", "und", "dan\u00b7noch", "wurd", "es", "ihr", "an", "Wis\u00b7sen\u00b7schaft", "ge\u00b7bre\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "VAFIN", "PPER", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ef\u00fcr welche von dem Rath der Vorzug auszusprechen.", "tokens": ["\u201e", "f\u00fcr", "wel\u00b7che", "von", "dem", "Rath", "der", "Vor\u00b7zug", "aus\u00b7zu\u00b7spre\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eman r\u00fchmt euch alle gleich, da\u00df jede nach dem Stand", "tokens": ["\u201e", "man", "r\u00fchmt", "euch", "al\u00b7le", "gleich", ",", "da\u00df", "je\u00b7de", "nach", "dem", "Stand"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "PPER", "PIS", "ADV", "$,", "KOUS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "190\u201dDer Pflichten und des Amts ihr Alles angewandt,", "tokens": ["\"", "Der", "Pflich\u00b7ten", "und", "des", "Amts", "ihr", "Al\u00b7les", "an\u00b7ge\u00b7wandt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eden GOtt-geweihten Sinn der K\u00f6niginn zu leiten,", "tokens": ["\u201e", "den", "Got\u00b7tge\u00b7weih\u00b7ten", "Sinn", "der", "K\u00f6\u00b7ni\u00b7ginn", "zu", "lei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201ein ihm den goldnen Thron der Tugend zu bereiten.", "tokens": ["\u201e", "in", "ihm", "den", "gold\u00b7nen", "Thron", "der", "Tu\u00b7gend", "zu", "be\u00b7rei\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201ezu dem ists auch gewi\u00df, da\u00df ihr einander gleicht,", "tokens": ["\u201e", "zu", "dem", "ists", "auch", "ge\u00b7wi\u00df", ",", "da\u00df", "ihr", "ein\u00b7an\u00b7der", "gleicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eeinander von dem Werth des eignen Ruhms nicht weicht.", "tokens": ["\u201e", "ein\u00b7an\u00b7der", "von", "dem", "Werth", "des", "eig\u00b7nen", "Ruhms", "nicht", "weicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "195\u201dMan lie\u00df ja keinen Saz des Vortrags unbestritten,", "tokens": ["\"", "Man", "lie\u00df", "ja", "kei\u00b7nen", "Saz", "des", "Vor\u00b7trags", "un\u00b7be\u00b7strit\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "ADV", "PIAT", "NN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eso viel man auch gewirckt, geholffen und gelitten.", "tokens": ["\u201e", "so", "viel", "man", "auch", "ge\u00b7wirckt", ",", "ge\u00b7holf\u00b7fen", "und", "ge\u00b7lit\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PIS", "ADV", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201edie trugen wenig vor, und andere zu viel;", "tokens": ["\u201e", "die", "tru\u00b7gen", "we\u00b7nig", "vor", ",", "und", "an\u00b7de\u00b7re", "zu", "viel", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PIS", "PTKVZ", "$,", "KON", "PIS", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201edie redeten zugleich; verschiedne schwiegen still;", "tokens": ["\u201e", "die", "re\u00b7de\u00b7ten", "zu\u00b7gleich", ";", "ver\u00b7schied\u00b7ne", "schwie\u00b7gen", "still", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "$.", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.23": {"text": "\u201ewas fast entschlossen war, das lie\u00df man euch verbieten,", "tokens": ["\u201e", "was", "fast", "ent\u00b7schlos\u00b7sen", "war", ",", "das", "lie\u00df", "man", "euch", "ver\u00b7bie\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "VVPP", "VAFIN", "$,", "PDS", "VVFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "200\u201dDa\u00df viele zu der Wahl des Mit-Regentens schritten.", "tokens": ["\"", "Da\u00df", "vie\u00b7le", "zu", "der", "Wahl", "des", "Mit\u00b7Re\u00b7gen\u00b7tens", "schrit\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "\u201ezwey ", "tokens": ["\u201e", "zwey"], "token_info": ["punct", "word"], "pos": ["$(", "CARD"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "\u201ewer ist, der alles die\u00df so gleich entscheiden mag?", "tokens": ["\u201e", "wer", "ist", ",", "der", "al\u00b7les", "die\u00df", "so", "gleich", "ent\u00b7schei\u00b7den", "mag", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "$,", "PRELS", "PIS", "PDS", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}