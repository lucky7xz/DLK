{"dta.poem.4292": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Auer-Ochs und der Tieger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier zeigt sich die Gerechtigkeit, hier wird die Grau-", "tokens": ["Hier", "zeigt", "sich", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "hier", "wird", "die", "Grau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "samkeit gestraft,", "tokens": ["sam\u00b7keit", "ge\u00b7straft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und manch verschlungnes Thier gerochen. Der Auer,", "tokens": ["Und", "manch", "ver\u00b7schlung\u00b7nes", "Thier", "ge\u00b7ro\u00b7chen", ".", "Der", "Au\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVPP", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "der halb tugendhaft,", "tokens": ["der", "halb", "tu\u00b7gend\u00b7haft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nie, als bis er gereizt, verletzet, bringt mit nicht unge-", "tokens": ["Nie", ",", "als", "bis", "er", "ge\u00b7reizt", ",", "ver\u00b7let\u00b7zet", ",", "bringt", "mit", "nicht", "un\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "PPER", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PTKNEG", "TRUNC"], "meter": "--+--+-+-++-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "rechtem Grimm,", "tokens": ["rech\u00b7tem", "Grimm", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Durch Vorsicht, Tapferkeit und St\u00e4rke, den mordbegier\u2019-", "tokens": ["Durch", "Vor\u00b7sicht", ",", "Tap\u00b7fer\u00b7keit", "und", "St\u00e4r\u00b7ke", ",", "den", "mord\u00b7be\u00b7gier'"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "gen Gegner \u00fcm.", "tokens": ["gen", "Geg\u00b7ner", "\u00fcm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Er dr\u00fcckt die fast gest\u00e4hlten H\u00f6rner dem Tieger in die", "tokens": ["Er", "dr\u00fcckt", "die", "fast", "ge\u00b7st\u00e4hl\u00b7ten", "H\u00f6r\u00b7ner", "dem", "Tie\u00b7ger", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJA", "NN", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "D\u00e4rmer ein;", "tokens": ["D\u00e4r\u00b7mer", "ein", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Man h\u00f6rt sein Angst-Geschrey mit Lust, und sieht mit", "tokens": ["Man", "h\u00f6rt", "sein", "Angst\u00b7Ge\u00b7schrey", "mit", "Lust", ",", "und", "sieht", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Anmuht seine Pein.", "tokens": ["An\u00b7muht", "sei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Man merkt an seiner schwehren Bratzen sich \u00e4ngstlich-", "tokens": ["Man", "merkt", "an", "sei\u00b7ner", "schweh\u00b7ren", "Brat\u00b7zen", "sich", "\u00e4ngst\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "spreizendem Gew\u00fchle,", "tokens": ["sprei\u00b7zen\u00b7dem", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Auch an dem finstern Feu\u2019r im Aug\u2019, da\u00df er sein nahes", "tokens": ["Auch", "an", "dem", "fins\u00b7tern", "Feu'r", "im", "Aug'", ",", "da\u00df", "er", "sein", "na\u00b7hes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sterben f\u00fchle.", "tokens": ["Ster\u00b7ben", "f\u00fch\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Sein reger schnell-gedreh\u2019ter Schweif wird bald erstarren", "tokens": ["Sein", "re\u00b7ger", "schnell\u00b7ge\u00b7dreh'\u00b7ter", "Schweif", "wird", "bald", "er\u00b7star\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "und sich strecken,", "tokens": ["und", "sich", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Und ein von seiner M\u00f6rder-Seele verla\u00dfnes starres Aas", "tokens": ["Und", "ein", "von", "sei\u00b7ner", "M\u00f6r\u00b7der\u00b7See\u00b7le", "ver\u00b7la\u00df\u00b7nes", "star\u00b7res", "Aas"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "entdecken.", "tokens": ["ent\u00b7de\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Des starken Siegers stramme Sehnen, die er erz\u00fcrnt zu-", "tokens": ["Des", "star\u00b7ken", "Sie\u00b7gers", "stram\u00b7me", "Seh\u00b7nen", ",", "die", "er", "er\u00b7z\u00fcrnt", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "sammen rafft,", "tokens": ["sam\u00b7men", "rafft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.23": {"text": "Belebt von regen Nerven-Geistern, giebt allen seinen", "tokens": ["Be\u00b7lebt", "von", "re\u00b7gen", "Ner\u00b7ven\u00b7Geis\u00b7tern", ",", "giebt", "al\u00b7len", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "PIAT", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Muskeln Kraft.", "tokens": ["Mus\u00b7keln", "Kraft", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Man sieht, wie hier des Schauers Blick sich an der Grau-", "tokens": ["Man", "sieht", ",", "wie", "hier", "des", "Schau\u00b7ers", "Blick", "sich", "an", "der", "Grau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "samkeit vergn\u00fcge;", "tokens": ["sam\u00b7keit", "ver\u00b7gn\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Wir sind dem Auer-Ochsen gut, und nehmen Theil an", "tokens": ["Wir", "sind", "dem", "Au\u00b7e\u00b7rOch\u00b7sen", "gut", ",", "und", "neh\u00b7men", "Theil", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,", "KON", "VVINF", "NN", "PTKVZ"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.28": {"text": "seinem Siege.", "tokens": ["sei\u00b7nem", "Sie\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}