{"dta.poem.23608": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die jungen Litterarhistoriker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00e4nntet ihr doch nur", "tokens": ["K\u00e4nn\u00b7tet", "ihr", "doch", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Unsre Sprach' und Gedichte,", "tokens": ["Uns\u00b7re", "Sprach'", "und", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Unserer Litteratur", "tokens": ["Un\u00b7se\u00b7rer", "Lit\u00b7te\u00b7ra\u00b7tur"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Tausendj\u00e4hr'ge Geschichte!", "tokens": ["Tau\u00b7send\u00b7j\u00e4hr'\u00b7ge", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "O so schwiegt ihr nur,", "tokens": ["O", "so", "schwiegt", "ihr", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "All ihr Pfuscher und Hudler,", "tokens": ["All", "ihr", "Pfu\u00b7scher", "und", "Hud\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Unserer Litteratur", "tokens": ["Un\u00b7se\u00b7rer", "Lit\u00b7te\u00b7ra\u00b7tur"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Allzeitfertige Sudler!", "tokens": ["All\u00b7zeit\u00b7fer\u00b7ti\u00b7ge", "Sud\u00b7ler", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Seidenschwanznatur", "tokens": ["Sei\u00b7den\u00b7schwanz\u00b7na\u00b7tur"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist euch aber gegeben,", "tokens": ["Ist", "euch", "a\u00b7ber", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und von der Litteratur", "tokens": ["Und", "von", "der", "Lit\u00b7te\u00b7ra\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "M\u00fcsset leider ihr leben.", "tokens": ["M\u00fcs\u00b7set", "lei\u00b7der", "ihr", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Nun, so schmiert denn nur", "tokens": ["Nun", ",", "so", "schmiert", "denn", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ohne Scham und Gewissen!", "tokens": ["Oh\u00b7ne", "Scham", "und", "Ge\u00b7wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Unsere Litteratur", "tokens": ["Un\u00b7se\u00b7re", "Lit\u00b7te\u00b7ra\u00b7tur"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Ward schon \u00f6fter beschmissen.", "tokens": ["Ward", "schon", "\u00f6f\u00b7ter", "be\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}