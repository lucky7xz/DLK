{"dta.poem.13141": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eist demnach euer Schlu\u00df, da\u00df ich den Spruch verfasse?", "tokens": ["\u201e", "ist", "dem\u00b7nach", "eu\u00b7er", "Schlu\u00df", ",", "da\u00df", "ich", "den", "Spruch", "ver\u00b7fas\u00b7se", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PAV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So fieng ihr Vortrag an, indem sie niedersasse.", "tokens": ["So", "fi\u00b7eng", "ihr", "Vor\u00b7trag", "an", ",", "in\u00b7dem", "sie", "nie\u00b7der\u00b7sas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u201ewahr ists, nicht jeder Hof geduldet meinen Rath;", "tokens": ["\u201e", "wahr", "ists", ",", "nicht", "je\u00b7der", "Hof", "ge\u00b7dul\u00b7det", "mei\u00b7nen", "Rath", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "$,", "PTKNEG", "PIAT", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ehier aber find\u2019 ich mehr als meine Feinde Statt.", "tokens": ["\u201e", "hier", "a\u00b7ber", "find'", "ich", "mehr", "als", "mei\u00b7ne", "Fein\u00b7de", "Statt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "PIAT", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201epflegt nicht ", "tokens": ["\u201e", "pflegt", "nicht"], "token_info": ["punct", "word", "word"], "pos": ["$(", "VVFIN", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "140\u201dWomit verborgne List sich trachtet einzufassen?", "tokens": ["\"", "Wo\u00b7mit", "ver\u00b7borg\u00b7ne", "List", "sich", "trach\u00b7tet", "ein\u00b7zu\u00b7fas\u00b7sen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJA", "NN", "PRF", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201esie nimmt, was ich gedenck\u2019 und rathe, gn\u00e4dig an;", "tokens": ["\u201e", "sie", "nimmt", ",", "was", "ich", "ge\u00b7denck", "und", "ra\u00b7the", ",", "gn\u00e4\u00b7dig", "an", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADJD", "KON", "VVFIN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201esie seufzet, wann sie mich nicht allzeit fragen kann;", "tokens": ["\u201e", "sie", "seuf\u00b7zet", ",", "wann", "sie", "mich", "nicht", "all\u00b7zeit", "fra\u00b7gen", "kann", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eein himmlisches Geh\u00f6r! o Klugheits-volle Sinnen!", "tokens": ["\u201e", "ein", "himm\u00b7li\u00b7sches", "Ge\u00b7h\u00f6r", "!", "o", "Klug\u00b7heits\u00b7vol\u00b7le", "Sin\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eso kann sie dem Betrug den Vortheil abgewinnen.", "tokens": ["\u201e", "so", "kann", "sie", "dem", "Be\u00b7trug", "den", "Vor\u00b7theil", "ab\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "145\u201dEuch hab ich diesen Trieb zu dancken: ihr verm\u00f6gt,", "tokens": ["\"", "Euch", "hab", "ich", "die\u00b7sen", "Trieb", "zu", "dan\u00b7cken", ":", "ihr", "ver\u00b7m\u00f6gt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "PDAT", "NN", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eda\u00df diese K\u00f6niginn f\u00fcr mich die Neigung h\u00e4gt.", "tokens": ["\u201e", "da\u00df", "die\u00b7se", "K\u00f6\u00b7ni\u00b7ginn", "f\u00fcr", "mich", "die", "Nei\u00b7gung", "h\u00e4gt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PDAT", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201edurch mich hat sie gelernt, was die gef\u00e4rbte Treue,", "tokens": ["\u201e", "durch", "mich", "hat", "sie", "ge\u00b7lernt", ",", "was", "die", "ge\u00b7f\u00e4rb\u00b7te", "Treu\u00b7e", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "\u201ewas Lieb und Freundlichkeit verstellter Herzen seye.", "tokens": ["\u201e", "was", "Lieb", "und", "Freund\u00b7lich\u00b7keit", "ver\u00b7stell\u00b7ter", "Her\u00b7zen", "se\u00b7ye", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "KON", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "\u201esie kennt, was hier der Mund und dort die Seele spricht.", "tokens": ["\u201e", "sie", "kennt", ",", "was", "hier", "der", "Mund", "und", "dort", "die", "See\u00b7le", "spricht", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "150\u201dKein schimmernd Lob-Gepr\u00e4ng verblendet ihr Gesicht.", "tokens": ["\"", "Kein", "schim\u00b7mernd", "Lob\u00b7Ge\u00b7pr\u00e4ng", "ver\u00b7blen\u00b7det", "ihr", "Ge\u00b7sicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edie Scharff-sicht ihres Augs wirckt auch in Finsternissen;", "tokens": ["\u201e", "die", "Scha\u00b7rff\u00b7sicht", "ih\u00b7res", "Augs", "wirckt", "auch", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.18": {"text": "\u201eso werd\u2019 ich ihrem Blick durch Wolcken nicht entrissen.", "tokens": ["\u201e", "so", "werd'", "ich", "ih\u00b7rem", "Blick", "durch", "Wol\u00b7cken", "nicht", "ent\u00b7ris\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201edurch mich erf\u00e4hrt ihr Herz, was wahre Freunde seynd;", "tokens": ["\u201e", "durch", "mich", "er\u00b7f\u00e4hrt", "ihr", "Herz", ",", "was", "wah\u00b7re", "Freun\u00b7de", "seynd", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWS", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201esie sieht, wer sie verehrt und liebt; sie kennt den Feind.", "tokens": ["\u201e", "sie", "sieht", ",", "wer", "sie", "ver\u00b7ehrt", "und", "liebt", ";", "sie", "kennt", "den", "Feind", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "155\u201dOb man mit ihr schon lacht, mit ihr sich auch betr\u00fcbet;", "tokens": ["\"", "Ob", "man", "mit", "ihr", "schon", "lacht", ",", "mit", "ihr", "sich", "auch", "be\u00b7tr\u00fc\u00b7bet", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "APPR", "PPER", "ADV", "VVFIN", "$,", "APPR", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201eso wei\u00df sie doch wer falsch der Freundschaft Zeichen \u00fcbet.", "tokens": ["\u201e", "so", "wei\u00df", "sie", "doch", "wer", "falsch", "der", "Freund\u00b7schaft", "Zei\u00b7chen", "\u00fc\u00b7bet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "PWS", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}