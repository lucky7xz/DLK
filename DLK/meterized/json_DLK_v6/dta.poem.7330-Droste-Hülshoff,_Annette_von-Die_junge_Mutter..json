{"dta.poem.7330": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die junge Mutter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Im gr\u00fcn verhangnen duftigen Gemach,", "tokens": ["Im", "gr\u00fcn", "ver\u00b7hang\u00b7nen", "duf\u00b7ti\u00b7gen", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf wei\u00dfen Kissen liegt die junge Mutter;", "tokens": ["Auf", "wei\u00b7\u00dfen", "Kis\u00b7sen", "liegt", "die", "jun\u00b7ge", "Mut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie brennt die Stirn! sie hebt das Auge schwach", "tokens": ["Wie", "brennt", "die", "Stirn", "!", "sie", "hebt", "das", "Au\u00b7ge", "schwach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Bauer, wo die Nachtigall das Futter", "tokens": ["Zum", "Bau\u00b7er", ",", "wo", "die", "Nach\u00b7ti\u00b7gall", "das", "Fut\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den nackten Jungen reicht: \u201emein armes Thier,\u201c", "tokens": ["Den", "nack\u00b7ten", "Jun\u00b7gen", "reicht", ":", "\u201e", "mein", "ar\u00b7mes", "Thier", ",", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So fl\u00fcstert sie, \u201eund bist du auch gefangen", "tokens": ["So", "fl\u00fcs\u00b7tert", "sie", ",", "\u201e", "und", "bist", "du", "auch", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "KON", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gleich mir, wenn drau\u00dfen Lenz und Sonne pragen,", "tokens": ["Gleich", "mir", ",", "wenn", "drau\u00b7\u00dfen", "Lenz", "und", "Son\u00b7ne", "pra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So hast du deine Kleinen doch bei dir.\u201c", "tokens": ["So", "hast", "du", "dei\u00b7ne", "Klei\u00b7nen", "doch", "bei", "dir", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Vorhang hebt die graue W\u00e4rterin,", "tokens": ["Den", "Vor\u00b7hang", "hebt", "die", "grau\u00b7e", "W\u00e4r\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und legt den Finger mahnend auf die Lippen;", "tokens": ["Und", "legt", "den", "Fin\u00b7ger", "mah\u00b7nend", "auf", "die", "Lip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Kranke dreht das schwere Auge hin,", "tokens": ["Die", "Kran\u00b7ke", "dreht", "das", "schwe\u00b7re", "Au\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gef\u00e4llig will sie von dem Tranke nippen;", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "will", "sie", "von", "dem", "Tran\u00b7ke", "nip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er mundet schon, und ihre bleiche Hand", "tokens": ["Er", "mun\u00b7det", "schon", ",", "und", "ih\u00b7re", "blei\u00b7che", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Fa\u00dft fester den Kristall, \u2014 o milde Labe! \u2014", "tokens": ["Fa\u00dft", "fes\u00b7ter", "den", "Kris\u00b7tall", ",", "o", "mil\u00b7de", "La\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "\u201eelisabet, was macht mein kleiner Knabe?\u201c", "tokens": ["\u201e", "e\u00b7lis\u00b7a\u00b7bet", ",", "was", "macht", "mein", "klei\u00b7ner", "Kna\u00b7be", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u201eer schl\u00e4ft,\u201c versetzt die Alte abgewandt.", "tokens": ["\u201e", "er", "schl\u00e4ft", ",", "\u201c", "ver\u00b7setzt", "die", "Al\u00b7te", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "$(", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie mag er zierlich liegen! \u2014 Kleines Ding! \u2014", "tokens": ["Wie", "mag", "er", "zier\u00b7lich", "lie\u00b7gen", "!", "Klei\u00b7nes", "Ding", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und selig l\u00e4chelnd sinkt sie in die Kissen;", "tokens": ["Und", "se\u00b7lig", "l\u00e4\u00b7chelnd", "sinkt", "sie", "in", "die", "Kis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob man den Schleyer um die Wiege hing,", "tokens": ["Ob", "man", "den", "Schle\u00b7yer", "um", "die", "Wie\u00b7ge", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Schleyer der am Erndtefest zerrissen?", "tokens": ["Den", "Schle\u00b7yer", "der", "am", "Ernd\u00b7te\u00b7fest", "zer\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man sieht es kaum, sie flickte ihn so nett,", "tokens": ["Man", "sieht", "es", "kaum", ",", "sie", "flick\u00b7te", "ihn", "so", "nett", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df alle Frauen h\u00f6chlich es gepriesen,", "tokens": ["Da\u00df", "al\u00b7le", "Frau\u00b7en", "h\u00f6ch\u00b7lich", "es", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und eine Ranke lie\u00df sie dr\u00fcber sprie\u00dfen.", "tokens": ["Und", "ei\u00b7ne", "Ran\u00b7ke", "lie\u00df", "sie", "dr\u00fc\u00b7ber", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u201ewas l\u00e4utet man im Dom, Elisabet?\u201c", "tokens": ["\u201e", "was", "l\u00e4u\u00b7tet", "man", "im", "Dom", ",", "E\u00b7lis\u00b7a\u00b7bet", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PIS", "APPRART", "NN", "$,", "NE", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201emadame, wir haben heut Mariatag.\u201c", "tokens": ["\u201e", "ma\u00b7da\u00b7me", ",", "wir", "ha\u00b7ben", "heut", "Ma\u00b7ri\u00b7a\u00b7tag", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$,", "PPER", "VAFIN", "ADV", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "So hoch im Mond? sie kann sich nicht besinnen. \u2014", "tokens": ["So", "hoch", "im", "Mond", "?", "sie", "kann", "sich", "nicht", "be\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$.", "PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie war es nur? \u2014 doch ihr Gehirn ist schwach,", "tokens": ["Wie", "war", "es", "nur", "?", "doch", "ihr", "Ge\u00b7hirn", "ist", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$.", "$(", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und leise suchend zieht sie aus den Linnen", "tokens": ["Und", "lei\u00b7se", "su\u00b7chend", "zieht", "sie", "aus", "den", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein H\u00e4ubchen, in dem Strale k\u00fcmmerlich", "tokens": ["Ein", "H\u00e4ub\u00b7chen", ",", "in", "dem", "Stra\u00b7le", "k\u00fcm\u00b7mer\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "L\u00e4\u00dft sie den Faden in die Nadel gleiten;", "tokens": ["L\u00e4\u00dft", "sie", "den", "Fa\u00b7den", "in", "die", "Na\u00b7del", "glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So ganz verborgen will sie es bereiten,", "tokens": ["So", "ganz", "ver\u00b7bor\u00b7gen", "will", "sie", "es", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und leise, leise zieht sie Stich um Stich.", "tokens": ["Und", "lei\u00b7se", ",", "lei\u00b7se", "zieht", "sie", "Stich", "um", "Stich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da \u00f6ffnet knarrend sich die Kammerth\u00fcr,", "tokens": ["Da", "\u00f6ff\u00b7net", "knar\u00b7rend", "sich", "die", "Kam\u00b7mert\u00b7h\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vorsicht'ge Schritte \u00fcber'n Teppich schleichen.", "tokens": ["Vor\u00b7sicht'\u00b7ge", "Schrit\u00b7te", "\u00fc\u00b7ber'n", "Tep\u00b7pich", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u201eich schlafe nicht, Rainer, komm her, komm hier!", "tokens": ["\u201e", "ich", "schla\u00b7fe", "nicht", ",", "Rai\u00b7ner", ",", "komm", "her", ",", "komm", "hier", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wann wird man endlich mir den Knaben reichen?\u201c", "tokens": ["Wann", "wird", "man", "end\u00b7lich", "mir", "den", "Kna\u00b7ben", "rei\u00b7chen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Gatte blickt verstohlen himmelw\u00e4rts,", "tokens": ["Der", "Gat\u00b7te", "blickt", "ver\u00b7stoh\u00b7len", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "K\u00fc\u00dft wie ein Hauch die kleinen hei\u00dfen H\u00e4nde:", "tokens": ["K\u00fc\u00dft", "wie", "ein", "Hauch", "die", "klei\u00b7nen", "hei\u00b7\u00dfen", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u201egeduld, Geduld, mein Liebchen, bis zum Ende!", "tokens": ["\u201e", "ge\u00b7duld", ",", "Ge\u00b7duld", ",", "mein", "Lieb\u00b7chen", ",", "bis", "zum", "En\u00b7de", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du bist noch gar zu leidend, gutes Herz.\u201c", "tokens": ["Du", "bist", "noch", "gar", "zu", "lei\u00b7dend", ",", "gu\u00b7tes", "Herz", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u201edu duftest Weihrauch, Mann.\u201c \u2014 \u201eIch war im Dom;", "tokens": ["\u201e", "du", "duf\u00b7test", "Weih\u00b7rauch", ",", "Mann", ".", "\u201c", "\u201e", "Ich", "war", "im", "Dom", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "$,", "NN", "$.", "$(", "$(", "$(", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schlaf, Kind\u201c; und wieder gleitet er von dannen.", "tokens": ["Schlaf", ",", "Kind", "\u201c", ";", "und", "wie\u00b7der", "glei\u00b7tet", "er", "von", "dan\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "$.", "KON", "ADV", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie aber n\u00e4ht, und liebliches Phantom", "tokens": ["Sie", "a\u00b7ber", "n\u00e4ht", ",", "und", "lieb\u00b7li\u00b7ches", "Phan\u00b7tom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Spielt um ihr Aug' von Auen, Blumen, Tannen. \u2014", "tokens": ["Spielt", "um", "ihr", "Aug'", "von", "Au\u00b7en", ",", "Blu\u00b7men", ",", "Tan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "wor