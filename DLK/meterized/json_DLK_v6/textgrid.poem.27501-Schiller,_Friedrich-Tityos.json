{"textgrid.poem.27501": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Tityos", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber Europa hinweg, das ihm huldigte, lag er gebreitet,", "tokens": ["\u00dc\u00b7ber", "Eu\u00b7ro\u00b7pa", "hin\u00b7weg", ",", "das", "ihm", "hul\u00b7dig\u00b7te", ",", "lag", "er", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APZR", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Voluminos wie er einst trat aus den Pressen zu Kehl.", "tokens": ["Vo\u00b7lu\u00b7mi\u00b7nos", "wie", "er", "einst", "trat", "aus", "den", "Pres\u00b7sen", "zu", "Kehl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sohn der Erde! So tief liegst du da, der so hoch einst gestanden,", "tokens": ["Sohn", "der", "Er\u00b7de", "!", "So", "tief", "liegst", "du", "da", ",", "der", "so", "hoch", "einst", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ADV", "ADJD", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und das gefr\u00e4\u00dfige Tier, das an der Leber dir pickt!", "tokens": ["Und", "das", "ge\u00b7fr\u00e4\u00b7\u00dfi\u00b7ge", "Tier", ",", "das", "an", "der", "Le\u00b7ber", "dir", "pickt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach, das ist Fr\u00e9rons unsterblicher Schnabel, der ewig mich peinigt,", "tokens": ["Ach", ",", "das", "ist", "Fr\u00e9rons", "uns\u00b7terb\u00b7li\u00b7cher", "Schna\u00b7bel", ",", "der", "e\u00b7wig", "mich", "pei\u00b7nigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "NE", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Weil ich mit schlechten Bonmots nach dem... gezielt.", "tokens": ["Weil", "ich", "mit", "schlech\u00b7ten", "Bon\u00b7mots", "nach", "dem", "...", "ge\u00b7zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "$(", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}