{"dta.poem.10041": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sehen zu GOttes Ehren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach schauet, wie im lauen Lentzen,", "tokens": ["Ach", "schau\u00b7et", ",", "wie", "im", "lau\u00b7en", "Lent\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die reine Lufft, die bunten Felder,", "tokens": ["Die", "rei\u00b7ne", "Lufft", ",", "die", "bun\u00b7ten", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die blaue Fluth, die gr\u00fcnen W\u00e4lder,", "tokens": ["Die", "blau\u00b7e", "Fluth", ",", "die", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihres Sch\u00f6pfers Ehre, gl\u00e4ntzen!", "tokens": ["Zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Eh\u00b7re", ",", "gl\u00e4nt\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird von euch diese Pracht, wird das bebl\u00fchmte Gr\u00fcn,", "tokens": ["Wird", "von", "euch", "die\u00b7se", "Pracht", ",", "wird", "das", "be\u00b7bl\u00fchm\u00b7te", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PDAT", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das ietzt Lufft, Fluth und Erde schm\u00fccket,", "tokens": ["Das", "ietzt", "Lufft", ",", "Fluth", "und", "Er\u00b7de", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur mit Empfindlichkeit von euch ietzt angeblicket;", "tokens": ["Nur", "mit", "Emp\u00b7find\u00b7lich\u00b7keit", "von", "euch", "ietzt", "an\u00b7ge\u00b7bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So preiset ihr schon GOtt, so r\u00fchmt und ehrt ihr Jhn.", "tokens": ["So", "prei\u00b7set", "ihr", "schon", "Gott", ",", "so", "r\u00fchmt", "und", "ehrt", "ihr", "Jhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}