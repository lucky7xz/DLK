{"textgrid.poem.23900": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, was sah ich im Traum:", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, was sah ich im Traum:", "tokens": ["Ach", ",", "was", "sah", "ich", "im", "Traum", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Du hast die Hand mir gegeben,", "tokens": ["Du", "hast", "die", "Hand", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und stumm sprach mir dein Mund:", "tokens": ["Und", "stumm", "sprach", "mir", "dein", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ja, ich f\u00fchle wie du.", "tokens": ["Ja", ",", "ich", "f\u00fch\u00b7le", "wie", "du", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Tief im Walde geschahs;", "tokens": ["Tief", "im", "Wal\u00b7de", "ge\u00b7schahs", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Es sangen um uns die V\u00f6gel,", "tokens": ["Es", "san\u00b7gen", "um", "uns", "die", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sonne k\u00fc\u00dfte das Moos", "tokens": ["Son\u00b7ne", "k\u00fc\u00df\u00b7te", "das", "Moos"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und deinen seidenen Schuh.", "tokens": ["Und", "dei\u00b7nen", "sei\u00b7de\u00b7nen", "Schuh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Nahe warst du mir so,", "tokens": ["Na\u00b7he", "warst", "du", "mir", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Da\u00df deinen Atem ich f\u00fchlte,", "tokens": ["Da\u00df", "dei\u00b7nen", "A\u00b7tem", "ich", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ich sah dir ins Aug,", "tokens": ["Und", "ich", "sah", "dir", "ins", "Aug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und ich weinte vor Gl\u00fcck.", "tokens": ["Und", "ich", "wein\u00b7te", "vor", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "M\u00e4dchen, was mir der Tag", "tokens": ["M\u00e4d\u00b7chen", ",", "was", "mir", "der", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "PPER", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "An K\u00fcmmernissen mag bringen:", "tokens": ["An", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen", "mag", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "L\u00e4chelnd denk ich des Traums,", "tokens": ["L\u00e4\u00b7chelnd", "denk", "ich", "des", "Traums", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Selig denk ich an dich.", "tokens": ["Se\u00b7lig", "denk", "ich", "an", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}