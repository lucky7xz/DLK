{"textgrid.poem.66356": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der R\u00fcckfall", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weg, o Liebe, mit dem Zauberbecher!", "tokens": ["Weg", ",", "o", "Lie\u00b7be", ",", "mit", "dem", "Zau\u00b7ber\u00b7be\u00b7cher", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weg von mir, du Flatterer, du Frecher,", "tokens": ["Weg", "von", "mir", ",", "du", "Flat\u00b7te\u00b7rer", ",", "du", "Fre\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Amor, mit dem Puppenangesicht!", "tokens": ["A\u00b7mor", ",", "mit", "dem", "Pup\u00b7pen\u00b7an\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Geht zum J\u00fcngling, der dort um der Traube", "tokens": ["Geht", "zum", "J\u00fcng\u00b7ling", ",", "der", "dort", "um", "der", "Trau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Mostbetr\u00e4ufte Berge irrt", "tokens": ["Most\u00b7be\u00b7tr\u00e4uf\u00b7te", "Ber\u00b7ge", "irrt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nach einer Lais, wie die Taube", "tokens": ["Und", "nach", "ei\u00b7ner", "Lais", ",", "wie", "die", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nach dem Tauber, girrt.", "tokens": ["Nach", "dem", "Tau\u00b7ber", ",", "girrt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Grazien und Amoretten,", "tokens": ["Gra\u00b7zi\u00b7en", "und", "A\u00b7mo\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Locket mich nicht mehr!", "tokens": ["Lo\u00b7cket", "mich", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Flechtet eure Blumenketten", "tokens": ["Flech\u00b7tet", "eu\u00b7re", "Blu\u00b7men\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Um den S\u00fc\u00dfling her,", "tokens": ["Um", "den", "S\u00fc\u00df\u00b7ling", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Der mit Spielwerk aus Paris beladen,", "tokens": ["Der", "mit", "Spiel\u00b7werk", "aus", "Pa\u00b7ris", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Dorten pfeift und spielt,", "tokens": ["Dor\u00b7ten", "pfeift", "und", "spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Und nach seinen Waden", "tokens": ["Und", "nach", "sei\u00b7nen", "Wa\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Beifall\u00e4chelnd schielt.", "tokens": ["Bei\u00b7fal\u00b7l\u00e4\u00b7chelnd", "schielt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Denn hier sitz' ich, wo, gen\u00e4hrt von Oele,", "tokens": ["Denn", "hier", "sitz'", "ich", ",", "wo", ",", "ge\u00b7n\u00e4hrt", "von", "O\u00b7e\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.17": {"text": "Dieses blaue Fl\u00e4mmchen z\u00fcckt;", "tokens": ["Die\u00b7ses", "blau\u00b7e", "Fl\u00e4mm\u00b7chen", "z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Wo mit Ernst und Staunen meine Seele", "tokens": ["Wo", "mit", "Ernst", "und", "Stau\u00b7nen", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "KON", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Auf sich selber niederblickt.", "tokens": ["Auf", "sich", "sel\u00b7ber", "nie\u00b7der\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Aller Weisen fromme Lehren schweben,", "tokens": ["Al\u00b7ler", "Wei\u00b7sen", "from\u00b7me", "Leh\u00b7ren", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Eingeh\u00fcllt in Bildern, um mich her;", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", "in", "Bil\u00b7dern", ",", "um", "mich", "her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KOUI", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Tausend Stimmen h\u00f6r' ich um mich beben:", "tokens": ["Tau\u00b7send", "Stim\u00b7men", "h\u00f6r'", "ich", "um", "mich", "be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.23": {"text": "Guter Mann, so liebe doch nicht mehr!", "tokens": ["Gu\u00b7ter", "Mann", ",", "so", "lie\u00b7be", "doch", "nicht", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Fahr empor! gen Himmel fahre!", "tokens": ["Fahr", "em\u00b7por", "!", "gen", "Him\u00b7mel", "fah\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Er allein ist deiner Liebe werth;", "tokens": ["Er", "al\u00b7lein", "ist", "dei\u00b7ner", "Lie\u00b7be", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Warte nicht, bis Wollust deiner Jahre", "tokens": ["War\u00b7te", "nicht", ",", "bis", "Wol\u00b7lust", "dei\u00b7ner", "Jah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Feuer aufgezehrt.", "tokens": ["Feu\u00b7er", "auf\u00b7ge\u00b7zehrt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will es thun! so sprach ich und die Saiten", "tokens": ["Ich", "will", "es", "thun", "!", "so", "sprach", "ich", "und", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$.", "ADV", "VVFIN", "PPER", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des hohen Fl\u00fcgels hallten's nach;", "tokens": ["Des", "ho\u00b7hen", "Fl\u00fc\u00b7gels", "hall\u00b7ten's", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Geisterchen, die mich umschwebten, freuten", "tokens": ["Die", "Geis\u00b7ter\u00b7chen", ",", "die", "mich", "um\u00b7schweb\u00b7ten", ",", "freu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich h\u00f6rbar, als ich's sprach.", "tokens": ["Sich", "h\u00f6r\u00b7bar", ",", "als", "ich's", "sprach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch, Himmel, ach! wie schnell bin ich gefallen!", "tokens": ["Doch", ",", "Him\u00b7mel", ",", "ach", "!", "wie", "schnell", "bin", "ich", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "ITJ", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du Zauberin, was willst du denn von mir?", "tokens": ["Du", "Zau\u00b7be\u00b7rin", ",", "was", "willst", "du", "denn", "von", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich seh' sie wohl, die goldnen Locken, wallen!", "tokens": ["Ich", "seh'", "sie", "wohl", ",", "die", "gold\u00b7nen", "Lo\u00b7cken", ",", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich seh' sie wohl, die blauen Augen, hier!", "tokens": ["Ich", "seh'", "sie", "wohl", ",", "die", "blau\u00b7en", "Au\u00b7gen", ",", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Was schaust du unterm sanftgeschweiften Hute", "tokens": ["Was", "schaust", "du", "un\u00b7term", "sanft\u00b7ge\u00b7schweif\u00b7ten", "Hu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Woll\u00fcstigsch\u00f6n, o Zauberin, hervor?", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig\u00b7sch\u00f6n", ",", "o", "Zau\u00b7be\u00b7rin", ",", "her\u00b7vor", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "La\u00df mich, schon st\u00fcrmt in meinem Blute", "tokens": ["La\u00df", "mich", ",", "schon", "st\u00fcrmt", "in", "mei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der Liebe Flamm' empor!", "tokens": ["Der", "Lie\u00b7be", "Flamm'", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich seh' es wohl das Schl\u00e4ngelchen am Munde,", "tokens": ["Ich", "seh'", "es", "wohl", "das", "Schl\u00e4n\u00b7ge\u00b7lchen", "am", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sich zum L\u00e4cheln kr\u00fcmmt;", "tokens": ["Das", "sich", "zum", "L\u00e4\u00b7cheln", "kr\u00fcmmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, ach! in einer finstern Stunde", "tokens": ["Und", ",", "ach", "!", "in", "ei\u00b7ner", "fins\u00b7tern", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euch Geisterchen des Weisen \u00fcberstimmt.", "tokens": ["Euch", "Geis\u00b7ter\u00b7chen", "des", "Wei\u00b7sen", "\u00fc\u00b7bers\u00b7timmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sinke schon an ihre Brust: \u2013 O blicke", "tokens": ["Ich", "sin\u00b7ke", "schon", "an", "ih\u00b7re", "Brust", ":", "\u2013", "O", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$(", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich nicht so schmachtend an! \u2013", "tokens": ["Mich", "nicht", "so", "schmach\u00b7tend", "an", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dank dir, Natur, da\u00df ich dein Meisterst\u00fccke", "tokens": ["Dank", "dir", ",", "Na\u00b7tur", ",", "da\u00df", "ich", "dein", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit diesem Arm umfassen kann!", "tokens": ["Mit", "die\u00b7sem", "Arm", "um\u00b7fas\u00b7sen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer lacht da? Ha! 's ist Amor und die Liebesg\u00f6tter,", "tokens": ["Wer", "lacht", "da", "?", "Ha", "!", "'s", "ist", "A\u00b7mor", "und", "die", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKVZ", "$.", "ITJ", "$.", "PPER", "VAFIN", "NE", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Umtanzt von Grazien;", "tokens": ["Um\u00b7tanzt", "von", "Gra\u00b7zi\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "H\u00f6r' doch, o ", "tokens": ["H\u00f6r'", "doch", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVIMP", "ADV", "$,", "FM"], "meter": "+-+", "measure": "trochaic.di"}}}}}