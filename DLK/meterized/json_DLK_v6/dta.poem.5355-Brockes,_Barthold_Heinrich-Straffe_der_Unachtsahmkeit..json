{"dta.poem.5355": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Straffe der Unachtsahmkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bewunderst du, o Mensch, des Sch\u00f6pfers Weisheit-", "tokens": ["Be\u00b7wun\u00b7derst", "du", ",", "o", "Mensch", ",", "des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Licht", "tokens": ["Licht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "In seinen Creaturen nicht;", "tokens": ["In", "sei\u00b7nen", "Crea\u00b7tu\u00b7ren", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So scheints, da\u00df du nur blos ein Werckzeug bist,", "tokens": ["So", "scheints", ",", "da\u00df", "du", "nur", "blos", "ein", "Wer\u00b7ck\u00b7zeug", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wodurch die Welt f\u00fcr andre Creaturen,", "tokens": ["Wo\u00b7durch", "die", "Welt", "f\u00fcr", "and\u00b7re", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und nicht f\u00fcr dich, geschm\u00fccket ist.", "tokens": ["Und", "nicht", "f\u00fcr", "dich", ",", "ge\u00b7schm\u00fc\u00b7cket", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Garten pflantzen, H\u00e4user bauen", "tokens": ["Dein", "Gar\u00b7ten", "pflant\u00b7zen", ",", "H\u00e4u\u00b7ser", "bau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00df, da die Menschen es, so bald sie es vollbracht,", "tokens": ["Mu\u00df", ",", "da", "die", "Men\u00b7schen", "es", ",", "so", "bald", "sie", "es", "voll\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "ART", "NN", "PPER", "$,", "ADV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht achten, nicht mit Lust, und nicht mit Danck beschauen,", "tokens": ["Nicht", "ach\u00b7ten", ",", "nicht", "mit", "Lust", ",", "und", "nicht", "mit", "Danck", "be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "APPR", "NN", "$,", "KON", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vermuhtlich Geistern seyn zu Nutz gemacht,", "tokens": ["Ver\u00b7muht\u00b7lich", "Geis\u00b7tern", "seyn", "zu", "Nutz", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAINF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die es nicht nur bey Tag\u2019, auch bey der Nacht,", "tokens": ["Die", "es", "nicht", "nur", "bey", "Tag'", ",", "auch", "bey", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "APPR", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wie es so ordentlich, so nett so sch\u00f6n,", "tokens": ["Wie", "es", "so", "or\u00b7dent\u00b7lich", ",", "so", "nett", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In ihrer Lust, zu GOttes Ehren, sehn.", "tokens": ["In", "ih\u00b7rer", "Lust", ",", "zu", "Got\u00b7tes", "Eh\u00b7ren", ",", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wann aber GOtt dennoch dich, eine F\u00e4higkeit,", "tokens": ["Wann", "a\u00b7ber", "Gott", "den\u00b7noch", "dich", ",", "ei\u00b7ne", "F\u00e4\u00b7hig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "ADV", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Sinnen auch dabey, gew\u00fcrdigt, dir zu schencken,", "tokens": ["Und", "Sin\u00b7nen", "auch", "da\u00b7bey", ",", "ge\u00b7w\u00fcr\u00b7digt", ",", "dir", "zu", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PAV", "$,", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So liegt es blos an dir, dein Dencken,", "tokens": ["So", "liegt", "es", "blos", "an", "dir", ",", "dein", "Den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Nebst ihnen auch darauf zu lencken.", "tokens": ["Nebst", "ih\u00b7nen", "auch", "da\u00b7rauf", "zu", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wann die\u00df geschicht, wird GOtt durch jene nicht allein,", "tokens": ["Wann", "die\u00df", "ge\u00b7schicht", ",", "wird", "Gott", "durch", "je\u00b7ne", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVPP", "$,", "VAFIN", "NN", "APPR", "PDS", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nein, auch durch dich zugleich, gepriesen seyn.", "tokens": ["Nein", ",", "auch", "durch", "dich", "zu\u00b7gleich", ",", "ge\u00b7prie\u00b7sen", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "PPER", "ADV", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Geschicht es nicht; so must du selbst gestehn,", "tokens": ["Ge\u00b7schicht", "es", "nicht", ";", "so", "must", "du", "selbst", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Da\u00df da du auch dazu erlesen,", "tokens": ["Da\u00df", "da", "du", "auch", "da\u00b7zu", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "In seiner Creatur sein\u2019 Allmacht anzusehn,", "tokens": ["In", "sei\u00b7ner", "Crea\u00b7tur", "sein'", "All\u00b7macht", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "In deiner Lust den Sch\u00f6pfer zu erh\u00f6hn;", "tokens": ["In", "dei\u00b7ner", "Lust", "den", "Sch\u00f6p\u00b7fer", "zu", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Es, wenn dasselbe nicht geschehn,", "tokens": ["Es", ",", "wenn", "das\u00b7sel\u00b7be", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PDAT", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Blo\u00df deine Schuld allein gewesen.", "tokens": ["Blo\u00df", "dei\u00b7ne", "Schuld", "al\u00b7lein", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}