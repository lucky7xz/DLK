{"textgrid.poem.44125": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ged\u00e4cht auch die Natur, du aller Musen Lust,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ged\u00e4cht auch die Natur, du aller Musen Lust,", "tokens": ["Ge\u00b7d\u00e4cht", "auch", "die", "Na\u00b7tur", ",", "du", "al\u00b7ler", "Mu\u00b7sen", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir jeden Augenblick mehr Herzen zu vermehlen,", "tokens": ["Mir", "je\u00b7den", "Au\u00b7gen\u00b7blick", "mehr", "Her\u00b7zen", "zu", "ver\u00b7meh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Seufzer herber Noth jezt meinen Zustand qu\u00e4len,", "tokens": ["Als", "Seuf\u00b7zer", "her\u00b7ber", "Noth", "jezt", "mei\u00b7nen", "Zu\u00b7stand", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Worte deine Treu vor meinen Trost gewust", "tokens": ["Als", "Wor\u00b7te", "dei\u00b7ne", "Treu", "vor", "mei\u00b7nen", "Trost", "ge\u00b7wust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und als ich Fehler zieh, die du ertragen must,", "tokens": ["Und", "als", "ich", "Feh\u00b7ler", "zieh", ",", "die", "du", "er\u00b7tra\u00b7gen", "must", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch seh ich zum voraus, du w\u00fcrdest alle stehlen,", "tokens": ["Doch", "seh", "ich", "zum", "vo\u00b7raus", ",", "du", "w\u00fcr\u00b7dest", "al\u00b7le", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "PTKVZ", "$,", "PPER", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ich behielte nichts als ein vergeblich Zehlen", "tokens": ["Und", "ich", "be\u00b7hiel\u00b7te", "nichts", "als", "ein", "ver\u00b7geb\u00b7lich", "Zeh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIS", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und eine durch den Raub mit Lieb erf\u00fcllte Brust.", "tokens": ["Und", "ei\u00b7ne", "durch", "den", "Raub", "mit", "Lieb", "er\u00b7f\u00fcll\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So pflegt die Redligkeit mit ihrer Gunst zu fangen.", "tokens": ["So", "pflegt", "die", "Red\u00b7lig\u00b7keit", "mit", "ih\u00b7rer", "Gunst", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Behalt, mein Jonathan, mich ewig in Verhaft,", "tokens": ["Be\u00b7halt", ",", "mein", "Jo\u00b7na\u00b7than", ",", "mich", "e\u00b7wig", "in", "Ver\u00b7haft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil diese Sclaverey der Weisen Freyheit schaft,", "tokens": ["Weil", "die\u00b7se", "Scla\u00b7ve\u00b7rey", "der", "Wei\u00b7sen", "Frey\u00b7heit", "schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit der mir dein Verstand so eifrig nachgegangen.", "tokens": ["Mit", "der", "mir", "dein", "Ver\u00b7stand", "so", "eif\u00b7rig", "nach\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Warheit schreib es auf: Bricht unsre Freundschaft ein,", "tokens": ["Die", "War\u00b7heit", "schreib", "es", "auf", ":", "Bricht", "uns\u00b7re", "Freund\u00b7schaft", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "So mag wohl Pylades ein griechsches M\u00e4hrchen seyn.", "tokens": ["So", "mag", "wohl", "Py\u00b7la\u00b7des", "ein", "griech\u00b7sches", "M\u00e4hr\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "NE", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}