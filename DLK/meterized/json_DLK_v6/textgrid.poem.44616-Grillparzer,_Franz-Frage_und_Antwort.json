{"textgrid.poem.44616": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Frage und Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebt der Mensch zu seinem Gl\u00fccke?", "tokens": ["Lebt", "der", "Mensch", "zu", "sei\u00b7nem", "Gl\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebt der Mensch zu seiner Qual?", "tokens": ["Lebt", "der", "Mensch", "zu", "sei\u00b7ner", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fragen weinend meine Blicke", "tokens": ["Fra\u00b7gen", "wei\u00b7nend", "mei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch, ihr Sterne ohne Zahl.", "tokens": ["Euch", ",", "ihr", "Ster\u00b7ne", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lockend winket dort die Freude,", "tokens": ["Lo\u00b7ckend", "win\u00b7ket", "dort", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00e4\u00dflich drohet hier der Schmerz!", "tokens": ["Gr\u00e4\u00df\u00b7lich", "dro\u00b7het", "hier", "der", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wonne wandelt sich zum Leide,", "tokens": ["Won\u00b7ne", "wan\u00b7delt", "sich", "zum", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterbend bricht das arme Herz.", "tokens": ["Ster\u00b7bend", "bricht", "das", "ar\u00b7me", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ne Phantasieen schwinden", "tokens": ["Sch\u00f6\u00b7ne", "Phan\u00b7ta\u00b7si\u00b7e\u00b7en", "schwin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie die leichten Nebel hin,", "tokens": ["Wie", "die", "leich\u00b7ten", "Ne\u00b7bel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die ernsten Sorgen binden", "tokens": ["Und", "die", "erns\u00b7ten", "Sor\u00b7gen", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An den Staub den freien Sinn.", "tokens": ["An", "den", "Staub", "den", "frei\u00b7en", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alles Wissen, alles Streben", "tokens": ["Al\u00b7les", "Wis\u00b7sen", ",", "al\u00b7les", "Stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Raubt uns nur der Einfalt Gl\u00fcck,", "tokens": ["Raubt", "uns", "nur", "der", "Ein\u00b7falt", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Weisheit kann uns geben", "tokens": ["Kei\u00b7ne", "Weis\u00b7heit", "kann", "uns", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfer Kindheit Traum zur\u00fcck.", "tokens": ["S\u00fc\u00b7\u00dfer", "Kind\u00b7heit", "Traum", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weinend fragen meine Blicke", "tokens": ["Wei\u00b7nend", "fra\u00b7gen", "mei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch, ihr Sterne ohne Zahl:", "tokens": ["Euch", ",", "ihr", "Ster\u00b7ne", "oh\u00b7ne", "Zahl", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebt der Mensch zu seinem Gl\u00fccke?", "tokens": ["Lebt", "der", "Mensch", "zu", "sei\u00b7nem", "Gl\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt der Mensch zu seiner Qual?", "tokens": ["Lebt", "der", "Mensch", "zu", "sei\u00b7ner", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lichte Wolken seh ich gl\u00e4nzen,", "tokens": ["Lich\u00b7te", "Wol\u00b7ken", "seh", "ich", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkel weicht dem Rosenschein \u2013", "tokens": ["Dun\u00b7kel", "weicht", "dem", "Ro\u00b7sen\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterne bilden sich zu Kr\u00e4nzen,", "tokens": ["Ster\u00b7ne", "bil\u00b7den", "sich", "zu", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geisterhauch durchrauscht den Hain,", "tokens": ["Geis\u00b7ter\u00b7hauch", "durc\u00b7hrauscht", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und mit s\u00fc\u00dfen Fl\u00f6tent\u00f6nen", "tokens": ["Und", "mit", "s\u00fc\u00b7\u00dfen", "Fl\u00f6\u00b7ten\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00fcsterts aus der L\u00fcfte Blau:", "tokens": ["Fl\u00fcs\u00b7terts", "aus", "der", "L\u00fcf\u00b7te", "Blau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weihe dich dem Guten, Sch\u00f6nen,", "tokens": ["Wei\u00b7he", "dich", "dem", "Gu\u00b7ten", ",", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchts auch hier im Tr\u00e4nentau;", "tokens": ["Bl\u00fchts", "auch", "hier", "im", "Tr\u00e4\u00b7nen\u00b7tau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "K\u00e4mpfe k\u00fchn mit dem Geschicke!", "tokens": ["K\u00e4mp\u00b7fe", "k\u00fchn", "mit", "dem", "Ge\u00b7schi\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, du lebest nicht zur Qual!", "tokens": ["Nein", ",", "du", "le\u00b7best", "nicht", "zur", "Qual", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmelan, zum ewgen Gl\u00fccke", "tokens": ["Him\u00b7me\u00b7lan", ",", "zum", "ew\u00b7gen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt dein Weg durchs Sorgental.", "tokens": ["F\u00fchrt", "dein", "Weg", "durchs", "Sor\u00b7gen\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}