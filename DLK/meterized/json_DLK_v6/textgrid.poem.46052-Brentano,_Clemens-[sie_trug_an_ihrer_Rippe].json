{"textgrid.poem.46052": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[sie trug an ihrer Rippe]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie trug an ihrer Rippe", "tokens": ["Sie", "trug", "an", "ih\u00b7rer", "Rip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den sch\u00f6nsten Blumenstrau\u00df,", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tod mit seiner Hippe,", "tokens": ["Der", "Tod", "mit", "sei\u00b7ner", "Hip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der tanzte ihr voraus;", "tokens": ["Der", "tanz\u00b7te", "ihr", "vo\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und als sie an den Brunnen kam,", "tokens": ["Und", "als", "sie", "an", "den", "Brun\u00b7nen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da tat sie einen Sprung,", "tokens": ["Da", "tat", "sie", "ei\u00b7nen", "Sprung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und tr\u00e4umte einen Nonnentraum,", "tokens": ["Und", "tr\u00e4um\u00b7te", "ei\u00b7nen", "Non\u00b7nen\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und war gleich wieder jung.", "tokens": ["Und", "war", "gleich", "wie\u00b7der", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}