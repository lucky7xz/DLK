{"textgrid.poem.30143": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wercke kluger Sinnen", "tokens": ["Die", "Wer\u00b7cke", "klu\u00b7ger", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat nie vertilgen k\u00fcnnen", "tokens": ["Hat", "nie", "ver\u00b7til\u00b7gen", "k\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Zeiten starcke Flucht,", "tokens": ["Der", "Zei\u00b7ten", "star\u00b7cke", "Flucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie viel sie sonst vermocht.", "tokens": ["Wie", "viel", "sie", "sonst", "ver\u00b7mocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auff Stahl und Stein zu bauen,", "tokens": ["Auff", "Stahl", "und", "Stein", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darff keiner sicher trauen;", "tokens": ["Darff", "kei\u00b7ner", "si\u00b7cher", "trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie nemen eher Bruch", "tokens": ["Sie", "ne\u00b7men", "e\u00b7her", "Bruch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als ein gelehrtes Buch.", "tokens": ["Als", "ein", "ge\u00b7lehr\u00b7tes", "Buch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}