{"textgrid.poem.48300": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Derffling", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es haben alle St\u00e4nde", "tokens": ["Es", "ha\u00b7ben", "al\u00b7le", "St\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So ihren Degenwert,", "tokens": ["So", "ih\u00b7ren", "De\u00b7gen\u00b7wert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und selbst in Schneiderh\u00e4nde", "tokens": ["Und", "selbst", "in", "Schnei\u00b7der\u00b7h\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kam einst das Heldenschwert;", "tokens": ["Kam", "einst", "das", "Hel\u00b7den\u00b7schwert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum jeder, der da z\u00fcnftig", "tokens": ["Drum", "je\u00b7der", ",", "der", "da", "z\u00fcnf\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PIS", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Nadel und mit Scher',", "tokens": ["Mit", "Na\u00b7del", "und", "mit", "Scher'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der mache jetzt und k\u00fcnftig", "tokens": ["Der", "ma\u00b7che", "jetzt", "und", "k\u00fcnf\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor Derffling sein Honneur.", "tokens": ["Vor", "Derf\u00b7fling", "sein", "Hon\u00b7neur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "In seinen jungen Tagen", "tokens": ["In", "sei\u00b7nen", "jun\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War das ein Schneiderblut,", "tokens": ["War", "das", "ein", "Schnei\u00b7der\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch mocht' ihm nicht behagen", "tokens": ["Doch", "mocht'", "ihm", "nicht", "be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So Zwirn wie Fingerhut;", "tokens": ["So", "Zwirn", "wie", "Fin\u00b7ger\u00b7hut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn er als Geselle", "tokens": ["Und", "wenn", "er", "als", "Ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So sa\u00df und f\u00e4delt' ein,", "tokens": ["So", "sa\u00df", "und", "f\u00e4\u00b7delt'", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schien ihm die Schneiderh\u00f6lle", "tokens": ["Schien", "ihm", "die", "Schnei\u00b7der\u00b7h\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die H\u00f6lle selbst zu sein.", "tokens": ["Die", "H\u00f6l\u00b7le", "selbst", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Einst, als das Nadelhalten", "tokens": ["Einst", ",", "als", "das", "Na\u00b7del\u00b7hal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm schier ans Leben ging,", "tokens": ["Ihm", "schier", "ans", "Le\u00b7ben", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dacht' er: \u203aDas Sch\u00e4delspalten", "tokens": ["Dacht'", "er", ":", "\u203a", "Das", "Sch\u00e4\u00b7del\u00b7spal\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist doch ein ander Ding\u2039;", "tokens": ["Ist", "doch", "ein", "an\u00b7der", "Ding", "\u2039", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fort warf er Ma\u00df und Elle", "tokens": ["Fort", "warf", "er", "Ma\u00df", "und", "El\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Voll Kriegslust an die Wand", "tokens": ["Voll", "Kriegs\u00b7lust", "an", "die", "Wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und nahm an Nadels Stelle", "tokens": ["Und", "nahm", "an", "Na\u00b7dels", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den S\u00e4bel in die Hand.", "tokens": ["Den", "S\u00e4\u00b7bel", "in", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sonst focht er still und friedlich", "tokens": ["Sonst", "focht", "er", "still", "und", "fried\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Handwerksburschen-Recht,", "tokens": ["Nach", "Hand\u00b7werks\u00b7bur\u00b7schen\u00b7Recht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt war er unerm\u00fcdlich", "tokens": ["Jetzt", "war", "er", "un\u00b7er\u00b7m\u00fcd\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim Fechten im Gefecht;", "tokens": ["Beim", "Fech\u00b7ten", "im", "Ge\u00b7fecht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es war der flinke Schneider", "tokens": ["Es", "war", "der", "flin\u00b7ke", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Stechen wohl geschickt,", "tokens": ["Zum", "Ste\u00b7chen", "wohl", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Oft hat er an die Kleider", "tokens": ["Oft", "hat", "er", "an", "die", "Klei\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem Feinde was geflickt.", "tokens": ["Dem", "Fein\u00b7de", "was", "ge\u00b7flickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er stieg zu hohen Ehren,", "tokens": ["Er", "stieg", "zu", "ho\u00b7hen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Feldmarschall ward er gar,", "tokens": ["Feld\u00b7mar\u00b7schall", "ward", "er", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es mocht' ihn wenig kehren,", "tokens": ["Es", "mocht'", "ihn", "we\u00b7nig", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df einst er Schneider war;", "tokens": ["Da\u00df", "einst", "er", "Schnei\u00b7der", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur, fand er einen Sp\u00f6tter,", "tokens": ["Nur", ",", "fand", "er", "ei\u00b7nen", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verstund er keinen Spa\u00df", "tokens": ["Ver\u00b7stund", "er", "kei\u00b7nen", "Spa\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und brummte: \u00bbF\u00fcr Hundsf\u00f6tter", "tokens": ["Und", "brumm\u00b7te", ":", "\u00bb", "F\u00fcr", "Hunds\u00b7f\u00f6t\u00b7ter"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist hier mein Ellenma\u00df.\u00ab", "tokens": ["Ist", "hier", "mein", "El\u00b7len\u00b7ma\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Krank lag in seinem Schlosse", "tokens": ["Krank", "lag", "in", "sei\u00b7nem", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der greise Feldmarschall,", "tokens": ["Der", "grei\u00b7se", "Feld\u00b7mar\u00b7schall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Keins seiner Lieblingsrosse", "tokens": ["Keins", "sei\u00b7ner", "Lieb\u00b7lings\u00b7ros\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kam wiehernd aus dem Stall;", "tokens": ["Kam", "wie\u00b7hernd", "aus", "dem", "Stall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Er sprach: \u00bbAls alter Schneider", "tokens": ["Er", "sprach", ":", "\u00bb", "Als", "al\u00b7ter", "Schnei\u00b7der"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "KOUS", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wei\u00df ich seit langer Zeit,", "tokens": ["Wei\u00df", "ich", "seit", "lan\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man wechselt seine Kleider \u2013", "tokens": ["Man", "wech\u00b7selt", "sei\u00b7ne", "Klei\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch hab' ich des nicht Leid.", "tokens": ["Auch", "hab'", "ich", "des", "nicht", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es fehlt der alten H\u00fclle", "tokens": ["Es", "fehlt", "der", "al\u00b7ten", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Breite schon und L\u00e4ng',", "tokens": ["In", "Brei\u00b7te", "schon", "und", "L\u00e4ng'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Geist tritt in die F\u00fclle,", "tokens": ["Der", "Geist", "tritt", "in", "die", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Leib wird ihm zu eng;", "tokens": ["Der", "Leib", "wird", "ihm", "zu", "eng", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gesegnet sei dein Wille,", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "dein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herr Gott, in letzter Not!\u00ab", "tokens": ["Herr", "Gott", ",", "in", "letz\u00b7ter", "Not", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sprach's und wurde stille \u2013", "tokens": ["Er", "sprach's", "und", "wur\u00b7de", "stil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der alte Held war tot.", "tokens": ["Der", "al\u00b7te", "Held", "war", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}