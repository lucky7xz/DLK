{"textgrid.poem.40527": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder seh' ein Kreuz ich ragen,", "tokens": ["Wie\u00b7der", "seh'", "ein", "Kreuz", "ich", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Ach, ich sah schon ihrer viel! \u2013", "tokens": ["\u2013", "Ach", ",", "ich", "sah", "schon", "ih\u00b7rer", "viel", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein Wandersmann, erschlagen,", "tokens": ["Wo", "ein", "Wan\u00b7ders\u00b7mann", ",", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterm Dolch des Meuchlers fiel!", "tokens": ["Un\u00b7term", "Dolch", "des", "Meuch\u00b7lers", "fiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nacktes Kreuz, er sah dich sprossen", "tokens": ["Nack\u00b7tes", "Kreuz", ",", "er", "sah", "dich", "spros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch als gr\u00fcnen schlanken Baum,", "tokens": ["Noch", "als", "gr\u00fc\u00b7nen", "schlan\u00b7ken", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von deinem Duft umflossen", "tokens": ["Und", "von", "dei\u00b7nem", "Duft", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schritt er hin im Fr\u00fchlingstraum.", "tokens": ["Schritt", "er", "hin", "im", "Fr\u00fch\u00b7lings\u00b7traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du allein sahst ihn verbluten,", "tokens": ["Du", "al\u00b7lein", "sahst", "ihn", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Einsam, fremd und unbekannt", "tokens": ["Ein\u00b7sam", ",", "fremd", "und", "un\u00b7be\u00b7kannt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf deinen Bl\u00fcthen ruhten", "tokens": ["Und", "auf", "dei\u00b7nen", "Bl\u00fc\u00b7then", "ruh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Blick' im Tod gebannt.", "tokens": ["Sei\u00b7ne", "Blick'", "im", "Tod", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du selbst, gef\u00e4llt, erschlagen,", "tokens": ["Und", "du", "selbst", ",", "ge\u00b7f\u00e4llt", ",", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fctest jetzt den Schreckensort;", "tokens": ["H\u00fc\u00b7test", "jetzt", "den", "Schre\u00b7cken\u00b7sort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein Denkmal mu\u00dft du ragen", "tokens": ["Als", "ein", "Denk\u00b7mal", "mu\u00dft", "du", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr so grausen Doppelmord.", "tokens": ["F\u00fcr", "so", "grau\u00b7sen", "Dop\u00b7pel\u00b7mord", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur der Vogel, der im Wipfel", "tokens": ["Nur", "der", "Vo\u00b7gel", ",", "der", "im", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NE", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Laubs dich preisend sang,", "tokens": ["Dei\u00b7nes", "Laubs", "dich", "prei\u00b7send", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Kreuzes nacktem Gipfel", "tokens": ["Auf", "des", "Kreu\u00b7zes", "nack\u00b7tem", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagt dein Todtenlied er bang.", "tokens": ["Klagt", "dein", "Tod\u00b7ten\u00b7lied", "er", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ein Rosenstrauch, als solle", "tokens": ["Und", "ein", "Ro\u00b7sen\u00b7strauch", ",", "als", "sol\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fccken er die\u00df kahle Holz,", "tokens": ["Schm\u00fc\u00b7cken", "er", "die\u00df", "kah\u00b7le", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klimmt hinan und pflanzt die volle", "tokens": ["Klimmt", "hi\u00b7nan", "und", "pflanzt", "die", "vol\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ros' am Kreuzesgiebel stolz.", "tokens": ["Ros'", "am", "Kreu\u00b7zes\u00b7gie\u00b7bel", "stolz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Orangenbaum, als wolle", "tokens": ["Ein", "O\u00b7ran\u00b7gen\u00b7baum", ",", "als", "wol\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bergen er die\u00df Kreuz der Schmach,", "tokens": ["Ber\u00b7gen", "er", "die\u00df", "Kreuz", "der", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fcllt es in das goldfruchtvolle,", "tokens": ["H\u00fcllt", "es", "in", "das", "gold\u00b7frucht\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Silberbl\u00fcthenreiche Dach.", "tokens": ["Sil\u00b7ber\u00b7bl\u00fc\u00b7then\u00b7rei\u00b7che", "Dach", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch es denken fern die Lieben", "tokens": ["Doch", "es", "den\u00b7ken", "fern", "die", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch des Manns, der sie verlie\u00df,", "tokens": ["Noch", "des", "Manns", ",", "der", "sie", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als es ihn nach S\u00fcd getrieben", "tokens": ["Als", "es", "ihn", "nach", "S\u00fcd", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die\u00df Bl\u00fcthenparadies.", "tokens": ["In", "die\u00df", "Bl\u00fc\u00b7then\u00b7pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und den L\u00e4ngstverschollnen sehen", "tokens": ["Und", "den", "L\u00e4ngst\u00b7ver\u00b7scholl\u00b7nen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie in bl\u00fchender Gestalt", "tokens": ["Sie", "in", "bl\u00fc\u00b7hen\u00b7der", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern noch durch die Rosen gehen,", "tokens": ["Fern", "noch", "durch", "die", "Ro\u00b7sen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummernd ruhn im Lorberwald.", "tokens": ["Schlum\u00b7mernd", "ruhn", "im", "Lor\u00b7ber\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}