{"dta.poem.1130": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "16. Die Sch\u00f6ne Kahle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ob du/ O Nymphe/ gleich der Haare bist beraubt/ ", "tokens": ["Ob", "du", "/", "O", "Nym\u00b7phe", "/", "gleich", "der", "Haa\u00b7re", "bist", "be\u00b7raubt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "NN", "$(", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist es darum nicht um deinen Ruhm geschehen.", "tokens": ["So", "ist", "es", "da\u00b7rum", "nicht", "um", "dei\u00b7nen", "Ruhm", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "L\u00e4st sich die Rose nie ohn Blatt und D\u00f6rner sehen?", "tokens": ["L\u00e4st", "sich", "die", "Ro\u00b7se", "nie", "ohn", "Blatt", "und", "D\u00f6r\u00b7ner", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist nicht der junge Wald im Mertzen unbelaubt?", "tokens": ["Ist", "nicht", "der", "jun\u00b7ge", "Wald", "im", "Mert\u00b7zen", "un\u00b7be\u00b7laubt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bestrau\u00dfter Sternen Haar (Erfahrung hats beglaubt/)", "tokens": ["Be\u00b7strau\u00df\u00b7ter", "Ster\u00b7nen", "Haar", "(", "Er\u00b7fah\u00b7rung", "hats", "be\u00b7glaubt", "/", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "NN", "$(", "NN", "VAFIN", "VVPP", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fchrt nach sich Ungl\u00fcck/ Sturm und toller Winde Wehen.", "tokens": ["F\u00fchrt", "nach", "sich", "Un\u00b7gl\u00fcck", "/", "Sturm", "und", "tol\u00b7ler", "Win\u00b7de", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "NN", "$(", "NN", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer pflegt den holden Ku\u00df des Gl\u00fcckes zu verschm\u00e4hen/", "tokens": ["Wer", "pflegt", "den", "hol\u00b7den", "Ku\u00df", "des", "Gl\u00fc\u00b7ckes", "zu", "ver\u00b7schm\u00e4\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob sich von hintenzu gleich zeigt sein kahles Haubt?", "tokens": ["Ob", "sich", "von", "hin\u00b7ten\u00b7zu", "gleich", "zeigt", "sein", "kah\u00b7les", "Haubt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn andre mit viel Furcht und Schmertz die Haut bepfl\u00fc-", "tokens": ["Wenn", "and\u00b7re", "mit", "viel", "Furcht", "und", "Schmertz", "die", "Haut", "be\u00b7pfl\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "KON", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "cken/ ", "tokens": ["cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.11": {"text": "So bleibstu ungeraufft/ dein Alter unentdeckt.", "tokens": ["So", "bleibs\u00b7tu", "un\u00b7ger\u00b7aufft", "/", "dein", "Al\u00b7ter", "un\u00b7ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$(", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kan wohl ein Zeug von Schwei\u00df und F\u00e4ulnis ausgeheckt", "tokens": ["Kan", "wohl", "ein", "Zeug", "von", "Schwei\u00df", "und", "F\u00e4ul\u00b7nis", "aus\u00b7ge\u00b7heckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Auswurff der Natur/ verstellen oder schm\u00fccken?", "tokens": ["Ein", "Aus\u00b7wurff", "der", "Na\u00b7tur", "/", "ver\u00b7stel\u00b7len", "o\u00b7der", "schm\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Das Schiff ist in Gefahr/ wenn Flagg\u2019 und Seegel fliegen:", "tokens": ["Das", "Schiff", "ist", "in", "Ge\u00b7fahr", "/", "wenn", "Flag\u00b7g'", "und", "See\u00b7gel", "flie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$(", "KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Man sieht es deren blo\u00df im sichern Hafen liegen.", "tokens": ["Man", "sieht", "es", "de\u00b7ren", "blo\u00df", "im", "si\u00b7chern", "Ha\u00b7fen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PDS", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}