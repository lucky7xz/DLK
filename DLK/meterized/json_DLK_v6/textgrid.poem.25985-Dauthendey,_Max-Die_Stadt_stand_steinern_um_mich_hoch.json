{"textgrid.poem.25985": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Stadt stand steinern um mich hoch", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging so hin in den dunkelnden Tag", "tokens": ["Ich", "ging", "so", "hin", "in", "den", "dun\u00b7keln\u00b7den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und z\u00e4hlte Stein bei Pflasterstein.", "tokens": ["Und", "z\u00e4hl\u00b7te", "Stein", "bei", "Pflas\u00b7ter\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht mal mein Schatten zur Seit' mir lag,", "tokens": ["Nicht", "mal", "mein", "Schat\u00b7ten", "zur", "Seit'", "mir", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die Leere war gro\u00df und Leere macht klein.", "tokens": ["Die", "Lee\u00b7re", "war", "gro\u00df", "und", "Lee\u00b7re", "macht", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Stadt stand steinern um mich hoch,", "tokens": ["Die", "Stadt", "stand", "stei\u00b7nern", "um", "mich", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Flu\u00df trieb Fluten durchs Br\u00fcckenjoch,", "tokens": ["Der", "Flu\u00df", "trieb", "Flu\u00b7ten", "durchs", "Br\u00fc\u00b7cken\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als ob er das Liebste sich suchen mu\u00df,", "tokens": ["Als", "ob", "er", "das", "Liebs\u00b7te", "sich", "su\u00b7chen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als m\u00fc\u00dften die Wasser sich sputen.", "tokens": ["Als", "m\u00fc\u00df\u00b7ten", "die", "Was\u00b7ser", "sich", "spu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Mein Herz mir nicht von der Stelle mehr kroch,", "tokens": ["Mein", "Herz", "mir", "nicht", "von", "der", "Stel\u00b7le", "mehr", "kroch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKNEG", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lag auf der Schwelle von Tag und Nacht,", "tokens": ["Lag", "auf", "der", "Schwel\u00b7le", "von", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ich habe es kaum zum Gehen gebracht.", "tokens": ["Ich", "ha\u00b7be", "es", "kaum", "zum", "Ge\u00b7hen", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Den Tag, den lockten die Berge fort,", "tokens": ["Den", "Tag", ",", "den", "lock\u00b7ten", "die", "Ber\u00b7ge", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie hockten dunkel wie Zauberer dort,", "tokens": ["Sie", "hock\u00b7ten", "dun\u00b7kel", "wie", "Zau\u00b7be\u00b7rer", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Berg machte Lachen aus Weinen.", "tokens": ["Ein", "Berg", "mach\u00b7te", "La\u00b7chen", "aus", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sein Blick konnt' unter Gru\u00df und Genick", "tokens": ["Sein", "Blick", "konnt'", "un\u00b7ter", "Gru\u00df", "und", "Ge\u00b7nick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die Toten lebendig mir machen.", "tokens": ["Die", "To\u00b7ten", "le\u00b7ben\u00b7dig", "mir", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Die Toten, die unter den Bl\u00e4ttern liegen,", "tokens": ["Die", "To\u00b7ten", ",", "die", "un\u00b7ter", "den", "Bl\u00e4t\u00b7tern", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die stiegen vom Berg mit z\u00e4rtlichen Wangen.", "tokens": ["Die", "stie\u00b7gen", "vom", "Berg", "mit", "z\u00e4rt\u00b7li\u00b7chen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und eine kam \u00fcber die Br\u00fccke gegangen,", "tokens": ["Und", "ei\u00b7ne", "kam", "\u00fc\u00b7ber", "die", "Br\u00fc\u00b7cke", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und sehen konnten es selbst die Laternen,", "tokens": ["Und", "se\u00b7hen", "konn\u00b7ten", "es", "selbst", "die", "La\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Tote blieb hei\u00df mir am Herzen stehen.", "tokens": ["Die", "To\u00b7te", "blieb", "hei\u00df", "mir", "am", "Her\u00b7zen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "mete