{"dta.poem.7142": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie aber, und warum wir eben die Jdeen", "tokens": ["Wie", "a\u00b7ber", ",", "und", "wa\u00b7rum", "wir", "e\u00b7ben", "die", "Jdeen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KON", "PWAV", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und and\u2019re nicht an deren Stelle sehen;", "tokens": ["Und", "an\u00b7d'\u00b7re", "nicht", "an", "de\u00b7ren", "Stel\u00b7le", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKNEG", "APPR", "PRELAT", "NN", "VVINF", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Warum so leicht die, welche l\u00e4ngst vergangen,", "tokens": ["Wa\u00b7rum", "so", "leicht", "die", ",", "wel\u00b7che", "l\u00e4ngst", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als unl\u00e4ngst erst gesehne Bilder kommen;", "tokens": ["Als", "un\u00b7l\u00e4ngst", "erst", "ge\u00b7seh\u00b7ne", "Bil\u00b7der", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob sie, als wie ein Schall,", "tokens": ["Ob", "sie", ",", "als", "wie", "ein", "Schall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der durch den Wiederhall", "tokens": ["Der", "durch", "den", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zur\u00fccke kehrt, von uns vernommen,", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "kehrt", ",", "von", "uns", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und so geh\u00f6ret wird, wie ihn der Mund gebohren,", "tokens": ["Und", "so", "ge\u00b7h\u00f6\u00b7ret", "wird", ",", "wie", "ihn", "der", "Mund", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ob er sich gleich schon eine Zeit verlohren;", "tokens": ["Ob", "er", "sich", "gleich", "schon", "ei\u00b7ne", "Zeit", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ob, sag\u2019 ich, etwan auch auf gleiche Weise,", "tokens": ["Ob", ",", "sag'", "ich", ",", "et\u00b7wan", "auch", "auf", "glei\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Bilder, die nicht mehr zu sehen,", "tokens": ["Die", "Bil\u00b7der", ",", "die", "nicht", "mehr", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "An statt vollkommen zu vergehen,", "tokens": ["An", "statt", "voll\u00b7kom\u00b7men", "zu", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Noch bleiben, und zur\u00fccke kehren;", "tokens": ["Noch", "blei\u00b7ben", ",", "und", "zu\u00b7r\u00fc\u00b7cke", "keh\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ist nicht so leichte zu erkl\u00e4ren.", "tokens": ["Ist", "nicht", "so", "leich\u00b7te", "zu", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht minder, ob und wie so Seel und Geist,", "tokens": ["Nicht", "min\u00b7der", ",", "ob", "und", "wie", "so", "Seel", "und", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "KON", "PWAV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Bey Tr\u00e4umen sich gesch\u00e4fftig weist,", "tokens": ["Bey", "Tr\u00e4u\u00b7men", "sich", "ge\u00b7sch\u00e4ff\u00b7tig", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da wir im Traum offt ernsthafft disputiren,", "tokens": ["Da", "wir", "im", "Traum", "offt", "ernst\u00b7hafft", "dis\u00b7pu\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und, wie bereits gesagt, auch gar poetisiren.", "tokens": ["Und", ",", "wie", "be\u00b7reits", "ge\u00b7sagt", ",", "auch", "gar", "po\u00b7e\u00b7ti\u00b7si\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "VVPP", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}