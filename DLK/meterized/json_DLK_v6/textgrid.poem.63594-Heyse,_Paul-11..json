{"textgrid.poem.63594": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommst du nun auch zu mir herangeschlichen,", "tokens": ["Kommst", "du", "nun", "auch", "zu", "mir", "her\u00b7an\u00b7ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein alter Hund, mit einer Beileidsmiene", "tokens": ["Mein", "al\u00b7ter", "Hund", ",", "mit", "ei\u00b7ner", "Bei\u00b7leids\u00b7mie\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ruhst nicht, bis ich dir das Fell gestrichen?", "tokens": ["Und", "ruhst", "nicht", ",", "bis", "ich", "dir", "das", "Fell", "ge\u00b7stri\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du senkst den Kopf so traurig, da\u00df es schiene,", "tokens": ["Du", "senkst", "den", "Kopf", "so", "trau\u00b7rig", ",", "da\u00df", "es", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als f\u00fchltest du, wie dieses Gramgeschick", "tokens": ["Als", "f\u00fchl\u00b7test", "du", ",", "wie", "die\u00b7ses", "Gram\u00b7ge\u00b7schick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "PWAV", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mitleid der stummen Kreatur verdiene.", "tokens": ["Mit\u00b7leid", "der", "stum\u00b7men", "Kre\u00b7a\u00b7tur", "ver\u00b7die\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jawohl! Nun trifft dich nimmermehr der Blick,", "tokens": ["Ja\u00b7wohl", "!", "Nun", "trifft", "dich", "nim\u00b7mer\u00b7mehr", "der", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Lockruf deines kleinen Spielgenossen;", "tokens": ["Der", "Lock\u00b7ruf", "dei\u00b7nes", "klei\u00b7nen", "Spiel\u00b7ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Kein ros'ges H\u00e4ndchen zaust dir das Genick.", "tokens": ["Kein", "ros'\u00b7ges", "H\u00e4nd\u00b7chen", "zaust", "dir", "das", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nicht gibst du dich, halb freundlich, halb verdrossen,", "tokens": ["Nicht", "gibst", "du", "dich", ",", "halb", "freund\u00b7lich", ",", "halb", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PRF", "$,", "ADJD", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zum Reiten her und wirst's nie \u00fcberdr\u00fcssig,", "tokens": ["Zum", "Rei\u00b7ten", "her", "und", "wir\u00b7st's", "nie", "\u00fc\u00b7berd\u00b7r\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Zu dulden willig tausend Kinderpossen.", "tokens": ["Zu", "dul\u00b7den", "wil\u00b7lig", "tau\u00b7send", "Kin\u00b7der\u00b7pos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wozu noch lebst du jetzt, als da\u00df du m\u00fc\u00dfig", "tokens": ["Wo\u00b7zu", "noch", "lebst", "du", "jetzt", ",", "als", "da\u00df", "du", "m\u00fc\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dich sonnst und schnarchst und Fliegen f\u00e4ngst im Traum,", "tokens": ["Dich", "sonnst", "und", "schnarchst", "und", "Flie\u00b7gen", "f\u00e4ngst", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "VVFIN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Welt und dir und mir h\u00f6chst \u00fcberfl\u00fcssig \u2013", "tokens": ["Der", "Welt", "und", "dir", "und", "mir", "h\u00f6chst", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "KON", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und atmest fort, und f\u00fcllst noch deinen Raum", "tokens": ["Und", "at\u00b7mest", "fort", ",", "und", "f\u00fcllst", "noch", "dei\u00b7nen", "Raum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mit tr\u00e4ger Masse, saugst noch Lebensluft \u2013 \u2013", "tokens": ["Mit", "tr\u00e4\u00b7ger", "Mas\u00b7se", ",", "saugst", "noch", "Le\u00b7bens\u00b7luft", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "ADV", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und unser Kind \u2013 o ich ertrag' es kaum!", "tokens": ["Und", "un\u00b7ser", "Kind", "\u2013", "o", "ich", "er\u00b7trag'", "es", "kaum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "FM", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mir aus den Augen, heuchlerischer Schuft!", "tokens": ["Mir", "aus", "den", "Au\u00b7gen", ",", "heuch\u00b7le\u00b7ri\u00b7scher", "Schuft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Dein Winseln tr\u00fcgt mich nicht. Ich wei\u00df, Geselle,", "tokens": ["Dein", "Win\u00b7seln", "tr\u00fcgt", "mich", "nicht", ".", "Ich", "wei\u00df", ",", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Dich r\u00fchrt kein Schauer an aus ihrer Gruft.", "tokens": ["Dich", "r\u00fchrt", "kein", "Schau\u00b7er", "an", "aus", "ih\u00b7rer", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Als kaum erblichen ihres Auges Helle,", "tokens": ["Als", "kaum", "er\u00b7bli\u00b7chen", "ih\u00b7res", "Au\u00b7ges", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Da lagst du, Wicht, als w\u00e4re nichts geschehn,", "tokens": ["Da", "lagst", "du", ",", "Wicht", ",", "als", "w\u00e4\u00b7re", "nichts", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "KOKOM", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und schliefst auf ihres Sterbezimmers Schwelle.", "tokens": ["Und", "schliefst", "auf", "ih\u00b7res", "Ster\u00b7be\u00b7zim\u00b7mers", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und als man sie hinaustrug und in Wehn", "tokens": ["Und", "als", "man", "sie", "hin\u00b7aust\u00b7rug", "und", "in", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "PPER", "VVFIN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Das Mutterherz und meines schier gebrochen,", "tokens": ["Das", "Mut\u00b7ter\u00b7herz", "und", "mei\u00b7nes", "schier", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Sah ich dich lungernd vor der T\u00fcre stehn.", "tokens": ["Sah", "ich", "dich", "lun\u00b7gernd", "vor", "der", "T\u00fc\u00b7re", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Du nagtest gierig einen leckern Knochen", "tokens": ["Du", "nag\u00b7test", "gie\u00b7rig", "ei\u00b7nen", "le\u00b7ckern", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und knurrtest scheel die fremden M\u00e4nner an,", "tokens": ["Und", "knurr\u00b7test", "scheel", "die", "frem\u00b7den", "M\u00e4n\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Die im gesch\u00e4ft'gen Fra\u00df dich unterbrochen.", "tokens": ["Die", "im", "ge\u00b7sch\u00e4ft'\u00b7gen", "Fra\u00df", "dich", "un\u00b7ter\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und jetzt scheinheilig schleichst du dich heran?", "tokens": ["Und", "jetzt", "schein\u00b7hei\u00b7lig", "schleichst", "du", "dich", "he\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Hinaus mit dir! Du bist ein Tier. Wir beide", "tokens": ["Hin\u00b7aus", "mit", "dir", "!", "Du", "bist", "ein", "Tier", ".", "Wir", "bei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$.", "PPER", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Sind andern Seelenm\u00e4chten untertan.", "tokens": ["Sind", "an\u00b7dern", "See\u00b7len\u00b7m\u00e4ch\u00b7ten", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Herzlos, wie die Natur, bei Menschenleide,", "tokens": ["Herz\u00b7los", ",", "wie", "die", "Na\u00b7tur", ",", "bei", "Men\u00b7schen\u00b7lei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Stimmst du in unsern ", "tokens": ["Stimmst", "du", "in", "un\u00b7sern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Du wei\u00dft, am Festtag gibt es fette Weide.", "tokens": ["Du", "wei\u00dft", ",", "am", "Fest\u00b7tag", "gibt", "es", "fet\u00b7te", "Wei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Des Menschen ", "tokens": ["Des", "Men\u00b7schen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Kein Gott, kein Tier. Der Kummer ist erlaucht,", "tokens": ["Kein", "Gott", ",", "kein", "Tier", ".", "Der", "Kum\u00b7mer", "ist", "er\u00b7laucht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und du, so treu du winselst, bist gemein.", "tokens": ["Und", "du", ",", "so", "treu", "du", "win\u00b7selst", ",", "bist", "ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Gram kennt kein Gestern. Du bist eingetaucht", "tokens": ["Gram", "kennt", "kein", "Ge\u00b7stern", ".", "Du", "bist", "ein\u00b7ge\u00b7taucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.41": {"text": "In dumpfes Heute. Hast du dich verkrochen", "tokens": ["In", "dum\u00b7pfes", "Heu\u00b7te", ".", "Hast", "du", "dich", "ver\u00b7kro\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Aus Furcht vor meinem Zorn? Der ist verraucht.", "tokens": ["Aus", "Furcht", "vor", "mei\u00b7nem", "Zorn", "?", "Der", "ist", "ver\u00b7raucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$.", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Troll dich hinaus und nage deinen Knochen!", "tokens": ["Troll", "dich", "hin\u00b7aus", "und", "na\u00b7ge", "dei\u00b7nen", "Kno\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}