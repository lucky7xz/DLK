{"textgrid.poem.34878": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mehr barfu\u00df sollst du traben,", "tokens": ["Nicht", "mehr", "bar\u00b7fu\u00df", "sollst", "du", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutsche Freiheit, durch die S\u00fcmpfe,", "tokens": ["Deut\u00b7sche", "Frei\u00b7heit", ",", "durch", "die", "S\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich kommst du auf die Str\u00fcmpfe,", "tokens": ["End\u00b7lich", "kommst", "du", "auf", "die", "Str\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auch Stiefeln sollst du haben!", "tokens": ["Und", "auch", "Stie\u00b7feln", "sollst", "du", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem Haupte sollst du tragen", "tokens": ["Auf", "dem", "Haup\u00b7te", "sollst", "du", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine warme Pudelm\u00fctze,", "tokens": ["Ei\u00b7ne", "war\u00b7me", "Pu\u00b7del\u00b7m\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie dir die Ohren sch\u00fctze", "tokens": ["Da\u00df", "sie", "dir", "die", "Oh\u00b7ren", "sch\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den kalten Wintertagen.", "tokens": ["In", "den", "kal\u00b7ten", "Win\u00b7ter\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bekommst sogar zu essen \u2013", "tokens": ["Du", "be\u00b7kommst", "so\u00b7gar", "zu", "es\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine gro\u00dfe Zukunft naht dir! \u2013", "tokens": ["Ei\u00b7ne", "gro\u00b7\u00dfe", "Zu\u00b7kunft", "naht", "dir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "La\u00df dich nur vom welschen Satyr", "tokens": ["La\u00df", "dich", "nur", "vom", "wel\u00b7schen", "Sa\u00b7tyr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Nicht verlocken zu Exzessen!", "tokens": ["Nicht", "ver\u00b7lo\u00b7cken", "zu", "Ex\u00b7zes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Werde nur nicht dreist und dreister!", "tokens": ["Wer\u00b7de", "nur", "nicht", "dreist", "und", "dreis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Setz nicht den Respekt beiseiten", "tokens": ["Setz", "nicht", "den", "Res\u00b7pekt", "bei\u00b7sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor den hohen Obrigkeiten", "tokens": ["Vor", "den", "ho\u00b7hen", "Ob\u00b7rig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem Herren B\u00fcrgermeister!", "tokens": ["Und", "dem", "Her\u00b7ren", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}