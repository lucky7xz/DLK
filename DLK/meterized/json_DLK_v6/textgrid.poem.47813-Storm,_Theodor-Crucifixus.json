{"textgrid.poem.47813": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Crucifixus", "genre": "verse", "period": "N.A.", "pub_year": 1865, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Kreuz hing sein gequ\u00e4lt Gebeine,", "tokens": ["Am", "Kreuz", "hing", "sein", "ge\u00b7qu\u00e4lt", "Ge\u00b7bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Blut besudelt und geschm\u00e4ht;", "tokens": ["Mit", "Blut", "be\u00b7su\u00b7delt", "und", "ge\u00b7schm\u00e4ht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann hat die stets jungfr\u00e4ulich reine", "tokens": ["Dann", "hat", "die", "stets", "jung\u00b7fr\u00e4u\u00b7lich", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Natur das Schreckensbild verweht.", "tokens": ["Na\u00b7tur", "das", "Schre\u00b7ckens\u00b7bild", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch die sich seine J\u00fcnger nannten,", "tokens": ["Doch", "die", "sich", "sei\u00b7ne", "J\u00fcn\u00b7ger", "nann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die formten es in Erz und Stein,", "tokens": ["Die", "form\u00b7ten", "es", "in", "Erz", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stellten's in des Tempels D\u00fcster", "tokens": ["Und", "stell\u00b7ten's", "in", "des", "Tem\u00b7pels", "D\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in die lichte Flur hinein.", "tokens": ["Und", "in", "die", "lich\u00b7te", "Flur", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So, jedem reinen Aug ein Schauder,", "tokens": ["So", ",", "je\u00b7dem", "rei\u00b7nen", "Aug", "ein", "Schau\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PIAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ragt es herein in unsre Zeit;", "tokens": ["Ragt", "es", "her\u00b7ein", "in", "uns\u00b7re", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verewigend den alten Frevel,", "tokens": ["Ve\u00b7re\u00b7wi\u00b7gend", "den", "al\u00b7ten", "Fre\u00b7vel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ein Bild der Unvers\u00f6hnlichkeit.", "tokens": ["Ein", "Bild", "der", "Un\u00b7ver\u00b7s\u00f6hn\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}