{"textgrid.poem.28532": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soldate k\u00fcmmt vom Sold; die au\u00dfge\u00fcbten Thaten,", "tokens": ["Sol\u00b7da\u00b7te", "k\u00fcmmt", "vom", "Sold", ";", "die", "au\u00df\u00b7ge\u00b7\u00fcb\u00b7ten", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sie auff freyer Stra\u00df in Hof und Haus ver\u00fcbet,", "tokens": ["Die", "sie", "auff", "frey\u00b7er", "Stra\u00df", "in", "Hof", "und", "Haus", "ver\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verdienten schlechten Sold. Was noch sich ietzt begibet,", "tokens": ["Ver\u00b7dien\u00b7ten", "schlech\u00b7ten", "Sold", ".", "Was", "noch", "sich", "ietzt", "be\u00b7gi\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$.", "PWS", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bringt Sold, dadurch sie sind Galgaten und Radaten.", "tokens": ["Bringt", "Sold", ",", "da\u00b7durch", "sie", "sind", "Gal\u00b7ga\u00b7ten", "und", "Ra\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PAV", "PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}