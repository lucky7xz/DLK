{"dta.poem.11802": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die dumme Fabel sagt, des Pfauen stolz Gefieder,", "tokens": ["Die", "dum\u00b7me", "Fa\u00b7bel", "sagt", ",", "des", "Pfau\u00b7en", "stolz", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht er auf seinen Fu\u00df, sink' ihm vor Scham danieder.", "tokens": ["Sieht", "er", "auf", "sei\u00b7nen", "Fu\u00df", ",", "sink'", "ihm", "vor", "Scham", "da\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer aber hat das Rad des Pfauen je gesehn,", "tokens": ["Wer", "a\u00b7ber", "hat", "das", "Rad", "des", "Pfau\u00b7en", "je", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf den Fu\u00df gemerkt, worauf es mochte stehn?", "tokens": ["Und", "auf", "den", "Fu\u00df", "ge\u00b7merkt", ",", "wo\u00b7rauf", "es", "moch\u00b7te", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn die Bewundrung nun er sieht sein Rad betrachten", "tokens": ["Wenn", "die", "Be\u00b7wund\u00b7rung", "nun", "er", "sieht", "sein", "Rad", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcbersehn den Fu\u00df, sollt' er ihn selbst beachten?", "tokens": ["Und", "\u00fc\u00b7ber\u00b7sehn", "den", "Fu\u00df", ",", "sollt'", "er", "ihn", "selbst", "be\u00b7ach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Sonne, die mit Lust vom Farbenbild betrogen,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "mit", "Lust", "vom", "Far\u00b7ben\u00b7bild", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich sieht im Pfauenrad alswie im Regenbogen,", "tokens": ["Sich", "sieht", "im", "Pfau\u00b7en\u00b7rad", "als\u00b7wie", "im", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Merkt nicht, da\u00df hier im Koth der sch\u00f6ne Vogel geht,", "tokens": ["Merkt", "nicht", ",", "da\u00df", "hier", "im", "Koth", "der", "sch\u00f6\u00b7ne", "Vo\u00b7gel", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "KOUS", "ADV", "APPRART", "NN", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie dort auf Erdengrund der Himmelsbogen steht.", "tokens": ["Wie", "dort", "auf", "Er\u00b7den\u00b7grund", "der", "Him\u00b7mels\u00b7bo\u00b7gen", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}