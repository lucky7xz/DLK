{"dta.poem.15693": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein freier Will', o Mensch, soll dein nicht seyn und eigen;", "tokens": ["Dein", "frei\u00b7er", "Will'", ",", "o", "Mensch", ",", "soll", "dein", "nicht", "seyn", "und", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$,", "VMFIN", "PPOSAT", "PTKNEG", "VAINF", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn in der Eigenheit will sich Unfreiheit zeigen.", "tokens": ["Denn", "in", "der", "Ei\u00b7gen\u00b7heit", "will", "sich", "Un\u00b7frei\u00b7heit", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "An der Uneigenheit ist Freiheit zu erkennen;", "tokens": ["An", "der", "Un\u00b7ei\u00b7gen\u00b7heit", "ist", "Frei\u00b7heit", "zu", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Was frei in Wahrheit ist, darf keiner eigen nennen.", "tokens": ["Was", "frei", "in", "Wahr\u00b7heit", "ist", ",", "darf", "kei\u00b7ner", "ei\u00b7gen", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NN", "VAFIN", "$,", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von allem, was sich r\u00fchmt der Freiheit, ist auf Erden", "tokens": ["Von", "al\u00b7lem", ",", "was", "sich", "r\u00fchmt", "der", "Frei\u00b7heit", ",", "ist", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "PRF", "VVFIN", "ART", "NN", "$,", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So frei nichts, als, o Mensch, dein Wille frei soll werden.", "tokens": ["So", "frei", "nichts", ",", "als", ",", "o", "Mensch", ",", "dein", "Wil\u00b7le", "frei", "soll", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "$,", "KOUS", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein freier Wille sei nicht eigen dem und dem,", "tokens": ["Dein", "frei\u00b7er", "Wil\u00b7le", "sei", "nicht", "ei\u00b7gen", "dem", "und", "dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "ADJD", "ART", "KON", "ART", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein eigen sei er nicht, so ists Gott angenehm.", "tokens": ["Dein", "ei\u00b7gen", "sei", "er", "nicht", ",", "so", "ists", "Gott", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gott selber will in dir, der deinen Willen schafft;", "tokens": ["Gott", "sel\u00b7ber", "will", "in", "dir", ",", "der", "dei\u00b7nen", "Wil\u00b7len", "schafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "APPR", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Gott zu wollen, ist des freien Willens Kraft.", "tokens": ["Und", "Gott", "zu", "wol\u00b7len", ",", "ist", "des", "frei\u00b7en", "Wil\u00b7lens", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VMINF", "$,", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}