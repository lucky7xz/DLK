{"textgrid.poem.56284": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Gro\u00dfmutter", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgro\u00dfmutter, schl\u00e4fst du? Deine Lippen pflegen", "tokens": ["\u00bb", "gro\u00df\u00b7mut\u00b7ter", ",", "schl\u00e4fst", "du", "?", "Dei\u00b7ne", "Lip\u00b7pen", "pfle\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie betend sich im Schlafe zu bewegen,", "tokens": ["Wie", "be\u00b7tend", "sich", "im", "Schla\u00b7fe", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie bist du heute regungslos und bleich?", "tokens": ["Wie", "bist", "du", "heu\u00b7te", "re\u00b7gungs\u00b7los", "und", "bleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die H\u00e4nde starr auf deiner Brust vereinet,", "tokens": ["Die", "H\u00e4n\u00b7de", "starr", "auf", "dei\u00b7ner", "Brust", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die nicht dein Atem zu erheben scheinet,", "tokens": ["Die", "nicht", "dein", "A\u00b7tem", "zu", "er\u00b7he\u00b7ben", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Marmorbild der Schmerzensmutter gleich.", "tokens": ["Dem", "Mar\u00b7mor\u00b7bild", "der", "Schmer\u00b7zens\u00b7mut\u00b7ter", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Blick auf, erwache, rede! wie betr\u00fcbest", "tokens": ["Blick", "auf", ",", "er\u00b7wa\u00b7che", ",", "re\u00b7de", "!", "wie", "be\u00b7tr\u00fc\u00b7best"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du, Mutter, deine Kinder, die du liebest?", "tokens": ["Du", ",", "Mut\u00b7ter", ",", "dei\u00b7ne", "Kin\u00b7der", ",", "die", "du", "lie\u00b7best", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was taten wir? wir waren beide fromm.", "tokens": ["Was", "ta\u00b7ten", "wir", "?", "wir", "wa\u00b7ren", "bei\u00b7de", "fromm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du z\u00fcrnest uns? du h\u00f6rst nicht unsre Stimmen?", "tokens": ["Du", "z\u00fcr\u00b7nest", "uns", "?", "du", "h\u00f6rst", "nicht", "uns\u00b7re", "Stim\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sieh her! die Lampe flackert im Verglimmen,", "tokens": ["Sieh", "her", "!", "die", "Lam\u00b7pe", "fla\u00b7ckert", "im", "Ver\u00b7glim\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schon das Feuer auf dem Herd verglomm.", "tokens": ["Und", "schon", "das", "Feu\u00b7er", "auf", "dem", "Herd", "ver\u00b7glomm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und willst du Licht und Feuer nicht erhalten,", "tokens": ["Und", "willst", "du", "Licht", "und", "Feu\u00b7er", "nicht", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So m\u00fcssen wir erstarren in dem kalten", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "er\u00b7star\u00b7ren", "in", "dem", "kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und finstren Haus; zu sp\u00e4t erwachst du dann,", "tokens": ["Und", "finst\u00b7ren", "Haus", ";", "zu", "sp\u00e4t", "er\u00b7wachst", "du", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "PTKA", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch wir beharren stumm in deinen Armen", "tokens": ["Auch", "wir", "be\u00b7har\u00b7ren", "stumm", "in", "dei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und k\u00f6nnen nicht an deiner Brust erwarmen,", "tokens": ["Und", "k\u00f6n\u00b7nen", "nicht", "an", "dei\u00b7ner", "Brust", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du rufst die Heiligen vergebens an.", "tokens": ["Du", "rufst", "die", "Hei\u00b7li\u00b7gen", "ver\u00b7ge\u00b7bens", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gro\u00dfmutter, o wie kalt sind deine H\u00e4nde!", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", ",", "o", "wie", "kalt", "sind", "dei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "KOKOM", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir wollen sie in unsern w\u00e4rmen, wende", "tokens": ["Wir", "wol\u00b7len", "sie", "in", "un\u00b7sern", "w\u00e4r\u00b7men", ",", "wen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur deinen Blick uns freundlich wieder zu;", "tokens": ["Nur", "dei\u00b7nen", "Blick", "uns", "freund\u00b7lich", "wie\u00b7der", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da hast du dein Gesangbuch, nimm es wieder,", "tokens": ["Da", "hast", "du", "dein", "Ge\u00b7sang\u00b7buch", ",", "nimm", "es", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du hast es fallen lassen, sing uns Lieder \u2013", "tokens": ["Du", "hast", "es", "fal\u00b7len", "las\u00b7sen", ",", "sing", "uns", "Lie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "VVINF", "$,", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du nimmst es nicht, und nichts erwiderst du?", "tokens": ["Du", "nimmst", "es", "nicht", ",", "und", "nichts", "er\u00b7wi\u00b7derst", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zeig uns, wir waren fromm, uns zu belohnen,", "tokens": ["Zeig", "uns", ",", "wir", "wa\u00b7ren", "fromm", ",", "uns", "zu", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Bild der Bibel, wo die Heil'gen wohnen", "tokens": ["Das", "Bild", "der", "Bi\u00b7bel", ",", "wo", "die", "Heil'\u00b7gen", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beim lieben Gott, umstrahlt von seinem Licht;", "tokens": ["Beim", "lie\u00b7ben", "Gott", ",", "um\u00b7strahlt", "von", "sei\u00b7nem", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erkl\u00e4r uns dann die g\u00f6ttlichen Gebote,", "tokens": ["Er\u00b7kl\u00e4r", "uns", "dann", "die", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sprich vom be\u00dfren Leben nach dem Tode, \u2013", "tokens": ["Und", "sprich", "vom", "be\u00df\u00b7ren", "Le\u00b7ben", "nach", "dem", "To\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ist der Tod? \u2013 du brichst das Schweigen nicht!\u00ab", "tokens": ["Was", "ist", "der", "Tod", "?", "\u2013", "du", "brichst", "das", "Schwei\u00b7gen", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So hallte lange noch der Waisen Klage,", "tokens": ["So", "hall\u00b7te", "lan\u00b7ge", "noch", "der", "Wai\u00b7sen", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht brach ein, sie wich dem jungen Tage,", "tokens": ["Die", "Nacht", "brach", "ein", ",", "sie", "wich", "dem", "jun\u00b7gen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Turm-Uhr ma\u00df die Zeit mit gleichem Schlag;", "tokens": ["Die", "Tur\u00b7mUhr", "ma\u00df", "die", "Zeit", "mit", "glei\u00b7chem", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur offnen T\u00fcre lauschend sah die Kleinen", "tokens": ["Zur", "off\u00b7nen", "T\u00fc\u00b7re", "lau\u00b7schend", "sah", "die", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am Sterbebette knieen, beten, weinen", "tokens": ["Am", "Ster\u00b7be\u00b7bet\u00b7te", "kni\u00b7e\u00b7en", ",", "be\u00b7ten", ",", "wei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ein Wandrer sp\u00e4te noch am andern Tag.", "tokens": ["Ein", "Wand\u00b7rer", "sp\u00e4\u00b7te", "noch", "am", "an\u00b7dern", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}