{"textgrid.poem.36257": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Erster", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00fc\u00dfe Unschuld kindlich sich erfreue,", "tokens": ["Wie", "s\u00fc\u00b7\u00dfe", "Un\u00b7schuld", "kind\u00b7lich", "sich", "er\u00b7freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das soll der Bl\u00fcmchen helles Bunt bedeuten,", "tokens": ["Das", "soll", "der", "Bl\u00fcm\u00b7chen", "hel\u00b7les", "Bunt", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ach! so gern dein gelbes Haar umstreuten,", "tokens": ["Die", "ach", "!", "so", "gern", "dein", "gel\u00b7bes", "Haar", "um\u00b7streu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "XY", "$.", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und demutsvoll dir weih'n die Kindestreue.", "tokens": ["Und", "de\u00b7muts\u00b7voll", "dir", "weih'n", "die", "Kin\u00b7des\u00b7treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Rose nur err\u00f6tet hold vor Reue,", "tokens": ["Die", "Ro\u00b7se", "nur", "er\u00b7r\u00f6\u00b7tet", "hold", "vor", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil sie, da \u00e4ltre Knospen noch sich scheuten,", "tokens": ["Weil", "sie", ",", "da", "\u00e4l\u00b7tre", "Knos\u00b7pen", "noch", "sich", "scheu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ADJA", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Kelch ge\u00f6ffnet schon gleich andern Br\u00e4uten,", "tokens": ["Den", "Kelch", "ge\u00b7\u00f6ff\u00b7net", "schon", "gleich", "an\u00b7dern", "Br\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df lieber Hauch den ihren sanft erneue.", "tokens": ["Da\u00df", "lie\u00b7ber", "Hauch", "den", "ih\u00b7ren", "sanft", "er\u00b7neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "ART", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie sie sch\u00fcchtern bl\u00fcht so bunt umkr\u00e4nzet,", "tokens": ["Und", "wie", "sie", "sch\u00fcch\u00b7tern", "bl\u00fcht", "so", "bunt", "um\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "VVFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So strebt dein junger Sinn in heil'ger Demut,", "tokens": ["So", "strebt", "dein", "jun\u00b7ger", "Sinn", "in", "heil'\u00b7ger", "De\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die innern Reiz' entfaltend auszuhauchen.", "tokens": ["Die", "in\u00b7nern", "Rei\u00b7z'", "ent\u00b7fal\u00b7tend", "aus\u00b7zu\u00b7hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVIZU", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Drum \u00fcberrascht dich oft so s\u00fc\u00dfe Wehmut;", "tokens": ["Drum", "\u00fc\u00b7berr\u00b7ascht", "dich", "oft", "so", "s\u00fc\u00b7\u00dfe", "Weh\u00b7mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo solches Aug' in solchen Perlen gl\u00e4nzet,", "tokens": ["Wo", "sol\u00b7ches", "Aug'", "in", "sol\u00b7chen", "Per\u00b7len", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird sich ein andres bald in Wonne tauchen.", "tokens": ["Wird", "sich", "ein", "and\u00b7res", "bald", "in", "Won\u00b7ne", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}