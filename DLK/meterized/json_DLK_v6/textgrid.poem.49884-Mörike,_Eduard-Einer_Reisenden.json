{"textgrid.poem.49884": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Einer Reisenden", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald an die Ufer des Sees, der uns von ferne die Herzen", "tokens": ["Bald", "an", "die", "U\u00b7fer", "des", "Sees", ",", "der", "uns", "von", "fer\u00b7ne", "die", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADV", "ART", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lockt in jeglichem Jahr, Gl\u00fcckliche! kehrst du zur\u00fcck.", "tokens": ["Lockt", "in", "jeg\u00b7li\u00b7chem", "Jahr", ",", "Gl\u00fcck\u00b7li\u00b7che", "!", "kehrst", "du", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tag und Nacht ist er dein, mit Sonn und Mond, mit der Alpen", "tokens": ["Tag", "und", "Nacht", "ist", "er", "dein", ",", "mit", "Sonn", "und", "Mond", ",", "mit", "der", "Al\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "PPOSAT", "$,", "APPR", "NN", "KON", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Glut und dem trauten Verkehr schwebender Schiffe dazu.", "tokens": ["Glut", "und", "dem", "trau\u00b7ten", "Ver\u00b7kehr", "schwe\u00b7ben\u00b7der", "Schif\u00b7fe", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Denk ich an ihn, gleich wird mir die Seele so weit wie sein lichter", "tokens": ["Denk", "ich", "an", "ihn", ",", "gleich", "wird", "mir", "die", "See\u00b7le", "so", "weit", "wie", "sein", "lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN"], "meter": "+-+--+--+--+-+--", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Spiegel; und bist ", "tokens": ["Spie\u00b7gel", ";", "und", "bist"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "KON", "VAFIN"], "meter": "+--+", "measure": "iambic.di.chol"}}}}}