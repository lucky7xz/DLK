{"textgrid.poem.38038": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Magdeburger Fehde", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbein guten Rath will ich euch geben,", "tokens": ["\u00bb", "ein", "gu\u00b7ten", "Rath", "will", "ich", "euch", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Gottes H\u00fclf wollen wir widerstreben,", "tokens": ["Mit", "Got\u00b7tes", "H\u00fclf", "wol\u00b7len", "wir", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wolln unsre Stadt befestigen,", "tokens": ["Wolln", "uns\u00b7re", "Stadt", "be\u00b7fes\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und harrn damit auch nicht zu lang,", "tokens": ["Und", "harrn", "da\u00b7mit", "auch", "nicht", "zu", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es kommen fremde G\u00e4ste.\u00ab", "tokens": ["Es", "kom\u00b7men", "frem\u00b7de", "G\u00e4s\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Arndt Jordan der Burgermeister genannt,", "tokens": ["Arndt", "Jor\u00b7dan", "der", "Bur\u00b7ger\u00b7meis\u00b7ter", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und Lindow, der auch wohl bekannt,", "tokens": ["Und", "Lin\u00b7dow", ",", "der", "auch", "wohl", "be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben dazu geschworen,", "tokens": ["Sie", "ha\u00b7ben", "da\u00b7zu", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verhegen die Stadt mit Treuen wohl,", "tokens": ["Ver\u00b7he\u00b7gen", "die", "Stadt", "mit", "Treu\u00b7en", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie sind dazu erkoren.", "tokens": ["Sie", "sind", "da\u00b7zu", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Bischof sprach hinwiederum:", "tokens": ["Der", "Bi\u00b7schof", "sprach", "hin\u00b7wie\u00b7de\u00b7rum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdie Feste sollt ihr ganz abthun,", "tokens": ["\u00bb", "die", "Fes\u00b7te", "sollt", "ihr", "ganz", "ab\u00b7thun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die ihr habt aufgerichtet,", "tokens": ["Die", "ihr", "habt", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das will ich von euch haben also,", "tokens": ["Das", "will", "ich", "von", "euch", "ha\u00b7ben", "al\u00b7so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des seyd von mir berichtet.\u00ab", "tokens": ["Des", "seyd", "von", "mir", "be\u00b7rich\u00b7tet", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Pfaffen treiben Wunderspiel,", "tokens": ["Die", "Pfaf\u00b7fen", "trei\u00b7ben", "Wun\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wolltag halten sie zuviel,", "tokens": ["Der", "Woll\u00b7tag", "hal\u00b7ten", "sie", "zu\u00b7viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die haben sich gar betrogen,", "tokens": ["Die", "ha\u00b7ben", "sich", "gar", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beflecken gar ihr eigen Nest,", "tokens": ["Be\u00b7fle\u00b7cken", "gar", "ihr", "ei\u00b7gen", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sind daraus geflogen.", "tokens": ["Und", "sind", "da\u00b7raus", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und dieser Bischof ich merken kann,", "tokens": ["Und", "die\u00b7ser", "Bi\u00b7schof", "ich", "mer\u00b7ken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ist auch wohl ein kluger Mann,", "tokens": ["Das", "ist", "auch", "wohl", "ein", "klu\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wills also bewinden,", "tokens": ["Ich", "wills", "al\u00b7so", "be\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Welch Vogel sich selbst die Federn ausrupft,", "tokens": ["Welch", "Vo\u00b7gel", "sich", "selbst", "die", "Fe\u00b7dern", "aus\u00b7rupft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den wird der Winter zwingen.", "tokens": ["Den", "wird", "der", "Win\u00b7ter", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbgedenke edler F\u00fcrste gut,", "tokens": ["\u00bb", "ge\u00b7den\u00b7ke", "ed\u00b7ler", "F\u00fcrs\u00b7te", "gut", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedenkt an euren eignen Muth,", "tokens": ["Ge\u00b7denkt", "an", "eu\u00b7ren", "eig\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00fcrzlich will ichs entdecken,", "tokens": ["K\u00fcrz\u00b7lich", "will", "ichs", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die edele Stadt Magdeburg", "tokens": ["Die", "e\u00b7de\u00b7le", "Stadt", "Mag\u00b7de\u00b7burg"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist frey auf allen Ecken.\u00ab", "tokens": ["Ist", "frey", "auf", "al\u00b7len", "E\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Burgermeister also sprach,", "tokens": ["Der", "Bur\u00b7ger\u00b7meis\u00b7ter", "al\u00b7so", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als er vor die Gemeinde trat:", "tokens": ["Als", "er", "vor", "die", "Ge\u00b7mein\u00b7de", "trat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "\u00bbberichtet, denn ich frage,", "tokens": ["\u00bb", "be\u00b7rich\u00b7tet", ",", "denn", "ich", "fra\u00b7ge", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns will ein Krieg hieraus entstehn.", "tokens": ["Uns", "will", "ein", "Krieg", "hier\u00b7aus", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was thut ihr hiezu sagen.\u00ab", "tokens": ["Was", "thut", "ihr", "hie\u00b7zu", "sa\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Gemeinde sprach auch wiederum,", "tokens": ["Die", "Ge\u00b7mein\u00b7de", "sprach", "auch", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gebt euren treuen Rath dazu,", "tokens": ["Gebt", "eu\u00b7ren", "treu\u00b7en", "Rath", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dabey so wolln wir bleiben,", "tokens": ["Da\u00b7bey", "so", "wolln", "wir", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir haben des Gelds und Guts genug,", "tokens": ["Wir", "ha\u00b7ben", "des", "Gelds", "und", "Guts", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir wagens mit unserm Leibe.", "tokens": ["Wir", "wa\u00b7gens", "mit", "un\u00b7serm", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbwenn die Bachm\u00fchlen stille stehn,", "tokens": ["\u00bb", "wenn", "die", "Bach\u00b7m\u00fch\u00b7len", "stil\u00b7le", "stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die gro\u00dfen Wasser in Wellen gehn,", "tokens": ["Die", "gro\u00b7\u00dfen", "Was\u00b7ser", "in", "Wel\u00b7len", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das ist allzeit zu loben,", "tokens": ["Das", "ist", "all\u00b7zeit", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sperling flieget in den Dohm,", "tokens": ["Der", "Sper\u00b7ling", "flie\u00b7get", "in", "den", "Dohm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Falke schwebet oben.", "tokens": ["Der", "Fal\u00b7ke", "schwe\u00b7bet", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr lieben B\u00fcrger lobelich", "tokens": ["Ihr", "lie\u00b7ben", "B\u00fcr\u00b7ger", "lo\u00b7be\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun merket mich auch allzugleich.", "tokens": ["Nun", "mer\u00b7ket", "mich", "auch", "all\u00b7zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich euch hab gesungen,", "tokens": ["Was", "ich", "euch", "hab", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Welcher Vogel welcher bauet hoch,", "tokens": ["Wel\u00b7cher", "Vo\u00b7gel", "wel\u00b7cher", "bau\u00b7et", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PWAT", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Beh\u00e4lt wohl seine Jungen.", "tokens": ["Be\u00b7h\u00e4lt", "wohl", "sei\u00b7ne", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das ich nun sage und dich warn,", "tokens": ["Das", "ich", "nun", "sa\u00b7ge", "und", "dich", "warn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "KON", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Magdeburg du bist ein wilder Arn,", "tokens": ["Mag\u00b7de\u00b7burg", "du", "bist", "ein", "wil\u00b7der", "Arn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dein Fl\u00fcgel sind unverhauen,", "tokens": ["Dein", "Fl\u00fc\u00b7gel", "sind", "un\u00b7ver\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du fleugst den Wald wohl auf und ab,", "tokens": ["Du", "fleugst", "den", "Wald", "wohl", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das mag man auch wohl schauen.\u00ab", "tokens": ["Das", "mag", "man", "auch", "wohl", "schau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die B\u00fcrger schrien alle dicht:", "tokens": ["Die", "B\u00fcr\u00b7ger", "schri\u00b7en", "al\u00b7le", "dicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmagdeburg ist kein Haase nicht;", "tokens": ["\u00bb", "mag\u00b7de\u00b7burg", "ist", "kein", "Haa\u00b7se", "nicht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VAFIN", "PIAT", "NN", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Es ist ein k\u00fchner L\u00f6we,", "tokens": ["Es", "ist", "ein", "k\u00fch\u00b7ner", "L\u00f6\u00b7we", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Winden zerbricht er ihre F\u00fc\u00df,", "tokens": ["Den", "Win\u00b7den", "zer\u00b7bricht", "er", "ih\u00b7re", "F\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das reden wir mit Vertrauen.\u00ab", "tokens": ["Das", "re\u00b7den", "wir", "mit", "Ver\u00b7trau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Die Pfaffen hattens nicht wohl bedacht,", "tokens": ["Die", "Pfaf\u00b7fen", "hat\u00b7tens", "nicht", "wohl", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Han ihren Herrn in Schaden gebracht,", "tokens": ["Han", "ih\u00b7ren", "Herrn", "in", "Scha\u00b7den", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und kr\u00e4nken ihre Feste,", "tokens": ["Und", "kr\u00e4n\u00b7ken", "ih\u00b7re", "Fes\