{"dta.poem.12896": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem frantz\u00f6sischen des   Pays.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, Zephyr-winde! seyd bey Daphnen nicht so k\u00fchne!", "tokens": ["Ach", ",", "Ze\u00b7phy\u00b7rwin\u00b7de", "!", "seyd", "bey", "Daph\u00b7nen", "nicht", "so", "k\u00fch\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "VAFIN", "APPR", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stellt die zu freyen seuffzer ein!", "tokens": ["Stellt", "die", "zu", "frey\u00b7en", "seuff\u00b7zer", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PTKZU", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bef\u00fcrchtet ihr euch nicht vor einer sauren miene?", "tokens": ["Be\u00b7f\u00fcrch\u00b7tet", "ihr", "euch", "nicht", "vor", "ei\u00b7ner", "sau\u00b7ren", "mie\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie k\u00f6nnt ihr so verwegen seyn?", "tokens": ["Wie", "k\u00f6nnt", "ihr", "so", "ver\u00b7we\u00b7gen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist euch denn nicht bewust,", "tokens": ["Ist", "euch", "denn", "nicht", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie \u00fcbel manchem schon das seuffzen ist bekommen,", "tokens": ["Wie", "\u00fc\u00b7bel", "man\u00b7chem", "schon", "das", "seuff\u00b7zen", "ist", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIAT", "ADV", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der sich doch nicht so viel, als ihr, heraus genommen?", "tokens": ["Der", "sich", "doch", "nicht", "so", "viel", ",", "als", "ihr", ",", "he\u00b7raus", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "PPER", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Viel hundert haben schon darum ins grab gemust.", "tokens": ["Viel", "hun\u00b7dert", "ha\u00b7ben", "schon", "da\u00b7rum", "ins", "grab", "ge\u00b7must", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VAFIN", "ADV", "PAV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}