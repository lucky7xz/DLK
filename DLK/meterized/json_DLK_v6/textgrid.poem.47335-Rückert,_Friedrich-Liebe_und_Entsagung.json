{"textgrid.poem.47335": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Entsagung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fclle Dschemschids Becher an", "tokens": ["F\u00fcl\u00b7le", "Dschem\u00b7schids", "Be\u00b7cher", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zum h\u00f6chsten Rande,", "tokens": ["Bis", "zum", "h\u00f6chs\u00b7ten", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6chsten Himmel bist du dann", "tokens": ["H\u00f6chs\u00b7ten", "Him\u00b7mel", "bist", "du", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin zu sehn im stande.", "tokens": ["Drin", "zu", "sehn", "im", "stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "APPRART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Kennst du nicht des Bechers Glanz?", "tokens": ["Kennst", "du", "nicht", "des", "Be\u00b7chers", "Glanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Gef\u00e4\u00df Dschemschidens", "tokens": ["Das", "Ge\u00b7f\u00e4\u00df", "Dschem\u00b7schi\u00b7dens"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist dein Herz; du f\u00fcll' es ganz", "tokens": ["Ist", "dein", "Herz", ";", "du", "f\u00fcll'", "es", "ganz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Schaum des Friedens!", "tokens": ["Mit", "dem", "Schaum", "des", "Frie\u00b7dens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Bleibst du niemals ohne Wein,", "tokens": ["Bleibst", "du", "nie\u00b7mals", "oh\u00b7ne", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Lieb' und Lieder,", "tokens": ["Oh\u00b7ne", "Lieb'", "und", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fehlt nicht Erdew\u00fcstenei'n", "tokens": ["Fehlt", "nicht", "Er\u00b7de\u00b7w\u00fcs\u00b7te\u00b7nei'n"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmlisches Gefieder.", "tokens": ["Himm\u00b7li\u00b7sches", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ihrer Liebe Schleier wird", "tokens": ["Ih\u00b7rer", "Lie\u00b7be", "Schlei\u00b7er", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00fcften deine Rose,", "tokens": ["L\u00fcf\u00b7ten", "dei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ostwind! wenn du ungeirrt", "tokens": ["Ost\u00b7wind", "!", "wenn", "du", "un\u00b7ge\u00b7irrt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fort\u00fcbst dein Gekose.", "tokens": ["Fort\u00b7\u00fcbst", "dein", "Ge\u00b7ko\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Sieh! kein Schleier deckt dein Bild;", "tokens": ["Sieh", "!", "kein", "Schlei\u00b7er", "deckt", "dein", "Bild", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rege, wo du gehest,", "tokens": ["Re\u00b7ge", ",", "wo", "du", "ge\u00b7hest", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Keinen Staub nur im Gefild,", "tokens": ["Kei\u00b7nen", "Staub", "nur", "im", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du klar mich sehest.", "tokens": ["Da\u00df", "du", "klar", "mich", "se\u00b7hest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ruf den Blitz auf deines Ichs", "tokens": ["Ruf", "den", "Blitz", "auf", "dei\u00b7nes", "Ichs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkles Wohngeb\u00e4ude,", "tokens": ["Dunk\u00b7les", "Wohn\u00b7ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und verkl\u00e4rt erweitre sich's", "tokens": ["Und", "ver\u00b7kl\u00e4rt", "er\u00b7weit\u00b7re", "sich's"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Palast der Freude.", "tokens": ["Zum", "Pa\u00b7last", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Zittre nicht, Verzicht zu thun,", "tokens": ["Zitt\u00b7re", "nicht", ",", "Ver\u00b7zicht", "zu", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz! auf deine Deinheit,", "tokens": ["Herz", "!", "auf", "dei\u00b7ne", "De\u00b7in\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+----", "measure": "unknown.measure.di"}, "line.3": {"text": "Wenn du aufgenommen ruhn", "tokens": ["Wenn", "du", "auf\u00b7ge\u00b7nom\u00b7men", "ruhn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst in meine Meinheit.", "tokens": ["Willst", "in", "mei\u00b7ne", "Mein\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Forderst du das volle Glas", "tokens": ["For\u00b7derst", "du", "das", "vol\u00b7le", "Glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Freundin Lippen,", "tokens": ["Von", "der", "Freun\u00b7din", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fordre nicht noch dies und das", "tokens": ["Ford\u00b7re", "nicht", "noch", "dies", "und", "das"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "PDS", "KON", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Erde Klippen.", "tokens": ["Von", "der", "Er\u00b7de", "Klip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Geh zufrieden wie Hafis", "tokens": ["Geh", "zu\u00b7frie\u00b7den", "wie", "Ha\u00b7fis"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "KOKOM", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Auf Entsagungs-Wegen,", "tokens": ["Auf", "Ent\u00b7sa\u00b7gungs\u00b7We\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und es geht dir hier gewi\u00df", "tokens": ["Und", "es", "geht", "dir", "hier", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst die Lieb' entgegen.", "tokens": ["Einst", "die", "Lieb'", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}