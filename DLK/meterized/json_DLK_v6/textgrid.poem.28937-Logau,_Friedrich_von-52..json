{"textgrid.poem.28937": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Calvus hat so grossen Schedel und noch dennoch kein Gehirne;", "tokens": ["Cal\u00b7vus", "hat", "so", "gros\u00b7sen", "Sche\u00b7del", "und", "noch", "den\u00b7noch", "kein", "Ge\u00b7hir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJA", "NN", "KON", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Voller Stirn ist auch sein Schedel; dennoch hat er keine Stirne.", "tokens": ["Vol\u00b7ler", "Stirn", "ist", "auch", "sein", "Sche\u00b7del", ";", "den\u00b7noch", "hat", "er", "kei\u00b7ne", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}