{"textgrid.poem.50450": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die wartet auf ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die wartet auf ", "tokens": ["Die", "war\u00b7tet", "auf"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Entsetze dich hierob, gleich wie wir Heut' erschracken,", "tokens": ["Ent\u00b7set\u00b7ze", "dich", "hier\u00b7ob", ",", "gleich", "wie", "wir", "Heut'", "er\u00b7schra\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "ADV", "KOKOM", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als du uns in der Brunst der ", "tokens": ["Als", "du", "uns", "in", "der", "Brunst", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versagt ein ", "tokens": ["Ver\u00b7sagt", "ein"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So mache ", "tokens": ["So", "ma\u00b7che"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}