{"textgrid.poem.50686": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Buhlereien der Deutschen in Frankreich", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Durch Pulver, welches ", "tokens": ["Durch", "Pul\u00b7ver", ",", "wel\u00b7ches"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df es in B\u00fcchern uns verlacht,", "tokens": ["Da\u00df", "es", "in", "B\u00fc\u00b7chern", "uns", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem das Drucken ", "tokens": ["Nach\u00b7dem", "das", "Dru\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da\u00df wir dort unser Geld verschwenden,", "tokens": ["Da\u00df", "wir", "dort", "un\u00b7ser", "Geld", "ver\u00b7schwen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem es uns hernach besticht,", "tokens": ["Mit", "dem", "es", "uns", "her\u00b7nach", "be\u00b7sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df es durch unsre L\u00e4nder bricht", "tokens": ["Da\u00df", "es", "durch", "uns\u00b7re", "L\u00e4n\u00b7der", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Pferden, die wir ihnen senden, \u2013", "tokens": ["Mit", "Pfer\u00b7den", ",", "die", "wir", "ih\u00b7nen", "sen\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "PPER", "VAFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Geht eh' in meinen Kopf hinein,", "tokens": ["Geht", "eh'", "in", "mei\u00b7nen", "Kopf", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als wenn wir dort die Kraft verlieren,", "tokens": ["Als", "wenn", "wir", "dort", "die", "Kraft", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df ihre Weiber wir verf\u00fchren", "tokens": ["Da\u00df", "ih\u00b7re", "Wei\u00b7ber", "wir", "ver\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und unsrer Feinde V\u00e4ter sei'n.", "tokens": ["Und", "uns\u00b7rer", "Fein\u00b7de", "V\u00e4\u00b7ter", "sei'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPOSAT", "NN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}