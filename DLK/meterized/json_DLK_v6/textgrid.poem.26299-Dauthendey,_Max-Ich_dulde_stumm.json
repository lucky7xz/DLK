{"textgrid.poem.26299": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ich dulde stumm", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt um mich ist ein Krankenzimmer", "tokens": ["Die", "Welt", "um", "mich", "ist", "ein", "Kran\u00b7ken\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mit geschlossenen L\u00e4den im Zwielichtschimmer.", "tokens": ["Mit", "ge\u00b7schlos\u00b7se\u00b7nen", "L\u00e4\u00b7den", "im", "Zwie\u00b7licht\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ich m\u00f6chte nur leise Schritte machen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "nur", "lei\u00b7se", "Schrit\u00b7te", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Meine Augen schmerzen vor n\u00e4chtlichem Wachen.", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "schmer\u00b7zen", "vor", "n\u00e4cht\u00b7li\u00b7chem", "Wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Meine Brust ist von Sorgen eng umbunden,", "tokens": ["Mei\u00b7ne", "Brust", "ist", "von", "Sor\u00b7gen", "eng", "um\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Inwendig bluten mir stechende Wunden.", "tokens": ["In\u00b7wen\u00b7dig", "blu\u00b7ten", "mir", "ste\u00b7chen\u00b7de", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich kann noch kein Ende der Krankheit sehen.", "tokens": ["Ich", "kann", "noch", "kein", "En\u00b7de", "der", "Krank\u00b7heit", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Werd ich je froh auf den F\u00fc\u00dfen stehen?", "tokens": ["Werd", "ich", "je", "froh", "auf", "den", "F\u00fc\u00b7\u00dfen", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Das Fieber des Krieges, Heimweh und Sehnen, \u2013", "tokens": ["Das", "Fie\u00b7ber", "des", "Krie\u00b7ges", ",", "Heim\u00b7weh", "und", "Seh\u00b7nen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "KON", "NN", "$,", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ich dulde stumm mit verbissenen Z\u00e4hnen.", "tokens": ["Ich", "dul\u00b7de", "stumm", "mit", "ver\u00b7bis\u00b7se\u00b7nen", "Z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}