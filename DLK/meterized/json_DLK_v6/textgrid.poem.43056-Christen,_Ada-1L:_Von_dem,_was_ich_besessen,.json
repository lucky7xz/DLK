{"textgrid.poem.43056": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von dem, was ich besessen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem, was ich besessen,", "tokens": ["Von", "dem", ",", "was", "ich", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist wenig mir geblieben,", "tokens": ["Ist", "we\u00b7nig", "mir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von meinen s\u00fc\u00dfen Tr\u00e4umen,", "tokens": ["Von", "mei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Glauben, Hoffen, Lieben!", "tokens": ["Von", "Glau\u00b7ben", ",", "Hof\u00b7fen", ",", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur schmerzliches Erinnern", "tokens": ["Nur", "schmerz\u00b7li\u00b7ches", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist's, was das Herz behielt,", "tokens": ["Ist's", ",", "was", "das", "Herz", "be\u00b7hielt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verachtung, Ha\u00df und Fl\u00fcche \u2013", "tokens": ["Ver\u00b7ach\u00b7tung", ",", "Ha\u00df", "und", "Fl\u00fc\u00b7che", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eines Mannes Bild.", "tokens": ["Und", "ei\u00b7nes", "Man\u00b7nes", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}