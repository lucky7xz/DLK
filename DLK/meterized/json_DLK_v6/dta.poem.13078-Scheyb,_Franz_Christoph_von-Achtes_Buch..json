{"dta.poem.13078": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Bau-Kunst, welche sich gleich andern vorgethan,", "tokens": ["Die", "Bau\u00b7Kunst", ",", "wel\u00b7che", "sich", "gleich", "an\u00b7dern", "vor\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "70Fieng voller Ungeduld darein zu reden an:", "tokens": ["vol\u00b7ler", "Un\u00b7ge\u00b7duld", "da\u00b7rein", "zu", "re\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "\u201ewas? sprach sie, trachtest du der Bau-Kunst nachzuahmen?", "tokens": ["\u201e", "was", "?", "sprach", "sie", ",", "trach\u00b7test", "du", "der", "Bau\u00b7Kunst", "nach\u00b7zu\u00b7ah\u00b7men", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder sich unsterblich nennt, verdanckt es meinem Nahmen.", "tokens": ["\u201e", "der", "sich", "uns\u00b7terb\u00b7lich", "nennt", ",", "ver\u00b7danckt", "es", "mei\u00b7nem", "Nah\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PRF", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eman reise durch die Welt, so weit sie die Natur", "tokens": ["\u201e", "man", "rei\u00b7se", "durch", "die", "Welt", ",", "so", "weit", "sie", "die", "Na\u00b7tur"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201emit Grund versehen hat; so zeigt euch jede Spuhr", "tokens": ["\u201e", "mit", "Grund", "ver\u00b7se\u00b7hen", "hat", ";", "so", "zeigt", "euch", "je\u00b7de", "Spuhr"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "VVPP", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "75\u201dWo solche Wercke stehn, die meine Kunst errichtet;", "tokens": ["\"", "Wo", "sol\u00b7che", "Wer\u00b7cke", "stehn", ",", "die", "mei\u00b7ne", "Kunst", "er\u00b7rich\u00b7tet", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda\u00df meine Kraft allein der Zeiten Macht zernichtet.", "tokens": ["\u201e", "da\u00df", "mei\u00b7ne", "Kraft", "al\u00b7lein", "der", "Zei\u00b7ten", "Macht", "zer\u00b7nich\u00b7tet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edas Erd-Rund, hat es was das ohne mich besteht?", "tokens": ["\u201e", "das", "Erd\u00b7Rund", ",", "hat", "es", "was", "das", "oh\u00b7ne", "mich", "be\u00b7steht", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "VAFIN", "PPER", "PWS", "PDS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eist etwas, welches nicht, wo ich nicht bin, vergeht?", "tokens": ["\u201e", "ist", "et\u00b7was", ",", "wel\u00b7ches", "nicht", ",", "wo", "ich", "nicht", "bin", ",", "ver\u00b7geht", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "$,", "PRELS", "PTKNEG", "$,", "PWAV", "PPER", "PTKNEG", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewer in der Vorwelt sich hat meiner Kunst bedienet,", "tokens": ["\u201e", "wer", "in", "der", "Vor\u00b7welt", "sich", "hat", "mei\u00b7ner", "Kunst", "be\u00b7die\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "NN", "PRF", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "80\u201dSieht, wie noch diesen Tag sein Nahm in Steinen gr\u00fcnet.", "tokens": ["\"", "Sieht", ",", "wie", "noch", "die\u00b7sen", "Tag", "sein", "Nahm", "in", "Stei\u00b7nen", "gr\u00fc\u00b7net", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "ADV", "PDAT", "NN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201egar selten hat der Fra\u00df der niemahls milden Zeit", "tokens": ["\u201e", "gar", "sel\u00b7ten", "hat", "der", "Fra\u00df", "der", "nie\u00b7mahls", "mil\u00b7den", "Zeit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edurch seiner Z\u00e4hne M\u00fch und Uners\u00e4tlichkeit", "tokens": ["\u201e", "durch", "sei\u00b7ner", "Z\u00e4h\u00b7ne", "M\u00fch", "und", "Un\u00b7er\u00b7s\u00e4t\u00b7lich\u00b7keit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eso grosse Macht gehabt, da\u00df ein Geb\u00e4u vergangen,", "tokens": ["\u201e", "so", "gros\u00b7se", "Macht", "ge\u00b7habt", ",", "da\u00df", "ein", "Ge\u00b7b\u00e4u", "ver\u00b7gan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "VAPP", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201edas seine Wesenheit von meiner Hand empfangen.", "tokens": ["\u201e", "das", "sei\u00b7ne", "We\u00b7sen\u00b7heit", "von", "mei\u00b7ner", "Hand", "emp\u00b7fan\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "85\u201dWoher ist uns bekannt, was man der ersten Welt", "tokens": ["\"", "Wo\u00b7her", "ist", "uns", "be\u00b7kannt", ",", "was", "man", "der", "ers\u00b7ten", "Welt"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201ezu der Bewunderung des Ruhms hat dargestellt?", "tokens": ["\u201e", "zu", "der", "Be\u00b7wun\u00b7de\u00b7rung", "des", "Ruhms", "hat", "dar\u00b7ge\u00b7stellt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eman sieht noch diesen Tag, wie vor drey tausend Jahren", "tokens": ["\u201e", "man", "sieht", "noch", "die\u00b7sen", "Tag", ",", "wie", "vor", "drey", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "ADV", "PDAT", "NN", "$,", "PWAV", "APPR", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eder F\u00fcrsten Tugenden durch mich ber\u00fchmet waren.", "tokens": ["\u201e", "der", "F\u00fcrs\u00b7ten", "Tu\u00b7gen\u00b7den", "durch", "mich", "be\u00b7r\u00fch\u00b7met", "wa\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201edie Th\u00fcrne stehen noch, wodurch das Alterthum", "tokens": ["\u201e", "die", "Th\u00fcr\u00b7ne", "ste\u00b7hen", "noch", ",", "wo\u00b7durch", "das", "Al\u00b7ter\u00b7thum"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "90\u201dDer Helden Ehr erh\u00f6ht', und ihrer Thaten Ruhm.", "tokens": ["\"", "Der", "Hel\u00b7den", "Ehr", "er\u00b7h\u00f6ht'", ",", "und", "ih\u00b7rer", "Tha\u00b7ten", "Ruhm", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201eh\u00e4tt man zu solchem Ziel nicht meine Kunst gebrauchet,", "tokens": ["\u201e", "h\u00e4tt", "man", "zu", "sol\u00b7chem", "Ziel", "nicht", "mei\u00b7ne", "Kunst", "ge\u00b7brau\u00b7chet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "APPR", "PIAT", "NN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201eso w\u00e4r die Vorwelt schon in ihrer Zeit verrauchet.", "tokens": ["\u201e", "so", "w\u00e4r", "die", "Vor\u00b7welt", "schon", "in", "ih\u00b7rer", "Zeit", "ver\u00b7rau\u00b7chet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}