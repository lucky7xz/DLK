{"dta.poem.8874": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  An F. v. B.   Bey demselben Anlasse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr dieses Lebens Herrlichstes und Meistes,", "tokens": ["F\u00fcr", "die\u00b7ses", "Le\u00b7bens", "Herr\u00b7lichs\u00b7tes", "und", "Meis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie hat hieher in unser unbereistes,", "tokens": ["Sie", "hat", "hie\u00b7her", "in", "un\u00b7ser", "un\u00b7be\u00b7reis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bescheidnes St\u00e4dtchen dich, o Freund, gef\u00fchret.", "tokens": ["Be\u00b7scheid\u00b7nes", "St\u00e4dt\u00b7chen", "dich", ",", "o", "Freund", ",", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "FM", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6ne Sehnsucht, welche du versp\u00fcret,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Sehn\u00b7sucht", ",", "wel\u00b7che", "du", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein H\u00f6chstes fr\u00fche zu verstehn und Freystes,", "tokens": ["Ein", "H\u00f6chs\u00b7tes", "fr\u00fc\u00b7he", "zu", "ver\u00b7stehn", "und", "Freys\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "PTKZU", "VVINF", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat auf die Spuren jenes gro\u00dfen Geistes", "tokens": ["Hat", "auf", "die", "Spu\u00b7ren", "je\u00b7nes", "gro\u00b7\u00dfen", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich hergef\u00fchrt, der alle Welt ber\u00fchret.", "tokens": ["Dich", "her\u00b7ge\u00b7f\u00fchrt", ",", "der", "al\u00b7le", "Welt", "be\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du hassest Alle, die nur Formeln schw\u00e4tzen,", "tokens": ["Du", "has\u00b7sest", "Al\u00b7le", ",", "die", "nur", "For\u00b7meln", "schw\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PRELS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du strebst das Innre jedes Dings zu sichten,", "tokens": ["Du", "strebst", "das", "Inn\u00b7re", "je\u00b7des", "Dings", "zu", "sich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcbst den Geist in schroffen Gegens\u00e4tzen.", "tokens": ["Und", "\u00fcbst", "den", "Geist", "in", "schrof\u00b7fen", "Ge\u00b7gen\u00b7s\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dies hatt' ich scheidend noch an dich zu richten,", "tokens": ["Dies", "hatt'", "ich", "schei\u00b7dend", "noch", "an", "dich", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du packe nun zu deinen andern Sch\u00e4tzen", "tokens": ["Du", "pa\u00b7cke", "nun", "zu", "dei\u00b7nen", "an\u00b7dern", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch diesen Schatz von n\u00e4rrischen Gedichten!", "tokens": ["Auch", "die\u00b7sen", "Schatz", "von", "n\u00e4r\u00b7ri\u00b7schen", "Ge\u00b7dich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}