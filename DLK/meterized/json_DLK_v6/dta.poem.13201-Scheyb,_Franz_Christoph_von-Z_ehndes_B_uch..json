{"dta.poem.13201": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejhr aber, Tugenden! ihr habt f\u00fcr mich so viel", "tokens": ["\u201e", "jhr", "a\u00b7ber", ",", "Tu\u00b7gen\u00b7den", "!", "ihr", "habt", "f\u00fcr", "mich", "so", "viel"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "ADV", "$,", "NN", "$.", "PPER", "VAFIN", "APPR", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "210\u201dErsonnen und gewirckt, da\u00df ich schon an dem Ziel", "tokens": ["\"", "Er\u00b7son\u00b7nen", "und", "ge\u00b7wirckt", ",", "da\u00df", "ich", "schon", "an", "dem", "Ziel"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "KON", "VVPP", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201enach dem ich strebte, bin. Jhr wu\u00dftet eure Pflichten", "tokens": ["\u201e", "nach", "dem", "ich", "streb\u00b7te", ",", "bin", ".", "Ihr", "wu\u00df\u00b7tet", "eu\u00b7re", "Pflich\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie GOtt euch auferlegt, nach jedem Fall zu richten.", "tokens": ["\u201e", "die", "Gott", "euch", "auf\u00b7er\u00b7legt", ",", "nach", "je\u00b7dem", "Fall", "zu", "rich\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPER", "VVPP", "$,", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewie manch Mahl kamet ihr dem Feind im Siegen vor?", "tokens": ["\u201e", "wie", "manch", "Mahl", "ka\u00b7met", "ihr", "dem", "Feind", "im", "Sie\u00b7gen", "vor", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ejhr botet ihm die Stirn, als er den Anfall schwor.", "tokens": ["\u201e", "jhr", "bo\u00b7tet", "ihm", "die", "Stirn", ",", "als", "er", "den", "An\u00b7fall", "schwor", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "215\u201dWo die Zaghaftigkeit der Freunde Sinnen kr\u00e4nckte,", "tokens": ["\"", "Wo", "die", "Zag\u00b7haf\u00b7tig\u00b7keit", "der", "Freun\u00b7de", "Sin\u00b7nen", "kr\u00e4nck\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "\u201eda wars, wo euer Amt und Sorgfallt sich hinlenckte.", "tokens": ["\u201e", "da", "wars", ",", "wo", "eu\u00b7er", "Amt", "und", "Sorg\u00b7fallt", "sich", "hin\u00b7lenck\u00b7te", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "KON", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewo Schrecken, Angst und Noth sich in die L\u00e4nder drang,", "tokens": ["\u201e", "wo", "Schre\u00b7cken", ",", "Angst", "und", "Noth", "sich", "in", "die", "L\u00e4n\u00b7der", "drang", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "$,", "NN", "KON", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edort st\u00e4rcktet ihr den Muth durch Liebe, Rath und Zwang.", "tokens": ["\u201e", "dort", "st\u00e4rck\u00b7tet", "ihr", "den", "Muth", "durch", "Lie\u00b7be", ",", "Rath", "und", "Zwang", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eward eines Freunds Gem\u00fcth durch Feindes List verf\u00fchret,", "tokens": ["\u201e", "ward", "ei\u00b7nes", "Freunds", "Ge\u00b7m\u00fcth", "durch", "Fein\u00b7des", "List", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "220\u201dDem wiest ihr, was der Treu vor Ehr und Ruhm geb\u00fchret.", "tokens": ["\"", "Dem", "wiest", "ihr", ",", "was", "der", "Treu", "vor", "Ehr", "und", "Ruhm", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die feyerliche Uebertragung\nder Mit-Regentschaft geschahe/ laut\n\nausgefertigter Acte/ bereits den 21.\nNovemb. 1740.", "tokens": ["Die", "fey\u00b7er\u00b7li\u00b7che", "Ue\u00b7ber\u00b7tra\u00b7gung", "der", "Mit\u00b7Re\u00b7gent\u00b7schaft", "ge\u00b7scha\u00b7he", "/", "laut", "aus\u00b7ge\u00b7fer\u00b7tig\u00b7ter", "Ac\u00b7te", "/", "be\u00b7reits", "den", "21.", "No\u00b7vemb", ".", "1740", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "ordinal", "word", "punct", "number", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN", "$(", "ADV", "ART", "ADJA", "NN", "$.", "CARD", "$."], "meter": "-+-+-+-+-+-+-+-+--+-+--+--+--+", "measure": "iambic.octa.plus"}, "line.14": {"text": "\u201ees ward der gr\u00f6ste Sturm durch eure Macht bek\u00e4mpft;", "tokens": ["\u201e", "es", "ward", "der", "gr\u00f6s\u00b7te", "Sturm", "durch", "eu\u00b7re", "Macht", "be\u00b7k\u00e4mpft", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201edann sagt, wer hatte sonst desselben Wuth ged\u00e4mpft?", "tokens": ["\u201e", "dann", "sagt", ",", "wer", "hat\u00b7te", "sonst", "des\u00b7sel\u00b7ben", "Wuth", "ge\u00b7d\u00e4mpft", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "PWS", "VAFIN", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201ehat irgendswo der Schwall des Waffen-Strohms gesch\u00e4umet,", "tokens": ["\u201e", "hat", "ir\u00b7gends\u00b7wo", "der", "Schwall", "des", "Waf\u00b7fen\u00b7Strohms", "ge\u00b7sch\u00e4u\u00b7met", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edort war durch eure Macht desselben Grimm gez\u00e4umet.", "tokens": ["\u201e", "dort", "war", "durch", "eu\u00b7re", "Macht", "des\u00b7sel\u00b7ben", "Grimm", "ge\u00b7z\u00e4u\u00b7met", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "PDS", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "225\u201dDes Ungewitters Lauf und Toben ist gestillt,", "tokens": ["\"", "Des", "Un\u00b7ge\u00b7wit\u00b7ters", "Lauf", "und", "To\u00b7ben", "ist", "ge\u00b7stillt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201enur weil ihr euers Amts Obligenheit erf\u00fcllt.", "tokens": ["\u201e", "nur", "weil", "ihr", "eu\u00b7ers", "Amts", "Ob\u00b7li\u00b7gen\u00b7heit", "er\u00b7f\u00fcllt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPER", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eland, Ordnung und Gesaz bekamen neue Kr\u00e4fte,", "tokens": ["\u201e", "land", ",", "Ord\u00b7nung", "und", "Ge\u00b7saz", "be\u00b7ka\u00b7men", "neu\u00b7e", "Kr\u00e4f\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "KON", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201ewarum? ihr sorgtet stets um jedes Staats-Gesch\u00e4fte.", "tokens": ["\u201e", "wa\u00b7rum", "?", "ihr", "sorg\u00b7tet", "stets", "um", "je\u00b7des", "Staats\u00b7Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "PPER", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201eder Thron, wie man ihn sieht; die Ruh des Vaterlands;", "tokens": ["\u201e", "der", "Thron", ",", "wie", "man", "ihn", "sieht", ";", "die", "Ruh", "des", "Va\u00b7ter\u00b7lands", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "230\u201dDer Waffen Gl\u00fcck und Ruhm; die Wohlfart jedes Stands;", "tokens": ["\"", "Der", "Waf\u00b7fen", "Gl\u00fcck", "und", "Ruhm", ";", "die", "Wohl\u00b7fart", "je\u00b7des", "Stands", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "KON", "NN", "$.", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201eja was man in dem Krieg von Heil wei\u00df auszudr\u00fccken,", "tokens": ["\u201e", "ja", "was", "man", "in", "dem", "Krieg", "von", "Heil", "wei\u00df", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWS", "PIS", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201eist ein Zusammenhang von euern Meisterst\u00fccken.", "tokens": ["\u201e", "ist", "ein", "Zu\u00b7sam\u00b7men\u00b7hang", "von", "eu\u00b7ern", "Meis\u00b7ter\u00b7st\u00fc\u00b7cken", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}