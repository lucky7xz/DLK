{"textgrid.poem.58476": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "514. Selige Ewigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins stille Nu so inniglich", "tokens": ["Ins", "stil\u00b7le", "Nu", "so", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sink' ich, mein einzig's Gut, in dich,", "tokens": ["Sink'", "ich", ",", "mein", "ein\u00b7zig's", "Gut", ",", "in", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In 'n Seelengrund hinein;", "tokens": ["In", "'n", "See\u00b7len\u00b7grund", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da leb' ich au\u00dfer Ort und Zeit", "tokens": ["Da", "leb'", "ich", "au\u00b7\u00dfer", "Ort", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von mir und Kreaturen weit", "tokens": ["Von", "mir", "und", "Kre\u00b7a\u00b7tu\u00b7ren", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dir vergn\u00fcgt allein.", "tokens": ["Mit", "dir", "ver\u00b7gn\u00fcgt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}