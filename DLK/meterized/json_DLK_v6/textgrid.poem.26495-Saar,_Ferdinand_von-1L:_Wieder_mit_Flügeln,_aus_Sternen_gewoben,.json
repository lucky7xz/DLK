{"textgrid.poem.26495": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wieder mit Fl\u00fcgeln, aus Sternen gewoben,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder mit Fl\u00fcgeln, aus Sternen gewoben,", "tokens": ["Wie\u00b7der", "mit", "Fl\u00fc\u00b7geln", ",", "aus", "Ster\u00b7nen", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Senkst du herab dich, o heilige Nacht;", "tokens": ["Senkst", "du", "her\u00b7ab", "dich", ",", "o", "hei\u00b7li\u00b7ge", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Was durch Jahrhunderte Alles zerstoben \u2013", "tokens": ["Was", "durch", "Jahr\u00b7hun\u00b7der\u00b7te", "Al\u00b7les", "zer\u00b7sto\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PIS", "VVPP", "$("], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du noch bewahrst deine leuchtende Pracht!", "tokens": ["Du", "noch", "be\u00b7wahrst", "dei\u00b7ne", "leuch\u00b7ten\u00b7de", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ging auch der Welt schon der Heiland verloren,", "tokens": ["Ging", "auch", "der", "Welt", "schon", "der", "Hei\u00b7land", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Der sich dem Dunkel der Zeiten entrang,", "tokens": ["Der", "sich", "dem", "Dun\u00b7kel", "der", "Zei\u00b7ten", "ent\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wird er doch immer auf's neue geboren,", "tokens": ["Wird", "er", "doch", "im\u00b7mer", "auf's", "neu\u00b7e", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nahst du, Geweihte, dem irdischen Drang.", "tokens": ["Nahst", "du", ",", "Ge\u00b7weih\u00b7te", ",", "dem", "ir\u00b7di\u00b7schen", "Drang", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Selig durchschauernd kindliche Herzen,", "tokens": ["Se\u00b7lig", "durch\u00b7schau\u00b7ernd", "kind\u00b7li\u00b7che", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bist du des Glaubens s\u00fc\u00dfester Rest;", "tokens": ["Bist", "du", "des", "Glau\u00b7bens", "s\u00fc\u00b7\u00dfes\u00b7ter", "Rest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Fr\u00f6hlich begangen bei flammenden Kerzen,", "tokens": ["Fr\u00f6h\u00b7lich", "be\u00b7gan\u00b7gen", "bei", "flam\u00b7men\u00b7den", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Bist du das sch\u00f6nste, das menschlichste Fest.", "tokens": ["Bist", "du", "das", "sch\u00f6ns\u00b7te", ",", "das", "menschlichs\u00b7te", "Fest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Leerend das F\u00fcllhorn begl\u00fcckender Liebe,", "tokens": ["Lee\u00b7rend", "das", "F\u00fcll\u00b7horn", "be\u00b7gl\u00fc\u00b7cken\u00b7der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schwebst von Geschlecht zu Geschlecht du vertraut \u2013", "tokens": ["Schwebst", "von", "Ge\u00b7schlecht", "zu", "Ge\u00b7schlecht", "du", "ver\u00b7traut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "NN", "PPER", "ADJD", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wo ist die Brust, die verschlossen dir bliebe,", "tokens": ["Wo", "ist", "die", "Brust", ",", "die", "ver\u00b7schlos\u00b7sen", "dir", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Nicht dich begr\u00fc\u00dfte mit innigstem Laut?", "tokens": ["Nicht", "dich", "be\u00b7gr\u00fc\u00df\u00b7te", "mit", "in\u00b7nigs\u00b7tem", "Laut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und so klingt heut' noch das Wort von der Lippe,", "tokens": ["Und", "so", "klingt", "heut'", "noch", "das", "Wort", "von", "der", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das einst in Bethlehem preisend erklang,", "tokens": ["Das", "einst", "in", "Beth\u00b7le\u00b7hem", "prei\u00b7send", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Strahlet noch immer die liebliche Krippe \u2013", "tokens": ["Strah\u00b7let", "noch", "im\u00b7mer", "die", "lieb\u00b7li\u00b7che", "Krip\u00b7pe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "T\u00f6nt aus der Ferne der Hirten Gesang ...", "tokens": ["T\u00f6nt", "aus", "der", "Fer\u00b7ne", "der", "Hir\u00b7ten", "Ge\u00b7sang", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Was auch im Sturme der Zeiten zerstoben \u2013", "tokens": ["Was", "auch", "im", "Stur\u00b7me", "der", "Zei\u00b7ten", "zer\u00b7sto\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Senke herab dich in ewiger Pracht,", "tokens": ["Sen\u00b7ke", "her\u00b7ab", "dich", "in", "e\u00b7wi\u00b7ger", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Leuchtende du, aus Sternen gewoben,", "tokens": ["Leuch\u00b7ten\u00b7de", "du", ",", "aus", "Ster\u00b7nen", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Frohe, harzduftende, heilige Nacht!", "tokens": ["Fro\u00b7he", ",", "harz\u00b7duf\u00b7ten\u00b7de", ",", "hei\u00b7li\u00b7ge", "Nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}