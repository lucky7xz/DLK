{"textgrid.poem.25758": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hundert der Sch\u00f6nen im Saal' gehn meinen Blicken vor\u00fcber,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hundert der Sch\u00f6nen im Saal' gehn meinen Blicken vor\u00fcber,", "tokens": ["Hun\u00b7dert", "der", "Sch\u00f6\u00b7nen", "im", "Saal'", "gehn", "mei\u00b7nen", "Bli\u00b7cken", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Lilla's schlanken Wuchs glaub' ich hier wieder zu sehn,", "tokens": ["Lil\u00b7la's", "schlan\u00b7ken", "Wuchs", "glaub'", "ich", "hier", "wie\u00b7der", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Lilla's dunkeles Haar, von selbst in Locken sich kr\u00e4uselnd,", "tokens": ["Lil\u00b7la's", "dun\u00b7ke\u00b7les", "Haar", ",", "von", "selbst", "in", "Lo\u00b7cken", "sich", "kr\u00e4u\u00b7selnd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "ADV", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "F\u00e4llt bei dieser und der wallend den Nacken herab;", "tokens": ["F\u00e4llt", "bei", "die\u00b7ser", "und", "der", "wal\u00b7lend", "den", "Na\u00b7cken", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "KON", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Jene schreitet gerade so leicht, so edel als Lilla,", "tokens": ["Je\u00b7ne", "schrei\u00b7tet", "ge\u00b7ra\u00b7de", "so", "leicht", ",", "so", "e\u00b7del", "als", "Lil\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "NE", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Aehnlich den Grazien, her, reitzend gekleidet wie sie;", "tokens": ["A\u00b7ehn\u00b7lich", "den", "Gra\u00b7zi\u00b7en", ",", "her", ",", "reit\u00b7zend", "ge\u00b7klei\u00b7det", "wie", "sie", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PTKVZ", "$,", "ADJD", "VVPP", "KOKOM", "PPER", "$."], "meter": "-+--+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Und bei dieser verspricht das sanfte Auge dem Herzen", "tokens": ["Und", "bei", "die\u00b7ser", "ver\u00b7spricht", "das", "sanf\u00b7te", "Au\u00b7ge", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "H\u00fclfe, das ihrer bedarf, lindernde Thr\u00e4nen und Trost;", "tokens": ["H\u00fcl\u00b7fe", ",", "das", "ih\u00b7rer", "be\u00b7darf", ",", "lin\u00b7dern\u00b7de", "Thr\u00e4\u00b7nen", "und", "Trost", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "VVFIN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "+---+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und dem Geiste verspricht's durch neckende Scherze, wie Lilla,", "tokens": ["Und", "dem", "Geis\u00b7te", "ver\u00b7spricht's", "durch", "ne\u00b7cken\u00b7de", "Scher\u00b7ze", ",", "wie", "Lil\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "PWAV", "NE", "$,"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "In das Pf\u00e4nderspiel gr\u00e4melnde Greise zu ziehn.", "tokens": ["In", "das", "Pf\u00e4n\u00b7der\u00b7spiel", "gr\u00e4\u00b7meln\u00b7de", "Grei\u00b7se", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Aber ach! Schade f\u00fcrwahr um diese schlanke Gestalten,", "tokens": ["A\u00b7ber", "ach", "!", "Scha\u00b7de", "f\u00fcr\u00b7wahr", "um", "die\u00b7se", "schlan\u00b7ke", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADJD", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Um das lockige Haar, Schad' um den Graziengang,", "tokens": ["Um", "das", "lo\u00b7cki\u00b7ge", "Haar", ",", "Schad'", "um", "den", "Gra\u00b7zi\u00b7en\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Um das wei\u00dfe Gewand, mit Blumenschnuren durchflochten,", "tokens": ["Um", "das", "wei\u00b7\u00dfe", "Ge\u00b7wand", ",", "mit", "Blu\u00b7men\u00b7schnu\u00b7ren", "durch\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Um das Auge, das viel \u2013 wenig nur haltend \u2013 verspricht!", "tokens": ["Um", "das", "Au\u00b7ge", ",", "das", "viel", "\u2013", "we\u00b7nig", "nur", "hal\u00b7tend", "\u2013", "ver\u00b7spricht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "ADV", "$(", "PIS", "ADV", "ADJD", "$(", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Unter dem Hundert gleicht auch nicht eine einzige, Lilla.", "tokens": ["Un\u00b7ter", "dem", "Hun\u00b7dert", "gleicht", "auch", "nicht", "ei\u00b7ne", "ein\u00b7zi\u00b7ge", ",", "Lil\u00b7la", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "$,", "NE", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Sie, an Herzlichkeit eine Germanierin,", "tokens": ["Sie", ",", "an", "Herz\u00b7lich\u00b7keit", "ei\u00b7ne", "Ger\u00b7ma\u00b7nie\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Eine Gallierin an Witz und Feinheit im Scherzen,", "tokens": ["Ei\u00b7ne", "Gal\u00b7lie\u00b7rin", "an", "Witz", "und", "Fein\u00b7heit", "im", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+-+---+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Eine Brittin dem Ernst' und der Bescheidenheit nach;", "tokens": ["Ei\u00b7ne", "Brit\u00b7tin", "dem", "Ernst'", "und", "der", "Be\u00b7schei\u00b7den\u00b7heit", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Und den Gl\u00fccklichen, der ihr Herz erobert, den wird sie", "tokens": ["Und", "den", "Gl\u00fcck\u00b7li\u00b7chen", ",", "der", "ihr", "Herz", "er\u00b7o\u00b7bert", ",", "den", "wird", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,", "ART", "VAFIN", "PPER"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Feurig lieben, wie einst Donna Poretta geliebt.", "tokens": ["Feu\u00b7rig", "lie\u00b7ben", ",", "wie", "einst", "Don\u00b7na", "Po\u00b7ret\u00b7ta", "ge\u00b7liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PWAV", "ADV", "NE", "NE", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}