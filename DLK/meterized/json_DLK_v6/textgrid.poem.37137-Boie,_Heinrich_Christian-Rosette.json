{"textgrid.poem.37137": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Rosette", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Rosettens Blicken hangend,", "tokens": ["An", "Ro\u00b7set\u00b7tens", "Bli\u00b7cken", "han\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schmachtend, seufzend und verlangend", "tokens": ["Schmach\u00b7tend", ",", "seuf\u00b7zend", "und", "ver\u00b7lan\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fleh ich mit vergebner M\u00fch:", "tokens": ["Fleh", "ich", "mit", "ver\u00b7geb\u00b7ner", "M\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kannst du ewig meinen Klagen,", "tokens": ["Kannst", "du", "e\u00b7wig", "mei\u00b7nen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meinen Thr\u00e4nen dich versagen?", "tokens": ["Mei\u00b7nen", "Thr\u00e4\u00b7nen", "dich", "ver\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lohnst du meine Treue nie?", "tokens": ["Lohnst", "du", "mei\u00b7ne", "Treu\u00b7e", "nie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber immer unbeweglich", "tokens": ["A\u00b7ber", "im\u00b7mer", "un\u00b7be\u00b7weg\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt das kalte M\u00e4dchen t\u00e4glich", "tokens": ["H\u00f6rt", "das", "kal\u00b7te", "M\u00e4d\u00b7chen", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Seufzer an und spricht:", "tokens": ["Mei\u00b7ne", "Seuf\u00b7zer", "an", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbhoffnung n\u00e4hrt allein die Liebe.", "tokens": ["\u00bb", "hoff\u00b7nung", "n\u00e4hrt", "al\u00b7lein", "die", "Lie\u00b7be", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Glaub', ich theilte deine Triebe,", "tokens": ["Glaub'", ",", "ich", "theil\u00b7te", "dei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcnscht' ich ihre Dauer nicht.\u00ab", "tokens": ["W\u00fcnscht'", "ich", "ih\u00b7re", "Dau\u00b7er", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}