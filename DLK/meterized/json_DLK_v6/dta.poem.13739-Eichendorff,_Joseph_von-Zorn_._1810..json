{"dta.poem.13739": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zorn .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Denk' ich dann, wie Du gestanden", "tokens": ["Denk'", "ich", "dann", ",", "wie", "Du", "ge\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PWAV", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treu, da niemand treu geblieben:", "tokens": ["Treu", ",", "da", "nie\u00b7mand", "treu", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht' ich, \u00fcber unsre Schande", "tokens": ["M\u00f6cht'", "ich", ",", "\u00fc\u00b7ber", "uns\u00b7re", "Schan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefentbrannt in zorn'gem Lieben,", "tokens": ["Tie\u00b7fent\u00b7brannt", "in", "zorn'\u00b7gem", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}