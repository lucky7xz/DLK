{"textgrid.poem.34272": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Blutige Lokken fallen von eingesunkenen Wangen;", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blutige Lokken fallen von eingesunkenen Wangen;", "tokens": ["Blu\u00b7ti\u00b7ge", "Lok\u00b7ken", "fal\u00b7len", "von", "ein\u00b7ge\u00b7sun\u00b7ke\u00b7nen", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Furchtbar, zwischen H\u00fclfe rufend ge\u00f6fneten, schwarzen", "tokens": ["Furcht\u00b7bar", ",", "zwi\u00b7schen", "H\u00fcl\u00b7fe", "ru\u00b7fend", "ge\u00b7\u00f6f\u00b7ne\u00b7ten", ",", "schwar\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,", "VVINF"], "meter": "+---+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Lippen laufen zwey Reihen scheu\u00dflicher Z\u00e4hne, so ragen", "tokens": ["Lip\u00b7pen", "lau\u00b7fen", "zwey", "Rei\u00b7hen", "scheu\u00df\u00b7li\u00b7cher", "Z\u00e4h\u00b7ne", ",", "so", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "CARD", "NN", "ADJA", "NN", "$,", "ADV", "VVINF"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "D\u00fcrre Beine aus Gr\u00e4bern hervor; die gefalteten H\u00e4nde", "tokens": ["D\u00fcr\u00b7re", "Bei\u00b7ne", "aus", "Gr\u00e4\u00b7bern", "her\u00b7vor", ";", "die", "ge\u00b7fal\u00b7te\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Dekket Bl\u00e4sse, die unter zersplitterten N\u00e4geln zum Blau wird:", "tokens": ["Dek\u00b7ket", "Bl\u00e4s\u00b7se", ",", "die", "un\u00b7ter", "zer\u00b7split\u00b7ter\u00b7ten", "N\u00e4\u00b7geln", "zum", "Blau", "wird", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Denn im einsamen schrekkenden Walde hat er sich \u00e4ngstlich", "tokens": ["Denn", "im", "ein\u00b7sa\u00b7men", "schrek\u00b7ken\u00b7den", "Wal\u00b7de", "hat", "er", "sich", "\u00e4ngst\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "ADJA", "NN", "VAFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Mit verlarvten M\u00f6rdern gerungen: es hallten die Wipfel", "tokens": ["Mit", "ver\u00b7larv\u00b7ten", "M\u00f6r\u00b7dern", "ge\u00b7run\u00b7gen", ":", "es", "hall\u00b7ten", "die", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Von seinem bangen Rufen und dem m\u00f6rdrischen Murmeln", "tokens": ["Von", "sei\u00b7nem", "ban\u00b7gen", "Ru\u00b7fen", "und", "dem", "m\u00f6rd\u00b7ri\u00b7schen", "Mur\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Seiner Gegner; bald erlagen die Kr\u00e4fte des K\u00e4mpfers,", "tokens": ["Sei\u00b7ner", "Geg\u00b7ner", ";", "bald", "er\u00b7la\u00b7gen", "die", "Kr\u00e4f\u00b7te", "des", "K\u00e4mp\u00b7fers", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Schlaffe Arme strekt' er vergeblich, die t\u00f6dtlichen Aexte", "tokens": ["Schlaf\u00b7fe", "Ar\u00b7me", "strekt'", "er", "ver\u00b7geb\u00b7lich", ",", "die", "t\u00f6dt\u00b7li\u00b7chen", "A\u00b7ex\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Von seinem Haupt abzuhalten; sie, die sonst sch\u00fcchterne V\u00f6gel", "tokens": ["Von", "sei\u00b7nem", "Haupt", "ab\u00b7zu\u00b7hal\u00b7ten", ";", "sie", ",", "die", "sonst", "sch\u00fcch\u00b7ter\u00b7ne", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$.", "PPER", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Aus den gef\u00e4llten B\u00e4umen verscheuchten, spalteten izo", "tokens": ["Aus", "den", "ge\u00b7f\u00e4ll\u00b7ten", "B\u00e4u\u00b7men", "ver\u00b7scheuch\u00b7ten", ",", "spal\u00b7te\u00b7ten", "i\u00b7zo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Grausam die Gehirnspr\u00fczzende Scheitel des sterbenden Mannes,", "tokens": ["Grau\u00b7sam", "die", "Ge\u00b7hirns\u00b7pr\u00fcz\u00b7zen\u00b7de", "Schei\u00b7tel", "des", "ster\u00b7ben\u00b7den", "Man\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+---+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Dessen Seele ungern vom r\u00f6chelnden Busen emporstieg. \u2013", "tokens": ["Des\u00b7sen", "See\u00b7le", "un\u00b7gern", "vom", "r\u00f6\u00b7cheln\u00b7den", "Bu\u00b7sen", "em\u00b7por\u00b7stieg", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+--+--+---+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Streifende J\u00e4ger fanden den zerzerreten K\u00f6rper", "tokens": ["Strei\u00b7fen\u00b7de", "J\u00e4\u00b7ger", "fan\u00b7den", "den", "zer\u00b7zer\u00b7re\u00b7ten", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "In dem See von eigenem Blut, aus welchem die Gr\u00e4sgen", "tokens": ["In", "dem", "See", "von", "ei\u00b7ge\u00b7nem", "Blut", ",", "aus", "wel\u00b7chem", "die", "Gr\u00e4s\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Ihre beflekten Spizzen scheu erhoben: sie brachten", "tokens": ["Ih\u00b7re", "be\u00b7flek\u00b7ten", "Spiz\u00b7zen", "scheu", "er\u00b7ho\u00b7ben", ":", "sie", "brach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "$.", "PPER", "VVFIN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Ihn der untr\u00f6stbaren Wittwe, die sein dunkeles Auge", "tokens": ["Ihn", "der", "un\u00b7tr\u00f6st\u00b7ba\u00b7ren", "Witt\u00b7we", ",", "die", "sein", "dun\u00b7ke\u00b7les", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "Noch zu bedauren schien: noch sichtbar war auf der Wange", "tokens": ["Noch", "zu", "be\u00b7dau\u00b7ren", "schien", ":", "noch", "sicht\u00b7bar", "war", "auf", "der", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$.", "ADV", "ADJD", "VAFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Der sonst freundliche Zug, auf der verunstalteten Stirne", "tokens": ["Der", "sonst", "freund\u00b7li\u00b7che", "Zug", ",", "auf", "der", "ver\u00b7un\u00b7stal\u00b7te\u00b7ten", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+---++--+-", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Die kenntbare Runzel, die oft ein ahndender Kummer", "tokens": ["Die", "kennt\u00b7ba\u00b7re", "Run\u00b7zel", ",", "die", "oft", "ein", "ahn\u00b7den\u00b7der", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "In melancholischen Stunden drauf pflanzte. \u2013", "tokens": ["In", "me\u00b7lan\u00b7cho\u00b7li\u00b7schen", "Stun\u00b7den", "drauf", "pflanz\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "VVFIN", "$.", "$("], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}}}}}