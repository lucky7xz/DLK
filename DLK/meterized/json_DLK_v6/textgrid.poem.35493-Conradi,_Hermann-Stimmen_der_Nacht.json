{"textgrid.poem.35493": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Stimmen der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennst du der Nacht geheimnisschweres Raunen?", "tokens": ["Kennst", "du", "der", "Nacht", "ge\u00b7heim\u00b7nis\u00b7schwe\u00b7res", "Rau\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ein verlorner Klang aus Jugendtagen,", "tokens": ["Wie", "ein", "ver\u00b7lor\u00b7ner", "Klang", "aus", "Ju\u00b7gend\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der j\u00e4h in deine Seele eingeschlagen \u2013", "tokens": ["Der", "j\u00e4h", "in", "dei\u00b7ne", "See\u00b7le", "ein\u00b7ge\u00b7schla\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der j\u00e4h erwacht nach jahrelangem Schweigen:", "tokens": ["Der", "j\u00e4h", "er\u00b7wacht", "nach", "jah\u00b7re\u00b7lan\u00b7gem", "Schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So kommt es \u00fcber dich ... Es quellen, steigen", "tokens": ["So", "kommt", "es", "\u00fc\u00b7ber", "dich", "...", "Es", "quel\u00b7len", ",", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$(", "PPER", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verge\u00dfne Bilder auf \u2013 und ein ergreifend Staunen", "tokens": ["Ver\u00b7ge\u00df\u00b7ne", "Bil\u00b7der", "auf", "\u2013", "und", "ein", "er\u00b7grei\u00b7fend", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "$(", "KON", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Packt dich ... Das also war dein Schw\u00e4rmen, war dein Wagen?", "tokens": ["Packt", "dich", "...", "Das", "al\u00b7so", "war", "dein", "Schw\u00e4r\u00b7men", ",", "war", "dein", "Wa\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PDS", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jetzt? Und heute? Wie die Wunden tropfen!", "tokens": ["Und", "jetzt", "?", "Und", "heu\u00b7te", "?", "Wie", "die", "Wun\u00b7den", "trop\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "KON", "ADV", "$.", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie die Reue mahnt mit wildem Klopfen!", "tokens": ["Und", "wie", "die", "Reu\u00b7e", "mahnt", "mit", "wil\u00b7dem", "Klop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch la\u00df begraben sein, was da vergangen!", "tokens": ["Doch", "la\u00df", "be\u00b7gra\u00b7ben", "sein", ",", "was", "da", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "VVPP", "VAINF", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bilder, die in der Erinnrung Hallen", "tokens": ["Die", "Bil\u00b7der", ",", "die", "in", "der", "E\u00b7rinn\u00b7rung", "Hal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In schwarzem Trauerflor ich aufgestellt:", "tokens": ["In", "schwar\u00b7zem", "Trau\u00b7er\u00b7flor", "ich", "auf\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie m\u00f6gen st\u00fcrzen, m\u00f6gen fallen \u2013", "tokens": ["Sie", "m\u00f6\u00b7gen", "st\u00fcr\u00b7zen", ",", "m\u00f6\u00b7gen", "fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit ihnen eine ganze Schmerzenswelt!", "tokens": ["Mit", "ih\u00b7nen", "ei\u00b7ne", "gan\u00b7ze", "Schmer\u00b7zens\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Entgegenklingt in wundersamen T\u00f6nen:", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7klingt", "in", "wun\u00b7der\u00b7sa\u00b7men", "T\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist ein Siegeslied! Es soll vers\u00f6hnen,", "tokens": ["Es", "ist", "ein", "Sie\u00b7ges\u00b7lied", "!", "Es", "soll", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie jenes Wort am Kreuz: Es ist vollbracht!", "tokens": ["Wie", "je\u00b7nes", "Wort", "am", "Kreuz", ":", "Es", "ist", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPRART", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es ist vollbracht! Die Augen heb' ich auf,", "tokens": ["Es", "ist", "voll\u00b7bracht", "!", "Die", "Au\u00b7gen", "heb'", "ich", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und von den Felsenh\u00e4uptern seh' ich gleiten", "tokens": ["Und", "von", "den", "Fel\u00b7sen\u00b7h\u00e4up\u00b7tern", "seh'", "ich", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu Tal des Nebels dunst'ge Wolkenbrut ...", "tokens": ["Zu", "Tal", "des", "Ne\u00b7bels", "dunst'\u00b7ge", "Wol\u00b7ken\u00b7brut", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und \u00fcber die Scheitel, \u00fcber die nachtgefeiten,", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Schei\u00b7tel", ",", "\u00fc\u00b7ber", "die", "nacht\u00b7ge\u00b7fei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In langem Zug die Heldengeister schreiten,", "tokens": ["In", "lan\u00b7gem", "Zug", "die", "Hel\u00b7den\u00b7geis\u00b7ter", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich aus Kluft und Kr\u00fcmmung hochgem\u00fcht,", "tokens": ["Die", "sich", "aus", "Kluft", "und", "Kr\u00fcm\u00b7mung", "hoch\u00b7ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo in der ", "tokens": ["Wo", "in", "der"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "So gibt's doch einen Lohn! So gibt's ein Ziel!", "tokens": ["So", "gibt's", "doch", "ei\u00b7nen", "Lohn", "!", "So", "gibt's", "ein", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Und aus den Stimmen, die der Nacht entklingen,", "tokens": ["Und", "aus", "den", "Stim\u00b7men", ",", "die", "der", "Nacht", "ent\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "T\u00f6nt die Gewi\u00dfheit mir: ", "tokens": ["T\u00f6nt", "die", "Ge\u00b7wi\u00df\u00b7heit", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}