{"textgrid.poem.58212": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "250. Freiheit der Kinder Gottes", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O edle Freiheit, da der Wille abgeschieden", "tokens": ["O", "ed\u00b7le", "Frei\u00b7heit", ",", "da", "der", "Wil\u00b7le", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von allem, was nicht Gott, steht innig willenlos,", "tokens": ["Von", "al\u00b7lem", ",", "was", "nicht", "Gott", ",", "steht", "in\u00b7nig", "wil\u00b7len\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PTKNEG", "NN", "$,", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Angst und Treiben frei, erhaben lebt im Frieden,", "tokens": ["Von", "Angst", "und", "Trei\u00b7ben", "frei", ",", "er\u00b7ha\u00b7ben", "lebt", "im", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da nichts ihn r\u00fchrt noch st\u00f6rt in seines Vaters Scho\u00df!", "tokens": ["Da", "nichts", "ihn", "r\u00fchrt", "noch", "st\u00f6rt", "in", "sei\u00b7nes", "Va\u00b7ters", "Scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}