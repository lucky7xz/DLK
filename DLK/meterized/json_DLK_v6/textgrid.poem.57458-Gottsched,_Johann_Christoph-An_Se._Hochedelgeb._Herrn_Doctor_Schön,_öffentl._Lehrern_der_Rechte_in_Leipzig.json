{"textgrid.poem.57458": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "An Se. Hochedelgeb. Herrn Doctor Sch\u00f6n, \u00f6ffentl. Lehrern der Rechte in Leipzig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Theurer Freund! den in den Rechten", "tokens": ["Theu\u00b7rer", "Freund", "!", "den", "in", "den", "Rech\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "ART", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst ", "tokens": ["Selbst"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dem die Musen Kr\u00e4nze flechten,", "tokens": ["Dem", "die", "Mu\u00b7sen", "Kr\u00e4n\u00b7ze", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sein Geist im Dichten brennt;", "tokens": ["Wenn", "sein", "Geist", "im", "Dich\u00b7ten", "brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich doch nur frey gestehen,", "tokens": ["La\u00df", "mich", "doch", "nur", "frey", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df dein Klagen mich ger\u00fchrt,", "tokens": ["Da\u00df", "dein", "Kla\u00b7gen", "mich", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mich selbst zu jenen H\u00f6hen,", "tokens": ["Und", "mich", "selbst", "zu", "je\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo dein Schatz itzt lebt, gef\u00fchrt.", "tokens": ["Wo", "dein", "Schatz", "itzt", "lebt", ",", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Betr\u00fcben, Aechzen, Flehen", "tokens": ["Dein", "Be\u00b7tr\u00fc\u00b7ben", ",", "A\u00b7ech\u00b7zen", ",", "Fle\u00b7hen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Scheint mir gar nicht ungerecht;", "tokens": ["Scheint", "mir", "gar", "nicht", "un\u00b7ge\u00b7recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freylich ist dir Weh geschehen,", "tokens": ["Frey\u00b7lich", "ist", "dir", "Weh", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da der Tod dein Wohl geschw\u00e4cht.", "tokens": ["Da", "der", "Tod", "dein", "Wohl", "ge\u00b7schw\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Sch\u00f6nen Schmuck und Preis,", "tokens": ["Al\u00b7ler", "Sch\u00f6\u00b7nen", "Schmuck", "und", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der dein Herz, an Witz und Gaben", "tokens": ["Der", "dein", "Herz", ",", "an", "Witz", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Keine zu vergleichen weis.", "tokens": ["Kei\u00b7ne", "zu", "ver\u00b7glei\u00b7chen", "weis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "So viel Sch\u00f6nheit, so viel Tugend,", "tokens": ["So", "viel", "Sch\u00f6n\u00b7heit", ",", "so", "viel", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So viel Lust zur Wissenschaft,", "tokens": ["So", "viel", "Lust", "zur", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ward in gar zu fr\u00fcher Jugend", "tokens": ["Ward", "in", "gar", "zu", "fr\u00fc\u00b7her", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADV", "PTKA", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Durch den Tod dahin gerafft!", "tokens": ["Durch", "den", "Tod", "da\u00b7hin", "ge\u00b7rafft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Dieses regt schon mein Erbarmen;", "tokens": ["Die\u00b7ses", "regt", "schon", "mein", "Er\u00b7bar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Doch das klingt mir doppelt hart,", "tokens": ["Doch", "das", "klingt", "mir", "dop\u00b7pelt", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Da\u00df sie gar aus deinen Armen,", "tokens": ["Da\u00df", "sie", "gar", "aus", "dei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Werther Freund! gerissen ward.", "tokens": ["Wert\u00b7her", "Freund", "!", "ge\u00b7ris\u00b7sen", "ward", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sprich, wie war dir bey dem Raube", "tokens": ["Sprich", ",", "wie", "war", "dir", "bey", "dem", "Rau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner sch\u00f6nsten Braut zu Muth?", "tokens": ["Dei\u00b7ner", "sch\u00f6ns\u00b7ten", "Braut", "zu", "Muth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Thatst du nicht, wie eine Taube", "tokens": ["Thatst", "du", "nicht", ",", "wie", "ei\u00b7ne", "Tau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey des Gatten Falle thut?", "tokens": ["Bey", "des", "Gat\u00b7ten", "Fal\u00b7le", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja du girrtest, weintest, riefest,", "tokens": ["Ja", "du", "girr\u00b7test", ",", "wein\u00b7test", ",", "rie\u00b7fest", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Du verweyster Br\u00e4utigam!", "tokens": ["Du", "ver\u00b7weys\u00b7ter", "Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df das Lager, da du schliefest,", "tokens": ["Da\u00df", "das", "La\u00b7ger", ",", "da", "du", "schlie\u00b7fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oft von deinen Thr\u00e4nen schwamm.", "tokens": ["Oft", "von", "dei\u00b7nen", "Thr\u00e4\u00b7nen", "schwamm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist von den edlen Seelen,", "tokens": ["Du", "bist", "von", "den", "ed\u00b7len", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein schn\u00f6der Trieb entz\u00fcndt,", "tokens": ["Die", "kein", "schn\u00f6\u00b7der", "Trieb", "ent\u00b7z\u00fcndt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber die, so bald sie w\u00e4hlen,", "tokens": ["A\u00b7ber", "die", ",", "so", "bald", "sie", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4rtlich und best\u00e4ndig sind.", "tokens": ["Z\u00e4rt\u00b7lich", "und", "be\u00b7st\u00e4n\u00b7dig", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Brust war schwer zu zwingen,", "tokens": ["Dei\u00b7ne", "Brust", "war", "schwer", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber da sie Fessel trug,", "tokens": ["A\u00b7ber", "da", "sie", "Fes\u00b7sel", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollte sie vor Gram zerspringen,", "tokens": ["Woll\u00b7te", "sie", "vor", "Gram", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil der Tod die\u00df Band zerschlug.", "tokens": ["Weil", "der", "Tod", "die\u00df", "Band", "zer\u00b7schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch du hast die\u00df Leid ertragen,", "tokens": ["Doch", "du", "hast", "die\u00df", "Leid", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PDS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein Weiser, als ein Christ:", "tokens": ["Als", "ein", "Wei\u00b7ser", ",", "als", "ein", "Christ", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der auch bey den z\u00e4rtsten Klagen", "tokens": ["Der", "auch", "bey", "den", "z\u00e4rts\u00b7ten", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Standhaft und gelassen ist.", "tokens": ["Stand\u00b7haft", "und", "ge\u00b7las\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, ich seh dein starkes Wesen", "tokens": ["Ja", ",", "ich", "seh", "dein", "star\u00b7kes", "We\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleichsam mit Erstaunen an,", "tokens": ["Gleich\u00b7sam", "mit", "Er\u00b7stau\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil es das, was du erlesen,", "tokens": ["Weil", "es", "das", ",", "was", "du", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "$,", "PWS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So gesetzt bedauren kann.", "tokens": ["So", "ge\u00b7setzt", "be\u00b7dau\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freund und G\u00f6nner! darf ichs wagen,", "tokens": ["Freund", "und", "G\u00f6n\u00b7ner", "!", "darf", "ichs", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dir, zwar mit Vorbedacht,", "tokens": ["Und", "dir", ",", "zwar", "mit", "Vor\u00b7be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber im Vertrauen sagen,", "tokens": ["A\u00b7ber", "im", "Ver\u00b7trau\u00b7en", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mich so empfindlich macht?", "tokens": ["Was", "mich", "so", "emp\u00b7find\u00b7lich", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blo\u00df die F\u00fchlung eigner Triebe", "tokens": ["Blo\u00df", "die", "F\u00fch\u00b7lung", "eig\u00b7ner", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat mich so geschickt gemacht,", "tokens": ["Hat", "mich", "so", "ge\u00b7schickt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich deiner zarten Liebe", "tokens": ["Da\u00df", "ich", "dei\u00b7ner", "zar\u00b7ten", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr, als andre, nachgedacht.", "tokens": ["Mehr", ",", "als", "and\u00b7re", ",", "nach\u00b7ge\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PIS", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich hab auch ein Herz gefunden,", "tokens": ["Ich", "hab", "auch", "ein", "Herz", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das durch Tugend und Verstand", "tokens": ["Das", "durch", "Tu\u00b7gend", "und", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measur