{"dta.poem.13266": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun kommen sie zur\u00fcck, weil in dem Vaterland\u201e", "tokens": ["\u201e", "nun", "kom\u00b7men", "sie", "zu\u00b7r\u00fcck", ",", "weil", "in", "dem", "Va\u00b7ter\u00b7land", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach noch die K\u00f6niginn, der ungeheure Brand,", "tokens": ["Sprach", "noch", "die", "K\u00f6\u00b7ni\u00b7ginn", ",", "der", "un\u00b7ge\u00b7heu\u00b7re", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "415\u201dDer L\u00e4nders\u00fcchtige, Recht-lose Krieg vollendet,", "tokens": ["\"", "Der", "L\u00e4n\u00b7der\u00b7s\u00fcch\u00b7ti\u00b7ge", ",", "Recht\u00b7lo\u00b7se", "Krieg", "voll\u00b7en\u00b7det", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201eund anderw\u00e4rts die Wuth der Flammen hingewendet.", "tokens": ["\u201e", "und", "an\u00b7der\u00b7w\u00e4rts", "die", "Wuth", "der", "Flam\u00b7men", "hin\u00b7ge\u00b7wen\u00b7det", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201esollt aber unsers Volcks Erstaunens-voller Geist,", "tokens": ["\u201e", "sollt", "a\u00b7ber", "un\u00b7sers", "Volcks", "Er\u00b7stau\u00b7nens\u00b7vol\u00b7ler", "Geist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADV", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eder, wann er etwas w\u00fcnscht, schon auf den Ausgang weist,", "tokens": ["\u201e", "der", ",", "wann", "er", "et\u00b7was", "w\u00fcnscht", ",", "schon", "auf", "den", "Aus\u00b7gang", "weist", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ehierdurch was k\u00fcnftiges uns anzuzeigen finden;", "tokens": ["\u201e", "hier\u00b7durch", "was", "k\u00fcnf\u00b7ti\u00b7ges", "uns", "an\u00b7zu\u00b7zei\u00b7gen", "fin\u00b7den", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "PWS", "ADJA", "PPER", "VVIZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "420\u201dSo wei\u00df ich nicht worauf der Wahn sich k\u00f6nne gr\u00fcnden.", "tokens": ["\"", "So", "wei\u00df", "ich", "nicht", "wo\u00b7rauf", "der", "Wahn", "sich", "k\u00f6n\u00b7ne", "gr\u00fcn\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "PWAV", "ART", "NN", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}