{"textgrid.poem.64646": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "273. Grabschrift der Fr. Hofr\u00e4thinn Baldinger", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Witz belebt, durch Kenntni\u00df aufgekl\u00e4rt;", "tokens": ["Durch", "Witz", "be\u00b7lebt", ",", "durch", "Kennt\u00b7ni\u00df", "auf\u00b7ge\u00b7kl\u00e4rt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch gut zu seyn, war ihr noch gr\u00f6\u00dfrer Werth;", "tokens": ["Doch", "gut", "zu", "seyn", ",", "war", "ihr", "noch", "gr\u00f6\u00df\u00b7rer", "Werth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VAINF", "$,", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Treu ihrer Pflicht, zu gl\u00e4nzen unbem\u00fcht:", "tokens": ["Treu", "ih\u00b7rer", "Pflicht", ",", "zu", "gl\u00e4n\u00b7zen", "un\u00b7be\u00b7m\u00fcht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun lohnt Er ihr, der in's Verborgne sieht.", "tokens": ["Nun", "lohnt", "Er", "ihr", ",", "der", "in's", "Ver\u00b7borg\u00b7ne", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}