{"textgrid.poem.38212": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Noten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahres Lieben, s\u00fc\u00dfes Leben,", "tokens": ["Wah\u00b7res", "Lie\u00b7ben", ",", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo zwei Herzen eins nur sind,", "tokens": ["Wo", "zwei", "Her\u00b7zen", "eins", "nur", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "PIS", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie zwei Turtelt\u00e4ublein schweben,", "tokens": ["Wie", "zwei", "Tur\u00b7tel\u00b7t\u00e4u\u00b7blein", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ein treues Band verbind,", "tokens": ["Die", "ein", "treu\u00b7es", "Band", "ver\u00b7bind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Lieb den Chor anstimmet,", "tokens": ["Wo", "die", "Lieb", "den", "Chor", "an\u00b7stim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Treue giebt den Takt,", "tokens": ["Und", "die", "Treu\u00b7e", "giebt", "den", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem Blut die Freude schwimmet,", "tokens": ["In", "dem", "Blut", "die", "Freu\u00b7de", "schwim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Puls auf Lauten schlagt.", "tokens": ["Und", "der", "Puls", "auf", "Lau\u00b7ten", "schlagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Wo die Spr\u00f6de mu\u00df pausiren,", "tokens": ["Wo", "die", "Spr\u00f6\u00b7de", "mu\u00df", "pau\u00b7si\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Lust ein Solo singt,", "tokens": ["Wenn", "die", "Lust", "ein", "So\u00b7lo", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Aeuglein pizikiren,", "tokens": ["Wenn", "die", "A\u00b7e\u00b7u\u00b7glein", "pi\u00b7zi\u00b7ki\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis der Lieb ein Saite springt,", "tokens": ["Bis", "der", "Lieb", "ein", "Sai\u00b7te", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Herzen konkordiren,", "tokens": ["Wenn", "die", "Her\u00b7zen", "kon\u00b7kor\u00b7di\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sch\u00f6n singen in dem Ton,", "tokens": ["Und", "sch\u00f6n", "sin\u00b7gen", "in", "dem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird der Mund auch sekundiren,", "tokens": ["Wird", "der", "Mund", "auch", "se\u00b7kun\u00b7di\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ein Ku\u00df giebt ihm den Lohn.", "tokens": ["Und", "ein", "Ku\u00df", "giebt", "ihm", "den", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Will ein Ton ins Kreuzlein steigen,", "tokens": ["Will", "ein", "Ton", "ins", "Kreuz\u00b7lein", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ein B wie Weh erschalln,", "tokens": ["Will", "ein", "B", "wie", "Weh", "er\u00b7schalln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag aufs Herz der Finger zeigen,", "tokens": ["Mag", "aufs", "Herz", "der", "Fin\u00b7ger", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Musik ganz leise halln,", "tokens": ["Und", "Mu\u00b7sik", "ganz", "lei\u00b7se", "halln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die Noten in zwei Herzen", "tokens": ["Weil", "die", "No\u00b7ten", "in", "zwei", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einfach stehen in der Terz,", "tokens": ["Ein\u00b7fach", "ste\u00b7hen", "in", "der", "Terz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df uns ganz piano scherzen,", "tokens": ["La\u00df", "uns", "ganz", "pi\u00b7a\u00b7no", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und allegro leiden Schmerz.", "tokens": ["Und", "al\u00b7le\u00b7gro", "lei\u00b7den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}