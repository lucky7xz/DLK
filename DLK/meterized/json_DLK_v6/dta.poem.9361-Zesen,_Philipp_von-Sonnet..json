{"dta.poem.9361": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Monde/ dessen blasser Schein bey der braunen nacht sich", "tokens": ["Mon\u00b7de", "/", "des\u00b7sen", "blas\u00b7ser", "Schein", "bey", "der", "brau\u00b7nen", "nacht", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "PRELAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "PRF"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "zeiget/", "tokens": ["zei\u00b7get", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "und jhr g\u00fcldnen Him\u0303els-Lichter/ g\u00f6nnet uns doch euer licht/", "tokens": ["und", "jhr", "g\u00fcld\u00b7nen", "Him\u0303els\u00b7Lich\u00b7ter", "/", "g\u00f6n\u00b7net", "uns", "doch", "eu\u00b7er", "licht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "schim\u0303ert doch mit euren strahlen/ bi\u00df der fr\u00fche tag anbricht/", "tokens": ["schim\u0303ert", "doch", "mit", "eu\u00b7ren", "strah\u00b7len", "/", "bi\u00df", "der", "fr\u00fc\u00b7he", "tag", "an\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$(", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "wolt jhr nicht? hier ist der glantz/ wo mich nicht jhr blitz betreu-", "tokens": ["wolt", "jhr", "nicht", "?", "hier", "ist", "der", "glantz", "/", "wo", "mich", "nicht", "jhr", "blitz", "be\u00b7treu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "$.", "ADV", "VAFIN", "ART", "NN", "$(", "PWAV", "PPER", "PTKNEG", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "so aus jhren augen geht und in mein gesichte steiget/ (get/", "tokens": ["so", "aus", "jhren", "au\u00b7gen", "geht", "und", "in", "mein", "ge\u00b7sich\u00b7te", "stei\u00b7get", "/", "(", "get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "APPR", "PPOSAT", "ADJA", "VVFIN", "$(", "$(", "VVPP", "$("], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "bleib ", "tokens": ["bleib"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "sicht/", "tokens": ["sicht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "das den Liljen/ das den Rosen/ ja der Sonnen weichet nicht;", "tokens": ["das", "den", "Lil\u00b7jen", "/", "das", "den", "Ro\u00b7sen", "/", "ja", "der", "Son\u00b7nen", "wei\u00b7chet", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$(", "PDS", "ART", "NN", "$(", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Bleibe bey uns diese Nacht/ da das s\u00fc\u00dfe Lafft-volck schweiget/", "tokens": ["Blei\u00b7be", "bey", "uns", "die\u00b7se", "Nacht", "/", "da", "das", "s\u00fc\u00b7\u00dfe", "Lafft\u00b7volck", "schwei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PDAT", "NN", "$(", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "da die Sonne nicht mehr scheinet und das traure\u0303 uns besitzt/", "tokens": ["da", "die", "Son\u00b7ne", "nicht", "mehr", "schei\u00b7net", "und", "das", "traur\u1ebd", "uns", "be\u00b7sitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "KON", "PDS", "VVFIN", "PPER", "VVFIN", "$("], "meter": "--+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sey mein Licht und meine Son\u0303e/ wetl so lieblich zu mir blitzt", "tokens": ["sey", "mein", "Licht", "und", "mei\u00b7ne", "So\u00f1e", "/", "wetl", "so", "lieb\u00b7lich", "zu", "mir", "blitzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$(", "VVIMP", "ADV", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "deiner augen g\u00fcldnes Licht/ wenn der Abend k\u00f6m\u0303t gegangen/", "tokens": ["dei\u00b7ner", "au\u00b7gen", "g\u00fcld\u00b7nes", "Licht", "/", "wenn", "der", "A\u00b7bend", "k\u00f6m\u0303t", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "KOUS", "ART", "NN", "VVFIN", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "wen die tr\u00fcbe nacht hergehet und das schwartze trauer kleid/", "tokens": ["wen", "die", "tr\u00fc\u00b7be", "nacht", "her\u00b7ge\u00b7het", "und", "das", "schwart\u00b7ze", "trau\u00b7er", "kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "KON", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00fcmb den braunen K\u00f6rper h\u00fcllet: zeige deine liebligkeit/", "tokens": ["\u00fcmb", "den", "brau\u00b7nen", "K\u00f6r\u00b7per", "h\u00fcl\u00b7let", ":", "zei\u00b7ge", "dei\u00b7ne", "lieb\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "dein gewohntes freundlich-sehn/ zeige deine Rosen-wangen!", "tokens": ["dein", "ge\u00b7wohn\u00b7tes", "freund\u00b7lich\u00b7sehn", "/", "zei\u00b7ge", "dei\u00b7ne", "Ro\u00b7sen\u00b7wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}