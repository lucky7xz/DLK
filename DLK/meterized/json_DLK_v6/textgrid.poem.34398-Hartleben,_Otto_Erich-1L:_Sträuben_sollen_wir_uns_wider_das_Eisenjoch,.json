{"textgrid.poem.34398": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Str\u00e4uben sollen wir uns wider das Eisenjoch,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Str\u00e4uben sollen wir uns wider das Eisenjoch,", "tokens": ["Str\u00e4u\u00b7ben", "sol\u00b7len", "wir", "uns", "wi\u00b7der", "das", "Ei\u00b7sen\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "dem der Gewohnheit Schmutz W\u00fcrde des Alters lieh \u2013", "tokens": ["dem", "der", "Ge\u00b7wohn\u00b7heit", "Schmutz", "W\u00fcr\u00b7de", "des", "Al\u00b7ters", "lieh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "wen das steigende Licht gr\u00fcsst,", "tokens": ["wen", "das", "stei\u00b7gen\u00b7de", "Licht", "gr\u00fcs\u00b7st", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "nie sehn er die Nacht zur\u00fcck!", "tokens": ["nie", "sehn", "er", "die", "Nacht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Feigheit knechtet die Zeit, beuget der Nacken Kraft:", "tokens": ["Feig\u00b7heit", "knech\u00b7tet", "die", "Zeit", ",", "beu\u00b7get", "der", "Na\u00b7cken", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "wagt, o wagt es mit mir, frei zu bekennen, was", "tokens": ["wagt", ",", "o", "wagt", "es", "mit", "mir", ",", "frei", "zu", "be\u00b7ken\u00b7nen", ",", "was"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "PPER", "APPR", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$,", "PWS"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "l\u00e4ngst der k\u00fchnere Blick sah,", "tokens": ["l\u00e4ngst", "der", "k\u00fch\u00b7ne\u00b7re", "Blick", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "l\u00e4ngst Allen im Busen lebt!", "tokens": ["l\u00e4ngst", "Al\u00b7len", "im", "Bu\u00b7sen", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Heilig gelten der Zeit Rechte des Alters nur:", "tokens": ["Hei\u00b7lig", "gel\u00b7ten", "der", "Zeit", "Rech\u00b7te", "des", "Al\u00b7ters", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "was da bestand vordem, heisst sie bestehenswerth,", "tokens": ["was", "da", "be\u00b7stand", "vor\u00b7dem", ",", "heisst", "sie", "be\u00b7ste\u00b7hens\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "heilig gelten der Zeit nicht", "tokens": ["hei\u00b7lig", "gel\u00b7ten", "der", "Zeit", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Treupflichten des eignen Sinns.", "tokens": ["Treu\u00b7pf\u00b7lich\u00b7ten", "des", "eig\u00b7nen", "Sinns", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Str\u00e4uben ", "tokens": ["Str\u00e4u\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "dem der Gewohnheit Schmutz W\u00fcrde des Alters lieh \u2013", "tokens": ["dem", "der", "Ge\u00b7wohn\u00b7heit", "Schmutz", "W\u00fcr\u00b7de", "des", "Al\u00b7ters", "lieh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "wen das steigende Licht gr\u00fcsst,", "tokens": ["wen", "das", "stei\u00b7gen\u00b7de", "Licht", "gr\u00fcs\u00b7st", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "nie sehn er die Nacht zur\u00fcck!", "tokens": ["nie", "sehn", "er", "die", "Nacht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}