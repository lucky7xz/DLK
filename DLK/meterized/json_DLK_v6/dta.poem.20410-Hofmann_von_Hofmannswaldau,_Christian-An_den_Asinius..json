{"dta.poem.20410": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An den Asinius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe vielmahl schon im hertzen nachgedacht/", "tokens": ["Ich", "ha\u00b7be", "viel\u00b7mahl", "schon", "im", "hert\u00b7zen", "nach\u00b7ge\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was doch die kluge welt auff diese thorheit bracht/", "tokens": ["Was", "doch", "die", "klu\u00b7ge", "welt", "auff", "die\u00b7se", "thor\u00b7heit", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df da die schulen noch voll rechter leute leben/", "tokens": ["Da\u00df", "da", "die", "schu\u00b7len", "noch", "voll", "rech\u00b7ter", "leu\u00b7te", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "ADV", "ADJD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie dir/ Asinius/ den doctor-hut gegeben.", "tokens": ["Sie", "dir", "/", "A\u00b7si\u00b7nius", "/", "den", "doc\u00b7tor\u00b7hut", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$(", "NE", "$(", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Du bist kein weiser nicht: denn sonsten th\u00e4test du/", "tokens": ["Du", "bist", "kein", "wei\u00b7ser", "nicht", ":", "denn", "sons\u00b7ten", "th\u00e4\u00b7test", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "PTKNEG", "$.", "KON", "ADV", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df andern/ was du wilst/ da\u00df man dir selber thu:", "tokens": ["Di\u00df", "an\u00b7dern", "/", "was", "du", "wilst", "/", "da\u00df", "man", "dir", "sel\u00b7ber", "thu", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$(", "PWS", "PPER", "VMFIN", "$(", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du w\u00fcrdest deine frau nicht wie der teuffel plagen/", "tokens": ["Du", "w\u00fcr\u00b7dest", "dei\u00b7ne", "frau", "nicht", "wie", "der", "teuf\u00b7fel", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wie ein lumpenhund dich mit den m\u00e4gden schlagen.", "tokens": ["Und", "wie", "ein", "lum\u00b7pen\u00b7hund", "dich", "mit", "den", "m\u00e4g\u00b7den", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du bist kein weltmann nicht; dieweil du nicht verstehst/", "tokens": ["Du", "bist", "kein", "welt\u00b7mann", "nicht", ";", "die\u00b7weil", "du", "nicht", "ver\u00b7stehst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$.", "KOUS", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Warum du deiner frau zur lincken seiten gehst:", "tokens": ["Wa\u00b7rum", "du", "dei\u00b7ner", "frau", "zur", "lin\u00b7cken", "sei\u00b7ten", "gehst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das heist: du solst dein weib nicht treiben/ sondern f\u00fchren/", "tokens": ["Das", "heist", ":", "du", "solst", "dein", "weib", "nicht", "trei\u00b7ben", "/", "son\u00b7dern", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$(", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und sie mit h\u00f6fligkeit/ nicht mit gewalt regieren.", "tokens": ["Und", "sie", "mit", "h\u00f6f\u00b7lig\u00b7keit", "/", "nicht", "mit", "ge\u00b7walt", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "$(", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du bist kein medicus/ sonst nehmstu in der pein", "tokens": ["Du", "bist", "kein", "me\u00b7di\u00b7cus", "/", "sonst", "nehms\u00b7tu", "in", "der", "pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "FM", "$(", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein treibendes clistir vor deine w\u00fcrmer ein", "tokens": ["Ein", "trei\u00b7ben\u00b7des", "clis\u00b7tir", "vor", "dei\u00b7ne", "w\u00fcr\u00b7mer", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So hastu auch nicht viel in GOttes wort vergessen/", "tokens": ["So", "has\u00b7tu", "auch", "nicht", "viel", "in", "Got\u00b7tes", "wort", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "ADV", "APPR", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sonst w\u00fcrdestu dein thun nach dem gewissen messen.", "tokens": ["Sonst", "w\u00fcr\u00b7des\u00b7tu", "dein", "thun", "nach", "dem", "ge\u00b7wis\u00b7sen", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVINF", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du bist auch kein jurist/ denn wer das recht erkl\u00e4rt/", "tokens": ["Du", "bist", "auch", "kein", "ju\u00b7rist", "/", "denn", "wer", "das", "recht", "er\u00b7kl\u00e4rt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$(", "KON", "PWS", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der wei\u00df wohl/ da\u00df das weib nicht einen mann ernehrt/", "tokens": ["Der", "wei\u00df", "wohl", "/", "da\u00df", "das", "weib", "nicht", "ei\u00b7nen", "mann", "er\u00b7nehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$(", "KOUS", "ART", "NN", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und da\u00df/ wenn eine frau im hause soll befehlen/", "tokens": ["Und", "da\u00df", "/", "wenn", "ei\u00b7ne", "frau", "im", "hau\u00b7se", "soll", "be\u00b7feh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$(", "KOUS", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Man ihr die kr\u00fcge nicht mu\u00df aus der kammer stehlen.", "tokens": ["Man", "ihr", "die", "kr\u00fc\u00b7ge", "nicht", "mu\u00df", "aus", "der", "kam\u00b7mer", "steh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ART", "NN", "PTKNEG", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was teuffel bistu denn? Ein narr/ der nichts gelernt/", "tokens": ["Was", "teuf\u00b7fel", "bis\u00b7tu", "denn", "?", "Ein", "narr", "/", "der", "nichts", "ge\u00b7lernt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "ADV", "$.", "ART", "NN", "$(", "ART", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein flegel/ der nur drischt/ was andre eingeerndt.", "tokens": ["Ein", "fle\u00b7gel", "/", "der", "nur", "drischt", "/", "was", "and\u00b7re", "ein\u00b7geerndt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADJD", "$(", "PWS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Und gleichwol bistu doch ein grosser doctor worden?", "tokens": ["Und", "gleich\u00b7wol", "bis\u00b7tu", "doch", "ein", "gros\u00b7ser", "doc\u00b7tor", "wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "O mein Asinius/ man kommt nicht in den orden/", "tokens": ["O", "mein", "A\u00b7si\u00b7nius", "/", "man", "kommt", "nicht", "in", "den", "or\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NE", "$(", "PIS", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "Wo man bey dieser zeit nicht k\u00fcnste mit sich bringt/", "tokens": ["Wo", "man", "bey", "die\u00b7ser", "zeit", "nicht", "k\u00fcns\u00b7te", "mit", "sich", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PDAT", "NN", "PTKNEG", "VVFIN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und wenn die kunst gebricht/ von grosser zahlung singt.", "tokens": ["Und", "wenn", "die", "kunst", "ge\u00b7bricht", "/", "von", "gros\u00b7ser", "zah\u00b7lung", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie geht es dann nun zu? das geld hat dich erhoben/", "tokens": ["Wie", "geht", "es", "dann", "nun", "zu", "?", "das", "geld", "hat", "dich", "er\u00b7ho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das geld/ das dir wie koth offt in der hand verstoben:", "tokens": ["Das", "geld", "/", "das", "dir", "wie", "koth", "offt", "in", "der", "hand", "ver\u00b7sto\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "KOKOM", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das deines vaters flei\u00df mit grosser m\u00fch gehegt/", "tokens": ["Das", "dei\u00b7nes", "va\u00b7ters", "flei\u00df", "mit", "gros\u00b7ser", "m\u00fch", "ge\u00b7hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und du schon/ eh er starb/ mit schanden angelegt.", "tokens": ["Und", "du", "schon", "/", "eh", "er", "starb", "/", "mit", "schan\u00b7den", "an\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$(", "KOUS", "PPER", "VVFIN", "$(", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Drum fiengstu nach der zeit dich endlich anzugr\u00e4men/", "tokens": ["Drum", "fi\u00b7engs\u00b7tu", "nach", "der", "zeit", "dich", "end\u00b7lich", "an\u00b7zu\u00b7gr\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "VVIZU", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Und dachst/ ich mu\u00df mir nur ein liebes weibgen nehmen/", "tokens": ["Und", "dachst", "/", "ich", "mu\u00df", "mir", "nur", "ein", "lie\u00b7bes", "weib\u00b7gen", "neh\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VMFIN", "PPER", "ADV", "ART", "ADJA", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die/ weil ich armer schelm in b\u00fcchern nichts gethan/", "tokens": ["Die", "/", "weil", "ich", "ar\u00b7mer", "schelm", "in", "b\u00fc\u00b7chern", "nichts", "ge\u00b7than", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADJA", "NN", "APPR", "VVFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und alles geld verzehrt/ mich noch erhalten kan.", "tokens": ["Und", "al\u00b7les", "geld", "ver\u00b7zehrt", "/", "mich", "noch", "er\u00b7hal\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVPP", "$(", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das gl\u00fccke war dir hold/ du wurdest angenommen/", "tokens": ["Das", "gl\u00fc\u00b7cke", "war", "dir", "hold", "/", "du", "wur\u00b7dest", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dein tittel hat ein weib/ nicht aber du bekommen:", "tokens": ["Dein", "tit\u00b7tel", "hat", "ein", "weib", "/", "nicht", "a\u00b7ber", "du", "be\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "PTKNEG", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nun hastu/ was du wilst/ du lebst/ wie dirs gef\u00e4llt/", "tokens": ["Nun", "has\u00b7tu", "/", "was", "du", "wilst", "/", "du", "lebst", "/", "wie", "dirs", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "PWS", "PPER", "VMFIN", "$(", "PPER", "VVFIN", "$(", "PWAV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die frau ernehret dich/ ihr vater schafft dir geld;", "tokens": ["Die", "frau", "er\u00b7neh\u00b7ret", "dich", "/", "ihr", "va\u00b7ter", "schafft", "dir", "geld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die braten m\u00fcssen dir fast in die gurgel fliegen/", "tokens": ["Die", "bra\u00b7ten", "m\u00fcs\u00b7sen", "dir", "fast", "in", "die", "gur\u00b7gel", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Du kanst den gantzen tag im bette schnarchen liegen/", "tokens": ["Du", "kanst", "den", "gant\u00b7zen", "tag", "im", "bet\u00b7te", "schnar\u00b7chen", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und hast kein tr\u00fcbsal/ als/ da\u00df diese die dich fretzt/", "tokens": ["Und", "hast", "kein", "tr\u00fcb\u00b7sal", "/", "als", "/", "da\u00df", "die\u00b7se", "die", "dich", "fretzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "ADJD", "$(", "KOUS", "$(", "KOUS", "PDS", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Sich t\u00e4glich/ wen du speist/ mit dir zu tische setzt.", "tokens": ["Sich", "t\u00e4g\u00b7lich", "/", "wen", "du", "speist", "/", "mit", "dir", "zu", "ti\u00b7sche", "setzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$(", "PWS", "PPER", "VVFIN", "$(", "APPR", "PPER", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Das aber \u00e4rgert dich: Dann du bist wie die raben/", "tokens": ["Das", "a\u00b7ber", "\u00e4r\u00b7gert", "dich", ":", "Dann", "du", "bist", "wie", "die", "ra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "$.", "ADV", "PPER", "VAFIN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Du schaffst nichts in das hau\u00df/ doch wilstu fressen haben.", "tokens": ["Du", "schaffst", "nichts", "in", "das", "hau\u00df", "/", "doch", "wils\u00b7tu", "fres\u00b7sen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$(", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und was ein ander schafft/ da\u00df soll f\u00fcr dich allein/", "tokens": ["Und", "was", "ein", "an\u00b7der", "schafft", "/", "da\u00df", "soll", "f\u00fcr", "dich", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJD", "VVFIN", "$(", "KOUS", "VMFIN", "APPR", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und nicht/ als wenn du wilst/ f\u00fcr deinen nechsten seyn.", "tokens": ["Und", "nicht", "/", "als", "wenn", "du", "wilst", "/", "f\u00fcr", "dei\u00b7nen", "nechs\u00b7ten", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$(", "KOKOM", "KOUS", "PPER", "VMFIN", "$(", "APPR", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die frau soll/ wenn sie dir den rachen voll l\u00e4st sch\u00fctten/", "tokens": ["Die", "frau", "soll", "/", "wenn", "sie", "dir", "den", "ra\u00b7chen", "voll", "l\u00e4st", "sch\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$(", "KOUS", "PPER", "PPER", "ART", "NN", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Dich dennoch/ sch\u00f6ner wirth/ um ieden groschen bitten/", "tokens": ["Dich", "den\u00b7noch", "/", "sch\u00f6\u00b7ner", "wirth", "/", "um", "ie\u00b7den", "gro\u00b7schen", "bit\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADJA", "NN", "$(", "APPR", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Und sagen: Lieber mann/ ich wei\u00df wohl/ da\u00df das geld", "tokens": ["Und", "sa\u00b7gen", ":", "Lie\u00b7ber", "mann", "/", "ich", "wei\u00df", "wohl", "/", "da\u00df", "das", "geld"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "ADJD", "NN", "$(", "PPER", "VVFIN", "ADV", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Dir aus der taschen nicht wie pregel-erbsen f\u00e4llt:", "tokens": ["Dir", "aus", "der", "ta\u00b7schen", "nicht", "wie", "pre\u00b7gel\u00b7erb\u00b7sen", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "PTKNEG", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich wei\u00df/ da\u00df du es schwer und sauer must erwerben;", "tokens": ["Ich", "wei\u00df", "/", "da\u00df", "du", "es", "schwer", "und", "sau\u00b7er", "must", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PPER", "ADJD", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Allein erbarme dich/ la\u00df meine str\u00fcmpffe f\u00e4rben/", "tokens": ["Al\u00b7lein", "er\u00b7bar\u00b7me", "dich", "/", "la\u00df", "mei\u00b7ne", "str\u00fcmpf\u00b7fe", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VVIMP", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und schicke meine schuh doch zu dem schuster hin/", "tokens": ["Und", "schi\u00b7cke", "mei\u00b7ne", "schuh", "doch", "zu", "dem", "schus\u00b7ter", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "So seh ich/ da\u00df ich noch dein liebes weibgen bin.", "tokens": ["So", "seh", "ich", "/", "da\u00df", "ich", "noch", "dein", "lie\u00b7bes", "weib\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "O grober eselskopff! ist das nicht zubeklagen/", "tokens": ["O", "gro\u00b7ber", "e\u00b7sels\u00b7kopff", "!", "ist", "das", "nicht", "zu\u00b7be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VAFIN", "PDS", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Da\u00df ein so sch\u00f6nes weib mu\u00df deine narrheit tragen.", "tokens": ["Da\u00df", "ein", "so", "sch\u00f6\u00b7nes", "weib", "mu\u00df", "dei\u00b7ne", "nar\u00b7rheit", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Du bist mehr katz und aff/ als einem menschen gleich/", "tokens": ["Du", "bist", "mehr", "katz", "und", "aff", "/", "als", "ei\u00b7nem", "men\u00b7schen", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "NE", "$(", "KOUS", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Die lippen hengen dir/ die wangen werden bleich/", "tokens": ["Die", "lip\u00b7pen", "hen\u00b7gen", "dir", "/", "die", "wan\u00b7gen", "wer\u00b7den", "bleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Das kinn ist zugespitzt/ gleich wie die bauer h\u00fctte/", "tokens": ["Das", "kinn", "ist", "zu\u00b7ge\u00b7spitzt", "/", "gleich", "wie", "die", "bau\u00b7er", "h\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Die nase kommt mir vor/ wie eine kramer-t\u00fctte/", "tokens": ["Die", "na\u00b7se", "kommt", "mir", "vor", "/", "wie", "ei\u00b7ne", "kra\u00b7mer\u00b7t\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "In welche man ein