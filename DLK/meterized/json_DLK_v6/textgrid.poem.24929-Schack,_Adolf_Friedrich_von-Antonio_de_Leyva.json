{"textgrid.poem.24929": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Antonio de Leyva", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rings von Pavias Mauerkranz", "tokens": ["Rings", "von", "Pa\u00b7vi\u00b7as", "Mau\u00b7er\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Gewahrt man blitzend Speer an Speer;", "tokens": ["Ge\u00b7wahrt", "man", "blit\u00b7zend", "Speer", "an", "Speer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Jahr umzingelt K\u00f6nig Franz", "tokens": ["Ein", "Jahr", "um\u00b7zin\u00b7gelt", "K\u00f6\u00b7nig", "Franz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die W\u00e4lle schon mit seinem Heer;", "tokens": ["Die", "W\u00e4l\u00b7le", "schon", "mit", "sei\u00b7nem", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon w\u00fcten Pest und Hunger drinnen,", "tokens": ["Schon", "w\u00fc\u00b7ten", "Pest", "und", "Hun\u00b7ger", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und keiner ist, der Hilfe bringt;", "tokens": ["Und", "kei\u00b7ner", "ist", ",", "der", "Hil\u00b7fe", "bringt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch, ob der Feind auch n\u00e4her dringt,", "tokens": ["Doch", ",", "ob", "der", "Feind", "auch", "n\u00e4\u00b7her", "dringt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es wanken nicht die Festungzinnen,", "tokens": ["Es", "wan\u00b7ken", "nicht", "die", "Fes\u00b7tung\u00b7zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So lang f\u00fcr Karl, dem er vereidigt,", "tokens": ["So", "lang", "f\u00fcr", "Karl", ",", "dem", "er", "ver\u00b7ei\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NE", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Antonio Leyva sie verteidigt.", "tokens": ["An\u00b7to\u00b7nio", "Ley\u00b7va", "sie", "ver\u00b7tei\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sendet Franz mit welscher List", "tokens": ["Da", "sen\u00b7det", "Franz", "mit", "wel\u00b7scher", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPR", "PWAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verkappte Sp\u00e4her in das Thor;", "tokens": ["Ver\u00b7kapp\u00b7te", "Sp\u00e4\u00b7her", "in", "das", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Trug, wie er am feinsten ist,", "tokens": ["Durch", "Trug", ",", "wie", "er", "am", "feins\u00b7ten", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "APPRART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beth\u00f6ren sie der Mannschaft Ohr;", "tokens": ["Be\u00b7th\u00f6\u00b7ren", "sie", "der", "Mann\u00b7schaft", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf Markt und Gassen, Wall und T\u00fcrmen", "tokens": ["Auf", "Markt", "und", "Gas\u00b7sen", ",", "Wall", "und", "T\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schleicht durch das Heer der Teufel Gold,", "tokens": ["Schleicht", "durch", "das", "Heer", "der", "Teu\u00b7fel", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis Aufruhr in Pavia grollt", "tokens": ["Bis", "Auf\u00b7ruhr", "in", "Pa\u00b7via", "grollt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NE", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und Meutrer zu Antonio st\u00fcrmen:", "tokens": ["Und", "Meut\u00b7rer", "zu", "An\u00b7to\u00b7nio", "st\u00fcr\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "\u00bbwas, Feldherr, hilft das Widerstreben?", "tokens": ["\u00bb", "was", ",", "Feld\u00b7herr", ",", "hilft", "das", "Wi\u00b7der\u00b7stre\u00b7ben", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Festung m\u00fc\u00dft Ihr \u00fcbergeben!\u00ab", "tokens": ["Die", "Fes\u00b7tung", "m\u00fc\u00dft", "Ihr", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf Leyva: \u00bbWeicht aus diesem Saal!", "tokens": ["Drauf", "Ley\u00b7va", ":", "\u00bb", "Weicht", "aus", "die\u00b7sem", "Saal", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "$.", "$(", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eu'r Hauptmann einzig bleibe hier", "tokens": ["Eu'r", "Haupt\u00b7mann", "ein\u00b7zig", "blei\u00b7be", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meld euch dann, was ich befahl!", "tokens": ["Und", "meld", "euch", "dann", ",", "was", "ich", "be\u00b7fahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nochmals hinweg! Was z\u00f6gert ihr?\u00ab", "tokens": ["Noch\u00b7mals", "hin\u00b7weg", "!", "Was", "z\u00f6\u00b7gert", "ihr", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "$.", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Der Hauptmann winkt und, zu vollf\u00fchren,", "tokens": ["Der", "Haupt\u00b7mann", "winkt", "und", ",", "zu", "voll\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was er gebeut, gehn jene stumm;", "tokens": ["Was", "er", "ge\u00b7beut", ",", "gehn", "je\u00b7ne", "stumm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PDS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Antonio aber schlie\u00dft ringsum", "tokens": ["An\u00b7to\u00b7nio", "a\u00b7ber", "schlie\u00dft", "ring\u00b7sum"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Des Saales feste Eisenth\u00fcren", "tokens": ["Des", "Saa\u00b7les", "fes\u00b7te", "Ei\u00b7sent\u00b7h\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und donnert in des Hauptmanns Ohren:", "tokens": ["Und", "don\u00b7nert", "in", "des", "Haupt\u00b7manns", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbzieh, Schurke, zieh! Du bist verloren!", "tokens": ["\u00bb", "zieh", ",", "Schur\u00b7ke", ",", "zieh", "!", "Du", "bist", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verr\u00e4ter nenn' ich dich an Gott", "tokens": ["Ver\u00b7r\u00e4\u00b7ter", "nenn'", "ich", "dich", "an", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und an des Kaisers Majest\u00e4t;", "tokens": ["Und", "an", "des", "Kai\u00b7sers", "Ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Gold, von Franken ausges\u00e4t,", "tokens": ["Um", "Gold", ",", "von", "Fran\u00b7ken", "aus\u00b7ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Treibst du mit Ehr' und Treue Spott!", "tokens": ["Treibst", "du", "mit", "Ehr'", "und", "Treu\u00b7e", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, zieh! Kein Weg zur Flucht ist offen!\u00ab", "tokens": ["Zieh", ",", "zieh", "!", "Kein", "Weg", "zur", "Flucht", "ist", "of\u00b7fen", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "PIAT", "NN", "APPRART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf den Best\u00fcrzten eilt er los,", "tokens": ["Auf", "den", "Be\u00b7st\u00fcrz\u00b7ten", "eilt", "er", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Hieb folgt auf Hieb und Sto\u00df auf Sto\u00df;", "tokens": ["Hieb", "folgt", "auf", "Hieb", "und", "Sto\u00df", "auf", "Sto\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbweh!\u00ab \u2013 ruft der Hauptmann \u2013 \u00bbweh! getroffen!\u00ab", "tokens": ["\u00bb", "weh", "!", "\u00ab", "\u2013", "ruft", "der", "Haupt\u00b7mann", "\u2013", "\u00bb", "weh", "!", "ge\u00b7trof\u00b7fen", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "$(", "VVFIN", "ART", "NN", "$(", "$(", "PTKVZ", "$.", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zu Boden taumelt der Beth\u00f6rte,", "tokens": ["Zu", "Bo\u00b7den", "tau\u00b7melt", "der", "Be\u00b7th\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durchbohrt von Don Antonios Schwerte.", "tokens": ["Durch\u00b7bohrt", "von", "Don", "An\u00b7to\u00b7ni\u00b7os", "Schwer\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NE", "NN", "$."], "meter": "++-+-++-+-", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Indessen t\u00f6nt von unten schon", "tokens": ["In\u00b7des\u00b7sen", "t\u00f6nt", "von", "un\u00b7ten", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Soldateska w\u00fcst Geschrei;", "tokens": ["Der", "Sol\u00b7da\u00b7tes\u00b7ka", "w\u00fcst", "Ge\u00b7schrei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Es w\u00e4chst und schwillt die Meuterei;", "tokens": ["Es", "w\u00e4chst", "und", "schwillt", "die", "Meu\u00b7te\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Hauptmann fordern sie und drohn", "tokens": ["Den", "Haupt\u00b7mann", "for\u00b7dern", "sie", "und", "drohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Lanzen und entflammten Lunten;", "tokens": ["Mit", "Lan\u00b7zen", "und", "ent\u00b7flamm\u00b7ten", "Lun\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Antonio aber tritt gefa\u00dft", "tokens": ["An\u00b7to\u00b7nio", "a\u00b7ber", "tritt", "ge\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "VVPP"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Auf den Balkon vor dem Palast", "tokens": ["Auf", "den", "Bal\u00b7kon", "vor", "dem", "Pa\u00b7last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und schleudert ins Gew\u00fchl nach unten", "tokens": ["Und", "schleu\u00b7dert", "ins", "Ge\u00b7w\u00fchl", "nach", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den kaum erbla\u00dften Toten nieder.", "tokens": ["Den", "kaum", "er\u00b7bla\u00df\u00b7ten", "To\u00b7ten", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbihr fordert ihn, da habt ihn wieder!\u00ab", "tokens": ["\u00bb", "ihr", "for\u00b7dert", "ihn", ",", "da", "habt", "ihn", "wie\u00b7der", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wild ert\u00f6nt das Racheschrein", "tokens": ["Und", "wild", "er\u00b7t\u00f6nt", "das", "Ra\u00b7ch\u00b7e\u00b7schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Kriegerhaufen; voll von Wut", "tokens": ["Der", "Krie\u00b7ger\u00b7hau\u00b7fen", ";", "voll", "von", "Wut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verlangen sie des Feldherrn Blut;", "tokens": ["Ver\u00b7lan\u00b7gen", "sie", "des", "Feld\u00b7herrn", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch festen Schritts in ihre Reihn", "tokens": ["Doch", "fes\u00b7ten", "Schritts", "in", "ih\u00b7re", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steigt er hinunter. \u00bbH\u00f6rt, ihr alle,", "tokens": ["Steigt", "er", "hin\u00b7un\u00b7ter", ".", "\u00bb", "H\u00f6rt", ",", "ihr", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "$(", "VVIMP", "$,", "PPER", "PIS", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df diesen f\u00fcr Verrat und Trug", "tokens": ["Da\u00df", "die\u00b7sen", "f\u00fcr", "Ver\u00b7rat", "und", "Trug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich in gerechtem Kampf erschlug!", "tokens": ["Ich", "in", "ge\u00b7rech\u00b7tem", "Kampf", "er\u00b7schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Leiche werft hinab vom Walle,", "tokens": ["Die", "Lei\u00b7che", "werft", "hin\u00b7ab", "vom", "Wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Damit wir K\u00f6nig Franz belehren,", "tokens": ["Da\u00b7mit", "wir", "K\u00f6\u00b7nig", "Franz", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.