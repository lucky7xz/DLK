{"dta.poem.7491": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XlVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach langem Ringen ist der Tag gewichen;", "tokens": ["Nach", "lan\u00b7gem", "Rin\u00b7gen", "ist", "der", "Tag", "ge\u00b7wi\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein reizend Weib im leichten Silberflor,", "tokens": ["Ein", "rei\u00b7zend", "Weib", "im", "leich\u00b7ten", "Sil\u00b7ber\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tritt Luna hinter dem Gebirge vor,", "tokens": ["Tritt", "Lu\u00b7na", "hin\u00b7ter", "dem", "Ge\u00b7bir\u00b7ge", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Der Ostwind ist ihr neckend nachgestrichen.", "tokens": ["Der", "Ost\u00b7wind", "ist", "ihr", "ne\u00b7ckend", "nach\u00b7ge\u00b7stri\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und eine bunte Schar von wunderlichen", "tokens": ["Und", "ei\u00b7ne", "bun\u00b7te", "Schar", "von", "wun\u00b7der\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gestalten taucht vor meinem Blick empor,", "tokens": ["Ge\u00b7stal\u00b7ten", "taucht", "vor", "mei\u00b7nem", "Blick", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie kommen zaghaft, wie ein M\u00e4dchenchor,", "tokens": ["Sie", "kom\u00b7men", "zag\u00b7haft", ",", "wie", "ein", "M\u00e4d\u00b7chen\u00b7chor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wie auf Zehen zu mir angeschlichen.", "tokens": ["Und", "wie", "auf", "Ze\u00b7hen", "zu", "mir", "an\u00b7ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "CARD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Rauschen naht von tausend, tausend Schwingen,", "tokens": ["Ein", "Rau\u00b7schen", "naht", "von", "tau\u00b7send", ",", "tau\u00b7send", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchl', wie Geister meine Stirne k\u00fcssen", "tokens": ["Ich", "f\u00fchl'", ",", "wie", "Geis\u00b7ter", "mei\u00b7ne", "Stir\u00b7ne", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mir die H\u00e4nde legen auf das Haubt.", "tokens": ["Und", "mir", "die", "H\u00e4n\u00b7de", "le\u00b7gen", "auf", "das", "Haubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6r' die Sterne aus den L\u00fcften singen:", "tokens": ["Ich", "h\u00f6r'", "die", "Ster\u00b7ne", "aus", "den", "L\u00fcf\u00b7ten", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201ewohl dem, den wir noch wachen Augs begr\u00fcssen,", "tokens": ["\u201e", "wohl", "dem", ",", "den", "wir", "noch", "wa\u00b7chen", "Augs", "be\u00b7gr\u00fcs\u00b7sen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "$,", "PRELS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der an die Nacht, die heilige, noch glaubt!\u201c", "tokens": ["Der", "an", "die", "Nacht", ",", "die", "hei\u00b7li\u00b7ge", ",", "noch", "glaubt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}