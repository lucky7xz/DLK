{"textgrid.poem.58633": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mir's zugesagt; wann wird es denn geschehen,", "tokens": ["Du", "hast", "mir's", "zu\u00b7ge\u00b7sagt", ";", "wann", "wird", "es", "denn", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVPP", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mich der Kindschaft Geist bei deinen Kindern setzt?", "tokens": ["Da\u00df", "mich", "der", "Kind\u00b7schaft", "Geist", "bei", "dei\u00b7nen", "Kin\u00b7dern", "setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich mu\u00df noch wie ein Knecht mit Furcht von ferne stehen;", "tokens": ["Ich", "mu\u00df", "noch", "wie", "ein", "Knecht", "mit", "Furcht", "von", "fer\u00b7ne", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KOKOM", "ART", "NN", "APPR", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mach mich freim\u00fctig dir, befreie mich zuletzt!", "tokens": ["Mach", "mich", "frei\u00b7m\u00fc\u00b7tig", "dir", ",", "be\u00b7frei\u00b7e", "mich", "zu\u00b7letzt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach, das erw\u00fcnschte Land wollst du mir endlich geben,", "tokens": ["Ach", ",", "das", "er\u00b7w\u00fcnschte", "Land", "wollst", "du", "mir", "end\u00b7lich", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Das Land der Ewigkeit er\u00f6ffne bald in mir,", "tokens": ["Das", "Land", "der", "E\u00b7wig\u00b7keit", "er\u00b7\u00f6ff\u00b7ne", "bald", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nimm ein mein Innerstes, la\u00df keine Heiden leben,", "tokens": ["Nimm", "ein", "mein", "In\u00b7ners\u00b7tes", ",", "la\u00df", "kei\u00b7ne", "Hei\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PPOSAT", "NN", "$,", "VVIMP", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann nenn' ich \u203aAbba\u2039 dich und weiche nicht von dir!", "tokens": ["Dann", "nenn'", "ich", "\u203a", "Ab\u00b7ba", "\u2039", "dich", "und", "wei\u00b7che", "nicht", "von", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NE", "$(", "PPER", "KON", "ADJA", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}