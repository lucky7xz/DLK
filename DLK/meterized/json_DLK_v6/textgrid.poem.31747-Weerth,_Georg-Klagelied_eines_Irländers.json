{"textgrid.poem.31747": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Klagelied eines Irl\u00e4nders", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sitz ich auf der Bank, Mary,", "tokens": ["Nun", "sitz", "ich", "auf", "der", "Bank", ",", "Ma\u00b7ry", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf der wir sa\u00dfen traut", "tokens": ["Auf", "der", "wir", "sa\u00b7\u00dfen", "traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An dem sch\u00f6nen Morgen im Monat Mai,", "tokens": ["An", "dem", "sch\u00f6\u00b7nen", "Mor\u00b7gen", "im", "Mo\u00b7nat", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als einst du meine Braut.", "tokens": ["Als", "einst", "du", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es spro\u00dfte frisch und gr\u00fcn das Korn,", "tokens": ["Es", "spro\u00df\u00b7te", "frisch", "und", "gr\u00fcn", "das", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und die Lerche sang so weit;", "tokens": ["Und", "die", "Ler\u00b7che", "sang", "so", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dein Mund war rosarot, Mary,", "tokens": ["Dein", "Mund", "war", "ro\u00b7sa\u00b7rot", ",", "Ma\u00b7ry", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Auge voll Lieblichkeit.", "tokens": ["Dein", "Au\u00b7ge", "voll", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Bank ist ganz wie sonst, Mary,", "tokens": ["Die", "Bank", "ist", "ganz", "wie", "sonst", ",", "Ma\u00b7ry", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KOKOM", "ADV", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00f6n ist des Morgens Gl\u00fchn.", "tokens": ["Sch\u00f6n", "ist", "des", "Mor\u00b7gens", "Gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADV", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wie damals steigt die Lerche auf,", "tokens": ["Wie", "da\u00b7mals", "steigt", "die", "Ler\u00b7che", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und das Korn ist wieder gr\u00fcn;", "tokens": ["Und", "das", "Korn", "ist", "wie\u00b7der", "gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch f\u00fchl ich nicht den Druck der Hand,", "tokens": ["Doch", "f\u00fchl", "ich", "nicht", "den", "Druck", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht deines Atems Hauch,", "tokens": ["Nicht", "dei\u00b7nes", "A\u00b7tems", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nicht t\u00f6nt mir deine Stimme mehr,", "tokens": ["Nicht", "t\u00f6nt", "mir", "dei\u00b7ne", "Stim\u00b7me", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So oft ich horche auch.", "tokens": ["So", "oft", "ich", "hor\u00b7che", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zur kleinen Kirche will ich gehn,", "tokens": ["Zur", "klei\u00b7nen", "Kir\u00b7che", "will", "ich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Turm seh ich von hier;", "tokens": ["Den", "Turm", "seh", "ich", "von", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In jener Kirche wurd ich einst,", "tokens": ["In", "je\u00b7ner", "Kir\u00b7che", "wurd", "ich", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mary, getraut mit dir.", "tokens": ["Ma\u00b7ry", ",", "ge\u00b7traut", "mit", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch \u00fcbern Kirchhof m\u00fc\u00dft ich ja \u2013", "tokens": ["Doch", "\u00fc\u00b7bern", "Kirch\u00b7hof", "m\u00fc\u00dft", "ich", "ja", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "M\u00f6cht st\u00f6ren deine Rast,", "tokens": ["M\u00f6cht", "st\u00f6\u00b7ren", "dei\u00b7ne", "Rast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lieb Mary, die du tief im Grab", "tokens": ["Lieb", "Ma\u00b7ry", ",", "die", "du", "tief", "im", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PRELS", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Dein Kind am Busen hast.", "tokens": ["Dein", "Kind", "am", "Bu\u00b7sen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verlassen bin ich \u2013 neue Freunde,", "tokens": ["Ver\u00b7las\u00b7sen", "bin", "ich", "\u2013", "neu\u00b7e", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Arme findet sie so schwer;", "tokens": ["Der", "Ar\u00b7me", "fin\u00b7det", "sie", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch oh, die wen'gen, die er findet,", "tokens": ["Doch", "oh", ",", "die", "wen'\u00b7gen", ",", "die", "er", "fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "FM", "$,", "PRELS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er liebt sie desto mehr!", "tokens": ["Er", "liebt", "sie", "des\u00b7to", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und du warst ja mein Alles, Mary,", "tokens": ["Und", "du", "warst", "ja", "mein", "Al\u00b7les", ",", "Ma\u00b7ry", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PPOSAT", "PIS", "$,", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Mein Stolz und meine Lust,", "tokens": ["Mein", "Stolz", "und", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Alles, ach, verlor ich, Mary,", "tokens": ["Und", "Al\u00b7les", ",", "ach", ",", "ver\u00b7lor", "ich", ",", "Ma\u00b7ry", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "ITJ", "$,", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Als sterben du gemu\u00dft.", "tokens": ["Als", "ster\u00b7ben", "du", "ge\u00b7mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit deinem treuen, guten Herzen,", "tokens": ["Mit", "dei\u00b7nem", "treu\u00b7en", ",", "gu\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hofftest du so lang,", "tokens": ["Wie", "hoff\u00b7test", "du", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als mit dem alten Gottvertrauen", "tokens": ["Als", "mit", "dem", "al\u00b7ten", "Gott\u00b7ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Arm ermattet sank!", "tokens": ["Mein", "Arm", "er\u00b7mat\u00b7tet", "sank", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trost sprachst du mir in meine Seele", "tokens": ["Trost", "sprachst", "du", "mir", "in", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sahst mich bittend an \u2013", "tokens": ["Und", "sahst", "mich", "bit\u00b7tend", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Dank sei, Mary, dir f\u00fcr Alles,", "tokens": ["Und", "Dank", "sei", ",", "Ma\u00b7ry", ",", "dir", "f\u00fcr", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "NE", "$,", "PPER", "APPR", "PIS", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Was du mir Liebes getan!", "tokens": ["Was", "du", "mir", "Lie\u00b7bes", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Dank dir f\u00fcr dein geduldig L\u00e4cheln,", "tokens": ["Dank", "dir", "f\u00fcr", "dein", "ge\u00b7dul\u00b7dig", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als du, vom Hunger geplagt,", "tokens": ["Als", "du", ",", "vom", "Hun\u00b7ger", "ge\u00b7plagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Deine Qual verbargst um meinetwillen", "tokens": ["Dei\u00b7ne", "Qual", "ver\u00b7bargst", "um", "mei\u00b7net\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und nicht ein Wort gesagt!", "tokens": ["Und", "nicht", "ein", "Wort", "ge\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Dank dir f\u00fcr dein letztes Gr\u00fc\u00dfen,", "tokens": ["Und", "Dank", "dir", "f\u00fcr", "dein", "letz\u00b7tes", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als ach, dein Herze brach,", "tokens": ["Als", "ach", ",", "dein", "Her\u00b7ze", "brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ITJ", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und oh, es freut mich, da\u00df du weilest,", "tokens": ["Und", "oh", ",", "es", "freut", "mich", ",", "da\u00df", "du", "wei\u00b7lest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo nichts nun kr\u00e4nken dich mag.", "tokens": ["Wo", "nichts", "nun", "kr\u00e4n\u00b7ken", "dich", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "PRF", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ade! Von dannen mu\u00df ich ziehen,", "tokens": ["A\u00b7de", "!", "Von", "dan\u00b7nen", "mu\u00df", "ich", "zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mu\u00df lassen der Heimat Strand;", "tokens": ["Mu\u00df", "las\u00b7sen", "der", "Hei\u00b7mat", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch werd ich auch dein gedenken, Mary,", "tokens": ["Doch", "werd", "ich", "auch", "dein", "ge\u00b7den\u00b7ken", ",", "Ma\u00b7ry", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "VVINF", "$,", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In dem fernen, neuen Land.", "tokens": ["In", "dem", "fer\u00b7nen", ",", "neu\u00b7en", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Man sagt, dort gibt es Brot genug,", "tokens": ["Man", "sagt", ",", "dort", "gibt", "es", "Brot", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und die Sonne geht nimmer zur Ruh \u2013", "tokens": ["Und", "die", "Son\u00b7ne", "geht", "nim\u00b7mer", "zur", "Ruh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "--+-+++-+", "measure": "anapaest.init"}, "line.7": {"text": "Doch nimmer verge\u00df ich, Alt-Irland, dich,", "tokens": ["Doch", "nim\u00b7mer", "ver\u00b7ge\u00df", "ich", ",", "Al\u00b7tIr\u00b7land", ",", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "W\u00e4r's auch dreimal sch\u00f6ner als du!", "tokens": ["W\u00e4r's", "auch", "drei\u00b7mal", "sch\u00f6\u00b7ner", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "In jenen alten, gro\u00dfen W\u00e4ldern", "tokens": ["In", "je\u00b7nen", "al\u00b7ten", ",", "gro\u00b7\u00dfen", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich sitzen, ein einsamer Mann;", "tokens": ["Will", "ich", "sit\u00b7zen", ",", "ein", "ein\u00b7sa\u00b7mer", "Mann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und zur\u00fcck nach dem Ort, wo Mary ruht,", "tokens": ["Und", "zu\u00b7r\u00fcck", "nach", "dem", "Ort", ",", "wo", "Ma\u00b7ry", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wird reisen mein Herze dann,", "tokens": ["Wird", "rei\u00b7sen", "mein", "Her\u00b7ze", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PPOSAT", "VVFIN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bis ich meine, ich s\u00e4he die kleine Bank,", "tokens": ["Bis", "ich", "mei\u00b7ne", ",", "ich", "s\u00e4\u00b7he", "die", "klei\u00b7ne", "Bank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wo zusammen wir sa\u00dfen traut", "tokens": ["Wo", "zu\u00b7sam\u00b7men", "wir", "sa\u00b7\u00dfen", "traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "An dem sch\u00f6nen Morgen im Monat Mai,", "tokens": ["An", "dem", "sch\u00f6\u00b7nen", "Mor\u00b7gen", "im", "Mo\u00b7nat", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als einst du meine Braut.", "tokens": ["Als", "einst", "du", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}