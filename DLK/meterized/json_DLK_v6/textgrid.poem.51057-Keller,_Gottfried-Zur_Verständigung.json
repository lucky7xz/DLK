{"textgrid.poem.51057": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Zur Verst\u00e4ndigung", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu bist ein Schreier, bist ein frecher Prahler,", "tokens": ["\u00bb", "du", "bist", "ein", "Schrei\u00b7er", ",", "bist", "ein", "fre\u00b7cher", "Prah\u00b7ler", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Drescher mehr auf abgedroschnen Halmen,", "tokens": ["Ein", "Dre\u00b7scher", "mehr", "auf", "ab\u00b7ge\u00b7drosc\u00b7hnen", "Hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein R\u00e4uchlein mehr in der Emp\u00f6rung Qualmen,", "tokens": ["Ein", "R\u00e4uch\u00b7lein", "mehr", "in", "der", "Em\u00b7p\u00f6\u00b7rung", "Qual\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Vielversprecher und ein Wenigzahler!\u00ab", "tokens": ["Ein", "Viel\u00b7ver\u00b7spre\u00b7cher", "und", "ein", "We\u00b7nig\u00b7zah\u00b7ler", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gemach, o du Philisterschwarm, du kahler!", "tokens": ["Ge\u00b7mach", ",", "o", "du", "Phi\u00b7lis\u00b7ter\u00b7schwarm", ",", "du", "kah\u00b7ler", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "PPER", "NN", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei dir nicht such und find ich meine Palmen;", "tokens": ["Bei", "dir", "nicht", "such", "und", "find", "ich", "mei\u00b7ne", "Pal\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "ADJD", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "S\u00e4ng ich, ein David, dir die hehrsten Psalmen,", "tokens": ["S\u00e4ng", "ich", ",", "ein", "Da\u00b7vid", ",", "dir", "die", "hehrs\u00b7ten", "Psal\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NE", "$,", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie deuchten durch dein Lob mir soviel schaler.", "tokens": ["Sie", "deuch\u00b7ten", "durch", "dein", "Lob", "mir", "so\u00b7viel", "scha\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PIAT", "ADJA", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich geb es zu, ich habe laut geschrieen,", "tokens": ["Ich", "geb", "es", "zu", ",", "ich", "ha\u00b7be", "laut", "ge\u00b7schri\u00b7een", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein rauhes Echo von geweihtern T\u00f6nen,", "tokens": ["Ein", "rau\u00b7hes", "E\u00b7cho", "von", "ge\u00b7weih\u00b7tern", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nur die gute Sache mag mich tragen!", "tokens": ["Und", "nur", "die", "gu\u00b7te", "Sa\u00b7che", "mag", "mich", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch ist's mein Herzblut, das ich ausgespieen,", "tokens": ["Doch", "ist's", "mein", "Herz\u00b7blut", ",", "das", "ich", "aus\u00b7ge\u00b7spi\u00b7een", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Schlachtschrei, der beim Angriff mu\u00df erdr\u00f6hnen;", "tokens": ["Der", "Schlacht\u00b7schrei", ",", "der", "beim", "An\u00b7griff", "mu\u00df", "er\u00b7dr\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf diesen folgt ein regelrechtes Schlagen!", "tokens": ["Auf", "die\u00b7sen", "folgt", "ein", "re\u00b7gel\u00b7rech\u00b7tes", "Schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}