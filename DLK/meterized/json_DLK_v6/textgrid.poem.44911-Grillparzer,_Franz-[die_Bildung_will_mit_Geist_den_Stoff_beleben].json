{"textgrid.poem.44911": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die Bildung will mit Geist den Stoff beleben]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bildung will mit Geist den Stoff beleben,", "tokens": ["Die", "Bil\u00b7dung", "will", "mit", "Geist", "den", "Stoff", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch findet sie Unl\u00f6sbares genug;", "tokens": ["Doch", "fin\u00b7det", "sie", "Un\u00b7l\u00f6s\u00b7ba\u00b7res", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wirst du nun aus dem bunten Zeug nicht klug,", "tokens": ["Wirst", "du", "nun", "aus", "dem", "bun\u00b7ten", "Zeug", "nicht", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So nimm es achselzuckend als gegeben.", "tokens": ["So", "nimm", "es", "ach\u00b7sel\u00b7zu\u00b7ckend", "als", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}