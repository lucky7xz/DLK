{"dta.poem.14413": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mari\u00e4 Sehnsucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging Maria in stiller Nacht,", "tokens": ["Es", "ging", "Ma\u00b7ria", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde schlief, der Himmel wacht',", "tokens": ["Die", "Er\u00b7de", "schlief", ",", "der", "Him\u00b7mel", "wacht'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch's Herze, wie sie ging und sann und dacht',", "tokens": ["Und", "durch's", "Her\u00b7ze", ",", "wie", "sie", "ging", "und", "sann", "und", "dacht'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Zogen die Sterne mit goldener Pracht.", "tokens": ["Zo\u00b7gen", "die", "Ster\u00b7ne", "mit", "gol\u00b7de\u00b7ner", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "\u201each, h\u00e4tt' ich das Brautkleid von Himmelsschein,", "tokens": ["\u201e", "ach", ",", "h\u00e4tt'", "ich", "das", "Braut\u00b7kleid", "von", "Him\u00b7mels\u00b7schein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und goldene Sterne gewoben drein!\u201c", "tokens": ["Und", "gol\u00b7de\u00b7ne", "Ster\u00b7ne", "ge\u00b7wo\u00b7ben", "drein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "VAINF", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}