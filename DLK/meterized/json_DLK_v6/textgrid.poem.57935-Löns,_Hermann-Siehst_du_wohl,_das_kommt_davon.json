{"textgrid.poem.57935": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Siehst du wohl, das kommt davon", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Eilenriede ein Wirtshaus steht,", "tokens": ["In", "der", "Ei\u00b7len\u00b7rie\u00b7de", "ein", "Wirts\u00b7haus", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So mancher dran vor\u00fcbergeht,", "tokens": ["So", "man\u00b7cher", "dran", "vor\u00b7\u00fc\u00b7ber\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wirt, der nahm die besten", "tokens": ["Der", "Wirt", ",", "der", "nahm", "die", "bes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Pl\u00e4tze und reservierte sie", "tokens": ["Pl\u00e4t\u00b7ze", "und", "re\u00b7ser\u00b7vier\u00b7te", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Den weinverzehrenden G\u00e4sten.", "tokens": ["Den", "wein\u00b7ver\u00b7zeh\u00b7ren\u00b7den", "G\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Herr Wirt hatt' aber manchen Gast,", "tokens": ["Herr", "Wirt", "hatt'", "a\u00b7ber", "man\u00b7chen", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem hatte dieses nicht gepa\u00dft;", "tokens": ["Dem", "hat\u00b7te", "die\u00b7ses", "nicht", "ge\u00b7pa\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PDS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sprach in seiner Gemeinheit:", "tokens": ["Er", "sprach", "in", "sei\u00b7ner", "Ge\u00b7mein\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbnun geh' ich ganz woanders hin;", "tokens": ["\u00bb", "nun", "geh'", "ich", "ganz", "woan\u00b7ders", "hin", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich fl\u00f6tje auf die Feinheit.\u00ab", "tokens": ["Ich", "fl\u00f6t\u00b7je", "auf", "die", "Fein\u00b7heit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Herr Wirt hatt' auch ein Portemonn\u00e4,", "tokens": ["Herr", "Wirt", "hatt'", "auch", "ein", "Por\u00b7te\u00b7mon\u00b7n\u00e4", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sah er das an, so ward ihm weh';", "tokens": ["Sah", "er", "das", "an", ",", "so", "ward", "ihm", "weh'", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ging zum Magistrate", "tokens": ["Er", "ging", "zum", "Ma\u00b7gist\u00b7ra\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach: \u00bbIch m\u00f6cht' entbunden sein!\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Ich", "m\u00f6cht'", "ent\u00b7bun\u00b7den", "sein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Worauf man das auch tate.", "tokens": ["Wo\u00b7rauf", "man", "das", "auch", "ta\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PDS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun kam ein anderer Wirt dahin,", "tokens": ["Nun", "kam", "ein", "an\u00b7de\u00b7rer", "Wirt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem wurde ziemlich mies zu Sinn,", "tokens": ["Dem", "wur\u00b7de", "ziem\u00b7lich", "mies", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als er tagt\u00e4glich merkte,", "tokens": ["Als", "er", "tag\u00b7t\u00e4g\u00b7lich", "merk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie man bei ihm vor\u00fcberging", "tokens": ["Wie", "man", "bei", "ihm", "vor\u00b7\u00fc\u00b7ber\u00b7ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sich woanders st\u00e4rkte.", "tokens": ["Und", "sich", "woan\u00b7ders", "st\u00e4rk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Er stiefelte zum Magistrat", "tokens": ["Er", "stie\u00b7fel\u00b7te", "zum", "Ma\u00b7gist\u00b7rat"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach: \u00bbNun schaffet einmal Rat!\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Nun", "schaf\u00b7fet", "ein\u00b7mal", "Rat", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VVFIN", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lie\u00df man nun demselben", "tokens": ["Da", "lie\u00df", "man", "nun", "dem\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PDAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein gro\u00dfes, gr\u00fcnes Mordsplakat,", "tokens": ["Ein", "gro\u00b7\u00dfes", ",", "gr\u00fc\u00b7nes", "Mord\u00b7spla\u00b7kat", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sich der Weg zwillt, w\u00f6lben.", "tokens": ["Wo", "sich", "der", "Weg", "zwillt", ",", "w\u00f6l\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jetzt l\u00e4uft kein Mensch mehr dort vorbei,", "tokens": ["Jetzt", "l\u00e4uft", "kein", "Mensch", "mehr", "dort", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jeder wird von weitem scheu,", "tokens": ["Ein", "je\u00b7der", "wird", "von", "wei\u00b7tem", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Biegt rechts ab oder zur Linken,", "tokens": ["Biegt", "rechts", "ab", "o\u00b7der", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um, wo er es bisher gewohnt,", "tokens": ["Um", ",", "wo", "er", "es", "bis\u00b7her", "ge\u00b7wohnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "PWAV", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sein Bitterbier zu trinken.", "tokens": ["Sein", "Bit\u00b7ter\u00b7bier", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}