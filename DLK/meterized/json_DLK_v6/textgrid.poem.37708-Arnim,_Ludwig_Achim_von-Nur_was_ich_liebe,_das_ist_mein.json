{"textgrid.poem.37708": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Nur was ich liebe, das ist mein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur was ich liebe, das ist mein,", "tokens": ["Nur", "was", "ich", "lie\u00b7be", ",", "das", "ist", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kann nur immer meiner werden;", "tokens": ["Und", "kann", "nur", "im\u00b7mer", "mei\u00b7ner", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du wei\u00dft von nichts, du l\u00e4\u00dft mich ganz allein,", "tokens": ["Du", "wei\u00dft", "von", "nichts", ",", "du", "l\u00e4\u00dft", "mich", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ich in dir geliebt, das bleibt doch mein.", "tokens": ["Was", "ich", "in", "dir", "ge\u00b7liebt", ",", "das", "bleibt", "doch", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$,", "PDS", "VVFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geh\u00f6rt dem Fl\u00fcgel dieser Ton", "tokens": ["Ge\u00b7h\u00f6rt", "dem", "Fl\u00fc\u00b7gel", "die\u00b7ser", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den meine Finger traurig weckten?", "tokens": ["Den", "mei\u00b7ne", "Fin\u00b7ger", "trau\u00b7rig", "weck\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, du bist mein, dir selber recht zum Hohn,", "tokens": ["Nein", ",", "du", "bist", "mein", ",", "dir", "sel\u00b7ber", "recht", "zum", "Hohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPOSAT", "$,", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was ich in dir erweckt, geh\u00f6rt mir schon.", "tokens": ["Was", "ich", "in", "dir", "er\u00b7weckt", ",", "ge\u00b7h\u00f6rt", "mir", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dein Haus ist mein, denn ach von dir", "tokens": ["Dein", "Haus", "ist", "mein", ",", "denn", "ach", "von", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "$,", "KON", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Umschlie\u00dft es so viel sch\u00f6ne Kinder;", "tokens": ["Um\u00b7schlie\u00dft", "es", "so", "viel", "sch\u00f6\u00b7ne", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ist mein die Perle, so geh\u00f6rt auch mir", "tokens": ["Ist", "mein", "die", "Per\u00b7le", ",", "so", "ge\u00b7h\u00f6rt", "auch", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ART", "NN", "$,", "ADV", "VVFIN", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Schale, deines Leibes sch\u00f6ne Zier.", "tokens": ["Die", "Scha\u00b7le", ",", "dei\u00b7nes", "Lei\u00b7bes", "sch\u00f6\u00b7ne", "Zier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ich geb' die Seele, du bist mein,", "tokens": ["Ich", "geb'", "die", "See\u00b7le", ",", "du", "bist", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Du sch\u00f6ner Teufel mu\u00dft mir dienen,", "tokens": ["Du", "sch\u00f6\u00b7ner", "Teu\u00b7fel", "mu\u00dft", "mir", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Hast mich verf\u00fchrt mit sch\u00f6nem Augenschein,", "tokens": ["Hast", "mich", "ver\u00b7f\u00fchrt", "mit", "sch\u00f6\u00b7nem", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Sei alles falsch und leer, du bist doch mein.", "tokens": ["Sei", "al\u00b7les", "falsch", "und", "leer", ",", "du", "bist", "doch", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}