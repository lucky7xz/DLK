{"textgrid.poem.47986": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Tiefes Schweigen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Schleier lichter Wolken", "tokens": ["Aus", "dem", "Schlei\u00b7er", "lich\u00b7ter", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "l\u00e4chelt matt der Sterne Schein", "tokens": ["l\u00e4\u00b7chelt", "matt", "der", "Ster\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "auf die dunkle Welt hernieder,", "tokens": ["auf", "die", "dunk\u00b7le", "Welt", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auf den totenstillen Hain.", "tokens": ["auf", "den", "to\u00b7tens\u00b7til\u00b7len", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lautlos ruht das Reh des Waldes,", "tokens": ["Laut\u00b7los", "ruht", "das", "Reh", "des", "Wal\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "lautlos steht des Waldes Baum,", "tokens": ["laut\u00b7los", "steht", "des", "Wal\u00b7des", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und, von Geisterhand gesponnen,", "tokens": ["und", ",", "von", "Geis\u00b7ter\u00b7hand", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "deckt die Welt ein tiefer Traum.", "tokens": ["deckt", "die", "Welt", "ein", "tie\u00b7fer", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lautlos, wie des Waldes Schweigen,", "tokens": ["Laut\u00b7los", ",", "wie", "des", "Wal\u00b7des", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ist der Menschenseele Schmerz \u2013", "tokens": ["ist", "der", "Men\u00b7schen\u00b7see\u00b7le", "Schmerz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterne blicken aus dem Schleier", "tokens": ["Ster\u00b7ne", "bli\u00b7cken", "aus", "dem", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "lichter Wolken niederw\u00e4rts.", "tokens": ["lich\u00b7ter", "Wol\u00b7ken", "nie\u00b7der\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}