{"textgrid.poem.34706": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unterm wei\u00dfen Baume sitzend,", "tokens": ["Un\u00b7term", "wei\u00b7\u00dfen", "Bau\u00b7me", "sit\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rst du fern die Winde schrillen,", "tokens": ["H\u00f6rst", "du", "fern", "die", "Win\u00b7de", "schril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst, wie oben stumme Wolken", "tokens": ["Siehst", ",", "wie", "o\u00b7ben", "stum\u00b7me", "Wol\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in Nebeldecken h\u00fcllen;", "tokens": ["Sich", "in", "Ne\u00b7bel\u00b7de\u00b7cken", "h\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Siehst, wie unten ausgestorben", "tokens": ["Siehst", ",", "wie", "un\u00b7ten", "aus\u00b7ge\u00b7stor\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wald und Flur, wie kahl geschoren; \u2013", "tokens": ["Wald", "und", "Flur", ",", "wie", "kahl", "ge\u00b7scho\u00b7ren", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um dich Winter, in dir Winter,", "tokens": ["Um", "dich", "Win\u00b7ter", ",", "in", "dir", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "$,", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein Herz ist eingefroren.", "tokens": ["Und", "dein", "Herz", "ist", "ein\u00b7ge\u00b7fro\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich fallen auf dich nieder", "tokens": ["Pl\u00f6tz\u00b7lich", "fal\u00b7len", "auf", "dich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfe Flocken, und verdrossen", "tokens": ["Wei\u00b7\u00dfe", "Flo\u00b7cken", ",", "und", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinst du schon, mit Schneegest\u00f6ber", "tokens": ["Meinst", "du", "schon", ",", "mit", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab der Baum dich \u00fcbergossen.", "tokens": ["Hab", "der", "Baum", "dich", "\u00fc\u00b7ber\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch es ist kein Schneegest\u00f6ber,", "tokens": ["Doch", "es", "ist", "kein", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Merkst es bald mit freud'gem Schrecken;", "tokens": ["Merkst", "es", "bald", "mit", "freu\u00b7d'\u00b7gem", "Schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Duft'ge Fr\u00fchlingsbl\u00fcten sind es,", "tokens": ["Duft'\u00b7ge", "Fr\u00fch\u00b7lings\u00b7bl\u00fc\u00b7ten", "sind", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dich necken und bedecken.", "tokens": ["Die", "dich", "ne\u00b7cken", "und", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Welch ein schauers\u00fc\u00dfer Zauber!", "tokens": ["Welch", "ein", "schau\u00b7er\u00b7s\u00fc\u00b7\u00dfer", "Zau\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winter wandelt sich in Maie,", "tokens": ["Win\u00b7ter", "wan\u00b7delt", "sich", "in", "Mai\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schnee verwandelt sich in Bl\u00fcten,", "tokens": ["Schnee", "ver\u00b7wan\u00b7delt", "sich", "in", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein Herz, es liebt aufs neue.", "tokens": ["Und", "dein", "Herz", ",", "es", "liebt", "aufs", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}