{"dta.poem.11546": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Heimweh.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Anders wird die Welt mit jedem Schritt,", "tokens": ["An\u00b7ders", "wird", "die", "Welt", "mit", "je\u00b7dem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den ich weiter von der Liebsten mache;", "tokens": ["Den", "ich", "wei\u00b7ter", "von", "der", "Liebs\u00b7ten", "ma\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mein Herz, das will nicht weiter mit!", "tokens": ["Mein", "Herz", ",", "das", "will", "nicht", "wei\u00b7ter", "mit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VMFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier scheint die Sonne kalt in's Land,", "tokens": ["Hier", "scheint", "die", "Son\u00b7ne", "kalt", "in's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier d\u00e4ucht mir Alles unbekannt,", "tokens": ["Hier", "d\u00e4ucht", "mir", "Al\u00b7les", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sogar die Blumen am Bache!", "tokens": ["So\u00b7gar", "die", "Blu\u00b7men", "am", "Ba\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Hat jede Sache", "tokens": ["Hat", "je\u00b7de", "Sa\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "So fremd eine Miene, so falsch ein Gesicht.", "tokens": ["So", "fremd", "ei\u00b7ne", "Mie\u00b7ne", ",", "so", "falsch", "ein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Das B\u00e4chlein murmelt wohl und spricht:", "tokens": ["Das", "B\u00e4ch\u00b7lein", "mur\u00b7melt", "wohl", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Armer Knabe, komm' bei mir vor\u00fcber,", "tokens": ["Ar\u00b7mer", "Kna\u00b7be", ",", "komm'", "bei", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Siehst auch hier Vergi\u00dfmeinnicht!", "tokens": ["Siehst", "auch", "hier", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "\u2014 Ja, die sind sch\u00f6n an jedem Ort,", "tokens": ["Ja", ",", "die", "sind", "sch\u00f6n", "an", "je\u00b7dem", "Ort", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PRELS", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Aber nicht wie dort!", "tokens": ["A\u00b7ber", "nicht", "wie", "dort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Fort, nur fort!", "tokens": ["Fort", ",", "nur", "fort", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Die Augen gehn mir \u00fcber!", "tokens": ["Die", "Au\u00b7gen", "gehn", "mir", "\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}