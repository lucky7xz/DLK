{"textgrid.poem.61153": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gegen den Strom", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freiheit hab' ich stets im Sinn getragen,", "tokens": ["Die", "Frei\u00b7heit", "hab'", "ich", "stets", "im", "Sinn", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch hass' ich eins noch grimmer als Despoten:", "tokens": ["Doch", "hass'", "ich", "eins", "noch", "grim\u00b7mer", "als", "Des\u00b7po\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PIS", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist der P\u00f6bel, wenn er sich den roten,", "tokens": ["Das", "ist", "der", "P\u00f6\u00b7bel", ",", "wenn", "er", "sich", "den", "ro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerfetzten K\u00f6nigsmantel umgeschlagen.", "tokens": ["Zer\u00b7fetz\u00b7ten", "K\u00f6\u00b7nigs\u00b7man\u00b7tel", "um\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die kleinen Seelen gl\u00fchn in solchen Tagen,", "tokens": ["Die", "klei\u00b7nen", "See\u00b7len", "gl\u00fchn", "in", "sol\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich aufzuspreizen als des Himmels Boten,", "tokens": ["Sich", "auf\u00b7zu\u00b7sprei\u00b7zen", "als", "des", "Him\u00b7mels", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und frech verl\u00e4stern sie die gro\u00dfen Toten,", "tokens": ["Und", "frech", "ver\u00b7l\u00e4s\u00b7tern", "sie", "die", "gro\u00b7\u00dfen", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, wem das Herz nur h\u00f6her wagt zu pochen,", "tokens": ["Ja", ",", "wem", "das", "Herz", "nur", "h\u00f6\u00b7her", "wagt", "zu", "po\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "ART", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus wem der Geist, der heil'ge, gottgesandte,", "tokens": ["Aus", "wem", "der", "Geist", ",", "der", "heil'\u00b7ge", ",", "gott\u00b7ge\u00b7sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PWS", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erhaben z\u00fcrnt, sein Urteil ist gesprochen.", "tokens": ["Er\u00b7ha\u00b7ben", "z\u00fcrnt", ",", "sein", "Ur\u00b7teil", "ist", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hat doch der P\u00f6bel einst, der wutentbrannte,", "tokens": ["Hat", "doch", "der", "P\u00f6\u00b7bel", "einst", ",", "der", "wu\u00b7tent\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob Aristides' Haupt den Stab gebrochen", "tokens": ["Ob", "A\u00b7ris\u00b7ti\u00b7des'", "Haupt", "den", "Stab", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "ART", "NN", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ins Exil versto\u00dfen einen Dante.", "tokens": ["Und", "ins", "E\u00b7xil", "ver\u00b7sto\u00b7\u00dfen", "ei\u00b7nen", "Dan\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}