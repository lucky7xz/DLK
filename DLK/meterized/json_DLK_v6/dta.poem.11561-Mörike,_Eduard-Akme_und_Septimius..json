{"dta.poem.11561": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Akme und Septimius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Akme, seine Geliebte, auf dem Schoose", "tokens": ["Ak\u00b7me", ",", "sei\u00b7ne", "Ge\u00b7lieb\u00b7te", ",", "auf", "dem", "Schoo\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Haltend, sagte Septimius: \u201eMeine Akme!", "tokens": ["Hal\u00b7tend", ",", "sag\u00b7te", "Sep\u00b7ti\u00b7mi\u00b7us", ":", "\u201e", "Mei\u00b7ne", "Ak\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "NE", "$.", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ueberm\u00e4\u00dfig hab' ich dich lieb und will auch", "tokens": ["Ue\u00b7ber\u00b7m\u00e4\u00b7\u00dfig", "hab'", "ich", "dich", "lieb", "und", "will", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "ADJD", "KON", "VMFIN", "ADV"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Jahr f\u00fcr Jahr dich best\u00e4ndig also lieben,", "tokens": ["Jahr", "f\u00fcr", "Jahr", "dich", "be\u00b7st\u00e4n\u00b7dig", "al\u00b7so", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "So arg wie nur ein Mensch jemals im Stand ist;", "tokens": ["So", "arg", "wie", "nur", "ein", "Mensch", "je\u00b7mals", "im", "Stand", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sieh, sonst mag mir's geschehn, da\u00df ich, ganz einsam,", "tokens": ["Sieh", ",", "sonst", "mag", "mir's", "ge\u00b7schehn", ",", "da\u00df", "ich", ",", "ganz", "ein\u00b7sam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VMFIN", "NE", "VVPP", "$,", "KOUS", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sey's in Libyen, sey's im hei\u00dfen Inder-", "tokens": ["Sey's", "in", "Li\u00b7by\u00b7en", ",", "sey's", "im", "hei\u00b7\u00dfen", "In\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "$,", "VAFIN", "APPRART", "ADJA", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Land, dem t\u00f6dtlichen Blick des Leu'n begegne!\u201c", "tokens": ["Land", ",", "dem", "t\u00f6dt\u00b7li\u00b7chen", "Blick", "des", "Leu'n", "be\u00b7geg\u00b7ne", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Wie er dieses gesagt, nies't Amor, herzlich", "tokens": ["Wie", "er", "die\u00b7ses", "ge\u00b7sagt", ",", "nies't", "A\u00b7mor", ",", "herz\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PDAT", "VVPP", "$,", "VVFIN", "NE", "$,", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Es bekr\u00e4ftigend (sonst war er ihm abhold).", "tokens": ["Es", "be\u00b7kr\u00e4f\u00b7ti\u00b7gend", "(", "sonst", "war", "er", "ihm", "ab\u00b7hold", ")", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVPP", "$(", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$(", "$."], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Akme, r\u00fcckw\u00e4rts ihr K\u00f6pfchen leicht gebogen,", "tokens": ["Ak\u00b7me", ",", "r\u00fcck\u00b7w\u00e4rts", "ihr", "K\u00f6pf\u00b7chen", "leicht", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und die trunkenen Augen ihres s\u00fc\u00dfen", "tokens": ["Und", "die", "trun\u00b7ke\u00b7nen", "Au\u00b7gen", "ih\u00b7res", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Knaben k\u00fcssend mit jenem Purpurmunde,", "tokens": ["Kna\u00b7ben", "k\u00fcs\u00b7send", "mit", "je\u00b7nem", "Pur\u00b7pur\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Sprach: \u201eMein Leben! o du mein Septiminchen!", "tokens": ["Sprach", ":", "\u201e", "Mein", "Le\u00b7ben", "!", "o", "du", "mein", "Sep\u00b7ti\u00b7min\u00b7chen", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPOSAT", "NN", "$.", "FM", "PPER", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "k\u00fcnftig dienen wir ", "tokens": ["k\u00fcnf\u00b7tig", "die\u00b7nen", "wir"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Hei\u00dfer Feuer im zarten Marke gl\u00fchet!\u201c", "tokens": ["Hei\u00b7\u00dfer", "Feu\u00b7er", "im", "zar\u00b7ten", "Mar\u00b7ke", "gl\u00fc\u00b7het", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Wie sie dieses gesagt, nies't Amor, herzlich", "tokens": ["Wie", "sie", "die\u00b7ses", "ge\u00b7sagt", ",", "nies't", "A\u00b7mor", ",", "herz\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PDS", "VVPP", "$,", "VVFIN", "NE", "$,", "ADJD"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Es bekr\u00e4ftigend (sonst war er ihr abhold).", "tokens": ["Es", "be\u00b7kr\u00e4f\u00b7ti\u00b7gend", "(", "sonst", "war", "er", "ihr", "ab\u00b7hold", ")", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVPP", "$(", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$(", "$."], "meter": "+-+-+-+--++", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Auf so g\u00fcnstige Zeichen nunmehr bauend", "tokens": ["Auf", "so", "g\u00fcns\u00b7ti\u00b7ge", "Zei\u00b7chen", "nun\u00b7mehr", "bau\u00b7end"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "ADV", "VVPP"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Tauschen Beide von Herzen Lieb' um Liebe.", "tokens": ["Tau\u00b7schen", "Bei\u00b7de", "von", "Her\u00b7zen", "Lieb'", "um", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Nur in Akme allein lebt noch Septimius,", "tokens": ["Nur", "in", "Ak\u00b7me", "al\u00b7lein", "lebt", "noch", "Sep\u00b7ti\u00b7mi\u00b7us", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "VVFIN", "ADV", "NE", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.22": {"text": "Die ihm theurer als Syrien und Britannien,", "tokens": ["Die", "ihm", "theu\u00b7rer", "als", "Sy\u00b7ri\u00b7en", "und", "Bri\u00b7tan\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KOKOM", "NE", "KON", "NE", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Nur Septimius widmet Akme treulich", "tokens": ["Nur", "Sep\u00b7ti\u00b7mi\u00b7us", "wid\u00b7met", "Ak\u00b7me", "treu\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "All' ihr S\u00fc\u00dfes und alle Liebeswonnen.", "tokens": ["All'", "ihr", "S\u00fc\u00b7\u00dfes", "und", "al\u00b7le", "Lie\u00b7bes\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Kein gl\u00fcckseliger Paar hat man gesehen,", "tokens": ["Kein", "gl\u00fcck\u00b7se\u00b7li\u00b7ger", "Paar", "hat", "man", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Keine Liebe, so sch\u00f6n vom Gott besiegelt!", "tokens": ["Kei\u00b7ne", "Lie\u00b7be", ",", "so", "sch\u00f6n", "vom", "Gott", "be\u00b7sie\u00b7gelt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+-+--+-+-++", "measure": "trochaic.hexa.relaxed"}}}}}