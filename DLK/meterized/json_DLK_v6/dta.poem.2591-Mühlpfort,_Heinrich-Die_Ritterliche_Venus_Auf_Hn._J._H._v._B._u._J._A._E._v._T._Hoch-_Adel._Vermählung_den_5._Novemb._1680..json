{"dta.poem.2591": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Ritterliche Venus  \n  Auf Hn. J. H. v. B. u. J. A. E. v. T. Hoch-  \n Adel. Verm\u00e4hlung/ den 5. Novemb.  \n 1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dars als er einst vergn\u00fcgt aus Venus Armen stieg/", "tokens": ["Dars", "als", "er", "einst", "ver\u00b7gn\u00fcgt", "aus", "Ve\u00b7nus", "Ar\u00b7men", "stieg", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "VVPP", "APPR", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Wollust \u00fcberstr\u00f6hmt/ von Liebe satt gespeiset/", "tokens": ["Von", "Wol\u00b7lust", "\u00fc\u00b7ber\u00b7str\u00f6hmt", "/", "von", "Lie\u00b7be", "satt", "ge\u00b7spei\u00b7set", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "APPR", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sprach; G\u00f6ttin da\u00df mein Hertz sich danckbahr dir er-", "tokens": ["Sprach", ";", "G\u00f6t\u00b7tin", "da\u00df", "mein", "Hertz", "sich", "dan\u00b7ck\u00b7bahr", "dir", "er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "KOUS", "PPOSAT", "NN", "PRF", "ADV", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dein erhitzter Ku\u00df mehr als ein blutig Sieg", "tokens": ["Da\u00df", "dein", "er\u00b7hitz\u00b7ter", "Ku\u00df", "mehr", "als", "ein", "blu\u00b7tig", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PIAT", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich auffgemuntert hat/ so schwer\u2019 ich bey den Leichen", "tokens": ["Mich", "auff\u00b7ge\u00b7mun\u00b7tert", "hat", "/", "so", "schwer'", "ich", "bey", "den", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die diese Faust gew\u00fcrgt/ ich wil dir alles reichen.", "tokens": ["Die", "die\u00b7se", "Faust", "ge\u00b7w\u00fcrgt", "/", "ich", "wil", "dir", "al\u00b7les", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$(", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Venus redte drauff und fiel ihm umb den Leib/", "tokens": ["Die", "Ve\u00b7nus", "red\u00b7te", "drauff", "und", "fiel", "ihm", "umb", "den", "Leib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Feld-Herr aller Welt/ sol ich noch was begehren?", "tokens": ["Du", "Feld\u00b7Herr", "al\u00b7ler", "Welt", "/", "sol", "ich", "noch", "was", "be\u00b7geh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$(", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df nicht Vulcanus Nutz uns wieder wird beschweren/", "tokens": ["Da\u00df", "nicht", "Vul\u00b7ca\u00b7nus", "Nutz", "uns", "wie\u00b7der", "wird", "be\u00b7schwe\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NE", "NN", "PPER", "ADV", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist auch buhlen nicht mein bester Zeit-vertreib", "tokens": ["So", "ist", "auch", "buh\u00b7len", "nicht", "mein", "bes\u00b7ter", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVINF", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe deinen Geist und Helden-Muth gefangen", "tokens": ["Ich", "ha\u00b7be", "dei\u00b7nen", "Geist", "und", "Hel\u00b7den\u00b7Muth", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stell\u2019 ein Thurniren an/ so stillst du mein Verlangen.", "tokens": ["Stell'", "ein", "Thur\u00b7ni\u00b7ren", "an", "/", "so", "stillst", "du", "mein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist ein K\u00fcra\u00df mir zu tragen gar nicht schwer.", "tokens": ["Es", "ist", "ein", "K\u00fc\u00b7ra\u00df", "mir", "zu", "tra\u00b7gen", "gar", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich lasse meinen Leib in blancken Stahl einschrauben/", "tokens": ["Ich", "las\u00b7se", "mei\u00b7nen", "Leib", "in", "blan\u00b7cken", "Stahl", "ein\u00b7schrau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sol hier dieser Hand niemand die Kolbe rauben.", "tokens": ["So", "sol", "hier", "die\u00b7ser", "Hand", "nie\u00b7mand", "die", "Kol\u00b7be", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PDAT", "NN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ach da\u00df doch Tag und Stund alsbald benimmet w\u00e4r/", "tokens": ["Ach", "da\u00df", "doch", "Tag", "und", "Stund", "als\u00b7bald", "be\u00b7nim\u00b7met", "w\u00e4r", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "NN", "KON", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie freudig wil ich nicht in hohen Zeugen stechen/", "tokens": ["Wie", "freu\u00b7dig", "wil", "ich", "nicht", "in", "ho\u00b7hen", "Zeu\u00b7gen", "ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nie werden B\u00fcgel-lo\u00df und frisch die Lantzen brechen.", "tokens": ["Nie", "wer\u00b7den", "B\u00fc\u00b7gel\u00b7lo\u00df", "und", "frisch", "die", "Lant\u00b7zen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Der strenge Waffen-Gott schrieb ein Thurniren aus/", "tokens": ["Der", "stren\u00b7ge", "Waf\u00b7fen\u00b7Gott", "schrieb", "ein", "Thur\u00b7ni\u00b7ren", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Pracht und Herrligkeit kan keine Feder melden:", "tokens": ["Die", "Pracht", "und", "Herr\u00b7lig\u00b7keit", "kan", "kei\u00b7ne", "Fe\u00b7der", "mel\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Kern der Ritterschafft/ die Blumen von den Helden", "tokens": ["Der", "Kern", "der", "Rit\u00b7ter\u00b7schafft", "/", "die", "Blu\u00b7men", "von", "den", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Seelen voller Gluth/ verliessen Hof und Hau\u00df", "tokens": ["Die", "See\u00b7len", "vol\u00b7ler", "Gluth", "/", "ver\u00b7lies\u00b7sen", "Hof", "und", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und eilten zu dem Ort! die Erde must\u2019 erzittern/", "tokens": ["Und", "eil\u00b7ten", "zu", "dem", "Ort", "!", "die", "Er\u00b7de", "must'", "er\u00b7zit\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als sie den Auffzug sah bekr\u00f6nt mit so viel Rittern.", "tokens": ["Als", "sie", "den", "Auff\u00b7zug", "sah", "be\u00b7kr\u00f6nt", "mit", "so", "viel", "Rit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADJD", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Erst schalte durch die Lufft der Tromm- und Paucken Klang.", "tokens": ["Erst", "schal\u00b7te", "durch", "die", "Lufft", "der", "Trom\u00b7m", "und", "Pau\u00b7cken", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "TRUNC", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn ritten Glied auff Glied/ die hohe Lantzen f\u00fchrten/", "tokens": ["Denn", "rit\u00b7ten", "Glied", "auff", "Glied", "/", "die", "ho\u00b7he", "Lant\u00b7zen", "f\u00fchr\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So noch zu gr\u00f6ssrer Pracht des Strausses Federn zierten;", "tokens": ["So", "noch", "zu", "gr\u00f6ss\u00b7rer", "Pracht", "des", "Straus\u00b7ses", "Fe\u00b7dern", "zier\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald kamen Fahnen nach/ darinn der L\u00f6we sprang/", "tokens": ["Bald", "ka\u00b7men", "Fah\u00b7nen", "nach", "/", "da\u00b7rinn", "der", "L\u00f6\u00b7we", "sprang", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "$(", "PAV", "ART", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Tyger Klauen wie\u00df/ der Drache Feuer speyte/", "tokens": ["Der", "Ty\u00b7ger", "Klau\u00b7en", "wie\u00df", "/", "der", "Dra\u00b7che", "Feu\u00b7er", "spey\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df das best\u00fcrtzte Volck vor Schrecken trat zu Seite/", "tokens": ["Da\u00df", "das", "be\u00b7st\u00fcrtz\u00b7te", "Volck", "vor", "Schre\u00b7cken", "trat", "zu", "Sei\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drauff zog als im Triumpffder Welt-Bezwinger ein.", "tokens": ["Drauff", "zog", "als", "im", "Tri\u00b7umpff\u00b7der", "Welt\u00b7Be\u00b7zwin\u00b7ger", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KOKOM", "APPRART", "NN", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Wagen troff voll Blut/ der Hengste Sprung und Sch\u00e4umen", "tokens": ["Der", "Wa\u00b7gen", "troff", "voll", "Blut", "/", "der", "Hengs\u00b7te", "Sprung", "und", "Sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$(", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Blie\u00df Feuer-Funcken aus/ die Bahn rein auszur\u00e4umen.", "tokens": ["Blie\u00df", "Feu\u00b7er\u00b7Fun\u00b7cken", "aus", "/", "die", "Bahn", "rein", "aus\u00b7zu\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$(", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zorn/ Rauben/ Mord und Tod sah man Gefehrten seyn.", "tokens": ["Zorn", "/", "Rau\u00b7ben", "/", "Mord", "und", "Tod", "sah", "man", "Ge\u00b7fehr\u00b7ten", "seyn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "VVFIN", "PIS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es klunge schon das Schwerd/ es schwirrten schon die Spiesse/", "tokens": ["Es", "klun\u00b7ge", "schon", "das", "Schwerd", "/", "es", "schwirr\u00b7ten", "schon", "die", "Spies\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als wenn der Marspiter in Klumpen alles schmisse.", "tokens": ["Als", "wenn", "der", "Mar\u00b7spi\u00b7ter", "in", "Klum\u00b7pen", "al\u00b7les", "schmis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "NN", "PIS", "VVFIN", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Hingegen Venus kam in Freundligkeiten an.", "tokens": ["Hin\u00b7ge\u00b7gen", "Ve\u00b7nus", "kam", "in", "Freund\u00b7lig\u00b7kei\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Jhr Herold war die Lieb/ ihr Auffboth s\u00fcsse Blicke/", "tokens": ["Ihr", "He\u00b7rold", "war", "die", "Lieb", "/", "ihr", "Auff\u00b7both", "s\u00fcs\u00b7se", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr Persevante Gunst und himmlisches Geschicke;", "tokens": ["Ihr", "Per\u00b7se\u00b7van\u00b7te", "Gunst", "und", "himm\u00b7li\u00b7sches", "Ge\u00b7schi\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erst traten Gratien verschwestert auff den Plan", "tokens": ["Erst", "tra\u00b7ten", "Gra\u00b7ti\u00b7en", "ver\u00b7schwes\u00b7tert", "auff", "den", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.i