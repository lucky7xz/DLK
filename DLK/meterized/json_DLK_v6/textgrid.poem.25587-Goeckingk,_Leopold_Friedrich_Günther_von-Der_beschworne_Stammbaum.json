, "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$.", "PPER", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}