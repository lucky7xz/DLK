{"textgrid.poem.54469": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragst wie ", "tokens": ["Du", "fragst", "wie"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Er sucht kein' alte Schrifft/ noch Bild/ noch Buch/ wie du.", "tokens": ["Er", "sucht", "kein'", "al\u00b7te", "Schrifft", "/", "noch", "Bild", "/", "noch", "Buch", "/", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$(", "ADV", "NN", "$(", "ADV", "NN", "$(", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er k\u00fcmmert sich nicht viel was man zu Hofe thu/", "tokens": ["Er", "k\u00fcm\u00b7mert", "sich", "nicht", "viel", "was", "man", "zu", "Ho\u00b7fe", "thu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "PWS", "PIS", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er fragt nicht ob der Bapst/ Bann oder Abla\u00df schreibt:", "tokens": ["Er", "fragt", "nicht", "ob", "der", "Bapst", "/", "Bann", "o\u00b7der", "Ab\u00b7la\u00df", "schreibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "ART", "NN", "$(", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Er acht kein ", "tokens": ["Er", "acht", "kein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "CARD", "PIAT"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Er spricht Gesandten nicht/ nicht Cardin\u00e4len zu:", "tokens": ["Er", "spricht", "Ge\u00b7sand\u00b7ten", "nicht", "/", "nicht", "Car\u00b7di\u00b7n\u00e4\u00b7len", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKNEG", "$(", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er g\u00f6nn't Gelehrten wol die hoch gew\u00fcndschte Ruh/", "tokens": ["Er", "g\u00f6nn't", "Ge\u00b7lehr\u00b7ten", "wol", "die", "hoch", "ge\u00b7w\u00fcnd\u00b7schte", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du wei\u00dft/ da\u00df er sich nicht an Ketzermeister reibe.", "tokens": ["Du", "wei\u00dft", "/", "da\u00df", "er", "sich", "nicht", "an", "Ket\u00b7zer\u00b7meis\u00b7ter", "rei\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von Kirchen h\u00e4lt er nichts; von G\u00e4rten nicht zu viel.", "tokens": ["Von", "Kir\u00b7chen", "h\u00e4lt", "er", "nichts", ";", "von", "G\u00e4r\u00b7ten", "nicht", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIS", "$.", "APPR", "NN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er lacht wenn ich die grufft der M\u00e4rter suchen wil.", "tokens": ["Er", "lacht", "wenn", "ich", "die", "grufft", "der", "M\u00e4r\u00b7ter", "su\u00b7chen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gutt ists/ da\u00df er sich nicht auff lieben hat verliebet.", "tokens": ["Gutt", "ists", "/", "da\u00df", "er", "sich", "nicht", "auff", "lie\u00b7ben", "hat", "ver\u00b7lie\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$(", "KOUS", "PPER", "PRF", "PTKNEG", "APPR", "VVINF", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kein Schawplatz steht jhm an. Kein singen geht jhm eyn.", "tokens": ["Kein", "Schaw\u00b7platz", "steht", "jhm", "an", ".", "Kein", "sin\u00b7gen", "geht", "jhm", "eyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er schl\u00e4fft wenn man dem Volck' ein k\u00fcnstlich Fewr-werck giebe:", "tokens": ["Er", "schl\u00e4fft", "wenn", "man", "dem", "Volck'", "ein", "k\u00fcnst\u00b7lich", "Fe\u00b7wr\u00b7\u00b7werck", "gie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PIS", "ART", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Was h\u00e4lt jhn denn zu Rom lang auff? ", "tokens": ["Was", "h\u00e4lt", "jhn", "denn", "zu", "Rom", "lang", "auff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NE", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}