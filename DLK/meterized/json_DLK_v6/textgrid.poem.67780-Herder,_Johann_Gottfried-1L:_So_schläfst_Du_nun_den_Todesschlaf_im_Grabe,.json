{"textgrid.poem.67780": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: So schl\u00e4fst Du nun den Todesschlaf im Grabe,", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schl\u00e4fst Du nun den Todesschlaf im Grabe,", "tokens": ["So", "schl\u00e4fst", "Du", "nun", "den", "To\u00b7des\u00b7schlaf", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du junger Held, der sch\u00f6ne Dornen trug.", "tokens": ["Du", "jun\u00b7ger", "Held", ",", "der", "sch\u00f6\u00b7ne", "Dor\u00b7nen", "trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Leben war f\u00fcr Tausend Lebensgabe,", "tokens": ["Dein", "Le\u00b7ben", "war", "f\u00fcr", "Tau\u00b7send", "Le\u00b7bens\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Tod erquickt auch Sterbende mit Muth.", "tokens": ["Dein", "Tod", "er\u00b7quickt", "auch", "Ster\u00b7ben\u00b7de", "mit", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ruh denn, erl\u00f6st von allem Jammer,", "tokens": ["Ruh", "denn", ",", "er\u00b7l\u00f6st", "von", "al\u00b7lem", "Jam\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Womit Dich Menschenh\u00e4rte traf,", "tokens": ["Wo\u00b7mit", "Dich", "Men\u00b7schen\u00b7h\u00e4r\u00b7te", "traf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Deiner stillen Kammer", "tokens": ["In", "Dei\u00b7ner", "stil\u00b7len", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den schwer errungnen Schlaf!", "tokens": ["Den", "schwer", "er\u00b7rung\u00b7nen", "Schlaf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du aber, Freund, an diesem bittern Tage", "tokens": ["Du", "a\u00b7ber", ",", "Freund", ",", "an", "die\u00b7sem", "bit\u00b7tern", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm, schau mit mir der Menschheit Scenen an!", "tokens": ["Komm", ",", "schau", "mit", "mir", "der", "Menschheit", "Sce\u00b7nen", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sieh, welch ein Mensch! betracht ihn tief und sage,", "tokens": ["Sieh", ",", "welch", "ein", "Mensch", "!", "be\u00b7tracht", "ihn", "tief", "und", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "$.", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer Menschen segnender je werden kann!", "tokens": ["Wer", "Men\u00b7schen", "seg\u00b7nen\u00b7der", "je", "wer\u00b7den", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Komm, la\u00df an seiner Gruft uns denken,", "tokens": ["Komm", ",", "la\u00df", "an", "sei\u00b7ner", "Gruft", "uns", "den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was uns im Tod allein erfreut!", "tokens": ["Was", "uns", "im", "Tod", "al\u00b7lein", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus Liebe sich zu kr\u00e4nken,", "tokens": ["Aus", "Lie\u00b7be", "sich", "zu", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist s\u00fc\u00dfe Dankbarkeit.", "tokens": ["Ist", "s\u00fc\u00b7\u00dfe", "Dank\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In Nazareth, am Galil\u00e4ermeere,", "tokens": ["In", "Na\u00b7za\u00b7reth", ",", "am", "Ga\u00b7li\u00b7l\u00e4\u00b7er\u00b7mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer gab dem J\u00fcnglinge den hohen Geist,", "tokens": ["Wer", "gab", "dem", "J\u00fcng\u00b7lin\u00b7ge", "den", "ho\u00b7hen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der, wie entkommen schon der Erden Schwere,", "tokens": ["Der", ",", "wie", "ent\u00b7kom\u00b7men", "schon", "der", "Er\u00b7den", "Schwe\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Reich den Himmel, Gott nur Vater hei\u00dft,", "tokens": ["Sein", "Reich", "den", "Him\u00b7mel", ",", "Gott", "nur", "Va\u00b7ter", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und schaut, wie seine Sonne leuchtet", "tokens": ["Und", "schaut", ",", "wie", "sei\u00b7ne", "Son\u00b7ne", "leuch\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf B\u00f6s' und Gute, wie sein Thau", "tokens": ["Auf", "B\u00f6s'", "und", "Gu\u00b7te", ",", "wie", "sein", "Thau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So Ros' als Dorn befeuchtet", "tokens": ["So", "Ros'", "als", "Dorn", "be\u00b7feuch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "\u00bbauf, la\u00dft uns Kinder sein der Vaterg\u00fcte,", "tokens": ["\u00bb", "auf", ",", "la\u00dft", "uns", "Kin\u00b7der", "sein", "der", "Va\u00b7ter\u00b7g\u00fc\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVIMP", "PPER", "NN", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vollkommen, wie der Herr vollkommen ist!\u00ab", "tokens": ["Voll\u00b7kom\u00b7men", ",", "wie", "der", "Herr", "voll\u00b7kom\u00b7men", "ist", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So pflanzt' er in der Sterblichen Gem\u00fcthe", "tokens": ["So", "pflanzt'", "er", "in", "der", "Sterb\u00b7li\u00b7chen", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unsterblich Wesen, das sich selbst vergi\u00dft", "tokens": ["U\u00b7nsterb\u00b7lich", "We\u00b7sen", ",", "das", "sich", "selbst", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und im Verborgnen schafft und betet,", "tokens": ["Und", "im", "Ver\u00b7borg\u00b7nen", "schafft", "und", "be\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Menschen schafft, f\u00fcr Feinde fleht,", "tokens": ["F\u00fcr", "Men\u00b7schen", "schafft", ",", "f\u00fcr", "Fein\u00b7de", "fleht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Still f\u00fcr die Zukunft s\u00e4et", "tokens": ["Still", "f\u00fcr", "die", "Zu\u00b7kunft", "s\u00e4et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und still von dannen geht.", "tokens": ["Und", "still", "von", "dan\u00b7nen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbgl\u00fccksel'ge Armen! gl\u00fccklich, die da leiden", "tokens": ["\u00bb", "gl\u00fcck\u00b7sel'\u00b7ge", "Ar\u00b7men", "!", "gl\u00fcck\u00b7lich", ",", "die", "da", "lei\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$.", "ADJD", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In sanfter Unschuld, die Erbarmenden,", "tokens": ["In", "sanf\u00b7ter", "Un\u00b7schuld", ",", "die", "Er\u00b7bar\u00b7men\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die, reines Herzens, Menschen Fried' und Freuden", "tokens": ["Die", ",", "rei\u00b7nes", "Her\u00b7zens", ",", "Men\u00b7schen", "Fried'", "und", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADJA", "NN", "$,", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Mitleid reichen und den Ha\u00df bestehn!", "tokens": ["Und", "Mit\u00b7leid", "rei\u00b7chen", "und", "den", "Ha\u00df", "be\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seid fr\u00f6hlich und getrost! Euch lohnet", "tokens": ["Seid", "fr\u00f6h\u00b7lich", "und", "ge\u00b7trost", "!", "Euch", "loh\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAIMP", "ADJD", "KON", "VVPP", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Himmel ew'ger Trost und Lohn,", "tokens": ["Im", "Him\u00b7mel", "ew'\u00b7ger", "Trost", "und", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo jeder Gute wohnet,", "tokens": ["Wo", "je\u00b7der", "Gu\u00b7te", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem Ha\u00df der Welt entflohn.", "tokens": ["Dem", "Ha\u00df", "der", "Welt", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr seid der Zeiten Licht, das Salz der Erde,", "tokens": ["Ihr", "seid", "der", "Zei\u00b7ten", "Licht", ",", "das", "Salz", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Stern der Nacht, ein Keim der Fruchtbarkeit;", "tokens": ["Ein", "Stern", "der", "Nacht", ",", "ein", "Keim", "der", "Frucht\u00b7bar\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Euch ist Glanz, damit Glanz um Euch werde,", "tokens": ["In", "Euch", "ist", "Glanz", ",", "da\u00b7mit", "Glanz", "um", "Euch", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$,", "KOUS", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Euch ist Reichthum, der die Erde weiht!", "tokens": ["In", "Euch", "ist", "Reicht\u00b7hum", ",", "der", "die", "Er\u00b7de", "weiht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf! dringet durch die enge Pforte!", "tokens": ["Auf", "!", "drin\u00b7get", "durch", "die", "en\u00b7ge", "Pfor\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eng ist die Pforte, schmal der Weg,", "tokens": ["Eng", "ist", "die", "Pfor\u00b7te", ",", "schmal", "der", "Weg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der zu dem Freudenorte", "tokens": ["Der", "zu", "dem", "Freu\u00b7den\u00b7or\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fchrt unbetretnen Steg.\u00ab", "tokens": ["F\u00fchrt", "un\u00b7be\u00b7tret\u00b7nen", "Steg", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So sprach er und ging selbst der Dornen Pfade,", "tokens": ["So", "sprach", "er", "und", "ging", "selbst", "der", "Dor\u00b7nen", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die noch dem Sterbenden sein blutig Haupt", "tokens": ["Die", "noch", "dem", "Ster\u00b7ben\u00b7den", "sein", "blu\u00b7tig", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Kranze schm\u00fcckten. Haupt, Du l\u00e4chelst Gnade,", "tokens": ["Im", "Kran\u00b7ze", "schm\u00fcck\u00b7ten", ".", "Haupt", ",", "Du", "l\u00e4\u00b7chelst", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als h\u00e4tte Ros' und Lorbeer Dich umlaubt!", "tokens": ["Als", "h\u00e4t\u00b7te", "Ros'", "und", "Lor\u00b7beer", "Dich", "um\u00b7laubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "NE", "KON", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Entschlummre! Bald wird Deine Krone,", "tokens": ["Ent\u00b7schlumm\u00b7re", "!", "Bald", "wird", "Dei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Siegprangend wie der Sterne Glanz,", "tokens": ["Sieg\u00b7pran\u00b7gend", "wie", "der", "Ster\u00b7ne", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Menschengott zum Lohne", "tokens": ["Dem", "Men\u00b7schen\u00b7gott", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein ew'ger Gotteskranz.", "tokens": ["Ein", "ew'\u00b7ger", "Got\u00b7tes\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Denn sanft wie Gott, gef\u00e4llig gleich den Engeln,", "tokens": ["Denn", "sanft", "wie", "Gott", ",", "ge\u00b7f\u00e4l\u00b7lig", "gleich", "den", "En\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "$,", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War G\u00fcte nur und Huld sein K\u00f6nigreich.", "tokens": ["War", "G\u00fc\u00b7te", "nur", "und", "Huld", "sein", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mitf\u00fchlend unsrer Last und unsern M\u00e4ngeln,", "tokens": ["Mit\u00b7f\u00fch\u00b7lend", "uns\u00b7rer", "Last", "und", "un\u00b7sern", "M\u00e4n\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur sich allein an Kraft und W\u00fcrde gleich,", "tokens": ["Nur", "sich", "al\u00b7lein", "an", "Kraft", "und", "W\u00fcr\u00b7de", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Gotteseifrer ohn' Entr\u00fcsten,", "tokens": ["Ein", "Got\u00b7te\u00b7sei\u00b7frer", "ohn'", "Ent\u00b7r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der, nie verh\u00f6hnend, oft beweint,", "tokens": ["Der", ",", "nie", "ver\u00b7h\u00f6h\u00b7nend", ",", "oft", "be\u00b7weint", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was Menschen dulden m\u00fc\u00dften,", "tokens": ["Was", "Men\u00b7schen", "dul\u00b7den", "m\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein \u00e4chter Menschenfreund.", "tokens": ["Ein", "\u00e4ch\u00b7ter", "Men\u00b7schen\u00b7freund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie? hatt' er nicht schon lebend g'nug gelitten?", "tokens": ["Wie", "?", "hatt'", "er", "nicht", "schon", "le\u00b7bend", "g'\u00b7nug", "ge\u00b7lit\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Er, dessen Herz das Mitleid selber war;", "tokens": ["Er", ",", "des\u00b7sen", "Herz", "das", "Mit\u00b7leid", "sel\u00b7ber", "war", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein zarter Spro\u00df, um den die St\u00fcrme stritten,", "tokens": ["Ein", "zar\u00b7ter", "Spro\u00df", ",", "um", "den", "die", "St\u00fcr\u00b7me", "strit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Arzt, dem fremdes eigen Leid gebar.", "tokens": ["Ein", "Arzt", ",", "dem", "frem\u00b7des", "ei\u00b7gen", "Leid", "ge\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbla\u00df diesen Kelch vor\u00fcbergehen!", "tokens": ["\u00bb", "la\u00df", "die\u00b7sen", "Kelch", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch, Vater, Du hast ihn gef\u00fcllt;", "tokens": ["Doch", ",", "Va\u00b7ter", ",", "Du", "hast", "ihn", "ge\u00b7f\u00fcllt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Wille soll geschehen!", "tokens": ["Dein", "Wil\u00b7le", "soll", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht ich \u2013 wie Du, Herr, willt.\u00ab", "tokens": ["Nicht", "ich", "\u2013", "wie", "Du", ",", "Herr", ",", "willt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PTKNEG", "PPER", "$(", "PWAV", "PPER", "$,", "NN", "$,", "VMFIN", "$.", "$("], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.10": {"line.1": {"text": "Er trank ihn! Als nun seine zarten Glieder", "tokens": ["Er", "trank", "ihn", "!", "Als", "nun", "sei\u00b7ne", "zar\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gef\u00fchl der Gottverlassenheit durchdrang;", "tokens": ["Ge\u00b7f\u00fchl", "der", "Gott\u00b7ver\u00b7las\u00b7sen\u00b7heit", "durch\u00b7drang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon dr\u00fcckte Nacht die matten Augenlider,", "tokens": ["Schon", "dr\u00fcck\u00b7te", "Nacht", "die", "mat\u00b7ten", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des schweren Hohnes schwarze Wolke sank;", "tokens": ["Des", "schwe\u00b7ren", "Hoh\u00b7nes", "schwar\u00b7ze", "Wol\u00b7ke", "sank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zerrissen war der letzten Schmerzen", "tokens": ["Zer\u00b7ris\u00b7sen", "war", "der", "letz\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geliebter Knote, der den Freund", "tokens": ["Ge\u00b7lieb\u00b7ter", "Kno\u00b7te", ",", "der", "den", "Freund"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Freund'- und Mutterherzen", "tokens": ["Mit", "Freun\u00b7d'", "und", "Mut\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Im Tode noch vereint;", "tokens": ["Im", "To\u00b7de", "noch", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da blickt' er auf und sah die sch\u00f6nen Auen,", "tokens": ["Da", "blickt'", "er", "auf", "und", "sah", "die", "sch\u00f6\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die er dem S\u00fcnder mitleidsvoll verhie\u00df.", "tokens": ["Die", "er", "dem", "S\u00fcn\u00b7der", "mit\u00b7le\u00b7ids\u00b7voll", "ver\u00b7hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "\u00bbgedenk an mich und la\u00df Dein Reich mich schauen!\u00ab", "tokens": ["\u00bb", "ge\u00b7denk", "an", "mich", "und", "la\u00df", "Dein", "Reich", "mich", "schau\u00b7en", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "KON", "VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbheut sollst Du's schau'n, der Freuden Paradies!", "tokens": ["\u00bb", "heut", "sollst", "Du's", "schau'n", ",", "der", "Freu\u00b7den", "Pa\u00b7ra\u00b7dies", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PIS", "VVINF", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u2013 Empfang in Deine Vaterh\u00e4nde", "tokens": ["\u2013", "Emp\u00b7fang", "in", "Dei\u00b7ne", "Va\u00b7ter\u00b7h\u00e4n\u00b7de"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"t