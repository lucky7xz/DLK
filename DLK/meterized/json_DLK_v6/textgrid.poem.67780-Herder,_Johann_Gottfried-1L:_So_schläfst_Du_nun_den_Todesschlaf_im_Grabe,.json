", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was er verschm\u00e4hte, sei uns schlechter Lohn!", "tokens": ["Was", "er", "ver\u00b7schm\u00e4h\u00b7te", ",", "sei", "uns", "schlech\u00b7ter", "Lohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit G\u00fcte Bosheit \u00fcberwinden,", "tokens": ["Mit", "G\u00fc\u00b7te", "Bos\u00b7heit", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Ha\u00df der Welt, wie er, verzeihn,", "tokens": ["Den", "Ha\u00df", "der", "Welt", ",", "wie", "er", ",", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Wohlthun Rache finden,", "tokens": ["Im", "Wohl\u00b7thun", "Ra\u00b7che", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Soll Christenthum uns sein.", "tokens": ["Soll", "Chris\u00b7ten\u00b7thum", "uns", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}