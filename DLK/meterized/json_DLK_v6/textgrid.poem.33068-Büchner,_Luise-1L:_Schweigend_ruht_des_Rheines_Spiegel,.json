{"textgrid.poem.33068": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schweigend ruht des Rheines Spiegel,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigend ruht des Rheines Spiegel,", "tokens": ["Schwei\u00b7gend", "ruht", "des", "Rhei\u00b7nes", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Golden schwebt der Mond dar\u00fcber,", "tokens": ["Gol\u00b7den", "schwebt", "der", "Mond", "da\u00b7r\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Senket aus den blauen H\u00f6hen", "tokens": ["Sen\u00b7ket", "aus", "den", "blau\u00b7en", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Strahlenbr\u00fccke nieder.", "tokens": ["Ei\u00b7ne", "Strah\u00b7len\u00b7br\u00fc\u00b7cke", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sie taucht die lichten Pfeiler", "tokens": ["Und", "sie", "taucht", "die", "lich\u00b7ten", "Pfei\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die tiefe, dunkle Welle,", "tokens": ["In", "die", "tie\u00b7fe", ",", "dunk\u00b7le", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df vor Wonne leise bebet", "tokens": ["Da\u00df", "vor", "Won\u00b7ne", "lei\u00b7se", "be\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glanzumwoben ihre Schwelle.", "tokens": ["Glan\u00b7zum\u00b7wo\u00b7ben", "ih\u00b7re", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dampfumh\u00fcllet, schwarz und n\u00e4chtig,", "tokens": ["Damp\u00b7fum\u00b7h\u00fcl\u00b7let", ",", "schwarz", "und", "n\u00e4ch\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt das Schiff einhergeflogen,", "tokens": ["Kommt", "das", "Schiff", "ein\u00b7her\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schneidet brausend mitten innen", "tokens": ["Schnei\u00b7det", "brau\u00b7send", "mit\u00b7ten", "in\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Br\u00fccke goldnen Bogen.", "tokens": ["Durch", "der", "Br\u00fc\u00b7cke", "gold\u00b7nen", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die so stille und so pr\u00e4chtig", "tokens": ["Die", "so", "stil\u00b7le", "und", "so", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "KON", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Festgezimmert hat gestanden,", "tokens": ["Fest\u00b7ge\u00b7zim\u00b7mert", "hat", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist zertr\u00fcmmert, ist zerborsten", "tokens": ["Ist", "zer\u00b7tr\u00fcm\u00b7mert", ",", "ist", "zer\u00b7bors\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In unz\u00e4hlige Demanten.", "tokens": ["In", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "De\u00b7man\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zuckend fliegen sie wie Blitze", "tokens": ["Zu\u00b7ckend", "flie\u00b7gen", "sie", "wie", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber die bewegten Fluthen,", "tokens": ["Ue\u00b7ber", "die", "be\u00b7weg\u00b7ten", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der heit're Bau sich w\u00f6lbte,", "tokens": ["Wo", "der", "heit'\u00b7re", "Bau", "sich", "w\u00f6lb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wogt ein wildes Meer von Gluthen.", "tokens": ["Wogt", "ein", "wil\u00b7des", "Meer", "von", "Glu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! so zieht durch eine Seele", "tokens": ["Ach", "!", "so", "zieht", "durch", "ei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft das Schicksal schwarz und m\u00e4chtig,", "tokens": ["Oft", "das", "Schick\u00b7sal", "schwarz", "und", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das in's Leben schlug die Br\u00fccke", "tokens": ["Das", "in's", "Le\u00b7ben", "schlug", "die", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch so golden, froh und pr\u00e4chtig!", "tokens": ["Auch", "so", "gol\u00b7den", ",", "froh", "und", "pr\u00e4ch\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber sieh \u2013 das Schiff enteilet,", "tokens": ["A\u00b7ber", "sieh", "\u2013", "das", "Schiff", "ent\u00b7ei\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhe deckt die Wasser wieder,", "tokens": ["Ru\u00b7he", "deckt", "die", "Was\u00b7ser", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf's Neue hell und golden", "tokens": ["Und", "auf's", "Neu\u00b7e", "hell", "und", "gol\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Senket sich die Br\u00fccke nieder.", "tokens": ["Sen\u00b7ket", "sich", "die", "Br\u00fc\u00b7cke", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie vers\u00f6hnet, ihre Strahlen", "tokens": ["Wie", "ver\u00b7s\u00f6h\u00b7net", ",", "ih\u00b7re", "Strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder in einander rinnen,", "tokens": ["Wie\u00b7der", "in", "ein\u00b7an\u00b7der", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahnet Niemand, da\u00df sie eben", "tokens": ["Ah\u00b7net", "Nie\u00b7mand", ",", "da\u00df", "sie", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War zerschnitten mitten innen.", "tokens": ["War", "zer\u00b7schnit\u00b7ten", "mit\u00b7ten", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Armes Herz! dem so gewaltsam", "tokens": ["Ar\u00b7mes", "Herz", "!", "dem", "so", "ge\u00b7walt\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward der goldne Bau zersplittert,", "tokens": ["Ward", "der", "gold\u00b7ne", "Bau", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es mild erbebend schl\u00e4get,", "tokens": ["Da\u00df", "es", "mild", "er\u00b7be\u00b7bend", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem tiefsten Weh durchzittert;", "tokens": ["Von", "dem", "tiefs\u00b7ten", "Weh", "durch\u00b7zit\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Reicher, goldner als die Br\u00fccke", "tokens": ["Rei\u00b7cher", ",", "gold\u00b7ner", "als", "die", "Br\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlest du nach deinen Wunden,", "tokens": ["Strah\u00b7lest", "du", "nach", "dei\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast vers\u00f6hnt und ganz dich wieder", "tokens": ["Hast", "ver\u00b7s\u00f6hnt", "und", "ganz", "dich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "KON", "ADV", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dir selbst zurecht gefunden!", "tokens": ["In", "dir", "selbst", "zu\u00b7recht", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}