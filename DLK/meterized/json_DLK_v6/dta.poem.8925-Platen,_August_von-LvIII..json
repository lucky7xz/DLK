{"dta.poem.8925": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Das wissen selbst die Blinden und die Tauben,", "tokens": ["Das", "wis\u00b7sen", "selbst", "die", "Blin\u00b7den", "und", "die", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich aber wei\u00df, was wen'ge Menschen glauben,", "tokens": ["Ich", "a\u00b7ber", "wei\u00df", ",", "was", "wen'\u00b7ge", "Men\u00b7schen", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wahre Freundschaft zarter ist als Liebe.", "tokens": ["Da\u00df", "wah\u00b7re", "Freund\u00b7schaft", "zar\u00b7ter", "ist", "als", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Liebe wird mit feurigem Betriebe", "tokens": ["Die", "Lie\u00b7be", "wird", "mit", "feu\u00b7ri\u00b7gem", "Be\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich in sich selber zu verzehren schnauben;", "tokens": ["Sich", "in", "sich", "sel\u00b7ber", "zu", "ver\u00b7zeh\u00b7ren", "schnau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PRF", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch meines Freundes kann mich nichts berauben,", "tokens": ["Doch", "mei\u00b7nes", "Freun\u00b7des", "kann", "mich", "nichts", "be\u00b7rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PRF", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis nicht ich selbst im leichten Staub zerstiebe.", "tokens": ["Bis", "nicht", "ich", "selbst", "im", "leich\u00b7ten", "Staub", "zer\u00b7stie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er zeigt mir K\u00e4lte nur und Uebelwollen,", "tokens": ["Er", "zeigt", "mir", "K\u00e4l\u00b7te", "nur", "und", "Ue\u00b7bel\u00b7wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er spottet mein, er hat mich l\u00e4ngst vergessen,", "tokens": ["Er", "spot\u00b7tet", "mein", ",", "er", "hat", "mich", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch dacht' ich nie daran, mit ihm zu grollen.", "tokens": ["Doch", "dacht'", "ich", "nie", "da\u00b7ran", ",", "mit", "ihm", "zu", "grol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nie wird er meine Hand in seine pressen,", "tokens": ["Nie", "wird", "er", "mei\u00b7ne", "Hand", "in", "sei\u00b7ne", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stets aber werd' ich neues Lob ihm zollen,", "tokens": ["Stets", "a\u00b7ber", "werd'", "ich", "neu\u00b7es", "Lob", "ihm", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was man lobt, hat man im Geist besessen.", "tokens": ["Und", "was", "man", "lobt", ",", "hat", "man", "im", "Geist", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "VVFIN", "$,", "VAFIN", "PIS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}