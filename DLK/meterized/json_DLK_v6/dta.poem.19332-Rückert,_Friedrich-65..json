{"dta.poem.19332": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe Gottes kann so werden \u00fcbertrieben,", "tokens": ["Die", "Lie\u00b7be", "Got\u00b7tes", "kann", "so", "wer\u00b7den", "\u00fc\u00b7bert\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie f\u00fcr S\u00fcnd' es h\u00e4lt den Menschen auch zu lieben;", "tokens": ["Da\u00df", "sie", "f\u00fcr", "S\u00fcnd'", "es", "h\u00e4lt", "den", "Men\u00b7schen", "auch", "zu", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als w\u00fcrde Gott um das, was ihm geb\u00fchrt, betrogen,", "tokens": ["Als", "w\u00fcr\u00b7de", "Gott", "um", "das", ",", "was", "ihm", "ge\u00b7b\u00fchrt", ",", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "NN", "APPR", "PDS", "$,", "PWS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Antheil, den du weihst dem Menschen, ihm entzogen.", "tokens": ["Der", "An\u00b7theil", ",", "den", "du", "weihst", "dem", "Men\u00b7schen", ",", "ihm", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So ists, den Menschen wenn du liebst als Kreatur;", "tokens": ["So", "ists", ",", "den", "Men\u00b7schen", "wenn", "du", "liebst", "als", "Kre\u00b7a\u00b7tur", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ART", "NN", "KOUS", "PPER", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lieb' ihn als ewigen Gedanken Gottes nur!", "tokens": ["Lieb'", "ihn", "als", "e\u00b7wi\u00b7gen", "Ge\u00b7dan\u00b7ken", "Got\u00b7tes", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "NN", "ADV", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.4": {"line.1": {"text": "Du liebest Gott nicht ganz, wenn du ihn liebst allein,", "tokens": ["Du", "lie\u00b7best", "Gott", "nicht", "ganz", ",", "wenn", "du", "ihn", "liebst", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nicht auch alles, was er liebet, gro\u00df und klein.", "tokens": ["Wenn", "nicht", "auch", "al\u00b7les", ",", "was", "er", "lie\u00b7bet", ",", "gro\u00df", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}