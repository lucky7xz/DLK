{"textgrid.poem.39743": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "5. Im Bergwirthshaus", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Braunes Bier und saure Gesichter!", "tokens": ["Brau\u00b7nes", "Bier", "und", "sau\u00b7re", "Ge\u00b7sich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Saures Bier, brauner Augen Lichter,", "tokens": ["Sau\u00b7res", "Bier", ",", "brau\u00b7ner", "Au\u00b7gen", "Lich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hell und freundlich, treu und gut: \u2013", "tokens": ["Hell", "und", "freund\u00b7lich", ",", "treu", "und", "gut", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirtin, mir wird wohl zu Mut!", "tokens": ["Wir\u00b7tin", ",", "mir", "wird", "wohl", "zu", "Mut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}}}}