{"dta.poem.14770": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die falsche Schwester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Schwester, die spielt' an der Linde &#8212;", "tokens": ["Mei\u00b7ne", "Schwes\u00b7ter", ",", "die", "spielt'", "an", "der", "Lin\u00b7de", "&#8212;"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NE", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Stille Zeit, wie so weit, so weit!", "tokens": ["Stil\u00b7le", "Zeit", ",", "wie", "so", "weit", ",", "so", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da spielten so sch\u00f6ne Kinder", "tokens": ["Da", "spiel\u00b7ten", "so", "sch\u00f6\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit ihr in der Einsamkeit.", "tokens": ["Mit", "ihr", "in", "der", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}