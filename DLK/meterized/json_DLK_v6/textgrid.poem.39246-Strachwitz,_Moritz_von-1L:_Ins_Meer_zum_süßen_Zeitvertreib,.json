{"textgrid.poem.39246": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ins Meer zum s\u00fc\u00dfen Zeitvertreib,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins Meer zum s\u00fc\u00dfen Zeitvertreib,", "tokens": ["Ins", "Meer", "zum", "s\u00fc\u00b7\u00dfen", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da f\u00e4hrt des Paschas Lieblingsweib. \u2013", "tokens": ["Da", "f\u00e4hrt", "des", "Pa\u00b7schas", "Lieb\u00b7lings\u00b7weib", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schwimmt auf lauer D\u00fcfte Flut", "tokens": ["Es", "schwimmt", "auf", "lau\u00b7er", "D\u00fcf\u00b7te", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Abend voller Farbenglut,", "tokens": ["Ein", "A\u00b7bend", "vol\u00b7ler", "Far\u00b7beng\u00b7lut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ihn die Liebe gern durchdehnt,", "tokens": ["Wie", "ihn", "die", "Lie\u00b7be", "gern", "durch\u00b7dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Ros' und B\u00fclb\u00fcl ihn ersehnt.", "tokens": ["Wie", "Ros'", "und", "B\u00fcl\u00b7b\u00fcl", "ihn", "er\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sonne w\u00e4lzt ihr spr\u00fchend Rad", "tokens": ["Die", "Son\u00b7ne", "w\u00e4lzt", "ihr", "spr\u00fc\u00b7hend", "Rad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins abendk\u00fchle Wogenbad", "tokens": ["Ins", "a\u00b7bend\u00b7k\u00fch\u00b7le", "Wo\u00b7gen\u00b7bad"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und pre\u00dft den letzten Flammenku\u00df", "tokens": ["Und", "pre\u00dft", "den", "letz\u00b7ten", "Flam\u00b7men\u00b7ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Aufs feuchte Aug' des Bosporus.", "tokens": ["Aufs", "feuch\u00b7te", "Aug'", "des", "Bos\u00b7po\u00b7rus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie liegt sie reich und \u00fcppig da,", "tokens": ["Wie", "liegt", "sie", "reich", "und", "\u00fcp\u00b7pig", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Kuppelstadt des Padischah,", "tokens": ["Die", "Kup\u00b7pel\u00b7stadt", "des", "Pa\u00b7di\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wie eine Braut voll Reiz und Scham,", "tokens": ["Wie", "ei\u00b7ne", "Braut", "voll", "Reiz", "und", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der man vom Aug' den Schleier nahm.", "tokens": ["Der", "man", "vom", "Aug'", "den", "Schlei\u00b7er", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Es spiegelt sich auf ebner See", "tokens": ["Es", "spie\u00b7gelt", "sich", "auf", "eb\u00b7ner", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Silberhalbmond der Moschee,", "tokens": ["Der", "Sil\u00b7ber\u00b7halb\u00b7mond", "der", "Mo\u00b7schee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "Und zitternd auf dem Flutenbett", "tokens": ["Und", "zit\u00b7ternd", "auf", "dem", "Flu\u00b7ten\u00b7bett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wiegt Wimpel sich und Minarett. \u2013", "tokens": ["Wiegt", "Wim\u00b7pel", "sich", "und", "Mi\u00b7na\u00b7rett", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "PRF", "KON", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Gem\u00e4chlich, wie ein sanfter Schwan,", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich", ",", "wie", "ein", "sanf\u00b7ter", "Schwan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Durchschwebt die Flut der bunte Kahn,", "tokens": ["Durch\u00b7schwebt", "die", "Flut", "der", "bun\u00b7te", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wie am Gestad' die Muschel schwimmt,", "tokens": ["Wie", "am", "Ge\u00b7stad'", "die", "Mu\u00b7schel", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Darin die edle Perle glimmt.", "tokens": ["Da\u00b7rin", "die", "ed\u00b7le", "Per\u00b7le", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wie war sie sch\u00f6n vom Haar zum Fu\u00df,", "tokens": ["Wie", "war", "sie", "sch\u00f6n", "vom", "Haar", "zum", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die Rose aus dem Kaukasus,", "tokens": ["Die", "Ro\u00b7se", "aus", "dem", "Kau\u00b7ka\u00b7sus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wie war ihr Auge blau und gro\u00df,", "tokens": ["Wie", "war", "ihr", "Au\u00b7ge", "blau", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ein unermess'ner Meeresscho\u00df.", "tokens": ["Ein", "un\u00b7er\u00b7mess'\u00b7ner", "Mee\u00b7res\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wie war ihr Antlitz glanzbesonnt,", "tokens": ["Wie", "war", "ihr", "Ant\u00b7litz", "glanz\u00b7be\u00b7sonnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ein ganzer Liebeshorizont,", "tokens": ["Ein", "gan\u00b7zer", "Lie\u00b7bes\u00b7ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So wei\u00df und rot, so rot und wei\u00df,", "tokens": ["So", "wei\u00df", "und", "rot", ",", "so", "rot", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Wie Morgenrot auf Kasbecks Eis.", "tokens": ["Wie", "Mor\u00b7gen\u00b7rot", "auf", "Kas\u00b7becks", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ein Schwanenflaum der stolze Hals,", "tokens": ["Ein", "Schwa\u00b7nen\u00b7flaum", "der", "stol\u00b7ze", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ein Silberschaum des Wasserfalls,", "tokens": ["Ein", "Sil\u00b7ber\u00b7schaum", "des", "Was\u00b7ser\u00b7falls", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Darauf die schwarze Locke lag,", "tokens": ["Da\u00b7rauf", "die", "schwar\u00b7ze", "Lo\u00b7cke", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Wie dunkle Nacht auf lichtem Tag.", "tokens": ["Wie", "dunk\u00b7le", "Nacht", "auf", "lich\u00b7tem", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Es flo\u00df der Kaftan himmelblau", "tokens": ["Es", "flo\u00df", "der", "Kaf\u00b7tan", "him\u00b7mel\u00b7blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Rings um den Antilopenbau,", "tokens": ["Rings", "um", "den", "An\u00b7ti\u00b7lo\u00b7pen\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Wie sich der Bl\u00e4tter wallend Kleid", "tokens": ["Wie", "sich", "der", "Bl\u00e4t\u00b7ter", "wal\u00b7lend", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Rings um den Wuchs der Palme reiht.", "tokens": ["Rings", "um", "den", "Wuchs", "der", "Pal\u00b7me", "reiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Sie war so voll und zart und schlank", "tokens": ["Sie", "war", "so", "voll", "und", "zart", "und", "schlank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ein fleischgewordner Saitenklang,", "tokens": ["Ein", "fleischge\u00b7word\u00b7ner", "Sai\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Ein Strahl aus Allahs Diadem,", "tokens": ["Ein", "Strahl", "aus", "Al\u00b7lahs", "Di\u00b7a\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Hell wie der Stern von Bethlehem.", "tokens": ["Hell", "wie", "der", "Stern", "von", "Beth\u00b7le\u00b7hem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Doch bei der Houri Mund an Mund,", "tokens": ["Doch", "bei", "der", "Hou\u00b7ri", "Mund", "an", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Da sitzt ein junger Christenhund,", "tokens": ["Da", "sitzt", "ein", "jun\u00b7ger", "Chris\u00b7ten\u00b7hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Die Rechte f\u00fchrt des Ruders Last,", "tokens": ["Die", "Rech\u00b7te", "f\u00fchrt", "des", "Ru\u00b7ders", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Die Linke h\u00e4lt die Maid umfa\u00dft.", "tokens": ["Die", "Lin\u00b7ke", "h\u00e4lt", "die", "Maid", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "So oft der Streich im Wasser rauscht,", "tokens": ["So", "oft", "der", "Streich", "im", "Was\u00b7ser", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "So oft wird Ku\u00df um Ku\u00df getauscht.", "tokens": ["So", "oft", "wird", "Ku\u00df", "um", "Ku\u00df", "ge\u00b7tauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "So haben sie manch laue Nacht", "tokens": ["So", "ha\u00b7ben", "sie", "manch", "lau\u00b7e", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Ins Meer die sel'ge Fahrt gemacht,", "tokens": ["Ins", "Meer", "die", "sel'\u00b7ge", "Fahrt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Indes der Pascha traumumnachtet", "tokens": ["In\u00b7des", "der", "Pa\u00b7scha", "trau\u00b7mu\u00b7mnach\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Nach seiner sch\u00f6nen Sklavin schmachtet. \u2013", "tokens": ["Nach", "sei\u00b7ner", "sch\u00f6\u00b7nen", "Skla\u00b7vin", "schmach\u00b7tet", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Es senkt die Nacht sich ernst und hehr,", "tokens": ["Es", "senkt", "die", "Nacht", "sich", "ernst", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Ein Riesenadler, \u00fcbers Meer.", "tokens": ["Ein", "Rie\u00b7se\u00b7nad\u00b7ler", ",", "\u00fc\u00b7bers", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Und matter wird des Nachens Schu\u00df,", "tokens": ["Und", "mat\u00b7ter", "wird", "des", "Na\u00b7chens", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Und hei\u00dfer wird des Franken Ku\u00df,", "tokens": ["Und", "hei\u00b7\u00dfer", "wird", "des", "Fran\u00b7ken", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": 