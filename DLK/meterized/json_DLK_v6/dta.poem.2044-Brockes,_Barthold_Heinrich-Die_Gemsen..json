{"dta.poem.2044": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Gemsen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses Thier verdient besonders, seiner Art zu leben", "tokens": ["Die\u00b7ses", "Thier", "ver\u00b7dient", "be\u00b7son\u00b7ders", ",", "sei\u00b7ner", "Art", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "wegen,", "tokens": ["we\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["APPR", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df wir auch ihr Daseyn froh und bedachtsam \u00fcber-", "tokens": ["Da\u00df", "wir", "auch", "ihr", "Da\u00b7seyn", "froh", "und", "be\u00b7dacht\u00b7sam", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "legen.", "tokens": ["le\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sollte man sie nicht zugleich B\u00fcrger schroff- und holer", "tokens": ["Soll\u00b7te", "man", "sie", "nicht", "zu\u00b7gleich", "B\u00fcr\u00b7ger", "schroff", "und", "ho\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPER", "PTKNEG", "ADV", "NN", "TRUNC", "KON", "NN"], "meter": "----+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kl\u00fcfte,", "tokens": ["Kl\u00fcf\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und erhabene Bewohner \u00fcber uns erhabner L\u00fcfte,", "tokens": ["Und", "er\u00b7ha\u00b7be\u00b7ne", "Be\u00b7woh\u00b7ner", "\u00fc\u00b7ber", "uns", "er\u00b7hab\u00b7ner", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Mit Erstaunen nennen m\u00fcssen? So wie jedem Thier hie-", "tokens": ["Mit", "Er\u00b7stau\u00b7nen", "nen\u00b7nen", "m\u00fcs\u00b7sen", "?", "So", "wie", "je\u00b7dem", "Thier", "hie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "VMINF", "$.", "ADV", "KOKOM", "PIAT", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "nieden,", "tokens": ["nie\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "So ist ihnen in der H\u00f6he nur ihr Aufenthalt beschieden.", "tokens": ["So", "ist", "ih\u00b7nen", "in", "der", "H\u00f6\u00b7he", "nur", "ihr", "Auf\u00b7ent\u00b7halt", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Damit nichts auf dieser Erde von des Sch\u00f6pfers Wun-", "tokens": ["Da\u00b7mit", "nichts", "auf", "die\u00b7ser", "Er\u00b7de", "von", "des", "Sch\u00f6p\u00b7fers", "Wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PIS", "APPR", "PDAT", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "dern leer,", "tokens": ["dern", "leer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Und vom Vorwurf seiner G\u00fcte jeder Raum erf\u00fcllet w\u00e4r\u2019,", "tokens": ["Und", "vom", "Vor\u00b7wurf", "sei\u00b7ner", "G\u00fc\u00b7te", "je\u00b7der", "Raum", "er\u00b7f\u00fcl\u00b7let", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Hat er in der Gemsen K\u00f6rper solche Werkzeug\u2019 f\u00fcgen", "tokens": ["Hat", "er", "in", "der", "Gem\u00b7sen", "K\u00f6r\u00b7per", "sol\u00b7che", "Werk\u00b7zeug'", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NN", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "wollen,", "tokens": ["wol\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Da\u00df sie Sturz und Fall nicht scheuen, und da gern sind,", "tokens": ["Da\u00df", "sie", "Sturz", "und", "Fall", "nicht", "scheu\u00b7en", ",", "und", "da", "gern", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,", "KON", "ADV", "ADV", "VAFIN", "$,"], "meter": "--+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "wo sie sollen.", "tokens": ["wo", "sie", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Jhre starke H\u00f6rner sind so verwunderlich gekr\u00fcmmt,", "tokens": ["Ih\u00b7re", "star\u00b7ke", "H\u00f6r\u00b7ner", "sind", "so", "ver\u00b7wun\u00b7der\u00b7lich", "ge\u00b7kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Da\u00df sie sich an ihnen hangen und vom Fall befreyen", "tokens": ["Da\u00df", "sie", "sich", "an", "ih\u00b7nen", "han\u00b7gen", "und", "vom", "Fall", "be\u00b7fre\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "KON", "APPRART", "NN", "VVINF"], "meter": "+-----+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "k\u00f6nnen.", "tokens": ["k\u00f6n\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Jhre starkbenervte Schenkel sind zum Springen recht", "tokens": ["Ih\u00b7re", "stark\u00b7be\u00b7nerv\u00b7te", "Schen\u00b7kel", "sind", "zum", "Sprin\u00b7gen", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "bestimmt,", "tokens": ["be\u00b7stimmt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Eine solche Augenmaa\u00dfe hat Gott ihnen wollen g\u00f6n-", "tokens": ["Ei\u00b7ne", "sol\u00b7che", "Au\u00b7gen\u00b7maa\u00b7\u00dfe", "hat", "Gott", "ih\u00b7nen", "wol\u00b7len", "g\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "NN", "PPER", "VMFIN", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "nen,", "tokens": ["nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Da\u00df sie nicht im Sprunge fehlen. Sie probiren einen", "tokens": ["Da\u00df", "sie", "nicht", "im", "Sprun\u00b7ge", "feh\u00b7len", ".", "Sie", "pro\u00b7bi\u00b7ren", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$.", "PPER", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.26": {"text": "Stein", "tokens": ["Stein"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Mit vorausgesetztem Fu\u00df, ob er los ist oder fest,", "tokens": ["Mit", "vor\u00b7aus\u00b7ge\u00b7setz\u00b7tem", "Fu\u00df", ",", "ob", "er", "los", "ist", "o\u00b7der", "fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Welches ja besondre Vorsicht in der Handlung blicken", "tokens": ["Wel\u00b7ches", "ja", "be\u00b7sond\u00b7re", "Vor\u00b7sicht", "in", "der", "Hand\u00b7lung", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJA", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.29": {"text": "l\u00e4\u00dft.", "tokens": ["l\u00e4\u00dft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}