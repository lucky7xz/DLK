{"textgrid.poem.47671": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mutter, es ist so kalt, so kalt]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmutter, es ist so kalt, so kalt;", "tokens": ["\u00bb", "mut\u00b7ter", ",", "es", "ist", "so", "kalt", ",", "so", "kalt", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es friert mich in das Herz hinein.\u00ab", "tokens": ["Es", "friert", "mich", "in", "das", "Herz", "hin\u00b7ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da hast du, liebes Kindelein,", "tokens": ["Da", "hast", "du", ",", "lie\u00b7bes", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hast du eine Schnur so fein,", "tokens": ["Da", "hast", "du", "ei\u00b7ne", "Schnur", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit selbiger umg\u00fcrte dich,", "tokens": ["Mit", "sel\u00b7bi\u00b7ger", "um\u00b7g\u00fcr\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denk', es sei ein Hemdelein!", "tokens": ["Und", "denk'", ",", "es", "sei", "ein", "Hem\u00b7del\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}