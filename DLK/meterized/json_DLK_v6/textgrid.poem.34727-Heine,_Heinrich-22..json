{"textgrid.poem.34727": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandle unter Blumen", "tokens": ["Ich", "wand\u00b7le", "un\u00b7ter", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bl\u00fche selber mit;", "tokens": ["Und", "bl\u00fc\u00b7he", "sel\u00b7ber", "mit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wandle wie im Traume,", "tokens": ["Ich", "wand\u00b7le", "wie", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schwanke bei jedem Schritt.", "tokens": ["Und", "schwan\u00b7ke", "bei", "je\u00b7dem", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Oh, halt mich fest, Geliebte!", "tokens": ["Oh", ",", "halt", "mich", "fest", ",", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor Liebestrunkenheit", "tokens": ["Vor", "Lie\u00b7be\u00b7strun\u00b7ken\u00b7heit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fall ich dir sonst zu F\u00fc\u00dfen,", "tokens": ["Fall", "ich", "dir", "sonst", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und der Garten ist voller Leut'.", "tokens": ["Und", "der", "Gar\u00b7ten", "ist", "vol\u00b7ler", "Leut'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}