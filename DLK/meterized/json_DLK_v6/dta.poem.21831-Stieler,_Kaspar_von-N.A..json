{"dta.poem.21831": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Vor grobes Eichen-Holz/ nu treuer Gar-", "tokens": ["Vor", "gro\u00b7bes", "Ei\u00b7chen\u00b7Holz", "/", "nu", "treu\u00b7er", "Ga\u00b7r"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "ten-w\u00e4chter/", "tokens": ["ten\u00b7w\u00e4ch\u00b7ter", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Priapus/ den das Volk der Jugend ehrt", "tokens": ["Pria\u00b7pus", "/", "den", "das", "Volk", "der", "Ju\u00b7gend", "ehrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "ART", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "und liebt", "tokens": ["und", "liebt"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "die Jungfer lauschet an mit schmuzelndem", "tokens": ["die", "Jung\u00b7fer", "lau\u00b7schet", "an", "mit", "schmu\u00b7zeln\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gel\u00e4chter", "tokens": ["Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nim di\u00df mein Opfer hin/ da\u00df eine Hand", "tokens": ["Nim", "di\u00df", "mein", "Op\u00b7fer", "hin", "/", "da\u00df", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PDS", "PPOSAT", "NN", "PTKVZ", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "dir gibt", "tokens": ["dir", "gibt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "die deinem Sch\u00f6pfer gleicht. Der Lenz bricht", "tokens": ["die", "dei\u00b7nem", "Sch\u00f6p\u00b7fer", "gleicht", ".", "Der", "Lenz", "bricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "dir Violen", "tokens": ["dir", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "und macht au\u00df buntem Mohn dir einen", "tokens": ["und", "macht", "au\u00df", "bun\u00b7tem", "Mohn", "dir", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Blumen-Kranz/", "tokens": ["Blu\u00b7men\u00b7Kranz", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "die Zeres pfleget dir ein Weizen-bund zu ho-", "tokens": ["die", "Ze\u00b7res", "pfle\u00b7get", "dir", "ein", "Wei\u00b7zen\u00b7bund", "zu", "ho"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "len/", "tokens": ["len", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.15": {"text": "die frohe Winzer-schaar tuht um dich man-", "tokens": ["die", "fro\u00b7he", "Win\u00b7zer\u00b7schaar", "tuht", "um", "dich", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PRF", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "chen Tanz/", "tokens": ["chen", "Tanz", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "und schenkt dir roten Wein/ der Winter win-", "tokens": ["und", "schenkt", "dir", "ro\u00b7ten", "Wein", "/", "der", "Win\u00b7ter", "win"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$(", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "det Str\u00e4uche", "tokens": ["det", "Str\u00e4u\u00b7che"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "von dem Oliven-Laub\u2019 und schlachtt sein", "tokens": ["von", "dem", "O\u00b7li\u00b7ven\u00b7Laub'", "und", "schlachtt", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ADJD", "VAINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "junges Vieh", "tokens": ["jun\u00b7ges", "Vieh"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "vor deinem Erd-Altar. Ich lobe die Gebr\u00e4u-", "tokens": ["vor", "dei\u00b7nem", "Erd\u00b7Al\u00b7tar", ".", "Ich", "lo\u00b7be", "die", "Ge\u00b7br\u00e4u"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "che", "tokens": ["che"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "und stelle mich auch ein zu k\u00fcssen deine", "tokens": ["und", "stel\u00b7le", "mich", "auch", "ein", "zu", "k\u00fcs\u00b7sen", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "PTKZU", "VVINF", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Knie", "tokens": ["Knie"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "wiewol du keine hast. Zwar bring\u2019 ich schlech-", "tokens": ["wie\u00b7wol", "du", "kei\u00b7ne", "hast", ".", "Zwar", "bring'", "ich", "schlech"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "te Reime", "tokens": ["te", "Rei\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "doch trozzen sie den Lentz/ Herbst/ Sommer/", "tokens": ["doch", "troz\u00b7zen", "sie", "den", "Lentz", "/", "Herbst", "/", "Som\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Winters-Zeit.", "tokens": ["Win\u00b7ter\u00b7sZeit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Nimstu sie g\u00fcnstig an: so sollen deine B\u00e4u-", "tokens": ["Nims\u00b7tu", "sie", "g\u00fcns\u00b7tig", "an", ":", "so", "sol\u00b7len", "dei\u00b7ne", "B\u00e4u"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADJD", "PTKVZ", "$.", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "me", "tokens": ["me"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "vor Wurm und Donnerschlag und Dieben", "tokens": ["vor", "Wurm", "und", "Don\u00b7ner\u00b7schlag", "und", "Die\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "sein befreyt/", "tokens": ["sein", "be\u00b7freyt", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Es soll die Schlangen-Zucht au\u00df deiner H\u00fct-", "tokens": ["Es", "soll", "die", "Schlan\u00b7gen\u00b7Zucht", "au\u00df", "dei\u00b7ner", "H\u00fct"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "ten weichen", "tokens": ["ten", "wei\u00b7chen"], "token_info": ["word", "word"], "pos": ["FM", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "kein Dorn noch Nessel-strauch verlezzen", "tokens": ["kein", "Dorn", "noch", "Nes\u00b7sel\u00b7strauch", "ver\u00b7lez\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "deine Brust/", "tokens": ["dei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "ein dikk-beschattend Laub soll dir Beschir-", "tokens": ["ein", "dik\u00b7k\u00b7be\u00b7schat\u00b7tend", "Laub", "soll", "dir", "Be\u00b7schir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "TRUNC"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "mung reichen", "tokens": ["mung", "rei\u00b7chen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "vor aller Sonnenhizz und schaffen k\u00fchle", "tokens": ["vor", "al\u00b7ler", "Son\u00b7nen\u00b7hizz", "und", "schaf\u00b7fen", "k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Lust/", "tokens": ["Lust", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.41": {"text": "der Nordwind sol dich nie mit rauhem Schnee", "tokens": ["der", "Nord\u00b7wind", "sol", "dich", "nie", "mit", "rau\u00b7hem", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "bewehen/", "tokens": ["be\u00b7we\u00b7hen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.43": {"text": "es soll kein Zapffen-ey\u00df beschweren deinen", "tokens": ["es", "soll", "kein", "Zapf\u00b7fen\u00b7ey\u00df", "be\u00b7schwe\u00b7ren", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Bart/", "tokens": ["Bart", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.45": {"text": "du sollst\u2019 den Zedern gleich die feulung \u00fcber-", "tokens": ["du", "sollst'", "den", "Ze\u00b7dern", "gleich", "die", "feu\u00b7lung", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "stehen", "tokens": ["ste\u00b7hen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "und\u2019 halten in der Gluht au\u00df auff Deman-", "tokens": ["und'", "hal\u00b7ten", "in", "der", "Gluht", "au\u00df", "auff", "De\u00b7man"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "APPR", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "ten-Art.", "tokens": ["ten\u00b7Art", "."], "token_info": ["word", "punct"], "pos": ["FM.la", "$."], "meter": "-+", "measure": "iambic.single"}}}}}