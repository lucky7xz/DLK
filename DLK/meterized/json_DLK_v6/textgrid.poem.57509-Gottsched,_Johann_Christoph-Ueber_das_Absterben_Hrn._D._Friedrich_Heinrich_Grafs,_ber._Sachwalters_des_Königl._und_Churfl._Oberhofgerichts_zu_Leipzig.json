alexandrine.iambic.hexa"}, "line.73": {"text": "Sein Wandel bleibt auch euch ein Muster wahrer Tugend,", "tokens": ["Sein", "Wan\u00b7del", "bleibt", "auch", "euch", "ein", "Mus\u00b7ter", "wah\u00b7rer", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Sein Leben dienet euch zur Sittenlehrerinn.", "tokens": ["Sein", "Le\u00b7ben", "die\u00b7net", "euch", "zur", "Sit\u00b7ten\u00b7leh\u00b7re\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Verla\u00dft euch nur auf Gott, den Vater frommer Jugend,", "tokens": ["Ver\u00b7la\u00dft", "euch", "nur", "auf", "Gott", ",", "den", "Va\u00b7ter", "from\u00b7mer", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Und k\u00fcsset seine Ruth in demuthsvollem Sinn.", "tokens": ["Und", "k\u00fcs\u00b7set", "sei\u00b7ne", "Ruth", "in", "de\u00b7muths\u00b7vol\u00b7lem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.77": {"text": "Wir selber ehren hier das himmlische Geschicke,", "tokens": ["Wir", "sel\u00b7ber", "eh\u00b7ren", "hier", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Das unser Lebensziel allein in H\u00e4nden hat;", "tokens": ["Das", "un\u00b7ser", "Le\u00b7bens\u00b7ziel", "al\u00b7lein", "in", "H\u00e4n\u00b7den", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Wir gehn von seiner Gruft weit muthiger zur\u00fccke,", "tokens": ["Wir", "gehn", "von", "sei\u00b7ner", "Gruft", "weit", "mut\u00b7hi\u00b7ger", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.80": {"text": "Und ehren lebenslang die theure Lagerstatt.", "tokens": ["Und", "eh\u00b7ren", "le\u00b7bens\u00b7lang", "die", "theu\u00b7re", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}}}}