{"textgrid.poem.57509": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "Ueber das Absterben Hrn. D. Friedrich Heinrich Grafs, ber. Sachwalters des K\u00f6nigl. und Churfl. Oberhofgerichts zu Leipzig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm hin, erbla\u00dfter Graf! der treuen Wehmuth Zeichen,", "tokens": ["Nimm", "hin", ",", "er\u00b7bla\u00df\u00b7ter", "Graf", "!", "der", "treu\u00b7en", "Weh\u00b7muth", "Zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimm hin die\u00df Klagelied, das deine Gruft erzwingt:", "tokens": ["Nimm", "hin", "die\u00df", "Kla\u00b7ge\u00b7lied", ",", "das", "dei\u00b7ne", "Gruft", "er\u00b7zwingt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn wir unsern Zweck nicht bey dir selbst erreichen,", "tokens": ["Und", "wenn", "wir", "un\u00b7sern", "Zweck", "nicht", "bey", "dir", "selbst", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Genug, wenn unsre Pflicht ein Thr\u00e4nenopfer bringt.", "tokens": ["Ge\u00b7nug", ",", "wenn", "uns\u00b7re", "Pflicht", "ein", "Thr\u00e4\u00b7nen\u00b7op\u00b7fer", "bringt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die\u00df fodert das Gebl\u00fct, es foderts unsre Liebe;", "tokens": ["Die\u00df", "fo\u00b7dert", "das", "Ge\u00b7bl\u00fct", ",", "es", "fo\u00b7derts", "uns\u00b7re", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Ehrfurcht selber spricht, da\u00df wir es schuldig sind.", "tokens": ["Die", "Ehr\u00b7furcht", "sel\u00b7ber", "spricht", ",", "da\u00df", "wir", "es", "schul\u00b7dig", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer widerst\u00fcnde wohl dem dreyfach starken Triebe,", "tokens": ["Wer", "wi\u00b7der\u00b7st\u00fcn\u00b7de", "wohl", "dem", "drey\u00b7fach", "star\u00b7ken", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der sich mit reger Kraft in unsern Seelen findt?", "tokens": ["Der", "sich", "mit", "re\u00b7ger", "Kraft", "in", "un\u00b7sern", "See\u00b7len", "findt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "O Himmel! mu\u00dfte denn das feste Band zerrei\u00dfen,", "tokens": ["O", "Him\u00b7mel", "!", "mu\u00df\u00b7te", "denn", "das", "fes\u00b7te", "Band", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das zwischen ihm und uns dein eigner Arm verschr\u00e4nkt?", "tokens": ["Das", "zwi\u00b7schen", "ihm", "und", "uns", "dein", "eig\u00b7ner", "Arm", "ver\u00b7schr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "KON", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und sollten wir ihn denn nur darum Bruder hei\u00dfen,", "tokens": ["Und", "soll\u00b7ten", "wir", "ihn", "denn", "nur", "da\u00b7rum", "Bru\u00b7der", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "ADV", "PAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df uns die Wehmuth itzt mit herben Z\u00e4hren tr\u00e4nkt?", "tokens": ["Da\u00df", "uns", "die", "Weh\u00b7muth", "itzt", "mit", "her\u00b7ben", "Z\u00e4h\u00b7ren", "tr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verwandtschaft! s\u00fc\u00dfes Wort, wenn deine Knoten dauren,", "tokens": ["Ver\u00b7wandt\u00b7schaft", "!", "s\u00fc\u00b7\u00dfes", "Wort", ",", "wenn", "dei\u00b7ne", "Kno\u00b7ten", "dau\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn kein zufr\u00fcher Tod den Lebensfaden trennt!", "tokens": ["Wenn", "kein", "zu\u00b7fr\u00fc\u00b7her", "Tod", "den", "Le\u00b7bens\u00b7fa\u00b7den", "trennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Verwandtschaft! herber Ton! wenn wir um Leichen trauren,", "tokens": ["Ver\u00b7wandt\u00b7schaft", "!", "her\u00b7ber", "Ton", "!", "wenn", "wir", "um", "Lei\u00b7chen", "trau\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$.", "KOUS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In deren treuer Brust der Schwester Liebe brennt.", "tokens": ["In", "de\u00b7ren", "treu\u00b7er", "Brust", "der", "Schwes\u00b7ter", "Lie\u00b7be", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was hilft uns nun der Ruhm so naher Anverwandten?", "tokens": ["Was", "hilft", "uns", "nun", "der", "Ruhm", "so", "na\u00b7her", "An\u00b7ver\u00b7wand\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da die Benennung uns so unverhofft betr\u00fcbt:", "tokens": ["Da", "die", "Be\u00b7nen\u00b7nung", "uns", "so", "un\u00b7ver\u00b7hofft", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was hilft es, da\u00df wir ihn so z\u00e4rtlich Bruder nannten?", "tokens": ["Was", "hilft", "es", ",", "da\u00df", "wir", "ihn", "so", "z\u00e4rt\u00b7lich", "Bru\u00b7der", "nann\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da dieses Beywort uns zu Thr\u00e4nen Anla\u00df giebt,", "tokens": ["Da", "die\u00b7ses", "Bey\u00b7wort", "uns", "zu", "Thr\u00e4\u00b7nen", "An\u00b7la\u00df", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O h\u00e4tt er sich nur nichts aus unserm Blut erkohren!", "tokens": ["O", "h\u00e4tt", "er", "sich", "nur", "nichts", "aus", "un\u00b7serm", "Blut", "er\u00b7koh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PRF", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So tr\u00e4f uns dieser Fall f\u00fcrwahr nicht halb so hart.", "tokens": ["So", "tr\u00e4f", "uns", "die\u00b7ser", "Fall", "f\u00fcr\u00b7wahr", "nicht", "halb", "so", "hart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "ADV", "PTKNEG", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "O h\u00e4tten wir an ihm ein fremdes Haupt verlohren!", "tokens": ["O", "h\u00e4t\u00b7ten", "wir", "an", "ihm", "ein", "frem\u00b7des", "Haupt", "ver\u00b7loh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "So w\u00fcrd uns durch sein Grab nur halb so viel verscharrt.", "tokens": ["So", "w\u00fcrd", "uns", "durch", "sein", "Grab", "nur", "halb", "so", "viel", "ver\u00b7scharrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jedoch erw\u00e4gen wir auch seiner Freundschaft Proben,", "tokens": ["Je\u00b7doch", "er\u00b7w\u00e4\u00b7gen", "wir", "auch", "sei\u00b7ner", "Freund\u00b7schaft", "Pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Betrachten wir den Trieb womit er uns geliebt:", "tokens": ["Be\u00b7trach\u00b7ten", "wir", "den", "Trieb", "wo\u00b7mit", "er", "uns", "ge\u00b7liebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So bleibt sein seltner Ruhm vor Tausenden erhoben,", "tokens": ["So", "bleibt", "sein", "selt\u00b7ner", "Ruhm", "vor", "Tau\u00b7sen\u00b7den", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So bleibt auch unsre Brust gedoppelt stark betr\u00fcbt.", "tokens": ["So", "bleibt", "auch", "uns\u00b7re", "Brust", "ge\u00b7dop\u00b7pelt", "stark", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "VVPP", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie mancher Blutsfreund lacht bey seiner Freunde Jammer;", "tokens": ["Wie", "man\u00b7cher", "Bluts\u00b7freund", "lacht", "bey", "sei\u00b7ner", "Freun\u00b7de", "Jam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und leugnet durch die That die Wirkung der Natur?", "tokens": ["Und", "leug\u00b7net", "durch", "die", "That", "die", "Wir\u00b7kung", "der", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Verschlie\u00dft man solche nun in ihre Todtenkammer:", "tokens": ["Ver\u00b7schlie\u00dft", "man", "sol\u00b7che", "nun", "in", "ih\u00b7re", "Tod\u00b7ten\u00b7kam\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So sieht man nirgendwo der Thr\u00e4nen mindste Spur.", "tokens": ["So", "sieht", "man", "nir\u00b7gend\u00b7wo", "der", "Thr\u00e4\u00b7nen", "minds\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wir seufzen um ein Haupt, das uns mit Vatersinnen", "tokens": ["Wir", "seuf\u00b7zen", "um", "ein", "Haupt", ",", "das", "uns", "mit", "Va\u00b7ter\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Von Herzen zugethan und stets gewogen war.", "tokens": ["Von", "Her\u00b7zen", "zu\u00b7ge\u00b7than", "und", "stets", "ge\u00b7wo\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Theils suchten wir sein Herz stets besser zu gewinnen,", "tokens": ["Theils", "such\u00b7ten", "wir", "sein", "Herz", "stets", "bes\u00b7ser", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Theils both er selber uns stets neue Proben dar.", "tokens": ["Theils", "both", "er", "sel\u00b7ber", "uns", "stets", "neu\u00b7e", "Pro\u00b7ben", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PPER", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sein Umgang war beliebt, sein Scherz und Ernst erlesen,", "tokens": ["Sein", "Um\u00b7gang", "war", "be\u00b7liebt", ",", "sein", "Scherz", "und", "Ernst", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Sein Herz voll Redlichkeit, die Lippen voller Treu:", "tokens": ["Sein", "Herz", "voll", "Red\u00b7lich\u00b7keit", ",", "die", "Lip\u00b7pen", "vol\u00b7ler", "Treu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und kurz, sein ganzes Thun und ungemeines Wesen", "tokens": ["Und", "kurz", ",", "sein", "gan\u00b7zes", "Thun", "und", "un\u00b7ge\u00b7mei\u00b7nes", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "War edel und gerecht und von Verstellung frey.", "tokens": ["War", "e\u00b7del", "und", "ge\u00b7recht", "und", "von", "Ver\u00b7stel\u00b7lung", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hex