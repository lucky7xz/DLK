{"textgrid.poem.62251": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlied", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00fcre auf, T\u00fcre zu,", "tokens": ["T\u00fc\u00b7re", "auf", ",", "T\u00fc\u00b7re", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Niemals Rast, niemals Ruh',", "tokens": ["Nie\u00b7mals", "Rast", ",", "nie\u00b7mals", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sch\u00f6ne Damen, liebe Herr'n", "tokens": ["Sch\u00f6\u00b7ne", "Da\u00b7men", ",", "lie\u00b7be", "Herr'n"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaufet freundlich, kaufet gern,", "tokens": ["Kau\u00b7fet", "freund\u00b7lich", ",", "kau\u00b7fet", "gern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brauch's auf Brot, und brauch's auf Bier,", "tokens": ["Brauch's", "auf", "Brot", ",", "und", "brauch's", "auf", "Bier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das g\u00f6nnt ihr sicher mir.", "tokens": ["Und", "das", "g\u00f6nnt", "ihr", "si\u00b7cher", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADJD", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00fcre auf, T\u00fcre zu,", "tokens": ["T\u00fc\u00b7re", "auf", ",", "T\u00fc\u00b7re", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Niemals Rast, niemals Ruh',", "tokens": ["Nie\u00b7mals", "Rast", ",", "nie\u00b7mals", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In der K\u00e4lte, in der Glut,", "tokens": ["In", "der", "K\u00e4l\u00b7te", ",", "in", "der", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner meiner F\u00fc\u00dfe ruht,", "tokens": ["Kei\u00b7ner", "mei\u00b7ner", "F\u00fc\u00b7\u00dfe", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Such' am Herde einen Platz,", "tokens": ["Such'", "am", "Her\u00b7de", "ei\u00b7nen", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Finde keinen, keinen Schatz.", "tokens": ["Fin\u00b7de", "kei\u00b7nen", ",", "kei\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ne Damen, liebe Herr'n", "tokens": ["Sch\u00f6\u00b7ne", "Da\u00b7men", ",", "lie\u00b7be", "Herr'n"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaufet freundlich, kaufet gern,", "tokens": ["Kau\u00b7fet", "freund\u00b7lich", ",", "kau\u00b7fet", "gern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm aus fernem Lande her", "tokens": ["Komm", "aus", "fer\u00b7nem", "Lan\u00b7de", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem Fremdling wird's so schwer:", "tokens": ["Und", "dem", "Fremd\u00b7ling", "wird's", "so", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00fcre auf, T\u00fcre zu,", "tokens": ["T\u00fc\u00b7re", "auf", ",", "T\u00fc\u00b7re", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Niemals Rast, niemals Ruh'!", "tokens": ["Nie\u00b7mals", "Rast", ",", "nie\u00b7mals", "Ruh'", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}