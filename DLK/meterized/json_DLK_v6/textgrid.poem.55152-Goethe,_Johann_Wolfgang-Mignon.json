{"textgrid.poem.55152": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Mignon", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hei\u00df mich nicht reden, hei\u00df mich schweigen,", "tokens": ["Hei\u00df", "mich", "nicht", "re\u00b7den", ",", "hei\u00df", "mich", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "VVINF", "$,", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Denn mein Geheimnis ist mir Pflicht;", "tokens": ["Denn", "mein", "Ge\u00b7heim\u00b7nis", "ist", "mir", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich m\u00f6chte dir mein ganzes Innre zeigen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dir", "mein", "gan\u00b7zes", "Inn\u00b7re", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein das Schicksal will es nicht.", "tokens": ["Al\u00b7lein", "das", "Schick\u00b7sal", "will", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zur rechten Zeit vertreibt der Sonne Lauf", "tokens": ["Zur", "rech\u00b7ten", "Zeit", "ver\u00b7treibt", "der", "Son\u00b7ne", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die finstre Nacht, und sie mu\u00df sich erhellen;", "tokens": ["Die", "finst\u00b7re", "Nacht", ",", "und", "sie", "mu\u00df", "sich", "er\u00b7hel\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der harte Fels schlie\u00dft seinen Busen auf,", "tokens": ["Der", "har\u00b7te", "Fels", "schlie\u00dft", "sei\u00b7nen", "Bu\u00b7sen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mi\u00dfg\u00f6nnt der Erde nicht die tiefverborgnen Quellen.", "tokens": ["Mi\u00df\u00b7g\u00f6nnt", "der", "Er\u00b7de", "nicht", "die", "tief\u00b7ver\u00b7borg\u00b7nen", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein jeder sucht im Arm des Freundes Ruh,", "tokens": ["Ein", "je\u00b7der", "sucht", "im", "Arm", "des", "Freun\u00b7des", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort kann die Brust in Klagen sich ergie\u00dfen;", "tokens": ["Dort", "kann", "die", "Brust", "in", "Kla\u00b7gen", "sich", "er\u00b7gie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein ein Schwur dr\u00fcckt mir die Lippen zu,", "tokens": ["Al\u00b7lein", "ein", "Schwur", "dr\u00fcckt", "mir", "die", "Lip\u00b7pen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nur ein Gott vermag sie aufzuschlie\u00dfen.", "tokens": ["Und", "nur", "ein", "Gott", "ver\u00b7mag", "sie", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}