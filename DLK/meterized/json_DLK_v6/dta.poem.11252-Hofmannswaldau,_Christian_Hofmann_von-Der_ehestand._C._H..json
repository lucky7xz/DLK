{"dta.poem.11252": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der ehestand.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der ehstand ist ein kartenspiel,", "tokens": ["Der", "eh\u00b7stand", "ist", "ein", "kar\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da spielt ein jeder, der nur will.", "tokens": ["Da", "spielt", "ein", "je\u00b7der", ",", "der", "nur", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "$,", "PRELS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald gehts, bald nicht, bald gar genau,", "tokens": ["Bald", "gehts", ",", "bald", "nicht", ",", "bald", "gar", "ge\u00b7nau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "PTKNEG", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald trumpfft den mann wohl gar die frau.", "tokens": ["Bald", "trumpfft", "den", "mann", "wohl", "gar", "die", "frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts kommt mir wunderlicher hier", "tokens": ["Nichts", "kommt", "mir", "wun\u00b7der\u00b7li\u00b7cher", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als di\u00df, in diesem spiele, f\u00fcr,", "tokens": ["Als", "di\u00df", ",", "in", "die\u00b7sem", "spie\u00b7le", ",", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "APPR", "PDAT", "VVFIN", "$,", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df der verspielt und unten liegt,", "tokens": ["Da\u00df", "der", "ver\u00b7spielt", "und", "un\u00b7ten", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der doch die besten schl\u00e4ge kriegt.", "tokens": ["Der", "doch", "die", "bes\u00b7ten", "schl\u00e4\u00b7ge", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}