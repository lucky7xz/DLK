{"textgrid.poem.24727": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die wahre Ernte aller Dinge bleibt", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die wahre Ernte aller Dinge bleibt", "tokens": ["Die", "wah\u00b7re", "Ern\u00b7te", "al\u00b7ler", "Din\u00b7ge", "bleibt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bl\u00fcht in hoher Luft wie lichte Zinken,", "tokens": ["Und", "bl\u00fcht", "in", "ho\u00b7her", "Luft", "wie", "lich\u00b7te", "Zin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das andere war nur da um wegzusinken.", "tokens": ["Das", "an\u00b7de\u00b7re", "war", "nur", "da", "um", "weg\u00b7zu\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADV", "KOUI", "VVIZU", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und irgendwie geheimnisvoll ertr\u00e4gt", "tokens": ["Und", "ir\u00b7gend\u00b7wie", "ge\u00b7heim\u00b7nis\u00b7voll", "er\u00b7tr\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es unser Geist nur immer auszuruhen", "tokens": ["Es", "un\u00b7ser", "Geist", "nur", "im\u00b7mer", "aus\u00b7zu\u00b7ru\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Gleitendem, wie die Meerv\u00f6gel tuen.", "tokens": ["Auf", "Glei\u00b7ten\u00b7dem", ",", "wie", "die", "Meer\u00b7v\u00f6\u00b7gel", "tu\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie f\u00fchrte uns verworrenes Gespr\u00e4ch", "tokens": ["Wie", "f\u00fchr\u00b7te", "uns", "ver\u00b7wor\u00b7re\u00b7nes", "Ge\u00b7spr\u00e4ch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verstellter Augen \u00fcber \u00f6de Klippen!", "tokens": ["Ver\u00b7stell\u00b7ter", "Au\u00b7gen", "\u00fc\u00b7ber", "\u00f6\u00b7de", "Klip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unsere allzusehr beredten Lippen", "tokens": ["Und", "un\u00b7se\u00b7re", "all\u00b7zu\u00b7sehr", "be\u00b7red\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Begierig, vielen G\u00f6ttern Dienst zu tun!", "tokens": ["Be\u00b7gie\u00b7rig", ",", "vie\u00b7len", "G\u00f6t\u00b7tern", "Dienst", "zu", "tun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PIAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu viele Schatten schwebten da verschlungen,", "tokens": ["Zu", "vie\u00b7le", "Schat\u00b7ten", "schweb\u00b7ten", "da", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und so sind wir einander zugedrungen", "tokens": ["Und", "so", "sind", "wir", "ein\u00b7an\u00b7der", "zu\u00b7ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie dem Ertrinkenden das sch\u00f6ne Bild", "tokens": ["Wie", "dem", "Er\u00b7trin\u00b7ken\u00b7den", "das", "sch\u00f6\u00b7ne", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wei\u00dfen Bucht, das er nicht mehr gelassen", "tokens": ["Der", "wei\u00b7\u00dfen", "Bucht", ",", "das", "er", "nicht", "mehr", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ertr\u00e4gt, vielmehr schon anf\u00e4ngt es zu hassen.", "tokens": ["Er\u00b7tr\u00e4gt", ",", "viel\u00b7mehr", "schon", "an\u00b7f\u00e4ngt", "es", "zu", "has\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dies alles war nur da, um wegzusinken.", "tokens": ["Dies", "al\u00b7les", "war", "nur", "da", ",", "um", "weg\u00b7zu\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "ADV", "$,", "KOUI", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es wohnen noch ganz andere Gewalten", "tokens": ["Es", "woh\u00b7nen", "noch", "ganz", "an\u00b7de\u00b7re", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In unserer T\u00e4nze namenlosen Falten.", "tokens": ["In", "un\u00b7se\u00b7rer", "T\u00e4n\u00b7ze", "na\u00b7men\u00b7lo\u00b7sen", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Lider unserer Augen sind nicht gleich", "tokens": ["Die", "Li\u00b7der", "un\u00b7se\u00b7rer", "Au\u00b7gen", "sind", "nicht", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Fleisch der Fr\u00fcchte, und die jungen Mienen", "tokens": ["Dem", "Fleisch", "der", "Fr\u00fcch\u00b7te", ",", "und", "die", "jun\u00b7gen", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht einerlei mit L\u00e4mmern und Delphinen!", "tokens": ["Nicht", "ei\u00b7ner\u00b7lei", "mit", "L\u00e4m\u00b7mern", "und", "Del\u00b7phi\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und nur die Ernte aller Dinge bleibt:", "tokens": ["Und", "nur", "die", "Ern\u00b7te", "al\u00b7ler", "Din\u00b7ge", "bleibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So fand ich dich im Garten ohne Klippen,", "tokens": ["So", "fand", "ich", "dich", "im", "Gar\u00b7ten", "oh\u00b7ne", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gro\u00dfes Leben hing um deine Lippen,", "tokens": ["Und", "gro\u00b7\u00dfes", "Le\u00b7ben", "hing", "um", "dei\u00b7ne", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Weil du an deiner Freundin losem Haar", "tokens": ["Weil", "du", "an", "dei\u00b7ner", "Freun\u00b7din", "lo\u00b7sem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu reden wu\u00dftest k\u00f6niglich wie eine,", "tokens": ["Zu", "re\u00b7den", "wu\u00df\u00b7test", "k\u00f6\u00b7nig\u00b7lich", "wie", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "ADJD", "KOKOM", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wissen lernte, was das Leben meine.", "tokens": ["Die", "wis\u00b7sen", "lern\u00b7te", ",", "was", "das", "Le\u00b7ben", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "$,", "PRELS", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und hinter dir die Ebne niederziehn", "tokens": ["Und", "hin\u00b7ter", "dir", "die", "Eb\u00b7ne", "nie\u00b7der\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich wie stille Gold- und Silberb\u00e4che", "tokens": ["Sah", "ich", "wie", "stil\u00b7le", "Gold", "und", "Sil\u00b7ber\u00b7b\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wege deiner Niedrigkeit und Schw\u00e4che.", "tokens": ["Die", "We\u00b7ge", "dei\u00b7ner", "Nied\u00b7rig\u00b7keit", "und", "Schw\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}