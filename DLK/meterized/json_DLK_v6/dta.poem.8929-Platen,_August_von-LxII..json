{"dta.poem.8929": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LxII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Im Schmerzensbecher, den du mir gereichet!", "tokens": ["Im", "Schmer\u00b7zens\u00b7be\u00b7cher", ",", "den", "du", "mir", "ge\u00b7rei\u00b7chet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O w\u00e4r' ein Kind ich, schnell und leicht erweichet,", "tokens": ["O", "w\u00e4r'", "ein", "Kind", "ich", ",", "schnell", "und", "leicht", "er\u00b7wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPER", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich in Thr\u00e4nen k\u00f6nnte ganz zerflie\u00dfen!", "tokens": ["Da\u00df", "ich", "in", "Thr\u00e4\u00b7nen", "k\u00f6nn\u00b7te", "ganz", "zer\u00b7flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da mich so hart von ihrer Seite stie\u00dfen", "tokens": ["Da", "mich", "so", "hart", "von", "ih\u00b7rer", "Sei\u00b7te", "stie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die unerme\u00dflich ich geliebt, erbleichet", "tokens": ["Die", "un\u00b7er\u00b7me\u00df\u00b7lich", "ich", "ge\u00b7liebt", ",", "er\u00b7blei\u00b7chet"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der lezte Glaube, bittre K\u00e4lte schleichet", "tokens": ["Der", "lez\u00b7te", "Glau\u00b7be", ",", "bitt\u00b7re", "K\u00e4l\u00b7te", "schlei\u00b7chet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ein Gem\u00fcth, das Lieb' und Muth verlie\u00dfen.", "tokens": ["In", "ein", "Ge\u00b7m\u00fcth", ",", "das", "Lieb'", "und", "Muth", "ver\u00b7lie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O wohl mir, da\u00df in ferne Regionen", "tokens": ["O", "wohl", "mir", ",", "da\u00df", "in", "fer\u00b7ne", "Re\u00b7gi\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich fl\u00fcchten darf, an einem fremden Strande", "tokens": ["Ich", "fl\u00fcch\u00b7ten", "darf", ",", "an", "ei\u00b7nem", "frem\u00b7den", "Stran\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Darf athmen unter g\u00fctigeren Zonen!", "tokens": ["Darf", "ath\u00b7men", "un\u00b7ter", "g\u00fc\u00b7ti\u00b7ge\u00b7ren", "Zo\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo mir zerrissen sind die lezten Bande,", "tokens": ["Wo", "mir", "zer\u00b7ris\u00b7sen", "sind", "die", "lez\u00b7ten", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Ha\u00df und Undank edle Liebe lohnen,", "tokens": ["Wo", "Ha\u00df", "und", "Un\u00b7dank", "ed\u00b7le", "Lie\u00b7be", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie bin ich satt von meinem Vaterlande!", "tokens": ["Wie", "bin", "ich", "satt", "von", "mei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}