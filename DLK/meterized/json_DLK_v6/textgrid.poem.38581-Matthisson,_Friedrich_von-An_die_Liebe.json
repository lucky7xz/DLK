{"textgrid.poem.38581": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn deine G\u00f6ttermacht, o Liebe,", "tokens": ["Wenn", "dei\u00b7ne", "G\u00f6t\u00b7ter\u00b7macht", ",", "o", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus der Verbannung Nebelthal", "tokens": ["Aus", "der", "Ver\u00b7ban\u00b7nung", "Ne\u00b7belt\u00b7hal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Sternenwelt uns nicht erh\u00fcbe,", "tokens": ["Zur", "Ster\u00b7nen\u00b7welt", "uns", "nicht", "er\u00b7h\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer tr\u00fcge dann des Lebens Qual?", "tokens": ["Wer", "tr\u00fc\u00b7ge", "dann", "des", "Le\u00b7bens", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ins Reich der Unerme\u00dflichkeiten,", "tokens": ["Ins", "Reich", "der", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis wo die letzte Sph\u00e4re klingt,", "tokens": ["Bis", "wo", "die", "letz\u00b7te", "Sph\u00e4\u00b7re", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Folgst du dem Fluge des Geweihten,", "tokens": ["Folgst", "du", "dem", "Flu\u00b7ge", "des", "Ge\u00b7weih\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er dem Staube sich entschwingt!", "tokens": ["Wenn", "er", "dem", "Stau\u00b7be", "sich", "ent\u00b7schwingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und st\u00fcrzt, umwogt von Feuerfluthen,", "tokens": ["Und", "st\u00fcrzt", ",", "um\u00b7wogt", "von", "Feu\u00b7er\u00b7flut\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erdball selbst ins Grab der Zeit,", "tokens": ["Der", "Erd\u00b7ball", "selbst", "ins", "Grab", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entschwebst, ein Ph\u00f6nix, du den Gluthen;", "tokens": ["Ent\u00b7schwebst", ",", "ein", "Ph\u00f6\u00b7nix", ",", "du", "den", "Glu\u00b7then", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$,", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Nam' ist Unverg\u00e4nglichkeit!", "tokens": ["Dein", "Nam'", "ist", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}