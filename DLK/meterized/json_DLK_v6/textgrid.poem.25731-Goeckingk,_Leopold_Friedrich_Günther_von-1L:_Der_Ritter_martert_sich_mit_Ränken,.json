{"textgrid.poem.25731": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Ritter martert sich mit R\u00e4nken,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ritter martert sich mit R\u00e4nken,", "tokens": ["Der", "Rit\u00b7ter", "mar\u00b7tert", "sich", "mit", "R\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie er den F\u00e4cher dir zerbricht?", "tokens": ["Wie", "er", "den", "F\u00e4\u00b7cher", "dir", "zer\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um einen sch\u00f6nern dir zu schenken:", "tokens": ["Um", "ei\u00b7nen", "sch\u00f6\u00b7nern", "dir", "zu", "schen\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du aber nimmst ihn nicht!", "tokens": ["Du", "a\u00b7ber", "nimmst", "ihn", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein St\u00e4ndchen lockt in blo\u00dfen Hemden", "tokens": ["Dein", "St\u00e4nd\u00b7chen", "lockt", "in", "blo\u00b7\u00dfen", "Hem\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nachbarn aus dem Bett' hervor;", "tokens": ["Die", "Nach\u00b7barn", "aus", "dem", "Bett'", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur du, zu aller Welt Befremden,", "tokens": ["Nur", "du", ",", "zu", "al\u00b7ler", "Welt", "Be\u00b7frem\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hast dann allein kein Ohr.", "tokens": ["Hast", "dann", "al\u00b7lein", "kein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Gehst du in einen Garten: Haufen", "tokens": ["Gehst", "du", "in", "ei\u00b7nen", "Gar\u00b7ten", ":", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der sch\u00f6nsten Fr\u00fcchte harren dort", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Fr\u00fcch\u00b7te", "har\u00b7ren", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf dich; vor deinem Gelde laufen", "tokens": ["Auf", "dich", ";", "vor", "dei\u00b7nem", "Gel\u00b7de", "lau\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die G\u00e4rtner aber fort.", "tokens": ["Die", "G\u00e4rt\u00b7ner", "a\u00b7ber", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Du tratst in eines Landmanns H\u00fctte,", "tokens": ["Du", "tratst", "in", "ei\u00b7nes", "Land\u00b7manns", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Als ein Gewitterschauer droht',", "tokens": ["Als", "ein", "Ge\u00b7wit\u00b7ter\u00b7schau\u00b7er", "droht'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Gleich bot er dir, auch ohne Bitte,", "tokens": ["Gleich", "bot", "er", "dir", ",", "auch", "oh\u00b7ne", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sein letztes Salz und Brod.", "tokens": ["Sein", "letz\u00b7tes", "Salz", "und", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und du, ger\u00fchrt, da\u00df keine Klage,", "tokens": ["Und", "du", ",", "ge\u00b7r\u00fchrt", ",", "da\u00df", "kei\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Kein Murren \u00fcber karges Gl\u00fcck", "tokens": ["Kein", "Mur\u00b7ren", "\u00fc\u00b7ber", "kar\u00b7ges", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Hier wohnet, lie\u00dfest auf acht Tage", "tokens": ["Hier", "woh\u00b7net", ",", "lie\u00b7\u00dfest", "auf", "acht", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ihm heimlich Brod zur\u00fcck.", "tokens": ["Ihm", "heim\u00b7lich", "Brod", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Und unter seinem Strohdach' kehrest", "tokens": ["Und", "un\u00b7ter", "sei\u00b7nem", "Stroh\u00b7da\u00b7ch'", "keh\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Du oft im Sommer wieder ein,", "tokens": ["Du", "oft", "im", "Som\u00b7mer", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und issest Milch mit ihm, und h\u00f6rest", "tokens": ["Und", "is\u00b7sest", "Milch", "mit", "ihm", ",", "und", "h\u00f6\u00b7rest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ihn Wetter prophezein.", "tokens": ["Ihn", "Wet\u00b7ter", "pro\u00b7phe\u00b7zein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Ist das der Mann, aus dessen H\u00e4nden", "tokens": ["Ist", "das", "der", "Mann", ",", "aus", "des\u00b7sen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Dir ein Geschenk den Starrsinn bricht?", "tokens": ["Dir", "ein", "Ge\u00b7schenk", "den", "Starr\u00b7sinn", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Auch ich kann nichts f\u00fcr dich verschwenden,", "tokens": ["Auch", "ich", "kann", "nichts", "f\u00fcr", "dich", "ver\u00b7schwen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Auch ich, ich murre nicht.", "tokens": ["Auch", "ich", ",", "ich", "mur\u00b7re", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Sey diese Sammlung meiner Lieder", "tokens": ["Sey", "die\u00b7se", "Samm\u00b7lung", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Drum ohne Weigerung auch dein!", "tokens": ["Drum", "oh\u00b7ne", "Wei\u00b7ge\u00b7rung", "auch", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Es ist ja selbst gepre\u00dfter Cyder,", "tokens": ["Es", "ist", "ja", "selbst", "ge\u00b7pre\u00df\u00b7ter", "Cy\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Kein theurer fremder Wein.", "tokens": ["Kein", "theu\u00b7rer", "frem\u00b7der", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Und wenn dein Herz nach Troste lechzet,", "tokens": ["Und", "wenn", "dein", "Herz", "nach", "Tros\u00b7te", "lech\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Da\u00df in des Habichts Krallen, dir", "tokens": ["Da\u00df", "in", "des", "Ha\u00b7bichts", "Kral\u00b7len", ",", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ein T\u00e4ubchen seine Schmerzen \u00e4chzet;", "tokens": ["Ein", "T\u00e4ub\u00b7chen", "sei\u00b7ne", "Schmer\u00b7zen", "\u00e4ch\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Dann kehr' du ein bei mir.", "tokens": ["Dann", "kehr'", "du", "ein", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}