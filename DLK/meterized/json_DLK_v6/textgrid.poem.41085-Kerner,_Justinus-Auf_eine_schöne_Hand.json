{"textgrid.poem.41085": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine sch\u00f6ne Hand", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich K\u00f6nig, spr\u00e4ch' ich: \u00bbDu", "tokens": ["W\u00e4r'", "ich", "K\u00f6\u00b7nig", ",", "spr\u00e4ch'", "ich", ":", "\u00bb", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["VAFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "$.", "$(", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Hand, bleib mir in Ruh'!", "tokens": ["Sch\u00f6\u00b7ne", "Hand", ",", "bleib", "mir", "in", "Ruh'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst nicht n\u00e4hen, sollst nicht stricken,", "tokens": ["Sollst", "nicht", "n\u00e4\u00b7hen", ",", "sollst", "nicht", "stri\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts tun sollst du, als einst dr\u00fccken", "tokens": ["Nichts", "tun", "sollst", "du", ",", "als", "einst", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir im Tod die m\u00fcden Augen zu.\u00ab", "tokens": ["Mir", "im", "Tod", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}