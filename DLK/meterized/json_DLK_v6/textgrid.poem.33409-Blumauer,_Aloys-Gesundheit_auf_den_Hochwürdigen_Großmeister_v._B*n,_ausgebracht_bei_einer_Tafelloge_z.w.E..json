{"textgrid.poem.33409": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Gesundheit auf den Hochw\u00fcrdigen Gro\u00dfmeister v. B*n, ausgebracht bei einer Tafelloge z.w.E.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl und Weh lag in der Stunde", "tokens": ["Wohl", "und", "Weh", "lag", "in", "der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die einst unsern Bund gebar;", "tokens": ["Die", "einst", "un\u00b7sern", "Bund", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer hier in uns'rer Runde", "tokens": ["Ei\u00b7ner", "hier", "in", "un\u00b7s'\u00b7rer", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Machte, da\u00df sie gl\u00fccklich war:", "tokens": ["Mach\u00b7te", ",", "da\u00df", "sie", "gl\u00fcck\u00b7lich", "war", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wir Br\u00fcder freu'n uns all'", "tokens": ["Und", "wir", "Br\u00fc\u00b7der", "freu'n", "uns", "all'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "VVFIN", "PPER", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihrer heut zum f\u00fcnftenmal.", "tokens": ["Ih\u00b7rer", "heut", "zum", "f\u00fcnf\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "APPRART", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00fchsam ist des Maurers Reise,", "tokens": ["M\u00fch\u00b7sam", "ist", "des", "Mau\u00b7rers", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo so mancher Irrwisch schleicht:", "tokens": ["Wo", "so", "man\u00b7cher", "Irr\u00b7wisch", "schleicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer hier aus unserm Kreise,", "tokens": ["Ei\u00b7ner", "hier", "aus", "un\u00b7serm", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fcder, machte sie uns leicht:", "tokens": ["Br\u00fc\u00b7der", ",", "mach\u00b7te", "sie", "uns", "leicht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Eine ist, o seht,", "tokens": ["Und", "der", "Ei\u00b7ne", "ist", ",", "o", "seht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VAFIN", "$,", "FM", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der voran als F\u00fchrer geht.", "tokens": ["Der", "vo\u00b7ran", "als", "F\u00fch\u00b7rer", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ist's, wenn nicht blo\u00df im Munde", "tokens": ["Sch\u00f6n", "ist's", ",", "wenn", "nicht", "blo\u00df", "im", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stets der Brudername schwebt:", "tokens": ["Stets", "der", "Bru\u00b7der\u00b7na\u00b7me", "schwebt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer hier aus unserm Bunde", "tokens": ["Ei\u00b7ner", "hier", "aus", "un\u00b7serm", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's, der uns in's Herz ihn gr\u00e4bt:", "tokens": ["Ist's", ",", "der", "uns", "in's", "Herz", "ihn", "gr\u00e4bt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Eine, Br\u00fcder, ist,", "tokens": ["Die\u00b7ser", "Ei\u00b7ne", ",", "Br\u00fc\u00b7der", ",", "ist", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "$,", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der als Vater uns umschlie\u00dft.", "tokens": ["Der", "als", "Va\u00b7ter", "uns", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aechte Weisheit nur und G\u00fcte", "tokens": ["A\u00b7ech\u00b7te", "Weis\u00b7heit", "nur", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind der Vorrang der Natur;", "tokens": ["Sind", "der", "Vor\u00b7rang", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Einer hier in uns'rer Mitte", "tokens": ["Ei\u00b7ner", "hier", "in", "un\u00b7s'\u00b7rer", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Herrscht durch diese beiden nur.", "tokens": ["Herrscht", "durch", "die\u00b7se", "bei\u00b7den", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "PIAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der als Bruder uns regiert,", "tokens": ["Der", "als", "Bru\u00b7der", "uns", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist's, der unsern Bund auch ziert.", "tokens": ["Ist's", ",", "der", "un\u00b7sern", "Bund", "auch", "ziert", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Uns're Liebe uns're Zierde,", "tokens": ["Un\u00b7s'\u00b7re", "Lie\u00b7be", "un\u00b7s'\u00b7re", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Unser Vater, unser Freund", "tokens": ["Un\u00b7ser", "Va\u00b7ter", ",", "un\u00b7ser", "Freund"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist nur einer, der mit W\u00fcrde", "tokens": ["Ist", "nur", "ei\u00b7ner", ",", "der", "mit", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles dies in sich vereint.", "tokens": ["Al\u00b7les", "dies", "in", "sich", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "D'rum Heil dem zu jeder Frist,", "tokens": ["D'\u00b7rum", "Heil", "dem", "zu", "je\u00b7der", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der uns allen alles ist!", "tokens": ["Der", "uns", "al\u00b7len", "al\u00b7les", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}