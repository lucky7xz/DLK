{"dta.poem.10007": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O wie wenige kennen den Werth der feineren", "tokens": ["O", "wie", "we\u00b7ni\u00b7ge", "ken\u00b7nen", "den", "Werth", "der", "fei\u00b7ne\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PIS", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Deren erh\u00f6htes Gef\u00fchl, in Scenen finsterer Schwer-", "tokens": ["De\u00b7ren", "er\u00b7h\u00f6h\u00b7tes", "Ge\u00b7f\u00fchl", ",", "in", "Sce\u00b7nen", "fins\u00b7te\u00b7rer", "Schwer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "APPR", "NN", "ADJA", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schnellere Freuden genie\u00dft, als die der Schimmer des", "tokens": ["Schnel\u00b7le\u00b7re", "Freu\u00b7den", "ge\u00b7nie\u00dft", ",", "als", "die", "der", "Schim\u00b7mer", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "KOUS", "ART", "ART", "NN", "ART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und die blendende Pracht des eitlen Stolzes ertheilet.", "tokens": ["Und", "die", "blen\u00b7den\u00b7de", "Pracht", "des", "eit\u00b7len", "Stol\u00b7zes", "er\u00b7thei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Eloise, die lang in Schmerzen der Liebe geschmachtet,", "tokens": ["E\u00b7loi\u00b7se", ",", "die", "lang", "in", "Schmer\u00b7zen", "der", "Lie\u00b7be", "ge\u00b7schmach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "F\u00fchlte gewi\u00df mehr h\u00f6here Freuden, mehr wahres Ent-", "tokens": ["F\u00fchl\u00b7te", "ge\u00b7wi\u00df", "mehr", "h\u00f6\u00b7he\u00b7re", "Freu\u00b7den", ",", "mehr", "wah\u00b7res", "Ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,", "ADV", "ADJA", "TRUNC"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Wenn, im flimmernden Kreis der Todtenkerzen, sie", "tokens": ["Wenn", ",", "im", "flim\u00b7mern\u00b7den", "Kreis", "der", "Tod\u00b7ten\u00b7ker\u00b7zen", ",", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "An ein Grab sich gelehnt, vielleicht auch unter den Pfei-", "tokens": ["An", "ein", "Grab", "sich", "ge\u00b7lehnt", ",", "viel\u00b7leicht", "auch", "un\u00b7ter", "den", "Pfei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$,", "ADV", "ADV", "APPR", "ART", "TRUNC"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Gothischer Tempel und unter Alt\u00e4ren der heiligen Bilder", "tokens": ["Go\u00b7thi\u00b7scher", "Tem\u00b7pel", "und", "un\u00b7ter", "Al\u00b7t\u00e4\u00b7ren", "der", "hei\u00b7li\u00b7gen", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Sie, als eine verschleyerte Nonne, voll Schwermuth", "tokens": ["Sie", ",", "als", "ei\u00b7ne", "ver\u00b7schley\u00b7er\u00b7te", "Non\u00b7ne", ",", "voll", "Schwer\u00b7muth"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "ADJD", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Als im goldnen Palast, stolz auf die Reitze der Jugend,", "tokens": ["Als", "im", "gold\u00b7nen", "Pa\u00b7last", ",", "stolz", "auf", "die", "Reit\u00b7ze", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Flavia f\u00fchlt, wenn unter den S\u00f6hnen des weichlichen", "tokens": ["Fla\u00b7via", "f\u00fchlt", ",", "wenn", "un\u00b7ter", "den", "S\u00f6h\u00b7nen", "des", "weich\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Sie im Zirkel des festlichen Balles bezaubernd einher-", "tokens": ["Sie", "im", "Zir\u00b7kel", "des", "fest\u00b7li\u00b7chen", "Bal\u00b7les", "be\u00b7zau\u00b7bernd", "ein\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "TRUNC"], "meter": "--+--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Und vor allen versammelten Sch\u00f6nen, die Sch\u00f6nste,", "tokens": ["Und", "vor", "al\u00b7len", "ver\u00b7sam\u00b7mel\u00b7ten", "Sch\u00f6\u00b7nen", ",", "die", "Sch\u00f6ns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}}}}