{"dta.poem.13431": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Auf die  \n von Beust und von Seebachische  \n Verbindung  \n zu Gotha den 8 Nov. 1747.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Dennoch stimm ich meine Sayten,", "tokens": ["Den\u00b7noch", "stimm", "ich", "mei\u00b7ne", "Say\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und versuch ein neues Lied,", "tokens": ["Und", "ver\u00b7such", "ein", "neu\u00b7es", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotz den ungeneigten Zeiten,", "tokens": ["Trotz", "den", "un\u00b7ge\u00b7neig\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Geist mit Schwermuth sieht;", "tokens": ["Die", "der", "Geist", "mit", "Schwer\u00b7muth", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob sich gleich die St\u00fcrme jagen;", "tokens": ["Ob", "sich", "gleich", "die", "St\u00fcr\u00b7me", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringt das Rasseln ihrer Wagen", "tokens": ["Bringt", "das", "Ras\u00b7seln", "ih\u00b7rer", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Regen, K\u00e4lte, Schnee und Ei\u00df,", "tokens": ["Re\u00b7gen", ",", "K\u00e4l\u00b7te", ",", "Schnee", "und", "Ei\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gnug, mir macht der Gott der Leyer", "tokens": ["Gnug", ",", "mir", "macht", "der", "Gott", "der", "Le\u00b7yer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Hier bey dieser Hochzeitfeyer", "tokens": ["Hier", "bey", "die\u00b7ser", "Hoch\u00b7zeit\u00b7fe\u00b7yer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Stirne, Brust und Eifer hei\u00df.", "tokens": ["Stir\u00b7ne", ",", "Brust", "und", "Ei\u00b7fer", "hei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welch ausnehmendes Entz\u00fccken", "tokens": ["Welch", "aus\u00b7neh\u00b7men\u00b7des", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht sich itzund offenbar!", "tokens": ["Macht", "sich", "it\u00b7zund", "of\u00b7fen\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja! man wird in frohen Blicken", "tokens": ["Ja", "!", "man", "wird", "in", "fro\u00b7hen", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PIS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Paradieses-Lust gewahr,", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses\u00b7Lust", "ge\u00b7wahr", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stifter dieser wahren Freude!", "tokens": ["Stif\u00b7ter", "die\u00b7ser", "wah\u00b7ren", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gn\u00e4dige verbundne Beyde!", "tokens": ["Gn\u00e4\u00b7di\u00b7ge", "ver\u00b7bund\u00b7ne", "Bey\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vorbild der Zufriedenheit!", "tokens": ["Vor\u00b7bild", "der", "Zu\u00b7frie\u00b7den\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Was die Welt nicht kann gew\u00e4hren,", "tokens": ["Was", "die", "Welt", "nicht", "kann", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sucht die Liebe zu beschehren,", "tokens": ["Sucht", "die", "Lie\u00b7be", "zu", "be\u00b7scheh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie gebiehrt Gl\u00fcckseeligkeit.", "tokens": ["Sie", "ge\u00b7biehrt", "Gl\u00fcck\u00b7see\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In dem andern sich zu sehen,", "tokens": ["In", "dem", "an\u00b7dern", "sich", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in sich die beste Welt,", "tokens": ["Und", "in", "sich", "die", "bes\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ist ein h\u00f6chstes Wohlergehen,", "tokens": ["Ist", "ein", "h\u00f6chs\u00b7tes", "Woh\u00b7ler\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welchem keines gleich gestellt;", "tokens": ["Wel\u00b7chem", "kei\u00b7nes", "gleich", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr einander blo\u00df zu leben,", "tokens": ["F\u00fcr", "ein\u00b7an\u00b7der", "blo\u00df", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch holdseeliges Bestreben", "tokens": ["Durch", "hold\u00b7see\u00b7li\u00b7ges", "Be\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich des Gatten Wonne weihn,", "tokens": ["Sich", "des", "Gat\u00b7ten", "Won\u00b7ne", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus den Augen zu studieren,", "tokens": ["Aus", "den", "Au\u00b7gen", "zu", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Was das Herz vermag zu r\u00fchren,", "tokens": ["Was", "das", "Herz", "ver\u00b7mag", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Liebe! kannst du st\u00e4rker seyn?", "tokens": ["Lie\u00b7be", "!", "kannst", "du", "st\u00e4r\u00b7ker", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O! was sch\u00f6ne Folgerungen", "tokens": ["O", "!", "was", "sch\u00f6\u00b7ne", "Fol\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bauen da ein frohes Haus!", "tokens": ["Bau\u00b7en", "da", "ein", "fro\u00b7hes", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche Sprachen, welche Zungen", "tokens": ["Wel\u00b7che", "Spra\u00b7chen", ",", "wel\u00b7che", "Zun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00fccken es nach W\u00fcrden aus!", "tokens": ["Dr\u00fc\u00b7cken", "es", "nach", "W\u00fcr\u00b7den", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was dem ersten Paar begegnet,", "tokens": ["Was", "dem", "ers\u00b7ten", "Paar", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welches GOtt selbst eingesegnet,", "tokens": ["Wel\u00b7ches", "Gott", "selbst", "ein\u00b7ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Welches er selbst werden hie\u00df,", "tokens": ["Wel\u00b7ches", "er", "selbst", "wer\u00b7den", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das erfahren auch die Seelen,", "tokens": ["Das", "er\u00b7fah\u00b7ren", "auch", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die sich dergestalt verm\u00e4hlen,", "tokens": ["Die", "sich", "der\u00b7ge\u00b7stalt", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie sind sich ein Paradies.", "tokens": ["Sie", "sind", "sich", "ein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Holde Liebe! sey gepriesen,", "tokens": ["Hol\u00b7de", "Lie\u00b7be", "!", "sey", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du dich so sch\u00f6n erkl\u00e4rt,", "tokens": ["Da\u00df", "du", "dich", "so", "sch\u00f6n", "er\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in diesem Paar gewiesen,", "tokens": ["Und", "in", "die\u00b7sem", "Paar", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es ist einander werth;", "tokens": ["Denn", "es", "ist", "ein\u00b7an\u00b7der", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es hat sich in sich gefunden,", "tokens": ["Es", "hat", "sich", "in", "sich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PRF", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und sobald es sich verbunden,", "tokens": ["Und", "so\u00b7bald", "es", "sich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ward es sich ein Himmelreich;", "tokens": ["Ward", "es", "sich", "ein", "Him\u00b7mel\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle, die es itzo sehen,", "tokens": ["Al\u00b7le", ",", "die", "es", "it\u00b7zo", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lassen diesen Wunsch ergehen:", "tokens": ["Las\u00b7sen", "die\u00b7sen", "Wunsch", "er\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Segen, Heyl und Gl\u00fcck mit Euch!", "tokens": ["Se\u00b7gen", ",", "Heyl", "und", "Gl\u00fcck", "mit", "Euch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Muse! komm und hilf mir singen!", "tokens": ["Mu\u00b7se", "!", "komm", "und", "hilf", "mir", "sin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singe du, der Seebach werth!", "tokens": ["Sin\u00b7ge", "du", ",", "der", "See\u00b7bach", "werth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hilf mein Lied der Wahrheit bringen,", "tokens": ["Hilf", "mein", "Lied", "der", "Wahr\u00b7heit", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie das von uns begehrt;", "tokens": ["Weil", "sie", "das", "von", "uns", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer Mutter fromme Tugend", "tokens": ["Ei\u00b7ner", "Mut\u00b7ter", "from\u00b7me", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist der Schmuck von ihrer Jugend,", "tokens": ["Ist", "der", "Schmuck", "von", "ih\u00b7rer", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Natur zeigt sie mit Pracht", "tokens": ["Die", "Na\u00b7tur", "zeigt", "sie", "mit", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "In leutseeligen Geberden,", "tokens": ["In", "leut\u00b7see\u00b7li\u00b7gen", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welche sittsam-freundlich werden,", "tokens": ["Wel\u00b7che", "sitt\u00b7sam\u00b7freund\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo der Demuth Unschuld lacht.", "tokens": ["Wo", "der", "De\u00b7muth", "Un\u00b7schuld", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch die Ehrfurcht heisst mich schweigen,", "tokens": ["Doch", "die", "Ehr\u00b7furcht", "heisst", "mich", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Wink gehorsam seyn,", "tokens": ["Und", "ein", "Wink", "ge\u00b7hor\u00b7sam", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahrer Demuth ist es eigen,", "tokens": ["Wah\u00b7rer", "De\u00b7muth", "ist", "es", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie verbirgt sich ungemein.", "tokens": ["Sie", "ver\u00b7birgt", "sich", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kenner tugendhafter Sch\u00f6nen!", "tokens": ["Ken\u00b7ner", "tu\u00b7gend\u00b7haf\u00b7ter", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lasst doch euer Lob ert\u00f6nen,", "tokens": ["Lasst", "doch", "eu\u00b7er",