ch", "be\u00b7schei\u00b7den", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bleib in deiner Niedrigkeit.", "tokens": ["Bleib", "in", "dei\u00b7ner", "Nied\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Himmel! eine solche Ehe", "tokens": ["Him\u00b7mel", "!", "ei\u00b7ne", "sol\u00b7che", "E\u00b7he"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey des Segens Gegenstand!", "tokens": ["Sey", "des", "Se\u00b7gens", "Ge\u00b7gen\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier ist Amen! es geschehe!", "tokens": ["Hier", "ist", "A\u00b7men", "!", "es", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Denn sie kommt von deiner Hand.", "tokens": ["Denn", "sie", "kommt", "von", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eines Vaters Wunsch und Hoffen", "tokens": ["Ei\u00b7nes", "Va\u00b7ters", "Wunsch", "und", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Siehet sich 