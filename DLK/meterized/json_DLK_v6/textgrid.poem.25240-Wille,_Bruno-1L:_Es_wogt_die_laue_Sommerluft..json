{"textgrid.poem.25240": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wogt die laue Sommerluft.", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wogt die laue Sommerluft.", "tokens": ["Es", "wogt", "die", "lau\u00b7e", "Som\u00b7mer\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wacholderb\u00fcsche, Brombeerranken", "tokens": ["Wa\u00b7chol\u00b7der\u00b7b\u00fc\u00b7sche", ",", "Brom\u00b7beer\u00b7ran\u00b7ken"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Adlerfarren nicken, wanken.", "tokens": ["Und", "Ad\u00b7ler\u00b7far\u00b7ren", "ni\u00b7cken", ",", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die struppigen Kiefernh\u00e4upter schwanken;", "tokens": ["Die", "strup\u00b7pi\u00b7gen", "Kie\u00b7fern\u00b7h\u00e4up\u00b7ter", "schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Rehbraune \u00c4ste knarren.", "tokens": ["Reh\u00b7brau\u00b7ne", "\u00c4s\u00b7te", "knar\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Von ihren zarten, schlanken,", "tokens": ["Von", "ih\u00b7ren", "zar\u00b7ten", ",", "schlan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Lichtgr\u00fcnen Schossen st\u00e4ubt", "tokens": ["Licht\u00b7gr\u00fc\u00b7nen", "Schos\u00b7sen", "st\u00e4ubt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der harzige Duft,", "tokens": ["Der", "har\u00b7zi\u00b7ge", "Duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und die weiche Luft", "tokens": ["Und", "die", "wei\u00b7che", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Wallt hin wie bet\u00e4ubt.", "tokens": ["Wallt", "hin", "wie", "be\u00b7t\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Auf einmal tut sich l\u00e4chelnd auf", "tokens": ["Auf", "ein\u00b7mal", "tut", "sich", "l\u00e4\u00b7chelnd", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PRF", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die freie sonnige Welt:", "tokens": ["Die", "frei\u00b7e", "son\u00b7ni\u00b7ge", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Weithin blendendes Himmelblau;", "tokens": ["Weit\u00b7hin", "blen\u00b7den\u00b7des", "Him\u00b7mel\u00b7blau", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Weithin heitre Wolken zu Hauf;", "tokens": ["Weit\u00b7hin", "heit\u00b7re", "Wol\u00b7ken", "zu", "Hauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Weithin wogendes \u00c4hrenfeld", "tokens": ["Weit\u00b7hin", "wo\u00b7gen\u00b7des", "\u00c4h\u00b7ren\u00b7feld"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und gr\u00fcne gr\u00fcne Auen ...", "tokens": ["Und", "gr\u00fc\u00b7ne", "gr\u00fc\u00b7ne", "Au\u00b7en", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hier an Kiefernwaldes Saum", "tokens": ["Hier", "an", "Kie\u00b7fern\u00b7wal\u00b7des", "Saum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ich weilen, will ich schauen/", "tokens": ["Will", "ich", "wei\u00b7len", ",", "will", "ich", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Unter zartem Akazienbaum,", "tokens": ["Un\u00b7ter", "zar\u00b7tem", "A\u00b7ka\u00b7zi\u00b7en\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Der vom muntern Wind ger\u00fcttelt", "tokens": ["Der", "vom", "mun\u00b7tern", "Wind", "ge\u00b7r\u00fct\u00b7telt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "S\u00fc\u00dfe Bl\u00fctentrauben sch\u00fcttelt.", "tokens": ["S\u00fc\u00b7\u00dfe", "Bl\u00fc\u00b7ten\u00b7trau\u00b7ben", "sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O Roggenhalme hin und her gebogen!", "tokens": ["O", "Rog\u00b7gen\u00b7hal\u00b7me", "hin", "und", "her", "ge\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sanft sie fl\u00fcstern, wie sie endlos wogen", "tokens": ["Wie", "sanft", "sie", "fl\u00fcs\u00b7tern", ",", "wie", "sie", "end\u00b7los", "wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VVINF", "$,", "PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu blau verschwommenen Fernen!", "tokens": ["Zu", "blau", "ver\u00b7schwom\u00b7me\u00b7nen", "Fer\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schon neigen sich und kernen", "tokens": ["Schon", "nei\u00b7gen", "sich", "und", "ker\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel H\u00e4upter silbergr\u00fcn.", "tokens": ["Viel", "H\u00e4up\u00b7ter", "sil\u00b7ber\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Andre bl\u00fchn,", "tokens": ["And\u00b7re", "bl\u00fchn", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Duftend wie frisches Brot.", "tokens": ["Duf\u00b7tend", "wie", "fri\u00b7sches", "Brot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Dazwischen gl\u00fchn", "tokens": ["Da\u00b7zwi\u00b7schen", "gl\u00fchn"], "token_info": ["word", "word"], "pos": ["PAV", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mohnblumen flammenrot", "tokens": ["Mohn\u00b7blu\u00b7men", "flam\u00b7men\u00b7rot"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Bei dunkelblauen Cyanen ...", "tokens": ["Bei", "dun\u00b7kel\u00b7blau\u00b7en", "Cy\u00b7a\u00b7nen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und droben wallen", "tokens": ["Und", "dro\u00b7ben", "wal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Durch lichtes Blau", "tokens": ["Durch", "lich\u00b7tes", "Blau"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wolkenballen,", "tokens": ["Wol\u00b7ken\u00b7bal\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Gebirgen gleich,", "tokens": ["Ge\u00b7bir\u00b7gen", "gleich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Halb golden und halb grau.", "tokens": ["Halb", "gol\u00b7den", "und", "halb", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Frau Sonne spreitet", "tokens": ["Frau", "Son\u00b7ne", "sprei\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Den Strahlenf\u00e4cher von Silberseide", "tokens": ["Den", "Strah\u00b7len\u00b7f\u00e4\u00b7cher", "von", "Sil\u00b7ber\u00b7sei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zur Erde nieder;", "tokens": ["Zur", "Er\u00b7de", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Dann taucht sie wieder", "tokens": ["Dann", "taucht", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Aus schneeigem Wolkenkleide", "tokens": ["Aus", "schne\u00b7e\u00b7i\u00b7gem", "Wol\u00b7ken\u00b7klei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Blendende Glieder", "tokens": ["Blen\u00b7den\u00b7de", "Glie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Und blitzt und spr\u00fcht", "tokens": ["Und", "blitzt", "und", "spr\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Verkl\u00e4rend Goldgefunkel", "tokens": ["Ver\u00b7kl\u00e4\u00b7rend", "Gold\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word"], "pos": ["VVPP", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Auf Auen, wo lachend bl\u00fcht", "tokens": ["Auf", "Au\u00b7en", ",", "wo", "la\u00b7chend", "bl\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Vergi\u00dfmeinnicht und gelbe Ranunkel", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "und", "gel\u00b7be", "Ra\u00b7nun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Und Sauerampfer ziegelrot ...", "tokens": ["Und", "Sau\u00b7e\u00b7ramp\u00b7fer", "zie\u00b7gel\u00b7rot", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O du sausender brausender Wogewind!", "tokens": ["O", "du", "sau\u00b7sen\u00b7der", "brau\u00b7sen\u00b7der", "Wo\u00b7ge\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie Freiheitsjubel, wie Orgelchor", "tokens": ["Wie", "Frei\u00b7hei\u00b7ts\u00b7ju\u00b7bel", ",", "wie", "Or\u00b7gel\u00b7chor"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$,", "PWAV", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Umrauschest du mein durstig Ohr;", "tokens": ["Um\u00b7rau\u00b7schest", "du", "mein", "durs\u00b7tig", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du k\u00fchlst mein Haupt, umsp\u00fclst die Gewandung,", "tokens": ["Du", "k\u00fchlst", "mein", "Haupt", ",", "um\u00b7sp\u00fclst", "die", "Ge\u00b7wan\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie den K\u00fcstenfelsen die sch\u00e4umende Brandung/", "tokens": ["Wie", "den", "K\u00fcs\u00b7ten\u00b7fel\u00b7sen", "die", "sch\u00e4u\u00b7men\u00b7de", "Bran\u00b7dung", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "O du sausender brausender Wogewind!", "tokens": ["O", "du", "sau\u00b7sen\u00b7der", "brau\u00b7sen\u00b7der", "Wo\u00b7ge\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Nun ebbest du, so weich, so lind/", "tokens": ["Nun", "eb\u00b7best", "du", ",", "so", "weich", ",", "so", "lind", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein S\u00e4useln, Lispeln, F\u00e4cheln.", "tokens": ["Ein", "S\u00e4u\u00b7seln", ",", "Lis\u00b7peln", ",", "F\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Bestrickte dich ein Sonnenl\u00e4cheln?", "tokens": ["Be\u00b7strick\u00b7te", "dich", "ein", "Son\u00b7nen\u00b7l\u00e4\u00b7cheln", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch dein Ges\u00e4usel stirbt;", "tokens": ["Auch", "dein", "Ge\u00b7s\u00e4u\u00b7sel", "stirbt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dann/ lauschige Stille.", "tokens": ["Dann", "/", "lau\u00b7schi\u00b7ge", "Stil\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Nur noch die Grille", "tokens": ["Nur", "noch", "die", "Gril\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Dengelt und zirpt", "tokens": ["Den\u00b7gelt", "und", "zirpt"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Im Erlengeb\u00fcsch, wo das W\u00e4sserlein tr\u00e4umt,", "tokens": ["Im", "Er\u00b7len\u00b7ge\u00b7b\u00fcsch", ",", "wo", "das", "W\u00e4s\u00b7ser\u00b7lein", "tr\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.15": {"text": "Von Lilien gelb ums\u00e4umt.", "tokens": ["Von", "Li\u00b7li\u00b7en", "gelb", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Ins Blaue weltverloren girrt", "tokens": ["Ins", "Blau\u00b7e", "welt\u00b7ver\u00b7lo\u00b7ren", "girrt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Inbr\u00fcnstig die Lerche/ schwirrt", "token