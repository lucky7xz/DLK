{"textgrid.poem.65430": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "3. Fang' auf!", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Engel Manna streuen, breite deinen Mantel aus!", "tokens": ["Wenn", "die", "En\u00b7gel", "Man\u00b7na", "streu\u00b7en", ",", "brei\u00b7te", "dei\u00b7nen", "Man\u00b7tel", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NE", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn die Liebe f\u00e4llt vom Himmel, fl\u00fcchte nicht dich in dein Haus!", "tokens": ["Wenn", "die", "Lie\u00b7be", "f\u00e4llt", "vom", "Him\u00b7mel", ",", "fl\u00fcch\u00b7te", "nicht", "dich", "in", "dein", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "PTKNEG", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hast du Manna nicht gesammelt, sprechen dir die Menschen Hohn;", "tokens": ["Hast", "du", "Man\u00b7na", "nicht", "ge\u00b7sam\u00b7melt", ",", "spre\u00b7chen", "dir", "die", "Men\u00b7schen", "Hohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKNEG", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Lie\u00dfest du die Liebe liegen, sch\u00e4mst du dich vor Gottes Thron.", "tokens": ["Lie\u00b7\u00dfest", "du", "die", "Lie\u00b7be", "lie\u00b7gen", ",", "sch\u00e4mst", "du", "dich", "vor", "Got\u00b7tes", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}