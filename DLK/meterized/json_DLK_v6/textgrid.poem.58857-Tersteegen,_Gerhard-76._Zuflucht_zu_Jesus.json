{"textgrid.poem.58857": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "76. Zuflucht zu Jesus", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer immer seine S\u00fcnden siehet,", "tokens": ["Wer", "im\u00b7mer", "sei\u00b7ne", "S\u00fcn\u00b7den", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird mutlos und dem Herren scheu;", "tokens": ["Wird", "mut\u00b7los", "und", "dem", "Her\u00b7ren", "scheu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer mit der S\u00fcnd' in Jesus fliehet,", "tokens": ["Wer", "mit", "der", "S\u00fcnd'", "in", "Je\u00b7sus", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird durch ihn von S\u00fcnden frei.", "tokens": ["Der", "wird", "durch", "ihn", "von", "S\u00fcn\u00b7den", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}