{"dta.poem.16119": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "173.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn in Geschichten wir von Noth und Jammer lesen,", "tokens": ["Wenn", "in", "Ge\u00b7schich\u00b7ten", "wir", "von", "Noth", "und", "Jam\u00b7mer", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tr\u00f6stet dieses uns: dis alles ist gewesen.", "tokens": ["So", "tr\u00f6s\u00b7tet", "die\u00b7ses", "uns", ":", "dis", "al\u00b7les", "ist", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "PPER", "$.", "PDS", "PIS", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Herzen ruhen l\u00e4ngst, die das erlitten haben,", "tokens": ["Die", "Her\u00b7zen", "ru\u00b7hen", "l\u00e4ngst", ",", "die", "das", "er\u00b7lit\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "PRELS", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihre S\u00fcnden sind mit ihnen auch begraben.", "tokens": ["Und", "ih\u00b7re", "S\u00fcn\u00b7den", "sind", "mit", "ih\u00b7nen", "auch", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ihre Lieb' und Treu, ihr Glauben und ihr Muth,", "tokens": ["Doch", "ih\u00b7re", "Lieb'", "und", "Treu", ",", "ihr", "Glau\u00b7ben", "und", "ihr", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind die auch hin wie Schaum geschwommen auf der Flut?", "tokens": ["Sind", "die", "auch", "hin", "wie", "Schaum", "ge\u00b7schwom\u00b7men", "auf", "der", "Flut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADV", "KOKOM", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mitnichten, diese sind am Leben uns geblieben,", "tokens": ["Mit\u00b7nich\u00b7ten", ",", "die\u00b7se", "sind", "am", "Le\u00b7ben", "uns", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn wozu w\u00fcrde wol Geschichte sonst geschrieben?", "tokens": ["Denn", "wo\u00b7zu", "w\u00fcr\u00b7de", "wol", "Ge\u00b7schich\u00b7te", "sonst", "ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ADV", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}