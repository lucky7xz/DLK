{"textgrid.poem.63118": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schlage schamlos in die Tasten.", "tokens": ["Ich", "schla\u00b7ge", "scham\u00b7los", "in", "die", "Tas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ampel t\u00f6nt. Es zwitschert das Bordell.", "tokens": ["Die", "Am\u00b7pel", "t\u00f6nt", ".", "Es", "zwit\u00b7schert", "das", "Bor\u00b7dell", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die schlanken Knaben bleich vom langen Fasten", "tokens": ["Die", "schlan\u00b7ken", "Kna\u00b7ben", "bleich", "vom", "lan\u00b7gen", "Fas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erheben k\u00fchl sich vom kastalschen Quell.", "tokens": ["Er\u00b7he\u00b7ben", "k\u00fchl", "sich", "vom", "kas\u00b7tal\u00b7schen", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie werfen ab die wolligen Gew\u00e4nder,", "tokens": ["Sie", "wer\u00b7fen", "ab", "die", "wol\u00b7li\u00b7gen", "Ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hemden kurz, die Mutter einst gen\u00e4ht.", "tokens": ["Die", "Hem\u00b7den", "kurz", ",", "die", "Mut\u00b7ter", "einst", "ge\u00b7n\u00e4ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schweben engverschlungne Negerl\u00e4nder,", "tokens": ["Sie", "schwe\u00b7ben", "eng\u00b7ver\u00b7schlung\u00b7ne", "Ne\u00b7ger\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In denen palmengleich die Liebe steht.", "tokens": ["In", "de\u00b7nen", "pal\u00b7men\u00b7gleich", "die", "Lie\u00b7be", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es neigen sich mit ihren schmalen M\u00fcndern", "tokens": ["Es", "nei\u00b7gen", "sich", "mit", "ih\u00b7ren", "schma\u00b7len", "M\u00fcn\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Huren in den unerfahrenen Scho\u00df,", "tokens": ["Die", "Hu\u00b7ren", "in", "den", "un\u00b7er\u00b7fah\u00b7re\u00b7nen", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und sie empfangen von den blassen Kindern", "tokens": ["Und", "sie", "emp\u00b7fan\u00b7gen", "von", "den", "blas\u00b7sen", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4chelnd ihr gutes oder schlimmes Los.", "tokens": ["L\u00e4\u00b7chelnd", "ihr", "gu\u00b7tes", "o\u00b7der", "schlim\u00b7mes", "Los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}