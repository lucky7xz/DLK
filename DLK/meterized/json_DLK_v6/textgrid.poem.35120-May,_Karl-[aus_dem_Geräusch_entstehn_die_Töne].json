{"textgrid.poem.35120": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "[aus dem Ger\u00e4usch entstehn die T\u00f6ne]", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Ger\u00e4usch entstehn die T\u00f6ne;", "tokens": ["Aus", "dem", "Ge\u00b7r\u00e4usch", "ent\u00b7stehn", "die", "T\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Reglung macht zum Klang den Schall.", "tokens": ["Die", "Re\u00b7glung", "macht", "zum", "Klang", "den", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das H\u00e4\u00dfliche erzeugt das Sch\u00f6ne,", "tokens": ["Das", "H\u00e4\u00df\u00b7li\u00b7che", "er\u00b7zeugt", "das", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenns dem Gesetz gehorcht, all\u00fcberall.", "tokens": ["Wenns", "dem", "Ge\u00b7setz", "ge\u00b7horcht", ",", "al\u00b7l\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}