{"textgrid.poem.62537": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "3. Nach Hugo von Werbenwag (Huk von Werbenwak)", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freudenreicher s\u00fc\u00dfer Mai,", "tokens": ["Freu\u00b7den\u00b7rei\u00b7cher", "s\u00fc\u00b7\u00dfer", "Mai", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du sollst uns willkommen sein,", "tokens": ["Du", "sollst", "uns", "will\u00b7kom\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Sch\u00f6ne Blumen mancherlei", "tokens": ["Sch\u00f6\u00b7ne", "Blu\u00b7men", "man\u00b7cher\u00b7lei"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringet uns dein lichter Schein.", "tokens": ["Brin\u00b7get", "uns", "dein", "lich\u00b7ter", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Ja, du hast die Welt versch\u00f6net;", "tokens": ["Ja", ",", "du", "hast", "die", "Welt", "ver\u00b7sch\u00f6\u00b7net", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frei gefr\u00f6hnet", "tokens": ["Frei", "ge\u00b7fr\u00f6h\u00b7net"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "V\u00f6gelein.", "tokens": ["V\u00f6\u00b7ge\u00b7lein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Nun h\u00f6rt man das s\u00fc\u00dfe Singen", "tokens": ["Nun", "h\u00f6rt", "man", "das", "s\u00fc\u00b7\u00dfe", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der geliebten Nachtigall,", "tokens": ["Der", "ge\u00b7lieb\u00b7ten", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Walde laut erklingen", "tokens": ["In", "dem", "Wal\u00b7de", "laut", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren wonniglichen Schall.", "tokens": ["Ih\u00b7ren", "won\u00b7nig\u00b7li\u00b7chen", "Schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo sie wol im Sommer hauset;", "tokens": ["Wo", "sie", "wol", "im", "Som\u00b7mer", "hau\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Verklauset", "tokens": ["Ver\u00b7klau\u00b7set"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Steht ihr Saal.", "tokens": ["Steht", "ihr", "Saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wenn wir hierbei traurig w\u00e4ren,", "tokens": ["Wenn", "wir", "hier\u00b7bei", "trau\u00b7rig", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie geziemt uns Jungen das?", "tokens": ["Wie", "ge\u00b7ziemt", "uns", "Jun\u00b7gen", "das", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PPER", "NN", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei so wonniglichen M\u00e4ren", "tokens": ["Bei", "so", "won\u00b7nig\u00b7li\u00b7chen", "M\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziemt uns gro\u00dfe Freude ba\u00df.", "tokens": ["Ziemt", "uns", "gro\u00b7\u00dfe", "Freu\u00b7de", "ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft uns Allen Freude machen", "tokens": ["La\u00dft", "uns", "Al\u00b7len", "Freu\u00b7de", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und verlachen", "tokens": ["Und", "ver\u00b7la\u00b7chen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Argen Ha\u00df.", "tokens": ["Ar\u00b7gen", "Ha\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}