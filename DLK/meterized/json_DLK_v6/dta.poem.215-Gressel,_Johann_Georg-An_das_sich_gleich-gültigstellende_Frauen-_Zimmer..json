{"dta.poem.215": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An das sich gleich-g\u00fcltigstellende Frauen-  \n Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Sch\u00f6nen z\u00fcndet ihr kein Liebes-Feur mehr an/", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", "z\u00fcn\u00b7det", "ihr", "kein", "Lie\u00b7bes\u00b7Feur", "mehr", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mu\u00df die holde Gluht in euch vergebens brennen?", "tokens": ["Und", "mu\u00df", "die", "hol\u00b7de", "Gluht", "in", "euch", "ver\u00b7ge\u00b7bens", "bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo sind die Flammen/ die euch ", "tokens": ["Wo", "sind", "die", "Flam\u00b7men", "/", "die", "euch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$(", "PRELS", "PPER"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Und wolt ihr keinen Blick den Manns-Persohnen g\u00f6nnen?", "tokens": ["Und", "wolt", "ihr", "kei\u00b7nen", "Blick", "den", "Manns\u00b7Per\u00b7soh\u00b7nen", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein! sagt ist das kein Feur/ das in der Aschen glimmt/", "tokens": ["Mein", "!", "sagt", "ist", "das", "kein", "Feur", "/", "das", "in", "der", "A\u00b7schen", "glimmt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "VVFIN", "VAFIN", "ART", "PIAT", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ist das keine Gluht/ was in den Augen stecket/", "tokens": ["Und", "ist", "das", "kei\u00b7ne", "Gluht", "/", "was", "in", "den", "Au\u00b7gen", "ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "PIAT", "NN", "$(", "PWS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie! da\u00df es denn so bald den freyhen Geist einnimmt/", "tokens": ["Wie", "!", "da\u00df", "es", "denn", "so", "bald", "den", "frey\u00b7hen", "Geist", "ein\u00b7nimmt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und in der harten Brust ein Liebes-Feur erwecket?", "tokens": ["Und", "in", "der", "har\u00b7ten", "Brust", "ein", "Lie\u00b7bes\u00b7Feur", "er\u00b7we\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer glaubte/ da\u00df noch Feur in ", "tokens": ["Wer", "glaub\u00b7te", "/", "da\u00df", "noch", "Feur", "in"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "ADV", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wenn nicht der Rauch und Dampff den innern Brand", "tokens": ["Wenn", "nicht", "der", "Rauch", "und", "Dampff", "den", "in\u00b7nern", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "bewiesen/", "tokens": ["be\u00b7wie\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Eur rohtes Angesicht verbirget nicht so sehr/", "tokens": ["Eur", "roh\u00b7tes", "An\u00b7ge\u00b7sicht", "ver\u00b7bir\u00b7get", "nicht", "so", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als ihr/ die sch\u00f6ne Gluht/ doch wirds von euch gepriesen.", "tokens": ["Als", "ihr", "/", "die", "sch\u00f6\u00b7ne", "Gluht", "/", "doch", "wirds", "von", "euch", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ART", "ADJA", "NN", "$(", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn ein beliebter Blick aus euren Augen geht/", "tokens": ["Wenn", "ein", "be\u00b7lieb\u00b7ter", "Blick", "aus", "eu\u00b7ren", "Au\u00b7gen", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das ist als wenn ein Blitz durch dicke Wolcken f\u00e4hret/", "tokens": ["Das", "ist", "als", "wenn", "ein", "Blitz", "durch", "di\u00b7cke", "Wol\u00b7cken", "f\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jemehr der Wind und Lufft dem Wetter widersteht/", "tokens": ["Je\u00b7mehr", "der", "Wind", "und", "Lufft", "dem", "Wet\u00b7ter", "wi\u00b7der\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jemehr man dessen Krafft und Donner-Schl\u00e4ge h\u00f6ret.", "tokens": ["Je\u00b7mehr", "man", "des\u00b7sen", "Krafft", "und", "Don\u00b7ner\u00b7Schl\u00e4\u00b7ge", "h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRELAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was euer Hertz gedenckt/ das zeigt das Auge an/", "tokens": ["Was", "eu\u00b7er", "Hertz", "ge\u00b7denckt", "/", "das", "zeigt", "das", "Au\u00b7ge", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$(", "PDS", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie sind/ wi\u00dft ihrs doch wol/ Verr\u00e4hter eurer Seelen/", "tokens": ["Sie", "sind", "/", "wi\u00dft", "ihrs", "doch", "wol", "/", "Ver\u00b7r\u00e4h\u00b7ter", "eu\u00b7rer", "See\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "VVFIN", "PPER", "ADV", "ADV", "$(", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein Spiegel holder Gunst/ der Liebe Renne-Bahn/", "tokens": ["Ein", "Spie\u00b7gel", "hol\u00b7der", "Gunst", "/", "der", "Lie\u00b7be", "Ren\u00b7ne\u00b7Bahn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wo selbst durch Blick und Strahl die Hertzen sich verm\u00e4hlen.", "tokens": ["Wo", "selbst", "durch", "Blick", "und", "Strahl", "die", "Hert\u00b7zen", "sich", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr werffet Donner zwar und Blitz auf den herzu/", "tokens": ["Ihr", "werf\u00b7fet", "Don\u00b7ner", "zwar", "und", "Blitz", "auf", "den", "her\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "KON", "NN", "APPR", "ART", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der eure Wangen will/ und auch die Lippen k\u00fcssen/", "tokens": ["Der", "eu\u00b7re", "Wan\u00b7gen", "will", "/", "und", "auch", "die", "Lip\u00b7pen", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "$(", "KON", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Schalckhaffte ists nicht wahr empfindet ihr eh Ruh", "tokens": ["Schalck\u00b7haff\u00b7te", "ists", "nicht", "wahr", "emp\u00b7fin\u00b7det", "ihr", "eh", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PTKNEG", "ADJD", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bis da\u00df man eurem Mund die K\u00fcsse lassen wissen?", "tokens": ["Bis", "da\u00df", "man", "eu\u00b7rem", "Mund", "die", "K\u00fcs\u00b7se", "las\u00b7sen", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "PPOSAT", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der mu\u00df verworffen seyn bis in das dunckle Grab/", "tokens": ["Der", "mu\u00df", "ver\u00b7worf\u00b7fen", "seyn", "bis", "in", "das", "dunck\u00b7le", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVPP", "VAINF", "KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer nach den Lippen will und nach den Br\u00fcsten l\u00e4chtzen/", "tokens": ["Wer", "nach", "den", "Lip\u00b7pen", "will", "und", "nach", "den", "Br\u00fcs\u00b7ten", "l\u00e4cht\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und der wird gar verdammt/ der da bricht Fr\u00fcchte ab/", "tokens": ["Und", "der", "wird", "gar", "ver\u00b7dammt", "/", "der", "da", "bricht", "Fr\u00fcch\u00b7te", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "VVPP", "$(", "ART", "ADV", "VVFIN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch sieht man insgeheim euch nach den M\u00e4nnern \u00e4chtzen.", "tokens": ["Doch", "sieht", "man", "ins\u00b7ge\u00b7heim", "euch", "nach", "den", "M\u00e4n\u00b7nern", "\u00e4cht\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPRART", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Euch scheint ein s\u00fcsser Ku\u00df ein Uberdrus zu seyn/", "tokens": ["Euch", "scheint", "ein", "s\u00fcs\u00b7ser", "Ku\u00df", "ein", "U\u00b7ber\u00b7drus", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch wolt ihr/ da\u00df man ihn euch auf die Lippen leget/", "tokens": ["Doch", "wolt", "ihr", "/", "da\u00df", "man", "ihn", "euch", "auf", "die", "Lip\u00b7pen", "le\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$(", "KOUS", "PIS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn man euch k\u00fc\u00dft/ so tr\u00e4nckt man euch mit ", "tokens": ["Wenn", "man", "euch", "k\u00fc\u00dft", "/", "so", "tr\u00e4nckt", "man", "euch", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PIS", "PPER", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Und euer Hertze wird zu s\u00fcsser Lust beweget.", "tokens": ["Und", "eu\u00b7er", "Hert\u00b7ze", "wird", "zu", "s\u00fcs\u00b7ser", "Lust", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Jhr gebt zum Schau-Gericht die milchern Br\u00fcste hin/", "tokens": ["Ihr", "gebt", "zum", "Schau\u00b7Ge\u00b7richt", "die", "mil\u00b7chern", "Br\u00fcs\u00b7te", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Seuffzer m\u00fcssen sie hoch in die H\u00f6he schwellen/", "tokens": ["Die", "Seuff\u00b7zer", "m\u00fcs\u00b7sen", "sie", "hoch", "in", "die", "H\u00f6\u00b7he", "schwel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Jhr liebt/ und wollet doch der ", "tokens": ["Ihr", "liebt", "/", "und", "wol\u00b7let", "doch", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "VMFIN", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Es ist euch gar kein Ernst eur Weigern und Verstellen.", "tokens": ["Es", "ist", "euch", "gar", "kein", "Ernst", "eur", "Wei\u00b7gern", "und", "Ver\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIAT", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn ihr die Brust beschaut/ wie da Corallen gl\u00fchn/", "tokens": ["Wenn", "ihr", "die", "Brust", "be\u00b7schaut", "/", "wie", "da", "Co\u00b7ral\u00b7len", "gl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "KOKOM", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So mu\u00df der Wollust-Wind sie in die H\u00f6he prallen/", "tokens": ["So", "mu\u00df", "der", "Wol\u00b7lust\u00b7Wind", "sie", "in", "die", "H\u00f6\u00b7he", "pral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Jhr wolt man soll daraus den Liebes-Zunder ziehn/", "tokens": ["Ihr", "wolt", "man", "soll", "da\u00b7raus", "den", "Lie\u00b7bes\u00b7Zun\u00b7der", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VMFIN", "PAV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und k\u00fcssend behten an die sch\u00f6nen Liebes-Ballen.", "tokens": ["Und", "k\u00fcs\u00b7send", "beh\u00b7ten", "an", "die", "sch\u00f6\u00b7nen", "Lie\u00b7bes\u00b7Bal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART",