{"textgrid.poem.34855": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir heben nun zu singen an", "tokens": ["Wir", "he\u00b7ben", "nun", "zu", "sin\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lied von einer Nummer,", "tokens": ["Das", "Lied", "von", "ei\u00b7ner", "Num\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ist gehei\u00dfen Nummer Drei;", "tokens": ["Die", "ist", "ge\u00b7hei\u00b7\u00dfen", "Num\u00b7mer", "Drei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Freuden kommt der Kummer.", "tokens": ["Nach", "Freu\u00b7den", "kommt", "der", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Arabischen Ursprungs war sie zwar,", "tokens": ["A\u00b7ra\u00b7bi\u00b7schen", "Ur\u00b7sprungs", "war", "sie", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Doch christent\u00fcmlich frummer", "tokens": ["Doch", "chris\u00b7ten\u00b7t\u00fcm\u00b7lich", "frum\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In ganz Europa niemand war", "tokens": ["In", "ganz", "Eu\u00b7ro\u00b7pa", "nie\u00b7mand", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NE", "PIS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie jene brave Nummer.", "tokens": ["Wie", "je\u00b7ne", "bra\u00b7ve", "Num\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie war ein Muster der Sittlichkeit", "tokens": ["Sie", "war", "ein", "Mus\u00b7ter", "der", "Sitt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wurde rot wie ein Hummer,", "tokens": ["Und", "wur\u00b7de", "rot", "wie", "ein", "Hum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fand sie den Knecht im Bette der Magd;", "tokens": ["Fand", "sie", "den", "Knecht", "im", "Bet\u00b7te", "der", "Magd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gab beiden einen Brummer.", "tokens": ["Gab", "bei\u00b7den", "ei\u00b7nen", "Brum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Des Morgens trank sie den Kaffee", "tokens": ["Des", "Mor\u00b7gens", "trank", "sie", "den", "Kaf\u00b7fee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um sieben Uhr im Summer,", "tokens": ["Um", "sie\u00b7ben", "Uhr", "im", "Sum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Winter um neun, und in der Nacht", "tokens": ["Im", "Win\u00b7ter", "um", "neun", ",", "und", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "CARD", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geno\u00df sie den besten Schlummer.", "tokens": ["Ge\u00b7no\u00df", "sie", "den", "bes\u00b7ten", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Jetzt aber \u00e4ndert sich der Reim,", "tokens": ["Jetzt", "a\u00b7ber", "\u00e4n\u00b7dert", "sich", "der", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00e4ndern sich die Tage;", "tokens": ["Und", "\u00e4n\u00b7dern", "sich", "die", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es mu\u00df die arme Nummer Drei", "tokens": ["Es", "mu\u00df", "die", "ar\u00b7me", "Num\u00b7mer", "Drei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erdulden Pein und Plage.", "tokens": ["E\u00b7rdul\u00b7den", "Pein", "und", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da kam ein Schuster und sagte: der Kopf", "tokens": ["Da", "kam", "ein", "Schus\u00b7ter", "und", "sag\u00b7te", ":", "der", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Nummer Drei, der s\u00e4he", "tokens": ["Der", "Num\u00b7mer", "Drei", ",", "der", "s\u00e4\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "CARD", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie eine kleine Sieben aus,", "tokens": ["Wie", "ei\u00b7ne", "klei\u00b7ne", "Sie\u00b7ben", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die auf einem Halbmond stehe.", "tokens": ["Die", "auf", "ei\u00b7nem", "Halb\u00b7mond", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Sieben sei aber die mystische Zahl", "tokens": ["Die", "Sie\u00b7ben", "sei", "a\u00b7ber", "die", "mys\u00b7ti\u00b7sche", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der alten Pythagoreer,", "tokens": ["Der", "al\u00b7ten", "Py\u00b7tha\u00b7go\u00b7reer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Halbmond bedeute Dianendienst,", "tokens": ["Der", "Halb\u00b7mond", "be\u00b7deu\u00b7te", "Di\u00b7a\u00b7nen\u00b7dienst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Er mahne auch an Sab\u00e4er.", "tokens": ["Er", "mah\u00b7ne", "auch", "an", "Sa\u00b7b\u00e4\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie selber, die Drei, sei Schibboleth", "tokens": ["Sie", "sel\u00b7ber", ",", "die", "Drei", ",", "sei", "Schib\u00b7bo\u00b7leth"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "CARD", "$,", "VAFIN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Oberbonzen von Babel;", "tokens": ["Des", "O\u00b7ber\u00b7bon\u00b7zen", "von", "Ba\u00b7bel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch dessen Buhlschaft sie einst gebar", "tokens": ["Durch", "des\u00b7sen", "Buhl\u00b7schaft", "sie", "einst", "ge\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die heil'ge Dreieinigkeitsfabel.", "tokens": ["Die", "heil'\u00b7ge", "Drei\u00b7ei\u00b7nig\u00b7keits\u00b7fa\u00b7bel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein K\u00fcrschner bemerkte dagegen: die Drei", "tokens": ["Ein", "K\u00fcrschner", "be\u00b7merk\u00b7te", "da\u00b7ge\u00b7gen", ":", "die", "Drei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "$.", "ART", "CARD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie eine fromme Trulle,", "tokens": ["Sie", "ei\u00b7ne", "from\u00b7me", "Trul\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verehrt von unsern V\u00e4tern, die einst", "tokens": ["Ver\u00b7ehrt", "von", "un\u00b7sern", "V\u00e4\u00b7tern", ",", "die", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Geglaubt an jede Schrulle.", "tokens": ["Ge\u00b7glaubt", "an", "je\u00b7de", "Schrul\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da war ein Schneider, der l\u00e4chelnd sprach,", "tokens": ["Da", "war", "ein", "Schnei\u00b7der", ",", "der", "l\u00e4\u00b7chelnd", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df gar nicht existiere", "tokens": ["Da\u00df", "gar", "nicht", "e\u00b7xis\u00b7tie\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Nummer Drei, da\u00df sie sich nur", "tokens": ["Die", "Num\u00b7mer", "Drei", ",", "da\u00df", "sie", "sich", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "CARD", "$,", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befinde auf dem Papiere.", "tokens": ["Be\u00b7fin\u00b7de", "auf", "dem", "Pa\u00b7pie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Als solches h\u00f6rte die arme Drei,", "tokens": ["Als", "sol\u00b7ches", "h\u00f6r\u00b7te", "die", "ar\u00b7me", "Drei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie eine verzweifelte Ente,", "tokens": ["Wie", "ei\u00b7ne", "ver\u00b7zwei\u00b7fel\u00b7te", "En\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie wackelte hin, sie wackelte her,", "tokens": ["Sie", "wa\u00b7ckel\u00b7te", "hin", ",", "sie", "wa\u00b7ckel\u00b7te", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie jammerte und flennte:", "tokens": ["Sie", "jam\u00b7mer\u00b7te", "und", "flenn\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbich bin so alt wie das Meer und der Wald,", "tokens": ["\u00bb", "ich", "bin", "so", "alt", "wie", "das", "Meer", "und", "der", "Wald", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "++-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie die Stern', die am Himmel blinken;", "tokens": ["Wie", "die", "Stern'", ",", "die", "am", "Him\u00b7mel", "blin\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sah Reiche entstehn, sah Reiche vergehn", "tokens": ["Sah", "Rei\u00b7che", "ent\u00b7stehn", ",", "sah", "Rei\u00b7che", "ver\u00b7gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "VVINF", "$,", "VVFIN", "NE", "VVINF"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und V\u00f6lker aufsteigen und sinken.", "tokens": ["Und", "V\u00f6l\u00b7ker", "auf\u00b7stei\u00b7gen", "und", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.13": {"line.1": {"text": "Ich stand am schnurrenden Webstuhl der Zeit", "tokens": ["Ich", "stand", "am", "schnur\u00b7ren\u00b7den", "Web\u00b7stuhl", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl manches lange Jahrtausend;", "tokens": ["Wohl", "man\u00b7ches", "lan\u00b7ge", "Jahr\u00b7tau\u00b7send", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich sah der Natur in den schaffenden Bauch,", "tokens": ["Ich", "sah", "der", "Na\u00b7tur", "in", "den", "schaf\u00b7fen\u00b7den", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das wogte brausend und sausend.", "tokens": ["Das", "wog\u00b7te", "brau\u00b7send", "und", "sau\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Und dennoch widerstand ich dem Sturm", "tokens": ["Und", "den\u00b7noch", "wi\u00b7der\u00b7stand", "ich", "dem", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der sinnlich dunkeln Gewalten \u2013", "tokens": ["Der", "sinn\u00b7lich", "dun\u00b7keln", "Ge\u00b7wal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich habe meine Jungferschaft", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7ne", "Jung\u00b7fer\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In all dem Spektakel behalten.", "tokens": ["In", "all", "dem", "Spek\u00b7ta\u00b7kel", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.