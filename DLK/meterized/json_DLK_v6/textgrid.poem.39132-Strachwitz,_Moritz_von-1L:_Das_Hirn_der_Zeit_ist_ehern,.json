{"textgrid.poem.39132": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Hirn der Zeit ist ehern,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Hirn der Zeit ist ehern,", "tokens": ["Das", "Hirn", "der", "Zeit", "ist", "e\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist verstockt, vertaubt,", "tokens": ["Es", "ist", "ver\u00b7stockt", ",", "ver\u00b7taubt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hat entflammten Sehern", "tokens": ["Es", "hat", "ent\u00b7flamm\u00b7ten", "Se\u00b7hern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch immer nicht geglaubt.", "tokens": ["Noch", "im\u00b7mer", "nicht", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat Gebet und Jammern", "tokens": ["Es", "hat", "Ge\u00b7bet", "und", "Jam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch nichts dar\u00fcber vermocht,", "tokens": ["Noch", "nichts", "da\u00b7r\u00fc\u00b7ber", "ver\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PAV", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn man mit eisernem Hammer", "tokens": ["Wenn", "man", "mit", "ei\u00b7ser\u00b7nem", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Nicht donnernd daran gepocht.", "tokens": ["Nicht", "don\u00b7nernd", "da\u00b7ran", "ge\u00b7pocht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PAV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Ro\u00df der Zeit w\u00e4lzt tr\u00e4ge", "tokens": ["Das", "Ro\u00df", "der", "Zeit", "w\u00e4lzt", "tr\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am liebsten im Kote sich;", "tokens": ["Am", "liebs\u00b7ten", "im", "Ko\u00b7te", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPRART", "NN", "PRF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da frommen nur Gei\u00dfelschl\u00e4ge", "tokens": ["Da", "from\u00b7men", "nur", "Gei\u00b7\u00dfel\u00b7schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und spitziger Sporen Stich.", "tokens": ["Und", "spit\u00b7zi\u00b7ger", "Spo\u00b7ren", "Stich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es brachte Liebkosen und Schmeicheln", "tokens": ["Es", "brach\u00b7te", "Lieb\u00b7ko\u00b7sen", "und", "Schmei\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Es nimmer noch von der Stell':", "tokens": ["Es", "nim\u00b7mer", "noch", "von", "der", "Stell'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Man mu\u00df es blutig streicheln,", "tokens": ["Man", "mu\u00df", "es", "blu\u00b7tig", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein dickes B\u00fcffelfell.", "tokens": ["Sein", "di\u00b7ckes", "B\u00fcf\u00b7fel\u00b7fell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Feld der Zeit ist steinig,", "tokens": ["Das", "Feld", "der", "Zeit", "ist", "stei\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es tr\u00e4gt nicht Bl\u00fcte noch Frucht,", "tokens": ["Es", "tr\u00e4gt", "nicht", "Bl\u00fc\u00b7te", "noch", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Pflug zersplittert schleunig,", "tokens": ["Der", "Pflug", "zer\u00b7split\u00b7tert", "schleu\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der d'rin zu w\u00fchlen versucht.", "tokens": ["Der", "d'\u00b7rin", "zu", "w\u00fch\u00b7len", "ver\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Man mu\u00df mit ganzen Geschwadern", "tokens": ["Man", "mu\u00df", "mit", "gan\u00b7zen", "Ge\u00b7schwa\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Es stampfen locker und weich,", "tokens": ["Es", "stamp\u00b7fen", "lo\u00b7cker", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Man mu\u00df des Erdreichs Adern", "tokens": ["Man", "mu\u00df", "des", "Er\u00b7dreichs", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aufrei\u00dfen mit Schwertesstreich.", "tokens": ["Auf\u00b7rei\u00b7\u00dfen", "mit", "Schwer\u00b7tes\u00b7streich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Wer reitet beherzt und wacker,", "tokens": ["Wer", "rei\u00b7tet", "be\u00b7herzt", "und", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wer zwingt das st\u00f6rrische Pferd,", "tokens": ["Wer", "zwingt", "das", "st\u00f6r\u00b7ri\u00b7sche", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wer pfl\u00fcgt den steinigen Acker", "tokens": ["Wer", "pfl\u00fcgt", "den", "stei\u00b7ni\u00b7gen", "A\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit dem schneidigen Heldenschwert?", "tokens": ["Mit", "dem", "schnei\u00b7di\u00b7gen", "Hel\u00b7den\u00b7schwert", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Das Ro\u00df bleibt unbezwungen,", "tokens": ["Das", "Ro\u00df", "bleibt", "un\u00b7be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Feld bleibt ungest\u00f6rt;", "tokens": ["Das", "Feld", "bleibt", "un\u00b7ge\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was ich hier gesungen,", "tokens": ["Und", "was", "ich", "hier", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird bleiben ungeh\u00f6rt.", "tokens": ["Wird", "blei\u00b7ben", "un\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}