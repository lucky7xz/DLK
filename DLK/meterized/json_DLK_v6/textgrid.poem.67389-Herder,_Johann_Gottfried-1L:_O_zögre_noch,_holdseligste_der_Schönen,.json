{"textgrid.poem.67389": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: O z\u00f6gre noch, holdseligste der Sch\u00f6nen,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O z\u00f6gre noch, holdseligste der Sch\u00f6nen,", "tokens": ["O", "z\u00f6g\u00b7re", "noch", ",", "hold\u00b7se\u00b7ligs\u00b7te", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aurora, la\u00df die Thr\u00e4ne Dich vers\u00f6hnen,", "tokens": ["Au\u00b7ro\u00b7ra", ",", "la\u00df", "die", "Thr\u00e4\u00b7ne", "Dich", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Thr\u00e4ne, die Dir reine Liebe weiht!", "tokens": ["Die", "Thr\u00e4\u00b7ne", ",", "die", "Dir", "rei\u00b7ne", "Lie\u00b7be", "weiht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Du in Deines grauen Tithon's Armen", "tokens": ["Wenn", "Du", "in", "Dei\u00b7nes", "grau\u00b7en", "Ti\u00b7thon's", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu fr\u00fch erwachst, so weile, hab Erbarmen!", "tokens": ["Zu", "fr\u00fch", "er\u00b7wachst", ",", "so", "wei\u00b7le", ",", "hab", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr uns, f\u00fcr uns ist es zu fr\u00fche Zeit.", "tokens": ["F\u00fcr", "uns", ",", "f\u00fcr", "uns", "ist", "es", "zu", "fr\u00fc\u00b7he", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die keusche Luna blickt von ihrem Throne", "tokens": ["Die", "keu\u00b7sche", "Lu\u00b7na", "blickt", "von", "ih\u00b7rem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gef\u00e4llig noch und g\u00f6nnt Dionens Sohne", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "noch", "und", "g\u00f6nnt", "Di\u00b7o\u00b7nens", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KON", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihr sanftes Licht der Herzvertraulichkeit.", "tokens": ["Ihr", "sanf\u00b7tes", "Licht", "der", "Herz\u00b7ver\u00b7trau\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbund komm' ich denn, um Euer Gl\u00fcck zu st\u00f6ren?", "tokens": ["\u00bb", "und", "komm'", "ich", "denn", ",", "um", "Eu\u00b7er", "Gl\u00fcck", "zu", "st\u00f6\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ADV", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beneidend komm' ich, Euer Gl\u00fcck zu mehren,", "tokens": ["Be\u00b7nei\u00b7dend", "komm'", "ich", ",", "Eu\u00b7er", "Gl\u00fcck", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sag': ", "tokens": ["Und", "sag'", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Mit neuer Liebe komm' ich Euch zu kr\u00f6nen", "tokens": ["Mit", "neu\u00b7er", "Lie\u00b7be", "komm'", "ich", "Euch", "zu", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und gebe Blumen, J\u00fcnglingen und Sch\u00f6nen,", "tokens": ["Und", "ge\u00b7be", "Blu\u00b7men", ",", "J\u00fcng\u00b7lin\u00b7gen", "und", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Erfrischend sie, der Morgenr\u00f6the Kleid\u00ab.", "tokens": ["Er\u00b7fri\u00b7schend", "sie", ",", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Kleid", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "PPER", "$,", "ART", "NN", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O Malerin Aurora, weile, weile!", "tokens": ["O", "Ma\u00b7le\u00b7rin", "Au\u00b7ro\u00b7ra", ",", "wei\u00b7le", ",", "wei\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Liebenden zu ihrem sch\u00f6nsten Theile", "tokens": ["Den", "Lie\u00b7ben\u00b7den", "zu", "ih\u00b7rem", "sch\u00f6ns\u00b7ten", "Thei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sei nie ein Gestern, sei ein ewig Heut!", "tokens": ["Sei", "nie", "ein", "Ge\u00b7stern", ",", "sei", "ein", "e\u00b7wig", "Heut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}