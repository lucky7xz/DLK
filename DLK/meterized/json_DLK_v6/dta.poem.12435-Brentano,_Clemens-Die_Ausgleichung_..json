{"dta.poem.12435": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Ausgleichung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig \u00fcber Tische sa\u00df,               ", "tokens": ["Der", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "Ti\u00b7sche", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm dienten F\u00fcrsten, Herren,", "tokens": ["Ihm", "dien\u00b7ten", "F\u00fcrs\u00b7ten", ",", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel edle Frauen sch\u00f6n und zart,", "tokens": ["Viel", "ed\u00b7le", "Frau\u00b7en", "sch\u00f6n", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sa\u00dfen sie paarweis.", "tokens": ["So", "sa\u00b7\u00dfen", "sie", "paar\u00b7weis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da man das erste Essen a\u00df,", "tokens": ["Da", "man", "das", "ers\u00b7te", "Es\u00b7sen", "a\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da kam in hohen Ehren,", "tokens": ["Da", "kam", "in", "ho\u00b7hen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein M\u00e4dchen jung, von edler Art,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "jung", ",", "von", "ed\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Also in kluger Weis.", "tokens": ["Al\u00b7so", "in", "klu\u00b7ger", "Weis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Den Becher, den sie schwebend h\u00e4lt,", "tokens": ["Den", "Be\u00b7cher", ",", "den", "sie", "schwe\u00b7bend", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Golde ausgetrieben,", "tokens": ["Von", "Gol\u00b7de", "aus\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nigin sie reicht ihn dar,", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "sie", "reicht", "ihn", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00f6nigin schenkt ein,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "schenkt", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihn vor den K\u00f6nig liebreich stellt:", "tokens": ["Ihn", "vor", "den", "K\u00f6\u00b7nig", "lieb\u00b7reich", "stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edas trink auf treue Liebe!\u201c", "tokens": ["\u201e", "das", "trink", "auf", "treu\u00b7e", "Lie\u00b7be", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da kommt ein Knab mit gelbem Haar,", "tokens": ["Da", "kommt", "ein", "Knab", "mit", "gel\u00b7bem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Tr\u00e4gt einen Mantel fein.", "tokens": ["Tr\u00e4gt", "ei\u00b7nen", "Man\u00b7tel", "fein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig biethet dar sogleich", "tokens": ["Der", "K\u00f6\u00b7nig", "bie\u00b7thet", "dar", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mantel wei\u00df und eben,", "tokens": ["Den", "Man\u00b7tel", "wei\u00df", "und", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nigin als Ehren-Dank:", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "als", "Eh\u00b7ren\u00b7Dank", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ewie sch\u00f6n wird er dir stehn!\u201c", "tokens": ["\u201e", "wie", "sch\u00f6n", "wird", "er", "dir", "stehn", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drauf will er trinken alsogleich,", "tokens": ["Drauf", "will", "er", "trin\u00b7ken", "al\u00b7so\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sprizt der Wein daneben,", "tokens": ["Da", "sprizt", "der", "Wein", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie will den Mantel legen an,", "tokens": ["Sie", "will", "den", "Man\u00b7tel", "le\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Mantel steht nicht sch\u00f6n.", "tokens": ["Der", "Man\u00b7tel", "steht", "nicht", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der K\u00f6nig und die K\u00f6nigin", "tokens": ["Der", "K\u00f6\u00b7nig", "und", "die", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verwundern sich gar sehre,", "tokens": ["Ver\u00b7wun\u00b7dern", "sich", "gar", "seh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig sieht den Becher an,", "tokens": ["Der", "K\u00f6\u00b7nig", "sieht", "den", "Be\u00b7cher", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Mantel sie ablegt;", "tokens": ["Den", "Man\u00b7tel", "sie", "ab\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da fanden sie dann beyder Sinn,", "tokens": ["Da", "fan\u00b7den", "sie", "dann", "bey\u00b7der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geschrieben hell und here:", "tokens": ["Ge\u00b7schrie\u00b7ben", "hell", "und", "he\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201enur treue Lieb draus trinken kann.\u201c", "tokens": ["\u201e", "nur", "treu\u00b7e", "Lieb", "draus", "trin\u00b7ken", "kann", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "PAV", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201edie Treu den Mantel tr\u00e4gt.\u201c", "tokens": ["\u201e", "die", "Treu", "den", "Man\u00b7tel", "tr\u00e4gt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nigin bracht ein Zwerglein klein,", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "bracht", "ein", "Zwerg\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Bechers Goldgemische,", "tokens": ["Des", "Be\u00b7chers", "Gold\u00b7ge\u00b7mi\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem K\u00f6nig lehrt die Feye sein,", "tokens": ["Dem", "K\u00f6\u00b7nig", "lehrt", "die", "Fe\u00b7ye", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Mantels alten Brauch;", "tokens": ["Des", "Man\u00b7tels", "al\u00b7ten", "Brauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Schimpf soll nun auch allen seyn,", "tokens": ["Der", "Schimpf", "soll", "nun", "auch", "al\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "PIAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Herrn und Fraun am Tische", "tokens": ["Und", "Herrn", "und", "Fraun", "am", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NE", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Versuchten auch den Becher Wein,", "tokens": ["Ver\u00b7such\u00b7ten", "auch", "den", "Be\u00b7cher", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Mantel also auch.", "tokens": ["Den", "Man\u00b7tel", "al\u00b7so", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den Herren wird der Bart so na\u00df,", "tokens": ["Den", "Her\u00b7ren", "wird", "der", "Bart", "so", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mantel Fraun entstellet,", "tokens": ["Der", "Man\u00b7tel", "Fraun", "ent\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis auf die j\u00fcngste Fraue sch\u00f6n,", "tokens": ["Bis", "auf", "die", "j\u00fcngs\u00b7te", "Frau\u00b7e", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem \u00e4ltsten Herrn vertraut,", "tokens": ["Dem", "\u00e4lts\u00b7ten", "Herrn", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem wird der wei\u00dfe Bart nicht na\u00df,", "tokens": ["Dem", "wird", "der", "wei\u00b7\u00dfe", "Bart", "nicht", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mantel leicht gesellet", "tokens": ["Der", "Man\u00b7tel", "leicht", "ge\u00b7sel\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich jedem Bug der Fraue sch\u00f6n,", "tokens": ["Sich", "je\u00b7dem", "Bug", "der", "Frau\u00b7e", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df man treu Lieben schaut.", "tokens": ["Da\u00df", "man", "treu", "Lie\u00b7ben", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Den Becher l\u00e4st der K\u00f6nig gleich", "tokens": ["Den", "Be\u00b7cher", "l\u00e4st", "der", "K\u00f6\u00b7nig", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Ritter voller Treue,", "tokens": ["Dem", "Rit\u00b7ter", "vol\u00b7ler", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die K\u00f6nigin das M\u00e4ntelein,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "das", "M\u00e4n\u00b7tel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fraue, die ihn trug,", "tokens": ["Der", "Frau\u00b7e", ",", "die", "ihn", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum Zwerglein ward der Ritter gleich,", "tokens": ["Zum", "Zwerg\u00b7lein", "ward", "der", "Rit\u00b7ter", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Fr\u00e4ulein wird zur Feye,", "tokens": ["Sein", "Fr\u00e4u\u00b7lein", "wird", "zur", "Fe\u00b7ye", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Becher und den Mantel fein,", "tokens": ["Den", "Be\u00b7cher", "und", "den", "Man\u00b7tel", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie nahmen voller Trug.", "tokens": ["Sie", "nah\u00b7men", "vol\u00b7ler", "Trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie gossen aus den Becher Wein,", "tokens": ["Sie", "gos\u00b7sen", "aus", "den", "Be\u00b7cher", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Tr\u00f6pflein auf den Mantel,", "tokens": ["Ein", "Tr\u00f6pf\u00b7lein", "auf", "den", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und gaben ihn der K\u00f6nigin,", 