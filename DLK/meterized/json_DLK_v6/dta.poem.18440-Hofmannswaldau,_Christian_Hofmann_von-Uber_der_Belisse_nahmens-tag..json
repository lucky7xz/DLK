{"dta.poem.18440": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber der Belisse nahmens-tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich soll Belissen heut im nahmens-tage binden/", "tokens": ["Ich", "soll", "Be\u00b7lis\u00b7sen", "heut", "im", "nah\u00b7mens\u00b7ta\u00b7ge", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rege von ihr selbst verstrickt noch hand noch fu\u00df/", "tokens": ["Und", "re\u00b7ge", "von", "ihr", "selbst", "ver\u00b7strickt", "noch", "hand", "noch", "fu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPR", "PPER", "ADV", "VVFIN", "ADV", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie will vielleicht ich soll im geist ein band er finden/", "tokens": ["Sie", "will", "viel\u00b7leicht", "ich", "soll", "im", "geist", "ein", "band", "er", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "VMFIN", "APPRART", "NN", "ART", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das band ist hertz und mund/ die knoten dessen ku\u00df.", "tokens": ["Das", "band", "ist", "hertz", "und", "mund", "/", "die", "kno\u00b7ten", "des\u00b7sen", "ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "NN", "KON", "NN", "$(", "ART", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}