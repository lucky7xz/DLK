{"textgrid.poem.31560": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe den Herren der Ehren empfangen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe den Herren der Ehren empfangen /", "tokens": ["Ich", "ha\u00b7be", "den", "Her\u00b7ren", "der", "Eh\u00b7ren", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "den Heiligen / Heiligen / Heiligen Christ.", "tokens": ["den", "Hei\u00b7li\u00b7gen", "/", "Hei\u00b7li\u00b7gen", "/", "Hei\u00b7li\u00b7gen", "Christ", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Himmel Vmspanner / in mir jetzund ist.", "tokens": ["Der", "Him\u00b7mel", "Vmspan\u00b7ner", "/", "in", "mir", "je\u00b7tzund", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "APPR", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Himmel Reich ist jetzt in mich eingegangen.", "tokens": ["Das", "Him\u00b7mel", "Reich", "ist", "jetzt", "in", "mich", "ein\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Mund will ihn fassen / die K\u00e4hl' ihn umfangen.", "tokens": ["Der", "Mund", "will", "ihn", "fas\u00b7sen", "/", "die", "K\u00e4hl'", "ihn", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$(", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die Seele mit Jubel und Jauchzen ihn k\u00fcsst.", "tokens": ["Die", "See\u00b7le", "mit", "Ju\u00b7bel", "und", "Jauch\u00b7zen", "ihn", "k\u00fcsst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "das Herze mit frohem Frolocken ihn gr\u00fcst.", "tokens": ["das", "Her\u00b7ze", "mit", "fro\u00b7hem", "Fro\u00b7lo\u00b7cken", "ihn", "gr\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Die Geister / als Meister des Herzens / hoch prangen", "tokens": ["Die", "Geis\u00b7ter", "/", "als", "Meis\u00b7ter", "des", "Her\u00b7zens", "/", "hoch", "pran\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "KOUS", "NN", "ART", "NN", "$(", "ADJD", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Vngreifflichkeit f\u00fchlt' ich anjetzo im Mund.", "tokens": ["Vn\u00b7greif\u00b7flich\u00b7keit", "f\u00fchlt'", "ich", "an\u00b7je\u00b7tzo", "im", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Die alles-erf\u00fcllende G\u00f6ttlichkeit schwebet", "tokens": ["Die", "al\u00b7les\u00b7er\u00b7f\u00fcl\u00b7len\u00b7de", "G\u00f6tt\u00b7lich\u00b7keit", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "mir jetz' auf der Zungen / die sonsten nicht kund", "tokens": ["mir", "jetz'", "auf", "der", "Zun\u00b7gen", "/", "die", "sons\u00b7ten", "nicht", "kund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$(", "ART", "ADJA", "PTKNEG", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "(dieweil sie im Englischen Jubel-Thron lebet / )", "tokens": ["(", "die\u00b7weil", "sie", "im", "Eng\u00b7li\u00b7schen", "Ju\u00b7bel\u00b7Thron", "le\u00b7bet", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$(", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "begreiffen das Irdisch und Himmlische Rund.", "tokens": ["be\u00b7greif\u00b7fen", "das", "Ir\u00b7disch", "und", "Himm\u00b7li\u00b7sche", "Rund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.14": {"text": "nun Glori / Lob' / Ehr' / Prei\u00df und Glauben ihm gebet!", "tokens": ["nun", "Glo\u00b7ri", "/", "Lob'", "/", "Ehr'", "/", "Prei\u00df", "und", "Glau\u00b7ben", "ihm", "ge\u00b7bet", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}