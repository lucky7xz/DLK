{"textgrid.poem.28475": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das b\u00f6se, wol gestellt, la\u00df stehen, wie es steht;", "tokens": ["Das", "b\u00f6\u00b7se", ",", "wol", "ge\u00b7stellt", ",", "la\u00df", "ste\u00b7hen", ",", "wie", "es", "steht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "VVPP", "$,", "VVIMP", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist noch ungewi\u00df, wie neues abegeht.", "tokens": ["Es", "ist", "noch", "un\u00b7ge\u00b7wi\u00df", ",", "wie", "neu\u00b7es", "a\u00b7be\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}