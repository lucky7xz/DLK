{"dta.poem.5924": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem, da\u00df diese nun bewegt, und sich gesellen,", "tokens": ["Nach\u00b7dem", ",", "da\u00df", "die\u00b7se", "nun", "be\u00b7wegt", ",", "und", "sich", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PDS", "ADV", "VVFIN", "$,", "KON", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So machen sie, auf gantz verschiedne Art,", "tokens": ["So", "ma\u00b7chen", "sie", ",", "auf", "gantz", "ver\u00b7schied\u00b7ne", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald C\u00f6rper, welche hart, bald andre, welche zart.", "tokens": ["Bald", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "hart", ",", "bald", "and\u00b7re", ",", "wel\u00b7che", "zart", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PRELS", "ADJD", "$,", "ADV", "PIS", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}