{"textgrid.poem.26671": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin ein Mann! \u2013 wer ist es mehr?", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Mann! \u2013 wer ist es mehr?", "tokens": ["Ich", "bin", "ein", "Mann", "!", "\u2013", "wer", "ist", "es", "mehr", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$(", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wers sagen kann, der springe", "tokens": ["Wers", "sa\u00b7gen", "kann", ",", "der", "sprin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frei unter Gottes Sonn einher", "tokens": ["Frei", "un\u00b7ter", "Got\u00b7tes", "Sonn", "ein\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "NN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und h\u00fcpfe hoch und singe!", "tokens": ["Und", "h\u00fcp\u00b7fe", "hoch", "und", "sin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu Gottes sch\u00f6nem Ebenbild", "tokens": ["Zu", "Got\u00b7tes", "sch\u00f6\u00b7nem", "E\u00b7ben\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann ich den ", "tokens": ["Kann", "ich", "den"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Zum Born, woraus der Himmel quillt,", "tokens": ["Zum", "Born", ",", "wo\u00b7raus", "der", "Him\u00b7mel", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darf ich hinunter steigen.", "tokens": ["Darf", "ich", "hin\u00b7un\u00b7ter", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wohl mir, da\u00df ichs darf und kann!", "tokens": ["Und", "wohl", "mir", ",", "da\u00df", "ichs", "darf", "und", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "KOUS", "PIS", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gehts M\u00e4dchen mir vor\u00fcber,", "tokens": ["Gehts", "M\u00e4d\u00b7chen", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rufts laut in mir: Du bist ein Mann!", "tokens": ["Rufts", "laut", "in", "mir", ":", "Du", "bist", "ein", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fcsse sie so lieber.", "tokens": ["Und", "k\u00fcs\u00b7se", "sie", "so", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und r\u00f6ter wird das M\u00e4dchen dann,", "tokens": ["Und", "r\u00f6\u00b7ter", "wird", "das", "M\u00e4d\u00b7chen", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und 's Mieder wird ihr enge \u2013", "tokens": ["Und", "'s", "Mie\u00b7der", "wird", "ihr", "en\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VAFIN", "PPOSAT", "ADJA", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Das M\u00e4dchen wei\u00dft, ich bin ein Mann,", "tokens": ["Das", "M\u00e4d\u00b7chen", "wei\u00dft", ",", "ich", "bin", "ein", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum wird ihr 's Mieder enge.", "tokens": ["Drum", "wird", "ihr", "'s", "Mie\u00b7der", "en\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "NN", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie wird sie erst um Gnade schrein,", "tokens": ["Wie", "wird", "sie", "erst", "um", "Gna\u00b7de", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ertapp ich sie im Bade?", "tokens": ["Er\u00b7tapp", "ich", "sie", "im", "Ba\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin ein Mann, das f\u00e4llt ihr ein,", "tokens": ["Ich", "bin", "ein", "Mann", ",", "das", "f\u00e4llt", "ihr", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie schrie sie sonst um Gnade?", "tokens": ["Wie", "schrie", "sie", "sonst", "um", "Gna\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich bin ein Mann, mit diesem Wort,", "tokens": ["Ich", "bin", "ein", "Mann", ",", "mit", "die\u00b7sem", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begegn' ich ihr alleine,", "tokens": ["Be\u00b7gegn'", "ich", "ihr", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jag ich des Kaisers Tochter fort,", "tokens": ["Jag", "ich", "des", "Kai\u00b7sers", "Toch\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lumpicht ich erscheine.", "tokens": ["So", "lum\u00b7picht", "ich", "er\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und dieses goldne W\u00f6rtchen macht", "tokens": ["Und", "die\u00b7ses", "gold\u00b7ne", "W\u00f6rt\u00b7chen", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir manche F\u00fcrstin holde,", "tokens": ["Mir", "man\u00b7che", "F\u00fcrs\u00b7tin", "hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich ruft sie \u2013 habt indessen Wacht,", "tokens": ["Mich", "ruft", "sie", "\u2013", "habt", "in\u00b7des\u00b7sen", "Wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Buben dort im Golde!", "tokens": ["Ihr", "Bu\u00b7ben", "dort", "im", "Gol\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich bin ein Mann, das k\u00f6nnt ihr schon", "tokens": ["Ich", "bin", "ein", "Mann", ",", "das", "k\u00f6nnt", "ihr", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meiner Leier riechen,", "tokens": ["An", "mei\u00b7ner", "Lei\u00b7er", "rie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie donnert wie im Sturm davon,", "tokens": ["Sie", "don\u00b7nert", "wie", "im", "Sturm", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst w\u00fcrde sie ja kriechen.", "tokens": ["Sonst", "w\u00fcr\u00b7de", "sie", "ja", "krie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zum Feuergeist im R\u00fcckenmark", "tokens": ["Zum", "Feu\u00b7er\u00b7geist", "im", "R\u00fc\u00b7cken\u00b7mark"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt meine Mannheit: Bruder.", "tokens": ["Sagt", "mei\u00b7ne", "Mann\u00b7heit", ":", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und herrschen beide l\u00f6wenstark", "tokens": ["Und", "herr\u00b7schen", "bei\u00b7de", "l\u00f6\u00b7wen\u00b7stark"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umarmend an dem Ruder.", "tokens": ["Um\u00b7ar\u00b7mend", "an", "dem", "Ru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Aus eben diesem Sch\u00f6pferflu\u00df,", "tokens": ["Aus", "e\u00b7ben", "die\u00b7sem", "Sch\u00f6p\u00b7fer\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woraus wir Menschen sprudeln;", "tokens": ["Wo\u00b7raus", "wir", "Men\u00b7schen", "spru\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Quillt G\u00f6tterkraft und Genius,", "tokens": ["Quillt", "G\u00f6t\u00b7ter\u00b7kraft", "und", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Nur leere Pfeifen dudeln.", "tokens": ["Nur", "lee\u00b7re", "Pfei\u00b7fen", "du\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Tyrannen ha\u00dft mein ", "tokens": ["Ty\u00b7ran\u00b7nen", "ha\u00dft", "mein"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und schmettert sie zu Boden,", "tokens": ["Und", "schmet\u00b7tert", "sie", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kann ers nicht, f\u00fchrt er die Bahn", "tokens": ["Und", "kann", "ers", "nicht", ",", "f\u00fchrt", "er", "die", "Bahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "PTKNEG", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Freiwillig zu den Toten.", "tokens": ["Frei\u00b7wil\u00b7lig", "zu", "den", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Pompejen hat mein Talisman", "tokens": ["Pom\u00b7pe\u00b7jen", "hat", "mein", "Ta\u00b7lis\u00b7man"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei Pharsalus bezwungen,", "tokens": ["Bei", "Phar\u00b7sa\u00b7lus", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Roms Woll\u00fcstlinge Mann f\u00fcr Mann", "tokens": ["Roms", "Wol\u00b7l\u00fcst\u00b7lin\u00b7ge", "Mann", "f\u00fcr", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf teutschen Sand gerungen.", "tokens": ["Auf", "teut\u00b7schen", "Sand", "ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Saht ihr den R\u00f6mer stolz und kraus", "tokens": ["Saht", "ihr", "den", "R\u00f6\u00b7mer", "stolz", "und", "kraus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Afrika dort sitzen?", "tokens": ["In", "Af\u00b7ri\u00b7ka", "dort", "sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Aug speit Feuerflammen aus", "tokens": ["Sein", "Aug", "speit", "Feu\u00b7er\u00b7flam\u00b7men", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als s\u00e4ht ihr Hekla blitzen.", "tokens": ["Als", "s\u00e4ht", "ihr", "He\u00b7kla", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da kommt ein Bube wohlgemut,", "tokens": ["Da", "kommt", "ein", "Bu\u00b7be", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gibt manches zu verstehen \u2013", "tokens": ["Gibt", "man\u00b7ches", "zu", "ver\u00b7ste\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbsprich, du h\u00e4ttst auf Karthagos Schutt", "tokens": ["\u00bb", "sprich", ",", "du", "h\u00e4ttst", "auf", "Kar\u00b7tha\u00b7gos", "Schutt"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "PPER", "VAFIN", "APPR", "NE", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Den Marius gesehen!\u00ab \u2013", "tokens": ["Den", "Ma\u00b7rius", "ge\u00b7se\u00b7hen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NE", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "So spricht der stolze R\u00f6mersmann,", "tokens": ["So", "spricht", "der", "stol\u00b7ze", "R\u00f6\u00b7mers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bub t\u00e4t f\u00fcrba\u00df eilen;", "tokens": ["Der", "Bub", "t\u00e4t", "f\u00fcr\u00b7ba\u00df", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "$."], "me