"], "pos": ["KON", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn l\u00fcstern Mund und Augenpaar", "tokens": ["Wenn", "l\u00fcs\u00b7tern", "Mund", "und", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Lustgenusse laden,", "tokens": ["Zum", "Lust\u00b7ge\u00b7nus\u00b7se", "la\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Und zehenmal das Halstuch f\u00e4llt,", "tokens": ["Und", "ze\u00b7hen\u00b7mal", "das", "Hal\u00b7stuch", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und aus den losen Schlingen,", "tokens": ["Und", "aus", "den", "lo\u00b7sen", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Halbkugeln einer bessern Welt,", "tokens": ["Halb\u00b7ku\u00b7geln", "ei\u00b7ner", "bes\u00b7sern", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die vollen Br\u00fcste springen, \u2013", "tokens": ["Die", "vol\u00b7len", "Br\u00fcs\u00b7te", "sprin\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "F\u00fchrt gar der h\u00f6llsche Schadenfroh", "tokens": ["F\u00fchrt", "gar", "der", "h\u00f6ll\u00b7sche", "Scha\u00b