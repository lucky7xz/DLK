{"textgrid.poem.35936": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was willst du noch, du zartes Kind, hienieden?", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was willst du noch, du zartes Kind, hienieden?", "tokens": ["Was", "willst", "du", "noch", ",", "du", "zar\u00b7tes", "Kind", ",", "hien\u00b7ie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lenz ist schon zu sch\u00f6n'rer Flur entr\u00fcckt.", "tokens": ["Der", "Lenz", "ist", "schon", "zu", "sch\u00f6n'\u00b7rer", "Flur", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann sind die bunten Schwestern auch geschieden,", "tokens": ["Dann", "sind", "die", "bun\u00b7ten", "Schwes\u00b7tern", "auch", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein fr\u00fcher Herbst hat sie im Keim geknickt.", "tokens": ["Ein", "fr\u00fc\u00b7her", "Herbst", "hat", "sie", "im", "Keim", "ge\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbich stand so einsam an der kalten Mauer,", "tokens": ["\u00bb", "ich", "stand", "so", "ein\u00b7sam", "an", "der", "kal\u00b7ten", "Mau\u00b7er", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von allen Freuden war ich fern gebannt,", "tokens": ["Von", "al\u00b7len", "Freu\u00b7den", "war", "ich", "fern", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und um mich her war Schatten nur und Trauer;", "tokens": ["Und", "um", "mich", "her", "war", "Schat\u00b7ten", "nur", "und", "Trau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APZR", "VAFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Dornen viel und Steine tr\u00e4gt das Land.", "tokens": ["Denn", "Dor\u00b7nen", "viel", "und", "Stei\u00b7ne", "tr\u00e4gt", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbda wu\u00dft' ich nicht in meinem \u00f6den Thale,", "tokens": ["\u00bb", "da", "wu\u00dft'", "ich", "nicht", "in", "mei\u00b7nem", "\u00f6\u00b7den", "Tha\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df auf der Flur die Maiensonne lacht,", "tokens": ["Da\u00df", "auf", "der", "Flur", "die", "Mai\u00b7en\u00b7son\u00b7ne", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis mir in ", "tokens": ["Bis", "mir", "in"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ein Rosenleben in der Brust erwacht.", "tokens": ["Ein", "Ro\u00b7sen\u00b7le\u00b7ben", "in", "der", "Brust", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbnun ", "tokens": ["\u00bb", "nun"], "token_info": ["punct", "word"], "pos": ["$(", "ADV"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Es brach so morgenroth aus lichtem Gr\u00fcn;", "tokens": ["Es", "brach", "so", "mor\u00b7gen\u00b7roth", "aus", "lich\u00b7tem", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich konnte nicht die Bl\u00e4ttlein l\u00e4nger halten,", "tokens": ["Ich", "konn\u00b7te", "nicht", "die", "Bl\u00e4tt\u00b7lein", "l\u00e4n\u00b7ger", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie wollten all' dem Licht entgegen bl\u00fchn.", "tokens": ["Sie", "woll\u00b7ten", "all'", "dem", "Licht", "ent\u00b7ge\u00b7gen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbnun wollt' ich recht in meiner F\u00fclle prangen", "tokens": ["\u00bb", "nun", "wollt'", "ich", "recht", "in", "mei\u00b7ner", "F\u00fcl\u00b7le", "pran\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sah umher, und suchte nun mein Licht;", "tokens": ["Und", "sah", "um\u00b7her", ",", "und", "such\u00b7te", "nun", "mein", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da war die milde Sonne weggegangen \u2013", "tokens": ["Da", "war", "die", "mil\u00b7de", "Son\u00b7ne", "weg\u00b7ge\u00b7gan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein rauher Wind fuhr um mein Angesicht.", "tokens": ["Ein", "rau\u00b7her", "Wind", "fuhr", "um", "mein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbda hab' ich fest mich wieder eingeschlossen", "tokens": ["\u00bb", "da", "hab'", "ich", "fest", "mich", "wie\u00b7der", "ein\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und habe still im Herzen fortgebl\u00fcht", "tokens": ["Und", "ha\u00b7be", "still", "im", "Her\u00b7zen", "fort\u00b7ge\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und um mich her ist linder Thau geflossen,", "tokens": ["Und", "um", "mich", "her", "ist", "lin\u00b7der", "Thau", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APZR", "VAFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sonst w\u00e4r' ich wohl erstorben und vergl\u00fcht.", "tokens": ["Sonst", "w\u00e4r'", "ich", "wohl", "ers\u00b7tor\u00b7ben", "und", "ver\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbnun will ich mich noch einmal hold erschlie\u00dfen,", "tokens": ["\u00bb", "nun", "will", "ich", "mich", "noch", "ein\u00b7mal", "hold", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der milden Abendsonne noch mich freu'n,", "tokens": ["Der", "mil\u00b7den", "A\u00b7bend\u00b7son\u00b7ne", "noch", "mich", "freu'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will einmal noch den s\u00fc\u00dfen Schimmer gr\u00fc\u00dfen,", "tokens": ["Will", "ein\u00b7mal", "noch", "den", "s\u00fc\u00b7\u00dfen", "Schim\u00b7mer", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann still die welken Bl\u00e4ttlein nieder streu'n.", "tokens": ["Dann", "still", "die", "wel\u00b7ken", "Bl\u00e4tt\u00b7lein", "nie\u00b7der", "streu'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbade! du mu\u00dft noch andern Fluren scheinen;", "tokens": ["\u00bb", "a\u00b7de", "!", "du", "mu\u00dft", "noch", "an\u00b7dern", "Flu\u00b7ren", "schei\u00b7nen", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "PPER", "VMFIN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mich hat ein fr\u00fcher Nord schon abgeknickt.", "tokens": ["Mich", "hat", "ein", "fr\u00fc\u00b7her", "Nord", "schon", "ab\u00b7ge\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ade! ein Fr\u00fchling wird uns einst vereinen,", "tokens": ["A\u00b7de", "!", "ein", "Fr\u00fch\u00b7ling", "wird", "uns", "einst", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ein Morgen, der uns ewiglich begl\u00fcckt.\u00ab", "tokens": ["Ein", "Mor\u00b7gen", ",", "der", "uns", "e\u00b7wig\u00b7lich", "be\u00b7gl\u00fcckt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}