{"textgrid.poem.41939": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Nebel wogt mit wandelbarem Walten,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nebel wogt mit wandelbarem Walten,", "tokens": ["Der", "Ne\u00b7bel", "wogt", "mit", "wan\u00b7del\u00b7ba\u00b7rem", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt dicht verwebt, vom Winde jetzt zerstreut.", "tokens": ["Jetzt", "dicht", "ver\u00b7webt", ",", "vom", "Win\u00b7de", "jetzt", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stets wechseln Berg und Thal ihr luftig Kleid,", "tokens": ["Stets", "wech\u00b7seln", "Berg", "und", "Thal", "ihr", "luf\u00b7tig", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und formlos ruhn im Schleyer die Gestalten.", "tokens": ["Und", "form\u00b7los", "ruhn", "im", "Schle\u00b7yer", "die", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wird's freundlich oder feindlich sich entfalten?", "tokens": ["Wird's", "freund\u00b7lich", "o\u00b7der", "feind\u00b7lich", "sich", "ent\u00b7fal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch wei\u00df ich's nicht; es schwankt von Lust zu Leid,", "tokens": ["Noch", "wei\u00df", "ich's", "nicht", ";", "es", "schwankt", "von", "Lust", "zu", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Nacht zu Licht mein Herz im ewgen Streit", "tokens": ["Von", "Nacht", "zu", "Licht", "mein", "Herz", "im", "ew\u00b7gen", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und will umsonst die fl\u00fccht'gen Bilder halten.", "tokens": ["Und", "will", "um\u00b7sonst", "die", "fl\u00fccht'\u00b7gen", "Bil\u00b7der", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch schon zerrinnt die rege Zauberwelt;", "tokens": ["Doch", "schon", "zer\u00b7rinnt", "die", "re\u00b7ge", "Zau\u00b7ber\u00b7welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon zeigt der Berge Stirn sich minder tr\u00fcbe,", "tokens": ["Schon", "zeigt", "der", "Ber\u00b7ge", "Stirn", "sich", "min\u00b7der", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PRF", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon l\u00e4\u00dft die Flur im bunten Glanz sich schauen.", "tokens": ["Schon", "l\u00e4\u00dft", "die", "Flur", "im", "bun\u00b7ten", "Glanz", "sich", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl starrt vom n\u00e4cht'gen Reif das weite Feld;", "tokens": ["Wohl", "starrt", "vom", "n\u00e4cht'\u00b7gen", "Reif", "das", "wei\u00b7te", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch freundlich steigt der Sonnenstrahl der Liebe", "tokens": ["Doch", "freund\u00b7lich", "steigt", "der", "Son\u00b7nen\u00b7strahl", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Himmel auf und w\u00e4rmt die kalten Auen.", "tokens": ["Am", "Him\u00b7mel", "auf", "und", "w\u00e4rmt", "die", "kal\u00b7ten", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}