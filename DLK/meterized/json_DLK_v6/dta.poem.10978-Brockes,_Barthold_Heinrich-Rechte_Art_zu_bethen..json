{"dta.poem.10978": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Rechte Art zu bethen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da alle K\u00f6rper, Geister, Umst\u00e4nd und Zuf\u00e4ll, hier auf\ndieser Welt,", "tokens": ["Da", "al\u00b7le", "K\u00f6r\u00b7per", ",", "Geis\u00b7ter", ",", "Um\u00b7st\u00e4nd", "und", "Zu\u00b7f\u00e4ll", ",", "hier", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-++--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Da alles das, was die Natur, im Schoo\u00df des tiefen Raums,", "tokens": ["Da", "al\u00b7les", "das", ",", "was", "die", "Na\u00b7tur", ",", "im", "Schoo\u00df", "des", "tie\u00b7fen", "Raums", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "$,", "PRELS", "ART", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ein\u2019 ewge Weisheit nicht allein zum Ursprung hat; nein, auch", "tokens": ["Ein'", "ew\u00b7ge", "Weis\u00b7heit", "nicht", "al\u00b7lein", "zum", "Ur\u00b7sprung", "hat", ";", "nein", ",", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "$.", "PTKANT", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Als die nicht aufgeh\u00f6rt, in einer ununterbrochnen Harmonie,", "tokens": ["Als", "die", "nicht", "auf\u00b7ge\u00b7h\u00f6rt", ",", "in", "ei\u00b7ner", "un\u00b7un\u00b7ter\u00b7broch\u00b7nen", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PTKNEG", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Gelenkt, geleitet und regieret,", "tokens": ["Ge\u00b7lenkt", ",", "ge\u00b7lei\u00b7tet", "und", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In majest\u00e4tisch weiser Ordnung, zum besten Endzweck stets", "tokens": ["In", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "wei\u00b7ser", "Ord\u00b7nung", ",", "zum", "bes\u00b7ten", "End\u00b7zweck", "stets"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+----+-+-+", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Und immer unterhalten wird, der freye Will auch dieser F\u00fchrung,", "tokens": ["Und", "im\u00b7mer", "un\u00b7ter\u00b7hal\u00b7ten", "wird", ",", "der", "frey\u00b7e", "Will", "auch", "die\u00b7ser", "F\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,", "ART", "ADJA", "VMFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "Und der allgegenw\u00e4rtgen Gottheit allgegenw\u00e4rtigen Regierung", "tokens": ["Und", "der", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "Got\u00b7theit", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gen", "Re\u00b7gie\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "--+--+--+-+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "So wenig uns entziehen kann; da\u00df wir vielmehr, da\u00df Gott", "tokens": ["So", "we\u00b7nig", "uns", "ent\u00b7zie\u00b7hen", "kann", ";", "da\u00df", "wir", "viel\u00b7mehr", ",", "da\u00df", "Gott"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIS", "PRF", "VVINF", "VMFIN", "$.", "KOUS", "PPER", "ADV", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "So wohl das Wollen, als Vollbringen, uns geben mu\u00df, be-", "tokens": ["So", "wohl", "das", "Wol\u00b7len", ",", "als", "Voll\u00b7brin\u00b7gen", ",", "uns", "ge\u00b7ben", "mu\u00df", ",", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KOUS", "NN", "$,", "PPER", "VVINF", "VMFIN", "$,", "TRUNC"], "meter": "-+-+-+++--+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Auch in der heilgen Bibel selbst: So mu\u00df auch billig unser Bethen,", "tokens": ["Auch", "in", "der", "heil\u00b7gen", "Bi\u00b7bel", "selbst", ":", "So", "mu\u00df", "auch", "bil\u00b7lig", "un\u00b7ser", "Be\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV", "$.", "ADV", "VMFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.12": {"text": "Wenn wir, vor Gottes Majest\u00e4t, in Ehrfurcht voller Andacht", "tokens": ["Wenn", "wir", ",", "vor", "Got\u00b7tes", "Ma\u00b7jes\u00b7t\u00e4t", ",", "in", "Ehr\u00b7furcht", "vol\u00b7ler", "An\u00b7dacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "NN", "$,", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Mit Ernst dahin gerichtet seyn, da\u00df die allweise, m\u00e4chtge Liebe,", "tokens": ["Mit", "Ernst", "da\u00b7hin", "ge\u00b7rich\u00b7tet", "seyn", ",", "da\u00df", "die", "all\u00b7wei\u00b7se", ",", "m\u00e4cht\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "PAV", "VVPP", "VAINF", "$,", "KOUS", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.14": {"text": "So wohl die Umst\u00e4nd und die Zuf\u00e4ll, als unsrer Seelen", "tokens": ["So", "wohl", "die", "Um\u00b7st\u00e4nd", "und", "die", "Zu\u00b7f\u00e4ll", ",", "als", "uns\u00b7rer", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Zu seiner Ehr und unserm Besten, aus Gnaden selber lenken", "tokens": ["Zu", "sei\u00b7ner", "Ehr", "und", "un\u00b7serm", "Bes\u00b7ten", ",", "aus", "Gna\u00b7den", "sel\u00b7ber", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Weil sonst, mit unsrer eignen Schw\u00e4che, selbst gegen uns nicht", "tokens": ["Weil", "sonst", ",", "mit", "uns\u00b7rer", "eig\u00b7nen", "Schw\u00e4\u00b7che", ",", "selbst", "ge\u00b7gen", "uns", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "APPR", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "Zugleich auch gegen Zuf\u00e4ll, Umst\u00e4nd, als Noth, Exempel", "tokens": ["Zu\u00b7gleich", "auch", "ge\u00b7gen", "Zu\u00b7f\u00e4ll", ",", "Um\u00b7st\u00e4nd", ",", "als", "Noth", ",", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "NN", "$,", "KOUS", "NN", "$,", "NN"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Zu k\u00e4mpfen uns nur gar zu schwer, ja ganz unm\u00f6glich, w\u00fcrde seyn.", "tokens": ["Zu", "k\u00e4mp\u00b7fen", "uns", "nur", "gar", "zu", "schwer", ",", "ja", "ganz", "un\u00b7m\u00f6g\u00b7lich", ",", "w\u00fcr\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "ADV", "ADV", "PTKA", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}