{"dta.poem.12080": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Traum, darein man leicht bei tr\u00e4ger Ruh versinkt,", "tokens": ["Der", "Traum", ",", "da\u00b7rein", "man", "leicht", "bei", "tr\u00e4\u00b7ger", "Ruh", "ver\u00b7sinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PIS", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin man dichtet, denkt, sieht, h\u00f6rt, spricht, i\u00dft und trinkt,", "tokens": ["Da\u00b7rin", "man", "dich\u00b7tet", ",", "denkt", ",", "sieht", ",", "h\u00f6rt", ",", "spricht", ",", "i\u00dft", "und", "trinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Darin spazieren geht im abgeme\u00dfnen Raum;", "tokens": ["Da\u00b7rin", "spa\u00b7zie\u00b7ren", "geht", "im", "ab\u00b7ge\u00b7me\u00df\u00b7nen", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin man wacht und schl\u00e4ft, und tr\u00e4umt im wachen Traum:", "tokens": ["Da\u00b7rin", "man", "wacht", "und", "schl\u00e4ft", ",", "und", "tr\u00e4umt", "im", "wa\u00b7chen", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn gr\u00fcndlich du daraus erwachen willst, la\u00df r\u00fctteln", "tokens": ["Wenn", "gr\u00fcnd\u00b7lich", "du", "da\u00b7raus", "er\u00b7wa\u00b7chen", "willst", ",", "la\u00df", "r\u00fct\u00b7teln"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "PAV", "VVINF", "VMFIN", "$,", "VVIMP", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Reisewagen dich, von Reisesorgen sch\u00fctteln.", "tokens": ["Vom", "Rei\u00b7se\u00b7wa\u00b7gen", "dich", ",", "von", "Rei\u00b7se\u00b7sor\u00b7gen", "sch\u00fct\u00b7teln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du mu\u00dft im fremden Land die Augen offen haben,", "tokens": ["Du", "mu\u00dft", "im", "frem\u00b7den", "Land", "die", "Au\u00b7gen", "of\u00b7fen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst stolperst du und f\u00e4llst in jeden Stra\u00dfengraben.", "tokens": ["Sonst", "stol\u00b7perst", "du", "und", "f\u00e4llst", "in", "je\u00b7den", "Stra\u00b7\u00dfen\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}