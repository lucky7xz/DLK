{"textgrid.poem.39500": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Sternen hat die Vorwelt uns gelehret,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Sternen hat die Vorwelt uns gelehret,", "tokens": ["Von", "Ster\u00b7nen", "hat", "die", "Vor\u00b7welt", "uns", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tugend, Frevel, Ruhm, Schmach, Gl\u00fcck, Mi\u00dflingen,", "tokens": ["Die", "Tu\u00b7gend", ",", "Fre\u00b7vel", ",", "Ruhm", ",", "Schmach", ",", "Gl\u00fcck", ",", "Mi\u00df\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus den geheimni\u00dfvollen Krei\u00dfen bringen,", "tokens": ["Aus", "den", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7len", "Krei\u00b7\u00dfen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deren Macht kein Wille sich erwehret;", "tokens": ["Und", "de\u00b7ren", "Macht", "kein", "Wil\u00b7le", "sich", "er\u00b7weh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Zeichen, die der Kund'ge sieht und h\u00f6ret,", "tokens": ["Von", "Zei\u00b7chen", ",", "die", "der", "Kun\u00b7d'\u00b7ge", "sieht", "und", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und den Orakeln leichter Vogelschwingen;", "tokens": ["Und", "den", "O\u00b7ra\u00b7keln", "leich\u00b7ter", "Vo\u00b7gel\u00b7schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch von Sirenen, deren zaubernd Singen", "tokens": ["Auch", "von", "Si\u00b7re\u00b7nen", ",", "de\u00b7ren", "zau\u00b7bernd", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "PRELAT", "ADJD", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Unwiderstehlich in den Tod beth\u00f6ret.", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7lich", "in", "den", "Tod", "be\u00b7th\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Jugend ziemt des Sinnbilds holde Leitung,", "tokens": ["Der", "Ju\u00b7gend", "ziemt", "des", "Sinn\u00b7bilds", "hol\u00b7de", "Lei\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch heb', erwachsen nun, der Mensch die Stirne,", "tokens": ["Doch", "heb'", ",", "er\u00b7wach\u00b7sen", "nun", ",", "der", "Mensch", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ADV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6r auf, was in ihm, au\u00dfer sich zu w\u00e4hnen.", "tokens": ["H\u00f6r", "auf", ",", "was", "in", "ihm", ",", "au\u00b7\u00dfer", "sich", "zu", "w\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "$,", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Muth, Freiheit, Kraft, sind seines Heils Gestirne,", "tokens": ["Muth", ",", "Frei\u00b7heit", ",", "Kraft", ",", "sind", "sei\u00b7nes", "Heils", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Weisheit Blicke seiner Zukunft Deutung,", "tokens": ["Der", "Weis\u00b7heit", "Bli\u00b7cke", "sei\u00b7ner", "Zu\u00b7kunft", "Deu\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wahn, Tr\u00e4gheit, Wollust, seiner Brust Sirenen.", "tokens": ["Wahn", ",", "Tr\u00e4g\u00b7heit", ",", "Wol\u00b7lust", ",", "sei\u00b7ner", "Brust", "Si\u00b7re\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}