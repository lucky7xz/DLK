{"textgrid.poem.43722": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Johannisw\u00fcrmchen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was tanzen so goldige Sternchen", "tokens": ["Was", "tan\u00b7zen", "so", "gol\u00b7di\u00b7ge", "Stern\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Umher in funkelnder Pracht?", "tokens": ["Um\u00b7her", "in", "fun\u00b7keln\u00b7der", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sind K\u00e4fer mit ihren Laternchen,", "tokens": ["Sind", "K\u00e4\u00b7fer", "mit", "ih\u00b7ren", "La\u00b7tern\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die fliegen spazieren bei Nacht.", "tokens": ["Die", "flie\u00b7gen", "spa\u00b7zie\u00b7ren", "bei", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wenn einer begegnet dem andern,", "tokens": ["Wenn", "ei\u00b7ner", "be\u00b7geg\u00b7net", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dann gr\u00fc\u00dfen sie sich, wie man thut,", "tokens": ["Dann", "gr\u00fc\u00b7\u00dfen", "sie", "sich", ",", "wie", "man", "thut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erz\u00e4hlen sich was und wandern", "tokens": ["Er\u00b7z\u00e4h\u00b7len", "sich", "was", "und", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "PWS", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann weiter wohlgemuth.", "tokens": ["Dann", "wei\u00b7ter", "wohl\u00b7ge\u00b7muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und kehrt der Morgen wieder,", "tokens": ["Und", "kehrt", "der", "Mor\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sucht Jeder eilig sein Haus,", "tokens": ["Sucht", "Je\u00b7der", "ei\u00b7lig", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch eh' er sich leget nieder,", "tokens": ["Doch", "eh'", "er", "sich", "le\u00b7get", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "L\u00f6scht er sein Laternchen aus.", "tokens": ["L\u00f6scht", "er", "sein", "La\u00b7tern\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}