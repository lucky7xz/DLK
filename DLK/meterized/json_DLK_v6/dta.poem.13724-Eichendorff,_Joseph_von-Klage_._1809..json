{"dta.poem.13724": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Klage .  \n 1809.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und d\u00fcrft' von allem nichts sp\u00fcren", "tokens": ["Und", "d\u00fcrft'", "von", "al\u00b7lem", "nichts", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PIS", "PIS", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In dieser dummen Zeit,", "tokens": ["In", "die\u00b7ser", "dum\u00b7men", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sie da unten handthieren,", "tokens": ["Was", "sie", "da", "un\u00b7ten", "handt\u00b7hie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von Gott verlassen, zerstreut;", "tokens": ["Von", "Gott", "ver\u00b7las\u00b7sen", ",", "zer\u00b7streut", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}