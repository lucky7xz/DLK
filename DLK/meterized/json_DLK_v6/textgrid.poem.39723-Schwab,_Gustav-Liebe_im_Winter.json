{"textgrid.poem.39723": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Liebe im Winter", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist o sch\u00f6n, des Winters stille Gegend,", "tokens": ["Sie", "ist", "o", "sch\u00f6n", ",", "des", "Win\u00b7ters", "stil\u00b7le", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "FM", "ADJD", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann rings die Flur im Schnee sich blendend hebt,", "tokens": ["Wann", "rings", "die", "Flur", "im", "Schnee", "sich", "blen\u00b7dend", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPRART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber ihr, den lichten Kreis bewegend,", "tokens": ["Und", "\u00fc\u00b7ber", "ihr", ",", "den", "lich\u00b7ten", "Kreis", "be\u00b7we\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mond mit seinem Sternenheere schwebt:", "tokens": ["Der", "Mond", "mit", "sei\u00b7nem", "Ster\u00b7nen\u00b7hee\u00b7re", "schwebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Wandrer liebt die Fluren zu durchschauen,", "tokens": ["Der", "Wand\u00b7rer", "liebt", "die", "Flu\u00b7ren", "zu", "durch\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht W\u00e4rme sucht er und nicht Fr\u00fchlingsduft,", "tokens": ["Nicht", "W\u00e4r\u00b7me", "sucht", "er", "und", "nicht", "Fr\u00fch\u00b7lings\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihm gn\u00fcgt die Gabe dieser k\u00fchlen Auen,", "tokens": ["Ihm", "gn\u00fcgt", "die", "Ga\u00b7be", "die\u00b7ser", "k\u00fch\u00b7len", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Himmels Stral und eine reine Luft.", "tokens": ["Des", "Him\u00b7mels", "Stral", "und", "ei\u00b7ne", "rei\u00b7ne", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da wandelt mir vor meinen k\u00fchlen Sinnen", "tokens": ["Da", "wan\u00b7delt", "mir", "vor", "mei\u00b7nen", "k\u00fch\u00b7len", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein liebes Bild vor\u00fcber als ein Geist,", "tokens": ["Dein", "lie\u00b7bes", "Bild", "vor\u00b7\u00fc\u00b7ber", "als", "ein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und all mein Wesen wird ein stilles Minnen,", "tokens": ["Und", "all", "mein", "We\u00b7sen", "wird", "ein", "stil\u00b7les", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein leises Lied, das deine G\u00fcte preist.", "tokens": ["Ein", "lei\u00b7ses", "Lied", ",", "das", "dei\u00b7ne", "G\u00fc\u00b7te", "preist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich liebe dich, wie jene goldnen Sterne", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "wie", "je\u00b7ne", "gold\u00b7nen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In ihrem Stral, der ohne Gluten gl\u00e4nzt,", "tokens": ["In", "ih\u00b7rem", "Stral", ",", "der", "oh\u00b7ne", "Glu\u00b7ten", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie jenen Aether, der in dunkler Ferne", "tokens": ["Wie", "je\u00b7nen", "A\u00b7e\u00b7ther", ",", "der", "in", "dunk\u00b7ler", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Mit liebevollem Blau das All umgr\u00e4nzt.", "tokens": ["Mit", "lie\u00b7be\u00b7vol\u00b7lem", "Blau", "das", "All", "um\u00b7gr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es bl\u00fcht nicht \u00fcppig unter deinen F\u00fc\u00dfen", "tokens": ["Es", "bl\u00fcht", "nicht", "\u00fcp\u00b7pig", "un\u00b7ter", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Flur zu einem Blumenwald empor,", "tokens": ["Die", "Flur", "zu", "ei\u00b7nem", "Blu\u00b7men\u00b7wald", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und von den kahlen B\u00e4umen t\u00f6nt kein Gr\u00fc\u00dfen", "tokens": ["Und", "von", "den", "kah\u00b7len", "B\u00e4u\u00b7men", "t\u00f6nt", "kein", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von frohen Vogels\u00e4ngen in dein Ohr.", "tokens": ["Von", "fro\u00b7hen", "Vo\u00b7ge\u00b7ls\u00e4n\u00b7gen", "in", "dein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du bl\u00fchst allein auf diesen weiten Feldern,", "tokens": ["Du", "bl\u00fchst", "al\u00b7lein", "auf", "die\u00b7sen", "wei\u00b7ten", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vom wei\u00dfen Schnee verkl\u00e4ret und versch\u00f6nt,", "tokens": ["Vom", "wei\u00b7\u00dfen", "Schnee", "ver\u00b7kl\u00e4\u00b7ret", "und", "ver\u00b7sch\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Schweigen herrscht im Thal und auf den W\u00e4ldern,", "tokens": ["Ein", "Schwei\u00b7gen", "herrscht", "im", "Thal", "und", "auf", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und deiner Z\u00fcge Harmonie nur t\u00f6nt.", "tokens": ["Und", "dei\u00b7ner", "Z\u00fc\u00b7ge", "Har\u00b7mo\u00b7nie", "nur", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Soll ich des Sch\u00f6nen Lieblichkeit gewahren,", "tokens": ["Soll", "ich", "des", "Sch\u00f6\u00b7nen", "Lieb\u00b7lich\u00b7keit", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So mag es wuchernd unter Blumen bl\u00fchn;", "tokens": ["So", "mag", "es", "wu\u00b7chernd", "un\u00b7ter", "Blu\u00b7men", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch soll sich seine Hoheit offenbaren,", "tokens": ["Doch", "soll", "sich", "sei\u00b7ne", "Ho\u00b7heit", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So mu\u00df es einsam in der Nacht ergl\u00fchn.", "tokens": ["So", "mu\u00df", "es", "ein\u00b7sam", "in", "der", "Nacht", "er\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Will sich die Liebe ganz als F\u00fcrstin zeigen,", "tokens": ["Will", "sich", "die", "Lie\u00b7be", "ganz", "als", "F\u00fcrs\u00b7tin", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So flieht der Lenz, die fremde Blume f\u00e4llt,", "tokens": ["So", "flieht", "der", "Lenz", ",", "die", "frem\u00b7de", "Blu\u00b7me", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Empor aus totem Eise mu\u00df sie steigen,", "tokens": ["Em\u00b7por", "aus", "to\u00b7tem", "Ei\u00b7se", "mu\u00df", "sie", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Blumenbeet, ein Fr\u00fchling, eine Welt.", "tokens": ["Ein", "Blu\u00b7men\u00b7beet", ",", "ein", "Fr\u00fch\u00b7ling", ",", "ei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}