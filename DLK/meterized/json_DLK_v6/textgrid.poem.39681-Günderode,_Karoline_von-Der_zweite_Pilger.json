{"textgrid.poem.39681": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Der zweite Pilger", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich scheide froh vom Vaterland", "tokens": ["Ich", "schei\u00b7de", "froh", "vom", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und suche den geliebten Strand,", "tokens": ["Und", "su\u00b7che", "den", "ge\u00b7lieb\u00b7ten", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Jesus Christus wallte;", "tokens": ["Wo", "Je\u00b7sus", "Chris\u00b7tus", "wall\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo er in Demuth angethan", "tokens": ["Wo", "er", "in", "De\u00b7muth", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Erdenlebens schwere Bahn,", "tokens": ["Des", "Er\u00b7den\u00b7le\u00b7bens", "schwe\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit stillem Sinne wallte.", "tokens": ["Mit", "stil\u00b7lem", "Sin\u00b7ne", "wall\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was ist die Herrlichkeit der Welt", "tokens": ["Was", "ist", "die", "Herr\u00b7lich\u00b7keit", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles, was dem Sinn gef\u00e4llt?", "tokens": ["Und", "al\u00b7les", ",", "was", "dem", "Sinn", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will ihm froh entsagen.", "tokens": ["Ich", "will", "ihm", "froh", "ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die irrdische Kette f\u00e4llt von mir,", "tokens": ["Die", "irr\u00b7di\u00b7sche", "Ket\u00b7te", "f\u00e4llt", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Jesu! nur zu dir! zu dir!", "tokens": ["Und", "Je\u00b7su", "!", "nur", "zu", "dir", "!", "zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "ADV", "APPR", "PPER", "$.", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich mein Sehnen tragen.", "tokens": ["Will", "ich", "mein", "Seh\u00b7nen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die M\u00e4rterkrone winket mir", "tokens": ["Die", "M\u00e4r\u00b7ter\u00b7kro\u00b7ne", "win\u00b7ket", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Seeligkeit wohl f\u00fcr und f\u00fcr,", "tokens": ["Und", "See\u00b7lig\u00b7keit", "wohl", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich vollendet habe.", "tokens": ["Wenn", "ich", "voll\u00b7en\u00b7det", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O s\u00fc\u00dfe Bu\u00dfe! himmlisch Leid!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Bu\u00b7\u00dfe", "!", "himm\u00b7lisch", "Leid", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In frommer Einfalt Seligkeit,", "tokens": ["In", "from\u00b7mer", "Ein\u00b7falt", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr wohnt am heiligen Grabe.", "tokens": ["Ihr", "wohnt", "am", "hei\u00b7li\u00b7gen", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}