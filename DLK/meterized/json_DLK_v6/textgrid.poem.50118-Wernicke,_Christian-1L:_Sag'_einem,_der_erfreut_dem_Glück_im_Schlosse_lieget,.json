{"textgrid.poem.50118": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sag' einem, der erfreut dem Gl\u00fcck im Schlosse lieget,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag' einem, der erfreut dem Gl\u00fcck im Schlosse lieget,", "tokens": ["Sag'", "ei\u00b7nem", ",", "der", "er\u00b7freut", "dem", "Gl\u00fcck", "im", "Schlos\u00b7se", "lie\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dass dessen ", "tokens": ["Dass", "des\u00b7sen"], "token_info": ["word", "word"], "pos": ["KOUS", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dass es uns, ehe wir es recht erkennt, verl\u00e4sst;", "tokens": ["Dass", "es", "uns", ",", "e\u00b7he", "wir", "es", "recht", "er\u00b7kennt", ",", "ver\u00b7l\u00e4sst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er h\u00f6ret dich nicht mehr, den ", "tokens": ["Er", "h\u00f6\u00b7ret", "dich", "nicht", "mehr", ",", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Er spottet deiner Gunst, und lachet deiner Lehren,", "tokens": ["Er", "spot\u00b7tet", "dei\u00b7ner", "Gunst", ",", "und", "la\u00b7chet", "dei\u00b7ner", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und alle deine Wort' entf\u00fchrt der schnelle Wind:", "tokens": ["Und", "al\u00b7le", "dei\u00b7ne", "Wort'", "ent\u00b7f\u00fchrt", "der", "schnel\u00b7le", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}}}}