{"dta.poem.20110": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Die Jahre fliehn; der Spielgeno\u00df", "tokens": ["Die", "Jah\u00b7re", "fliehn", ";", "der", "Spiel\u00b7ge\u00b7no\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Hirsch und Reh, von Quell und Wind,", "tokens": ["Von", "Hirsch", "und", "Reh", ",", "von", "Quell", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist jetzt, auf seines Retters Schlo\u00df,", "tokens": ["Ist", "jetzt", ",", "auf", "sei\u00b7nes", "Ret\u00b7ters", "Schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des kinderlosen Grafen Kind; \u2014", "tokens": ["Des", "kin\u00b7der\u00b7lo\u00b7sen", "Gra\u00b7fen", "Kind", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sch\u00f6n, und erkoren Lieb\u2019 und Land", "tokens": ["Sch\u00f6n", ",", "und", "er\u00b7ko\u00b7ren", "Lieb'", "und", "Land"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Des alten Grafen einst zu erben,", "tokens": ["Des", "al\u00b7ten", "Gra\u00b7fen", "einst", "zu", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sieht man um Sittah\u2019s Herz und Hand", "tokens": ["Sieht", "man", "um", "Sit\u00b7tah's", "Herz", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "NE", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Landes stolzen Adel werben.", "tokens": ["Des", "Lan\u00b7des", "stol\u00b7zen", "A\u00b7del", "wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}