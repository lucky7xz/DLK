{"dta.poem.6367": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dieser Wahrheit scheint recht \u00fcberein zu kommen,", "tokens": ["Mit", "die\u00b7ser", "Wahr\u00b7heit", "scheint", "recht", "\u00fc\u00b7be\u00b7re\u00b7in", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was uns Aesopus dort in kluger Fabel sagt:", "tokens": ["Was", "uns", "A\u00b7e\u00b7so\u00b7pus", "dort", "in", "klu\u00b7ger", "Fa\u00b7bel", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NE", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--++-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Von einem Wald-Gott ward, aus Mitleid, aufgenommen", "tokens": ["Von", "ei\u00b7nem", "Wald\u00b7Gott", "ward", ",", "aus", "Mit\u00b7leid", ",", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "$,", "APPR", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Reisender, den Frost und Hunger plagt:", "tokens": ["Ein", "Rei\u00b7sen\u00b7der", ",", "den", "Frost", "und", "Hun\u00b7ger", "plagt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er haucht in seine Hand, um minder Frost zu f\u00fchlen;", "tokens": ["Er", "haucht", "in", "sei\u00b7ne", "Hand", ",", "um", "min\u00b7der", "Frost", "zu", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUI", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er bl\u00e4set auch beym Tisch, die heisse Kost zu k\u00fchlen.", "tokens": ["Er", "bl\u00e4\u00b7set", "auch", "beym", "Tisch", ",", "die", "heis\u00b7se", "Kost", "zu", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Wald-Gott, der den Handel nicht verstunde,", "tokens": ["Der", "Wald\u00b7Gott", ",", "der", "den", "Han\u00b7del", "nicht", "ver\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "War gantz best\u00fcrtzt und ausser sich, zu sehn,", "tokens": ["War", "gantz", "be\u00b7st\u00fcrtzt", "und", "aus\u00b7ser", "sich", ",", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "APPR", "PRF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So kalt und warm aus einem Munde", "tokens": ["So", "kalt", "und", "warm", "aus", "ei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und fast in selbiger Minute gehn.", "tokens": ["Und", "fast", "in", "sel\u00b7bi\u00b7ger", "Mi\u00b7nu\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Derselbe Hauch geschah nur auf verschiedne Weise,", "tokens": ["Der\u00b7sel\u00b7be", "Hauch", "ge\u00b7schah", "nur", "auf", "ver\u00b7schied\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Indem er langsam haucht, und bl\u00e4st den Athem leise;", "tokens": ["In\u00b7dem", "er", "lang\u00b7sam", "haucht", ",", "und", "bl\u00e4st", "den", "A\u00b7them", "lei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erw\u00e4rmt er seine Hand, die gantz durch Frost erstarrt.", "tokens": ["Er\u00b7w\u00e4rmt", "er", "sei\u00b7ne", "Hand", ",", "die", "gantz", "durch", "Frost", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "An statt, da\u00df da die Lufft aus seiner Lunge", "tokens": ["An", "statt", ",", "da\u00df", "da", "die", "Lufft", "aus", "sei\u00b7ner", "Lun\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Durch einen Sto\u00df gerad\u2019 und hefftig drunge,", "tokens": ["Durch", "ei\u00b7nen", "Sto\u00df", "ge\u00b7rad'", "und", "heff\u00b7tig", "drun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der Speisen heisser Dampff dadurch verjaget ward.", "tokens": ["Der", "Spei\u00b7sen", "heis\u00b7ser", "Dampff", "da\u00b7durch", "ver\u00b7ja\u00b7get", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}