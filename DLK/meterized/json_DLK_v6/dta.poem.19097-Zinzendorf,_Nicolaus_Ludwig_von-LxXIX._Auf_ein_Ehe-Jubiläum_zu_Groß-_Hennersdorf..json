{"dta.poem.19097": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXIX.   Auf ein Ehe-Jubil\u00e4um zu Gro\u00df-  \n Hennersdorf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Osalomo! hie sind die Menschen-Kinder,", "tokens": ["O\u00b7sa\u00b7lo\u00b7mo", "!", "hie", "sind", "die", "Men\u00b7schen\u00b7Kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die haben sich schon funfzig Jahr geliebt,", "tokens": ["Die", "ha\u00b7ben", "sich", "schon", "funf\u00b7zig", "Jahr", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Freud und Leid gemeynet und ge\u00fcbt.", "tokens": ["In", "Freud", "und", "Leid", "ge\u00b7mey\u00b7net", "und", "ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie lange ists, du Seelen-Uberwinder,", "tokens": ["Wie", "lan\u00b7ge", "ists", ",", "du", "See\u00b7len\u00b7U\u00b7ber\u00b7win\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da\u00df du uns \u00fcberredt und wirs gewagt,", "tokens": ["Da\u00df", "du", "uns", "\u00fc\u00b7ber\u00b7redt", "und", "wirs", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "KON", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df wir die Eh einander zugesagt!", "tokens": ["Da\u00df", "wir", "die", "Eh", "ein\u00b7an\u00b7der", "zu\u00b7ge\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O grosser GOtt! wie viele sind vorhanden.", "tokens": ["O", "gros\u00b7ser", "Gott", "!", "wie", "vie\u00b7le", "sind", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWAV", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dich als GOtt in Wahrheit angebett,", "tokens": ["Die", "dich", "als", "Gott", "in", "Wahr\u00b7heit", "an\u00b7ge\u00b7bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die dich als HErrn von Hertzen angeredt,", "tokens": ["Die", "dich", "als", "Herrn", "von", "Hert\u00b7zen", "an\u00b7ge\u00b7redt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die deine Meisterschaft im Geist verstanden:", "tokens": ["Die", "dei\u00b7ne", "Meis\u00b7ter\u00b7schaft", "im", "Geist", "ver\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer ist, der deine Hirten-St\u00e4be kennt,", "tokens": ["Wer", "ist", ",", "der", "dei\u00b7ne", "Hir\u00b7ten\u00b7St\u00e4\u00b7be", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer aber, der dich Lieb in Wollust nennt.", "tokens": ["Wer", "a\u00b7ber", ",", "der", "dich", "Lieb", "in", "Wol\u00b7lust", "nennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PRELS", "PPER", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach Liebe! ach die meisten Menschen gehen", "tokens": ["Ach", "Lie\u00b7be", "!", "ach", "die", "meis\u00b7ten", "Men\u00b7schen", "ge\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "XY", "ART", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit hohem Aug und krummer Seele hin,", "tokens": ["Mit", "ho\u00b7hem", "Aug", "und", "krum\u00b7mer", "See\u00b7le", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie haben einen niedertr\u00e4chtgen Sinn,", "tokens": ["Sie", "ha\u00b7ben", "ei\u00b7nen", "nie\u00b7der\u00b7tr\u00e4cht\u00b7gen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und lassen sich auch nicht zu dir erh\u00f6hen.", "tokens": ["Und", "las\u00b7sen", "sich", "auch", "nicht", "zu", "dir", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Leiter zu dem Himmel heisset Creutz,", "tokens": ["Die", "Lei\u00b7ter", "zu", "dem", "Him\u00b7mel", "heis\u00b7set", "Creutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und diesem Steg entweicht man allerseits.", "tokens": ["Und", "die\u00b7sem", "Steg", "ent\u00b7weicht", "man", "al\u00b7ler\u00b7seits", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Creutzes Stamm ist von dem Fluch gerissen,", "tokens": ["Des", "Creut\u00b7zes", "Stamm", "ist", "von", "dem", "Fluch", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er ist den Seelen nun ein ein Segens-Holtz:", "tokens": ["Er", "ist", "den", "See\u00b7len", "nun", "ein", "ein", "Se\u00b7gens\u00b7Holtz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Allein der Creatur verwegner Stoltz", "tokens": ["Al\u00b7lein", "der", "Crea\u00b7tur", "ver\u00b7weg\u00b7ner", "Stoltz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Greift nach der Lust, und tritt das Creutz mit F\u00fcssen,", "tokens": ["Greift", "nach", "der", "Lust", ",", "und", "tritt", "das", "Creutz", "mit", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Heyland hatte einen andern Sinn:", "tokens": ["Der", "Hey\u00b7land", "hat\u00b7te", "ei\u00b7nen", "an\u00b7dern", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er nahm die Angst vor alle Freude hin.", "tokens": ["Er", "nahm", "die", "Angst", "vor", "al\u00b7le", "Freu\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}