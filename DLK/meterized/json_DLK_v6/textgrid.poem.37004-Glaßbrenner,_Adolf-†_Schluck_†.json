{"textgrid.poem.37004": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "\u2020 Schluck \u2020", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Glauben, Hoffen, Lieben war die Flasche!", "tokens": ["Sein", "Glau\u00b7ben", ",", "Hof\u00b7fen", ",", "Lie\u00b7ben", "war", "die", "Fla\u00b7sche", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "ADJA", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Weines Feuer, das durch ihn gerannt,", "tokens": ["Des", "Wei\u00b7nes", "Feu\u00b7er", ",", "das", "durch", "ihn", "ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat seine Seele \u2013 ach, zu fr\u00fch! \u2013 verbrannt.", "tokens": ["Hat", "sei\u00b7ne", "See\u00b7le", "\u2013", "ach", ",", "zu", "fr\u00fch", "!", "\u2013", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$(", "XY", "$,", "PTKA", "ADJD", "$.", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sanft ruhe seine Asche!", "tokens": ["Sanft", "ru\u00b7he", "sei\u00b7ne", "A\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}