{"textgrid.poem.66704": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Runen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4ume mein Leben", "tokens": ["Ich", "tr\u00e4u\u00b7me", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hinab in die Tiefen,", "tokens": ["Hin\u00b7ab", "in", "die", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich tauch' in die Gr\u00fcnde", "tokens": ["Ich", "tauch'", "in", "die", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des Schicksals den Blick.", "tokens": ["Des", "Schick\u00b7sals", "den", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es gl\u00fchen und schweben", "tokens": ["Es", "gl\u00fc\u00b7hen", "und", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Hieroglyphen \u2013", "tokens": ["Die", "Hie\u00b7ro\u00b7gly\u00b7phen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wer ganz sie verst\u00fcnde,", "tokens": ["Wer", "ganz", "sie", "ver\u00b7st\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Erf\u00fchr' sein Geschick.", "tokens": ["Er\u00b7f\u00fchr'", "sein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Wie mag ich sie deuten,", "tokens": ["Wie", "mag", "ich", "sie", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die zuckenden Zeichen,", "tokens": ["Die", "zu\u00b7cken\u00b7den", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Bald leuchtend wie Flamme,", "tokens": ["Bald", "leuch\u00b7tend", "wie", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Bald schattenbedeckt?:", "tokens": ["Bald", "schat\u00b7ten\u00b7be\u00b7deckt", "?", ":"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$.", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "\u00bbdu willst es erbeuten,", "tokens": ["\u00bb", "du", "willst", "es", "er\u00b7beu\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Du sollst es erreichen,", "tokens": ["Du", "sollst", "es", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Du bist von dem Stamme,", "tokens": ["Du", "bist", "von", "dem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Den Irrtum nicht schreckt.", "tokens": ["Den", "Irr\u00b7tum", "nicht", "schreckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Sonst l\u00e4gest vernichtet", "tokens": ["Sonst", "l\u00e4\u00b7gest", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Du l\u00e4ngst von D\u00e4monen,", "tokens": ["Du", "l\u00e4ngst", "von", "D\u00e4\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Die frech dich bedr\u00e4ngten", "tokens": ["Die", "frech", "dich", "be\u00b7dr\u00e4ng\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Mit furchtbarer Macht;", "tokens": ["Mit", "furcht\u00b7ba\u00b7rer", "Macht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Sonst h\u00e4ttest verzichtet", "tokens": ["Sonst", "h\u00e4t\u00b7test", "ver\u00b7zich\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Du droben zu thronen,", "tokens": ["Du", "dro\u00b7ben", "zu", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Und dich verh\u00e4ngten", "tokens": ["Und", "dich", "ver\u00b7h\u00e4ng\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Die Schatten der Nacht.", "tokens": ["Die", "Schat\u00b7ten", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Nun bist du geborgen", "tokens": ["Nun", "bist", "du", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Vor schm\u00e4hlichen Schlingen,", "tokens": ["Vor", "schm\u00e4h\u00b7li\u00b7chen", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Sie liegen zerrissen", "tokens": ["Sie", "lie\u00b7gen", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Von trotziger Kraft \u2013", "tokens": ["Von", "trot\u00b7zi\u00b7ger", "Kraft", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "Kein zehrendes Sorgen", "tokens": ["Kein", "zeh\u00b7ren\u00b7des", "Sor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Soll je dich bezwingen,", "tokens": ["Soll", "je", "dich", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.31": {"text": "Kein zages Gewissen", "tokens": ["Kein", "za\u00b7ges", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Dich wieder erschlafft.", "tokens": ["Dich", "wie\u00b7der", "er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Sprich, kannst du's versp\u00fcren,", "tokens": ["Sprich", ",", "kannst", "du's", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.34": {"text": "Was leise wir raunen,", "tokens": ["Was", "lei\u00b7se", "wir", "rau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Kannst weise du l\u00f6sen", "tokens": ["Kannst", "wei\u00b7se", "du", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Die R\u00e4tsel der Schrift?:", "tokens": ["Die", "R\u00e4t\u00b7sel", "der", "Schrift", "?", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Dein Blut wird dich f\u00fchren", "tokens": ["Dein", "Blut", "wird", "dich", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "Durch Zickzack und Launen,", "tokens": ["Durch", "Zick\u00b7zack", "und", "Lau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Bis grade dein Wesen", "tokens": ["Bis", "gra\u00b7de", "dein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "Sein K\u00f6nigtum trifft.", "tokens": ["Sein", "K\u00f6\u00b7nig\u00b7tum", "trifft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.41": {"text": "Denn du bist von jenen,", "tokens": ["Denn", "du", "bist", "von", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PDAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.42": {"text": "Die nimmer zu leiten", "tokens": ["Die", "nim\u00b7mer", "zu", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Von anderen H\u00e4nden,", "tokens": ["Von", "an\u00b7de\u00b7ren", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Bestimmung und Rat;", "tokens": ["Be\u00b7stim\u00b7mung", "und", "Rat", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.45": {"text": "Selbsteigenes Sehnen", "tokens": ["Selbs\u00b7tei\u00b7ge\u00b7nes", "Seh\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "Mu\u00df stark dir bereiten", "tokens": ["Mu\u00df", "stark", "dir", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Und mutig vollenden", "tokens": ["Und", "mu\u00b7tig", "voll\u00b7en\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "Den f\u00e4hrlichen Pfad.", "tokens": ["Den", "f\u00e4hr\u00b7li\u00b7chen", "Pfad", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "So lasse dich walten", "tokens": ["So", "las\u00b7se", "dich", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.50": {"text": "Und walte du deiner,", "tokens": ["Und", "wal\u00b7te", "du", "dei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.51": {"text": "Mit wachem Besinnen", "tokens": ["Mit", "wa\u00b7chem", "Be\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.52": {"text": "Dir selber vertraut!", "tokens": ["Dir", "sel\u00b7ber", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.53": {"text": "Du sollst dich entfalten", "tokens": ["Du", "sollst", "dich", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.54": {"text": "Nur freier und reiner,", "tokens": ["Nur", "frei\u00b7er", "und", "rei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.55": {"text": "Und ganz sie gewinnen,", "tokens": ["Und", "ganz", "sie", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.56": {"text": "Die sch\u00f6nste, die flammenumschlungene Braut.\u00ab", "tokens": ["Die", "sch\u00f6ns\u00b7te", ",", "die", "flam\u00b7me\u00b7num\u00b7schlun\u00b7ge\u00b7ne", "Braut", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}