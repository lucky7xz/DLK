{"textgrid.poem.42967": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging auf Abenteuer", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging auf Abenteuer", "tokens": ["Ich", "ging", "auf", "A\u00b7bent\u00b7eu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch finsteres Gassengewirr.", "tokens": ["Durch", "fins\u00b7te\u00b7res", "Gas\u00b7sen\u00b7ge\u00b7wirr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein Fenster in schiefem Gem\u00e4uer.", "tokens": ["Ein", "Fens\u00b7ter", "in", "schie\u00b7fem", "Ge\u00b7m\u00e4u\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Inseits ein leises Geklirr", "tokens": ["In\u00b7seits", "ein", "lei\u00b7ses", "Ge\u00b7klirr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und ein kleines, bl\u00e4uliches Feuer. \u2013", "tokens": ["Und", "ein", "klei\u00b7nes", ",", "bl\u00e4u\u00b7li\u00b7ches", "Feu\u00b7er", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Durchaus ganz geheuer:", "tokens": ["Durc\u00b7haus", "ganz", "ge\u00b7heu\u00b7er", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Feuerzangen", "tokens": ["Feu\u00b7er\u00b7zan\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Bowle. Bin weitergegangen.", "tokens": ["Bow\u00b7le", ".", "Bin", "wei\u00b7ter\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Das Eckhaus ist ein Bordell,", "tokens": ["Das", "Eck\u00b7haus", "ist", "ein", "Bor\u00b7dell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ganze Stadt wei\u00df es.", "tokens": ["Die", "gan\u00b7ze", "Stadt", "wei\u00df", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich ging ganz langsam, nicht schnell,", "tokens": ["Ich", "ging", "ganz", "lang\u00b7sam", ",", "nicht", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wegen des Glatteises", "tokens": ["We\u00b7gen", "des", "Glat\u00b7tei\u00b7ses"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hin und hinein.", "tokens": ["Hin", "und", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Da sa\u00df unterm Christbaum allein", "tokens": ["Da", "sa\u00df", "un\u00b7term", "Christ\u00b7baum", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ein magerer Zuh\u00e4lter.", "tokens": ["Ein", "ma\u00b7ge\u00b7rer", "Zu\u00b7h\u00e4l\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er konnte siebzig, auch \u00e4lter,", "tokens": ["Er", "konn\u00b7te", "sieb\u00b7zig", ",", "auch", "\u00e4l\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Er konnte auch Lebegreis sein.", "tokens": ["Er", "konn\u00b7te", "auch", "Le\u00b7be\u00b7greis", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wir wechselten falsche Namen,", "tokens": ["Wir", "wech\u00b7sel\u00b7ten", "fal\u00b7sche", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und weil gar keine Damen", "tokens": ["Und", "weil", "gar", "kei\u00b7ne", "Da\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da waren, sangen wir traurig ein Lied,", "tokens": ["Da", "wa\u00b7ren", ",", "san\u00b7gen", "wir", "trau\u00b7rig", "ein", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seltsam war die Stimme des Greises.", "tokens": ["Selt\u00b7sam", "war", "die", "Stim\u00b7me", "des", "Grei\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ich schied,", "tokens": ["Ich", "schied", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Schlich langsam wegen des Glatteises.", "tokens": ["Schlich", "lang\u00b7sam", "we\u00b7gen", "des", "Glat\u00b7tei\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das gl\u00e4ttste von allen Wintern,", "tokens": ["Das", "gl\u00e4tts\u00b7te", "von", "al\u00b7len", "Win\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die je ich erlebt.", "tokens": ["Die", "je", "ich", "er\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kein Sand gestreut.", "tokens": ["Kein", "Sand", "ge\u00b7streut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Man geht \u2013 sitzt auf dem Hintern,", "tokens": ["Man", "geht", "\u2013", "sitzt", "auf", "dem", "Hin\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hat nichts gebrochen \u2013 erhebt", "tokens": ["Hat", "nichts", "ge\u00b7bro\u00b7chen", "\u2013", "er\u00b7hebt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PIS", "VVPP", "$(", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sich wieder \u2013 und sitzt erneut.", "tokens": ["Sich", "wie\u00b7der", "\u2013", "und", "sitzt", "er\u00b7neut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$(", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Quer \u00fcbern Weg pl\u00f6tzlich lief", "tokens": ["Quer", "\u00fc\u00b7bern", "Weg", "pl\u00f6tz\u00b7lich", "lief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Eine Katze. Also: Ich trat", "tokens": ["Ei\u00b7ne", "Kat\u00b7ze", ".", "Al\u00b7so", ":", "Ich", "trat"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "$.", "PPER", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Schnell drei Schritt zur\u00fcck. Da rief", "tokens": ["Schnell", "drei", "Schritt", "zu\u00b7r\u00fcck", ".", "Da", "rief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "CARD", "NN", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Hinter mir \u00bbAu!\u00ab ein Marinesoldat.", "tokens": ["Hin\u00b7ter", "mir", "\u00bb", "Au", "!", "\u00ab", "ein", "Ma\u00b7ri\u00b7ne\u00b7sol\u00b7dat", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "NN", "$.", "$(", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir gestanden als Wasserratten,", "tokens": ["Wir", "ge\u00b7stan\u00b7den", "als", "Was\u00b7ser\u00b7rat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KOKOM", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Was wir zuvor schon getrunken hatten.", "tokens": ["Was", "wir", "zu\u00b7vor", "schon", "ge\u00b7trun\u00b7ken", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir haben uns an-ahoit.", "tokens": ["Wir", "ha\u00b7ben", "uns", "an\u00b7a\u00b7hoit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kein Sand war gestreut.", "tokens": ["Kein", "Sand", "war", "ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wir lagen. \u2013 Was soll ich lange noch sagen \u2013", "tokens": ["Wir", "la\u00b7gen", ".", "\u2013", "Was", "soll", "ich", "lan\u00b7ge", "noch", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Liefen, lagen, liefen \u2013.", "tokens": ["Lie\u00b7fen", ",", "la\u00b7gen", ",", "lie\u00b7fen", "\u2013", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Und riefen", "tokens": ["Und", "rie\u00b7fen"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die Damen herunter, wollten was tun,", "tokens": ["Die", "Da\u00b7men", "her\u00b7un\u00b7ter", ",", "woll\u00b7ten", "was", "tun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wildes, wie Stierkampf oder Taifun.", "tokens": ["Wil\u00b7des", ",", "wie", "Stier\u00b7kampf", "o\u00b7der", "Tai\u00b7fun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "KON", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch wir entschliefen", "tokens": ["Doch", "wir", "ent\u00b7schlie\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ohne Weiber unter dem Baum.", "tokens": ["Oh\u00b7ne", "Wei\u00b7ber", "un\u00b7ter", "dem", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der Lebezuh\u00e4lter", "tokens": ["Der", "Le\u00b7be\u00b7zu\u00b7h\u00e4l\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Pfiff r\u00fcckw\u00e4rts im Traum.", "tokens": ["Pfiff", "r\u00fcck\u00b7w\u00e4rts", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Der n\u00e4chste Tag war viel k\u00e4lter.", "tokens": ["Der", "n\u00e4chs\u00b7te", "Tag", "war", "viel", "k\u00e4l\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}