{"dta.poem.11091": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie verreisen wolte.  \n  C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was treibt dich, Clelie! zu solchen harten schl\u00fcssen?", "tokens": ["Was", "treibt", "dich", ",", "Cle\u00b7lie", "!", "zu", "sol\u00b7chen", "har\u00b7ten", "schl\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NE", "$.", "APPR", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was hat dir dieser ort, was diese brust gethan?", "tokens": ["Was", "hat", "dir", "die\u00b7ser", "ort", ",", "was", "die\u00b7se", "brust", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PDAT", "NN", "$,", "PWS", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sich dein hertze nicht zu frieden geben kan,", "tokens": ["Da\u00df", "sich", "dein", "hert\u00b7ze", "nicht", "zu", "frie\u00b7den", "ge\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und deinen sch\u00f6nen leib von mir entfernt will wissen?", "tokens": ["Und", "dei\u00b7nen", "sch\u00f6\u00b7nen", "leib", "von", "mir", "ent\u00b7fernt", "will", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach sch\u00f6nste! wo du ja wilst fremde l\u00fcffte k\u00fcssen,", "tokens": ["Ach", "sch\u00f6ns\u00b7te", "!", "wo", "du", "ja", "wilst", "frem\u00b7de", "l\u00fcff\u00b7te", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "PWAV", "PPER", "ADV", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Tritt dann dein zarter fu\u00df auf die verwehrte bahn?", "tokens": ["Tritt", "dann", "dein", "zar\u00b7ter", "fu\u00df", "auf", "die", "ver\u00b7wehr\u00b7te", "bahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "So fang ich allbereit vor angst zu sterben an,", "tokens": ["So", "fang", "ich", "all\u00b7be\u00b7reit", "vor", "angst", "zu", "ster\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und werde bald mein hau\u00df und grab bestellen m\u00fcssen.", "tokens": ["Und", "wer\u00b7de", "bald", "mein", "hau\u00df", "und", "grab", "be\u00b7stel\u00b7len", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch will ich dich noch hier zu halten mich bem\u00fchn:", "tokens": ["Doch", "will", "ich", "dich", "noch", "hier", "zu", "hal\u00b7ten", "mich", "be\u00b7m\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein seufftzen soll den wind dir gantz zuwider fachen,", "tokens": ["Mein", "seufft\u00b7zen", "soll", "den", "wind", "dir", "gantz", "zu\u00b7wi\u00b7der", "fa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "ART", "VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein w\u00fcnschen soll das rad des wagens r\u00fcckw\u00e4rts ziehn,", "tokens": ["Mein", "w\u00fcn\u00b7schen", "soll", "das", "rad", "des", "wa\u00b7gens", "r\u00fcck\u00b7w\u00e4rts", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den pferden will ich was an ihre f\u00fcsse machen;", "tokens": ["Den", "pfer\u00b7den", "will", "ich", "was", "an", "ih\u00b7re", "f\u00fcs\u00b7se", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ja ich will eher nicht alsdenn zu frieden seyn,", "tokens": ["Ja", "ich", "will", "e\u00b7her", "nicht", "als\u00b7denn", "zu", "frie\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bi\u00df dich, o Clelie! dein abschied wird gereun.", "tokens": ["Bi\u00df", "dich", ",", "o", "Cle\u00b7lie", "!", "dein", "ab\u00b7schied", "wird", "ge\u00b7reun", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NE", "$.", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}