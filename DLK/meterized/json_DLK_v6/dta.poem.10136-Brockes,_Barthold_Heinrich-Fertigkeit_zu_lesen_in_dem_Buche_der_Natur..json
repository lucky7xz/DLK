{"dta.poem.10136": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fertigkeit zu lesen in dem Buche der  \n Natur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In ieder Wissenschaft und Kunst, die Menschen wissen,", "tokens": ["In", "ie\u00b7der", "Wis\u00b7sen\u00b7schaft", "und", "Kunst", ",", "die", "Men\u00b7schen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat man ja Arbeit, Flei\u00df und M\u00fch,", "tokens": ["Hat", "man", "ja", "Ar\u00b7beit", ",", "Flei\u00df", "und", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zu erlernen, nehmen m\u00fcssen;", "tokens": ["Sie", "zu", "er\u00b7ler\u00b7nen", ",", "neh\u00b7men", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch wiederholen fasst man sie.", "tokens": ["Durch", "wie\u00b7der\u00b7ho\u00b7len", "fasst", "man", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die F\u00e4higkeit, die in der Seelen stecket,", "tokens": ["Die", "F\u00e4\u00b7hig\u00b7keit", ",", "die", "in", "der", "See\u00b7len", "ste\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entwickelt sich allm\u00e4hlich, wird erwecket,", "tokens": ["Ent\u00b7wi\u00b7ckelt", "sich", "all\u00b7m\u00e4h\u00b7lich", ",", "wird", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und nimmt durch Ubung zu. Ist denn die Wissenschaft", "tokens": ["Und", "nimmt", "durch", "U\u00b7bung", "zu", ".", "Ist", "denn", "die", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$.", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Jm Buch der Creatur, den Sch\u00f6pfer selbst zu finden,", "tokens": ["Jm", "Buch", "der", "Crea\u00b7tur", ",", "den", "Sch\u00f6p\u00b7fer", "selbst", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und Seine Weisheit, Lieb\u2019 und Allmacht zu ergr\u00fcnden,", "tokens": ["Und", "Sei\u00b7ne", "Weis\u00b7heit", ",", "Lieb'", "und", "All\u00b7macht", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht einst der M\u00fche wehrt, da\u00df wir der Seelen Krafft", "tokens": ["Nicht", "einst", "der", "M\u00fc\u00b7he", "wehrt", ",", "da\u00df", "wir", "der", "See\u00b7len", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Offt auf so edlen Vorwurff lencken,", "tokens": ["Offt", "auf", "so", "ed\u00b7len", "Vor\u00b7wurff", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und, wann wir h\u00f6ren, sehn und schmecken, Des gedencken,", "tokens": ["Und", ",", "wann", "wir", "h\u00f6\u00b7ren", ",", "sehn", "und", "schme\u00b7cken", ",", "Des", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVINF", "$,", "VVINF", "KON", "VVINF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der uns f\u00fcr so viel Guts, und Seiner Wercke Pracht", "tokens": ["Der", "uns", "f\u00fcr", "so", "viel", "Guts", ",", "und", "Sei\u00b7ner", "Wer\u00b7cke", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADV", "PIAT", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So wunderbarlich sinnlich macht?", "tokens": ["So", "wun\u00b7der\u00b7bar\u00b7lich", "sinn\u00b7lich", "macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Je \u00f6ffter man sich \u00fcbt, die Creatur zu sehn,", "tokens": ["Je", "\u00f6ff\u00b7ter", "man", "sich", "\u00fcbt", ",", "die", "Crea\u00b7tur", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "PRF", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Je fertiger wird man im lesen,", "tokens": ["Je", "fer\u00b7ti\u00b7ger", "wird", "man", "im", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIS", "APPRART", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Je deutlicher wird man der Gottheit Wesen,", "tokens": ["Je", "deut\u00b7li\u00b7cher", "wird", "man", "der", "Got\u00b7theit", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIS", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Des Welt-Buchs Inhalt, Kern und Zweck verstehn,", "tokens": ["Des", "Welt\u00b7Buchs", "In\u00b7halt", ",", "Kern", "und", "Zweck", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und, immer br\u00fcnstiger, Sein herrlichs Lob erh\u00f6hn.", "tokens": ["Und", ",", "im\u00b7mer", "br\u00fcns\u00b7ti\u00b7ger", ",", "Sein", "herr\u00b7lichs", "Lob", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach so gewehnet euch, geliebte Menschen, doch", "tokens": ["Ach", "so", "ge\u00b7weh\u00b7net", "euch", ",", "ge\u00b7lieb\u00b7te", "Men\u00b7schen", ",", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu dieser s\u00fcssen M\u00fch, zu diesem leichten Joch!", "tokens": ["Zu", "die\u00b7ser", "s\u00fcs\u00b7sen", "M\u00fch", ",", "zu", "die\u00b7sem", "leich\u00b7ten", "Joch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Besch\u00e4fftigt euch, und lernt aufmercksam, GOTT zu Ehren,", "tokens": ["Be\u00b7sch\u00e4ff\u00b7tigt", "euch", ",", "und", "lernt", "auf\u00b7merck\u00b7sam", ",", "GoTT", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "ADJD", "$,", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Empfinden, schmecken, sehn und h\u00f6ren!", "tokens": ["Emp\u00b7fin\u00b7den", ",", "schme\u00b7cken", ",", "sehn", "und", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}