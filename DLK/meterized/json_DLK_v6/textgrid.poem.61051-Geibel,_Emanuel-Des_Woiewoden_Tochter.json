{"textgrid.poem.61051": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Des Woiewoden Tochter", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht im Wald, im tiefen Wald", "tokens": ["Es", "steht", "im", "Wald", ",", "im", "tie\u00b7fen", "Wald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Haus des Woiewoden;", "tokens": ["Das", "Haus", "des", "Woi\u00b7e\u00b7wo\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Eiszapfen hangen am Dache kalt,", "tokens": ["Eis\u00b7zap\u00b7fen", "han\u00b7gen", "am", "Da\u00b7che", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und Schnee bedeckt den Boden.", "tokens": ["Und", "Schnee", "be\u00b7deckt", "den", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Fr\u00e4ulein sitzt am Herd und spinnt", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "sitzt", "am", "Herd", "und", "spinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ihrem Hochzeitschleier;", "tokens": ["Zu", "ih\u00b7rem", "Hoch\u00b7zeit\u00b7schlei\u00b7er", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie h\u00f6rt im Rauchfang gehn den Wind", "tokens": ["Sie", "h\u00f6rt", "im", "Rauch\u00b7fang", "gehn", "den", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00fcrt empor das Feuer.", "tokens": ["Und", "sch\u00fcrt", "em\u00b7por", "das", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da tritt die Waldfrau zu ihr ein,", "tokens": ["Da", "tritt", "die", "Wald\u00b7frau", "zu", "ihr", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die pflegt nicht Guts zu bringen:", "tokens": ["Die", "pflegt", "nicht", "Guts", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbguten Abend, feines Goldt\u00f6chterlein!", "tokens": ["\u00bb", "gu\u00b7ten", "A\u00b7bend", ",", "fei\u00b7nes", "Gold\u00b7t\u00f6ch\u00b7ter\u00b7lein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Will dir ein Liedchen singen!\u00ab", "tokens": ["Will", "dir", "ein", "Lied\u00b7chen", "sin\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbwas sollen deine Lieder mir?", "tokens": ["\u00bb", "was", "sol\u00b7len", "dei\u00b7ne", "Lie\u00b7der", "mir", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Liebster, der kommt balde.", "tokens": ["Mein", "Liebs\u00b7ter", ",", "der", "kommt", "bal\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da hast du Brot, da hast du Bier,", "tokens": ["Da", "hast", "du", "Brot", ",", "da", "hast", "du", "Bier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh wieder heim zum Walde!\u00ab", "tokens": ["Geh", "wie\u00b7der", "heim", "zum", "Wal\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "PTKVZ", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Alte sprach: \u00bbHast immer Zeit,", "tokens": ["Die", "Al\u00b7te", "sprach", ":", "\u00bb", "Hast", "im\u00b7mer", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Schatz wird nimmer kommen,", "tokens": ["Dein", "Schatz", "wird", "nim\u00b7mer", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Wald ist tief, der Weg ist weit;", "tokens": ["Der", "Wald", "ist", "tief", ",", "der", "Weg", "ist", "weit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat andern Weg genommen.\u00ab", "tokens": ["Hat", "an\u00b7dern", "Weg", "ge\u00b7nom\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwas qu\u00e4lst du mich mit falschem Weh?", "tokens": ["\u00bb", "was", "qu\u00e4lst", "du", "mich", "mit", "fal\u00b7schem", "Weh", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treu wird mein Liebster bleiben,", "tokens": ["Treu", "wird", "mein", "Liebs\u00b7ter", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schwur es mir, bis aus dem Schnee", "tokens": ["Er", "schwur", "es", "mir", ",", "bis", "aus", "dem", "Schnee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Einst rote R\u00f6slein treiben.\u00ab", "tokens": ["Einst", "ro\u00b7te", "R\u00f6s\u00b7lein", "trei\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Fr\u00e4ulein rief's, doch war ihr bang,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "rie\u00b7f's", ",", "doch", "war", "ihr", "bang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Wind pfiff nicht geheuer,", "tokens": ["Der", "Wind", "pfiff", "nicht", "ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Alte blieb, die Alte sang", "tokens": ["Die", "Al\u00b7te", "blieb", ",", "die", "Al\u00b7te", "sang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr dumpfes Lied ins Feuer:", "tokens": ["Ihr", "dum\u00b7pfes", "Lied", "ins", "Feu\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbund als ich ging die Schlucht entlang,", "tokens": ["\u00bb", "und", "als", "ich", "ging", "die", "Schlucht", "ent\u00b7lang", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kamen drei W\u00f6lfe gesprungen,", "tokens": ["Da", "ka\u00b7men", "drei", "W\u00f6l\u00b7fe", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die heulten wie ob gutem Fang", "tokens": ["Die", "heul\u00b7ten", "wie", "ob", "gu\u00b7tem", "Fang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KOKOM", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hatten blutige Zungen.", "tokens": ["Und", "hat\u00b7ten", "blu\u00b7ti\u00b7ge", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Und als ich kam zum Fichtenzaun,", "tokens": ["Und", "als", "ich", "kam", "zum", "Fich\u00b7ten\u00b7zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drei Raben h\u00f6rt' ich schreien;", "tokens": ["Drei", "Ra\u00b7ben", "h\u00f6rt'", "ich", "schrei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schrien: \u203aIhr Jungen, euch sollt' traun", "tokens": ["Sie", "schri\u00b7en", ":", "\u203a", "Ihr", "Jun\u00b7gen", ",", "euch", "sollt'", "traun"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der frische Schmaus gedeihen!\u2039", "tokens": ["Der", "fri\u00b7sche", "Schmaus", "ge\u00b7dei\u00b7hen", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und als ich kam zum eis'gen See,", "tokens": ["Und", "als", "ich", "kam", "zum", "eis'\u00b7gen", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich einen Knaben gefunden;", "tokens": ["Hab'", "ich", "ei\u00b7nen", "Kna\u00b7ben", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Es flo\u00df wohl \u00fcber den Winterschnee", "tokens": ["Es", "flo\u00df", "wohl", "\u00fc\u00b7ber", "den", "Win\u00b7ter\u00b7schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Blut aus tiefen Wunden.", "tokens": ["Sein", "Blut", "aus", "tie\u00b7fen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Rot R\u00f6slein bl\u00fcht aus dem Schnee so kalt,", "tokens": ["Rot", "R\u00f6s\u00b7lein", "bl\u00fcht", "aus", "dem", "Schnee", "so", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun hast du's selbst vernommen.", "tokens": ["Nun", "hast", "du's", "selbst", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Weg ist weit, und tief der Wald,", "tokens": ["Der", "Weg", "ist", "weit", ",", "und", "tief", "der", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Schatz wird nimmer kommen.\u00ab", "tokens": ["Dein", "Schatz", "wird", "nim\u00b7mer", "kom\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Das Lied war aus, die Alte fort,", "tokens": ["Das", "Lied", "war", "aus", ",", "die", "Al\u00b7te", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herdes Glut vergangen,", "tokens": ["Des", "Her\u00b7des", "Glut", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfrau sa\u00df und sprach kein Wort,", "tokens": ["Die", "Jung\u00b7frau", "sa\u00df", "und", "sprach", "kein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr waren so bleich die Wangen.", "tokens": ["Ihr", "wa\u00b7ren", "so", "bleich", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Und lauter drau\u00dfen pfiff der Wind,", "tokens": ["Und", "lau\u00b7ter", "drau\u00b7\u00dfen", "pfiff", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lauter schrien die Raben.", "tokens": ["Und", "lau\u00b7ter", "schri\u00b7en", "die", "Ra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drei Tage nach diesem hat sein Kind", "tokens": ["Drei", "Ta\u00b7ge", "nach", "die\u00b7sem", "hat", "sein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "PDAT", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Woiewod begraben.", "tokens": ["Der", "Woi\u00b7e\u00b7wod", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}