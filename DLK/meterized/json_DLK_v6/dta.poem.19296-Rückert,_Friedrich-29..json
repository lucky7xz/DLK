{"dta.poem.19296": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vier Dinge sind zugleich unendlich weit und schmal,", "tokens": ["Vier", "Din\u00b7ge", "sind", "zu\u00b7gleich", "un\u00b7end\u00b7lich", "weit", "und", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unendlich gro\u00df und klein: Zeit, Raum, Bewegung, Zahl.", "tokens": ["Un\u00b7end\u00b7lich", "gro\u00df", "und", "klein", ":", "Zeit", ",", "Raum", ",", "Be\u00b7we\u00b7gung", ",", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$.", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du kanst die gr\u00f6ste Zahl vergr\u00f6\u00dfern immer doch,", "tokens": ["Du", "kanst", "die", "gr\u00f6s\u00b7te", "Zahl", "ver\u00b7gr\u00f6\u00b7\u00dfern", "im\u00b7mer", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kanst die kleinste Zahl verkleinern immer noch.", "tokens": ["Du", "kanst", "die", "kleins\u00b7te", "Zahl", "ver\u00b7klei\u00b7nern", "im\u00b7mer", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So kanst du jeden Raum erweitern und verengern,", "tokens": ["So", "kanst", "du", "je\u00b7den", "Raum", "er\u00b7wei\u00b7tern", "und", "ver\u00b7en\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kanst du jede Zeit verk\u00fcrzen und verl\u00e4ngern.", "tokens": ["So", "kanst", "du", "je\u00b7de", "Zeit", "ver\u00b7k\u00fcr\u00b7zen", "und", "ver\u00b7l\u00e4n\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und so verlangsamen kanst du und vergeschwindern", "tokens": ["Und", "so", "ver\u00b7lang\u00b7sa\u00b7men", "kanst", "du", "und", "ver\u00b7ge\u00b7schwin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "PPER", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jede Bewegung auch, vermehren und vermindern.", "tokens": ["Je\u00b7de", "Be\u00b7we\u00b7gung", "auch", ",", "ver\u00b7meh\u00b7ren", "und", "ver\u00b7min\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "So haben diese vier, Zahl, Raum, Bewegung, Zeit,", "tokens": ["So", "ha\u00b7ben", "die\u00b7se", "vier", ",", "Zahl", ",", "Raum", ",", "Be\u00b7we\u00b7gung", ",", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "CARD", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach beiden Seiten hin zwiefach Unendlichkeit.", "tokens": ["Nach", "bei\u00b7den", "Sei\u00b7ten", "hin", "zwie\u00b7fach", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "Und wie sie wechselnd sich verbinden und bedingen,", "tokens": ["Und", "wie", "sie", "wech\u00b7selnd", "sich", "ver\u00b7bin\u00b7den", "und", "be\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "PRF", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siehst du unendliche Verh\u00e4ltnisse entspringen.", "tokens": ["Siehst", "du", "un\u00b7end\u00b7li\u00b7che", "Ver\u00b7h\u00e4lt\u00b7nis\u00b7se", "ent\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch unersch\u00fctterlich auf den vier Pfeilern steht", "tokens": ["Doch", "un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7lich", "auf", "den", "vier", "Pfei\u00b7lern", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "CARD", "NN", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Mathematik Bau in kalter Majest\u00e4t.", "tokens": ["Der", "Ma\u00b7the\u00b7ma\u00b7tik", "Bau", "in", "kal\u00b7ter", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dieselbe Grundlag' h\u00e4lt und tr\u00e4gt den Bau der Welt;", "tokens": ["Die\u00b7sel\u00b7be", "Grund\u00b7lag'", "h\u00e4lt", "und", "tr\u00e4gt", "den", "Bau", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo aber ist der Geist, der selbst sie tr\u00e4gt und h\u00e4lt?", "tokens": ["Wo", "a\u00b7ber", "ist", "der", "Geist", ",", "der", "selbst", "sie", "tr\u00e4gt", "und", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}