{"textgrid.poem.47792": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Engel-Ehe", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Flederwisch und B\u00fcrste sie regiert!", "tokens": ["Wie", "Fle\u00b7der\u00b7wisch", "und", "B\u00fcrs\u00b7te", "sie", "re\u00b7giert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Glas und Ger\u00e4t, es blitzt nur alles so", "tokens": ["Glas", "und", "Ge\u00b7r\u00e4t", ",", "es", "blitzt", "nur", "al\u00b7les", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADV", "PIS", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und lacht und lebt! Nur, ach, sie selber nicht.", "tokens": ["Und", "lacht", "und", "lebt", "!", "Nur", ",", "ach", ",", "sie", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$.", "ADV", "$,", "ITJ", "$,", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr schmuck Gesicht, dem Manne ihrer Wahl,", "tokens": ["Ihr", "schmuck", "Ge\u00b7sicht", ",", "dem", "Man\u00b7ne", "ih\u00b7rer", "Wahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ihre wirtschaftliche Bahn er kreuzt,", "tokens": ["Wenn", "ih\u00b7re", "wirt\u00b7schaft\u00b7li\u00b7che", "Bahn", "er", "kreuzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gleich einer Maske h\u00e4lt sie's ihm entgegen;", "tokens": ["Gleich", "ei\u00b7ner", "Mas\u00b7ke", "h\u00e4lt", "sie's", "ihm", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und fragt er gar, so wirft sie ihm das Wort", "tokens": ["Und", "fragt", "er", "gar", ",", "so", "wirft", "sie", "ihm", "das", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als wie dem Hunde einen Knochen zu.", "tokens": ["Als", "wie", "dem", "Hun\u00b7de", "ei\u00b7nen", "Kno\u00b7chen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn er ist schuld an allem, was sie plagt,", "tokens": ["Denn", "er", "ist", "schuld", "an", "al\u00b7lem", ",", "was", "sie", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Am Trotz der M\u00e4gde, an den gro\u00dfen W\u00e4schen,", "tokens": ["Am", "Trotz", "der", "M\u00e4g\u00b7de", ",", "an", "den", "gro\u00b7\u00dfen", "W\u00e4\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Am Tagesm\u00fchsal und der N\u00e4chte Wachen,", "tokens": ["Am", "Ta\u00b7ges\u00b7m\u00fch\u00b7sal", "und", "der", "N\u00e4ch\u00b7te", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Schuld an dem schmutz'gen Pudel und den Kindern. \u2013", "tokens": ["Schuld", "an", "dem", "schmutz'\u00b7gen", "Pu\u00b7del", "und", "den", "Kin\u00b7dern", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und er? \u2013 Er wei\u00df, wenn kaum der grimme Tod", "tokens": ["Und", "er", "?", "\u2013", "Er", "wei\u00df", ",", "wenn", "kaum", "der", "grim\u00b7me", "Tod"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$.", "$(", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sein unverkennbar Mal ihm aufgepr\u00e4gt,", "tokens": ["Sein", "un\u00b7ver\u00b7kenn\u00b7bar", "Mal", "ihm", "auf\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dann wird, der doch in jedem Weibe schl\u00e4ft,", "tokens": ["Dann", "wird", ",", "der", "doch", "in", "je\u00b7dem", "Wei\u00b7be", "schl\u00e4ft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Engel auch in seinem Weib erwachen;", "tokens": ["Der", "En\u00b7gel", "auch", "in", "sei\u00b7nem", "Weib", "er\u00b7wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ihr eigen Weh bezwingend, wird sie dann,", "tokens": ["Ihr", "ei\u00b7gen", "Weh", "be\u00b7zwin\u00b7gend", ",", "wird", "sie", "dann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Was aus der Jugend S\u00fc\u00dfes ihr verblieb,", "tokens": ["Was", "aus", "der", "Ju\u00b7gend", "S\u00fc\u00b7\u00dfes", "ihr", "ver\u00b7blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Heraufbeschw\u00f6ren; leuchten wird es ihm", "tokens": ["Her\u00b7auf\u00b7be\u00b7schw\u00f6\u00b7ren", ";", "leuch\u00b7ten", "wird", "es", "ihm"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Aus ihren Augen, lind wie Sommeratem", "tokens": ["Aus", "ih\u00b7ren", "Au\u00b7gen", ",", "lind", "wie", "Som\u00b7me\u00b7ra\u00b7tem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wird dann ihr Wort zu seinem Herzen gehn. \u2013", "tokens": ["Wird", "dann", "ihr", "Wort", "zu", "sei\u00b7nem", "Her\u00b7zen", "gehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Doch w\u00e4hnet nicht, da\u00df dies ihn tr\u00f6ste! Nein,", "tokens": ["Doch", "w\u00e4h\u00b7net", "nicht", ",", "da\u00df", "dies", "ihn", "tr\u00f6s\u00b7te", "!", "Nein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PDS", "PPER", "VVFIN", "$.", "PTKANT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Den k\u00fcnft'gen Engel, greulich ha\u00dft er ihn;", "tokens": ["Den", "k\u00fcnft'\u00b7gen", "En\u00b7gel", ",", "greu\u00b7lich", "ha\u00dft", "er", "ihn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Er magert ab, er schlottert im Gebein,", "tokens": ["Er", "ma\u00b7gert", "ab", ",", "er", "schlot\u00b7tert", "im", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Er wird daran ersticken jedenfalls.", "tokens": ["Er", "wird", "da\u00b7ran", "er\u00b7sti\u00b7cken", "je\u00b7den\u00b7falls", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Doch eh ihm ganz die Kehle zugeschn\u00fcrt,", "tokens": ["Doch", "eh", "ihm", "ganz", "die", "Keh\u00b7le", "zu\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Mu\u00df er sein Weib in Himmelsglorie sehn;", "tokens": ["Mu\u00df", "er", "sein", "Weib", "in", "Him\u00b7mels\u00b7glo\u00b7rie", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die Rede, die er br\u00fctend ausstudiert,", "tokens": ["Die", "Re\u00b7de", ",", "die", "er", "br\u00fc\u00b7tend", "aus\u00b7stu\u00b7diert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Womit vor seinem letzten Atemzug,", "tokens": ["Wo\u00b7mit", "vor", "sei\u00b7nem", "letz\u00b7ten", "A\u00b7tem\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Jedwedes Wort ein Schwert, auf einen Schlag", "tokens": ["Jed\u00b7we\u00b7des", "Wort", "ein", "Schwert", ",", "auf", "ei\u00b7nen", "Schlag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Er alles Ungemach ihr hat vergelten wollen,", "tokens": ["Er", "al\u00b7les", "Un\u00b7ge\u00b7mach", "ihr", "hat", "ver\u00b7gel\u00b7ten", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PPER", "VAFIN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Er wird sie nimmer halten; Segenstammeln", "tokens": ["Er", "wird", "sie", "nim\u00b7mer", "hal\u00b7ten", ";", "Se\u00b7gen\u00b7stam\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wird noch von seinen toten Lippen fliehn.", "tokens": ["Wird", "noch", "von", "sei\u00b7nen", "to\u00b7ten", "Lip\u00b7pen", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Das alles wei\u00df er, und es macht ihn toll;", "tokens": ["Das", "al\u00b7les", "wei\u00df", "er", ",", "und", "es", "macht", "ihn", "toll", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Er geht umher und fluchet innerlich.", "tokens": ["Er", "geht", "um\u00b7her", "und", "flu\u00b7chet", "in\u00b7ner\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Ja, manches Mal im hellsten Sonnenschein", "tokens": ["Ja", ",", "man\u00b7ches", "Mal", "im", "hells\u00b7ten", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Durchf\u00e4hrt es ihn, als st\u00fcrz er in das Grab.", "tokens": ["Durch\u00b7f\u00e4hrt", "es", "ihn", ",", "als", "st\u00fcrz", "er", "in", "das", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Es war sein Weib, sie sprach ein sanftes Wort;", "tokens": ["Es", "war", "sein", "Weib", ",", "sie", "sprach", "ein", "sanf\u00b7tes", "Wort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und zitternd blickt er auf: \u00bbOh, Gott sei Dank,", "tokens": ["Und", "zit\u00b7ternd", "blickt", "er", "auf", ":", "\u00bb", "Oh", ",", "Gott", "sei", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "ITJ", "$,", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Noch nicht, noch nicht das Engelsangesicht!\u00ab", "tokens": ["Noch", "nicht", ",", "noch", "nicht", "das", "En\u00b7gel\u00b7san\u00b7ge\u00b7sicht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PTKNEG", "$,", "ADV", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}