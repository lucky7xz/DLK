{"textgrid.poem.34053": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Venedig", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein verwirklichter Traum begr\u00fc\u00dft dich das bunte Venedig,", "tokens": ["Wie", "ein", "ver\u00b7wirk\u00b7lich\u00b7ter", "Traum", "be\u00b7gr\u00fc\u00dft", "dich", "das", "bun\u00b7te", "Ve\u00b7ne\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wenn du es fl\u00fcchtig durchschiffst: nicht die versunkene Stadt", "tokens": ["Wenn", "du", "es", "fl\u00fcch\u00b7tig", "durch\u00b7schiffst", ":", "nicht", "die", "ver\u00b7sun\u00b7ke\u00b7ne", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$.", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Glaubst du vor dir zu sehen, von welcher die Dichter erz\u00e4hlen,", "tokens": ["Glaubst", "du", "vor", "dir", "zu", "se\u00b7hen", ",", "von", "wel\u00b7cher", "die", "Dich\u00b7ter", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Diese d\u00fcnkt dir im Meer gleich von Tritonen erbaut,", "tokens": ["Die\u00b7se", "d\u00fcnkt", "dir", "im", "Meer", "gleich", "von", "Tri\u00b7to\u00b7nen", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Und du taumelst dahin, wie unter Korallen und Muscheln,", "tokens": ["Und", "du", "tau\u00b7melst", "da\u00b7hin", ",", "wie", "un\u00b7ter", "Ko\u00b7ral\u00b7len", "und", "Mu\u00b7scheln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "$,", "PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und verwunderst dich nur, da\u00df dich die Flut nicht ereilt.", "tokens": ["Und", "ver\u00b7wun\u00b7derst", "dich", "nur", ",", "da\u00df", "dich", "die", "Flut", "nicht", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Alles Uebrige pa\u00dft hinein in den Rahmen: der Doge,", "tokens": ["Al\u00b7les", "Ueb\u00b7ri\u00b7ge", "pa\u00dft", "hin\u00b7ein", "in", "den", "Rah\u00b7men", ":", "der", "Do\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "ART", "NN", "$,"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Der sich den Wellen verm\u00e4hlt, und das vermummte Gericht,", "tokens": ["Der", "sich", "den", "Wel\u00b7len", "ver\u00b7m\u00e4hlt", ",", "und", "das", "ver\u00b7mumm\u00b7te", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVPP", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Ja die Br\u00fccke der Seufzer, erscheinen dir hier so nat\u00fcrlich,", "tokens": ["Ja", "die", "Br\u00fc\u00b7cke", "der", "Seuf\u00b7zer", ",", "er\u00b7schei\u00b7nen", "dir", "hier", "so", "na\u00b7t\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Wie in des Oceans Nacht Fische mit S\u00e4gen im Haupt.", "tokens": ["Wie", "in", "des", "O\u00b7ceans", "Nacht", "Fi\u00b7sche", "mit", "S\u00e4\u00b7gen", "im", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "La\u00df dir aber vom F\u00fchrer berichten, wie Alles entstanden,", "tokens": ["La\u00df", "dir", "a\u00b7ber", "vom", "F\u00fch\u00b7rer", "be\u00b7rich\u00b7ten", ",", "wie", "Al\u00b7les", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Und das phantastische Bild l\u00f6s't in Vernunft sich dir auf!", "tokens": ["Und", "das", "phan\u00b7tas\u00b7ti\u00b7sche", "Bild", "l\u00f6s't", "in", "Ver\u00b7nunft", "sich", "dir", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PRF", "PPER", "PTKVZ", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}}}}}