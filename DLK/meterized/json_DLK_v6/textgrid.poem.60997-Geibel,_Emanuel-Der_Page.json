{"textgrid.poem.60997": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Page", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich nun entsagen m\u00fcssen", "tokens": ["Da", "ich", "nun", "ent\u00b7sa\u00b7gen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allem, was mein Herz erbeten,", "tokens": ["Al\u00b7lem", ",", "was", "mein", "Herz", "er\u00b7be\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich diese Schwelle k\u00fcssen,", "tokens": ["La\u00df", "mich", "die\u00b7se", "Schwel\u00b7le", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dein sch\u00f6ner Fu\u00df betreten.", "tokens": ["Die", "dein", "sch\u00f6\u00b7ner", "Fu\u00df", "be\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Darf ich auch als Ritter nimmer", "tokens": ["Darf", "ich", "auch", "als", "Rit\u00b7ter", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "KOUS", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir begl\u00fcckt zur Seite schreiten,", "tokens": ["Dir", "be\u00b7gl\u00fcckt", "zur", "Sei\u00b7te", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich doch als Pagen immer", "tokens": ["La\u00df", "mich", "doch", "als", "Pa\u00b7gen", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "KOUS", "NN", "ADV"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "In die Messe dich begleiten.", "tokens": ["In", "die", "Mes\u00b7se", "dich", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will ja treu sein und verschwiegen,", "tokens": ["Will", "ja", "treu", "sein", "und", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tags dem kleinsten Winke lauschen,", "tokens": ["Tags", "dem", "kleins\u00b7ten", "Win\u00b7ke", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nachts auf deiner Schwelle liegen,", "tokens": ["Nachts", "auf", "dei\u00b7ner", "Schwel\u00b7le", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag auch Sturm und Hagel rauschen;", "tokens": ["Mag", "auch", "Sturm", "und", "Ha\u00b7gel", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Will dir stets mit sitt'gen Gr\u00fc\u00dfen", "tokens": ["Will", "dir", "stets", "mit", "sitt'\u00b7gen", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgens frische Rosen bringen,", "tokens": ["Mor\u00b7gens", "fri\u00b7sche", "Ro\u00b7sen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will des Abends dir zu F\u00fc\u00dfen", "tokens": ["Will", "des", "A\u00b7bends", "dir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieder zur Gitarre singen;", "tokens": ["Lie\u00b7der", "zur", "Gi\u00b7tar\u00b7re", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Will den wei\u00dfen Renner z\u00e4umen,", "tokens": ["Will", "den", "wei\u00b7\u00dfen", "Ren\u00b7ner", "z\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn's dich l\u00fcstet frisch zu jagen,", "tokens": ["Wenn's", "dich", "l\u00fcs\u00b7tet", "frisch", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will dir in des Waldes R\u00e4umen", "tokens": ["Will", "dir", "in", "des", "Wal\u00b7des", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Dienend Speer und Falken tragen;", "tokens": ["Die\u00b7nend", "Speer", "und", "Fal\u00b7ken", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Will auf deinen Liebeswegen", "tokens": ["Will", "auf", "dei\u00b7nen", "Lie\u00b7bes\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst den Fackeltr\u00e4ger machen", "tokens": ["Selbst", "den", "Fa\u00b7ckel\u00b7tr\u00e4\u00b7ger", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am Tor mit blankem Degen,", "tokens": ["Und", "am", "Tor", "mit", "blan\u00b7kem", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn den Freund du k\u00fcssest, wachen.", "tokens": ["Wenn", "den", "Freund", "du", "k\u00fcs\u00b7sest", ",", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und das alles ohne Klage,", "tokens": ["Und", "das", "al\u00b7les", "oh\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Flehn, nicht laut noch leise,", "tokens": ["Oh\u00b7ne", "Flehn", ",", "nicht", "laut", "noch", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKNEG", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mir nach vollbrachtem Tage", "tokens": ["Wenn", "mir", "nach", "voll\u00b7brach\u00b7tem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein L\u00e4cheln wird zum Preise;", "tokens": ["Nur", "ein", "L\u00e4\u00b7cheln", "wird", "zum", "Prei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn gleich einem Segenssterne,", "tokens": ["Wenn", "gleich", "ei\u00b7nem", "Se\u00b7gens\u00b7ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mein ganzes Wesen lenket,", "tokens": ["Der", "mein", "gan\u00b7zes", "We\u00b7sen", "len\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur dein Aug' aus weiter Ferne", "tokens": ["Nur", "dein", "Aug'", "aus", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen einz'gen Strahl mir schenket.", "tokens": ["Ei\u00b7nen", "einz'\u00b7gen", "Strahl", "mir", "schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}