{"textgrid.poem.57673": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Lustgeschmetter", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldene Knospenh\u00fclle sch\u00fctten", "tokens": ["Gol\u00b7de\u00b7ne", "Knos\u00b7pen\u00b7h\u00fcl\u00b7le", "sch\u00fct\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "All die jungen Buchenbl\u00e4tter,", "tokens": ["All", "die", "jun\u00b7gen", "Bu\u00b7chen\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den ganzen Wald durchjubelt", "tokens": ["Und", "den", "gan\u00b7zen", "Wald", "durch\u00b7ju\u00b7belt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebessang und Lustgeschmetter.", "tokens": ["Lie\u00b7bes\u00b7sang", "und", "Lust\u00b7ge\u00b7schmet\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Um die wei\u00dfen Sterngrasblumen", "tokens": ["Um", "die", "wei\u00b7\u00dfen", "Stern\u00b7gras\u00b7blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzen goldne Schmetterlinge,", "tokens": ["Tan\u00b7zen", "gold\u00b7ne", "Schmet\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und um jede kleine Bl\u00fcte", "tokens": ["Und", "um", "je\u00b7de", "klei\u00b7ne", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht ein summendes Geklinge.", "tokens": ["Geht", "ein", "sum\u00b7men\u00b7des", "Ge\u00b7klin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lachend fa\u00df ich deine H\u00fcfte,", "tokens": ["La\u00b7chend", "fa\u00df", "ich", "dei\u00b7ne", "H\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab so lange dursten m\u00fcssen,", "tokens": ["Hab", "so", "lan\u00b7ge", "durs\u00b7ten", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lange lange lange Jahre,", "tokens": ["Lan\u00b7ge", "lan\u00b7ge", "lan\u00b7ge", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach so sehr, nach deinen K\u00fcssen.", "tokens": ["Ach", "so", "sehr", ",", "nach", "dei\u00b7nen", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}