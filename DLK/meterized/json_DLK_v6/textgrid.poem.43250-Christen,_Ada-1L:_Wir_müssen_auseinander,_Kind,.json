{"textgrid.poem.43250": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir m\u00fcssen auseinander, Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen auseinander, Kind,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "aus\u00b7ein\u00b7an\u00b7der", ",", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich dr\u00e4ngt es in die Fluth hinaus.", "tokens": ["Mich", "dr\u00e4ngt", "es", "in", "die", "Fluth", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Still wie Dein Wesen war Dein Haus,", "tokens": ["Still", "wie", "Dein", "We\u00b7sen", "war", "Dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sehne mich nach Sturm und Wind.", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "Sturm", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bekr\u00e4nze doch Dein Flammenhaar,", "tokens": ["Be\u00b7kr\u00e4n\u00b7ze", "doch", "Dein", "Flam\u00b7men\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sing' einem Anderen Dein Lied;", "tokens": ["Sing'", "ei\u00b7nem", "An\u00b7de\u00b7ren", "Dein", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der heute ruhig von Dir schied,", "tokens": ["Der", "heu\u00b7te", "ru\u00b7hig", "von", "Dir", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ging wie er kam, der Liebe bar.", "tokens": ["Ging", "wie", "er", "kam", ",", "der", "Lie\u00b7be", "bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}