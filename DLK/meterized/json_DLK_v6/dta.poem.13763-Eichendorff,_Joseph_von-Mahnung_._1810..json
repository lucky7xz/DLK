{"dta.poem.13763": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Weich, aufgelegt zu Lust und fr\u00f6hlichem Dichten,", "tokens": ["Weich", ",", "auf\u00b7ge\u00b7legt", "zu", "Lust", "und", "fr\u00f6h\u00b7li\u00b7chem", "Dich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "M\u00f6cht' er so gern sich mit der Welt vertragen,", "tokens": ["M\u00f6cht'", "er", "so", "gern", "sich", "mit", "der", "Welt", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch, Rache fordernd, aus den leichten Tagen", "tokens": ["Doch", ",", "Ra\u00b7che", "for\u00b7dernd", ",", "aus", "den", "leich\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sieht er der V\u00e4ter Geist sich stets aufrichten.", "tokens": ["Sieht", "er", "der", "V\u00e4\u00b7ter", "Geist", "sich", "stets", "auf\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}