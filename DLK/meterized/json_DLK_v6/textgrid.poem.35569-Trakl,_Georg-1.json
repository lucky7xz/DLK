{"textgrid.poem.35569": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vollkommen ist die Stille dieses goldenen Tags.", "tokens": ["Voll\u00b7kom\u00b7men", "ist", "die", "Stil\u00b7le", "die\u00b7ses", "gol\u00b7de\u00b7nen", "Tags", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Unter alten Eichen", "tokens": ["Un\u00b7ter", "al\u00b7ten", "Ei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Erscheinst du, Elis, ein Ruhender mit runden Augen.", "tokens": ["Er\u00b7scheinst", "du", ",", "E\u00b7lis", ",", "ein", "Ru\u00b7hen\u00b7der", "mit", "run\u00b7den", "Au\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihre Bl\u00e4ue spiegelt den Schlummer der Liebenden.", "tokens": ["Ih\u00b7re", "Bl\u00e4u\u00b7e", "spie\u00b7gelt", "den", "Schlum\u00b7mer", "der", "Lie\u00b7ben\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "An deinem Mund", "tokens": ["An", "dei\u00b7nem", "Mund"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verstummten ihre rosigen Seufzer.", "tokens": ["Ver\u00b7stumm\u00b7ten", "ih\u00b7re", "ro\u00b7si\u00b7gen", "Seuf\u00b7zer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Am Abend zog der Fischer die schweren Netze ein.", "tokens": ["Am", "A\u00b7bend", "zog", "der", "Fi\u00b7scher", "die", "schwe\u00b7ren", "Net\u00b7ze", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein guter Hirt", "tokens": ["Ein", "gu\u00b7ter", "Hirt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "F\u00fchrt seine Herde am Waldsaum hin.", "tokens": ["F\u00fchrt", "sei\u00b7ne", "Her\u00b7de", "am", "Wald\u00b7saum", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "O! wie gerecht sind, Elis, alle deine Tage.", "tokens": ["O", "!", "wie", "ge\u00b7recht", "sind", ",", "E\u00b7lis", ",", "al\u00b7le", "dei\u00b7ne", "Ta\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VAFIN", "$,", "NE", "$,", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Leise sinkt", "tokens": ["Lei\u00b7se", "sinkt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "An kahlen Mauern des \u00d6lbaums blaue Stille,", "tokens": ["An", "kah\u00b7len", "Mau\u00b7ern", "des", "\u00d6l\u00b7baums", "blau\u00b7e", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erstirbt eines Greisen dunkler Gesang.", "tokens": ["Er\u00b7stirbt", "ei\u00b7nes", "Grei\u00b7sen", "dunk\u00b7ler", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein goldener Kahn", "tokens": ["Ein", "gol\u00b7de\u00b7ner", "Kahn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Schaukelt, Elis, dein Herz am einsamen Himmel.", "tokens": ["Schau\u00b7kelt", ",", "E\u00b7lis", ",", "dein", "Herz", "am", "ein\u00b7sa\u00b7men", "Him\u00b7mel", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}