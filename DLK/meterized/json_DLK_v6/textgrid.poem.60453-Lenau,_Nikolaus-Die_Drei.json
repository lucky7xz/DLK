{"textgrid.poem.60453": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Drei", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Reiter nach verlorner Schlacht,", "tokens": ["Drei", "Rei\u00b7ter", "nach", "ver\u00b7lor\u00b7ner", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie reiten sie so sacht, so sacht!", "tokens": ["Wie", "rei\u00b7ten", "sie", "so", "sacht", ",", "so", "sacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus tiefen Wunden quillt das Blut,", "tokens": ["Aus", "tie\u00b7fen", "Wun\u00b7den", "quillt", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sp\u00fcrt das Ro\u00df die warme Flut.", "tokens": ["Es", "sp\u00fcrt", "das", "Ro\u00df", "die", "war\u00b7me", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vom Sattel tropft das Blut, vom Zaum,", "tokens": ["Vom", "Sat\u00b7tel", "tropft", "das", "Blut", ",", "vom", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sp\u00fclt hinunter Staub und Schaum.", "tokens": ["Und", "sp\u00fclt", "hin\u00b7un\u00b7ter", "Staub", "und", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Rosse schreiten sanft und weich,", "tokens": ["Die", "Ros\u00b7se", "schrei\u00b7ten", "sanft", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst fl\u00f6\u00df das Blut zu rasch, zu reich.", "tokens": ["Sonst", "fl\u00f6\u00df", "das", "Blut", "zu", "rasch", ",", "zu", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Reiter reiten dicht gesellt,", "tokens": ["Die", "Rei\u00b7ter", "rei\u00b7ten", "dicht", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und einer sich am andern h\u00e4lt.", "tokens": ["Und", "ei\u00b7ner", "sich", "am", "an\u00b7dern", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie sehn sich traurig ins Gesicht,", "tokens": ["Sie", "sehn", "sich", "trau\u00b7rig", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und einer um den andern spricht:", "tokens": ["Und", "ei\u00b7ner", "um", "den", "an\u00b7dern", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbmir bl\u00fcht daheim die sch\u00f6nste Maid,", "tokens": ["\u00bb", "mir", "bl\u00fcht", "da\u00b7heim", "die", "sch\u00f6ns\u00b7te", "Maid", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum tut mein fr\u00fcher Tod mir leid.\u00ab", "tokens": ["Drum", "tut", "mein", "fr\u00fc\u00b7her", "Tod", "mir", "leid", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbhab Haus und Hof und gr\u00fcnen Wald,", "tokens": ["\u00bb", "hab", "Haus", "und", "Hof", "und", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sterben mu\u00df ich hier so bald!\u00ab", "tokens": ["Und", "ster\u00b7ben", "mu\u00df", "ich", "hier", "so", "bald", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbden Blick hab ich in Gottes Welt,", "tokens": ["\u00bb", "den", "Blick", "hab", "ich", "in", "Got\u00b7tes", "Welt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst nichts, noch schwer mirs Sterben f\u00e4llt.\u00ab", "tokens": ["Sonst", "nichts", ",", "noch", "schwer", "mirs", "Ster\u00b7ben", "f\u00e4llt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIS", "$,", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und lauernd auf den Todesritt", "tokens": ["Und", "lau\u00b7ernd", "auf", "den", "To\u00b7des\u00b7ritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ziehn durch die Luft drei Geier mit.", "tokens": ["Ziehn", "durch", "die", "Luft", "drei", "Gei\u00b7er", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sie teilen kreischend unter sich:", "tokens": ["Sie", "tei\u00b7len", "krei\u00b7schend", "un\u00b7ter", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbden speisest du, den du, den ich.\u00ab", "tokens": ["\u00bb", "den", "spei\u00b7sest", "du", ",", "den", "du", ",", "den", "ich", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VVFIN", "PPER", "$,", "PRELS", "PPER", "$,", "PRELS", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}