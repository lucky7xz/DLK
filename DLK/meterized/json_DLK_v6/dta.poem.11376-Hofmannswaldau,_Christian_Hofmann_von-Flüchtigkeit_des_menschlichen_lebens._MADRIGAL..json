{"dta.poem.11376": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Fl\u00fcchtigkeit des menschlichen lebens.  \n  MADRIGAL.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat sonst alles seine zeit:", "tokens": ["Es", "hat", "sonst", "al\u00b7les", "sei\u00b7ne", "zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der fr\u00fchling darff nicht vor dem sommer sterben:", "tokens": ["Der", "fr\u00fch\u00b7ling", "darff", "nicht", "vor", "dem", "som\u00b7mer", "ster\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den sommer kan nichts, als der herbst, verderben:", "tokens": ["Den", "som\u00b7mer", "kan", "nichts", ",", "als", "der", "herbst", ",", "ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "$,", "KOUS", "ART", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der herbst weicht eher nicht, als bi\u00df der winter schneyt.", "tokens": ["Der", "herbst", "weicht", "e\u00b7her", "nicht", ",", "als", "bi\u00df", "der", "win\u00b7ter", "schneyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "PTKNEG", "$,", "KOUS", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein der arme mensch,", "tokens": ["Al\u00b7lein", "der", "ar\u00b7me", "mensch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der billig, wie das jahr, vier alter solte leben:", "tokens": ["Der", "bil\u00b7lig", ",", "wie", "das", "jahr", ",", "vier", "al\u00b7ter", "sol\u00b7te", "le\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "CARD", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mu\u00df geist und leben offt, eh\u2019 er gelebt, hingeben.", "tokens": ["Mu\u00df", "geist", "und", "le\u00b7ben", "offt", ",", "eh'", "er", "ge\u00b7lebt", ",", "hin\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "VVPP", "KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die kindheit stirbt, eh\u2019 sie die welt erblickt:", "tokens": ["Die", "kind\u00b7heit", "stirbt", ",", "eh'", "sie", "die", "welt", "er\u00b7blickt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die jugend, eh\u2019 sie uns l\u00e4st ihre bl\u00fcthe schauen:", "tokens": ["Die", "ju\u00b7gend", ",", "eh'", "sie", "uns", "l\u00e4st", "ih\u00b7re", "bl\u00fc\u00b7the", "schau\u00b7en", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein weib, eh\u2019 noch die frucht aus ihrem lager r\u00fcckt.", "tokens": ["Ein", "weib", ",", "eh'", "noch", "die", "frucht", "aus", "ih\u00b7rem", "la\u00b7ger", "r\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und also ist zwar allem hier zu trauen;", "tokens": ["Und", "al\u00b7so", "ist", "zwar", "al\u00b7lem", "hier", "zu", "trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nur unserm leben nicht.", "tokens": ["Nur", "un\u00b7serm", "le\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Jedoch geduld! wir \u00e4ndern nur den garten,", "tokens": ["Je\u00b7doch", "ge\u00b7duld", "!", "wir", "\u00e4n\u00b7dern", "nur", "den", "gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und unser sonnen-licht;", "tokens": ["Und", "un\u00b7ser", "son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Denn weil uns t\u00e4glich hier an pflegung was gebricht,", "tokens": ["Denn", "weil", "uns", "t\u00e4g\u00b7lich", "hier", "an", "pfle\u00b7gung", "was", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "APPR", "NN", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So kan der himmel uns bey sich am besten warten.", "tokens": ["So", "kan", "der", "him\u00b7mel", "uns", "bey", "sich", "am", "bes\u00b7ten", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "APPR", "PRF", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}}}}