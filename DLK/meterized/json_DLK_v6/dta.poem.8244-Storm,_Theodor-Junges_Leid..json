{"dta.poem.8244": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Junges Leid.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ergriff dich denn im kerzenhellen Saal,", "tokens": ["Er\u00b7griff", "dich", "denn", "im", "ker\u00b7zen\u00b7hel\u00b7len", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hinschleichend in des Tanzes Zaubert\u00f6nen,", "tokens": ["Hin\u00b7schlei\u00b7chend", "in", "des", "Tan\u00b7zes", "Zau\u00b7ber\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Niemals ein dunkler Schauer meiner Qual?", "tokens": ["Nie\u00b7mals", "ein", "dunk\u00b7ler", "Schau\u00b7er", "mei\u00b7ner", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "O f\u00fchltest du's! Nicht l\u00e4nger kann ich's tragen;", "tokens": ["O", "f\u00fchl\u00b7test", "du's", "!", "Nicht", "l\u00e4n\u00b7ger", "kann", "ich's", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "$.", "PTKNEG", "ADJD", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wei\u00dft, das ganze Leben bist du mir,", "tokens": ["Du", "wei\u00dft", ",", "das", "gan\u00b7ze", "Le\u00b7ben", "bist", "du", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Seligkeit von allen k\u00fcnftgen Tagen", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "von", "al\u00b7len", "k\u00fcnft\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meiner Jugend Zauber ruht auf dir.", "tokens": ["Und", "mei\u00b7ner", "Ju\u00b7gend", "Zau\u00b7ber", "ruht", "auf", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In meiner Liebe bist du auferzogen;", "tokens": ["In", "mei\u00b7ner", "Lie\u00b7be", "bist", "du", "auf\u00b7er\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist mein Kind \u2014 ich habe dich geliebt,", "tokens": ["Du", "bist", "mein", "Kind", "ich", "ha\u00b7be", "dich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als fessellos noch deine Locken flogen,", "tokens": ["Als", "fes\u00b7sel\u00b7los", "noch", "dei\u00b7ne", "Lo\u00b7cken", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als deine Sch\u00f6nheit noch kein Aug' getr\u00fcbt.", "tokens": ["Als", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "noch", "kein", "Aug'", "ge\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob du dich nimmer nach dem Freunde sehntest,", "tokens": ["Ob", "du", "dich", "nim\u00b7mer", "nach", "dem", "Freun\u00b7de", "sehn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Abends dir die sch\u00f6nen Lieder sang,", "tokens": ["Der", "A\u00b7bends", "dir", "die", "sch\u00f6\u00b7nen", "Lie\u00b7der", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df du stumm an seine Schulter lehntest,", "tokens": ["In\u00b7de\u00df", "du", "stumm", "an", "sei\u00b7ne", "Schul\u00b7ter", "lehn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "And\u00e4chtig lauschend in den vollen Klang?", "tokens": ["An\u00b7d\u00e4ch\u00b7tig", "lau\u00b7schend", "in", "den", "vol\u00b7len", "Klang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O f\u00fchl' es nimmer, wie Vergangnes qu\u00e4le!", "tokens": ["O", "f\u00fchl'", "es", "nim\u00b7mer", ",", "wie", "Ver\u00b7gang\u00b7nes", "qu\u00e4\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wirst du's f\u00fchlen; wei\u00df ich's doch gewi\u00df", "tokens": ["Doch", "wirst", "du's", "f\u00fch\u00b7len", ";", "wei\u00df", "ich's", "doch", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIS", "VVINF", "$.", "VVFIN", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An jedem Funken deiner, meiner Seele,", "tokens": ["An", "je\u00b7dem", "Fun\u00b7ken", "dei\u00b7ner", ",", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott gab dich mir, als er dich werden hie\u00df.", "tokens": ["Gott", "gab", "dich", "mir", ",", "als", "er", "dich", "wer\u00b7den", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PRF", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O kehr' zur\u00fcck, und wandle, was vergangen,", "tokens": ["O", "kehr'", "zu\u00b7r\u00fcck", ",", "und", "wand\u00b7le", ",", "was", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dunkle Schmerzen der Erinnerung!", "tokens": ["In", "dunk\u00b7le", "Schmer\u00b7zen", "der", "E\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Noch bl\u00fcht dein Mund, noch gl\u00fchen deine Wangen,", "tokens": ["Noch", "bl\u00fcht", "dein", "Mund", ",", "noch", "gl\u00fc\u00b7hen", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch ist mein Herz wie deines stark und jung.", "tokens": ["Noch", "ist", "mein", "Herz", "wie", "dei\u00b7nes", "stark", "und", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KOKOM", "PPOSAT", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}