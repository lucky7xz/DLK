{"textgrid.poem.42145": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An einen J\u00fcngling", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngling, la\u00df dich nicht gel\u00fcsten", "tokens": ["J\u00fcng\u00b7ling", ",", "la\u00df", "dich", "nicht", "ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach des Paradieses \u00c4pfeln;", "tokens": ["Nach", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "\u00c4p\u00b7feln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den straffsten M\u00e4dchenbr\u00fcsten", "tokens": ["Von", "den", "straffs\u00b7ten", "M\u00e4d\u00b7chen\u00b7br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dir nichts als Kummer tr\u00f6pfeln.", "tokens": ["Wird", "dir", "nichts", "als", "Kum\u00b7mer", "tr\u00f6p\u00b7feln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wagst du dich heran und findst du", "tokens": ["Wagst", "du", "dich", "he\u00b7ran", "und", "findst", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lust an diesen wei\u00dfen Teufeln,", "tokens": ["Lust", "an", "die\u00b7sen", "wei\u00b7\u00dfen", "Teu\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Armer Freund, wie bald beginnst du", "tokens": ["Ar\u00b7mer", "Freund", ",", "wie", "bald", "be\u00b7ginnst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADV", "VVFIN", "PPER"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Selbst von Traurigkeit zu tr\u00e4ufeln.", "tokens": ["Selbst", "von", "Trau\u00b7rig\u00b7keit", "zu", "tr\u00e4u\u00b7feln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Just die K\u00fchnsten, Elegantsten", "tokens": ["Just", "die", "K\u00fchns\u00b7ten", ",", "E\u00b7leg\u00b7ants\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden fr\u00fch zu m\u00fcden Kr\u00fcppeln,", "tokens": ["Wer\u00b7den", "fr\u00fch", "zu", "m\u00fc\u00b7den", "Kr\u00fcp\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die einst am flottsten tanzten,", "tokens": ["Und", "die", "einst", "am", "flotts\u00b7ten", "tanz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "M\u00fcssen lahm zur Grube trippeln.", "tokens": ["M\u00fcs\u00b7sen", "lahm", "zur", "Gru\u00b7be", "trip\u00b7peln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}