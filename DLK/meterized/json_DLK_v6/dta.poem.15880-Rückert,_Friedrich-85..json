{"dta.poem.15880": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du den armen Mann beschenkt hast mild und g\u00fctig,", "tokens": ["Wenn", "du", "den", "ar\u00b7men", "Mann", "be\u00b7schenkt", "hast", "mild", "und", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wend' auch von seinem Dank dich dann nicht ab hochm\u00fcthig.", "tokens": ["Wend'", "auch", "von", "sei\u00b7nem", "Dank", "dich", "dann", "nicht", "ab", "hoch\u00b7m\u00fct\u00b7hig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "PPER", "ADV", "PTKNEG", "PTKVZ", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zehn, hundert, tausendfach w\u00fcnscht er dir Gottes Lohn,", "tokens": ["Zehn", ",", "hun\u00b7dert", ",", "tau\u00b7send\u00b7fach", "w\u00fcnscht", "er", "dir", "Got\u00b7tes", "Lohn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "CARD", "$,", "ADV", "VVFIN", "PPER", "PPER", "NN", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gibt mehr dir, als du ihm, la\u00df ihm den Stolz, o Sohn,", "tokens": ["Gibt", "mehr", "dir", ",", "als", "du", "ihm", ",", "la\u00df", "ihm", "den", "Stolz", ",", "o", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "KOUS", "PPER", "PPER", "$,", "VVIMP", "PPER", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und geh statt seiner selbst als Schuldner du davon!", "tokens": ["Und", "geh", "statt", "sei\u00b7ner", "selbst", "als", "Schuld\u00b7ner", "du", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADV", "KOUS", "NN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}