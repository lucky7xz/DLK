{"textgrid.poem.47007": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' es wohl, was hilft mir, da\u00df ich's sehe?", "tokens": ["Ich", "seh'", "es", "wohl", ",", "was", "hilft", "mir", ",", "da\u00df", "ich's", "se\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Vater, Mutter, alle deine Leute,", "tokens": ["Da\u00df", "Va\u00b7ter", ",", "Mut\u00b7ter", ",", "al\u00b7le", "dei\u00b7ne", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl wissend, was mein Gehn und Kommen deute,", "tokens": ["Wohl", "wis\u00b7send", ",", "was", "mein", "Gehn", "und", "Kom\u00b7men", "deu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch freundlich drein sehn, wenn ich komm' und gehe.", "tokens": ["Doch", "freund\u00b7lich", "drein", "sehn", ",", "wenn", "ich", "komm'", "und", "ge\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch seh' ich auch, o weh mir, da\u00df ich's sehe,", "tokens": ["Doch", "seh'", "ich", "auch", ",", "o", "weh", "mir", ",", "da\u00df", "ich's", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "FM", "ADV", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du, viel schlauer zwar als all die Leute,", "tokens": ["Da\u00df", "du", ",", "viel", "schlau\u00b7er", "zwar", "als", "all", "die", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "ADV", "KOKOM", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nicht willst wissen, was mein Kommen deute,", "tokens": ["Doch", "nicht", "willst", "wis\u00b7sen", ",", "was", "mein", "Kom\u00b7men", "deu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und freundlich drein siehst stets nur, wann ich gehe.", "tokens": ["Und", "freund\u00b7lich", "drein", "siehst", "stets", "nur", ",", "wann", "ich", "ge\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Ich wollt', ich k\u00f6nnt' es ihnen all erlassen,", "tokens": ["Ich", "wollt'", ",", "ich", "k\u00f6nnt'", "es", "ih\u00b7nen", "all", "er\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "PPER", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, wenn ich k\u00fcnftig k\u00e4me, mein Willkommen", "tokens": ["Da\u00df", ",", "wenn", "ich", "k\u00fcnf\u00b7tig", "k\u00e4\u00b7me", ",", "mein", "Will\u00b7kom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Niemand mehr rief', als du im Herzensgrunde.", "tokens": ["Nie\u00b7mand", "mehr", "rie\u00b7f'", ",", "als", "du", "im", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn du mich liebtest, m\u00f6chten sie mich hassen;", "tokens": ["Wenn", "du", "mich", "lieb\u00b7test", ",", "m\u00f6ch\u00b7ten", "sie", "mich", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du mich hassest, kann mir's wenig frommen,", "tokens": ["Wenn", "du", "mich", "has\u00b7sest", ",", "kann", "mir's", "we\u00b7nig", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "NE", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob all die Welt mich lieb hat in die Runde.", "tokens": ["Ob", "all", "die", "Welt", "mich", "lieb", "hat", "in", "die", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "PPER", "ADJD", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}