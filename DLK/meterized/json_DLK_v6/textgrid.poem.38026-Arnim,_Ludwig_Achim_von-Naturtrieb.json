{"textgrid.poem.38026": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Naturtrieb", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die goldnen Bienlein schweben", "tokens": ["Wie", "die", "gold\u00b7nen", "Bien\u00b7lein", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf der bunten Blumenfahrt,", "tokens": ["Auf", "der", "bun\u00b7ten", "Blu\u00b7men\u00b7fahrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hundert tausend K\u00fc\u00dfe geben", "tokens": ["Hun\u00b7dert", "tau\u00b7send", "K\u00fc\u00b7\u00dfe", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "CARD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "All den Kr\u00e4utlein mancher Art,", "tokens": ["All", "den", "Kr\u00e4ut\u00b7lein", "man\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So in meines Herzens Grunde", "tokens": ["So", "in", "mei\u00b7nes", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt es mich, nach deinem Munde,", "tokens": ["Treibt", "es", "mich", ",", "nach", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Spei\u00df und Wein,", "tokens": ["Spei\u00df", "und", "Wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "K\u00fc\u00df und Freude,", "tokens": ["K\u00fc\u00df", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mehrt die Pein,", "tokens": ["Mehrt", "die", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Die ich leide,", "tokens": ["Die", "ich", "lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Ohne dich, mein Leben!", "tokens": ["Oh\u00b7ne", "dich", ",", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Durch Umfangen", "tokens": ["Durch", "Um\u00b7fan\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Stillt dein Mund", "tokens": ["Stillt", "dein", "Mund"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mein Verlangen,", "tokens": ["Mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Bin ich wund,", "tokens": ["Bin", "ich", "wund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Kannst du mir Gesundheit geben.", "tokens": ["Kannst", "du", "mir", "Ge\u00b7sund\u00b7heit", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}