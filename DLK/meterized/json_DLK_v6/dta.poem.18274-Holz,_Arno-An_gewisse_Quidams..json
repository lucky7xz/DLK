{"dta.poem.18274": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An gewisse Quidams.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr geberdet euch t\u00e4glich entz\u00fcckter;", "tokens": ["Ihr", "ge\u00b7ber\u00b7det", "euch", "t\u00e4g\u00b7lich", "ent\u00b7z\u00fcck\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Drum flucht nur, er ist uns nichts weiter, als", "tokens": ["Drum", "flucht", "nur", ",", "er", "ist", "uns", "nichts", "wei\u00b7ter", ",", "als"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PPER", "PIS", "PTKVZ", "$,", "KOUS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein verr\u00fcckt gewordner Verr\u00fcckter!", "tokens": ["Ein", "ver\u00b7r\u00fcckt", "ge\u00b7word\u00b7ner", "Ver\u00b7r\u00fcck\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und der Teufel kommt gleich, euch zu holen,", "tokens": ["Und", "der", "Teu\u00b7fel", "kommt", "gleich", ",", "euch", "zu", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denn ich habe noch nie eine Jungfer beschwatzt", "tokens": ["Denn", "ich", "ha\u00b7be", "noch", "nie", "ei\u00b7ne", "Jung\u00b7fer", "be\u00b7schwatzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und silberne L\u00f6ffel gestohlen!", "tokens": ["Und", "sil\u00b7ber\u00b7ne", "L\u00f6f\u00b7fel", "ge\u00b7stoh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}