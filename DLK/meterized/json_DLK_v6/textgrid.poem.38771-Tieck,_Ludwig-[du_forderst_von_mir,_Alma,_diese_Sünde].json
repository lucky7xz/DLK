{"textgrid.poem.38771": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[du forderst von mir, Alma, diese S\u00fcnde]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du forderst von mir, Alma, diese S\u00fcnde,", "tokens": ["Du", "for\u00b7derst", "von", "mir", ",", "Al\u00b7ma", ",", "die\u00b7se", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$,", "NE", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein heilges Wort, das Trauer durfte sprechen,", "tokens": ["Ein", "heil\u00b7ges", "Wort", ",", "das", "Trau\u00b7er", "durf\u00b7te", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir Trost zugleich, muthwillig zu zerbrechen,", "tokens": ["Mir", "Trost", "zu\u00b7gleich", ",", "mut\u00b7hwil\u00b7lig", "zu", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein sehnend Leid, wie Fr\u00fchlingsluft gelinde.", "tokens": ["Ein", "seh\u00b7nend", "Leid", ",", "wie", "Fr\u00fch\u00b7lings\u00b7luft", "ge\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PWAV", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie das Lied ich sinnend l\u00f6s' und binde,", "tokens": ["Und", "wie", "das", "Lied", "ich", "sin\u00b7nend", "l\u00f6s'", "und", "bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erscheint mir mein Beginnen ein Erfrechen,", "tokens": ["Er\u00b7scheint", "mir", "mein", "Be\u00b7gin\u00b7nen", "ein", "Er\u00b7fre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder Laut will meine Seele stechen,", "tokens": ["Ein", "je\u00b7der", "Laut", "will", "mei\u00b7ne", "See\u00b7le", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie wenn ich kalt vor diesen Thr\u00e4nen st\u00fcnde.", "tokens": ["Wie", "wenn", "ich", "kalt", "vor", "die\u00b7sen", "Thr\u00e4\u00b7nen", "st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So mu\u00df ich mit Gewalt mich selbst bezwingen;", "tokens": ["So", "mu\u00df", "ich", "mit", "Ge\u00b7walt", "mich", "selbst", "be\u00b7zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erst r\u00fchrte mich der Inhalt dieser Klagen,", "tokens": ["Erst", "r\u00fchr\u00b7te", "mich", "der", "In\u00b7halt", "die\u00b7ser", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt mehr, da\u00df ich gef\u00fchllos sie soll meistern.", "tokens": ["Jetzt", "mehr", ",", "da\u00df", "ich", "ge\u00b7f\u00fchl\u00b7los", "sie", "soll", "meis\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So mu\u00df ich sprechend auch zu Boden ringen,", "tokens": ["So", "mu\u00df", "ich", "spre\u00b7chend", "auch", "zu", "Bo\u00b7den", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was immer dir die Seele m\u00f6chte sagen,", "tokens": ["Was", "im\u00b7mer", "dir", "die", "See\u00b7le", "m\u00f6ch\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur stummes Weinen bebt in allen Geistern.", "tokens": ["Nur", "stum\u00b7mes", "Wei\u00b7nen", "bebt", "in", "al\u00b7len", "Geis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}