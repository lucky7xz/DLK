{"textgrid.poem.24590": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Antwort in Nahmen der ersten S\u00e4ngerin", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die bey den Heyden sonst als ", "tokens": ["Die", "bey", "den", "Hey\u00b7den", "sonst", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die d\u00fcrffen Christen itzt vor Ehrbarkeit nicht nennen.", "tokens": ["Die", "d\u00fcrf\u00b7fen", "Chris\u00b7ten", "itzt", "vor", "Ehr\u00b7bar\u00b7keit", "nicht", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da Engel aber mehr und unvergleichlich seyn/", "tokens": ["Da", "En\u00b7gel", "a\u00b7ber", "mehr", "und", "un\u00b7ver\u00b7gleich\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da simmet alle Welt/ ja selbst der Himmel ein.", "tokens": ["Da", "sim\u00b7met", "al\u00b7le", "Welt", "/", "ja", "selbst", "der", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$(", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}