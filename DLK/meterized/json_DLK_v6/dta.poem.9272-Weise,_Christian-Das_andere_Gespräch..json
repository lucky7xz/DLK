{"dta.poem.9272": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das andere Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.71", "tl:0.14", "en:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Was hat man sonst davon/ als da\u00df man lustig ist/", "tokens": ["Was", "hat", "man", "sonst", "da\u00b7von", "/", "als", "da\u00df", "man", "lus\u00b7tig", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "ADV", "PAV", "$(", "KOKOM", "KOUS", "PIS", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bey der compagnie der andern noth vergist/", "tokens": ["Und", "bey", "der", "com\u00b7pag\u00b7nie", "der", "an\u00b7dern", "noth", "ver\u00b7gist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit sauer sehn und sorgen", "tokens": ["Mit", "sau\u00b7er", "sehn", "und", "sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "VVINF", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird selten was gethan/", "tokens": ["Wird", "sel\u00b7ten", "was", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PWS", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl dem/ der sie auff morgen", "tokens": ["Wohl", "dem", "/", "der", "sie", "auff", "mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$(", "PRELS", "PPER", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Getrost verweisen kan.", "tokens": ["Ge\u00b7trost", "ver\u00b7wei\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Ein angenehmer trunck/ ein ungef\u00e4rbter freund/", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "trunck", "/", "ein", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "freund", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zwar den becher wein/ doch mich viel besser meynt/", "tokens": ["Der", "zwar", "den", "be\u00b7cher", "wein", "/", "doch", "mich", "viel", "bes\u00b7ser", "meynt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$(", "ADV", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das sind dieselben gaben/", "tokens": ["Das", "sind", "die\u00b7sel\u00b7ben", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die schleichen lieblich ein/", "tokens": ["Die", "schlei\u00b7chen", "lieb\u00b7lich", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bi\u00df wir die ladung haben", "tokens": ["Bi\u00df", "wir", "die", "la\u00b7dung", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und heute fertig seyn.", "tokens": ["Und", "heu\u00b7te", "fer\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. So geht den andern tag die freude wieder an/", "tokens": ["So", "geht", "den", "an\u00b7dern", "tag", "die", "freu\u00b7de", "wie\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bleibt die traurigkeit auch morgen ausgethan/", "tokens": ["So", "bleibt", "die", "trau\u00b7rig\u00b7keit", "auch", "mor\u00b7gen", "aus\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird der safft von reben", "tokens": ["So", "wird", "der", "safft", "von", "re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch lieb und treu vers\u00fcsst/", "tokens": ["Durch", "lieb", "und", "treu", "ver\u00b7s\u00fcsst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So f\u00fchren wir ein leben/", "tokens": ["So", "f\u00fch\u00b7ren", "wir", "ein", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das \u00fcberirrdisch ist.", "tokens": ["Das", "\u00fc\u00b7be\u00b7rirr\u00b7disch", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Wolan/ was s\u00e4umen wir? da steht der blancke wein/", "tokens": ["Wo\u00b7lan", "/", "was", "s\u00e4u\u00b7men", "wir", "?", "da", "steht", "der", "blan\u00b7cke", "wein", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWS", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da mu\u00df der liebe trunck undisputirlich seyn/", "tokens": ["Da", "mu\u00df", "der", "lie\u00b7be", "trunck", "un\u00b7dis\u00b7pu\u00b7tir\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sonst kan es nicht ergetzen/", "tokens": ["Sonst", "kan", "es", "nicht", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da mu\u00df man sich bem\u00fchn", "tokens": ["Da", "mu\u00df", "man", "sich", "be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auff einmahl anzusetzen/", "tokens": ["Auff", "ein\u00b7mahl", "an\u00b7zu\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVIZU", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auff einmahl abzuziehn.", "tokens": ["Auff", "ein\u00b7mahl", "ab\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}