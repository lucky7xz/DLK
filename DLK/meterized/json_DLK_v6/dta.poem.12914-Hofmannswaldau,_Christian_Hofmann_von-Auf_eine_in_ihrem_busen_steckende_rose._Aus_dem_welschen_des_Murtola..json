{"dta.poem.12914": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine in ihrem busen steckende  \n rose.  \n Aus dem welschen des   Murtola.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die blume, so aus deinen br\u00fcsten,", "tokens": ["Die", "blu\u00b7me", ",", "so", "aus", "dei\u00b7nen", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie die morgenr\u00f6th aus sch\u00f6nen wolcken lacht,", "tokens": ["Als", "wie", "die", "mor\u00b7gen\u00b7r\u00f6th", "aus", "sch\u00f6\u00b7nen", "wol\u00b7cken", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist zwar dem scheine nach zur ros\u2019 allein gemacht;", "tokens": ["Ist", "zwar", "dem", "schei\u00b7ne", "nach", "zur", "ros'", "al\u00b7lein", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "APPR", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich aber schau in ihr die liebe selber nisten.", "tokens": ["Ich", "a\u00b7ber", "schau", "in", "ihr", "die", "lie\u00b7be", "sel\u00b7ber", "nis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "PPER", "ART", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der helle purpur, der allhier", "tokens": ["Der", "hel\u00b7le", "pur\u00b7pur", ",", "der", "all\u00b7hier"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "$,", "PRELS", "ADV"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Die weichen alabastev-h\u00fcgel", "tokens": ["Die", "wei\u00b7chen", "a\u00b7la\u00b7bas\u00b7te\u00b7vh\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit seinem glantze cr\u00f6nt, stellt ihre fackel f\u00fcr,", "tokens": ["Mit", "sei\u00b7nem", "glant\u00b7ze", "cr\u00f6nt", ",", "stellt", "ih\u00b7re", "fa\u00b7ckel", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die dsrnen sind die pfeil, die bl\u00e4tter sind die fl\u00fcgel.", "tokens": ["Die", "dsr\u00b7nen", "sind", "die", "pfeil", ",", "die", "bl\u00e4t\u00b7ter", "sind", "die", "fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}