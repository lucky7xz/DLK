{"textgrid.poem.47441": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[so steh'n wir, ich und meine Weide]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So steh'n wir, ich und meine Weide,", "tokens": ["So", "steh'n", "wir", ",", "ich", "und", "mei\u00b7ne", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So leider mit einander beide:", "tokens": ["So", "lei\u00b7der", "mit", "ein\u00b7an\u00b7der", "bei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PRF", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie kann ich ihr was thun zu Liebe,", "tokens": ["Nie", "kann", "ich", "ihr", "was", "thun", "zu", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nie kann sie mir was thun zu Leide.", "tokens": ["Nie", "kann", "sie", "mir", "was", "thun", "zu", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit einem Diadem bekleide;", "tokens": ["Mit", "ei\u00b7nem", "Di\u00b7a\u00b7dem", "be\u00b7klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Huld, f\u00fcr ihre Zornbescheide.", "tokens": ["Der", "Huld", ",", "f\u00fcr", "ih\u00b7re", "Zorn\u00b7be\u00b7schei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}