{"textgrid.poem.39499": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O heil'ge Treue! sittsame Vestale,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O heil'ge Treue! sittsame Vestale,", "tokens": ["O", "heil'\u00b7ge", "Treu\u00b7e", "!", "sitt\u00b7sa\u00b7me", "Ves\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die auf der Seel' Altar die Flamme h\u00fctet!", "tokens": ["Die", "auf", "der", "Seel'", "Al\u00b7tar", "die", "Flam\u00b7me", "h\u00fc\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Astraea, die dem Neid des Gl\u00fccks gebietet,", "tokens": ["A\u00b7strae\u00b7a", ",", "die", "dem", "Neid", "des", "Gl\u00fccks", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Lieb' und Gegenlieb' in gleicher Schale!", "tokens": ["Mit", "Lieb'", "und", "Ge\u00b7gen\u00b7lieb'", "in", "glei\u00b7cher", "Scha\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du Flora, die im thr\u00e4nbethauten Thale", "tokens": ["Du", "Flo\u00b7ra", ",", "die", "im", "thr\u00e4n\u00b7be\u00b7thau\u00b7ten", "Tha\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens auch den Winter sch\u00f6n bebl\u00fctet!", "tokens": ["Des", "Le\u00b7bens", "auch", "den", "Win\u00b7ter", "sch\u00f6n", "be\u00b7bl\u00fc\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wenn der Tod mit harter Trennung w\u00fctet,", "tokens": ["Doch", ",", "wenn", "der", "Tod", "mit", "har\u00b7ter", "Tren\u00b7nung", "w\u00fc\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Parce mit verh\u00e4ngni\u00dfvollem Stahle!", "tokens": ["Du", "Par\u00b7ce", "mit", "ver\u00b7h\u00e4ng\u00b7ni\u00df\u00b7vol\u00b7lem", "Stah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sag', wo sind deine Wunder hingeschwunden?", "tokens": ["Sag'", ",", "wo", "sind", "dei\u00b7ne", "Wun\u00b7der", "hin\u00b7ge\u00b7schwun\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind dir zu dienen w\u00fcrdig nur Heroen,", "tokens": ["Sind", "dir", "zu", "die\u00b7nen", "w\u00fcr\u00b7dig", "nur", "He\u00b7roen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "ADJD", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und kann die schlaffe Welt nicht mehr gesunden?", "tokens": ["Und", "kann", "die", "schlaf\u00b7fe", "Welt", "nicht", "mehr", "ge\u00b7sun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du wirst vermi\u00dft an Mann, Weib, Niedern, Hohen,", "tokens": ["Du", "wirst", "ver\u00b7mi\u00dft", "an", "Mann", ",", "Weib", ",", "Nie\u00b7dern", ",", "Ho\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mancher h\u00f6hnt, nie sei'st du wahr erfunden,", "tokens": ["Und", "man\u00b7cher", "h\u00f6hnt", ",", "nie", "sei'st", "du", "wahr", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil du aus seiner falschen Brust entflohen.", "tokens": ["Weil", "du", "aus", "sei\u00b7ner", "fal\u00b7schen", "Brust", "ent\u00b7flo\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}