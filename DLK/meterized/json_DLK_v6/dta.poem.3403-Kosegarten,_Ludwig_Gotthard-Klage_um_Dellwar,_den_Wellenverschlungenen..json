{"dta.poem.3403": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Klage  \n um Dellwar, den Wellenverschlungenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Luftgebilde, das nebelumg&#252;rtet mit triefenden", "tokens": ["Luft\u00b7ge\u00b7bil\u00b7de", ",", "das", "ne\u00b7be\u00b7lumg", "&#252;", "rtet", "mit", "trie\u00b7fen\u00b7den"], "token_info": ["word", "punct", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "$(", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+--++-+--", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Locken", "tokens": ["Lo\u00b7cken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und mit rettungflehenden H\u00e4nden vor\u00fcber mir", "tokens": ["Und", "mit", "ret\u00b7tun\u00b7gfle\u00b7hen\u00b7den", "H\u00e4n\u00b7den", "vor\u00b7\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPER"], "meter": "----+--+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "wandelt,", "tokens": ["wan\u00b7delt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Rede, wer bist du? \u2014 Bist du es, mein wellen-", "tokens": ["Re\u00b7de", ",", "wer", "bist", "du", "?", "Bist", "du", "es", ",", "mein", "wel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "$.", "$(", "VAFIN", "PPER", "PPER", "$,", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "verschlungener Dellwar?", "tokens": ["ver\u00b7schlun\u00b7ge\u00b7ner", "Dell\u00b7war", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dellwar, Dellwar, du bist es! Mein trauter", "tokens": ["Dell\u00b7war", ",", "Dell\u00b7war", ",", "du", "bist", "es", "!", "Mein", "trau\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NE", "$,", "PPER", "VAFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dellwar, wie anders!", "tokens": ["Dell\u00b7war", ",", "wie", "an\u00b7ders", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "O, wie anders, Geliebter, als in den Tagen, die waren!", "tokens": ["O", ",", "wie", "an\u00b7ders", ",", "Ge\u00b7lieb\u00b7ter", ",", "als", "in", "den", "Ta\u00b7gen", ",", "die", "wa\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "$,", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,", "PRELS", "VAFIN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Stattlich war an der Warne dein Wandeln,", "tokens": ["Statt\u00b7lich", "war", "an", "der", "War\u00b7ne", "dein", "Wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "erhaben dein Herschritt", "tokens": ["er\u00b7ha\u00b7ben", "dein", "Her\u00b7schritt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN"], "meter": "-+-+++", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Unter den Edeln! Nun schwebst du ein Schatten", "tokens": ["Un\u00b7ter", "den", "E\u00b7deln", "!", "Nun", "schwebst", "du", "ein", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "mit nichtigen Schatten!", "tokens": ["mit", "nich\u00b7ti\u00b7gen", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Liebend flogen dir sonst die J\u00fcnglinge, liebend", "tokens": ["Lie\u00b7bend", "flo\u00b7gen", "dir", "sonst", "die", "J\u00fcng\u00b7lin\u00b7ge", ",", "lie\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ADJD"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "die M\u00e4dchen", "tokens": ["die", "M\u00e4d\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dir entgegen. Nun schaudert zur\u00fcck, wer dem", "tokens": ["Dir", "ent\u00b7ge\u00b7gen", ".", "Nun", "schau\u00b7dert", "zu\u00b7r\u00fcck", ",", "wer", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PTKVZ", "$,", "PWS", "ART"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Blassen begegnet.", "tokens": ["Blas\u00b7sen", "be\u00b7geg\u00b7net", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Blasser, du warest mir lieb in deiner rosigen", "tokens": ["Blas\u00b7ser", ",", "du", "wa\u00b7rest", "mir", "lieb", "in", "dei\u00b7ner", "ro\u00b7si\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Jugend,", "tokens": ["Ju\u00b7gend", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Bist mir lieb in deinem Erblassen, mir lieb auf ewig!", "tokens": ["Bist", "mir", "lieb", "in", "dei\u00b7nem", "Er\u00b7blas\u00b7sen", ",", "mir", "lieb", "auf", "e\u00b7wig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADJD", "APPR", "ADJD", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Bruder, du warest mir werth vor jedem deiner", "tokens": ["Bru\u00b7der", ",", "du", "wa\u00b7rest", "mir", "werth", "vor", "je\u00b7dem", "dei\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "APPR", "PIAT", "PPOSAT"], "meter": "+-++--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gespielen!", "tokens": ["Ge\u00b7spie\u00b7len", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Werth auch dir, mein Bruder, war ich vor deinen", "tokens": ["Werth", "auch", "dir", ",", "mein", "Bru\u00b7der", ",", "war", "ich", "vor", "dei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "APPR", "PPOSAT"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gesellen!", "tokens": ["Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Unsre Seelen ahneten, naheten, flogen einander", "tokens": ["Uns\u00b7re", "See\u00b7len", "ah\u00b7ne\u00b7ten", ",", "na\u00b7he\u00b7ten", ",", "flo\u00b7gen", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PRF"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Feurig entgegen. Nun wallst du ein Schatten mit", "tokens": ["Feu\u00b7rig", "ent\u00b7ge\u00b7gen", ".", "Nun", "wallst", "du", "ein", "Schat\u00b7ten", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "kalten Schatten.", "tokens": ["kal\u00b7ten", "Schat\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Bruder, ich denke der seligen Tage, der seligen", "tokens": ["Bru\u00b7der", ",", "ich", "den\u00b7ke", "der", "se\u00b7li\u00b7gen", "Ta\u00b7ge", ",", "der", "se\u00b7li\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "N\u00e4chte,", "tokens": ["N\u00e4ch\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wo wir wallten im thauenden Grase des sch\u00f6nen", "tokens": ["Wo", "wir", "wall\u00b7ten", "im", "thau\u00b7en\u00b7den", "Gra\u00b7se", "des", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Eylands,", "tokens": ["Ey\u00b7lands", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wo wir gr\u00fcssten in D\u00e4mmerschatten die rauschende", "tokens": ["Wo", "wir", "gr\u00fcs\u00b7sten", "in", "D\u00e4m\u00b7mer\u00b7schat\u00b7ten", "die", "rau\u00b7schen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Eiche,", "tokens": ["Ei\u00b7che", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Wo wir, sitzend am flisternden See im schaurigen", "tokens": ["Wo", "wir", ",", "sit\u00b7zend", "am", "flis\u00b7tern\u00b7den", "See", "im", "schau\u00b7ri\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ADJD", "APPRART", "ADJA", "NN", "APPRART", "ADJA"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Mondschein,", "tokens": ["Mond\u00b7schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Mit den Thaten der V\u00e4ter, und mit den Ges\u00e4ngen", "tokens": ["Mit", "den", "Tha\u00b7ten", "der", "V\u00e4\u00b7ter", ",", "und", "mit", "den", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "der Vorwelt", "tokens": ["der", "Vor\u00b7welt"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.11": {"text": "Unsre Seelen erh\u00f6hten. \u2014 Wie gl\u00e4nzte dein Antlitz", "tokens": ["Uns\u00b7re", "See\u00b7len", "er\u00b7h\u00f6h\u00b7ten", ".", "Wie", "gl\u00e4nz\u00b7te", "dein", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$(", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "im Mondlicht,", "tokens": ["im", "Mond\u00b7licht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Wie der Mond in der Thr\u00e4ne, die deinen Wimpern", "tokens": ["Wie", "der", "Mond", "in", "der", "Thr\u00e4\u00b7ne", ",", "die", "dei\u00b7nen", "Wim\u00b7pern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "entbebte!", "tokens": ["ent\u00b7beb\u00b7te", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Bruder, die Tage sind hin! Verloren die seligen", "tokens": ["Bru\u00b7der", ",", "die", "Ta\u00b7ge", "sind", "hin", "!", "Ver\u00b7lo\u00b7ren", "die", "se\u00b7li\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$.", "NN", "ART", "ADJA"], "meter": "+--+--+-+-+---", "measure": "dactylic.di.plus"}, "line.16": {"text": "N\u00e4chte.", "tokens": ["N\u00e4ch\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Nimmer kehren sie. Nimmer erschau' ich dich! Ein-", "tokens": ["Nim\u00b7mer", "keh\u00b7ren", "sie", ".", "Nim\u00b7mer", "er\u00b7schau'", "ich", "dich", "!", "Ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PPER", "PRF", "$.", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "sam und schweigend", "tokens": ["sam", "und", "schwei\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Wallst du im dumpfen Todtenreich, ein Schatten", "tokens": ["Wallst", "du", "im", "dum\u00b7pfen", "Tod\u00b7ten\u00b7reich", ",", "ein", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "mit Schatten.", "tokens": ["mit", "Schat\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Klagt, ihr J\u00fcngling' am Nebelbach, um eurer", "tokens": ["Klagt", ",", "ihr", "J\u00fcng\u00b7ling'", "am", "Ne\u00b7bel\u00b7bach", ",", "um", "eu\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,", "KOUI", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gesellen", "tokens": ["Ge\u00b7sel\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Edelsten, klagt! Ihn hat die Welle der Warne ver-", "tokens": ["E\u00b7dels\u00b7ten", ",", "klagt", "!", "Ihn", "hat", "die", "Wel\u00b7le", "der", "War\u00b7ne", "ver"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "schlungen", "tokens": ["schlun\u00b7gen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Weinet, ihr T\u00f6chter der Stadt am Nebel, um", "tokens": ["Wei\u00b7net", ",", "ihr", "T\u00f6ch\u00b7ter", "der", "Stadt", "am", "Ne\u00b7bel", ",", "um"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$,", "KOUI"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "eurer Verehrer", "tokens": ["eu\u00b7rer", "Ver\u00b7eh\u00b7rer"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Holdesten, weint! Er ist erstarrt im Eise der Warne.", "tokens": ["Hol\u00b7des\u00b7ten", ",", "weint", "!", "Er", "ist", "er\u00b7starrt", "im", "Ei\u00b7se", "der", "War\u00b7ne", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}}, "stanza.11": {"line.1": {"text": "Lange stand an der Th\u00fcr der heimlichen Pforte", "tokens": ["Lan\u00b7ge", "stand", "an", "der", "Th\u00fcr", "der", "heim\u00b7li\u00b7chen", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "das M\u00e4dchen", "tokens": ["das", "M\u