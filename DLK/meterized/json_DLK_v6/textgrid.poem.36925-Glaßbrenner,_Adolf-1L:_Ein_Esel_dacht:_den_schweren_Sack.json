{"textgrid.poem.36925": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Esel dacht: den schweren Sack", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Esel dacht: den schweren Sack", "tokens": ["Ein", "E\u00b7sel", "dacht", ":", "den", "schwe\u00b7ren", "Sack"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willst du nicht l\u00e4nger tragen;", "tokens": ["Willst", "du", "nicht", "l\u00e4n\u00b7ger", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wurde hager, bla\u00df und spack", "tokens": ["Er", "wur\u00b7de", "ha\u00b7ger", ",", "bla\u00df", "und", "spack"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00f6hnte und th\u00e4t klagen;", "tokens": ["Und", "st\u00f6hn\u00b7te", "und", "th\u00e4t", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jedoch, sobald der M\u00fcller da,", "tokens": ["Je\u00b7doch", ",", "so\u00b7bald", "der", "M\u00fcl\u00b7ler", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So rief er blos I\u2013a, I\u2013a!", "tokens": ["So", "rief", "er", "blos", "I", "\u2013", "a", ",", "I", "\u2013", "a", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "CARD", "$(", "NE", "$,", "XY", "$(", "NE", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Es ging ein Jahr um's andere hin.", "tokens": ["Es", "ging", "ein", "Jahr", "um's", "an\u00b7de\u00b7re", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "PIS", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Esel mu\u00dfte tragen;", "tokens": ["Der", "E\u00b7sel", "mu\u00df\u00b7te", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch tr\u00fcb und tr\u00fcber ward sein Sinn,", "tokens": ["Doch", "tr\u00fcb", "und", "tr\u00fc\u00b7ber", "ward", "sein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er th\u00e4t erb\u00e4rmlich klagen.", "tokens": ["Er", "th\u00e4t", "er\u00b7b\u00e4rm\u00b7lich", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jedoch, wenn er den M\u00fcller sah,", "tokens": ["Je\u00b7doch", ",", "wenn", "er", "den", "M\u00fcl\u00b7ler", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So rief er blo\u00df I\u2013a, I\u2013a!", "tokens": ["So", "rief", "er", "blo\u00df", "I", "\u2013", "a", ",", "I", "\u2013", "a", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "CARD", "$(", "NE", "$,", "XY", "$(", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch endlich kommt er nicht mehr fort,", "tokens": ["Doch", "end\u00b7lich", "kommt", "er", "nicht", "mehr", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Konnt' nicht den Sack mehr tragen;", "tokens": ["Konnt'", "nicht", "den", "Sack", "mehr", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drauf gab er sich sein Ehrenwort,", "tokens": ["Drauf", "gab", "er", "sich", "sein", "Eh\u00b7ren\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Leiden ", "tokens": ["Sein", "Lei\u00b7den"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Jedoch, als er den M\u00fcller sah,", "tokens": ["Je\u00b7doch", ",", "als", "er", "den", "M\u00fcl\u00b7ler", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So rief er blos I\u2013a, I\u2013a!", "tokens": ["So", "rief", "er", "blos", "I", "\u2013", "a", ",", "I", "\u2013", "a", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "CARD", "$(", "NE", "$,", "XY", "$(", "NE", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Er wurde alt, er wurde krank,", "tokens": ["Er", "wur\u00b7de", "alt", ",", "er", "wur\u00b7de", "krank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Th\u00e4t immer heft'ger klagen,", "tokens": ["Th\u00e4t", "im\u00b7mer", "heft'\u00b7ger", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jedoch sein ganzes Lebelang", "tokens": ["Je\u00b7doch", "sein", "gan\u00b7zes", "Le\u00b7be\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er den Sack getragen.", "tokens": ["Hat", "er", "den", "Sack", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als sterbend er den M\u00fcller sah,", "tokens": ["Als", "ster\u00b7bend", "er", "den", "M\u00fcl\u00b7ler", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da r\u00f6chelt er I\u2013a, I\u2013a!", "tokens": ["Da", "r\u00f6\u00b7chelt", "er", "I", "\u2013", "a", ",", "I", "\u2013", "a", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "XY", "$(", "NE", "$,", "XY", "$(", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "In Stokau, Augs- und L\u00fcneburg,", "tokens": ["In", "Sto\u00b7kau", ",", "Augs", "und", "L\u00fc\u00b7ne\u00b7burg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "TRUNC", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Cassel und in Wesel!", "tokens": ["In", "Cas\u00b7sel", "und", "in", "We\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was einmal Esel durch und durch,", "tokens": ["Was", "ein\u00b7mal", "E\u00b7sel", "durch", "und", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist und bleibt ein Esel!", "tokens": ["Das", "ist", "und", "bleibt", "ein", "E\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Was auch geschieht und was geschah,", "tokens": ["Was", "auch", "ge\u00b7schieht", "und", "was", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Esel schreit I\u2013a, I\u2013a!", "tokens": ["Der", "E\u00b7sel", "schreit", "I", "\u2013", "a", ",", "I", "\u2013", "a", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "XY", "$(", "NE", "$,", "XY", "$(", "NE", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}}}}