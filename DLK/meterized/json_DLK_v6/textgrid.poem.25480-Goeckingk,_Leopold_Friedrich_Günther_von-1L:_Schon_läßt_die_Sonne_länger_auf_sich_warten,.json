{"textgrid.poem.25480": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon l\u00e4\u00dft die Sonne l\u00e4nger auf sich warten,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon l\u00e4\u00dft die Sonne l\u00e4nger auf sich warten,", "tokens": ["Schon", "l\u00e4\u00dft", "die", "Son\u00b7ne", "l\u00e4n\u00b7ger", "auf", "sich", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat mich der Hahn mit seinem Kr\u00e4hn geweckt;", "tokens": ["Hat", "mich", "der", "Hahn", "mit", "sei\u00b7nem", "Kr\u00e4hn", "ge\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon hat der Reif die Wiesen wei\u00df bedeckt,", "tokens": ["Schon", "hat", "der", "Reif", "die", "Wie\u00b7sen", "wei\u00df", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ach! den letzten Schmuck im Garten,", "tokens": ["Und", "ach", "!", "den", "letz\u00b7ten", "Schmuck", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die K\u00fcrbisse, die noch auf W\u00e4rme harrten,", "tokens": ["Die", "K\u00fcr\u00b7bis\u00b7se", ",", "die", "noch", "auf", "W\u00e4r\u00b7me", "harr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verschrumpft zur Erde hingestreckt.", "tokens": ["Ver\u00b7schrumpft", "zur", "Er\u00b7de", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon ziehen dunkelgrau am Felsen", "tokens": ["Schon", "zie\u00b7hen", "dun\u00b7kel\u00b7grau", "am", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Regenwolken hin; die rasche Zorga schwillt;", "tokens": ["Die", "Re\u00b7gen\u00b7wol\u00b7ken", "hin", ";", "die", "ra\u00b7sche", "Zor\u00b7ga", "schwillt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Am Ufer klappert schon das Haupt der nackten Elsen:", "tokens": ["Am", "U\u00b7fer", "klap\u00b7pert", "schon", "das", "Haupt", "der", "nack\u00b7ten", "El\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach! alles war so sanft, und alles wird so wild! \u2013", "tokens": ["Ach", "!", "al\u00b7les", "war", "so", "sanft", ",", "und", "al\u00b7les", "wird", "so", "wild", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PIS", "VAFIN", "ADV", "ADJD", "$,", "KON", "PIS", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So pack' denn ein, lieb Frauchen; la\u00df noch heute", "tokens": ["So", "pack'", "denn", "ein", ",", "lieb", "Frau\u00b7chen", ";", "la\u00df", "noch", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "ADJD", "NN", "$.", "VVIMP", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zur\u00fcck uns kehren nach der Stadt.", "tokens": ["Zu\u00b7r\u00fcck", "uns", "keh\u00b7ren", "nach", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Du seufzest? Aber wie? wenn uns ein Wasserbad", "tokens": ["Du", "seuf\u00b7zest", "?", "A\u00b7ber", "wie", "?", "wenn", "uns", "ein", "Was\u00b7ser\u00b7bad"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "PWAV", "$.", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Umringt', und hier der hohle Weg verschneite:", "tokens": ["Um\u00b7ringt'", ",", "und", "hier", "der", "hoh\u00b7le", "Weg", "ver\u00b7schnei\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wie machst du dann ein Haus voll Menschen satt?", "tokens": ["Wie", "machst", "du", "dann", "ein", "Haus", "voll", "Men\u00b7schen", "satt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Inde\u00df das Corpus juris und zwei Wiegen,", "tokens": ["In\u00b7de\u00df", "das", "Cor\u00b7pus", "ju\u00b7ris", "und", "zwei", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "FM", "FM", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ein Globus und ein Kucheneisen sich,", "tokens": ["Ein", "Glo\u00b7bus", "und", "ein", "Ku\u00b7chen\u00b7ei\u00b7sen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wie auf der Post ein ", "tokens": ["Wie", "auf", "der", "Post", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Auf einem Wagen, br\u00fcderlich,", "tokens": ["Auf", "ei\u00b7nem", "Wa\u00b7gen", ",", "br\u00fc\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Zum Umziehn in einander schmiegen,", "tokens": ["Zum", "Um\u00b7ziehn", "in", "ein\u00b7an\u00b7der", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Reis' ich auf diesem Blatt' Papier,", "tokens": ["Reis'", "ich", "auf", "die\u00b7sem", "Blatt'", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "(es kostet gar zu viel, im Wagen!)", "tokens": ["(", "es", "kos\u00b7tet", "gar", "zu", "viel", ",", "im", "Wa\u00b7gen", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PTKA", "PIS", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Im Herbst' err\u00f6thend Dank zu sagen.", "tokens": ["Im", "Herbst'", "er\u00b7r\u00f6\u00b7thend", "Dank", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Denn seit dem Mai hatt' ich das Sitzen,", "tokens": ["Denn", "seit", "dem", "Mai", "hatt'", "ich", "das", "Sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So wie das Schreiben, ganz verlernt;", "tokens": ["So", "wie", "das", "Schrei\u00b7ben", ",", "ganz", "ver\u00b7lernt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Dein Lob, das von der Berge Spitzen", "tokens": ["Dein", "Lob", ",", "das", "von", "der", "Ber\u00b7ge", "Spit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Mich sonst herab zum Schreibepult' gek\u00f6rnt,", "tokens": ["Mich", "sonst", "her\u00b7ab", "zum", "Schrei\u00b7be\u00b7pult'", "ge\u00b7k\u00f6rnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "War, einen Reim aus mir heraus zu pressen,", "tokens": ["War", ",", "ei\u00b7nen", "Reim", "aus", "mir", "he\u00b7raus", "zu", "pres\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "APPR", "PPER", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Itzt selbst zu schwach. Ein Landhaus, Freund, ward mein!", "tokens": ["Itzt", "selbst", "zu", "schwach", ".", "Ein", "Land\u00b7haus", ",", "Freund", ",", "ward", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADJD", "$.", "ART", "NN", "$,", "NN", "$,", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und kaum trat ich mit Weib und Kind hinein,", "tokens": ["Und", "kaum", "trat", "ich", "mit", "Weib", "und", "Kind", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "So war die Welt rund um mich her vergessen;", "tokens": ["So", "war", "die", "Welt", "rund", "um", "mich", "her", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "APPR", "PRF", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und wer wird das der Liebe nicht verzeihn?", "tokens": ["Und", "wer", "wird", "das", "der", "Lie\u00b7be", "nicht", "ver\u00b7zeihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PDS", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ich aber liebe nicht viel minder", "tokens": ["Ich", "a\u00b7ber", "lie\u00b7be", "nicht", "viel", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Die\u00df Haus, als meine ", "tokens": ["Die\u00df", "Haus", ",", "als", "mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "NN", "$,", "KOUS", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Es ward mein Arzt; denn hier ward ich ges\u00fcnder;", "tokens": ["Es", "ward", "mein", "Arzt", ";", "denn", "hier", "ward", "ich", "ge\u00b7s\u00fcn\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Es ward der Lehrer meiner Kinder,", "tokens": ["Es", "ward", "der", "Leh\u00b7rer", "mei\u00b7ner", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Die hier erst sahn, wie fr\u00fch die Sonne scheint.", "tokens": ["Die", "hier", "erst", "sahn", ",", "wie", "fr\u00fch", "die", "Son\u00b7ne", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Kann Niemand der Versuchung widerstehen,", "tokens": ["Kann", "Nie\u00b7mand", "der", "Ver\u00b7su\u00b7chung", "wi\u00b7der\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Das M\u00e4dchen, das sein Herz ihm stahl,", "tokens": ["Das", "M\u00e4d\u00b7chen", ",", "das", "sein", "Herz", "ihm", "stahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Von ihrer Stirn' bis zu den Zehen", "tokens": ["Von", "ih\u00b7rer", "Stirn'", "bis", "zu", "den", "Ze\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Uns zu beschreiben: sag' einmal,", "tokens": ["Uns", "zu", "be\u00b7schrei\u00b7ben", ":", "sag'", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Wie k\u00f6nnt' auch ich mein Landhaus und mein Thal", "tokens": ["Wie", "k\u00f6nnt'", "auch", "ich", "mein", "Land\u00b7haus", "und", "mein", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Mit dir so still vor\u00fcber gehen?", "tokens": ["Mit", "dir", "so", "still", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Im sch\u00f6nsten Thal', durchschnitten von dem Lauf'", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Thal'", ",", "durch\u00b7schnit\u00b7ten", "von", "dem", "Lauf'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.47": {"text": "Doch s\u00e4h' ich gern, tr\u00fcg's ", "tokens": ["Doch", "s\u00e4h'", "ich", "gern", ",", "tr\u00fcg's"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADJA"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.48": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.49": {"text": "Zum Gl\u00fcck' inde\u00df blieb mir von allen Seiten", "tokens": ["Zum", "Gl\u00fcck", "in\u00b7de\u00df", "blieb", "mir", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Die Aussicht frei; denn, einer Insel gleich,", "tokens": ["Die", "Aus\u00b7sicht", "frei", ";", "denn", ",", "ei\u00b7ner", "In\u00b7sel", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV",