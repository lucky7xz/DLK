{"textgrid.poem.39714": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Der Luftschiffer", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gefahren bin ich in schwankendem Kahne", "tokens": ["Ge\u00b7fah\u00b7ren", "bin", "ich", "in", "schwan\u00b7ken\u00b7dem", "Kah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf dem blaulichen Oceane,", "tokens": ["Auf", "dem", "blau\u00b7li\u00b7chen", "O\u00b7cea\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die leuchtenden Sterne umflie\u00dft,", "tokens": ["Der", "die", "leuch\u00b7ten\u00b7den", "Ster\u00b7ne", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Habe die himmlischen M\u00e4chte begr\u00fc\u00dft.", "tokens": ["Ha\u00b7be", "die", "himm\u00b7li\u00b7schen", "M\u00e4ch\u00b7te", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "War, in ihrer Betrachtung versunken,", "tokens": ["War", ",", "in", "ih\u00b7rer", "Be\u00b7trach\u00b7tung", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Habe den ewigen Aether getrunken,", "tokens": ["Ha\u00b7be", "den", "e\u00b7wi\u00b7gen", "A\u00b7e\u00b7ther", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Habe dem Irdischen ganz mich entwandt,", "tokens": ["Ha\u00b7be", "dem", "Ir\u00b7di\u00b7schen", "ganz", "mich", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Droben die Schriften der Sterne erkannt", "tokens": ["Dro\u00b7ben", "die", "Schrif\u00b7ten", "der", "Ster\u00b7ne", "er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVPP"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Und in ihrem Kreisen und Drehen", "tokens": ["Und", "in", "ih\u00b7rem", "Krei\u00b7sen", "und", "Dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Bildlich den heiligen Rhythmus gesehen,", "tokens": ["Bild\u00b7lich", "den", "hei\u00b7li\u00b7gen", "Rhyth\u00b7mus", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Der gewaltig auch jeglichen Klang", "tokens": ["Der", "ge\u00b7wal\u00b7tig", "auch", "jeg\u00b7li\u00b7chen", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "PIAT", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Rei\u00dft zu des Wohllauts wogendem Drang.", "tokens": ["Rei\u00dft", "zu", "des", "Wohl\u00b7lauts", "wo\u00b7gen\u00b7dem", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Aber ach! es ziehet mich hernieder,", "tokens": ["A\u00b7ber", "ach", "!", "es", "zie\u00b7het", "mich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Nebel \u00fcberschleiert meinen Blick,", "tokens": ["Ne\u00b7bel", "\u00fc\u00b7bersc\u00b7hlei\u00b7ert", "mei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Und der Erde Grenzen seh' ich wieder,", "tokens": ["Und", "der", "Er\u00b7de", "Gren\u00b7zen", "seh'", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Wolken treiben mich zur\u00fcck.", "tokens": ["Wol\u00b7ken", "trei\u00b7ben", "mich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wehe! Das Gesetz der Schwere", "tokens": ["We\u00b7he", "!", "Das", "Ge\u00b7setz", "der", "Schwe\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Es behauptet nur sein Recht,", "tokens": ["Es", "be\u00b7haup\u00b7tet", "nur", "sein", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Keiner darf sich ihm entziehen", "tokens": ["Kei\u00b7ner", "darf", "sich", "ihm", "ent\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Von dem irdischen Geschlecht.", "tokens": ["Von", "dem", "ir\u00b7di\u00b7schen", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}