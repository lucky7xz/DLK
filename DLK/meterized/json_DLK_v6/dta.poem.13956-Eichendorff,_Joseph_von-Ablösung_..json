{"dta.poem.13956": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abl\u00f6sung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sa\u00dfen gelagert im Gr\u00fcnen,", "tokens": ["Wir", "sa\u00b7\u00dfen", "ge\u00b7la\u00b7gert", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So traulich und lustig gesellt,", "tokens": ["So", "trau\u00b7lich", "und", "lus\u00b7tig", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Lichter des Fr\u00fchlings schienen", "tokens": ["Die", "Lich\u00b7ter", "des", "Fr\u00fch\u00b7lings", "schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hold spielend durch's gr\u00fcne Gezelt.", "tokens": ["Hold", "spie\u00b7lend", "durch's", "gr\u00fc\u00b7ne", "Ge\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}