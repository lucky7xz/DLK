{"dta.poem.19567": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die todte Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Im Schatten wir,", "tokens": ["Im", "Schat\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das Dorf im Sonnenku\u00df,", "tokens": ["Das", "Dorf", "im", "Son\u00b7nen\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "(fast wie das J\u00fcngerpaar,", "tokens": ["(", "fast", "wie", "das", "J\u00fcn\u00b7ger\u00b7paar", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das ging nach Emmaus,", "tokens": ["Das", "ging", "nach", "Em\u00b7maus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dazwischen leise", "tokens": ["Da\u00b7zwi\u00b7schen", "lei\u00b7se"], "token_info": ["word", "word"], "pos": ["PAV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Redend schritt", "tokens": ["Re\u00b7dend", "schritt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Der Meister, dem sie folgten", "tokens": ["Der", "Meis\u00b7ter", ",", "dem", "sie", "folg\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und der den Tod erlitt.)", "tokens": ["Und", "der", "den", "Tod", "er\u00b7litt", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So schreitet zwischen uns", "tokens": ["So", "schrei\u00b7tet", "zwi\u00b7schen", "uns"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Im D\u00e4mmerlicht", "tokens": ["Im", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Unsre todte Liebe,", "tokens": ["Uns\u00b7re", "tod\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Die leise spricht.", "tokens": ["Die", "lei\u00b7se", "spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Sie wei\u00df f\u00fcr das Geheimni\u00df", "tokens": ["Sie", "wei\u00df", "f\u00fcr", "das", "Ge\u00b7heim\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ein heimlich Wort,", "tokens": ["Ein", "heim\u00b7lich", "Wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Sie kennt der Seelen", "tokens": ["Sie", "kennt", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Allertiefsten Hort.", "tokens": ["Al\u00b7ler\u00b7tiefs\u00b7ten", "Hort", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Sie deutet und erl\u00e4utert", "tokens": ["Sie", "deu\u00b7tet", "und", "er\u00b7l\u00e4u\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Uns jedes Ding,", "tokens": ["Uns", "je\u00b7des", "Ding", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Sie sagt: So ist's gekommen,", "tokens": ["Sie", "sagt", ":", "So", "ist's", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Da\u00df ich am Holze hing.", "tokens": ["Da\u00df", "ich", "am", "Hol\u00b7ze", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Ihr habet mich verleugnet", "tokens": ["Ihr", "ha\u00b7bet", "mich", "ver\u00b7leug\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und schlimm verh\u00f6hnt,", "tokens": ["Und", "schlimm", "ver\u00b7h\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Ich sa\u00df im Purpur,", "tokens": ["Ich", "sa\u00df", "im", "Pur\u00b7pur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Blutig, dorngekr\u00f6nt,", "tokens": ["Blu\u00b7tig", ",", "dorn\u00b7ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.25": {"text": "Ich habe Tod erlitten,", "tokens": ["Ich", "ha\u00b7be", "Tod", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Den Tod bezwang ich bald,", "tokens": ["Den", "Tod", "be\u00b7zwang", "ich", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Und geh' in eurer Mitten", "tokens": ["Und", "geh'", "in", "eu\u00b7rer", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Als geistige Gestalt \u2014", "tokens": ["Als", "geis\u00b7ti\u00b7ge", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Die Weggesellin", "tokens": ["Die", "Weg\u00b7ge\u00b7sel\u00b7lin"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Blieb unerkannt,", "tokens": ["Blieb", "un\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Doch hat uns wie den J\u00fcngern", "tokens": ["Doch", "hat", "uns", "wie", "den", "J\u00fcn\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Das Herz gebrannt.", "tokens": ["Das", "Herz", "ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}