{"dta.poem.7412": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Rheinweinlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo solch ein Feuer noch gedeiht", "tokens": ["Wo", "solch", "ein", "Feu\u00b7er", "noch", "ge\u00b7deiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und solch ein Wein noch Flammen speit,", "tokens": ["Und", "solch", "ein", "Wein", "noch", "Flam\u00b7men", "speit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lassen wir in Ewigkeit", "tokens": ["Da", "las\u00b7sen", "wir", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns nimmermehr vertreiben.", "tokens": ["Uns", "nim\u00b7mer\u00b7mehr", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sto\u00dft an! Sto\u00dft an! der Rhein,", "tokens": ["Sto\u00dft", "an", "!", "Sto\u00dft", "an", "!", "der", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "ART", "NE", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und w\u00e4r's nur um den Wein,", "tokens": ["Und", "w\u00e4r's", "nur", "um", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Rhein soll deutsch verbleiben.", "tokens": ["Der", "Rhein", "soll", "deutsch", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herab die B\u00fcchsen von der Wand,", "tokens": ["Her\u00b7ab", "die", "B\u00fcch\u00b7sen", "von", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die alten Schl\u00e4ger in die Hand,", "tokens": ["Die", "al\u00b7ten", "Schl\u00e4\u00b7ger", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sobald der Feind dem welschen Land", "tokens": ["So\u00b7bald", "der", "Feind", "dem", "wel\u00b7schen", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Rhein will einverleiben!", "tokens": ["Den", "Rhein", "will", "ein\u00b7ver\u00b7lei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Haut, Br\u00fcder, mutig drein!", "tokens": ["Haut", ",", "Br\u00fc\u00b7der", ",", "mu\u00b7tig", "drein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der alte Vater Rhein,", "tokens": ["Der", "al\u00b7te", "Va\u00b7ter", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Rhein soll deutsch verbleiben.", "tokens": ["Der", "Rhein", "soll", "deutsch", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Recht und Link, das Link und Recht,", "tokens": ["Das", "Recht", "und", "Link", ",", "das", "Link", "und", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie klingt es falsch, wie klingt es schlecht!", "tokens": ["Wie", "klingt", "es", "falsch", ",", "wie", "klingt", "es", "schlecht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Tropfen soll, ein feiger Knecht,", "tokens": ["Kein", "Trop\u00b7fen", "soll", ",", "ein", "fei\u00b7ger", "Knecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Franzmanns M\u00fchlen treiben.", "tokens": ["Des", "Franz\u00b7manns", "M\u00fch\u00b7len", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sto\u00dft an! Sto\u00dft an! der Rhein,", "tokens": ["Sto\u00dft", "an", "!", "Sto\u00dft", "an", "!", "der", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "ART", "NE", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und w\u00e4r's nur um den Wein,", "tokens": ["Und", "w\u00e4r's", "nur", "um", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Rhein soll deutsch verbleiben.", "tokens": ["Der", "Rhein", "soll", "deutsch", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der ist sein Rebenblut nicht wert,", "tokens": ["Der", "ist", "sein", "Re\u00b7ben\u00b7blut", "nicht", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das deutsche Weib, den deutschen Herd,", "tokens": ["Das", "deut\u00b7sche", "Weib", ",", "den", "deut\u00b7schen", "Herd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der nicht auch freudig schwingt sein Schwert,", "tokens": ["Der", "nicht", "auch", "freu\u00b7dig", "schwingt", "sein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Feinde aufzureiben.", "tokens": ["Die", "Fein\u00b7de", "auf\u00b7zu\u00b7rei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Frisch in die Schlacht hinein!", "tokens": ["Frisch", "in", "die", "Schlacht", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hinein f\u00fcr unsern Rhein!", "tokens": ["Hin\u00b7ein", "f\u00fcr", "un\u00b7sern", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Rhein soll deutsch verbleiben.", "tokens": ["Der", "Rhein", "soll", "deutsch", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O edler Saft, o lauter Gold,", "tokens": ["O", "ed\u00b7ler", "Saft", ",", "o", "lau\u00b7ter", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist kein ekler Sklavensold!", "tokens": ["Du", "bist", "kein", "ek\u00b7ler", "Skla\u00b7ven\u00b7sold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ihr Franken kommen wollt,", "tokens": ["Und", "wenn", "ihr", "Fran\u00b7ken", "kom\u00b7men", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00dft euch vorher schreiben.", "tokens": ["So", "la\u00dft", "euch", "vor\u00b7her", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah! Hurrah! Der Rhein,", "tokens": ["Hur\u00b7rah", "!", "Hur\u00b7rah", "!", "Der", "Rhein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "ART", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und w\u00e4r's nur um den Wein,", "tokens": ["Und", "w\u00e4r's", "nur", "um", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Rhein soll deutsch verbleiben.", "tokens": ["Der", "Rhein", "soll", "deutsch", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}