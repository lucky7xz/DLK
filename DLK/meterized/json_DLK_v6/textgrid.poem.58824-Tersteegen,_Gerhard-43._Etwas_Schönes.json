{"textgrid.poem.58824": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "43. Etwas Sch\u00f6nes", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Welt und aller S\u00fcnd' absagen,", "tokens": ["Der", "Welt", "und", "al\u00b7ler", "S\u00fcnd'", "ab\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein liebstes Eigen auch dran wagen", "tokens": ["Sein", "liebs\u00b7tes", "Ei\u00b7gen", "auch", "dran", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PAV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Gottes Wink gelassen stehn", "tokens": ["Und", "Got\u00b7tes", "Wink", "ge\u00b7las\u00b7sen", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVPP", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In stillem Geiste: das ist sch\u00f6n.", "tokens": ["In", "stil\u00b7lem", "Geis\u00b7te", ":", "das", "ist", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}