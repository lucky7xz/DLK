{"dta.poem.18602": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D es  G rafen von  V illa  M edi-  \n na/ welcher der Spanischen K\u00f6nigin an  \n die F\u00fcssen gegriffen/ und deswe-  \n gen erschossen wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrtzt Phaeton/ wenn er zur Sonnen denckt zu steigen/", "tokens": ["St\u00fcrtzt", "Phae\u00b7ton", "/", "wenn", "er", "zur", "Son\u00b7nen", "denckt", "zu", "stei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$(", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "So stiege meine Gluth hergegen in das Thal.", "tokens": ["So", "stie\u00b7ge", "mei\u00b7ne", "Gluth", "her\u00b7ge\u00b7gen", "in", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man darff sich sonst geb\u00fcckt zu hohen F\u00fcssen neigen/", "tokens": ["Man", "darff", "sich", "sonst", "ge\u00b7b\u00fcckt", "zu", "ho\u00b7hen", "F\u00fcs\u00b7sen", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch diese D", "tokens": ["Doch", "die\u00b7se", "D"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mu\u00df jener nun zu hoch/ und ich zu tieff erkalten/", "tokens": ["Mu\u00df", "je\u00b7ner", "nun", "zu", "hoch", "/", "und", "ich", "zu", "tieff", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "PTKA", "ADJD", "$(", "KON", "PPER", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bleibt das Sicherste: man sol das Mittel halten.", "tokens": ["So", "bleibt", "das", "Si\u00b7chers\u00b7te", ":", "man", "sol", "das", "Mit\u00b7tel", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$.", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}