{"textgrid.poem.55405": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarzes Fahrzeug teilt die Welle", "tokens": ["Schwar\u00b7zes", "Fahr\u00b7zeug", "teilt", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00e4chst der K\u00fcste von Kassandra,", "tokens": ["N\u00e4chst", "der", "K\u00fcs\u00b7te", "von", "Kas\u00b7sand\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber ihm die schwarzen Segel,", "tokens": ["\u00dc\u00b7ber", "ihm", "die", "schwar\u00b7zen", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber ihnen Himmelsbl\u00e4ue.", "tokens": ["\u00dc\u00b7ber", "ih\u00b7nen", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt ein T\u00fcrkenschiff entgegen,", "tokens": ["Kommt", "ein", "T\u00fcr\u00b7ken\u00b7schiff", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Scharlachwimpel wehen gl\u00e4nzend,", "tokens": ["Schar\u00b7lach\u00b7wim\u00b7pel", "we\u00b7hen", "gl\u00e4n\u00b7zend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbstreich die Segel unverz\u00fcglich,", "tokens": ["\u00bb", "streich", "die", "Se\u00b7gel", "un\u00b7ver\u00b7z\u00fcg\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nieder la\u00df die Segel du!\u00ab", "tokens": ["Nie\u00b7der", "la\u00df", "die", "Se\u00b7gel", "du", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbnein, ich streiche nicht die Segel,", "tokens": ["\u00bb", "nein", ",", "ich", "strei\u00b7che", "nicht", "die", "Se\u00b7gel", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nimmer la\u00df ich sie herab,", "tokens": ["Nim\u00b7mer", "la\u00df", "ich", "sie", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Droht ihr doch, als w\u00e4r ich Br\u00e4utchen,", "tokens": ["Droht", "ihr", "doch", ",", "als", "w\u00e4r", "ich", "Br\u00e4ut\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Br\u00e4utchen, das zu schrecken ist.", "tokens": ["Br\u00e4ut\u00b7chen", ",", "das", "zu", "schre\u00b7cken", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Jannis bin ich, Sohn des Stada,", "tokens": ["Jan\u00b7nis", "bin", "ich", ",", "Sohn", "des", "Sta\u00b7da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Eidam des Bukovalas.", "tokens": ["Ei\u00b7dam", "des", "Bu\u00b7ko\u00b7va\u00b7las", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Frisch, Gesellen, frisch zur Arbeit!", "tokens": ["Frisch", ",", "Ge\u00b7sel\u00b7len", ",", "frisch", "zur", "Ar\u00b7beit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Auf zum Vorderteil des Schiffes:", "tokens": ["Auf", "zum", "Vor\u00b7der\u00b7teil", "des", "Schif\u00b7fes", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "T\u00fcrkenblut ist zu vergie\u00dfen,", "tokens": ["T\u00fcr\u00b7ken\u00b7blut", "ist", "zu", "ver\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Schont nicht der Ungl\u00e4ubigen.\u00ab", "tokens": ["Schont", "nicht", "der", "Un\u00b7gl\u00e4u\u00b7bi\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und mit einer klugen Wendung", "tokens": ["Und", "mit", "ei\u00b7ner", "klu\u00b7gen", "Wen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Beut das T\u00fcrkenschiff die Spitze;", "tokens": ["Beut", "das", "T\u00fcr\u00b7ken\u00b7schiff", "die", "Spit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Jannis aber schwingt hinauf sich", "tokens": ["Jan\u00b7nis", "a\u00b7ber", "schwingt", "hin\u00b7auf", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ADV", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Mit dem S\u00e4bel in der Faust,", "tokens": ["Mit", "dem", "S\u00e4\u00b7bel", "in", "der", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Das Geb\u00e4lke trieft vom Blute,", "tokens": ["Das", "Ge\u00b7b\u00e4l\u00b7ke", "trieft", "vom", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und ger\u00f6tet sind die Wellen.", "tokens": ["Und", "ge\u00b7r\u00f6\u00b7tet", "sind", "die", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "\u00bballah! Allah!\u00ab schrein um Gnade", "tokens": ["\u00bb", "al\u00b7lah", "!", "Al\u00b7lah", "!", "\u00ab", "schrein", "um", "Gna\u00b7de"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "$.", "NN", "$.", "$(", "ADJD", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "Die Ungl\u00e4ubigen auf den Knien.", "tokens": ["Die", "Un\u00b7gl\u00e4u\u00b7bi\u00b7gen", "auf", "den", "Kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "\u00bbtraurig Leben\u00ab, ruft der Sieger,", "tokens": ["\u00bb", "trau\u00b7rig", "Le\u00b7ben", "\u00ab", ",", "ruft", "der", "Sie\u00b7ger", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "\u00bbbleibe den Besiegten nun!\u00ab", "tokens": ["\u00bb", "blei\u00b7be", "den", "Be\u00b7sieg\u00b7ten", "nun", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}