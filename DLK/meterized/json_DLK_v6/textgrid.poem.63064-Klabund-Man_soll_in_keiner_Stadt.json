{"textgrid.poem.63064": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Man soll in keiner Stadt", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man soll in keiner Stadt l\u00e4nger bleiben als ein halbes Jahr.", "tokens": ["Man", "soll", "in", "kei\u00b7ner", "Stadt", "l\u00e4n\u00b7ger", "blei\u00b7ben", "als", "ein", "hal\u00b7bes", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PIAT", "NN", "ADJD", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wenn man wei\u00df, wie sie wurde und war,", "tokens": ["Wenn", "man", "wei\u00df", ",", "wie", "sie", "wur\u00b7de", "und", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "KON", "VAFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wenn man die M\u00e4nner hat weinen sehen", "tokens": ["Wenn", "man", "die", "M\u00e4n\u00b7ner", "hat", "wei\u00b7nen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VAFIN", "VVINF", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die Frauen lachen,", "tokens": ["Und", "die", "Frau\u00b7en", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Soll man von dannen gehen,", "tokens": ["Soll", "man", "von", "dan\u00b7nen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ADV", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Neue St\u00e4dte zu bewachen.", "tokens": ["Neu\u00b7e", "St\u00e4d\u00b7te", "zu", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4\u00dft man Freunde und Geliebte zur\u00fcck,", "tokens": ["L\u00e4\u00dft", "man", "Freun\u00b7de", "und", "Ge\u00b7lieb\u00b7te", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wandert die Stadt mit einem als ein ewiges Gl\u00fcck.", "tokens": ["Wan\u00b7dert", "die", "Stadt", "mit", "ei\u00b7nem", "als", "ein", "e\u00b7wi\u00b7ges", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Meine Lippen singen zuweilen", "tokens": ["Mei\u00b7ne", "Lip\u00b7pen", "sin\u00b7gen", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Lieder, die ich in ihr gelernt,", "tokens": ["Lie\u00b7der", ",", "die", "ich", "in", "ihr", "ge\u00b7lernt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Meine Sohlen eilen", "tokens": ["Mei\u00b7ne", "Soh\u00b7len", "ei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Unter einem Himmel, der auch sie besternt.", "tokens": ["Un\u00b7ter", "ei\u00b7nem", "Him\u00b7mel", ",", "der", "auch", "sie", "bes\u00b7ternt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}